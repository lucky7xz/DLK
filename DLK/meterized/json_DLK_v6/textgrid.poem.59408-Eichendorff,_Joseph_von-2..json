{"textgrid.poem.59408": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und zu den Felseng\u00e4ngen", "tokens": ["Und", "zu", "den", "Fel\u00b7sen\u00b7g\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der n\u00e4cht'ge S\u00e4nger flieht,", "tokens": ["Der", "n\u00e4cht'\u00b7ge", "S\u00e4n\u00b7ger", "flieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn wie mit Wahnsinus Kl\u00e4ngen", "tokens": ["Denn", "wie", "mit", "Wahn\u00b7si\u00b7nus", "Kl\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Treibt ihn sein eignes Lied.", "tokens": ["Treibt", "ihn", "sein", "eig\u00b7nes", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bei leuchtenden Gewittern", "tokens": ["Bei", "leuch\u00b7ten\u00b7den", "Ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schreckt ihn das stille Land,", "tokens": ["Schreckt", "ihn", "das", "stil\u00b7le", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein wunderbar Ersch\u00fcttern", "tokens": ["Ein", "wun\u00b7der\u00b7bar", "Er\u00b7sch\u00fct\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat ihm das Herz gewandt.", "tokens": ["Hat", "ihm", "das", "Herz", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Bereuend sinkt sein Auge \u2013", "tokens": ["Be\u00b7reu\u00b7end", "sinkt", "sein", "Au\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da blickt durch Nacht und Schmerz", "tokens": ["Da", "blickt", "durch", "Nacht", "und", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein unsichtbares Auge", "tokens": ["Ein", "un\u00b7sicht\u00b7ba\u00b7res", "Au\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihm klar ins tiefste Herz.", "tokens": ["Ihm", "klar", "ins", "tiefs\u00b7te", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sein Saitenspiel zur Stunde", "tokens": ["Sein", "Sai\u00b7ten\u00b7spiel", "zur", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wirft er in tiefsten Schlund,", "tokens": ["Wirft", "er", "in", "tiefs\u00b7ten", "Schlund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und weint aus Herzensgrunde,", "tokens": ["Und", "weint", "aus", "Her\u00b7zens\u00b7grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ewig schweigt sein Mund.", "tokens": ["Und", "e\u00b7wig", "schweigt", "sein", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}