{"textgrid.poem.44574": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[in dieser Zeit, wo alles sich bestrebt]", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dieser Zeit, wo alles sich bestrebt,", "tokens": ["In", "die\u00b7ser", "Zeit", ",", "wo", "al\u00b7les", "sich", "be\u00b7strebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PWAV", "PIS", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Leben m\u00f6glichst sauer uns zu machen,", "tokens": ["Das", "Le\u00b7ben", "m\u00f6g\u00b7lichst", "sau\u00b7er", "uns", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erbarmst du dich der Kranken und der Schwachen", "tokens": ["Er\u00b7barmst", "du", "dich", "der", "Kran\u00b7ken", "und", "der", "Schwa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und hast mit Ku\u00df und Anteil sie belebt.", "tokens": ["Und", "hast", "mit", "Ku\u00df", "und", "An\u00b7teil", "sie", "be\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "KON", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Magst diesem Volk, magst jenem du nun eignen,", "tokens": ["Magst", "die\u00b7sem", "Volk", ",", "magst", "je\u00b7nem", "du", "nun", "eig\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$,", "VMFIN", "PDAT", "PPER", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Nationen stehn ja doch auf dem Bankrutt,", "tokens": ["Die", "Na\u00b7ti\u00b7o\u00b7nen", "stehn", "ja", "doch", "auf", "dem", "Ban\u00b7krutt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Solang sie was gemeinsam nicht verleugnen,", "tokens": ["So\u00b7lang", "sie", "was", "ge\u00b7mein\u00b7sam", "nicht", "ver\u00b7leug\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ADJD", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Stehts immer noch mit ihrer Rechnung gut.", "tokens": ["Stehts", "im\u00b7mer", "noch", "mit", "ih\u00b7rer", "Rech\u00b7nung", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du hast als Mensch in Tugenden und Schw\u00e4chen", "tokens": ["Du", "hast", "als", "Mensch", "in", "Tu\u00b7gen\u00b7den", "und", "Schw\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gef\u00fchlt, geirrt, gescholten und geliebt,", "tokens": ["Ge\u00b7f\u00fchlt", ",", "ge\u00b7irrt", ",", "ge\u00b7schol\u00b7ten", "und", "ge\u00b7liebt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df unsern Dank dir denn desgleichen sprechen:", "tokens": ["La\u00df", "un\u00b7sern", "Dank", "dir", "denn", "des\u00b7glei\u00b7chen", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein Volk besteht, solang es Menschen gibt.", "tokens": ["Dein", "Volk", "be\u00b7steht", ",", "so\u00b7lang", "es", "Men\u00b7schen", "gibt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}