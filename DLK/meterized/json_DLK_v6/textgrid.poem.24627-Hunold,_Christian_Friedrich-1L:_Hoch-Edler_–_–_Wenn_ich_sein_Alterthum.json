{"textgrid.poem.24627": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hoch-Edler \u2013 \u2013 Wenn ich sein Alterthum/", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoch-Edler \u2013 \u2013 Wenn ich sein Alterthum/", "tokens": ["Hoch\u00b7Ed\u00b7ler", "\u2013", "\u2013", "Wenn", "ich", "sein", "Al\u00b7ter\u00b7thum", "/"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "$(", "KOUS", "PPER", "PPOSAT", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und was ihm Gottes-Huld auf Erden zugeme\u00dfen/", "tokens": ["Und", "was", "ihm", "Got\u00b7tes\u00b7Huld", "auf", "Er\u00b7den", "zu\u00b7ge\u00b7me\u00b7\u00dfen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Genau erwegen will/ so wie den Ehren Ruhm/", "tokens": ["Ge\u00b7nau", "er\u00b7we\u00b7gen", "will", "/", "so", "wie", "den", "Eh\u00b7ren", "Ruhm", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VMFIN", "$(", "ADV", "KOKOM", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den er bey redlichen so lange Zeit bese\u00dfen/", "tokens": ["Den", "er", "bey", "red\u00b7li\u00b7chen", "so", "lan\u00b7ge", "Zeit", "be\u00b7se\u00b7\u00dfen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "ADV", "ADJA", "NN", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "So bin ich h\u00f6chst vergn\u00fcgt/ und preise: da\u00df auf Erden", "tokens": ["So", "bin", "ich", "h\u00f6chst", "ver\u00b7gn\u00fcgt", "/", "und", "prei\u00b7se", ":", "da\u00df", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$(", "KON", "NN", "$.", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dieselben/ die Gott liebt/ so alt als gl\u00fccklich werden.", "tokens": ["Die\u00b7sel\u00b7ben", "/", "die", "Gott", "liebt", "/", "so", "alt", "als", "gl\u00fcck\u00b7lich", "wer\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "$(", "ART", "NN", "VVFIN", "$(", "ADV", "ADJD", "KOKOM", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es ist der Tugend-Sold: allhier gesegnet seyn.", "tokens": ["Es", "ist", "der", "Tu\u00b7gen\u00b7dSold", ":", "all\u00b7hier", "ge\u00b7seg\u00b7net", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Frucht der Gottes-Furcht wird ihm zu lauter Leben.", "tokens": ["Die", "Frucht", "der", "Got\u00b7tes\u00b7Furcht", "wird", "ihm", "zu", "lau\u00b7ter", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Noch heute stellet sich des Himmels Seegen ein/", "tokens": ["Noch", "heu\u00b7te", "stel\u00b7let", "sich", "des", "Him\u00b7mels", "See\u00b7gen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "ART", "NN", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den ersten Lebens-Tag ihm wiederum zu geben.", "tokens": ["Den", "ers\u00b7ten", "Le\u00b7bens\u00b7Tag", "ihm", "wie\u00b7de\u00b7rum", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So gro\u00df ist Gottes Huld/ so gro\u00df ist sein Ergetzen/", "tokens": ["So", "gro\u00df", "ist", "Got\u00b7tes", "Huld", "/", "so", "gro\u00df", "ist", "sein", "Er\u00b7get\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "NN", "NN", "$(", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hochwehrter/ und so hoch auch unsre Lust zu sch\u00e4tzen.", "tokens": ["Hoch\u00b7wehr\u00b7ter", "/", "und", "so", "hoch", "auch", "uns\u00b7re", "Lust", "zu", "sch\u00e4t\u00b7zen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KON", "ADV", "ADJD", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ist nun das h\u00f6chste Gl\u00fcck: ins Himmels Gnade stehn/", "tokens": ["Ist", "nun", "das", "h\u00f6chs\u00b7te", "Gl\u00fcck", ":", "ins", "Him\u00b7mels", "Gna\u00b7de", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$.", "APPRART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ist ein Heil dabey: dem F\u00fcrsten Wohlgefallen/", "tokens": ["So", "ist", "ein", "Heil", "da\u00b7bey", ":", "dem", "F\u00fcrs\u00b7ten", "Wohl\u00b7ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ", "$.", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bey G\u00f6ttern dieser Welt ein gn\u00e4digs Auge sehn.", "tokens": ["Bey", "G\u00f6t\u00b7tern", "die\u00b7ser", "Welt", "ein", "gn\u00e4\u00b7digs", "Au\u00b7ge", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PDAT", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und so begl\u00fccket ihn der F\u00fcrsten-Gunst in allen.", "tokens": ["Und", "so", "be\u00b7gl\u00fc\u00b7cket", "ihn", "der", "F\u00fcrs\u00b7ten\u00b7Gunst", "in", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Neid kan sein Verdienst/ Hoch-Edler/ daraus lesen/", "tokens": ["Der", "Neid", "kan", "sein", "Ver\u00b7dienst", "/", "Hoch\u00b7Ed\u00b7ler", "/", "da\u00b7raus", "le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "NN", "$(", "NE", "$(", "PAV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df ihm ein theurer Hoff so lange hold gewesen", "tokens": ["Da\u00df", "ihm", "ein", "theu\u00b7rer", "Hoff", "so", "lan\u00b7ge", "hold", "ge\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJD", "VVFIN", "ADV", "ADV", "ADJD", "VAPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Di\u00df alles/ und was ihn/ Herr \u2013 \u2013 mehr erqvickt/", "tokens": ["Di\u00df", "al\u00b7les", "/", "und", "was", "ihn", "/", "Herr", "\u2013", "\u2013", "mehr", "er\u00b7qvickt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PDS", "PIS", "$(", "KON", "PWS", "PPER", "$(", "NN", "$(", "$(", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie sein Durchl\u00e4uchtigster ihm neue W\u00fcrden schencket;", "tokens": ["Wie", "sein", "Durch\u00b7l\u00e4uch\u00b7tigs\u00b7ter", "ihm", "neu\u00b7e", "W\u00fcr\u00b7den", "schen\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wie Gottes Majest\u00e4t auf ihn von oben blickt/", "tokens": ["Wie", "Got\u00b7tes", "Ma\u00b7jes\u00b7t\u00e4t", "auf", "ihn", "von", "o\u00b7ben", "blickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "APPR", "PPER", "APPR", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die heute seinen Geist mit neuer Krafft bedencket/", "tokens": ["Die", "heu\u00b7te", "sei\u00b7nen", "Geist", "mit", "neu\u00b7er", "Krafft", "be\u00b7den\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ist von so hohen Wehrt/ das viele Hertzen lachen/", "tokens": ["Ist", "von", "so", "ho\u00b7hen", "Wehrt", "/", "das", "vie\u00b7le", "Hert\u00b7zen", "la\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADV", "ADJA", "NN", "$(", "ART", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und sich sein Lebens-Licht zum Freuden-Feste machen.", "tokens": ["Und", "sich", "sein", "Le\u00b7bens\u00b7Licht", "zum", "Freu\u00b7den\u00b7Fes\u00b7te", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Gott lege diesem Jahr noch viele gl\u00fccklich bey.", "tokens": ["Gott", "le\u00b7ge", "die\u00b7sem", "Jahr", "noch", "vie\u00b7le", "gl\u00fcck\u00b7lich", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PDAT", "NN", "ADV", "PIS", "ADJD", "PTKVZ", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Sein Ehren-Alter mu\u00df in steten Seegen schweben.", "tokens": ["Sein", "Eh\u00b7ren\u00b7Al\u00b7ter", "mu\u00df", "in", "ste\u00b7ten", "See\u00b7gen", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vergn\u00fcgung wolle Gott/ der Frommen stets getreu/", "tokens": ["Ver\u00b7gn\u00fc\u00b7gung", "wol\u00b7le", "Gott", "/", "der", "From\u00b7men", "stets", "ge\u00b7treu", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "NN", "$(", "ART", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und sein Durchlauchtigster ihm Gnaden-Blicke geben.", "tokens": ["Und", "sein", "Durch\u00b7lauch\u00b7tigs\u00b7ter", "ihm", "Gna\u00b7den\u00b7Bli\u00b7cke", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df sich das G\u00f6ttliche bey zeitlichen vermehre/", "tokens": ["Da\u00df", "sich", "das", "G\u00f6tt\u00b7li\u00b7che", "bey", "zeit\u00b7li\u00b7chen", "ver\u00b7meh\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPR", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und man der Tugend-Gl\u00fcck in seinem Wohl verehre/", "tokens": ["Und", "man", "der", "Tu\u00b7gen\u00b7dG\u00b7l\u00fcck", "in", "sei\u00b7nem", "Wohl", "ver\u00b7eh\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}