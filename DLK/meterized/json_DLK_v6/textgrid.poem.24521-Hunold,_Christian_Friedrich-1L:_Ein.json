{"textgrid.poem.24521": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein ", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Er bat ihn Himmel-hoch/ noch etwas zu verweilen.", "tokens": ["Er", "bat", "ihn", "Him\u00b7mel\u00b7hoch", "/", "noch", "et\u00b7was", "zu", "ver\u00b7wei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$(", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Pulver sey noch da/ ein Pulver/ welches sch\u00f6n/", "tokens": ["Ein", "Pul\u00b7ver", "sey", "noch", "da", "/", "ein", "Pul\u00b7ver", "/", "wel\u00b7ches", "sch\u00f6n", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "$(", "ART", "NN", "$(", "PWS", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von dem der ", "tokens": ["Von", "dem", "der"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Hier auf verzog der Tod; das Pulver kam im Magen;", "tokens": ["Hier", "auf", "ver\u00b7zog", "der", "Tod", ";", "das", "Pul\u00b7ver", "kam", "im", "Ma\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "VVFIN", "ART", "NN", "$.", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Allein der Krancke starb und ward zur Gruft getragen.", "tokens": ["Al\u00b7lein", "der", "Kran\u00b7cke", "starb", "und", "ward", "zur", "Gruft", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "KON", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn ward ein andrer kranck/ von dem der Tod noch fern;", "tokens": ["Denn", "ward", "ein", "an\u00b7drer", "kranck", "/", "von", "dem", "der", "Tod", "noch", "fern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "$(", "APPR", "PRELS", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Er rief: das Pulver her. Der ", "tokens": ["Er", "rief", ":", "das", "Pul\u00b7ver", "her", ".", "Der"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "PTKVZ", "$.", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Das sch\u00f6ne Pulver half dem Krancken von dem Leben.", "tokens": ["Das", "sch\u00f6\u00b7ne", "Pul\u00b7ver", "half", "dem", "Kran\u00b7cken", "von", "dem", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Doch lie\u00df der ", "tokens": ["Doch", "lie\u00df", "der"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Weil Menschen Staub und Koth/ was siehst du sauer aus:", "tokens": ["Weil", "Men\u00b7schen", "Staub", "und", "Koth", "/", "was", "siehst", "du", "sau\u00b7er", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "KON", "NN", "$(", "PWS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Zum Vortheil machen wir nur Pulver wieder draus.", "tokens": ["Zum", "Vor\u00b7theil", "ma\u00b7chen", "wir", "nur", "Pul\u00b7ver", "wie\u00b7der", "draus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}