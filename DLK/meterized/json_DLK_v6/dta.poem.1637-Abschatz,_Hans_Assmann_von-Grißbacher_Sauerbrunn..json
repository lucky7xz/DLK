{"dta.poem.1637": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Gri\u00dfbacher Sauerbrunn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.85", "en:0.14"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ort/ wo man die Artzney ohn theure Kosten hat/", "tokens": ["Ort", "/", "wo", "man", "die", "Artz\u00b7ney", "ohn", "theu\u00b7re", "Kos\u00b7ten", "hat", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PWAV", "PIS", "ART", "NN", "APPR", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo die Natur vertritt des Apothekers Statt/", "tokens": ["Wo", "die", "Na\u00b7tur", "ver\u00b7tritt", "des", "A\u00b7pot\u00b7he\u00b7kers", "Statt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.3": {"text": "nd uns der Erde Schos den Geist von Stal bereitet/", "tokens": ["nd", "uns", "der", "Er\u00b7de", "Schos", "den", "Geist", "von", "Stal", "be\u00b7rei\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "er Tod und Traurigkeit/ den \u00e4rgsten Feind bestreitet/", "tokens": ["er", "Tod", "und", "Trau\u00b7rig\u00b7keit", "/", "den", "\u00e4rgs\u00b7ten", "Feind", "be\u00b7strei\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "$(", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "o unsren Mund erfrischt der halb-ges\u00e4urte Safft/", "tokens": ["o", "un\u00b7sren", "Mund", "er\u00b7frischt", "der", "halb\u00b7ge\u00b7s\u00e4ur\u00b7te", "Safft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "o unsre Leber k\u00fchlt der frischen Quelle Krafft/", "tokens": ["o", "uns\u00b7re", "Le\u00b7ber", "k\u00fchlt", "der", "fri\u00b7schen", "Quel\u00b7le", "Krafft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "o unser Aug erg\u00f6zt durch so viel sch\u00f6ne G\u00e4nge", "tokens": ["o", "un\u00b7ser", "Aug", "er\u00b7g\u00f6zt", "durch", "so", "viel", "sch\u00f6\u00b7ne", "G\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "PPOSAT", "NN", "VVPP", "APPR", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "er gr\u00fcnen H\u00fcgel Schaar/ der k\u00fchlen Th\u00e4ler Menge/", "tokens": ["er", "gr\u00fc\u00b7nen", "H\u00fc\u00b7gel", "Schaar", "/", "der", "k\u00fch\u00b7len", "Th\u00e4\u00b7ler", "Men\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "NN", "$(", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "er B\u00e4che Gla\u00df-Crystall/ der L\u00fcffte Reinigkeit/", "tokens": ["er", "B\u00e4\u00b7che", "Gla\u00df\u00b7Cry\u00b7stall", "/", "der", "L\u00fcff\u00b7te", "Rei\u00b7nig\u00b7keit", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "NN", "$(", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "d was uns mehr zur Lust des Himmels Gunst geweyht.", "tokens": ["d", "was", "uns", "mehr", "zur", "Lust", "des", "Him\u00b7mels", "Gunst", "ge\u00b7weyht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["XY", "PWS", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "r Nimphen dieses Orts/ ihr freundlichen Najaden/", "tokens": ["r", "Nim\u00b7phen", "die\u00b7ses", "Orts", "/", "ihr", "freund\u00b7li\u00b7chen", "Na\u00b7ja\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PDAT", "NN", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "ch will in eurer Flutt mein Eingeweyde baden:", "tokens": ["ch", "will", "in", "eu\u00b7rer", "Flutt", "mein", "Ein\u00b7ge\u00b7wey\u00b7de", "ba\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Trifft k\u00fcnfftig diese Cur mit meinem W\u00fcnschen ein/", "tokens": ["Trifft", "k\u00fcnff\u00b7tig", "die\u00b7se", "Cur", "mit", "mei\u00b7nem", "W\u00fcn\u00b7schen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PDAT", "NN", "APPR", "PPOSAT", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So soll eur Nahme nie bey mir vergessen seyn;", "tokens": ["So", "soll", "eur", "Nah\u00b7me", "nie", "bey", "mir", "ver\u00b7ges\u00b7sen", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "ADV", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Das Gri\u00dfbach/ das von mir hat Gri\u00df und Sand getrieben/", "tokens": ["Das", "Gri\u00df\u00b7bach", "/", "das", "von", "mir", "hat", "Gri\u00df", "und", "Sand", "ge\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "APPR", "PPER", "VAFIN", "NE", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Soll meinem Hertzen stets mit Ruhm seyn eingeschrieben.", "tokens": ["Soll", "mei\u00b7nem", "Hert\u00b7zen", "stets", "mit", "Ruhm", "seyn", "ein\u00b7ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "APPR", "NN", "VAINF", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}