{"textgrid.poem.32303": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Der Genu\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So bringst du mich um meine Liebe,", "tokens": ["So", "bringst", "du", "mich", "um", "mei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Unseliger Genu\u00df? Betr\u00fcbter Tag f\u00fcr mich!", "tokens": ["Un\u00b7se\u00b7li\u00b7ger", "Ge\u00b7nu\u00df", "?", "Be\u00b7tr\u00fcb\u00b7ter", "Tag", "f\u00fcr", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NN", "NN", "APPR", "PPER", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Sie zu verlieren, \u2013 meine Liebe, \u2013", "tokens": ["Sie", "zu", "ver\u00b7lie\u00b7ren", ",", "\u2013", "mei\u00b7ne", "Lie\u00b7be", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "$(", "PPOSAT", "NN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie zu verlieren, w\u00fcnscht' ich dich?", "tokens": ["Sie", "zu", "ver\u00b7lie\u00b7ren", ",", "w\u00fcnscht'", "ich", "dich", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nimm sie, den Wunsch so mancher Lieder,", "tokens": ["Nimm", "sie", ",", "den", "Wunsch", "so", "man\u00b7cher", "Lie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ART", "NN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nimm sie zur\u00fcck, die kurze Lust!", "tokens": ["Nimm", "sie", "zu\u00b7r\u00fcck", ",", "die", "kur\u00b7ze", "Lust", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nimm sie, und gib der \u00f6den Brust,", "tokens": ["Nimm", "sie", ",", "und", "gib", "der", "\u00f6\u00b7den", "Brust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KON", "VVIMP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der ewig \u00f6den Brust, die be\u00dfre Liebe wieder!", "tokens": ["Der", "e\u00b7wig", "\u00f6\u00b7den", "Brust", ",", "die", "be\u00df\u00b7re", "Lie\u00b7be", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}