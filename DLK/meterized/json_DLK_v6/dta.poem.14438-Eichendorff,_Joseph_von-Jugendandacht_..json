{"dta.poem.14438": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Jugendandacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Mit eigner Kette so Dich s\u00fc\u00df umwinden,", "tokens": ["Mit", "eig\u00b7ner", "Ket\u00b7te", "so", "Dich", "s\u00fc\u00df", "um\u00b7win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hinauf zieh'n Dich an Mund und bl\u00fchend Herze \u2014", "tokens": ["Hin\u00b7auf", "zieh'n", "Dich", "an", "Mund", "und", "bl\u00fc\u00b7hend", "Her\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was Himmel schlo\u00df, mag nicht der Himmel brechen.\u201c", "tokens": ["Was", "Him\u00b7mel", "schlo\u00df", ",", "mag", "nicht", "der", "Him\u00b7mel", "bre\u00b7chen", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "NN", "VVFIN", "$,", "VMFIN", "PTKNEG", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}