{"textgrid.poem.65325": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Gefangene", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An dem Zitronenbaum vor deinem Hause,", "tokens": ["An", "dem", "Zit\u00b7ro\u00b7nen\u00b7baum", "vor", "dei\u00b7nem", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da hab' ich aufgeh\u00e4ngt zu deinem Preise", "tokens": ["Da", "hab'", "ich", "auf\u00b7ge\u00b7h\u00e4ngt", "zu", "dei\u00b7nem", "Prei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein freies Herz und lass' es mich nicht dauern.", "tokens": ["Mein", "frei\u00b7es", "Herz", "und", "lass'", "es", "mich", "nicht", "dau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "VVFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Siehst du es nicht? Und hast doch selbst die Banden", "tokens": ["Siehst", "du", "es", "nicht", "?", "Und", "hast", "doch", "selbst", "die", "Ban\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "$.", "KON", "VAFIN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geflochten, die so eng' es rings umwinden,", "tokens": ["Ge\u00b7floch\u00b7ten", ",", "die", "so", "eng'", "es", "rings", "um\u00b7win\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und fest es in den gr\u00fcnen Zweigen halten!", "tokens": ["Und", "fest", "es", "in", "den", "gr\u00fc\u00b7nen", "Zwei\u00b7gen", "hal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die V\u00f6glein fliegen darauf zu und staunen,", "tokens": ["Die", "V\u00f6\u00b7glein", "flie\u00b7gen", "da\u00b7rauf", "zu", "und", "stau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie sich solch loser Vogel mag gew\u00f6hnen,", "tokens": ["Wie", "sich", "solch", "lo\u00b7ser", "Vo\u00b7gel", "mag", "ge\u00b7w\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PIAT", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So still und steif im Gr\u00fcnen auszudauern.", "tokens": ["So", "still", "und", "steif", "im", "Gr\u00fc\u00b7nen", "aus\u00b7zu\u00b7dau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}