{"textgrid.poem.65279": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Umkehr", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf die Alpen dort bin ich gestiegen,", "tokens": ["Auf", "die", "Al\u00b7pen", "dort", "bin", "ich", "ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Habe weit und breit mich umgesehn:", "tokens": ["Ha\u00b7be", "weit", "und", "breit", "mich", "um\u00b7ge\u00b7sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Heerden sah ich in dem Grase liegen,", "tokens": ["Heer\u00b7den", "sah", "ich", "in", "dem", "Gra\u00b7se", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sch\u00e4ferinnen bei den Sch\u00e4fern stehn.", "tokens": ["Sch\u00e4\u00b7fe\u00b7rin\u00b7nen", "bei", "den", "Sch\u00e4\u00b7fern", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Aber auf den sch\u00f6nen gr\u00fcnen Auen", "tokens": ["A\u00b7ber", "auf", "den", "sch\u00f6\u00b7nen", "gr\u00fc\u00b7nen", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Fand ich Eine, die ich suchte, nicht;", "tokens": ["Fand", "ich", "Ei\u00b7ne", ",", "die", "ich", "such\u00b7te", ",", "nicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "$,", "PRELS", "PPER", "VVFIN", "$,", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und das lange, ferne, starre Schauen", "tokens": ["Und", "das", "lan\u00b7ge", ",", "fer\u00b7ne", ",", "star\u00b7re", "Schau\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Machte tr\u00fcbe meiner Augen Licht.", "tokens": ["Mach\u00b7te", "tr\u00fc\u00b7be", "mei\u00b7ner", "Au\u00b7gen", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "In das Thal bin ich zur\u00fcckgegangen,", "tokens": ["In", "das", "Thal", "bin", "ich", "zu\u00b7r\u00fcck\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In das kleine, tiefe, finstre Thal,", "tokens": ["In", "das", "klei\u00b7ne", ",", "tie\u00b7fe", ",", "finst\u00b7re", "Thal", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Habe meinen Mantel umgehangen", "tokens": ["Ha\u00b7be", "mei\u00b7nen", "Man\u00b7tel", "um\u00b7ge\u00b7han\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und mich hingestreckt mit meiner Qual.", "tokens": ["Und", "mich", "hin\u00b7ge\u00b7streckt", "mit", "mei\u00b7ner", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Ja, und wenn die Engel einst mich f\u00fchren", "tokens": ["Ja", ",", "und", "wenn", "die", "En\u00b7gel", "einst", "mich", "f\u00fch\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KON", "KOUS", "ART", "NN", "ADV", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aus dem Grabe nach dem Paradies,", "tokens": ["Aus", "dem", "Gra\u00b7be", "nach", "dem", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Seh' ich erst vor seinen goldnen Th\u00fcren", "tokens": ["Seh'", "ich", "erst", "vor", "sei\u00b7nen", "gold\u00b7nen", "Th\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Weit und breit mich um nach ", "tokens": ["Weit", "und", "breit", "mich", "um", "nach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "PRF", "APPR", "APPR"], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Wenn ", "tokens": ["Wenn"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Kehr' ich um und schaue nicht hinein.", "tokens": ["Kehr'", "ich", "um", "und", "schau\u00b7e", "nicht", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "KON", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Will in' s enge, dunkle Grab mich dr\u00fccken", "tokens": ["Will", "in'", "s", "en\u00b7ge", ",", "dunk\u00b7le", "Grab", "mich", "dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "FM.fr", "FM.fr", "FM.fr", "$,", "ADJA", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und verschlafen alle Freud' und Pein.", "tokens": ["Und", "ver\u00b7schla\u00b7fen", "al\u00b7le", "Freud'", "und", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}