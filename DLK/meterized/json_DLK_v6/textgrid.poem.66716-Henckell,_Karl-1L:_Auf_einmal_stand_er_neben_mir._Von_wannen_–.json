{"textgrid.poem.66716": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf einmal stand er neben mir. Von wannen \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf einmal stand er neben mir. Von wannen \u2013", "tokens": ["Auf", "ein\u00b7mal", "stand", "er", "ne\u00b7ben", "mir", ".", "Von", "wan\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "APPR", "PPER", "$.", "APPR", "ADV", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das wei\u00df ich nicht und nicht, wie jetzt er kam.", "tokens": ["Das", "wei\u00df", "ich", "nicht", "und", "nicht", ",", "wie", "jetzt", "er", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "KON", "PTKNEG", "$,", "PWAV", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf seinem Antlitz lief ein leiser Gram", "tokens": ["Auf", "sei\u00b7nem", "Ant\u00b7litz", "lief", "ein", "lei\u00b7ser", "Gram"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In Furchen hin, die r\u00e4tselhaft verrannen.", "tokens": ["In", "Fur\u00b7chen", "hin", ",", "die", "r\u00e4t\u00b7sel\u00b7haft", "ver\u00b7ran\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "PRELS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich sah: er wollte mich nicht schreckhaft bannen,", "tokens": ["Ich", "sah", ":", "er", "woll\u00b7te", "mich", "nicht", "schreck\u00b7haft", "ban\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "War wie ein Freund, der bei der Hand mich nahm,", "tokens": ["War", "wie", "ein", "Freund", ",", "der", "bei", "der", "Hand", "mich", "nahm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein sanftes L\u00e4cheln spielte wundersam", "tokens": ["Ein", "sanf\u00b7tes", "L\u00e4\u00b7cheln", "spiel\u00b7te", "wun\u00b7der\u00b7sam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Um seinen Mund. Die Augen ruhend sannen.", "tokens": ["Um", "sei\u00b7nen", "Mund", ".", "Die", "Au\u00b7gen", "ru\u00b7hend", "san\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "$.", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Er sprach: Ich bin der Sohn der Lebensspur,", "tokens": ["Er", "sprach", ":", "Ich", "bin", "der", "Sohn", "der", "Le\u00b7bens\u00b7spur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die du von Anfang bis hierher gezogen,", "tokens": ["Die", "du", "von", "An\u00b7fang", "bis", "hier\u00b7her", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gezeugt aus Wolkenschwarz und Goldazur.", "tokens": ["Ge\u00b7zeugt", "aus", "Wol\u00b7ken\u00b7schwarz", "und", "Gol\u00b7da\u00b7zur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Des Lenzes Leuchten hat mich \u00fcberflogen,", "tokens": ["Des", "Len\u00b7zes", "Leuch\u00b7ten", "hat", "mich", "\u00fc\u00b7berf\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "All deiner Tr\u00e4nen Tau hab ich gesogen \u2013", "tokens": ["All", "dei\u00b7ner", "Tr\u00e4\u00b7nen", "Tau", "hab", "ich", "ge\u00b7so\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus Licht und Schatten bin dein Selbst ich nur ...", "tokens": ["Aus", "Licht", "und", "Schat\u00b7ten", "bin", "dein", "Selbst", "ich", "nur", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "PPOSAT", "NN", "PPER", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}