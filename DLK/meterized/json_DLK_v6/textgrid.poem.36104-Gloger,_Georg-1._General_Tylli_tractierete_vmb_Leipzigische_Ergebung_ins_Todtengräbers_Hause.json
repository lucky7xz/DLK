{"textgrid.poem.36104": {"metadata": {"author": {"name": "Gloger, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1. General Tylli tractierete vmb Leipzigische Ergebung ins Todtengr\u00e4bers Hause", "genre": "verse", "period": "N.A.", "pub_year": 1617, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was kan man anders wol beym Todtengr\u00e4ber machen,", "tokens": ["Was", "kan", "man", "an\u00b7ders", "wol", "beym", "Tod\u00b7ten\u00b7gr\u00e4\u00b7ber", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PIS", "ADV", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als dingen vmb die Bahr' vnd andre Leichen-Sachen?", "tokens": ["Als", "din\u00b7gen", "vmb", "die", "Bahr'", "vnd", "and\u00b7re", "Lei\u00b7chen\u00b7Sa\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dr\u00fcmb weil auch ", "tokens": ["Dr\u00fcmb", "weil", "auch"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KOUS", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Vnd nun zu ", "tokens": ["Vnd", "nun", "zu"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "PTKZU"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "So ging' er selbst mit Flei\u00df, ohn alle Schew vnd Sch\u00e4men", "tokens": ["So", "ging'", "er", "selbst", "mit", "Flei\u00df", ",", "ohn", "al\u00b7le", "Schew", "vnd", "Sch\u00e4\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,", "KOUI", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ins Todtengr\u00e4bers Haus, vnd lie\u00df ein Ma\u00df ihm nehmen", "tokens": ["Ins", "Tod\u00b7ten\u00b7gr\u00e4\u00b7bers", "Haus", ",", "vnd", "lie\u00df", "ein", "Ma\u00df", "ihm", "neh\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "NN", "$,", "KON", "VVFIN", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Zum Sarg vnd zu der Gruft, besah auch selbst die Bahre,", "tokens": ["Zum", "Sarg", "vnd", "zu", "der", "Gruft", ",", "be\u00b7sah", "auch", "selbst", "die", "Bah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "ART", "NN", "$,", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den Kirchhof vnd den Ort, der gegen\u00fcber ware.", "tokens": ["Den", "Kirch\u00b7hof", "vnd", "den", "Ort", ",", "der", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "wa\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "PRELS", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vnd zwar er solte fast gar wol gethan dran haben:", "tokens": ["Vnd", "zwar", "er", "sol\u00b7te", "fast", "gar", "wol", "ge\u00b7than", "dran", "ha\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VMFIN", "ADV", "ADV", "ADV", "VVPP", "PAV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Denn ihn der Todten-Mann bald m\u00fcssen mit begraben.", "tokens": ["Denn", "ihn", "der", "Tod\u00b7ten\u00b7Mann", "bald", "m\u00fcs\u00b7sen", "mit", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "ADV", "VMFIN", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ja w\u00e4r er in der Flucht so feste nicht gewesen,", "tokens": ["Ja", "w\u00e4r", "er", "in", "der", "Flucht", "so", "fes\u00b7te", "nicht", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VAFIN", "PPER", "APPR", "ART", "NN", "ADV", "ADJA", "PTKNEG", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So w\u00e4r er auch allhier, wie ", "tokens": ["So", "w\u00e4r", "er", "auch", "all\u00b7hier", ",", "wie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "$,", "PWAV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Nun ist er itzt gleich fort, so mu\u00df er doch beklagen,", "tokens": ["Nun", "ist", "er", "itzt", "gleich", "fort", ",", "so", "mu\u00df", "er", "doch", "be\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "PTKVZ", "$,", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df er drey Sch\u00f6sse hab', vnd sey aufs Haupt geschlagen.", "tokens": ["Da\u00df", "er", "drey", "Sch\u00f6s\u00b7se", "hab'", ",", "vnd", "sey", "aufs", "Haupt", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "CARD", "NN", "VAFIN", "$,", "KON", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}