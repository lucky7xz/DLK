{"dta.poem.9693": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Laura will in keinen spiegel mehr sehen.  \n E. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Narcissus liebet sich/", "tokens": ["Nar\u00b7cis\u00b7sus", "lie\u00b7bet", "sich", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "$("], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.2": {"text": "Wenn er sein angesicht in einem brunnen sieht/", "tokens": ["Wenn", "er", "sein", "an\u00b7ge\u00b7sicht", "in", "ei\u00b7nem", "brun\u00b7nen", "sieht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie milch und blut so sch\u00f6n darinnen bl\u00fcht:", "tokens": ["Wie", "milch", "und", "blut", "so", "sch\u00f6n", "da\u00b7rin\u00b7nen", "bl\u00fcht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "NN", "ADV", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Drumb will er nicht von diesem spiegel gehen/", "tokens": ["Drumb", "will", "er", "nicht", "von", "die\u00b7sem", "spie\u00b7gel", "ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PTKNEG", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Er sieht sie drinnen stehen/", "tokens": ["Er", "sieht", "sie", "drin\u00b7nen", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und denckt/ da\u00df ihn ein g\u00f6ttlich bild beth\u00f6rt.", "tokens": ["Und", "denckt", "/", "da\u00df", "ihn", "ein", "g\u00f6tt\u00b7lich", "bild", "be\u00b7th\u00f6rt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PPER", "ART", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Hier ist es umgekehrt/", "tokens": ["Hier", "ist", "es", "um\u00b7ge\u00b7kehrt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wenn Laura sich im spiegel will beschauen/", "tokens": ["Wenn", "Lau\u00b7ra", "sich", "im", "spie\u00b7gel", "will", "be\u00b7schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "APPRART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "So f\u00e4ngt ihr an zu grauen/", "tokens": ["So", "f\u00e4ngt", "ihr", "an", "zu", "grau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Und laufft zum hause naus/", "tokens": ["Und", "laufft", "zum", "hau\u00b7se", "naus", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Als wenn der teuffel selbst zum spiegel seh\u2019 heraus.", "tokens": ["Als", "wenn", "der", "teuf\u00b7fel", "selbst", "zum", "spie\u00b7gel", "seh'", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "ADV", "APPRART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}