{"dta.poem.7671": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "43.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.71", "fr:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn eine Uhr du hast, mu\u00dft du doch jedes Nu", "tokens": ["Wenn", "ei\u00b7ne", "Uhr", "du", "hast", ",", "mu\u00dft", "du", "doch", "je\u00b7des", "Nu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "VAFIN", "$,", "VMFIN", "PPER", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darnach nicht sehn, viel Zeit damit vers\u00e4umtest du.", "tokens": ["Dar\u00b7nach", "nicht", "sehn", ",", "viel", "Zeit", "da\u00b7mit", "ver\u00b7s\u00e4um\u00b7test", "du", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKNEG", "VVINF", "$,", "PIAT", "NN", "PAV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Thu dein Gesch\u00e4fte nur mit Lust und aus dem Grunde,", "tokens": ["Thu", "dein", "Ge\u00b7sch\u00e4f\u00b7te", "nur", "mit", "Lust", "und", "aus", "dem", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "APPR", "NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und frage nicht, ob es grad aufgeht mit der Stunde.", "tokens": ["Und", "fra\u00b7ge", "nicht", ",", "ob", "es", "grad", "auf\u00b7geht", "mit", "der", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "La\u00df andre von der Zeit gar raschem Laufe sprechen,", "tokens": ["La\u00df", "and\u00b7re", "von", "der", "Zeit", "gar", "ra\u00b7schem", "Lau\u00b7fe", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIS", "APPR", "ART", "NN", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr rennen nach und vor, um sich den Hals zu brechen.", "tokens": ["Ihr", "ren\u00b7nen", "nach", "und", "vor", ",", "um", "sich", "den", "Hals", "zu", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "KON", "PTKVZ", "$,", "KOUI", "PRF", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und bliebst du auch zur\u00fcck, merkst du's nach einer Frist,", "tokens": ["Und", "bliebst", "du", "auch", "zu\u00b7r\u00fcck", ",", "merkst", "du's", "nach", "ei\u00b7ner", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und holst die Zeit schon ein, wenn an der Zeit es ist.", "tokens": ["Und", "holst", "die", "Zeit", "schon", "ein", ",", "wenn", "an", "der", "Zeit", "es", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,", "KOUS", "APPR", "ART", "NN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Genug, wenn du nur mit fortkommst in Bausch und Bogen,", "tokens": ["Ge\u00b7nug", ",", "wenn", "du", "nur", "mit", "fort\u00b7kommst", "in", "Bausch", "und", "Bo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "APPR", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du im Strome schwimmst, und z\u00e4hlest nicht die Wogen.", "tokens": ["Wenn", "du", "im", "Stro\u00b7me", "schwimmst", ",", "und", "z\u00e4h\u00b7lest", "nicht", "die", "Wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,", "KON", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Den Zeitungsschreibern und Zeitschreibern la\u00df die Lust,", "tokens": ["Den", "Zei\u00b7tungs\u00b7schrei\u00b7bern", "und", "Zeit\u00b7schrei\u00b7bern", "la\u00df", "die", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Genau zu merken, was nun an der Zeit ist just.", "tokens": ["Ge\u00b7nau", "zu", "mer\u00b7ken", ",", "was", "nun", "an", "der", "Zeit", "ist", "just", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Dir aber wo die Uhr die Zeit nicht sagt, da sage", "tokens": ["Dir", "a\u00b7ber", "wo", "die", "Uhr", "die", "Zeit", "nicht", "sagt", ",", "da", "sa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "PWAV", "ART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie dir der Sterne Stand Nachts und der Sonn' am Tage.", "tokens": ["Sie", "dir", "der", "Ster\u00b7ne", "Stand", "Nachts", "und", "der", "Sonn'", "am", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ART", "NN", "NN", "ADV", "KON", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}}}}