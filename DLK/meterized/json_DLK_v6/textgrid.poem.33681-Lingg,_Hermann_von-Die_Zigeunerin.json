{"textgrid.poem.33681": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Die Zigeunerin", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sechzehn Jahr alt ist die kleine", "tokens": ["Sech\u00b7zehn", "Jahr", "alt", "ist", "die", "klei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "ADJD", "VAFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sittah, die Zigeunerin.", "tokens": ["Sit\u00b7tah", ",", "die", "Zi\u00b7geu\u00b7ne\u00b7rin", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wild wie sie tanzt keine, keine", "tokens": ["Wild", "wie", "sie", "tanzt", "kei\u00b7ne", ",", "kei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "KOKOM", "PPER", "VVFIN", "PIAT", "$,", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwingt wie sie das Tamburin.", "tokens": ["Schwingt", "wie", "sie", "das", "Tam\u00b7bu\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kauernd an der alten Mauer", "tokens": ["Kau\u00b7ernd", "an", "der", "al\u00b7ten", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor des Mohrenk\u00f6nigs Tor", "tokens": ["Vor", "des", "Moh\u00b7ren\u00b7k\u00f6\u00b7nigs", "Tor"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fand ich sie im Fieberschauer,", "tokens": ["Fand", "ich", "sie", "im", "Fie\u00b7ber\u00b7schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich hob sie sanft empor.", "tokens": ["Und", "ich", "hob", "sie", "sanft", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Fliehe, sprach sie, geh vor\u00fcber,", "tokens": ["Flie\u00b7he", ",", "sprach", "sie", ",", "geh", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00f6dlich ist mein Fieberhauch!", "tokens": ["T\u00f6d\u00b7lich", "ist", "mein", "Fie\u00b7ber\u00b7hauch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kind, erwidert' ich, am Fieber,", "tokens": ["Kind", ",", "er\u00b7wi\u00b7dert'", "ich", ",", "am", "Fie\u00b7ber", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, am schlimmsten leid' ich auch:", "tokens": ["Ach", ",", "am", "schlimms\u00b7ten", "leid'", "ich", "auch", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPRART", "ADJA", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Liebe hei\u00dft es, dies verzehrend", "tokens": ["Lie\u00b7be", "hei\u00dft", "es", ",", "dies", "ver\u00b7zeh\u00b7rend"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "PDS", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hei\u00dfe Fieber; doch gesund", "tokens": ["Hei\u00b7\u00dfe", "Fie\u00b7ber", ";", "doch", "ge\u00b7sund"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00fcsse, mir nicht l\u00e4nger wehrend,", "tokens": ["K\u00fcs\u00b7se", ",", "mir", "nicht", "l\u00e4n\u00b7ger", "weh\u00b7rend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fcsse mich dein roter Mund. \u2013", "tokens": ["K\u00fcs\u00b7se", "mich", "dein", "ro\u00b7ter", "Mund", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sechzehn Jahr alt ist die kleine", "tokens": ["Sech\u00b7zehn", "Jahr", "alt", "ist", "die", "klei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "ADJD", "VAFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sittah, die Zigeunerin,", "tokens": ["Sit\u00b7tah", ",", "die", "Zi\u00b7geu\u00b7ne\u00b7rin", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wild wie sie tanzt keine, keine", "tokens": ["Wild", "wie", "sie", "tanzt", "kei\u00b7ne", ",", "kei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "KOKOM", "PPER", "VVFIN", "PIAT", "$,", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwingt wie sie das Tamburin.", "tokens": ["Schwingt", "wie", "sie", "das", "Tam\u00b7bu\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ihres Busens zarte Welle", "tokens": ["Ih\u00b7res", "Bu\u00b7sens", "zar\u00b7te", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gl\u00e4nzt, von Seide halb verh\u00fcllt,", "tokens": ["Gl\u00e4nzt", ",", "von", "Sei\u00b7de", "halb", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NE", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie des Mondes reine Helle,", "tokens": ["Wie", "des", "Mon\u00b7des", "rei\u00b7ne", "Hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die mit Sehnsucht uns erf\u00fcllt.", "tokens": ["Die", "mit", "Sehn\u00b7sucht", "uns", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie der Tau in Nachtviolen", "tokens": ["Wie", "der", "Tau", "in", "Nacht\u00b7vi\u00b7o\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blitzt ihr Auge wunderbar,", "tokens": ["Blitzt", "ihr", "Au\u00b7ge", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ein Goldschmuck unter Kohlen", "tokens": ["Wie", "ein", "Gold\u00b7schmuck", "un\u00b7ter", "Koh\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus dem schwarzen Lockenhaar.", "tokens": ["Aus", "dem", "schwar\u00b7zen", "Lo\u00b7cken\u00b7haar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Solchen Glutblick hat nur Eine,", "tokens": ["Sol\u00b7chen", "Glut\u00b7blick", "hat", "nur", "Ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ART", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine nur ist's, die so lacht,", "tokens": ["Ei\u00b7ne", "nur", "ist's", ",", "die", "so", "lacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So satanisch lacht beim Weine", "tokens": ["So", "sa\u00b7ta\u00b7nisch", "lacht", "beim", "Wei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und so himmlisch in der Nacht.", "tokens": ["Und", "so", "himm\u00b7lisch", "in", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}