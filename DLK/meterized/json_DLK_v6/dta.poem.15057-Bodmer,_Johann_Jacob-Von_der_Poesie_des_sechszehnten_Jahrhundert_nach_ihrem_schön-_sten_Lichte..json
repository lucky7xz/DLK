{"dta.poem.15057": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von der Poesie des sechszehnten  \n Jahrhundert nach ihrem sch\u00f6n-  \n sten Lichte.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198476", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So hat sein gl\u00fccklich Schiff zwar einen lustgen Grund,", "tokens": ["So", "hat", "sein", "gl\u00fcck\u00b7lich", "Schiff", "zwar", "ei\u00b7nen", "lust\u00b7gen", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJD", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und giebt doch die Natur in starcken Proben kund.", "tokens": ["Und", "giebt", "doch", "die", "Na\u00b7tur", "in", "star\u00b7cken", "Pro\u00b7ben", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durchsichten, Wasserf\u00e4ll\u2019, als so verschiedne B\u00fchnen,", "tokens": ["Durch\u00b7sich\u00b7ten", ",", "Was\u00b7ser\u00b7f\u00e4ll'", ",", "als", "so", "ver\u00b7schied\u00b7ne", "B\u00fch\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "KOUS", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Character, Neigungen, auch Reden und Maschinen;", "tokens": ["Cha\u00b7rac\u00b7ter", ",", "Nei\u00b7gun\u00b7gen", ",", "auch", "Re\u00b7den", "und", "Ma\u00b7schi\u00b7nen", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "Dies alles fehlt hier nicht.", "tokens": ["Dies", "al\u00b7les", "fehlt", "hier", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}