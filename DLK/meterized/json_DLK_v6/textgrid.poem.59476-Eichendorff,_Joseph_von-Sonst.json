{"textgrid.poem.59476": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonst", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es gl\u00e4nzt der Tulpenflor, durchschnitten von Alleen,", "tokens": ["Es", "gl\u00e4nzt", "der", "Tul\u00b7pen\u00b7flor", ",", "durch\u00b7schnit\u00b7ten", "von", "Al\u00b7leen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo zwischen Taxus still die wei\u00dfen Statuen stehen,", "tokens": ["Wo", "zwi\u00b7schen", "Ta\u00b7xus", "still", "die", "wei\u00b7\u00dfen", "Sta\u00b7tu\u00b7en", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Mit goldnen Kugeln spielt die Wasserkunst im Becken,", "tokens": ["Mit", "gold\u00b7nen", "Ku\u00b7geln", "spielt", "die", "Was\u00b7ser\u00b7kunst", "im", "Be\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Im Laube lauert Sphinx, anmutig zu erschrecken.", "tokens": ["Im", "Lau\u00b7be", "lau\u00b7ert", "Sphinx", ",", "an\u00b7mu\u00b7tig", "zu", "er\u00b7schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "$,", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die sch\u00f6ne Chloe heut spazieret in dem Garten,", "tokens": ["Die", "sch\u00f6\u00b7ne", "Chloe", "heut", "spa\u00b7zie\u00b7ret", "in", "dem", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+---+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zur Seit ein Kavalier, ihr h\u00f6flich aufzuwarten,", "tokens": ["Zur", "Seit", "ein", "Ka\u00b7va\u00b7lier", ",", "ihr", "h\u00f6f\u00b7lich", "auf\u00b7zu\u00b7war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PPER", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und hinter ihnen leis Cupido kommt gezogen,", "tokens": ["Und", "hin\u00b7ter", "ih\u00b7nen", "leis", "Cu\u00b7pi\u00b7do", "kommt", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADJD", "NE", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bald duckend sich im Gr\u00fcn, bald zielend mit dem Bogen.", "tokens": ["Bald", "du\u00b7ckend", "sich", "im", "Gr\u00fcn", ",", "bald", "zie\u00b7lend", "mit", "dem", "Bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "APPRART", "NN", "$,", "ADV", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es neigt der Kavalier sich in galantem Kosen,", "tokens": ["Es", "neigt", "der", "Ka\u00b7va\u00b7lier", "sich", "in", "ga\u00b7lan\u00b7tem", "Ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit ihrem F\u00e4cher schl\u00e4gt sie manchmal nach dem Losen,", "tokens": ["Mit", "ih\u00b7rem", "F\u00e4\u00b7cher", "schl\u00e4gt", "sie", "manch\u00b7mal", "nach", "dem", "Lo\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es rauscht der taftne Rock, es blitzen seine Schnallen,", "tokens": ["Es", "rauscht", "der", "taft\u00b7ne", "Rock", ",", "es", "blit\u00b7zen", "sei\u00b7ne", "Schnal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dazwischen h\u00f6rt man oft ein art'ges Lachen schallen.", "tokens": ["Da\u00b7zwi\u00b7schen", "h\u00f6rt", "man", "oft", "ein", "art'\u00b7ges", "La\u00b7chen", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Jetzt aber hebt vom Schlo\u00df, da sich's im West will r\u00f6ten,", "tokens": ["Jetzt", "a\u00b7ber", "hebt", "vom", "Schlo\u00df", ",", "da", "sich's", "im", "West", "will", "r\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPRART", "NN", "$,", "KOUS", "PIS", "APPRART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Spieluhr schmachtend an, ein Menuett zu fl\u00f6ten,", "tokens": ["Die", "Spie\u00b7luhr", "schmach\u00b7tend", "an", ",", "ein", "Me\u00b7nu\u00b7ett", "zu", "fl\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Laube ist so still, er wirft sein Tuch zur Erde", "tokens": ["Die", "Lau\u00b7be", "ist", "so", "still", ",", "er", "wirft", "sein", "Tuch", "zur", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und st\u00fcrzet auf ein Knie mit z\u00e4rtlicher Geb\u00e4rde.", "tokens": ["Und", "st\u00fcr\u00b7zet", "auf", "ein", "Knie", "mit", "z\u00e4rt\u00b7li\u00b7cher", "Ge\u00b7b\u00e4r\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "\u00bbwie wird mir, ach, ach, ach, es f\u00e4ngt schon an zu dunkeln \u2013\u00ab", "tokens": ["\u00bb", "wie", "wird", "mir", ",", "ach", ",", "ach", ",", "ach", ",", "es", "f\u00e4ngt", "schon", "an", "zu", "dun\u00b7keln", "\u2013", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "VAFIN", "PPER", "$,", "ITJ", "$,", "ITJ", "$,", "ITJ", "$,", "PPER", "VVFIN", "ADV", "APPR", "PTKZU", "VVINF", "$(", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u00bbso angenehmer nur seh ich zwei Sterne funkeln \u2013\u00ab", "tokens": ["\u00bb", "so", "an\u00b7ge\u00b7neh\u00b7mer", "nur", "seh", "ich", "zwei", "Ster\u00b7ne", "fun\u00b7keln", "\u2013", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADJD", "ADV", "VVFIN", "PPER", "CARD", "NN", "VVINF", "$(", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "\u00bbverwegner Kavalier!\u00ab \u2013 \u00bbHa, Chloe, darf ich hoffen? \u2013\u00ab", "tokens": ["\u00bb", "ver\u00b7weg\u00b7ner", "Ka\u00b7va\u00b7lier", "!", "\u00ab", "\u2013", "\u00bb", "Ha", ",", "Chloe", ",", "darf", "ich", "hof\u00b7fen", "?", "\u2013", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "$.", "$(", "$(", "$(", "ITJ", "$,", "NE", "$,", "VMFIN", "PPER", "VVINF", "$.", "$(", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Da schie\u00dft Cupido los und hat sie gut getroffen.", "tokens": ["Da", "schie\u00dft", "Cu\u00b7pi\u00b7do", "los", "und", "hat", "sie", "gut", "ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "PTKVZ", "KON", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}