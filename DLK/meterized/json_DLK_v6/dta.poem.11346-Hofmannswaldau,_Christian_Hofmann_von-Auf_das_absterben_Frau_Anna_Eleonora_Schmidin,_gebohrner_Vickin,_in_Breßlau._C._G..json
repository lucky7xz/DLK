bic.hexa"}, "line.64": {"text": "Auf den gecr\u00f6nten sarg! Macht der vergn\u00fcgung platz!", "tokens": ["Auf", "den", "ge\u00b7cr\u00f6n\u00b7ten", "sarg", "!", "Macht", "der", "ver\u00b7gn\u00fc\u00b7gung", "platz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.65": {"text": "Trennt die vereinigten durch kein vergeblich gr\u00e4men,", "tokens": ["Trennt", "die", "ver\u00b7ei\u00b7nig\u00b7ten", "durch", "kein", "ver\u00b7geb\u00b7lich", "gr\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "APPR", "PIAT", "ADJD", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.66": {"text": "G\u00f6nnt ihnen ihre ruh und sanffte sicherheit!", "tokens": ["G\u00f6nnt", "ih\u00b7nen", "ih\u00b7re", "ruh", "und", "sanff\u00b7te", "si\u00b7cher\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Sonst d\u00f6rfften euch so Griech- als R\u00f6mer bald besch\u00e4men,", "tokens": ["Sonst", "d\u00f6rff\u00b7ten", "euch", "so", "Griech", "als", "R\u00f6\u00b7mer", "bald", "be\u00b7sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "TRUNC", "KON", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Die bey dergleichen fall sich sonderbar erfreut.", "tokens": ["Die", "bey", "derg\u00b7lei\u00b7chen", "fall", "sich", "son\u00b7der\u00b7bar", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "