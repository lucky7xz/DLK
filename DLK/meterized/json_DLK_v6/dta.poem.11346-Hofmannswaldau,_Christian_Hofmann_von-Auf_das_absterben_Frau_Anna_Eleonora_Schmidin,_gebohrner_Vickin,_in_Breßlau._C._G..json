{"dta.poem.11346": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf das absterben Frau Anna Eleonora  \n Schmidin, gebohrner Vickin,  \n in Bre\u00dflau.  \n C. G.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was kan die liebe nicht vor wunder-dinge ze\u00fcgen?", "tokens": ["Was", "kan", "die", "lie\u00b7be", "nicht", "vor", "wun\u00b7der\u00b7din\u00b7ge", "ze\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "VVFIN", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jhr sonnen-gleicher glantz dringt durch des todes nacht.", "tokens": ["Ihr", "son\u00b7nen\u00b7glei\u00b7cher", "glantz", "dringt", "durch", "des", "to\u00b7des", "nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie ist durch keinen sturm, wie sehr er tobt, zu beugen,", "tokens": ["Sie", "ist", "durch", "kei\u00b7nen", "sturm", ",", "wie", "sehr", "er", "tobt", ",", "zu", "beu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "$,", "PWAV", "ADV", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil sie, wie Argus selbst, bey einem grabe wacht.", "tokens": ["Weil", "sie", ",", "wie", "Ar\u00b7gus", "selbst", ",", "bey", "ei\u00b7nem", "gra\u00b7be", "wacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "NE", "ADV", "$,", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kommt, die ihr zweifeln m\u00f6gt! Erweget, was ich schreibe!", "tokens": ["Kommt", ",", "die", "ihr", "zwei\u00b7feln", "m\u00f6gt", "!", "Er\u00b7we\u00b7get", ",", "was", "ich", "schrei\u00b7be", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$.", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Seht unsrer Schmidin grufft nicht ohn entsetzen an!", "tokens": ["Seht", "uns\u00b7rer", "Schmi\u00b7din", "grufft", "nicht", "ohn", "ent\u00b7set\u00b7zen", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVFIN", "PTKNEG", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und sprecht: O ebenbild von einem treuen weibe!", "tokens": ["Und", "sprecht", ":", "O", "e\u00b7ben\u00b7bild", "von", "ei\u00b7nem", "treu\u00b7en", "wei\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NE", "ADV", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Di\u00df hat der heisse trieb der keuschen gluth gethan.", "tokens": ["Di\u00df", "hat", "der", "heis\u00b7se", "trieb", "der", "keu\u00b7schen", "gluth", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Man mache nur nicht mehr ein sonderbares wesen,", "tokens": ["Man", "ma\u00b7che", "nur", "nicht", "mehr", "ein", "son\u00b7der\u00b7ba\u00b7res", "we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wann sich in Indien ein frauenbild verbrennt.", "tokens": ["Wann", "sich", "in", "In\u00b7di\u00b7en", "ein", "frau\u00b7en\u00b7bild", "ver\u00b7brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPR", "NE", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was wir in Thevenots und andrer reisen lesen,", "tokens": ["Was", "wir", "in", "The\u00b7ve\u00b7nots", "und", "an\u00b7drer", "rei\u00b7sen", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NE", "KON", "PIS", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wird billich eine frucht der raserey genennt;", "tokens": ["Wird", "bil\u00b7lich", "ei\u00b7ne", "frucht", "der", "ra\u00b7se\u00b7rey", "ge\u00b7nennt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wenn sie ein toller brauch nicht auf den holtz-sto\u00df tr\u00fcge,", "tokens": ["Wenn", "sie", "ein", "tol\u00b7ler", "brauch", "nicht", "auf", "den", "holtz\u00b7sto\u00df", "tr\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So w\u00fcrde man gewi\u00df ein klares beyspiel schaun,", "tokens": ["So", "w\u00fcr\u00b7de", "man", "ge\u00b7wi\u00df", "ein", "kla\u00b7res", "bey\u00b7spiel", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wie sie der m\u00e4nner tod zum \u00f6fftersten vergn\u00fcge,", "tokens": ["Wie", "sie", "der", "m\u00e4n\u00b7ner", "tod", "zum", "\u00f6ff\u00b7ters\u00b7ten", "ver\u00b7gn\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und wie der M\u00f6hrin pflicht gar wenig zuzutraun.", "tokens": ["Und", "wie", "der", "M\u00f6h\u00b7rin", "pflicht", "gar", "we\u00b7nig", "zu\u00b7zu\u00b7traun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "ADV", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Was ein Euripides von der Alceste tichtet,", "tokens": ["Was", "ein", "Eu\u00b7ri\u00b7pi\u00b7des", "von", "der", "Al\u00b7ces\u00b7te", "tich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Das wird als fabel-werck ver\u00e4chtlich angesehn:", "tokens": ["Das", "wird", "als", "fa\u00b7bel\u00b7\u00b7werck", "ver\u00b7\u00e4cht\u00b7lich", "an\u00b7ge\u00b7sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "KOKOM", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und was die Porcia, wie Brutus fiel, verrichtet,", "tokens": ["Und", "was", "die", "Por\u00b7cia", ",", "wie", "Bru\u00b7tus", "fiel", ",", "ver\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWS", "ART", "NE", "$,", "PWAV", "NE", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Ist aus verzweifelung, nicht wahrer treu, geschehn.", "tokens": ["Ist", "aus", "ver\u00b7zwei\u00b7fe\u00b7lung", ",", "nicht", "wah\u00b7rer", "treu", ",", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "$,", "PTKNEG", "ADJD", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Hier aber finden wir ein sehnliches verlangen,", "tokens": ["Hier", "a\u00b7ber", "fin\u00b7den", "wir", "ein", "sehn\u00b7li\u00b7ches", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Das ein getrenntes hertz nach seiner helffte tr\u00e4gt,", "tokens": ["Das", "ein", "ge\u00b7trenn\u00b7tes", "hertz", "nach", "sei\u00b7ner", "helff\u00b7te", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und eine traurigkeit, die sonder eitles prangen", "tokens": ["Und", "ei\u00b7ne", "trau\u00b7rig\u00b7keit", ",", "die", "son\u00b7der", "eit\u00b7les", "pran\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Sich mit dem ehgemahl auf eine baare legt.", "tokens": ["Sich", "mit", "dem", "eh\u00b7ge\u00b7mahl", "auf", "ei\u00b7ne", "baa\u00b7re", "legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Mag doch Sulpicia den Lentulus begleiten:", "tokens": ["Mag", "doch", "Sul\u00b7pi\u00b7cia", "den", "Len\u00b7tu\u00b7lus", "be\u00b7glei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NE", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.26": {"text": "Sperr\u2019 Epponine sich mit dem Sabinus ein;", "tokens": ["Sperr'", "Ep\u00b7po\u00b7ni\u00b7ne", "sich", "mit", "dem", "Sa\u00b7bi\u00b7nus", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PRF", "APPR", "ART", "NE", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Du solst, o seelige! bey diesen letzten zeiten", "tokens": ["Du", "solst", ",", "o", "see\u00b7li\u00b7ge", "!", "bey", "die\u00b7sen", "letz\u00b7ten", "zei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "FM", "FM", "$.", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Ein muster treuer pflicht und keuscher liebe seyn.", "tokens": ["Ein", "mus\u00b7ter", "treu\u00b7er", "pflicht", "und", "keu\u00b7scher", "lie\u00b7be", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "KON", "ADJA", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Dein eh-schatz, welchen du bey leben hochgehalten,", "tokens": ["Dein", "eh\u00b7schatz", ",", "wel\u00b7chen", "du", "bey", "le\u00b7ben", "hoch\u00b7ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAT", "PPER", "APPR", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der dich hinwiederum mehr, als sich selbst, geliebt,", "tokens": ["Der", "dich", "hin\u00b7wie\u00b7de\u00b7rum", "mehr", ",", "als", "sich", "selbst", ",", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "$,", "KOUS", "PRF", "ADV", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und der dich, was man sonst an unterschiednen alten", "tokens": ["Und", "der", "dich", ",", "was", "man", "sonst", "an", "un\u00b7ter\u00b7schied\u00b7nen", "al\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PPER", "$,", "PRELS", "PIS", "ADV", "APPR", "ADJA", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Als etwas fremdes lobt, mit willen nie betr\u00fcbt:", "tokens": ["Als", "et\u00b7was", "frem\u00b7des", "lobt", ",", "mit", "wil\u00b7len", "nie", "be\u00b7tr\u00fcbt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "VVFIN", "$,", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Dein eh-schatz mu\u00df sich itzt in jener welt erfreuen,", "tokens": ["Dein", "eh\u00b7schatz", "mu\u00df", "sich", "itzt", "in", "je\u00b7ner", "welt", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PRF", "ADV", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wenn er dich so geschwind an seiner seite sieht;", "tokens": ["Wenn", "er", "dich", "so", "ge\u00b7schwind", "an", "sei\u00b7ner", "sei\u00b7te", "sieht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und wenn ihr beyderseits, als wie die sch\u00f6nen m\u00e4yen,", "tokens": ["Und", "wenn", "ihr", "bey\u00b7der\u00b7seits", ",", "als", "wie", "die", "sch\u00f6\u00b7nen", "m\u00e4\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "$,", "KOUS", "KOKOM", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.36": {"text": "Jm pfingst-fest vor dem thron des Allerh\u00f6chsten bl\u00fcht.", "tokens": ["Jm", "pfingst\u00b7fest", "vor", "dem", "thron", "des", "Al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Jhm gieng, als er nichts mehr nach unserm thun gefraget,", "tokens": ["Jhm", "gieng", ",", "als", "er", "nichts", "mehr", "nach", "un\u00b7serm", "thun", "ge\u00b7fra\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PIS", "ADV", "APPR", "PPOSAT", "VVINF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Doch deine k\u00fcmmerni\u00df und dein betr\u00fcbni\u00df nah.", "tokens": ["Doch", "dei\u00b7ne", "k\u00fcm\u00b7mer\u00b7ni\u00df", "und", "dein", "be\u00b7tr\u00fcb\u00b7ni\u00df", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Er wuste, wie man hier die frommen witben plaget:", "tokens": ["Er", "wus\u00b7te", ",", "wie", "man", "hier", "die", "from\u00b7men", "wit\u00b7ben", "pla\u00b7get", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PIS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wenn manche seufftzt und \u00e4chtzt, so ist kein helffer da.", "tokens": ["Wenn", "man\u00b7che", "seufftzt", "und", "\u00e4chtzt", ",", "so", "ist", "kein", "helf\u00b7fer", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "KON", "ADJD", "$,", "ADV", "VAFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Elias kan nicht stets das \u00f6l im kruge f\u00fcllen,", "tokens": ["E\u00b7lias", "kan", "nicht", "stets", "das", "\u00f6l", "im", "kru\u00b7ge", "f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PTKNEG", "ADV", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.42": {"text": "Und das propheten-weib mist den Elisa sehr;", "tokens": ["Und", "das", "pro\u00b7phe\u00b7ten\u00b7weib", "mist", "den", "E\u00b7li\u00b7sa", "sehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NE", "ADV", "$."], "meter": "+--+-++--+-+", "measure": "iambic.hexa.invert"}, "line.43": {"text": "Wann GOttes h\u00fclffe fehlt, so geht es nicht nach willen:", "tokens": ["Wann", "Got\u00b7tes", "h\u00fclf\u00b7fe", "fehlt", ",", "so", "geht", "es", "nicht", "nach", "wil\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Man giebt den thr\u00e4nenden gar selten ein geh\u00f6r.", "tokens": ["Man", "giebt", "den", "thr\u00e4\u00b7nen\u00b7den", "gar", "sel\u00b7ten", "ein", "ge\u00b7h\u00f6r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Drum stieg ihm deine qual, dein jammer-stand zu hertzen;", "tokens": ["Drum", "stieg", "ihm", "dei\u00b7ne", "qual", ",", "dein", "jam\u00b7mer\u00b7stand", "zu", "hert\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Doch stellt er alles GOtt, der waysen vater, heim:", "tokens": ["Doch", "stellt", "er", "al\u00b7les", "Gott", ",", "der", "way\u00b7sen", "va\u00b7ter", ",", "heim", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "$,", "ART", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Und schau, wie wenden sich die \u00fcberh\u00e4ufften schmertzen,", "tokens": ["Und", "schau", ",", "wie", "wen\u00b7den", "sich", "die", "\u00fc\u00b7berh\u00b7\u00e4uff\u00b7ten", "schmert\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "$,", "PWAV", "PIS", "PRF", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Und wie empfindest du den s\u00fcssen honigseim!", "tokens": ["Und", "wie", "emp\u00b7fin\u00b7dest", "du", "den", "s\u00fcs\u00b7sen", "ho\u00b7ni\u00b7gseim", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Ich kan dich itzt nicht mehr vor eine wittib sch\u00e4tzen,", "tokens": ["Ich", "kan", "dich", "itzt", "nicht", "mehr", "vor", "ei\u00b7ne", "wit\u00b7tib", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Denn JEsus hat dich selbst zu seiner braut erkiest.", "tokens": ["Denn", "Je\u00b7sus", "hat", "dich", "selbst"