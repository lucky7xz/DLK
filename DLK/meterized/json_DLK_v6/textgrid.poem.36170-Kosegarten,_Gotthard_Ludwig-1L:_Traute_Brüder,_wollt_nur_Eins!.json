{"textgrid.poem.36170": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Traute Br\u00fcder, wollt nur Eins!", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Traute Br\u00fcder, wollt nur Eins!", "tokens": ["Trau\u00b7te", "Br\u00fc\u00b7der", ",", "wollt", "nur", "Eins", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VMFIN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Minder noch frommt viel, denn Keins!", "tokens": ["Min\u00b7der", "noch", "frommt", "viel", ",", "denn", "Keins", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "ADV", "$,", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Viel zerstreut, verwirrt, erschlafft,", "tokens": ["Viel", "zer\u00b7streut", ",", "ver\u00b7wirrt", ",", "er\u00b7schlafft", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADJD", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00fcbt den Blick und l\u00e4hmt die Kraft.", "tokens": ["Tr\u00fcbt", "den", "Blick", "und", "l\u00e4hmt", "die", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eins ist Noth und Eins gen\u00fcgt!", "tokens": ["Eins", "ist", "Noth", "und", "Eins", "ge\u00b7n\u00fcgt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Eins bricht Bahn sich, Eins obsiegt!", "tokens": ["Eins", "bricht", "Bahn", "sich", ",", "Eins", "ob\u00b7siegt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "PRF", "$,", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieses: da\u00df das Recht besteh',", "tokens": ["Die\u00b7ses", ":", "da\u00df", "das", "Recht", "be\u00b7steh'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "$.", "KOUS", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob die Welt zu Grunde geh'!", "tokens": ["Ob", "die", "Welt", "zu", "Grun\u00b7de", "geh'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "M\u00e4nniglich ins Auge schau'n", "tokens": ["M\u00e4n\u00b7nig\u00b7lich", "ins", "Au\u00b7ge", "schau'n"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sonder Trutz und sonder Grau'n,", "tokens": ["Son\u00b7der", "Trutz", "und", "son\u00b7der", "Grau'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herzhaft auf dem Recht bestehn,", "tokens": ["Herz\u00b7haft", "auf", "dem", "Recht", "be\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr das Recht zum Richtplatz gehn;", "tokens": ["F\u00fcr", "das", "Recht", "zum", "Richt\u00b7platz", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Meister unsers Bodens seyn,", "tokens": ["Meis\u00b7ter", "un\u00b7sers", "Bo\u00b7dens", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der Weichsel bis zum Rhein,", "tokens": ["Von", "der", "Weich\u00b7sel", "bis", "zum", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "APPRART", "NE", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Von des Histreich fernstem Rand,", "tokens": ["Von", "des", "Hist\u00b7reich", "ferns\u00b7tem", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis zur heil'gen Ostsee Strand;", "tokens": ["Bis", "zur", "heil'\u00b7gen", "Ost\u00b7see", "Strand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Schirmen unser Eigenthum,", "tokens": ["Schir\u00b7men", "un\u00b7ser", "Ei\u00b7gen\u00b7thum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsern Leumund, unsern Ruhm,", "tokens": ["Un\u00b7sern", "Leu\u00b7mund", ",", "un\u00b7sern", "Ruhm", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unser Hab' und unser Gut,", "tokens": ["Un\u00b7ser", "Hab'", "und", "un\u00b7ser", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsern Schwei\u00df und unser Blut;", "tokens": ["Un\u00b7sern", "Schwei\u00df", "und", "un\u00b7ser", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Los des fremden Jochs, allein", "tokens": ["Los", "des", "frem\u00b7den", "Jochs", ",", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NE", "ART", "ADJA", "NN", "$,", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Pflichtig unsern F\u00fcrsten seyn:", "tokens": ["Pflich\u00b7tig", "un\u00b7sern", "F\u00fcrs\u00b7ten", "seyn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Niemands Herr und Niemands Knecht,", "tokens": ["Nie\u00b7mands", "Herr", "und", "Nie\u00b7mands", "Knecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NE", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das ist, traun, des Deutschen Recht.", "tokens": ["Das", "ist", ",", "traun", ",", "des", "Deut\u00b7schen", "Recht", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "VVINF", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht zu starr und nicht zu zart", "tokens": ["Nicht", "zu", "starr", "und", "nicht", "zu", "zart"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PTKA", "ADJD", "KON", "PTKNEG", "PTKA", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist des Deutschen Sinn und Art.", "tokens": ["Ist", "des", "Deut\u00b7schen", "Sinn", "und", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Da\u00df nun solches Recht besteh',", "tokens": ["Da\u00df", "nun", "sol\u00b7ches", "Recht", "be\u00b7steh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Solche Art nicht untergeh',", "tokens": ["Sol\u00b7che", "Art", "nicht", "un\u00b7ter\u00b7geh'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die\u00df gen\u00fcgt und die\u00df ist noth,", "tokens": ["Die\u00df", "ge\u00b7n\u00fcgt", "und", "die\u00df", "ist", "noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "PDS", "VAFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lebend noth und noth im Tod.", "tokens": ["Le\u00b7bend", "noth", "und", "noth", "im", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "So ihr die\u00df nur, die\u00df nur meint,", "tokens": ["So", "ihr", "die\u00df", "nur", ",", "die\u00df", "nur", "meint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PDS", "ADV", "$,", "PDS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Trotz dann, Br\u00fcder, Trotz dem Feind!", "tokens": ["Trotz", "dann", ",", "Br\u00fc\u00b7der", ",", "Trotz", "dem", "Feind", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unser Werk wird f\u00fcrder gehn:", "tokens": ["Un\u00b7ser", "Werk", "wird", "f\u00fcr\u00b7der", "gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Licht und Recht den Sieg bestehn!", "tokens": ["Licht", "und", "Recht", "den", "Sieg", "be\u00b7stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}