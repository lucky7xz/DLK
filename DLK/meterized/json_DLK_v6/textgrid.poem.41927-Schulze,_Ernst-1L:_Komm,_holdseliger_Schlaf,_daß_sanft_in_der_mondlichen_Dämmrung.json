{"textgrid.poem.41927": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Komm, holdseliger Schlaf, da\u00df sanft in der mondlichen D\u00e4mmrung", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm, holdseliger Schlaf, da\u00df sanft in der mondlichen D\u00e4mmrung", "tokens": ["Komm", ",", "hold\u00b7se\u00b7li\u00b7ger", "Schlaf", ",", "da\u00df", "sanft", "in", "der", "mond\u00b7li\u00b7chen", "D\u00e4mm\u00b7rung"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "KOUS", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+---+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit dem erm\u00fcdeten Blick schlummre das m\u00fcdere Herz!", "tokens": ["Mit", "dem", "er\u00b7m\u00fc\u00b7de\u00b7ten", "Blick", "schlumm\u00b7re", "das", "m\u00fc\u00b7de\u00b7re", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Du nur t\u00e4uschest allein die verwirrenden Sorgen der Liebe,", "tokens": ["Du", "nur", "t\u00e4u\u00b7schest", "al\u00b7lein", "die", "ver\u00b7wir\u00b7ren\u00b7den", "Sor\u00b7gen", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Und aus dem wachenden Traum weckst du zum Leben mich auf.", "tokens": ["Und", "aus", "dem", "wa\u00b7chen\u00b7den", "Traum", "weckst", "du", "zum", "Le\u00b7ben", "mich", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Denn l\u00e4ngst schweben um mich im verwandelten Tanze die Zeiten,", "tokens": ["Denn", "l\u00e4ngst", "schwe\u00b7ben", "um", "mich", "im", "ver\u00b7wan\u00b7del\u00b7ten", "Tan\u00b7ze", "die", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PRF", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Wo sich der Tag sonst hob, senkt sich die wolkige Nacht.", "tokens": ["Wo", "sich", "der", "Tag", "sonst", "hob", ",", "senkt", "sich", "die", "wol\u00b7ki\u00b7ge", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "ADV", "VVFIN", "$,", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Zweifel und Wahn und beth\u00f6rende Furcht und beth\u00f6rende Hoffnung", "tokens": ["Zwei\u00b7fel", "und", "Wahn", "und", "be\u00b7th\u00f6\u00b7ren\u00b7de", "Furcht", "und", "be\u00b7th\u00f6\u00b7ren\u00b7de", "Hoff\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Sendet der fr\u00fcheste Strahl in die verdunkelte Brust.", "tokens": ["Sen\u00b7det", "der", "fr\u00fc\u00b7hes\u00b7te", "Strahl", "in", "die", "ver\u00b7dun\u00b7kel\u00b7te", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Irrend treib' ich umher in dem eitelen Spiel der Gedanken,", "tokens": ["Ir\u00b7rend", "treib'", "ich", "um\u00b7her", "in", "dem", "ei\u00b7te\u00b7len", "Spiel", "der", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Und wie ein Schattengebild hasch' ich nach Schatten allein.", "tokens": ["Und", "wie", "ein", "Schat\u00b7ten\u00b7ge\u00b7bild", "hasch'", "ich", "nach", "Schat\u00b7ten", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "PPER", "APPR", "NN", "ADV", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Aber im friedlichen Schlaf tagt hell mir die selige Wahrheit;", "tokens": ["A\u00b7ber", "im", "fried\u00b7li\u00b7chen", "Schlaf", "tagt", "hell", "mir", "die", "se\u00b7li\u00b7ge", "Wahr\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN", "ADJD", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.12": {"text": "Was ich vom Leben gew\u00fcnscht, giebt mir der liebliche Traum,", "tokens": ["Was", "ich", "vom", "Le\u00b7ben", "ge\u00b7w\u00fcnscht", ",", "giebt", "mir", "der", "lieb\u00b7li\u00b7che", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVPP", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Und mich selber erkenn' ich und dich und die gn\u00e4digen G\u00f6tter;", "tokens": ["Und", "mich", "sel\u00b7ber", "er\u00b7kenn'", "ich", "und", "dich", "und", "die", "gn\u00e4\u00b7di\u00b7gen", "G\u00f6t\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "PPER", "KON", "PPER", "KON", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.14": {"text": "Darum nenn' ich den Traum Wachen, das Wachen nur Traum.", "tokens": ["Da\u00b7rum", "nenn'", "ich", "den", "Traum", "Wa\u00b7chen", ",", "das", "Wa\u00b7chen", "nur", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "NN", "$,", "ART", "NN", "ADV", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}}}}}