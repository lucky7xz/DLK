{"textgrid.poem.34562": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Deines Gartens armer Spatz", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deines Gartens armer Spatz", "tokens": ["Dei\u00b7nes", "Gar\u00b7tens", "ar\u00b7mer", "Spatz"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "z\u00e4hlet dir zum Ruhme", "tokens": ["z\u00e4h\u00b7let", "dir", "zum", "Ruh\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "jede ihm zur Winterzeit", "tokens": ["je\u00b7de", "ihm", "zur", "Win\u00b7ter\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "PPER", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "hingestreute Krume.", "tokens": ["hin\u00b7ge\u00b7streu\u00b7te", "Kru\u00b7me", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Und die Blumen unter", "tokens": ["Und", "die", "Blu\u00b7men", "un\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "sind des Glaubens m\u00e4chtig,", "tokens": ["sind", "des", "Glau\u00b7bens", "m\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "dass so Vieh- als Menschenvolk", "tokens": ["dass", "so", "Vieh", "als", "Men\u00b7schen\u00b7volk"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "TRUNC", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "v\u00f6llig niedertr\u00e4chtig.", "tokens": ["v\u00f6l\u00b7lig", "nie\u00b7der\u00b7tr\u00e4ch\u00b7tig", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Nur von dir erwarten sie", "tokens": ["Nur", "von", "dir", "er\u00b7war\u00b7ten", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "jede seltne G\u00fcte,", "tokens": ["je\u00b7de", "selt\u00b7ne", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "weil du deine Rose nicht", "tokens": ["weil", "du", "dei\u00b7ne", "Ro\u00b7se", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "brachtest, als sie bl\u00fchte.", "tokens": ["brach\u00b7test", ",", "als", "sie", "bl\u00fch\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}