{"textgrid.poem.49040": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "6. Klopstock", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dann, wann er, unter den s\u00e4uselnden Aesten des Fr\u00fchlings,", "tokens": ["Dann", ",", "wann", "er", ",", "un\u00b7ter", "den", "s\u00e4u\u00b7seln\u00b7den", "A\u00b7es\u00b7ten", "des", "Fr\u00fch\u00b7lings", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+---+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vom Himmel die Gottheit ruft,", "tokens": ["Vom", "Him\u00b7mel", "die", "Got\u00b7theit", "ruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Oder, mit unsterblichen Schwingen,", "tokens": ["O\u00b7der", ",", "mit", "uns\u00b7terb\u00b7li\u00b7chen", "Schwin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ueber die Sonn' ihr entgegenfliegt;", "tokens": ["Ue\u00b7ber", "die", "Sonn'", "ihr", "ent\u00b7ge\u00b7gen\u00b7fliegt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "M\u00f6chte dann ein einziger Blick,", "tokens": ["M\u00f6ch\u00b7te", "dann", "ein", "ein\u00b7zi\u00b7ger", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Den er der fallenden Erde nachwirft,", "tokens": ["Den", "er", "der", "fal\u00b7len\u00b7den", "Er\u00b7de", "nach\u00b7wirft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.7": {"text": "Meinem bewundernden Blicke begegnen!", "tokens": ["Mei\u00b7nem", "be\u00b7wun\u00b7dern\u00b7den", "Bli\u00b7cke", "be\u00b7geg\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Ach, dann w\u00e4r diese Thr\u00e4ne verkl\u00e4rt!", "tokens": ["Ach", ",", "dann", "w\u00e4r", "die\u00b7se", "Thr\u00e4\u00b7ne", "ver\u00b7kl\u00e4rt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VAFIN", "PDAT", "NN", "VVPP", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.9": {"text": "Dann m\u00fc\u00dfte das schattige Ungeheur Vergessenheit", "tokens": ["Dann", "m\u00fc\u00df\u00b7te", "das", "schat\u00b7ti\u00b7ge", "Un\u00b7ge\u00b7heur", "Ver\u00b7ges\u00b7sen\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Ewig tief", "tokens": ["E\u00b7wig", "tief"], "token_info": ["word", "word"], "pos": ["ADV", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Unter meinem Namen hinschweben!", "tokens": ["Un\u00b7ter", "mei\u00b7nem", "Na\u00b7men", "hin\u00b7schwe\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}