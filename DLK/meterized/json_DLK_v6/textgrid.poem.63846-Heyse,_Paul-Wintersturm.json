{"textgrid.poem.63846": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Wintersturm", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun braust's in den L\u00fcften, nun donnert der See,", "tokens": ["Nun", "braust's", "in", "den", "L\u00fcf\u00b7ten", ",", "nun", "don\u00b7nert", "der", "See", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Aus schwarzblauen Kl\u00fcften sch\u00e4umt's wild in die H\u00f6h'.", "tokens": ["Aus", "schwarz\u00b7blau\u00b7en", "Kl\u00fcf\u00b7ten", "sch\u00e4umt's", "wild", "in", "die", "H\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mit wiehernden Rossen kommt \u00fcber den Plan", "tokens": ["Mit", "wie\u00b7hern\u00b7den", "Ros\u00b7sen", "kommt", "\u00fc\u00b7ber", "den", "Plan"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wei\u00dfm\u00e4hnig geschossen der Herrscher Orkan.", "tokens": ["Wei\u00df\u00b7m\u00e4h\u00b7nig", "ge\u00b7schos\u00b7sen", "der", "Herr\u00b7scher", "Or\u00b7kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "ART", "NN", "NE", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Es zittern die Mauern, die Scheiben erklirr'n,", "tokens": ["Es", "zit\u00b7tern", "die", "Mau\u00b7ern", ",", "die", "Schei\u00b7ben", "er\u00b7klirr'n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Palmen erschauern und beugen die Stirn.", "tokens": ["Die", "Pal\u00b7men", "er\u00b7schau\u00b7ern", "und", "beu\u00b7gen", "die", "Stirn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die hohen Oliven mit silbernem Laub,", "tokens": ["Die", "ho\u00b7hen", "O\u00b7li\u00b7ven", "mit", "sil\u00b7ber\u00b7nem", "Laub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Sie schwanken und triefen, dem Sturme zum Raub.", "tokens": ["Sie", "schwan\u00b7ken", "und", "trie\u00b7fen", ",", "dem", "Stur\u00b7me", "zum", "Raub", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Zu Nacht auf den Kissen bang horcht' ich hinaus,", "tokens": ["Zu", "Nacht", "auf", "den", "Kis\u00b7sen", "bang", "horcht'", "ich", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "ADJD", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem Schlummer entrissen vom w\u00fcsten Gebraus.", "tokens": ["Dem", "Schlum\u00b7mer", "ent\u00b7ris\u00b7sen", "vom", "w\u00fcs\u00b7ten", "Ge\u00b7braus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPRART", "VVFIN", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "O wehe, mein Garten, um dich ist's geschehn!", "tokens": ["O", "we\u00b7he", ",", "mein", "Gar\u00b7ten", ",", "um", "dich", "ist's", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "PPOSAT", "NN", "$,", "KOUI", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie wird es den zarten Jungpflanzen ergehn?", "tokens": ["Wie", "wird", "es", "den", "zar\u00b7ten", "Jung\u00b7pflan\u00b7zen", "er\u00b7gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Doch sieh, da die Sonne mich fr\u00fch schon erquickt,", "tokens": ["Doch", "sieh", ",", "da", "die", "Son\u00b7ne", "mich", "fr\u00fch", "schon", "er\u00b7quickt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+--+-++--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im Garten, o Wonne! kein Pfl\u00e4nzchen geknickt,", "tokens": ["Im", "Gar\u00b7ten", ",", "o", "Won\u00b7ne", "!", "kein", "Pfl\u00e4nz\u00b7chen", "ge\u00b7knickt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "FM", "NN", "$.", "PIAT", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Nur feucht noch die Bl\u00e4tter, nur schimmernd erfrischt", "tokens": ["Nur", "feucht", "noch", "die", "Bl\u00e4t\u00b7ter", ",", "nur", "schim\u00b7mernd", "er\u00b7frischt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "ART", "NN", "$,", "ADV", "ADJD", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Vom sausenden Wetter und spr\u00fchenden Gischt.", "tokens": ["Vom", "sau\u00b7sen\u00b7den", "Wet\u00b7ter", "und", "spr\u00fc\u00b7hen\u00b7den", "Gischt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}, "line.6": {"text": "Italiens Kinder, so lodert die Glut", "tokens": ["I\u00b7ta\u00b7li\u00b7ens", "Kin\u00b7der", ",", "so", "lo\u00b7dert", "die", "Glut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Des Zornes nicht minder euch j\u00e4hlings im Blut:", "tokens": ["Des", "Zor\u00b7nes", "nicht", "min\u00b7der", "euch", "j\u00e4h\u00b7lings", "im", "Blut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Ein w\u00fctend Gebr\u00fclle, morddroh'nden Gesichts,", "tokens": ["Ein", "w\u00fc\u00b7tend", "Ge\u00b7br\u00fcl\u00b7le", ",", "mord\u00b7droh'n\u00b7den", "Ge\u00b7sichts", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+---+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Dann pl\u00f6tzliche Stille \u2013 viel L\u00e4rmen um nichts!", "tokens": ["Dann", "pl\u00f6tz\u00b7li\u00b7che", "Stil\u00b7le", "\u2013", "viel", "L\u00e4r\u00b7men", "um", "nichts", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$(", "PIAT", "NN", "APPR", "PIS", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}