{"textgrid.poem.38079": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Weltlich Recht", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Joseph, lieber Joseph, was hast du gedacht,", "tokens": ["Jo\u00b7se\u00b7ph", ",", "lie\u00b7ber", "Jo\u00b7se\u00b7ph", ",", "was", "hast", "du", "ge\u00b7dacht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "NE", "$,", "PWS", "VAFIN", "PPER", "VVPP", "$,"], "meter": "---+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Da\u00df du die sch\u00f6ne Nanerl ins Ungl\u00fcck gebracht.", "tokens": ["Da\u00df", "du", "die", "sch\u00f6\u00b7ne", "Na\u00b7nerl", "ins", "Un\u00b7gl\u00fcck", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Joseph, lieber Joseph, mit mir ists bald aus,", "tokens": ["Jo\u00b7se\u00b7ph", ",", "lie\u00b7ber", "Jo\u00b7se\u00b7ph", ",", "mit", "mir", "ists", "bald", "aus", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "NE", "$,", "APPR", "PPER", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "+--+-+----+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und wird mich bald f\u00fchren zu dem Schandthor hinaus.", "tokens": ["Und", "wird", "mich", "bald", "f\u00fch\u00b7ren", "zu", "dem", "Schand\u00b7thor", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVINF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Zu dem Schandthor hinaus, auf einen gr\u00fcnen Platz,", "tokens": ["Zu", "dem", "Schand\u00b7thor", "hin\u00b7aus", ",", "auf", "ei\u00b7nen", "gr\u00fc\u00b7nen", "Platz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APZR", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--++-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Da wirst du bald sehen, was die Lieb hat gemacht.", "tokens": ["Da", "wirst", "du", "bald", "se\u00b7hen", ",", "was", "die", "Lieb", "hat", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVINF", "$,", "PRELS", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+--++--+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Richter, lieber Richter, richt nur fein geschwind,", "tokens": ["Rich\u00b7ter", ",", "lie\u00b7ber", "Rich\u00b7ter", ",", "richt", "nur", "fein", "ge\u00b7schwind", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "NN", "$,", "VVFIN", "ADV", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Ich will ja gern sterben, da\u00df ich komm zu meinem Kind.", "tokens": ["Ich", "will", "ja", "gern", "ster\u00b7ben", ",", "da\u00df", "ich", "komm", "zu", "mei\u00b7nem", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+---+-+-+-+-+", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Joseph, lieber Joseph, reich mir deine Hand,", "tokens": ["Jo\u00b7se\u00b7ph", ",", "lie\u00b7ber", "Jo\u00b7se\u00b7ph", ",", "reich", "mir", "dei\u00b7ne", "Hand", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "NE", "$,", "ADJD", "PPER", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Ich will dir verzeihen, das ist Gott wohl bekannt.", "tokens": ["Ich", "will", "dir", "ver\u00b7zei\u00b7hen", ",", "das", "ist", "Gott", "wohl", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,", "PDS", "VAFIN", "NN", "ADV", "ADJD", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Der F\u00e4hndrich kam geritten und schwenket seine Fahn,", "tokens": ["Der", "F\u00e4hn\u00b7drich", "kam", "ge\u00b7rit\u00b7ten", "und", "schwen\u00b7ket", "sei\u00b7ne", "Fahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Halt still mit der sch\u00f6nen Nanerl, ich bringe Pardon.", "tokens": ["Halt", "still", "mit", "der", "sch\u00f6\u00b7nen", "Na\u00b7nerl", ",", "ich", "brin\u00b7ge", "Par\u00b7don", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "APPR", "ART", "ADJA", "NE", "$,", "PPER", "VVFIN", "NN", "$."], "meter": "-+--+--+-+--+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "F\u00e4hndrich, lieber F\u00e4hndrich, sie ist ja schon todt:", "tokens": ["F\u00e4hn\u00b7drich", ",", "lie\u00b7ber", "F\u00e4hn\u00b7drich", ",", "sie", "ist", "ja", "schon", "todt", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "NN", "$,", "PPER", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Gut Nacht, meine sch\u00f6ne Nanerl, deine Seel ist bei Gott.", "tokens": ["Gut", "Nacht", ",", "mei\u00b7ne", "sch\u00f6\u00b7ne", "Na\u00b7nerl", ",", "dei\u00b7ne", "Seel", "ist", "bei", "Gott", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "-+--+-+-+-+--+", "measure": "iambic.hexa.relaxed"}}}}}