{"dta.poem.7483": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XxXIX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O heiss' mich nicht von Deinem Antlitz fliehn,", "tokens": ["O", "heiss'", "mich", "nicht", "von", "Dei\u00b7nem", "Ant\u00b7litz", "fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf dem der Liebe heilige Gedanken", "tokens": ["Auf", "dem", "der", "Lie\u00b7be", "hei\u00b7li\u00b7ge", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gleich goldnen Sternen auf und nieder schwanken,", "tokens": ["Gleich", "gold\u00b7nen", "Ster\u00b7nen", "auf", "und", "nie\u00b7der", "schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKVZ", "KON", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die still und furchenlos am Himmel ziehn!", "tokens": ["Die", "still", "und", "fur\u00b7chen\u00b7los", "am", "Him\u00b7mel", "ziehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hier ist mein Tempel und hier will ich knien,", "tokens": ["Hier", "ist", "mein", "Tem\u00b7pel", "und", "hier", "will", "ich", "kni\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "KON", "ADV", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um diesen Altar meine Arme ranken,", "tokens": ["Um", "die\u00b7sen", "Al\u00b7tar", "mei\u00b7ne", "Ar\u00b7me", "ran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+++-+-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "In diesen Armen meinen G\u00f6ttern danken,", "tokens": ["In", "die\u00b7sen", "Ar\u00b7men", "mei\u00b7nen", "G\u00f6t\u00b7tern", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df sie mir ihre Seligkeit verliehn!", "tokens": ["Da\u00df", "sie", "mir", "ih\u00b7re", "Se\u00b7lig\u00b7keit", "ver\u00b7liehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Bist Du, mein Herz, selbst wider dich im Bunde?", "tokens": ["Bist", "Du", ",", "mein", "Herz", ",", "selbst", "wi\u00b7der", "dich", "im", "Bun\u00b7de", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was soll der volle sch\u00e4umende Pokal,", "tokens": ["Was", "soll", "der", "vol\u00b7le", "sch\u00e4u\u00b7men\u00b7de", "Po\u00b7kal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was die Unendlichkeit dem Mann der Stunde?", "tokens": ["Was", "die", "Un\u00b7end\u00b7lich\u00b7keit", "dem", "Mann", "der", "Stun\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Begehre nicht die Herrlichkeit zumal!", "tokens": ["Be\u00b7geh\u00b7re", "nicht", "die", "Herr\u00b7lich\u00b7keit", "zu\u00b7mal", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bitt' um Ein Wort nur aus dem lieben Munde,", "tokens": ["Bitt'", "um", "Ein", "Wort", "nur", "aus", "dem", "lie\u00b7ben", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein halbes L\u00e4cheln, Einen Sonnenstrahl!", "tokens": ["Ein", "hal\u00b7bes", "L\u00e4\u00b7cheln", ",", "Ei\u00b7nen", "Son\u00b7nen\u00b7strahl", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}