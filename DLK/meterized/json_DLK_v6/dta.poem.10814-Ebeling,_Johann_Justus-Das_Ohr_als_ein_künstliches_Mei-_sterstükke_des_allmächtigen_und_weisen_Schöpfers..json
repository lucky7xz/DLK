{"dta.poem.10814": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Das  \n Ohr als ein k\u00fcnstliches Mei-  \n sterst\u00fckke des allm\u00e4chtigen und  \n weisen Sch\u00f6pfers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott man wird an denen Sinnen,", "tokens": ["Gott", "man", "wird", "an", "de\u00b7nen", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "VAFIN", "APPR", "PRELS", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Ueberzeugend sichtbar innen,", "tokens": ["Ue\u00b7ber\u00b7zeu\u00b7gend", "sicht\u00b7bar", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df du seist ein HErr von Macht,", "tokens": ["Da\u00df", "du", "seist", "ein", "Herr", "von", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der die wunderbahren R\u00f6hren,", "tokens": ["Der", "die", "wun\u00b7der\u00b7bah\u00b7ren", "R\u00f6h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dadurch wir die Stimmen h\u00f6ren,", "tokens": ["Da\u00b7durch", "wir", "die", "Stim\u00b7men", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "K\u00fcnstlich hat herf\u00fcrgebracht;", "tokens": ["K\u00fcnst\u00b7lich", "hat", "her\u00b7f\u00fcr\u00b7ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Es stellt uns auch unser Ohr", "tokens": ["Es", "stellt", "uns", "auch", "un\u00b7ser", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Deine weise Allmacht vor.", "tokens": ["Dei\u00b7ne", "wei\u00b7se", "All\u00b7macht", "vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}