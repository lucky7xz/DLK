{"dta.poem.19392": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "125.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sei wahr zu jeder Zeit, wahr in der Gegenwart,", "tokens": ["Sei", "wahr", "zu", "je\u00b7der", "Zeit", ",", "wahr", "in", "der", "Ge\u00b7gen\u00b7wart", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "APPR", "PIAT", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr die Vergangenheit, und auf die k\u00fcnft'ge Fahrt.", "tokens": ["F\u00fcr", "die", "Ver\u00b7gan\u00b7gen\u00b7heit", ",", "und", "auf", "die", "k\u00fcnft'\u00b7ge", "Fahrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wahr in der Gegenwart, so wie du bist, dich zeigend;", "tokens": ["Wahr", "in", "der", "Ge\u00b7gen\u00b7wart", ",", "so", "wie", "du", "bist", ",", "dich", "zei\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "ADV", "KOKOM", "PPER", "VAFIN", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wahr f\u00fcr Vergangenheit, Gethanes nicht verschweigend;", "tokens": ["Wahr", "f\u00fcr", "Ver\u00b7gan\u00b7gen\u00b7heit", ",", "Ge\u00b7tha\u00b7nes", "nicht", "ver\u00b7schwei\u00b7gend", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$,", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "In Zukunft wahr, bereit, was du versprichst, zu halten;", "tokens": ["In", "Zu\u00b7kunft", "wahr", ",", "be\u00b7reit", ",", "was", "du", "ver\u00b7sprichst", ",", "zu", "hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "ADJD", "$,", "PWS", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So bist du wahrhaft wahr in allen Zeitgestalten.", "tokens": ["So", "bist", "du", "wahr\u00b7haft", "wahr", "in", "al\u00b7len", "Zeit\u00b7ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}