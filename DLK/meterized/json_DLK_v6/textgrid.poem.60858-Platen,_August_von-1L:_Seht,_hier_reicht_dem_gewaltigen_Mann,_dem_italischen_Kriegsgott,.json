{"textgrid.poem.60858": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seht, hier reicht dem gewaltigen Mann, dem italischen Kriegsgott,", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht, hier reicht dem gewaltigen Mann, dem italischen Kriegsgott,", "tokens": ["Seht", ",", "hier", "reicht", "dem", "ge\u00b7wal\u00b7ti\u00b7gen", "Mann", ",", "dem", "i\u00b7ta\u00b7li\u00b7schen", "Kriegs\u00b7gott", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Als holdselige Braut Bianca Visconti die Hand;", "tokens": ["Als", "hold\u00b7se\u00b7li\u00b7ge", "Braut", "Bi\u00b7an\u00b7ca", "Vis\u00b7con\u00b7ti", "die", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "NE", "NE", "ART", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Doch sie entspro\u00dfte dem Stamm blutsaugender Menschenverderber:", "tokens": ["Doch", "sie", "ent\u00b7spro\u00df\u00b7te", "dem", "Stamm", "blu\u00b7tsau\u00b7gen\u00b7der", "Men\u00b7schen\u00b7ver\u00b7der\u00b7ber", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Traun, es erblickte die Welt selten entsetzlichere!", "tokens": ["Traun", ",", "es", "er\u00b7blick\u00b7te", "die", "Welt", "sel\u00b7ten", "ent\u00b7setz\u00b7li\u00b7che\u00b7re", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+--+--++--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Ach, und die Sch\u00f6ne gebar dem Gemahl ein verruchtes Geschlecht nur,", "tokens": ["Ach", ",", "und", "die", "Sch\u00f6\u00b7ne", "ge\u00b7bar", "dem", "Ge\u00b7mahl", "ein", "ver\u00b7ruch\u00b7tes", "Ge\u00b7schlecht", "nur", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "ART", "NN", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Das nach Italien bald fremde Tyrannen berief!", "tokens": ["Das", "nach", "I\u00b7ta\u00b7li\u00b7en", "bald", "frem\u00b7de", "Ty\u00b7ran\u00b7nen", "be\u00b7rief", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NE", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "---+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}