{"textgrid.poem.42816": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Meine Tante", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Tante ist eine Blinde", "tokens": ["Mei\u00b7ne", "Tan\u00b7te", "ist", "ei\u00b7ne", "Blin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN"], "meter": "+-+-++-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Und obendrein geistesgest\u00f6rt,", "tokens": ["Und", "o\u00b7ben\u00b7drein", "geis\u00b7tes\u00b7ge\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Was ich doch noch r\u00fcstig empfinde,", "tokens": ["Was", "ich", "doch", "noch", "r\u00fcs\u00b7tig", "emp\u00b7fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Weil sie auf dem einen Ohr h\u00f6rt.", "tokens": ["Weil", "sie", "auf", "dem", "ei\u00b7nen", "Ohr", "h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr R\u00fcckgrat ist wie ein Henkel.", "tokens": ["Ihr", "R\u00fcck\u00b7grat", "ist", "wie", "ein", "Hen\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie geht deshalb etwas geb\u00fcckt.", "tokens": ["Sie", "geht", "des\u00b7halb", "et\u00b7was", "ge\u00b7b\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch hat sie am oberen Schenkel", "tokens": ["Doch", "hat", "sie", "am", "o\u00b7be\u00b7ren", "Schen\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ein Gr\u00fcbchen, das jeden entz\u00fcckt.", "tokens": ["Ein", "Gr\u00fcb\u00b7chen", ",", "das", "je\u00b7den", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Ein Gr\u00fcbchen, wie manch eine Haut hat,", "tokens": ["Ein", "Gr\u00fcb\u00b7chen", ",", "wie", "manch", "ei\u00b7ne", "Haut", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PIAT", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur zarter und doch wieder stark,", "tokens": ["Nur", "zar\u00b7ter", "und", "doch", "wie\u00b7der", "stark", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "KON", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df jeder, der es geschaut hat,", "tokens": ["Da\u00df", "je\u00b7der", ",", "der", "es", "ge\u00b7schaut", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Erfreut etwas zahlt. Meist drei Mark.", "tokens": ["Er\u00b7freut", "et\u00b7was", "zahlt", ".", "Meist", "drei", "Mark", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVFIN", "$.", "NN", "CARD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie hat Perioden mit \u00c4ther.", "tokens": ["Sie", "hat", "Pe\u00b7ri\u00b7o\u00b7den", "mit", "\u00c4\u00b7ther", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ich breche mitunter mit ihr", "tokens": ["Ich", "bre\u00b7che", "mi\u00b7tun\u00b7ter", "mit", "ihr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Beziehungen ab, die ich sp\u00e4ter", "tokens": ["Be\u00b7zie\u00b7hun\u00b7gen", "ab", ",", "die", "ich", "sp\u00e4\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "PRELS", "PPER", "ADJD"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Erneure bei angeblich Bier.", "tokens": ["Er\u00b7neu\u00b7re", "bei", "an\u00b7ge\u00b7blich", "Bier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJD", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Denn sie ist doch eine volle", "tokens": ["Denn", "sie", "ist", "doch", "ei\u00b7ne", "vol\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mimosengestalt, ein Genie,", "tokens": ["Mi\u00b7mo\u00b7sen\u00b7ge\u00b7stalt", ",", "ein", "Ge\u00b7nie", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und immer noch unter Kontrolle.", "tokens": ["Und", "im\u00b7mer", "noch", "un\u00b7ter", "Kont\u00b7rol\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ich garantiere f\u00fcr sie.", "tokens": ["Ich", "ga\u00b7ran\u00b7tie\u00b7re", "f\u00fcr", "sie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}