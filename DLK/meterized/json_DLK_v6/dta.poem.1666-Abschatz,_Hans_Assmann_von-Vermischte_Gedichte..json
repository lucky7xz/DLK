{"dta.poem.1666": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vermischte  \n Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Des Monden halb-erstorbnes Licht ", "tokens": ["Des", "Mon\u00b7den", "halb\u00b7er\u00b7storb\u00b7nes", "Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weist sein erbla\u00dftes Angesicht/", "tokens": ["Weist", "sein", "er\u00b7bla\u00df\u00b7tes", "An\u00b7ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch tr\u00fcber Wolcken finstre Decken.", "tokens": ["Durch", "tr\u00fc\u00b7ber", "Wol\u00b7cken", "finst\u00b7re", "De\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Schatten tunckel-brauner Nacht/", "tokens": ["Der", "Schat\u00b7ten", "tun\u00b7ckel\u00b7brau\u00b7ner", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Aug und Ohren irrend macht/", "tokens": ["Der", "Aug", "und", "Oh\u00b7ren", "ir\u00b7rend", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Umgiebt den Erdenkrei\u00df mit Schrecken.", "tokens": ["Um\u00b7giebt", "den", "Er\u00b7den\u00b7krei\u00df", "mit", "Schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du liegst/ mein Freund/ in sanffter Ruh/", "tokens": ["Du", "liegst", "/", "mein", "Freund", "/", "in", "sanff\u00b7ter", "Ruh", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPOSAT", "NN", "$(", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schleust die verliebten Augen zu/", "tokens": ["Schleust", "die", "ver\u00b7lieb\u00b7ten", "Au\u00b7gen", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PTKZU", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kanst/ was du wilt/ im Traum umfassen.", "tokens": ["Kanst", "/", "was", "du", "wilt", "/", "im", "Traum", "um\u00b7fas\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "$(", "PWS", "PPER", "VMFIN", "$(", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich mu\u00df zu ungewohnter Zeit/", "tokens": ["Ich", "mu\u00df", "zu", "un\u00b7ge\u00b7wohn\u00b7ter", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "An Leib und Geist voll M\u00fcdigkeit/", "tokens": ["An", "Leib", "und", "Geist", "voll", "M\u00fc\u00b7dig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das kaum erwarmte Lager lassen.", "tokens": ["Das", "kaum", "er\u00b7warm\u00b7te", "La\u00b7ger", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich mu\u00df durch alles Ungemach/", "tokens": ["Ich", "mu\u00df", "durch", "al\u00b7les", "Un\u00b7ge\u00b7mach", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Wind und Winter ziehen nach/", "tokens": ["Das", "Wind", "und", "Win\u00b7ter", "zie\u00b7hen", "nach", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In unbekandter Gegend reisen/", "tokens": ["In", "un\u00b7be\u00b7kand\u00b7ter", "Ge\u00b7gend", "rei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wohin mich durch ein schlimmes Land", "tokens": ["Wo\u00b7hin", "mich", "durch", "ein", "schlim\u00b7mes", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Welt-Beschreibers k\u00fchne Hand", "tokens": ["Des", "Welt\u00b7Be\u00b7schrei\u00b7bers", "k\u00fch\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und theur-gekauffte F\u00fchrer weisen.", "tokens": ["Und", "theur\u00b7ge\u00b7kauff\u00b7te", "F\u00fch\u00b7rer", "wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich \u00e4ndre t\u00e4glich Kost und Heerd/", "tokens": ["Ich", "\u00e4nd\u00b7re", "t\u00e4g\u00b7lich", "Kost", "und", "Heerd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vertausche Wohnung/ Bett und Pferd/", "tokens": ["Ver\u00b7tau\u00b7sche", "Woh\u00b7nung", "/", "Bett", "und", "Pferd", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nichts/ und doch alles ist mein eigen/", "tokens": ["Nichts", "/", "und", "doch", "al\u00b7les", "ist", "mein", "ei\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$(", "KON", "ADV", "PIS", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nichts ruhet an mir/ als der Mund/", "tokens": ["Nichts", "ru\u00b7het", "an", "mir", "/", "als", "der", "Mund", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPER", "$(", "KOUS", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem offt den gantzen Tag vergunt", "tokens": ["Dem", "offt", "den", "gant\u00b7zen", "Tag", "ver\u00b7gunt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In stiller Einsamkeit zu schweigen.", "tokens": ["In", "stil\u00b7ler", "Ein\u00b7sam\u00b7keit", "zu", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wo gehn wir izt mit gleichem Sinn", "tokens": ["Wo", "gehn", "wir", "izt", "mit", "glei\u00b7chem", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In zwey ungleiche Strassen hin/", "tokens": ["In", "zwey", "un\u00b7glei\u00b7che", "Stras\u00b7sen", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die ich und du so offt gemessen?", "tokens": ["Die", "ich", "und", "du", "so", "offt", "ge\u00b7mes\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KON", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo k\u00f6nnen wir izt unsrer Pein", "tokens": ["Wo", "k\u00f6n\u00b7nen", "wir", "izt", "uns\u00b7rer", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Lust geheime Zeugen seyn/", "tokens": ["Und", "Lust", "ge\u00b7hei\u00b7me", "Zeu\u00b7gen", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zusammen schlaffen/ trincken/ essen?", "tokens": ["Zu\u00b7sam\u00b7men", "schlaf\u00b7fen", "/", "trin\u00b7cken", "/", "es\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VVINF", "$(", "VVINF", "$(", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wer sieht uns/ wenn der Tag entsteht/", "tokens": ["Wer", "sieht", "uns", "/", "wenn", "der", "Tag", "ent\u00b7steht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$(", "KOUS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Ph\u00f6bus gegen Westen geht", "tokens": ["Und", "Ph\u00f6\u00b7bus", "ge\u00b7gen", "Wes\u00b7ten", "geht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NE", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um Leydens sch\u00f6ne Stadt spatzieren?", "tokens": ["Um", "Ley\u00b7dens", "sch\u00f6\u00b7ne", "Stadt", "spat\u00b7zie\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer sieht uns in Vertrauligkeit/", "tokens": ["Wer", "sieht", "uns", "in", "Ver\u00b7trau\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auff nahen Gr\u00e4ntzen steten Streit", "tokens": ["Auff", "na\u00b7hen", "Gr\u00e4nt\u00b7zen", "ste\u00b7ten", "Streit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und s\u00fcssen Krieg zusammen f\u00fchren?", "tokens": ["Und", "s\u00fcs\u00b7sen", "Krieg", "zu\u00b7sam\u00b7men", "f\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ach/ Hertze/ k\u00f6nnt ich bey dir seyn!", "tokens": ["Ach", "/", "Hert\u00b7ze", "/", "k\u00f6nnt", "ich", "bey", "dir", "seyn", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$(", "VVFIN", "$(", "VVFIN", "PPER", "APPR", "PPER", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch weil der Himmel nicht stimmt ein", "tokens": ["Doch", "weil", "der", "Him\u00b7mel", "nicht", "stimmt", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PTKNEG", "VVFIN", "ART"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Mit meinem W\u00fcnschen und Verlangen/", "tokens": ["Mit", "mei\u00b7nem", "W\u00fcn\u00b7schen", "und", "Ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So lebe wohl-begl\u00fcckt/ mein Kind/", "tokens": ["So", "le\u00b7be", "wohl\u00b7be\u00b7gl\u00fcckt", "/", "mein", "Kind", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$(", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bi\u00df mir die Vater-Erde g\u00fcnnt/", "tokens": ["Bi\u00df", "mir", "die", "Va\u00b7ter\u00b7Er\u00b7de", "g\u00fcnnt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dich mit Vergn\u00fcgen zu umfangen.", "tokens": ["Dich", "mit", "Ver\u00b7gn\u00fc\u00b7gen", "zu", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Indessen soll die Ewigkeit", "tokens": ["In\u00b7des\u00b7sen", "soll", "die", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In das Register grauer Zeit", "tokens": ["In", "das", "Re\u00b7gis\u00b7ter", "grau\u00b7er", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und an des Himmels W\u00e4nde schreiben/", "tokens": ["Und", "an", "des", "Him\u00b7mels", "W\u00e4n\u00b7de", "schrei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df dir und deiner Br\u00fcderschafft", "tokens": ["Da\u00df", "dir", "und", "dei\u00b7ner", "Br\u00fc\u00b7der\u00b7schafft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bi\u00df ihn der blasse Tod wegrafft/", "tokens": ["Bi\u00df", "ihn", "der", "blas\u00b7se", "Tod", "weg\u00b7rafft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Damon treu und hold wird bleiben.", "tokens": ["Dein", "Da\u00b7mon", "treu", "und", "hold", "wird", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "ADJD", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}