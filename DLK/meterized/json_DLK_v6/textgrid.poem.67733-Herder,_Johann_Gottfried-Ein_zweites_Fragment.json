{"textgrid.poem.67733": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Ein zweites Fragment", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich, wo man nichts denkt, nichts f\u00fchlt,", "tokens": ["Als", "ich", ",", "wo", "man", "nichts", "denkt", ",", "nichts", "f\u00fchlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "PIS", "PIS", "VVFIN", "$,", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Einst Ketten trug, durchnagt von Staub und Schwei\u00df und Thr\u00e4nen,", "tokens": ["Einst", "Ket\u00b7ten", "trug", ",", "durch\u00b7nagt", "von", "Staub", "und", "Schwei\u00df", "und", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$,", "VVFIN", "APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Seufzt' ich (denn ", "tokens": ["Seufzt'", "ich", "(", "denn"], "token_info": ["word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "$(", "KON"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Da kam Apoll, der Gott:", "tokens": ["Da", "kam", "A\u00b7poll", ",", "der", "Gott", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$,", "ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Die Fessel weg! mein Erdenblick", "tokens": ["Die", "Fes\u00b7sel", "weg", "!", "mein", "Er\u00b7den\u00b7blick"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ward hoch \u2013 er gab mir ", "tokens": ["Ward", "hoch", "\u2013", "er", "gab", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "$(", "PPER", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}}}}}