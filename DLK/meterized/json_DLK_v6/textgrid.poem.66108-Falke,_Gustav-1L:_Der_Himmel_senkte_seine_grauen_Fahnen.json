{"textgrid.poem.66108": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Himmel senkte seine grauen Fahnen", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel senkte seine grauen Fahnen", "tokens": ["Der", "Him\u00b7mel", "senk\u00b7te", "sei\u00b7ne", "grau\u00b7en", "Fah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Tief auf des Parks umflorte Sommerwipfel,", "tokens": ["Tief", "auf", "des", "Parks", "um\u00b7flor\u00b7te", "Som\u00b7mer\u00b7wip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und durch die stillen Schatteng\u00e4nge schwebten", "tokens": ["Und", "durch", "die", "stil\u00b7len", "Schat\u00b7ten\u00b7g\u00e4n\u00b7ge", "schweb\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Schwermut dunkle Falter leisen Fluges.", "tokens": ["Der", "Schwer\u00b7mut", "dunk\u00b7le", "Fal\u00b7ter", "lei\u00b7sen", "Flu\u00b7ges", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die hohen Ulmen weinten und die Birken,", "tokens": ["Die", "ho\u00b7hen", "Ul\u00b7men", "wein\u00b7ten", "und", "die", "Bir\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die ernsten Koniferen und die Rosen,", "tokens": ["Die", "erns\u00b7ten", "Ko\u00b7ni\u00b7fe\u00b7ren", "und", "die", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und durch den feuchten Schleier sah das Haus", "tokens": ["Und", "durch", "den", "feuch\u00b7ten", "Schlei\u00b7er", "sah", "das", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mit seinen dichtverh\u00e4ngten Fenstern, wie", "tokens": ["Mit", "sei\u00b7nen", "dicht\u00b7ver\u00b7h\u00e4ng\u00b7ten", "Fens\u00b7tern", ",", "wie"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "PWAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ein m\u00fcdes, bleiches Menschenangesicht,", "tokens": ["Ein", "m\u00fc\u00b7des", ",", "blei\u00b7ches", "Men\u00b7schen\u00b7an\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Dem Gram die hei\u00dfen kranken Lider schlo\u00df.", "tokens": ["Dem", "Gram", "die", "hei\u00b7\u00dfen", "kran\u00b7ken", "Li\u00b7der", "schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Des Gartens offnes Gitter lockte mich,", "tokens": ["Des", "Gar\u00b7tens", "off\u00b7nes", "Git\u00b7ter", "lock\u00b7te", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und ich trat ein. Die dunklen Ulmen leerten", "tokens": ["Und", "ich", "trat", "ein", ".", "Die", "dunk\u00b7len", "Ul\u00b7men", "leer\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "$.", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie fassungslos des Kummers Schalen aus,", "tokens": ["Wie", "fas\u00b7sungs\u00b7los", "des", "Kum\u00b7mers", "Scha\u00b7len", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und auf den Beeten weinten alle Blumen,", "tokens": ["Und", "auf", "den", "Bee\u00b7ten", "wein\u00b7ten", "al\u00b7le", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und von dem Rasen neigten sich die Gr\u00e4ser", "tokens": ["Und", "von", "dem", "Ra\u00b7sen", "neig\u00b7ten", "sich", "die", "Gr\u00e4\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Auf meinen Fu\u00df und netzten ihn mit Tr\u00e4nen.", "tokens": ["Auf", "mei\u00b7nen", "Fu\u00df", "und", "netz\u00b7ten", "ihn", "mit", "Tr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die erzgegossene Sphinx nur an der Treppe", "tokens": ["Die", "erz\u00b7ge\u00b7gos\u00b7se\u00b7ne", "Sphinx", "nur", "an", "der", "Trep\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sah kalt und unbewegt in diesen Jammer,", "tokens": ["Sah", "kalt", "und", "un\u00b7be\u00b7wegt", "in", "die\u00b7sen", "Jam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit gro\u00dfen, leeren Augen, da\u00df mir grauste.", "tokens": ["Mit", "gro\u00b7\u00dfen", ",", "lee\u00b7ren", "Au\u00b7gen", ",", "da\u00df", "mir", "graus\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,", "KOUS", "PPER", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und doch war \u00fcber ihren schwarzen Leib", "tokens": ["Und", "doch", "war", "\u00fc\u00b7ber", "ih\u00b7ren", "schwar\u00b7zen", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein ganzer Zweig voll schwerer gelber Rosen,", "tokens": ["Ein", "gan\u00b7zer", "Zweig", "voll", "schwe\u00b7rer", "gel\u00b7ber", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie aufgel\u00f6st in lauter Leid, gesunken", "tokens": ["Wie", "auf\u00b7ge\u00b7l\u00f6st", "in", "lau\u00b7ter", "Leid", ",", "ge\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "VVPP", "APPR", "PIAT", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und sch\u00fcttete der Schmerzen heiligen Tau", "tokens": ["Und", "sch\u00fct\u00b7te\u00b7te", "der", "Schmer\u00b7zen", "hei\u00b7li\u00b7gen", "Tau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Aus seinen goldenen Kelchen auf sie nieder.", "tokens": ["Aus", "sei\u00b7nen", "gol\u00b7de\u00b7nen", "Kel\u00b7chen", "auf", "sie", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Und aus der Villa trat ein d\u00fcrres M\u00e4nnchen,", "tokens": ["Und", "aus", "der", "Vil\u00b7la", "trat", "ein", "d\u00fcr\u00b7res", "M\u00e4nn\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein alter Herr mit einer Aktenmappe,", "tokens": ["Ein", "al\u00b7ter", "Herr", "mit", "ei\u00b7ner", "Ak\u00b7ten\u00b7map\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Mit Brille, Regenschirm und Florzylinder.", "tokens": ["Mit", "Bril\u00b7le", ",", "Re\u00b7gen\u00b7schirm", "und", "Flor\u00b7zy\u00b7lin\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er sah mich fragend an: Was suchst du hier?", "tokens": ["Er", "sah", "mich", "fra\u00b7gend", "an", ":", "Was", "suchst", "du", "hier", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und z\u00f6gernd kam es von den schmalen Lippen:", "tokens": ["Und", "z\u00f6\u00b7gernd", "kam", "es", "von", "den", "schma\u00b7len", "Lip\u00b7pen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie wissen doch? Die Poesie ist tot.", "tokens": ["Sie", "wis\u00b7sen", "doch", "?", "Die", "Poe\u00b7sie", "ist", "tot", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Wie Dolchstich traf das Wort, und ich erschrak.", "tokens": ["Wie", "Dolchs\u00b7tich", "traf", "das", "Wort", ",", "und", "ich", "er\u00b7schrak", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und wie ein Schluchzen ging es durch die B\u00e4ume,", "tokens": ["Und", "wie", "ein", "Schluch\u00b7zen", "ging", "es", "durch", "die", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Stieg aus den Wurzeln bis in alle Kronen.", "tokens": ["Stieg", "aus", "den", "Wur\u00b7zeln", "bis", "in", "al\u00b7le", "Kro\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "APPR", "PIAT", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Die Birken weinten und die hohen Ulmen,", "tokens": ["Die", "Bir\u00b7ken", "wein\u00b7ten", "und", "die", "ho\u00b7hen", "Ul\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Koniferen und die dunklen Rosen,", "tokens": ["Die", "Ko\u00b7ni\u00b7fe\u00b7ren", "und", "die", "dunk\u00b7len", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und wie ein Sch\u00fcttelfrost durchlief es j\u00e4h", "tokens": ["Und", "wie", "ein", "Sch\u00fct\u00b7tel\u00b7frost", "durch\u00b7lief", "es", "j\u00e4h"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Den gramgebeugten gelben Rosenstrauch,", "tokens": ["Den", "gram\u00b7ge\u00b7beug\u00b7ten", "gel\u00b7ben", "Ro\u00b7sen\u00b7strauch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der um den Hals der strengen, starren Sphinx", "tokens": ["Der", "um", "den", "Hals", "der", "stren\u00b7gen", ",", "star\u00b7ren", "Sphinx"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die schlanken Arme warf: F\u00fchlst du denn nichts?", "tokens": ["Die", "schlan\u00b7ken", "Ar\u00b7me", "warf", ":", "F\u00fchlst", "du", "denn", "nichts", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "VVFIN", "PPER", "ADV", "PIS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "F\u00fchlst du denn nichts? Die Poesie ist tot.", "tokens": ["F\u00fchlst", "du", "denn", "nichts", "?", "Die", "Poe\u00b7sie", "ist", "tot", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIS", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}