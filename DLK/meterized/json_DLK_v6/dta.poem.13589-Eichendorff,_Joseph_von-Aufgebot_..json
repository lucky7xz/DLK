{"dta.poem.13589": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Aufgebot .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Waldhorn bringt Kund' getragen,", "tokens": ["Wald\u00b7horn", "bringt", "Kund'", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "VVPP", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Es hab' nun aufgeschlagen", "tokens": ["Es", "hab'", "nun", "auf\u00b7ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf Berg und Thal und Feld", "tokens": ["Auf", "Berg", "und", "Thal", "und", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Lenz seine bunte Zelt'!", "tokens": ["Der", "Lenz", "sei\u00b7ne", "bun\u00b7te", "Zelt'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "In's Gr\u00fcn zieh'n S\u00e4nger, Reiter,", "tokens": ["In's", "Gr\u00fcn", "zieh'n", "S\u00e4n\u00b7ger", ",", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "$,", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein jeglich Herz wird weiter,", "tokens": ["Ein", "jeg\u00b7lich", "Herz", "wird", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "M\u00f6cht' jauchzend \u00fcber's Gr\u00fcn", "tokens": ["M\u00f6cht'", "jauch\u00b7zend", "\u00fc\u00b7ber's", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "VVPP", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit den Lerchen in's Blaue zieh'n.", "tokens": ["Mit", "den", "Ler\u00b7chen", "in's", "Blau\u00b7e", "zieh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "VVFIN", "NE"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Was stehst du so alleine,", "tokens": ["Was", "stehst", "du", "so", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Pilgrim, im gr\u00fcnen Scheine?", "tokens": ["Pil\u00b7grim", ",", "im", "gr\u00fc\u00b7nen", "Schei\u00b7ne", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Lockt dicht der Wunderlaut", "tokens": ["Lockt", "dicht", "der", "Wun\u00b7der\u00b7laut"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nicht auch zur fernen Braut?", "tokens": ["Nicht", "auch", "zur", "fer\u00b7nen", "Braut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u201each! diese tausendfachen", "tokens": ["\u201e", "ach", "!", "die\u00b7se", "tau\u00b7send\u00b7fa\u00b7chen"], "token_info": ["punct", "word", "punct", "word", "word"], "pos": ["$(", "ITJ", "$.", "PDAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Heilig verschlung'nen Sprachen", "tokens": ["Hei\u00b7lig", "ver\u00b7schlung'\u00b7nen", "Spra\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "So lockend Lust wie Schmerz", "tokens": ["So", "lo\u00b7ckend", "Lust", "wie", "Schmerz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "NN", "KOKOM", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zerrei\u00dfen mir das Herz.\u201c", "tokens": ["Zer\u00b7rei\u00b7\u00dfen", "mir", "das", "Herz", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u201eein Wort will mir's verk\u00fcnden,", "tokens": ["\u201e", "ein", "Wort", "will", "mir's", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VMFIN", "NE", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Oft ist's, als m\u00fc\u00dft' ich's finden,", "tokens": ["Oft", "ist's", ",", "als", "m\u00fc\u00dft'", "ich's", "fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOUS", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wieder ist's nicht so,", "tokens": ["Und", "wie\u00b7der", "ist's", "nicht", "so", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und ewig frag' ich: Wo?\u201c \u2014", "tokens": ["Und", "e\u00b7wig", "frag'", "ich", ":", "Wo", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$.", "PWAV", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "So st\u00fcrz' dich einmal, Geselle,", "tokens": ["So", "st\u00fcrz'", "dich", "ein\u00b7mal", ",", "Ge\u00b7sel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "NN", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Nur frisch in die Fr\u00fchlingswelle!", "tokens": ["Nur", "frisch", "in", "die", "Fr\u00fch\u00b7lings\u00b7wel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da sp\u00fcrst du's im Innersten gleich", "tokens": ["Da", "sp\u00fcrst", "du's", "im", "In\u00b7ners\u00b7ten", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo's rechte Himmelreich.", "tokens": ["Wo's", "rech\u00b7te", "Him\u00b7mel\u00b7reich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und wer dann noch mag fragen:", "tokens": ["Und", "wer", "dann", "noch", "mag", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Freudlos in blauen Tagen", "tokens": ["Freud\u00b7los", "in", "blau\u00b7en", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der wandern und fragen mag", "tokens": ["Der", "wan\u00b7dern", "und", "fra\u00b7gen", "mag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "VVINF", "VMFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bis an den j\u00fcngsten Tag!", "tokens": ["Bis", "an", "den", "j\u00fcng\u00b7sten", "Tag", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}