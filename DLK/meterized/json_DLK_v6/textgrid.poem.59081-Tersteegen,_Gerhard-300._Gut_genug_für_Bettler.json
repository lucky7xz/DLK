{"textgrid.poem.59081": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "300. Gut genug f\u00fcr Bettler", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum Schlechtsten mu\u00df dein Herz sich neigen,", "tokens": ["Zum", "Schlechts\u00b7ten", "mu\u00df", "dein", "Herz", "sich", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch nicht, 'was Sonderlich's zu zeigen;", "tokens": ["Doch", "nicht", ",", "'was", "Son\u00b7der\u00b7lich's", "zu", "zei\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "$,", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Mindste dir noch Gnade sei!", "tokens": ["Das", "Minds\u00b7te", "dir", "noch", "Gna\u00b7de", "sei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beim Kleinen bleibst du besser frei.", "tokens": ["Beim", "Klei\u00b7nen", "bleibst", "du", "bes\u00b7ser", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}