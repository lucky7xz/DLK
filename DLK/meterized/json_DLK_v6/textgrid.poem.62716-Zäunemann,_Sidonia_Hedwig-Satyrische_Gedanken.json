{"textgrid.poem.62716": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Satyrische Gedanken", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Helden z\u00fcrnet nicht! der g\u00fcldne Friede bl\u00fcht!", "tokens": ["Ihr", "Hel\u00b7den", "z\u00fcr\u00b7net", "nicht", "!", "der", "g\u00fcld\u00b7ne", "Frie\u00b7de", "bl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "$.", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Steckt Schwerd und Degen ein, und seyd vielmehr bem\u00fcht", "tokens": ["Steckt", "Schwerd", "und", "De\u00b7gen", "ein", ",", "und", "seyd", "viel\u00b7mehr", "be\u00b7m\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN", "PTKVZ", "$,", "KON", "VAFIN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nach Mavors kluger Art, die Venus zu bedienen!", "tokens": ["Nach", "Ma\u00b7vors", "klu\u00b7ger", "Art", ",", "die", "Ve\u00b7nus", "zu", "be\u00b7die\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Geht! opfert h\u00f6chtsvergn\u00fcgt den reitzungsvollen Mienen!", "tokens": ["Geht", "!", "op\u00b7fert", "h\u00f6chts\u00b7ver\u00b7gn\u00fcgt", "den", "reit\u00b7zungs\u00b7vol\u00b7len", "Mie\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVFIN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bellona hat bereits die ", "tokens": ["Bel\u00b7lo\u00b7na", "hat", "be\u00b7reits", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADV", "ART"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Seht! wie sich ", "tokens": ["Seht", "!", "wie", "sich"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVFIN", "$.", "PWAV", "PRF"], "meter": "+--", "measure": "dactylic.init"}, "line.7": {"text": "Hier k\u00f6nnt ihr ohne Furcht in s\u00fcsser Ruhe streiten,", "tokens": ["Hier", "k\u00f6nnt", "ihr", "oh\u00b7ne", "Furcht", "in", "s\u00fcs\u00b7ser", "Ru\u00b7he", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und sonder Schwei\u00df und Blut den besten Schatz erbeuten.", "tokens": ["Und", "son\u00b7der", "Schwei\u00df", "und", "Blut", "den", "bes\u00b7ten", "Schatz", "er\u00b7beu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Feind ", "tokens": ["Der", "Feind"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "Aus seinem ", "tokens": ["Aus", "sei\u00b7nem"], "token_info": ["word", "word"], "pos": ["APPR", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.12": {"text": "Und haben bey der Flucht ihr ", "tokens": ["Und", "ha\u00b7ben", "bey", "der", "Flucht", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Ist alles ", "tokens": ["Ist", "al\u00b7les"], "token_info": ["word", "word"], "pos": ["VAFIN", "PIS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Ja k\u00f6nte man es nicht aus jener Oefnung lesen,", "tokens": ["Ja", "k\u00f6n\u00b7te", "man", "es", "nicht", "aus", "je\u00b7ner", "Oef\u00b7nung", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VMFIN", "PIS", "PPER", "PTKNEG", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die bey dem ", "tokens": ["Die", "bey", "dem"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.16": {"text": "Mit Bomben ", "tokens": ["Mit", "Bom\u00b7ben"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "So w\u00fcrde niemand leicht an unsern Ausen Werken,", "tokens": ["So", "w\u00fcr\u00b7de", "nie\u00b7mand", "leicht", "an", "un\u00b7sern", "Au\u00b7sen", "Wer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ihr habt schon ", "tokens": ["Ihr", "habt", "schon"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Und seit dem ", "tokens": ["Und", "seit", "dem"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.20": {"text": "Den ganzen ", "tokens": ["Den", "gan\u00b7zen"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "So bald sich nur von fern die Feinde blicken liessen.", "tokens": ["So", "bald", "sich", "nur", "von", "fern", "die", "Fein\u00b7de", "bli\u00b7cken", "lies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PRF", "ADV", "APPR", "ADJD", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ihr haltet euch ge\u00fcbt; ihr giengt auf Hieb und Sto\u00df;", "tokens": ["Ihr", "hal\u00b7tet", "euch", "ge\u00b7\u00fcbt", ";", "ihr", "giengt", "auf", "Hieb", "und", "Sto\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "$.", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Kaum ward der Hahn ger\u00fcckt, so schlug die Flinte lo\u00df;", "tokens": ["Kaum", "ward", "der", "Hahn", "ge\u00b7r\u00fcckt", ",", "so", "schlug", "die", "Flin\u00b7te", "lo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wie manche Nachtigall verlie\u00df nicht Baum und", "tokens": ["Wie", "man\u00b7che", "Nach\u00b7ti\u00b7gall", "ver\u00b7lie\u00df", "nicht", "Baum", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "PTKNEG", "NE", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Und wolte weiter nicht in Busch und Hecken warten.", "tokens": ["Und", "wol\u00b7te", "wei\u00b7ter", "nicht", "in", "Busch", "und", "He\u00b7cken", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PTKNEG", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Begebt euch denn zur Ruh; ", "tokens": ["Be\u00b7gebt", "euch", "denn", "zur", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Der Winter, wie ihr seht, ist jetzo vor der Th\u00fcr;", "tokens": ["Der", "Win\u00b7ter", ",", "wie", "ihr", "seht", ",", "ist", "jet\u00b7zo", "vor", "der", "Th\u00fcr", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und solte ja der Feind vor ", "tokens": ["Und", "sol\u00b7te", "ja", "der", "Feind", "vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "ART", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "So wartet wenigstens und schont euch nur sechs Wochen.", "tokens": ["So", "war\u00b7tet", "we\u00b7nigs\u00b7tens", "und", "schont", "euch", "nur", "sechs", "Wo\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "KON", "VVFIN", "PPER", "ADV", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Erweget selbst bey euch: Wie manche sch\u00f6ne Nacht", "tokens": ["Er\u00b7we\u00b7get", "selbst", "bey", "euch", ":", "Wie", "man\u00b7che", "sch\u00f6\u00b7ne", "Nacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "$.", "PWAV", "PIAT", "ADJA", "NN"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.31": {"text": "Habt ihr in K\u00fcmmerni\u00df und Sorgen zugebracht.", "tokens": ["Habt", "ihr", "in", "K\u00fcm\u00b7mer\u00b7ni\u00df", "und", "Sor\u00b7gen", "zu\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Im ", "tokens": ["Im"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.33": {"text": "Und dachtet kaum darauf, die ", "tokens": ["Und", "dach\u00b7tet", "kaum", "da\u00b7rauf", ",", "die"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "ADV", "PAV", "$,", "PRELS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Ihr meintet ganz gewi\u00df, der ", "tokens": ["Ihr", "mein\u00b7tet", "ganz", "ge\u00b7wi\u00df", ",", "der"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "PRELS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Und vortheilhaft verwahrt; doch wer sich drauf verl\u00e4\u00dft,", "tokens": ["Und", "vor\u00b7theil\u00b7haft", "ver\u00b7wahrt", ";", "doch", "wer", "sich", "drauf", "ver\u00b7l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$.", "KON", "PWS", "PRF", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Dem geht es oft sehr schlecht. ", "tokens": ["Dem", "geht", "es", "oft", "sehr", "schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "Die Feinde liessen nicht an Bley und Pulver spahren;", "tokens": ["Die", "Fein\u00b7de", "lies\u00b7sen", "nicht", "an", "Bley", "und", "Pul\u00b7ver", "spah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die Festung wurde bald mit Feuer angef\u00fcllt;", "tokens": ["Die", "Fes\u00b7tung", "wur\u00b7de", "bald", "mit", "Feu\u00b7er", "an\u00b7ge\u00b7f\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Da nahm die Furcht erst zu, die aus dem Kummer quillt.", "tokens": ["Da", "nahm", "die", "Furcht", "erst", "zu", ",", "die", "aus", "dem", "Kum\u00b7mer", "quillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ihr woltet zwar den Feind durch ", "tokens": ["Ihr", "wol\u00b7tet", "zwar", "den", "Feind", "durch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.41": {"text": "Allein ihr kuntet nicht durch die Mor\u00e4ste dringen.", "tokens": ["Al\u00b7lein", "ihr", "kun\u00b7tet", "nicht", "durch", "die", "Mo\u00b7r\u00e4s\u00b7te", "drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Ihr fielt bis an den Leib in solchen Schlamm hinein,", "tokens": ["Ihr", "fielt", "bis", "an", "den", "Leib", "in", "sol\u00b7chen", "Schlamm", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Und w\u00fcrdet in der That bald gar ", "tokens": ["Und", "w\u00fcr\u00b7det", "in", "der", "That", "bald", "gar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Die Noth war schrecklich gro\u00df; ihr lie\u00dft die Schuhe stecken,", "tokens": ["Die", "Noth", "war", "schreck\u00b7lich", "gro\u00df", ";", "ihr", "lie\u00dft", "die", "Schu\u00b7he", "ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADJD", "$.", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Und eiltet nur geschwind, die F\u00fcsse zu bedecken.", "tokens": ["Und", "eil\u00b7tet", "nur", "ge\u00b7schwind", ",", "die", "F\u00fcs\u00b7se", "zu", "be\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "So gieng es damahls her; drum schont euch, da der Feind", "tokens": ["So", "gieng", "es", "da\u00b7mahls", "her", ";", "drum", "schont", "euch", ",", "da", "der", "Feind"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "PAV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Den Platz verlassen hat; ihr seyd ja, wie es scheint,", "tokens": ["Den", "Platz", "ver\u00b7las\u00b7sen", "hat", ";", "ihr", "seyd", "ja", ",", "wie", "es", "scheint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$.", "PPER", "VAFIN", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Nicht wenig ", "tokens": ["Nicht", "we\u00b7nig"], "token_info": ["word", "word"], "pos": ["PTKNEG", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.49": {"text": "Ihr d\u00fcrft euch eben nicht zum ", "tokens": ["Ihr", "d\u00fcrft", "euch", "e\u00b7ben", "nicht", "zum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "APPRART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.50": {"text": "Nein, dieses w\u00e4r zu viel; gebrauchet nur der Ruh,", "tokens": ["Nein", ",", "die\u00b7ses", "w\u00e4r", "zu", "viel", ";", "ge\u00b7brau\u00b7chet", "nur", "der", "Ruh", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VAFIN", "PTKA", "PIS", "$.", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Und bringet eure Zeit in