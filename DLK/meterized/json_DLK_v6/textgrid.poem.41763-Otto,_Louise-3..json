{"textgrid.poem.41763": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcnf Jahre sind im Kerker schon vergangen \u2013", "tokens": ["F\u00fcnf", "Jah\u00b7re", "sind", "im", "Ker\u00b7ker", "schon", "ver\u00b7gan\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "APPRART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum f\u00fcnften mal kehrt Dein Geburtstag wieder \u2013", "tokens": ["Zum", "f\u00fcnf\u00b7ten", "mal", "kehrt", "Dein", "Ge\u00b7burts\u00b7tag", "wie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADV", "VVFIN", "PPOSAT", "NN", "ADV", "$("], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ich kam zu Dir mit Sehnen und mit Bangen \u2013", "tokens": ["Ich", "kam", "zu", "Dir", "mit", "Seh\u00b7nen", "und", "mit", "Ban\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "NN", "KON", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und tief besch\u00e4mt senk' ich mein Auge nieder", "tokens": ["Und", "tief", "be\u00b7sch\u00e4mt", "senk'", "ich", "mein", "Au\u00b7ge", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vor Deiner Herrlichkeit in Schmach und Leiden,", "tokens": ["Vor", "Dei\u00b7ner", "Herr\u00b7lich\u00b7keit", "in", "Schmach", "und", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor Deiner Kraft im Dulden und Entbehren!", "tokens": ["Vor", "Dei\u00b7ner", "Kraft", "im", "Dul\u00b7den", "und", "Ent\u00b7beh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du sprichst von Liebe nur, von Seligkeiten,", "tokens": ["Du", "sprichst", "von", "Lie\u00b7be", "nur", ",", "von", "Se\u00b7lig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADV", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo andre sich in Schmerz und Zorn verzehren!", "tokens": ["Wo", "and\u00b7re", "sich", "in", "Schmerz", "und", "Zorn", "ver\u00b7zeh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PRF", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Uns trennend, die wir ewig doch verbunden \u2013", "tokens": ["Uns", "tren\u00b7nend", ",", "die", "wir", "e\u00b7wig", "doch", "ver\u00b7bun\u00b7den", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "PRELS", "PPER", "ADJD", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die wir ganz eins im Streben und im Lieben,", "tokens": ["Die", "wir", "ganz", "eins", "im", "Stre\u00b7ben", "und", "im", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PIS", "APPRART", "NN", "KON", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie That und Wort seit Jahren es bekunden!", "tokens": ["Wie", "That", "und", "Wort", "seit", "Jah\u00b7ren", "es", "be\u00b7kun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O la\u00df mich Dir die Hand durchs Gitter reichen!", "tokens": ["O", "la\u00df", "mich", "Dir", "die", "Hand", "durchs", "Git\u00b7ter", "rei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPER", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du neigst Dich nieder \u2013 k\u00fc\u00dft sie s\u00fc\u00df und hei\u00df,", "tokens": ["Du", "neigst", "Dich", "nie\u00b7der", "\u2013", "k\u00fc\u00dft", "sie", "s\u00fc\u00df", "und", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$(", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dazu des Blickes holdes Liebeszeichen \u2013", "tokens": ["Da\u00b7zu", "des", "Bli\u00b7ckes", "hol\u00b7des", "Lie\u00b7bes\u00b7zei\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kein andres brauchts, da ich so froh Dich wei\u00df!", "tokens": ["Kein", "and\u00b7res", "brauchts", ",", "da", "ich", "so", "froh", "Dich", "wei\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIS", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sieh, Deiner K\u00fcsse und des Gitters Spuren", "tokens": ["Sieh", ",", "Dei\u00b7ner", "K\u00fcs\u00b7se", "und", "des", "Git\u00b7ters", "Spu\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "NN", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind meiner Hand so sichtbar eingepr\u00e4gt", "tokens": ["Sind", "mei\u00b7ner", "Hand", "so", "sicht\u00b7bar", "ein\u00b7ge\u00b7pr\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie N\u00e4gelmale, wie auf Fr\u00fchlingsfluren", "tokens": ["Wie", "N\u00e4\u00b7gel\u00b7ma\u00b7le", ",", "wie", "auf", "Fr\u00fch\u00b7lings\u00b7flu\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "$,", "PWAV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Quell hervorbricht und drin Wunden schl\u00e4gt.", "tokens": ["Ein", "Quell", "her\u00b7vor\u00b7bricht", "und", "drin", "Wun\u00b7den", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "ADV", "NN", "VVFIN", "$."], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Von N\u00e4gelmalen wissen wir zu sagen,", "tokens": ["Von", "N\u00e4\u00b7gel\u00b7ma\u00b7len", "wis\u00b7sen", "wir", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Quellen, die als helle Thr\u00e4nen flossen,", "tokens": ["Von", "Quel\u00b7len", ",", "die", "als", "hel\u00b7le", "Thr\u00e4\u00b7nen", "flos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "KOUS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch auch von Bl\u00fcten, die wir in uns tragen,", "tokens": ["Doch", "auch", "von", "Bl\u00fc\u00b7ten", ",", "die", "wir", "in", "uns", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "$,", "PRELS", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die aus den liebeselgen Herzen sprossen!", "tokens": ["Die", "aus", "den", "lie\u00b7be\u00b7sel\u00b7gen", "Her\u00b7zen", "spros\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}