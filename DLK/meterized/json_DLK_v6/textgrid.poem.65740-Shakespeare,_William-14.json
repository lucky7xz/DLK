{"textgrid.poem.65740": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "14", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht in den Sternen sch\u00e4rf' ich meinen Blick,", "tokens": ["Nicht", "in", "den", "Ster\u00b7nen", "sch\u00e4r\u00b7f'", "ich", "mei\u00b7nen", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Und denke doch ein Astronom zu sein;", "tokens": ["Und", "den\u00b7ke", "doch", "ein", "Ast\u00b7ro\u00b7nom", "zu", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht weil ich gutes oder Mi\u00dfgeschick,", "tokens": ["Nicht", "weil", "ich", "gu\u00b7tes", "o\u00b7der", "Mi\u00df\u00b7ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "PPER", "ADJA", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Pest, Hunger, Wittrung k\u00f6nnt prophezein:", "tokens": ["Pest", ",", "Hun\u00b7ger", ",", "Witt\u00b7rung", "k\u00f6nnt", "pro\u00b7phe\u00b7zein", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "VVFIN", "PTKVZ", "$."], "meter": "++-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Noch wei\u00df ich auf ein Haar das Gl\u00fcck zu deuten,", "tokens": ["Noch", "wei\u00df", "ich", "auf", "ein", "Haar", "das", "Gl\u00fcck", "zu", "deu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wann einen Donner, Wind und Regen trifft;", "tokens": ["Wann", "ei\u00b7nen", "Don\u00b7ner", ",", "Wind", "und", "Re\u00b7gen", "trifft", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der F\u00fcrsten Wohlergehn und Widrigkeiten", "tokens": ["Der", "F\u00fcrs\u00b7ten", "Woh\u00b7ler\u00b7gehn", "und", "Wid\u00b7rig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Les' ich nicht m\u00fchsam aus des Himmels Schrift:", "tokens": ["Les'", "ich", "nicht", "m\u00fch\u00b7sam", "aus", "des", "Him\u00b7mels", "Schrift", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Nein, deine Augen sind mein Quell der Klarheit;", "tokens": ["Nein", ",", "dei\u00b7ne", "Au\u00b7gen", "sind", "mein", "Quell", "der", "Klar\u00b7heit", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Da\u00df Sch\u00f6nheit weiter bl\u00fchen wird und Wahrheit,", "tokens": ["Da\u00df", "Sch\u00f6n\u00b7heit", "wei\u00b7ter", "bl\u00fc\u00b7hen", "wird", "und", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "VVINF", "VAFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Wenn ", "tokens": ["Wenn"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Wo nicht, dann sag' ich dies von dir voraus:", "tokens": ["Wo", "nicht", ",", "dann", "sag'", "ich", "dies", "von", "dir", "vo\u00b7raus", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "PDS", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Mit dir stirbt Sch\u00f6nheit und lischt Wahrheit aus.", "tokens": ["Mit", "dir", "stirbt", "Sch\u00f6n\u00b7heit", "und", "lischt", "Wahr\u00b7heit", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "NN", "KON", "VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}