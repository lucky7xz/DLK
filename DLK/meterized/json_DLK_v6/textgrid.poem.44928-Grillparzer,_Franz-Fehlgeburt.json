{"textgrid.poem.44928": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Fehlgeburt", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Teufel wollte einen M\u00f6rder schaffen", "tokens": ["Der", "Teu\u00b7fel", "woll\u00b7te", "ei\u00b7nen", "M\u00f6r\u00b7der", "schaf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und nahm dazu den Stoff von manchem Tiere:", "tokens": ["Und", "nahm", "da\u00b7zu", "den", "Stoff", "von", "man\u00b7chem", "Tie\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wolf, Fuchs und Schakal gaben her das ihre;", "tokens": ["Wolf", ",", "Fuchs", "und", "Scha\u00b7kal", "ga\u00b7ben", "her", "das", "ih\u00b7re", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "KON", "NN", "VVFIN", "ADV", "ART", "PPOSAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur eins verga\u00df der Ehrenmann: den Mut.", "tokens": ["Nur", "eins", "ver\u00b7ga\u00df", "der", "Eh\u00b7ren\u00b7mann", ":", "den", "Mut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da dr\u00fcckt' er ihm die Nase ein voll Wut", "tokens": ["Da", "dr\u00fcckt'", "er", "ihm", "die", "Na\u00b7se", "ein", "voll", "Wut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und rief: Lump, werd ein Jud und rezensiere!", "tokens": ["Und", "rief", ":", "Lump", ",", "werd", "ein", "Jud", "und", "re\u00b7zen\u00b7sie\u00b7re", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NN", "$,", "VAFIN", "ART", "NN", "KON", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}