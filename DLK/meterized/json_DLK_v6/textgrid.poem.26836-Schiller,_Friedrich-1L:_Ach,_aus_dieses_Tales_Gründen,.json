{"textgrid.poem.26836": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach, aus dieses Tales Gr\u00fcnden,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, aus dieses Tales Gr\u00fcnden,", "tokens": ["Ach", ",", "aus", "die\u00b7ses", "Ta\u00b7les", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die der kalte Nebel dr\u00fcckt,", "tokens": ["Die", "der", "kal\u00b7te", "Ne\u00b7bel", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nnt ich doch den Ausgang finden,", "tokens": ["K\u00f6nnt", "ich", "doch", "den", "Aus\u00b7gang", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach wie f\u00fchlt ich mich begl\u00fcckt!", "tokens": ["Ach", "wie", "f\u00fchlt", "ich", "mich", "be\u00b7gl\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "VVFIN", "PPER", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dort erblick ich sch\u00f6ne H\u00fcgel,", "tokens": ["Dort", "er\u00b7blick", "ich", "sch\u00f6\u00b7ne", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ewig jung und ewig gr\u00fcn!", "tokens": ["E\u00b7wig", "jung", "und", "e\u00b7wig", "gr\u00fcn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "H\u00e4tt ich Schwingen, h\u00e4tt ich Fl\u00fcgel,", "tokens": ["H\u00e4tt", "ich", "Schwin\u00b7gen", ",", "h\u00e4tt", "ich", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nach den H\u00fcgeln z\u00f6g ich hin.", "tokens": ["Nach", "den", "H\u00fc\u00b7geln", "z\u00f6g", "ich", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Harmonien h\u00f6r ich klingen,", "tokens": ["Har\u00b7mo\u00b7ni\u00b7en", "h\u00f6r", "ich", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00f6ne s\u00fc\u00dfer Himmelsruh,", "tokens": ["T\u00f6\u00b7ne", "s\u00fc\u00b7\u00dfer", "Him\u00b7mels\u00b7ruh", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die leichten Winde bringen", "tokens": ["Und", "die", "leich\u00b7ten", "Win\u00b7de", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir der D\u00fcfte Balsam zu,", "tokens": ["Mir", "der", "D\u00fcf\u00b7te", "Bal\u00b7sam", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NE", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Goldne Fr\u00fcchte seh ich gl\u00fchen,", "tokens": ["Gold\u00b7ne", "Fr\u00fcch\u00b7te", "seh", "ich", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Winkend zwischen dunkelm Laub,", "tokens": ["Win\u00b7kend", "zwi\u00b7schen", "dun\u00b7kelm", "Laub", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die Blumen, die dort bl\u00fchen,", "tokens": ["Und", "die", "Blu\u00b7men", ",", "die", "dort", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Werden keines Winters Raub.", "tokens": ["Wer\u00b7den", "kei\u00b7nes", "Win\u00b7ters", "Raub", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach wie sch\u00f6n mu\u00df sichs ergehen", "tokens": ["Ach", "wie", "sch\u00f6n", "mu\u00df", "sichs", "er\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ADJD", "VMFIN", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dort im ewgen Sonnenschein,", "tokens": ["Dort", "im", "ew\u00b7gen", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Luft auf jenen H\u00f6hen,", "tokens": ["Und", "die", "Luft", "auf", "je\u00b7nen", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O wie labend mu\u00df sie sein!", "tokens": ["O", "wie", "la\u00b7bend", "mu\u00df", "sie", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VMFIN", "PPER", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch mir wehrt des Stromes Toben,", "tokens": ["Doch", "mir", "wehrt", "des", "Stro\u00b7mes", "To\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der ergrimmt dazwischen braust,", "tokens": ["Der", "er\u00b7grimmt", "da\u00b7zwi\u00b7schen", "braust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PAV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Seine Wellen sind gehoben,", "tokens": ["Sei\u00b7ne", "Wel\u00b7len", "sind", "ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df die Seele mir ergraust.", "tokens": ["Da\u00df", "die", "See\u00b7le", "mir", "er\u00b7graust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Einen Nachen seh ich schwanken,", "tokens": ["Ei\u00b7nen", "Na\u00b7chen", "seh", "ich", "schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber ach! der F\u00e4hrmann fehlt.", "tokens": ["A\u00b7ber", "ach", "!", "der", "F\u00e4hr\u00b7mann", "fehlt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Frisch hinein und ohne Wanken,", "tokens": ["Frisch", "hin\u00b7ein", "und", "oh\u00b7ne", "Wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine Segel sind beseelt.", "tokens": ["Sei\u00b7ne", "Se\u00b7gel", "sind", "be\u00b7seelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Du mu\u00dft glauben, du mu\u00dft wagen,", "tokens": ["Du", "mu\u00dft", "glau\u00b7ben", ",", "du", "mu\u00dft", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn die G\u00f6tter leihn kein Pfand,", "tokens": ["Denn", "die", "G\u00f6t\u00b7ter", "leihn", "kein", "Pfand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur ein Wunder kann dich tragen", "tokens": ["Nur", "ein", "Wun\u00b7der", "kann", "dich", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VMFIN", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In das sch\u00f6ne Wunderland.", "tokens": ["In", "das", "sch\u00f6\u00b7ne", "Wun\u00b7der\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}