{"dta.poem.8327": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Ein um ein Licht flatterndes  \n Nacht-Eulchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sa\u00df bei sp\u00e4ter Abends-Zeit, und dach-\nte nach was ich gelesen,", "tokens": ["Ich", "sa\u00df", "bei", "sp\u00e4\u00b7ter", "A\u00b7bends\u00b7Zeit", ",", "und", "da\u00b7ch", "te", "nach", "was", "ich", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,", "KON", "TRUNC", "NN", "APPR", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Von einer losen Sp\u00f6tter Zahl, die sich", "tokens": ["Von", "ei\u00b7ner", "lo\u00b7sen", "Sp\u00f6t\u00b7ter", "Zahl", ",", "die", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,", "PRELS", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das unergr\u00fcndlich, zu ergr\u00fcnden. Ich", "tokens": ["Das", "un\u00b7er\u00b7gr\u00fcnd\u00b7lich", ",", "zu", "er\u00b7gr\u00fcn\u00b7den", ".", "Ich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PDS", "ADJD", "$,", "PTKZU", "VVINF", "$.", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den diese spizzen K\u00f6pfe haben, den sie misbrau-", "tokens": ["Den", "die\u00b7se", "spiz\u00b7zen", "K\u00f6p\u00b7fe", "ha\u00b7ben", ",", "den", "sie", "mis\u00b7brau"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PDAT", "ADJA", "NN", "VAFIN", "$,", "PRELS", "PPER", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Die Dreistigkeit ist gar zu gros, wie werden sie", "tokens": ["Die", "Dreis\u00b7tig\u00b7keit", "ist", "gar", "zu", "gros", ",", "wie", "wer\u00b7den", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$,", "PWAV", "VAFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Beim hellen Licht der Ewigkeit. So dacht ich da", "tokens": ["Beim", "hel\u00b7len", "Licht", "der", "E\u00b7wig\u00b7keit", ".", "So", "dacht", "ich", "da"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein Thiergen mich im Denken st\u00f6hrte. Ich sah in", "tokens": ["Ein", "Thier\u00b7gen", "mich", "im", "Den\u00b7ken", "st\u00f6hr\u00b7te", ".", "Ich", "sah", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "APPR"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Es um das Licht so em\u00dfig flattern, und seinen lust-", "tokens": ["Es", "um", "das", "Licht", "so", "em\u00b7\u00dfig", "flat\u00b7tern", ",", "und", "sei\u00b7nen", "lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "ADV", "ADJD", "VVINF", "$,", "KON", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Es flog bald hin, bald wieder her, als wolt es mit", "tokens": ["Es", "flog", "bald", "hin", ",", "bald", "wie\u00b7der", "her", ",", "als", "wolt", "es", "mit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "ADV", "ADV", "PTKVZ", "$,", "KOUS", "VMFIN", "PPER", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich dachte du wirst dir gar bald zur Straf ein bren-", "tokens": ["Ich", "dach\u00b7te", "du", "wirst", "dir", "gar", "bald", "zur", "Straf", "ein", "bren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es kam auch, wie ich eben dachte, es kam dem Lich-", "tokens": ["Es", "kam", "auch", ",", "wie", "ich", "e\u00b7ben", "dach\u00b7te", ",", "es", "kam", "dem", "Lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,", "PPER", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Und da es sich daran verbrennet, und seine Fl\u00fcgel", "tokens": ["Und", "da", "es", "sich", "da\u00b7ran", "ver\u00b7bren\u00b7net", ",", "und", "sei\u00b7ne", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PRF", "PAV", "VVFIN", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Fiel es als todt mir auf das Buch, das Aberwiz", "tokens": ["Fiel", "es", "als", "todt", "mir", "auf", "das", "Buch", ",", "das", "A\u00b7ber\u00b7wiz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "KOUS", "ADJD", "PPER", "APPR", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das flatterhafte Thier lag da mit seinen F\u00fcsse aus-", "tokens": ["Das", "flat\u00b7ter\u00b7haf\u00b7te", "Thier", "lag", "da", "mit", "sei\u00b7nen", "F\u00fcs\u00b7se", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.15": {"text": "Du hast den rechten Ort erw\u00e4hlet, sprach ich, denn", "tokens": ["Du", "hast", "den", "rech\u00b7ten", "Ort", "er\u00b7w\u00e4h\u00b7let", ",", "sprach", "ich", ",", "denn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "PPER", "$,", "KON"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ist sch\u00f6n mit dir drin zu vergleichen, du warest", "tokens": ["Ist", "sch\u00f6n", "mit", "dir", "drin", "zu", "ver\u00b7glei\u00b7chen", ",", "du", "wa\u00b7rest"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADJD", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "$,", "PPER", "VAFIN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.17": {"text": "Und dieser blinde Aberwiz will ", "tokens": ["Und", "die\u00b7ser", "blin\u00b7de", "A\u00b7ber\u00b7wiz", "will"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Er waget auch wie du zu viel, er wird die Fl\u00fcgel", "tokens": ["Er", "wa\u00b7get", "auch", "wie", "du", "zu", "viel", ",", "er", "wird", "die", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "KOKOM", "PPER", "PTKA", "PIS", "$,", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ein Sp\u00f6tter der im Finstern tappet, will ", "tokens": ["Ein", "Sp\u00f6t\u00b7ter", "der", "im", "Fins\u00b7tern", "tap\u00b7pet", ",", "will"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ART", "APPRART", "NN", "VVFIN", "$,", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Er brummet um der Warheit Kerze und will den", "tokens": ["Er", "brum\u00b7met", "um", "der", "War\u00b7heit", "Ker\u00b7ze", "und", "will", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "KON", "VMFIN", "ART"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Er will das helle Wort der Schrift mit seiner Un-", "tokens": ["Er", "will", "das", "hel\u00b7le", "Wort", "der", "Schrift", "mit", "sei\u00b7ner", "Un"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "ART", "NN", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Doch wenn er lang genug gesum\u00dft, gehts ihm wie", "tokens": ["Doch", "wenn", "er", "lang", "ge\u00b7nug", "ge\u00b7sum\u00dft", ",", "gehts", "ihm", "wie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "ADV", "VVPP", "$,", "VVFIN", "PPER", "KOKOM"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.23": {"text": "Die Dreistigkeit wird ihm vergolten, und seine", "tokens": ["Die", "Dreis\u00b7tig\u00b7keit", "wird", "ihm", "ver\u00b7gol\u00b7ten", ",", "und", "sei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "$,", "KON", "PPOSAT"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.24": {"text": "Wenn ihn ein schr\u00f6kliches Gerichte ins ewge Feur", "tokens": ["Wenn", "ihn", "ein", "schr\u00f6k\u00b7li\u00b7ches", "Ge\u00b7rich\u00b7te", "ins", "ew\u00b7ge", "Feur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}