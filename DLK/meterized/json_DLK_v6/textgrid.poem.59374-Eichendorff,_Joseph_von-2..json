{"textgrid.poem.59374": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie in einer Blume himmelblauen", "tokens": ["Wie", "in", "ei\u00b7ner", "Blu\u00b7me", "him\u00b7mel\u00b7blau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Grund, wo schlummernd tr\u00e4umen stille Regenbogen,", "tokens": ["Grund", ",", "wo", "schlum\u00b7mernd", "tr\u00e4u\u00b7men", "stil\u00b7le", "Re\u00b7gen\u00b7bo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADJD", "VVINF", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Ist mein Leben ein unendlich Schauen,", "tokens": ["Ist", "mein", "Le\u00b7ben", "ein", "un\u00b7end\u00b7lich", "Schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "ADJD", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Klar durchs ganze Herz ein s\u00fc\u00dfes Bild gezogen.", "tokens": ["Klar", "durchs", "gan\u00b7ze", "Herz", "ein", "s\u00fc\u00b7\u00dfes", "Bild", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Stille sa\u00df ich, sah die Jahre fliegen,", "tokens": ["Stil\u00b7le", "sa\u00df", "ich", ",", "sah", "die", "Jah\u00b7re", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bin im Innersten dein treues Kind geblieben;", "tokens": ["Bin", "im", "In\u00b7ners\u00b7ten", "dein", "treu\u00b7es", "Kind", "ge\u00b7blie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Aus dem duft'gen Kelche aufgestiegen,", "tokens": ["Aus", "dem", "duft'\u00b7gen", "Kel\u00b7che", "auf\u00b7ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ach! wann lohnst du endlich auch mein treues Lieben!", "tokens": ["Ach", "!", "wann", "lohnst", "du", "end\u00b7lich", "auch", "mein", "treu\u00b7es", "Lie\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}