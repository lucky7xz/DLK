{"textgrid.poem.25777": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: In Felsen kauert die Seele.", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Felsen kauert die Seele.", "tokens": ["In", "Fel\u00b7sen", "kau\u00b7ert", "die", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie lauscht ihrem Atem,", "tokens": ["Sie", "lauscht", "ih\u00b7rem", "A\u00b7tem", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und atmet ihre Gedanken.", "tokens": ["Und", "at\u00b7met", "ih\u00b7re", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Aber die Ruhe allein gibt ihr nicht die Kraft,", "tokens": ["A\u00b7ber", "die", "Ru\u00b7he", "al\u00b7lein", "gibt", "ihr", "nicht", "die", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Sie saugt ihre Kraft aus der Ersch\u00f6pfung,", "tokens": ["Sie", "saugt", "ih\u00b7re", "Kraft", "aus", "der", "Er\u00b7sch\u00f6p\u00b7fung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+---+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Aus dem Vert\u00f6nen erschlaffender Kr\u00e4fte.", "tokens": ["Aus", "dem", "Ver\u00b7t\u00f6\u00b7nen", "er\u00b7schlaf\u00b7fen\u00b7der", "Kr\u00e4f\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Meer w\u00e4lzt seine Berge um die Stille,", "tokens": ["Das", "Meer", "w\u00e4lzt", "sei\u00b7ne", "Ber\u00b7ge", "um", "die", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ihre Einsamkeit umbr\u00fcllen die Wellen,", "tokens": ["Und", "ih\u00b7re", "Ein\u00b7sam\u00b7keit", "um\u00b7br\u00fcl\u00b7len", "die", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "\u00dcber die Felsen fliegt gieriger Schaum,", "tokens": ["\u00dc\u00b7ber", "die", "Fel\u00b7sen", "fliegt", "gie\u00b7ri\u00b7ger", "Schaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Er netzt nicht, \u2013 er zerspringt in Luft \u2013", "tokens": ["Er", "netzt", "nicht", ",", "\u2013", "er", "zer\u00b7springt", "in", "Luft", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "$(", "PPER", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kein Hauch ber\u00fchret die Seele.", "tokens": ["Kein", "Hauch", "be\u00b7r\u00fch\u00b7ret", "die", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und dann, ein Tag! Ein Jahr! Ein Jahrhundert!", "tokens": ["Und", "dann", ",", "ein", "Tag", "!", "Ein", "Jahr", "!", "Ein", "Jahr\u00b7hun\u00b7dert", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ART", "NN", "$.", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Kein Zeitraum, der den Triumph ", "tokens": ["Kein", "Zeit\u00b7raum", ",", "der", "den", "Tri\u00b7umph"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.2": {"text": "Die Felsen wanken, bersten, zerkrachen.", "tokens": ["Die", "Fel\u00b7sen", "wan\u00b7ken", ",", "bers\u00b7ten", ",", "zer\u00b7kra\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das Echo sprengt splitternd die L\u00fcfte,", "tokens": ["Das", "E\u00b7cho", "sprengt", "split\u00b7ternd", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Spaltet die Ruhe, schleudert Berge empor,", "tokens": ["Spal\u00b7tet", "die", "Ru\u00b7he", ",", "schleu\u00b7dert", "Ber\u00b7ge", "em\u00b7por", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Und schroff auf, wild im Flammensprung,", "tokens": ["Und", "schroff", "auf", ",", "wild", "im", "Flam\u00b7men\u00b7sprung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im begeisterten Arme die Fackel,", "tokens": ["Im", "be\u00b7geis\u00b7ter\u00b7ten", "Ar\u00b7me", "die", "Fa\u00b7ckel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Geb\u00e4rt das Genie sich dem Lichte!", "tokens": ["Ge\u00b7b\u00e4rt", "das", "Ge\u00b7nie", "sich", "dem", "Lich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "PRF", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}