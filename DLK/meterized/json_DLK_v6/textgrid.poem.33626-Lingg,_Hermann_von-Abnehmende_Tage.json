{"textgrid.poem.33626": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Abnehmende Tage", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach langen sonnighellen Wochen", "tokens": ["Nach", "lan\u00b7gen", "son\u00b7nig\u00b7hel\u00b7len", "Wo\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie hat es heut mich \u00fcberrascht,", "tokens": ["Wie", "hat", "es", "heut", "mich", "\u00fc\u00b7berr\u00b7ascht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sah das Sonnlicht wie gebrochen,", "tokens": ["Ich", "sah", "das", "Sonn\u00b7licht", "wie", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schon von der D\u00e4mmrung Flug erhascht!", "tokens": ["Schon", "von", "der", "D\u00e4mm\u00b7rung", "Flug", "er\u00b7hascht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es lag ein hold und sanft Verglimmen,", "tokens": ["Es", "lag", "ein", "hold", "und", "sanft", "Ver\u00b7glim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Welch eine Stille auf der Welt!", "tokens": ["Welch", "ei\u00b7ne", "Stil\u00b7le", "auf", "der", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Wald die letzten Vogelstimmen,", "tokens": ["Im", "Wald", "die", "letz\u00b7ten", "Vo\u00b7gel\u00b7stim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Flur vom Abendrot erhellt.", "tokens": ["Die", "Flur", "vom", "A\u00b7ben\u00b7drot", "er\u00b7hellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Noch war mit ihren Blumen allen", "tokens": ["Noch", "war", "mit", "ih\u00b7ren", "Blu\u00b7men", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wiese bunt geschm\u00fcckt und reich,", "tokens": ["Die", "Wie\u00b7se", "bunt", "ge\u00b7schm\u00fcckt", "und", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch wie der Sense schon verfallen", "tokens": ["Doch", "wie", "der", "Sen\u00b7se", "schon", "ver\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wie von Ahnungsgrauen bleich!", "tokens": ["Und", "wie", "von", "Ah\u00b7nungs\u00b7grau\u00b7en", "bleich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es klang ein Echo ferner Laute,", "tokens": ["Es", "klang", "ein", "E\u00b7cho", "fer\u00b7ner", "Lau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ach, in diesem Abend lag", "tokens": ["Und", "ach", ",", "in", "die\u00b7sem", "A\u00b7bend", "lag"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "XY", "$,", "APPR", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Etwas, das mir still vertraute:", "tokens": ["Ein", "Et\u00b7was", ",", "das", "mir", "still", "ver\u00b7trau\u00b7te", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von heute nimmt nun ab der Tag!", "tokens": ["Von", "heu\u00b7te", "nimmt", "nun", "ab", "der", "Tag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Vergleichen mu\u00dft' ich's mit den Jahren,", "tokens": ["Ver\u00b7glei\u00b7chen", "mu\u00dft'", "ich's", "mit", "den", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo erstes Alter uns beschleicht,", "tokens": ["Wo", "ers\u00b7tes", "Al\u00b7ter", "uns", "be\u00b7schleicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo staunend wir und ernst gewahren,", "tokens": ["Wo", "stau\u00b7nend", "wir", "und", "ernst", "ge\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df uns ein k\u00fchler Hauch erreicht.", "tokens": ["Da\u00df", "uns", "ein", "k\u00fch\u00b7ler", "Hauch", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ob auch noch stolze Freuden kommen", "tokens": ["Ob", "auch", "noch", "stol\u00b7ze", "Freu\u00b7den", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und alles uns noch gl\u00fccken mag,", "tokens": ["Und", "al\u00b7les", "uns", "noch", "gl\u00fc\u00b7cken", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch wirklich hat schon abgenommen", "tokens": ["Doch", "wirk\u00b7lich", "hat", "schon", "ab\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Licht von unserm Lebenstag.", "tokens": ["Das", "Licht", "von", "un\u00b7serm", "Le\u00b7bens\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}