{"dta.poem.11628": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Das lustige Wirthshaus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Das lass' ich nun gerne dahin gestellt seyn;", "tokens": ["Das", "lass'", "ich", "nun", "ger\u00b7ne", "da\u00b7hin", "ge\u00b7stellt", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "PAV", "VVPP", "VAINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Was k\u00fcmmert mich Silber und Gold!", "tokens": ["Was", "k\u00fcm\u00b7mert", "mich", "Sil\u00b7ber", "und", "Gold", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und z\u00f6g' ich auf Bettel Land aus und Land ein,", "tokens": ["Und", "z\u00f6g'", "ich", "auf", "Bet\u00b7tel", "Land", "aus", "und", "Land", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "NN", "PTKVZ", "KON", "NN", "PTKVZ", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mein Sch\u00e4tzchen das bliebe mir hold.", "tokens": ["Mein", "Sch\u00e4tz\u00b7chen", "das", "blie\u00b7be", "mir", "hold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDS", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}