{"textgrid.poem.62145": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ruh' in Frieden, gro\u00dfer Dichter,", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ruh' in Frieden, gro\u00dfer Dichter,", "tokens": ["Ruh'", "in", "Frie\u00b7den", ",", "gro\u00b7\u00dfer", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruh' in Frieden, Dichtergeist,", "tokens": ["Ruh'", "in", "Frie\u00b7den", ",", "Dich\u00b7ter\u00b7geist", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruh' in Frieden, Herz voll Saiten,", "tokens": ["Ruh'", "in", "Frie\u00b7den", ",", "Herz", "voll", "Sai\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das kein Mi\u00dfton mehr zerrei\u00dft.", "tokens": ["Das", "kein", "Mi\u00df\u00b7ton", "mehr", "zer\u00b7rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Oder singe, spiele weiter,", "tokens": ["O\u00b7der", "sin\u00b7ge", ",", "spie\u00b7le", "wei\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der selbstgeschaff'nen Art", "tokens": ["In", "der", "selbst\u00b7ge\u00b7schaff'\u00b7nen", "Art"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jener Lieder s\u00fc\u00dfe Worte,", "tokens": ["Je\u00b7ner", "Lie\u00b7der", "s\u00fc\u00b7\u00dfe", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unvergleichlich, geistvoll, zart:", "tokens": ["Un\u00b7ver\u00b7gleich\u00b7lich", ",", "geist\u00b7voll", ",", "zart", ":"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Von des Fichtenbaumes Tr\u00e4umen", "tokens": ["Von", "des", "Fich\u00b7ten\u00b7bau\u00b7mes", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In des Nordens kalter H\u00f6h',", "tokens": ["In", "des", "Nor\u00b7dens", "kal\u00b7ter", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von der armen S\u00fcnderblume,", "tokens": ["Von", "der", "ar\u00b7men", "S\u00fcn\u00b7der\u00b7blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von Ramiro's d\u00fcstrem Weh'!", "tokens": ["Von", "Ra\u00b7mi\u00b7ro's", "d\u00fcst\u00b7rem", "Weh'", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Singe in des Himmels Sph\u00e4re,", "tokens": ["Sin\u00b7ge", "in", "des", "Him\u00b7mels", "Sph\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle Engel stimmen ein,", "tokens": ["Al\u00b7le", "En\u00b7gel", "stim\u00b7men", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Witzli Putzli sei vergeben \u2013", "tokens": ["Witz\u00b7li", "Putz\u00b7li", "sei", "ver\u00b7ge\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle Poesie ist rein!", "tokens": ["Al\u00b7le", "Poe\u00b7sie", "ist", "rein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}}}}