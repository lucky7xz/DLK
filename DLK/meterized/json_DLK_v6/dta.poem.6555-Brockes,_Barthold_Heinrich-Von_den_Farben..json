{"dta.poem.6555": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Farben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Doch w\u00fcrcken Vorw\u00fcrff' offt von einer Art", "tokens": ["Doch", "w\u00fcr\u00b7cken", "Vor\u00b7w\u00fcrff'", "offt", "von", "ei\u00b7ner", "Art"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf unterschiedne Weis\u2019 in unterschiednen Augen.", "tokens": ["Auf", "un\u00b7ter\u00b7schied\u00b7ne", "Weis'", "in", "un\u00b7ter\u00b7schied\u00b7nen", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es d\u00fcncken Farben dem unangenehm und hart,", "tokens": ["Es", "d\u00fcn\u00b7cken", "Far\u00b7ben", "dem", "un\u00b7an\u00b7ge\u00b7nehm", "und", "hart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ART", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die jenen zu vergn\u00fcgen taugen.", "tokens": ["Die", "je\u00b7nen", "zu", "ver\u00b7gn\u00fc\u00b7gen", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Farben Gegenwart w\u00fcrckt stets mit Unterscheid", "tokens": ["Der", "Far\u00b7ben", "Ge\u00b7gen\u00b7wart", "w\u00fcrckt", "stets", "mit", "Un\u00b7ter\u00b7scheid"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Auf einen jeglichen insonderheit,", "tokens": ["Auf", "ei\u00b7nen", "jeg\u00b7li\u00b7chen", "in\u00b7son\u00b7der\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und man betr\u00fcget sich zu aller Zeit,", "tokens": ["Und", "man", "be\u00b7tr\u00fc\u00b7get", "sich", "zu", "al\u00b7ler", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wenn man die Farben stets nach seiner Meinung richt.", "tokens": ["Wenn", "man", "die", "Far\u00b7ben", "stets", "nach", "sei\u00b7ner", "Mei\u00b7nung", "richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Man sp\u00fcret beym gelb-s\u00fcchtigen Gesicht,", "tokens": ["Man", "sp\u00fc\u00b7ret", "beym", "gelb\u00b7s\u00fcch\u00b7ti\u00b7gen", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Das \u00fcberall sein Elend mit sich tr\u00e4get,", "tokens": ["Das", "\u00fc\u00b7be\u00b7rall", "sein", "E\u00b7lend", "mit", "sich", "tr\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PPOSAT", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Wie er das gelb\u2019, so er im Augen heget,", "tokens": ["Wie", "er", "das", "gelb'", ",", "so", "er", "im", "Au\u00b7gen", "he\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "$,", "ADV", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Auf \u00e4usserliche Ding\u2019 unbillig leget.", "tokens": ["Auf", "\u00e4us\u00b7ser\u00b7li\u00b7che", "Ding'", "un\u00b7bil\u00b7lig", "le\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Er theilet andern mit die Gall\u2019 in seinen Blicken,", "tokens": ["Er", "thei\u00b7let", "an\u00b7dern", "mit", "die", "Gall'", "in", "sei\u00b7nen", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und sein beklebend Gifft", "tokens": ["Und", "sein", "be\u00b7kle\u00b7bend", "Gifft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Macht gelb, das Blau und Gr\u00fcn, so Lufft und Wiesen schm\u00fccken.", "tokens": ["Macht", "gelb", ",", "das", "Blau", "und", "Gr\u00fcn", ",", "so", "Lufft", "und", "Wie\u00b7sen", "schm\u00fc\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ART", "NN", "KON", "NN", "$,", "ADV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}