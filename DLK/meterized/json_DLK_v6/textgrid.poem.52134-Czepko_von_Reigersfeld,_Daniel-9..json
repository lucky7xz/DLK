{"textgrid.poem.52134": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Lerche, so das Garn kan unterm Stoppel decken,", "tokens": ["Die", "Ler\u00b7che", ",", "so", "das", "Garn", "kan", "un\u00b7term", "Stop\u00b7pel", "de\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als Meiner Stellung Raub, h\u00f6rt man ein Lob erwecken", "tokens": ["Als", "Mei\u00b7ner", "Stel\u00b7lung", "Raub", ",", "h\u00f6rt", "man", "ein", "Lob", "er\u00b7we\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "$,", "VVFIN", "PIS", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Um des Appeles Tisch: Sie wollen ingemein", "tokens": ["Um", "des", "Ap\u00b7pe\u00b7les", "Tisch", ":", "Sie", "wol\u00b7len", "in\u00b7ge\u00b7mein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "$.", "PPER", "VMFIN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Koches s\u00fc\u00dfer Spie\u00df und zarte Lantze seyn.", "tokens": ["Des", "Ko\u00b7ches", "s\u00fc\u00b7\u00dfer", "Spie\u00df", "und", "zar\u00b7te", "Lant\u00b7ze", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "KON", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ihr B\u00fcrger meines Gutts, so man mir eingeri\u00dfen,", "tokens": ["Ihr", "B\u00fcr\u00b7ger", "mei\u00b7nes", "Gutts", ",", "so", "man", "mir", "ein\u00b7ge\u00b7ri\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$,", "ADV", "PIS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da wo F\u00fcrst Bolco vor sein Aecker wolte schl\u00fc\u00dfen,", "tokens": ["Da", "wo", "F\u00fcrst", "Bol\u00b7co", "vor", "sein", "A\u00b7e\u00b7cker", "wol\u00b7te", "schl\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "NN", "NE", "APPR", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+----+-+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Geht hin und bringet es des Phoebus K\u00fcchen bey,", "tokens": ["Geht", "hin", "und", "brin\u00b7get", "es", "des", "Phoe\u00b7bus", "K\u00fc\u00b7chen", "bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "PPER", "ART", "NE", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df Eures Herren Leid nicht weit von seinem sey.", "tokens": ["Da\u00df", "Eu\u00b7res", "Her\u00b7ren", "Leid", "nicht", "weit", "von", "sei\u00b7nem", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "PTKNEG", "ADJD", "APPR", "PPOSAT", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die St\u00e4lle stehn ohn Vieh, ohn Vorwerck sind die Auen,", "tokens": ["Die", "St\u00e4l\u00b7le", "stehn", "ohn", "Vieh", ",", "ohn", "Vor\u00b7werck", "sind", "die", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,", "KOUI", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Scheunen ohne Korn, nichts ist, wie vor zu schauen:", "tokens": ["Die", "Scheu\u00b7nen", "oh\u00b7ne", "Korn", ",", "nichts", "ist", ",", "wie", "vor", "zu", "schau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "PIS", "VAFIN", "$,", "PWAV", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als der Gefangne Sachs und mit ihm Torsten Sohn", "tokens": ["Als", "der", "Ge\u00b7fang\u00b7ne", "Sachs", "und", "mit", "ihm", "Tors\u00b7ten", "Sohn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NE", "KON", "APPR", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In diesem Hause lag, war dis mein Danck und Lohn.", "tokens": ["In", "die\u00b7sem", "Hau\u00b7se", "lag", ",", "war", "dis", "mein", "Danck", "und", "Lohn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "$,", "VAFIN", "PDS", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch was ist Gut und Haus, wenn St\u00e4dte selber brechen,", "tokens": ["Doch", "was", "ist", "Gut", "und", "Haus", ",", "wenn", "St\u00e4d\u00b7te", "sel\u00b7ber", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "ADJD", "KON", "NN", "$,", "KOUS", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Pest Menschen, Metzger Vieh, und Steller Vogel stechen:", "tokens": ["Pest", "Men\u00b7schen", ",", "Metz\u00b7ger", "Vieh", ",", "und", "Stel\u00b7ler", "Vo\u00b7gel", "ste\u00b7chen", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$,", "KON", "NN", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nihm nur die A\u00e4\u00dfer hin, so gut sie immer seyn,", "tokens": ["Nihm", "nur", "die", "A\u00b7\u00e4\u00b7\u00dfer", "hin", ",", "so", "gut", "sie", "im\u00b7mer", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "PTKVZ", "$,", "ADV", "ADJD", "PPER", "ADV", "VAINF", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "Und scharr' in deine Kehl ihr edle Leiber ein.", "tokens": ["Und", "scharr'", "in", "dei\u00b7ne", "Kehl", "ihr", "ed\u00b7le", "Lei\u00b7ber", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Das Grab sey Mund, die Hand die Baar', und Wein die Z\u00e4hren,", "tokens": ["Das", "Grab", "sey", "Mund", ",", "die", "Hand", "die", "Baa\u00b7r'", ",", "und", "Wein", "die", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,", "ART", "NN", "ART", "NN", "$,", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ihr Andenckmahl der Schmack und was du wirst begehren:", "tokens": ["Ihr", "An\u00b7denck\u00b7mahl", "der", "Schmack", "und", "was", "du", "wirst", "be\u00b7geh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "KON", "PWS", "PPER", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So recht, also mu\u00df es den Indianern gehn,", "tokens": ["So", "recht", ",", "al\u00b7so", "mu\u00df", "es", "den", "In\u00b7di\u00b7a\u00b7nern", "gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die be\u00dfer auff dem Tisch als w\u00fcsten Aeckern stehn.", "tokens": ["Die", "be\u00b7\u00dfer", "auff", "dem", "Tisch", "als", "w\u00fcs\u00b7ten", "A\u00b7e\u00b7ckern", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "KOKOM", "VVFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.6": {"line.1": {"text": "Die G\u00f6tter la\u00dfen ja den Phoebus meine Gaben,", "tokens": ["Die", "G\u00f6t\u00b7ter", "la\u00b7\u00dfen", "ja", "den", "Phoe\u00b7bus", "mei\u00b7ne", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NE", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor andern lieb und werth vor meinetwegen haben:", "tokens": ["Vor", "an\u00b7dern", "lieb", "und", "werth", "vor", "mei\u00b7net\u00b7we\u00b7gen", "ha\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADJD", "KON", "ADJD", "APPR", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es schmeck' ihm meine Tracht, dem sonst nichts schmecken kan,", "tokens": ["Es", "schmeck'", "ihm", "mei\u00b7ne", "Tracht", ",", "dem", "sonst", "nichts", "schme\u00b7cken", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "PRELS", "ADV", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als was nach Wei\u00dfheit schmeckt und ihr ist beygethan.", "tokens": ["Als", "was", "nach", "Wei\u00df\u00b7heit", "schmeckt", "und", "ihr", "ist", "bey\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "VVFIN", "KON", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}