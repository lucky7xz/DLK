{"textgrid.poem.50231": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es hat ein jeder Mensch ", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hat ein jeder Mensch ", "tokens": ["Es", "hat", "ein", "je\u00b7der", "Mensch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Als er ", "tokens": ["Als", "er"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Drum kommt der immer besser an,", "tokens": ["Drum", "kommt", "der", "im\u00b7mer", "bes\u00b7ser", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer ", "tokens": ["Wer"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Denn weil sich jener nur allein ", "tokens": ["Denn", "weil", "sich", "je\u00b7ner", "nur", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PRF", "PDAT", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So zeiget dieser sich ", "tokens": ["So", "zei\u00b7get", "die\u00b7ser", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PDAT", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Man kan sehr viel bey dem ", "tokens": ["Man", "kan", "sehr", "viel", "bey", "dem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ADV", "ADV", "APPR", "ART"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}