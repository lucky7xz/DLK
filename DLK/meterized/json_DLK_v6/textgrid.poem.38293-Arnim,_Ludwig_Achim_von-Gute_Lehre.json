{"textgrid.poem.38293": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Gute Lehre", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Grad Herz brich nicht,", "tokens": ["Grad", "Herz", "brich", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Lieb mich und sags nicht,", "tokens": ["Lieb", "mich", "und", "sags", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "ADV", "PTKNEG", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Liebst du mich,", "tokens": ["Liebst", "du", "mich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Wie ich dich,", "tokens": ["Wie", "ich", "dich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Bleibt die Lieb best\u00e4ndiglich.", "tokens": ["Bleibt", "die", "Lieb", "be\u00b7st\u00e4n\u00b7dig\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6nste Rose fall nicht ab,", "tokens": ["Sch\u00f6ns\u00b7te", "Ro\u00b7se", "fall", "nicht", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis ich komm und brech dich ab;", "tokens": ["Bis", "ich", "komm", "und", "brech", "dich", "ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn mich schon die Dornen stechen,", "tokens": ["Wenn", "mich", "schon", "die", "Dor\u00b7nen", "ste\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will ich doch die Ros' abbrechen.", "tokens": ["Will", "ich", "doch", "die", "Ros'", "ab\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wer die Rosen will abbrechen,", "tokens": ["Wer", "die", "Ro\u00b7sen", "will", "ab\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df nicht achten der Dornen Stechen;", "tokens": ["Mu\u00df", "nicht", "ach\u00b7ten", "der", "Dor\u00b7nen", "Ste\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "VVINF", "ART", "NN", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Rosendornen stechen sehr,", "tokens": ["Ro\u00b7sen\u00b7dor\u00b7nen", "ste\u00b7chen", "sehr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Falsche Liebe noch viel mehr!", "tokens": ["Fal\u00b7sche", "Lie\u00b7be", "noch", "viel", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}