{"textgrid.poem.39680": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich bin erkranket", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin erkranket", "tokens": ["Ich", "bin", "er\u00b7kran\u00b7ket"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "An Liebespein,", "tokens": ["An", "Lie\u00b7be\u00b7spein", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "M\u00f6gt' nur genesen,", "tokens": ["M\u00f6gt'", "nur", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVPP", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "Wollst du mein seyn.", "tokens": ["Wollst", "du", "mein", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Dein lieblich Wesen,", "tokens": ["Dein", "lieb\u00b7lich", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Dein Lippenroth,", "tokens": ["Dein", "Lip\u00b7pen\u00b7roth", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "H\u00e4lt mich gefangen", "tokens": ["H\u00e4lt", "mich", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "VVPP"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Bis an den Tod.", "tokens": ["Bis", "an", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Mein Aug' ist tr\u00fcbe,", "tokens": ["Mein", "Aug'", "ist", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Mein' Jugend verdorrt,", "tokens": ["Mein'", "Ju\u00b7gend", "ver\u00b7dorrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Doch kenn' ich noch Heilung,", "tokens": ["Doch", "kenn'", "ich", "noch", "Hei\u00b7lung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wohl wei\u00df ich den Port.", "tokens": ["Wohl", "wei\u00df", "ich", "den", "Port", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NE", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Zu dem will ich wallen", "tokens": ["Zu", "dem", "will", "ich", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ob L\u00e4nder und Meer,", "tokens": ["Ob", "L\u00e4n\u00b7der", "und", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die Brust ist beklommen,", "tokens": ["Die", "Brust", "ist", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Das Herz ist mir schwer.", "tokens": ["Das", "Herz", "ist", "mir", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Ich greife zum Stabe,", "tokens": ["Ich", "grei\u00b7fe", "zum", "Sta\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ich walle zum Meer;", "tokens": ["Ich", "wal\u00b7le", "zum", "Meer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Es brausen die Winde,", "tokens": ["Es", "brau\u00b7sen", "die", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Es tobet das Meer.", "tokens": ["Es", "to\u00b7bet", "das", "Meer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Die V\u00f6glein fliegen", "tokens": ["Die", "V\u00f6\u00b7glein", "flie\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "So lustig voran,", "tokens": ["So", "lus\u00b7tig", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Sie suchen den Fr\u00fchling", "tokens": ["Sie", "su\u00b7chen", "den", "Fr\u00fch\u00b7ling"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und treffen ihn an.", "tokens": ["Und", "tref\u00b7fen", "ihn", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Es h\u00e4lt mich die Liebe,", "tokens": ["Es", "h\u00e4lt", "mich", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ich bliebe so gern,", "tokens": ["Ich", "blie\u00b7be", "so", "gern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Doch ziehet mich Sehnsucht", "tokens": ["Doch", "zie\u00b7het", "mich", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zum Grabe des Herrn.", "tokens": ["Zum", "Gra\u00b7be", "des", "Herrn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Lebt wohl dann ihr Augen", "tokens": ["Lebt", "wohl", "dann", "ihr", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Von freundlichem Schein,", "tokens": ["Von", "freund\u00b7li\u00b7chem", "Schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mein Blick soll zum Himmel", "tokens": ["Mein", "Blick", "soll", "zum", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Gerichtet nur seyn.", "tokens": ["Ge\u00b7rich\u00b7tet", "nur", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VAINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Mich sehnet, o s\u00fc\u00dfe", "tokens": ["Mich", "seh\u00b7net", ",", "o", "s\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "FM", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Geliebte, nach dir!", "tokens": ["Ge\u00b7lieb\u00b7te", ",", "nach", "dir", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Doch w\u00e4hl' ich das Grab mir,", "tokens": ["Doch", "w\u00e4hl'", "ich", "das", "Grab", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ART", "NN", "PPER", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Des Heilands daf\u00fcr.", "tokens": ["Des", "Hei\u00b7lands", "da\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.10": {"line.1": {"text": "Da kniee ich nieder", "tokens": ["Da", "kni\u00b7ee", "ich", "nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Voll bitterem Schmerz;", "tokens": ["Voll", "bit\u00b7te\u00b7rem", "Schmerz", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Da kann ich dich lassen,", "tokens": ["Da", "kann", "ich", "dich", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.4": {"text": "Da bricht mir das Herz.", "tokens": ["Da", "bricht", "mir", "das", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.11": {"line.1": {"text": "Die Heilung ist bitter,", "tokens": ["Die", "Hei\u00b7lung", "ist", "bit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der Weg ist wohl weit;", "tokens": ["Der", "Weg", "ist", "wohl", "weit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Doch greif' ich zum Stabe", "tokens": ["Doch", "greif'", "ich", "zum", "Sta\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und ende mein Leid.", "tokens": ["Und", "en\u00b7de", "mein", "Leid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}