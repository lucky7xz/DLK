{"dta.poem.11522": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Ein St\u00fcndlein wohl vor Tag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Derweil ich schlafend lag,", "tokens": ["Der\u00b7weil", "ich", "schla\u00b7fend", "lag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein St\u00fcndlein wohl vor Tag,", "tokens": ["Ein", "St\u00fcnd\u00b7lein", "wohl", "vor", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sang vor dem Fenster auf dem Baum", "tokens": ["Sang", "vor", "dem", "Fens\u00b7ter", "auf", "dem", "Baum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ein Schw\u00e4lblein mir, ich h\u00f6rt' es kaum,", "tokens": ["Ein", "Schw\u00e4lb\u00b7lein", "mir", ",", "ich", "h\u00f6rt'", "es", "kaum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein St\u00fcndlein wohl vor Tag:", "tokens": ["Ein", "St\u00fcnd\u00b7lein", "wohl", "vor", "Tag", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "H\u00f6r' an, was ich dir sag',", "tokens": ["H\u00f6r'", "an", ",", "was", "ich", "dir", "sag'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PWS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dein Sch\u00e4tzlein ich verklag':", "tokens": ["Dein", "Sch\u00e4tz\u00b7lein", "ich", "ver\u00b7klag'", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Derweil ich dieses singen thu',", "tokens": ["Der\u00b7weil", "ich", "die\u00b7ses", "sin\u00b7gen", "thu'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Herzt er ein Lieb in guter Ruh,", "tokens": ["Herzt", "er", "ein", "Lieb", "in", "gu\u00b7ter", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein St\u00fcndlein wohl vor Tag.", "tokens": ["Ein", "St\u00fcnd\u00b7lein", "wohl", "vor", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O weh! nicht weiter sag'!", "tokens": ["O", "weh", "!", "nicht", "wei\u00b7ter", "sag'", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "O still! nichts h\u00f6ren mag!", "tokens": ["O", "still", "!", "nichts", "h\u00f6\u00b7ren", "mag", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Flieg' ab, flieg' ab von meinem Baum!", "tokens": ["Flieg'", "ab", ",", "flieg'", "ab", "von", "mei\u00b7nem", "Baum", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u2014 Ach, Lieb' und Treu ist wie ein Traum", "tokens": ["Ach", ",", "Lieb'", "und", "Treu", "ist", "wie", "ein", "Traum"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ITJ", "$,", "NN", "KON", "NN", "VAFIN", "KOKOM", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ein St\u00fcndlein wohl vor Tag.", "tokens": ["Ein", "St\u00fcnd\u00b7lein", "wohl", "vor", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}