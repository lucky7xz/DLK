{"textgrid.poem.32333": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "10. Auf Lucinden", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie hat viel Welt, die muntere Lucinde.", "tokens": ["Sie", "hat", "viel", "Welt", ",", "die", "mun\u00b7te\u00b7re", "Lu\u00b7cin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch nichts wird sie mehr rot gemacht.", "tokens": ["Durch", "nichts", "wird", "sie", "mehr", "rot", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Zweideutigkeit und Schmutz und Schand' und S\u00fcnde,", "tokens": ["Zwei\u00b7deu\u00b7tig\u00b7keit", "und", "Schmutz", "und", "Schand'", "und", "S\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sprecht was ihr wollt: sie winkt euch zu, und lacht.", "tokens": ["Sprecht", "was", "ihr", "wollt", ":", "sie", "winkt", "euch", "zu", ",", "und", "lacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "PPER", "VMFIN", "$.", "PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Err\u00f6te wenigstens, Lucinde,", "tokens": ["Er\u00b7r\u00f6\u00b7te", "we\u00b7nigs\u00b7tens", ",", "Lu\u00b7cin\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df nichts dich mehr err\u00f6ten macht!", "tokens": ["Da\u00df", "nichts", "dich", "mehr", "er\u00b7r\u00f6\u00b7ten", "macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}