{"textgrid.poem.66797": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Leben schwankt durch Nebelgrau", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Leben schwankt durch Nebelgrau", "tokens": ["Das", "Le\u00b7ben", "schwankt", "durch", "Ne\u00b7bel\u00b7grau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Fl\u00fcgeln m\u00fcd und bleiern ...", "tokens": ["Mit", "Fl\u00fc\u00b7geln", "m\u00fcd", "und", "blei\u00b7ern", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wohin ich hellen Auges schau,", "tokens": ["Wo\u00b7hin", "ich", "hel\u00b7len", "Au\u00b7ges", "schau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur Spuk von Dunst und Schleiern.", "tokens": ["Nur", "Spuk", "von", "Dunst", "und", "Schlei\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O br\u00e4che, br\u00e4che doch einmal \u2013", "tokens": ["O", "br\u00e4\u00b7che", ",", "br\u00e4\u00b7che", "doch", "ein\u00b7mal", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "ADV", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sonst wird mein Blick noch bl\u00f6de \u2013", "tokens": ["Sonst", "wird", "mein", "Blick", "noch", "bl\u00f6\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein wilder Blitz, ein heiliger Strahl", "tokens": ["Ein", "wil\u00b7der", "Blitz", ",", "ein", "hei\u00b7li\u00b7ger", "Strahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Durch diese Grabes\u00f6de!", "tokens": ["Durch", "die\u00b7se", "Gra\u00b7be\u00b7s\u00f6\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}