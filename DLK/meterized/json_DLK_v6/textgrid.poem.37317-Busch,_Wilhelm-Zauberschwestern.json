{"textgrid.poem.37317": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Zauberschwestern", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwiefach sind die Phantasien,", "tokens": ["Zwie\u00b7fach", "sind", "die", "Phan\u00b7ta\u00b7si\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Sind ein Zauberschwesternpaar,", "tokens": ["Sind", "ein", "Zau\u00b7ber\u00b7schwes\u00b7tern\u00b7paar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie erscheinen, singen, fliehen", "tokens": ["Sie", "er\u00b7schei\u00b7nen", ",", "sin\u00b7gen", ",", "flie\u00b7hen"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wesenlos und wunderbar.", "tokens": ["We\u00b7sen\u00b7los", "und", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eine ist die himmelblaue,", "tokens": ["Ei\u00b7ne", "ist", "die", "him\u00b7mel\u00b7blau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die uns froh entgegenlacht;", "tokens": ["Die", "uns", "froh", "ent\u00b7ge\u00b7gen\u00b7lacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch die andre ist die graue,", "tokens": ["Doch", "die", "and\u00b7re", "ist", "die", "grau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIS", "VAFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welche angst und bange macht.", "tokens": ["Wel\u00b7che", "angst", "und", "ban\u00b7ge", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "KON", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jene singt von lauter Rosen,", "tokens": ["Je\u00b7ne", "singt", "von", "lau\u00b7ter", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Singt von Liebe und Genu\u00df;", "tokens": ["Singt", "von", "Lie\u00b7be", "und", "Ge\u00b7nu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese st\u00fcrzt den Hoffnungslosen", "tokens": ["Die\u00b7se", "st\u00fcrzt", "den", "Hoff\u00b7nungs\u00b7lo\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von der Br\u00fccke in den Flu\u00df.", "tokens": ["Von", "der", "Br\u00fc\u00b7cke", "in", "den", "Flu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}