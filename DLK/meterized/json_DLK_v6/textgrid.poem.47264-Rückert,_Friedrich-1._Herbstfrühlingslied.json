{"textgrid.poem.47264": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1. Herbstfr\u00fchlingslied", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So oft der Herbst die Rosen stahl,", "tokens": ["So", "oft", "der", "Herbst", "die", "Ro\u00b7sen", "stahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich wei\u00df nicht, wie's entsprungen,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wie's", "ent\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch ist mir hell noch jedesmal", "tokens": ["Doch", "ist", "mir", "hell", "noch", "je\u00b7des\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Fr\u00fchlingslied entklungen.", "tokens": ["Ein", "Fr\u00fch\u00b7lings\u00b7lied", "ent\u00b7klun\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Fr\u00fchling, der vor\u00fcberfuhr,", "tokens": ["Der", "Fr\u00fch\u00b7ling", ",", "der", "vor\u00b7\u00fc\u00b7ber\u00b7fuhr", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und der aus Zukunft winket,", "tokens": ["Und", "der", "aus", "Zu\u00b7kunft", "win\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die beiden werden einer nur,", "tokens": ["Die", "bei\u00b7den", "wer\u00b7den", "ei\u00b7ner", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VAFIN", "ART", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Glanz mein Herz durchblinket.", "tokens": ["Des", "Glanz", "mein", "Herz", "durch\u00b7blin\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So hoff' ich, wenn den Lebensbaum", "tokens": ["So", "hoff'", "ich", ",", "wenn", "den", "Le\u00b7bens\u00b7baum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Alters Hauch entlaubet,", "tokens": ["Des", "Al\u00b7ters", "Hauch", "ent\u00b7lau\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nicht soll ein goldner Jugendtraum", "tokens": ["Nicht", "soll", "ein", "gold\u00b7ner", "Ju\u00b7gend\u00b7traum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Herzen sein geraubet.", "tokens": ["Dem", "Her\u00b7zen", "sein", "ge\u00b7rau\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Jugend, die vor\u00fcberfuhr,", "tokens": ["Die", "Ju\u00b7gend", ",", "die", "vor\u00b7\u00fc\u00b7ber\u00b7fuhr", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird sich im Liede paaren", "tokens": ["Wird", "sich", "im", "Lie\u00b7de", "paa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PRF", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit jener, die auf Edens Flur", "tokens": ["Mit", "je\u00b7ner", ",", "die", "auf", "E\u00b7dens", "Flur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "$,", "PRELS", "APPR", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht wird vor\u00fcberfahren.", "tokens": ["Nicht", "wird", "vor\u00b7\u00fc\u00b7berf\u00b7ah\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}