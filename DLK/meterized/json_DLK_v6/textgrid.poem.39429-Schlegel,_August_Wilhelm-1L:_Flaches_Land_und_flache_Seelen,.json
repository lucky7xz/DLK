{"textgrid.poem.39429": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Flaches Land und flache Seelen,", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Flaches Land und flache Seelen,", "tokens": ["Fla\u00b7ches", "Land", "und", "fla\u00b7che", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die der Erde sch\u00f6ne Zier", "tokens": ["Die", "der", "Er\u00b7de", "sch\u00f6\u00b7ne", "Zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den Himmel mir verhehlen,", "tokens": ["Und", "den", "Him\u00b7mel", "mir", "ver\u00b7heh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleibet endlich hinter mir!", "tokens": ["Blei\u00b7bet", "end\u00b7lich", "hin\u00b7ter", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mir beklemmte Brust und Odem", "tokens": ["Mir", "be\u00b7klemm\u00b7te", "Brust", "und", "O\u00b7dem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieser freudenlose Boden.", "tokens": ["Die\u00b7ser", "freu\u00b7den\u00b7lo\u00b7se", "Bo\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Fernher blinkt der Alpen Kette,", "tokens": ["Fern\u00b7her", "blinkt", "der", "Al\u00b7pen", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon erathm' ich Schweizerluft.", "tokens": ["Schon", "e\u00b7rathm'", "ich", "Schwei\u00b7zer\u00b7luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sei gegr\u00fc\u00dft im Felsenbette,", "tokens": ["Sei", "ge\u00b7gr\u00fc\u00dft", "im", "Fel\u00b7sen\u00b7bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rhodan, Sohn der dunkeln Kluft!", "tokens": ["Rho\u00b7dan", ",", "Sohn", "der", "dun\u00b7keln", "Kluft", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Du auch kommst ja hergezogen,", "tokens": ["Du", "auch", "kommst", "ja", "her\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie ein Gast, mit freien Wogen.", "tokens": ["Wie", "ein", "Gast", ",", "mit", "frei\u00b7en", "Wo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Fremde Sitten, fremde Zungen", "tokens": ["Frem\u00b7de", "Sit\u00b7ten", ",", "frem\u00b7de", "Zun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lernt' ich \u00fcben her und hin;", "tokens": ["Lernt'", "ich", "\u00fc\u00b7ben", "her", "und", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht im Herzen angeklungen", "tokens": ["Nicht", "im", "Her\u00b7zen", "an\u00b7ge\u00b7klun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00e4rkten sie den deutschen Sinn.", "tokens": ["St\u00e4rk\u00b7ten", "sie", "den", "deut\u00b7schen", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lang' ein umgetriebner Wandrer,", "tokens": ["Lang'", "ein", "um\u00b7ge\u00b7trieb\u00b7ner", "Wand\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wurd' ich niemals doch ein Andrer.", "tokens": ["Wurd'", "ich", "nie\u00b7mals", "doch", "ein", "A\u00b7ndrer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Theure Br\u00fcder in Bedr\u00e4ngni\u00df!", "tokens": ["Theu\u00b7re", "Br\u00fc\u00b7der", "in", "Be\u00b7dr\u00e4ng\u00b7ni\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Euch geweiht ist all mein Schmerz.", "tokens": ["Euch", "ge\u00b7weiht", "ist", "all", "mein", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "VAFIN", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was euch trifft, ist ", "tokens": ["Was", "euch", "trifft", ",", "ist"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Fallt ihr, so begehrt mein Herz,", "tokens": ["Fallt", "ihr", ",", "so", "be\u00b7gehrt", "mein", "Herz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VVPP", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df nur bald sich mein Gebeine", "tokens": ["Da\u00df", "nur", "bald", "sich", "mein", "Ge\u00b7bei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "PRF", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vaterl\u00e4nd'schem Staub vereine.", "tokens": ["Va\u00b7ter\u00b7l\u00e4n\u00b7d'\u00b7schem", "Staub", "ver\u00b7ei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}