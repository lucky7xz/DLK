{"textgrid.poem.44691": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[ein T\u00e4nzer, weit ber\u00fchmt in seinem Fach]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein T\u00e4nzer, weit ber\u00fchmt in seinem Fach,", "tokens": ["Ein", "T\u00e4n\u00b7zer", ",", "weit", "be\u00b7r\u00fchmt", "in", "sei\u00b7nem", "Fach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Lie\u00df einst in einer Stadt sein Bestes sehen;", "tokens": ["Lie\u00df", "einst", "in", "ei\u00b7ner", "Stadt", "sein", "Bes\u00b7tes", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er zog den Beifall aller Kenner nach,", "tokens": ["Er", "zog", "den", "Bei\u00b7fall", "al\u00b7ler", "Ken\u00b7ner", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie er erschien, stand alles auf den Zehen.", "tokens": ["Wie", "er", "er\u00b7schien", ",", "stand", "al\u00b7les", "auf", "den", "Ze\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PIS", "APPR", "ART", "CARD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein Boteng\u00e4nger, drau\u00df vom Dorf herein,", "tokens": ["Ein", "Bo\u00b7ten\u00b7g\u00e4n\u00b7ger", ",", "drau\u00df", "vom", "Dorf", "her\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wollt auch ein Zeuge so viel Wunders sein;", "tokens": ["Wollt", "auch", "ein", "Zeu\u00b7ge", "so", "viel", "Wun\u00b7ders", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADV", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Er tritt ins Haus und schaut und steht, und steht und schaut,", "tokens": ["Er", "tritt", "ins", "Haus", "und", "schaut", "und", "steht", ",", "und", "steht", "und", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "KON", "VVFIN", "$,", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Zuletzt ruft er voll innern Ingrimms laut:", "tokens": ["Zu\u00b7letzt", "ruft", "er", "voll", "in\u00b7nern", "In\u00b7grimms", "laut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADJA", "NN", "ADJD", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "O Unsinn, unerh\u00f6rt in diesen Landen,", "tokens": ["O", "Un\u00b7sinn", ",", "un\u00b7er\u00b7h\u00f6rt", "in", "die\u00b7sen", "Lan\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Da keucht und schwitzt der Tor die halbe Nacht", "tokens": ["Da", "keucht", "und", "schwitzt", "der", "Tor", "die", "hal\u00b7be", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und steht zuletzt am Fleck, wo er zuerst gestanden,", "tokens": ["Und", "steht", "zu\u00b7letzt", "am", "Fleck", ",", "wo", "er", "zu\u00b7erst", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "$,", "PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich h\u00e4tt indes zwei Meilen wohl gemacht!", "tokens": ["Ich", "h\u00e4tt", "in\u00b7des", "zwei", "Mei\u00b7len", "wohl", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "CARD", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Er aber wird noch gar daf\u00fcr bewundert.", "tokens": ["Er", "a\u00b7ber", "wird", "noch", "gar", "da\u00b7f\u00fcr", "be\u00b7wun\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Da sieht man, wie das Sprichwort sagt: Ein Narr macht hundert.", "tokens": ["Da", "sieht", "man", ",", "wie", "das", "Sprich\u00b7wort", "sagt", ":", "Ein", "Narr", "macht", "hun\u00b7dert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PWAV", "ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "CARD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der K\u00fcnstler h\u00f6rts und gar nicht stolz, wie sonst wohl K\u00fcnstlerbrauch,", "tokens": ["Der", "K\u00fcnst\u00b7ler", "h\u00f6rts", "und", "gar", "nicht", "stolz", ",", "wie", "sonst", "wohl", "K\u00fcnst\u00b7ler\u00b7brauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "ADV", "PTKNEG", "ADJD", "$,", "PWAV", "ADV", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.16": {"text": "Ruft er: du hast wohl recht, allein ich auch,", "tokens": ["Ruft", "er", ":", "du", "hast", "wohl", "recht", ",", "al\u00b7lein", "ich", "auch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "PPER", "VAFIN", "ADV", "ADJD", "$,", "ADV", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Ein kleiner Unterschied macht hier das Ganze:", "tokens": ["Ein", "klei\u00b7ner", "Un\u00b7ter\u00b7schied", "macht", "hier", "das", "Gan\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Du gehst, mein Lieber, und ich tanze!", "tokens": ["Du", "gehst", ",", "mein", "Lie\u00b7ber", ",", "und", "ich", "tan\u00b7ze", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Drauf schwieg der Eiferer f\u00fcrs N\u00fctzliche und Wahre,", "tokens": ["Drauf", "schwieg", "der", "Ei\u00b7fe\u00b7rer", "f\u00fcrs", "N\u00fctz\u00b7li\u00b7che", "und", "Wah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Doch ging er hin und schrieb die falschen Wanderjahre.", "tokens": ["Doch", "ging", "er", "hin", "und", "schrieb", "die", "fal\u00b7schen", "Wan\u00b7der\u00b7jah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}