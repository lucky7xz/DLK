{"textgrid.poem.61968": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein kleines, ernstes B\u00e4umchen", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein kleines, ernstes B\u00e4umchen", "tokens": ["Ein", "klei\u00b7nes", ",", "erns\u00b7tes", "B\u00e4um\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Streckt seine Zweige aus,", "tokens": ["Streckt", "sei\u00b7ne", "Zwei\u00b7ge", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es lie\u00df nicht gern sich essen,", "tokens": ["Es", "lie\u00df", "nicht", "gern", "sich", "es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Ha\u00df war drum sein Los!", "tokens": ["Und", "Ha\u00df", "war", "drum", "sein", "Los", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PAV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hellrot sind seine Fr\u00fcchte,", "tokens": ["Hell\u00b7rot", "sind", "sei\u00b7ne", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Bl\u00fcten wei\u00df wie Schnee,", "tokens": ["Die", "Bl\u00fc\u00b7ten", "wei\u00df", "wie", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es zeuget die Geschichte", "tokens": ["Es", "zeu\u00b7get", "die", "Ge\u00b7schich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von B\u00e4umchens Schmerzensweh!", "tokens": ["Von", "B\u00e4um\u00b7chens", "Schmer\u00b7zens\u00b7weh", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}