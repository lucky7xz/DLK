{"dta.poem.5370": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vergn\u00fcgen eine Gabe GOttes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nachdem ich \u00f6fters nachgedacht,", "tokens": ["Nach\u00b7dem", "ich", "\u00f6f\u00b7ters", "nach\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Woher es komme, da\u00df die Pracht", "tokens": ["Wo\u00b7her", "es", "kom\u00b7me", ",", "da\u00df", "die", "Pracht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Creaturen uns nicht r\u00fchret,", "tokens": ["Der", "Crea\u00b7tu\u00b7ren", "uns", "nicht", "r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und da\u00df von ihrer Herrlichkeit,", "tokens": ["Und", "da\u00df", "von", "ih\u00b7rer", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schmuck, Ordnung und Vollkommenheit", "tokens": ["Schmuck", ",", "Ord\u00b7nung", "und", "Voll\u00b7kom\u00b7men\u00b7heit"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Man wenig f\u00fchlt, fast nichts versp\u00fchret?", "tokens": ["Man", "we\u00b7nig", "f\u00fchlt", ",", "fast", "nichts", "ver\u00b7sp\u00fch\u00b7ret", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "PIS", "VVFIN", "$,", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So f\u00e4llt mir diese Frage bey:", "tokens": ["So", "f\u00e4llt", "mir", "die\u00b7se", "Fra\u00b7ge", "bey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ob etwann die Empfindlichkeit,", "tokens": ["Ob", "et\u00b7wann", "die", "Emp\u00b7find\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die unser innerstes erfreut,", "tokens": ["Die", "un\u00b7ser", "in\u00b7ners\u00b7tes", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Nicht eine gantz besondre Kraft,", "tokens": ["Nicht", "ei\u00b7ne", "gantz", "be\u00b7sond\u00b7re", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ein\u2019 angeschafne Eigenschaft", "tokens": ["Ein'", "an\u00b7ge\u00b7schaf\u00b7ne", "Ei\u00b7gen\u00b7schaft"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und Sch\u00f6nheit einer Seelen sey,", "tokens": ["Und", "Sch\u00f6n\u00b7heit", "ei\u00b7ner", "See\u00b7len", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Die wir, so wie all andre Gaben,", "tokens": ["Die", "wir", ",", "so", "wie", "all", "and\u00b7re", "Ga\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "ADV", "KOKOM", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Von unserm Sch\u00f6pfer m\u00fcssen haben,", "tokens": ["Von", "un\u00b7serm", "Sch\u00f6p\u00b7fer", "m\u00fcs\u00b7sen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und da\u00df wir von uns selber nicht", "tokens": ["Und", "da\u00df", "wir", "von", "uns", "sel\u00b7ber", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPER", "ADV", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Das sicht- und unsichtbare Licht", "tokens": ["Das", "sicht", "und", "un\u00b7sicht\u00b7ba\u00b7re", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "TRUNC", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "In GOttes Wunder-reichen Wercken", "tokens": ["In", "Got\u00b7tes", "Wun\u00b7der\u00b7rei\u00b7chen", "Wer\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Geschickt und f\u00e4hig zu bemercken.", "tokens": ["Ge\u00b7schickt", "und", "f\u00e4\u00b7hig", "zu", "be\u00b7mer\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Weil, da wir sonst in diesem Leben,", "tokens": ["Weil", ",", "da", "wir", "sonst", "in", "die\u00b7sem", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Mit solchem eifrigen Bem\u00fchn,", "tokens": ["Mit", "sol\u00b7chem", "eif\u00b7ri\u00b7gen", "Be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Uns zu vergn\u00fcgen, uns bestreben,", "tokens": ["Uns", "zu", "ver\u00b7gn\u00fc\u00b7gen", ",", "uns", "be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Es sonst ja fast unm\u00f6glich schien,", "tokens": ["Es", "sonst", "ja", "fast", "un\u00b7m\u00f6g\u00b7lich", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Die eintzge Quelle wahrer Freuden,", "tokens": ["Die", "eintz\u00b7ge", "Quel\u00b7le", "wah\u00b7rer", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "(da doch derselben heller Schein", "tokens": ["(", "da", "doch", "der\u00b7sel\u00b7ben", "hel\u00b7ler", "Schein"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "ADV", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "So Sonnen klar, als allgemein;", "tokens": ["So", "Son\u00b7nen", "klar", ",", "als", "all\u00b7ge\u00b7mein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJD", "$,", "KOUS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Worin sich, wenn wirs recht ergr\u00fcnden,", "tokens": ["Wo\u00b7rin", "sich", ",", "wenn", "wirs", "recht", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "$,", "KOUS", "PIS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Selbst Lust und GOttes Dienst verbinden)", "tokens": ["Selbst", "Lust", "und", "Got\u00b7tes", "Dienst", "ver\u00b7bin\u00b7den", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "So sehr zu fliehen zu vermeiden.", "tokens": ["So", "sehr", "zu", "flie\u00b7hen", "zu", "ver\u00b7mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKZU", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Wofern nun (wie es in der That)", "tokens": ["Wo\u00b7fern", "nun", "(", "wie", "es", "in", "der", "That", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$(", "PWAV", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Es die Bewandni\u00df damit hat;", "tokens": ["Es", "die", "Be\u00b7wand\u00b7ni\u00df", "da\u00b7mit", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PAV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}