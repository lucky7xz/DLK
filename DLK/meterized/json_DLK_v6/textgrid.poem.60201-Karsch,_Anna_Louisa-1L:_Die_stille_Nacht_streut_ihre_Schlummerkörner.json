{"textgrid.poem.60201": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die stille Nacht streut ihre Schlummerk\u00f6rner", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die stille Nacht streut ihre Schlummerk\u00f6rner", "tokens": ["Die", "stil\u00b7le", "Nacht", "streut", "ih\u00b7re", "Schlum\u00b7mer\u00b7k\u00f6r\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf den, der mit dem Pfluge zog,", "tokens": ["Auf", "den", ",", "der", "mit", "dem", "Pflu\u00b7ge", "zog", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und in ein krummes Joch, trotz stolz gewachsaer H\u00f6rner", "tokens": ["Und", "in", "ein", "krum\u00b7mes", "Joch", ",", "trotz", "stolz", "ge\u00b7wach\u00b7saer", "H\u00f6r\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Stieres Nacken bog!", "tokens": ["Des", "Stie\u00b7res", "Na\u00b7cken", "bog", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Wanderer wirft seine m\u00fcden Glieder", "tokens": ["Der", "Wan\u00b7de\u00b7rer", "wirft", "sei\u00b7ne", "m\u00fc\u00b7den", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf unbepf\u00fchlte Lagerstatt;", "tokens": ["Auf", "un\u00b7be\u00b7pf\u00fchl\u00b7te", "La\u00b7ger\u00b7statt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ruhet k\u00f6niglich, wenn auf ihn sein Gefieder", "tokens": ["Und", "ru\u00b7het", "k\u00f6\u00b7nig\u00b7lich", ",", "wenn", "auf", "ihn", "sein", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "$,", "KOUS", "APPR", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Schlaf verbreitet hat.", "tokens": ["Der", "Schlaf", "ver\u00b7brei\u00b7tet", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Freund, von Olymp versenden ihn die G\u00f6tter", "tokens": ["Freund", ",", "von", "O\u00b7lymp", "ver\u00b7sen\u00b7den", "ihn", "die", "G\u00f6t\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "NE", "VVFIN", "PPER", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Sie wachen \u00fcber ihre Welt,", "tokens": ["Sie", "wa\u00b7chen", "\u00fc\u00b7ber", "ih\u00b7re", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn er so sanft herab, wie weiche Rosenbl\u00e4tter", "tokens": ["Wenn", "er", "so", "sanft", "her\u00b7ab", ",", "wie", "wei\u00b7che", "Ro\u00b7sen\u00b7bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ADV", "$,", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf deine Augen f\u00e4llt.", "tokens": ["Auf", "dei\u00b7ne", "Au\u00b7gen", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er tr\u00e4ufelt Balsam in die Seele nieder,", "tokens": ["Er", "tr\u00e4u\u00b7felt", "Bal\u00b7sam", "in", "die", "See\u00b7le", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ganz des Tages Last gef\u00fchlt.", "tokens": ["Die", "ganz", "des", "Ta\u00b7ges", "Last", "ge\u00b7f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "NE", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So wird das welke Gra\u00df nach heisser Sonne wieder", "tokens": ["So", "wird", "das", "wel\u00b7ke", "Gra\u00df", "nach", "heis\u00b7ser", "Son\u00b7ne", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vom Abendthau gek\u00fchlt!", "tokens": ["Vom", "A\u00b7bendt\u00b7hau", "ge\u00b7k\u00fchlt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O er besucht mit Tr\u00e4umen k\u00fcnftger Erndte", "tokens": ["O", "er", "be\u00b7sucht", "mit", "Tr\u00e4u\u00b7men", "k\u00fcnft\u00b7ger", "Ernd\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "VVFIN", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den, welcher Weitzen ausgeklopft;", "tokens": ["Den", ",", "wel\u00b7cher", "Weit\u00b7zen", "aus\u00b7ge\u00b7klopft", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und flieht den reichen Mann der k\u00fcnstlich schwelgen lernte,", "tokens": ["Und", "flieht", "den", "rei\u00b7chen", "Mann", "der", "k\u00fcnst\u00b7lich", "schwel\u00b7gen", "lern\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJD", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Speis' auf Speise stopft!", "tokens": ["Und", "Speis'", "auf", "Spei\u00b7se", "stopft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er flattert von dem Auge des Gecr\u00f6nten,", "tokens": ["Er", "flat\u00b7tert", "von", "dem", "Au\u00b7ge", "des", "Ge\u00b7cr\u00f6n\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der, an das Kriegesschild gest\u00fctzt,", "tokens": ["Der", ",", "an", "das", "Krie\u00b7ges\u00b7schild", "ge\u00b7st\u00fctzt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da stehet, und sein Land vor dem unausges\u00f6hnten", "tokens": ["Da", "ste\u00b7het", ",", "und", "sein", "Land", "vor", "dem", "un\u00b7aus\u00b7ge\u00b7s\u00f6hn\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KON", "PPOSAT", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ergrimmten Feinde sch\u00fctzt!", "tokens": ["Er\u00b7grimm\u00b7ten", "Fein\u00b7de", "sch\u00fctzt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der Geitzige verwachet sich zur Strafe", "tokens": ["Der", "Geit\u00b7zi\u00b7ge", "ver\u00b7wa\u00b7chet", "sich", "zur", "Stra\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und f\u00fcrchtet seines G\u00f6tzen Raub", "tokens": ["Und", "f\u00fcrch\u00b7tet", "sei\u00b7nes", "G\u00f6t\u00b7zen", "Raub"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der weise Monadist entreisset sich dem Schlafe", "tokens": ["Der", "wei\u00b7se", "Mo\u00b7na\u00b7dist", "en\u00b7treis\u00b7set", "sich", "dem", "Schla\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und theilet Sonnenstaub.", "tokens": ["Und", "thei\u00b7let", "Son\u00b7nen\u00b7staub", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Von dir, o Freund, ist nie der Schlaf gewichen", "tokens": ["Von", "dir", ",", "o", "Freund", ",", "ist", "nie", "der", "Schlaf", "ge\u00b7wi\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "FM", "NN", "$,", "VAFIN", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als wenn du hast nach Mitternacht", "tokens": ["Als", "wenn", "du", "hast", "nach", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Voll Patrioten-Ernst den gr\u00f6sten Held verglichen", "tokens": ["Voll", "Pa\u00b7tri\u00b7o\u00b7ten\u00b7Ernst", "den", "gr\u00f6s\u00b7ten", "Held", "ver\u00b7gli\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "ART", "ADJA", "NN", "VVINF"], "meter": "+----+-+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Mit Herculs K\u00e4mpfer-Macht.", "tokens": ["Mit", "Her\u00b7culs", "K\u00e4mp\u00b7fer\u00b7Macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Noch schlummerst du gleich z\u00e4rtlichen Entz\u00fcckten", "tokens": ["Noch", "schlum\u00b7merst", "du", "gleich", "z\u00e4rt\u00b7li\u00b7chen", "Ent\u00b7z\u00fcck\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In sanfter Ruh; so, wie zur Zeit,", "tokens": ["In", "sanf\u00b7ter", "Ruh", ";", "so", ",", "wie", "zur", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "ADV", "$,", "PWAV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da Liebes-G\u00f6tter dich mit Veilchen, die sie pfl\u00fcckten,", "tokens": ["Da", "Lie\u00b7bes\u00b7G\u00f6t\u00b7ter", "dich", "mit", "Veil\u00b7chen", ",", "die", "sie", "pfl\u00fcck\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "APPR", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Geworfen und bestreut;", "tokens": ["Ge\u00b7wor\u00b7fen", "und", "be\u00b7streut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und Ph\u00f6bus dir von dem Parna\u00df hernieder", "tokens": ["Und", "Ph\u00f6\u00b7bus", "dir", "von", "dem", "Par\u00b7na\u00df", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "PPER", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Drey Musen an die Wiege gab!", "tokens": ["Drey", "Mu\u00b7sen", "an", "die", "Wie\u00b7ge", "gab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sangen dich in Schlaf, und wehrten dir durch Lieder", "tokens": ["Sie", "san\u00b7gen", "dich", "in", "Schlaf", ",", "und", "wehr\u00b7ten", "dir", "durch", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,", "KON", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den schweren Traum-Gott ab!", "tokens": ["Den", "schwe\u00b7ren", "Traum\u00b7Gott", "ab", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}