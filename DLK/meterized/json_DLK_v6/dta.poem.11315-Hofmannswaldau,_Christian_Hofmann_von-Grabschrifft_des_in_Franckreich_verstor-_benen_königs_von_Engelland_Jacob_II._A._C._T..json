{"dta.poem.11315": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrifft des in Franckreich verstor-  \n benen k\u00f6nigs von Engelland  \n Jacob  II.  \n A. C. T.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.85", "da:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer ist der fremde stein, wo fl\u00fcchtig und verjagt", "tokens": ["Hjer", "ist", "der", "frem\u00b7de", "stein", ",", "wo", "fl\u00fcch\u00b7tig", "und", "ver\u00b7jagt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,", "PWAV", "ADJD", "KON", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein neuer Jacob schl\u00e4ft, ein alter k\u00f6nig blieben:", "tokens": ["Ein", "neu\u00b7er", "Ja\u00b7cob", "schl\u00e4ft", ",", "ein", "al\u00b7ter", "k\u00f6\u00b7nig", "blie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "$,", "ART", "ADJA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sein Albion hat ihn aus ha\u00df hieher getrieben,", "tokens": ["Sein", "Al\u00b7bi\u00b7on", "hat", "ihn", "aus", "ha\u00df", "hie\u00b7her", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "APPR", "VVIMP", "PAV", "VVPP", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Weil in des vaters hau\u00df er was zu viel gewagt.", "tokens": ["Weil", "in", "des", "va\u00b7ters", "hau\u00df", "er", "was", "zu", "viel", "ge\u00b7wagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "PPER", "PIS", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jtzt sihet er im traum, bey sturm und ungl\u00fccks-wettern,", "tokens": ["Jtzt", "si\u00b7het", "er", "im", "traum", ",", "bey", "sturm", "und", "un\u00b7gl\u00fccks\u00b7wet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,", "APPR", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Auf leitern himmel-an den frischen printzen klettern;", "tokens": ["Auf", "lei\u00b7tern", "him\u00b7mel\u00b7an", "den", "fri\u00b7schen", "print\u00b7zen", "klet\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er schaut, wie zwar sein fu\u00df, und die so schwache hand", "tokens": ["Er", "schaut", ",", "wie", "zwar", "sein", "fu\u00df", ",", "und", "die", "so", "schwa\u00b7che", "hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADV", "VAINF", "PTKVZ", "$,", "KON", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Steigt bi\u00df zur Engelsburg, doch nicht in Engelland.", "tokens": ["Steigt", "bi\u00df", "zur", "En\u00b7gels\u00b7burg", ",", "doch", "nicht", "in", "En\u00b7gel\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$,", "ADV", "PTKNEG", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}