{"dta.poem.19609": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Michel Angelo und seine Statuen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Du \u00f6ffnest, Sklave, deinen Mund,", "tokens": ["Du", "\u00f6ff\u00b7nest", ",", "Skla\u00b7ve", ",", "dei\u00b7nen", "Mund", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch st\u00f6hnst du nicht. Die Lippe schweigt.", "tokens": ["Doch", "st\u00f6hnst", "du", "nicht", ".", "Die", "Lip\u00b7pe", "schweigt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht dr\u00fcckt, Gedankenvoller, dich", "tokens": ["Nicht", "dr\u00fcckt", ",", "Ge\u00b7dan\u00b7ken\u00b7vol\u00b7ler", ",", "dich"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PTKNEG", "VVFIN", "$,", "NN", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die B\u00fcrde der behelmten Stirn.", "tokens": ["Die", "B\u00fcr\u00b7de", "der", "be\u00b7helm\u00b7ten", "Stirn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du packst mit nerv'ger Hand den Bart,", "tokens": ["Du", "packst", "mit", "ner\u00b7v'\u00b7ger", "Hand", "den", "Bart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Doch springst du, Moses, nicht empor.", "tokens": ["Doch", "springst", "du", ",", "Mo\u00b7ses", ",", "nicht", "em\u00b7por", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "NE", "$,", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Maria mit dem todten Sohn,", "tokens": ["Ma\u00b7ria", "mit", "dem", "tod\u00b7ten", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Du weinst, doch rinnt die Thr\u00e4ne nicht.", "tokens": ["Du", "weinst", ",", "doch", "rinnt", "die", "Thr\u00e4\u00b7ne", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ihr stellt des Leids Geberde dar,", "tokens": ["Ihr", "stellt", "des", "Leids", "Ge\u00b7ber\u00b7de", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ihr meine Kinder, ohne Leid!", "tokens": ["Ihr", "mei\u00b7ne", "Kin\u00b7der", ",", "oh\u00b7ne", "Leid", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "KOUI", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "So sieht der freigewordne Geist", "tokens": ["So", "sieht", "der", "frei\u00b7ge\u00b7word\u00b7ne", "Geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Des Lebens \u00fcberwundne Qual.", "tokens": ["Des", "Le\u00b7bens", "\u00fc\u00b7berw\u00b7und\u00b7ne", "Qual", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Was martert die lebend'ge Brust,", "tokens": ["Was", "mar\u00b7tert", "die", "le\u00b7ben\u00b7d'\u00b7ge", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Beseligt und erg\u00f6tzt im Stein.", "tokens": ["Be\u00b7se\u00b7ligt", "und", "er\u00b7g\u00f6tzt", "im", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.15": {"text": "Den Augenblick verewigt ihr", "tokens": ["Den", "Au\u00b7gen\u00b7blick", "ve\u00b7re\u00b7wigt", "ihr"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER"], "meter": "-+-----+", "measure": "dactylic.init"}, "line.16": {"text": "Und sterbt ihr, sterbt ihr ohne Tod.", "tokens": ["Und", "sterbt", "ihr", ",", "sterbt", "ihr", "oh\u00b7ne", "Tod", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Im Schilfe wartet Charon mein,", "tokens": ["Im", "Schil\u00b7fe", "war\u00b7tet", "Cha\u00b7ron", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NE", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Der pfeifend sich die Zeit vertreibt.", "tokens": ["Der", "pfei\u00b7fend", "sich", "die", "Zeit", "ver\u00b7treibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}