{"textgrid.poem.48527": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "30. Aus Sarbievs seinem Lateine \u00fcber die Worte Jesu: Mich d\u00fcrstet", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbach, ach, wie d\u00fcrstet mich!\u00ab schreist du, du sch\u00f6nster F\u00fcrst.", "tokens": ["\u00bb", "ach", ",", "ach", ",", "wie", "d\u00fcrs\u00b7tet", "mich", "!", "\u00ab", "schreist", "du", ",", "du", "sch\u00f6ns\u00b7ter", "F\u00fcrst", "."], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "ITJ", "$,", "PWAV", "VMFIN", "PPER", "$.", "$(", "VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier ist kein s\u00fc\u00dfer Wein, da\u00df du erlabet wirst.", "tokens": ["Hier", "ist", "kein", "s\u00fc\u00b7\u00dfer", "Wein", ",", "da\u00df", "du", "er\u00b7la\u00b7bet", "wirst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "ADJA", "NN", "$,", "KOUS", "PPER", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "D\u00fcrst' immer! Aber ach, du schreist aus aller Kraft!", "tokens": ["D\u00fcr\u00b7st'", "im\u00b7mer", "!", "A\u00b7ber", "ach", ",", "du", "schreist", "aus", "al\u00b7ler", "Kraft", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "KON", "XY", "$,", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "Da hast du, Br\u00e4utgam, nim den gallgemischten Saft!", "tokens": ["Da", "hast", "du", ",", "Br\u00e4ut\u00b7gam", ",", "nim", "den", "gall\u00b7ge\u00b7mischten", "Saft", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "NE", "$,", "VVIMP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Nim Br\u00e4utgam, Br\u00e4utgam, trink! \u00bbWem bring' ichs?\u00ab fragest du.", "tokens": ["Nim", "Br\u00e4ut\u00b7gam", ",", "Br\u00e4ut\u00b7gam", ",", "trink", "!", "\u00bb", "Wem", "bring'", "ichs", "?", "\u00ab", "fra\u00b7gest", "du", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NE", "$,", "VVFIN", "$.", "$(", "PWS", "VVFIN", "PIS", "$.", "$(", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Trink mir es auf der Welt Gesundheit, Jesu, zu!", "tokens": ["Trink", "mir", "es", "auf", "der", "Welt", "Ge\u00b7sund\u00b7heit", ",", "Je\u00b7su", ",", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "NN", "$,", "NE", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}