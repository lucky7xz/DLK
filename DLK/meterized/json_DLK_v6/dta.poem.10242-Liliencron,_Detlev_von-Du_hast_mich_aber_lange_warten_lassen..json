{"dta.poem.10242": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Du hast mich aber lange  \n warten lassen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Es lauscht der Wald.             ", "tokens": ["Es", "lauscht", "der", "Wald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Komm bald, komm bald,", "tokens": ["Komm", "bald", ",", "komm", "bald", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Eh\u2019 noch verschallt im L\u00e4rm des neuen Tages", "tokens": ["Eh'", "noch", "ver\u00b7schallt", "im", "L\u00e4rm", "des", "neu\u00b7en", "Ta\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Quelle Murmeln, und verhallt.", "tokens": ["Der", "Quel\u00b7le", "Mur\u00b7meln", ",", "und", "ver\u00b7hallt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Geschwind, geschwind,", "tokens": ["Ge\u00b7schwind", ",", "ge\u00b7schwind", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Mein s\u00fc\u00dfes Kind,", "tokens": ["Mein", "s\u00fc\u00b7\u00dfes", "Kind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Eh\u2019 noch im Wind die Schauer tiefer Stille", "tokens": ["Eh'", "noch", "im", "Wind", "die", "Schau\u00b7er", "tie\u00b7fer", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPRART", "NN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verzogen und verflogen sind.", "tokens": ["Ver\u00b7zo\u00b7gen", "und", "ver\u00b7flo\u00b7gen", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Durch Wipfel bricht", "tokens": ["Durch", "Wip\u00b7fel", "bricht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Das Morgenlicht.", "tokens": ["Das", "Mor\u00b7gen\u00b7licht", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "O, l\u00e4nger nicht, mein holdes kleines M\u00e4dchen,", "tokens": ["O", ",", "l\u00e4n\u00b7ger", "nicht", ",", "mein", "hol\u00b7des", "klei\u00b7nes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "PTKNEG", "$,", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "La\u00df nun mich warten, l\u00e4nger nicht.", "tokens": ["La\u00df", "nun", "mich", "war\u00b7ten", ",", "l\u00e4n\u00b7ger", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PRF", "VVINF", "$,", "ADJD", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Sonne siegt,", "tokens": ["Die", "Son\u00b7ne", "siegt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Allendlich schmiegt", "tokens": ["Al\u00b7lend\u00b7lich", "schmiegt"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und lachend wiegt sie sich in meinen Armen.", "tokens": ["Und", "la\u00b7chend", "wiegt", "sie", "sich", "in", "mei\u00b7nen", "Ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum Himmel auf die Lerche fliegt.", "tokens": ["Zum", "Him\u00b7mel", "auf", "die", "Ler\u00b7che", "fliegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}