{"textgrid.poem.42512": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mitten im Eichforst,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mitten im Eichforst,", "tokens": ["Mit\u00b7ten", "im", "Eich\u00b7forst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Am lodernden Feuer,", "tokens": ["Am", "lo\u00b7dern\u00b7den", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Tanzt das Zigeunerm\u00e4dchen.", "tokens": ["Tanzt", "das", "Zi\u00b7geu\u00b7ner\u00b7m\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ihre wei\u00dfen Z\u00e4hne l\u00e4cheln", "tokens": ["Ih\u00b7re", "wei\u00b7\u00dfen", "Z\u00e4h\u00b7ne", "l\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Im Mondstrahl;", "tokens": ["Im", "Mond\u00b7strahl", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Und in den Augen brennt ihr die Glut.", "tokens": ["Und", "in", "den", "Au\u00b7gen", "brennt", "ihr", "die", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Sie tanzt den Fandango,", "tokens": ["Sie", "tanzt", "den", "Fan\u00b7dan\u00b7go", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ziert sich,", "tokens": ["Ziert", "sich", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Ziert sich nicht;", "tokens": ["Ziert", "sich", "nicht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKNEG", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Die nackten Arme \u00fcber den Kopf schnellend,", "tokens": ["Die", "nack\u00b7ten", "Ar\u00b7me", "\u00fc\u00b7ber", "den", "Kopf", "schnel\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Klirrt sie den Takt", "tokens": ["Klirrt", "sie", "den", "Takt"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Mit den silberbeschlagenen Kastagnetten.", "tokens": ["Mit", "den", "sil\u00b7ber\u00b7be\u00b7schla\u00b7ge\u00b7nen", "Kas\u00b7tag\u00b7net\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.13": {"text": "Und der Fiedler rast mit dem Bogen,", "tokens": ["Und", "der", "Fied\u00b7ler", "rast", "mit", "dem", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Da\u00df kreischend die T\u00f6ne entfliehen", "tokens": ["Da\u00df", "krei\u00b7schend", "die", "T\u00f6\u00b7ne", "ent\u00b7flie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ART", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.15": {"text": "Ins Walddunkel.", "tokens": ["Ins", "Wald\u00b7dun\u00b7kel", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-++-", "measure": "unknown.measure.di"}, "line.16": {"text": "Grell auf leuchtet das Feuer,", "tokens": ["Grell", "auf", "leuch\u00b7tet", "das", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.17": {"text": "Dann bricht es zusammen.", "tokens": ["Dann", "bricht", "es", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "Aber von frischem gesch\u00fcrt", "tokens": ["A\u00b7ber", "von", "fri\u00b7schem", "ge\u00b7sch\u00fcrt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "VVPP"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.19": {"text": "Wirft es Lichter weit in die Baumschatten,", "tokens": ["Wirft", "es", "Lich\u00b7ter", "weit", "in", "die", "Baum\u00b7schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--++-", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "Auf Farrenkraut und Glockenblumen.", "tokens": ["Auf", "Far\u00b7ren\u00b7kraut", "und", "Glo\u00b7cken\u00b7blu\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Klagend f\u00e4llt die Fl\u00f6te ein;", "tokens": ["Kla\u00b7gend", "f\u00e4llt", "die", "Fl\u00f6\u00b7te", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Aber dazwischen", "tokens": ["A\u00b7ber", "da\u00b7zwi\u00b7schen"], "token_info": ["word", "word"], "pos": ["KON", "PAV"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "Kichern die Saiten der Mandoline...", "tokens": ["Ki\u00b7chern", "die", "Sai\u00b7ten", "der", "Man\u00b7do\u00b7li\u00b7ne", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$("], "meter": "+--+-+-+--", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Aus lischt der Brand.", "tokens": ["Aus", "lischt", "der", "Brand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Nur noch Mondlicht", "tokens": ["Nur", "noch", "Mond\u00b7licht"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Lauscht durch die Bl\u00e4tter;", "tokens": ["Lauscht", "durch", "die", "Bl\u00e4t\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "++-+-", "measure": "iambic.di"}, "line.4": {"text": "Still wirds.", "tokens": ["Still", "wirds", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Die kleinen Steppenpferde rupfen,", "tokens": ["Die", "klei\u00b7nen", "Step\u00b7pen\u00b7pfer\u00b7de", "rup\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vom Z\u00fcgel befreit,", "tokens": ["Vom", "Z\u00fc\u00b7gel", "be\u00b7freit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Die feinen Gr\u00e4ser.", "tokens": ["Die", "fei\u00b7nen", "Gr\u00e4\u00b7ser", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Czico, der Knabe,", "tokens": ["Czi\u00b7co", ",", "der", "Kna\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "H\u00e4lt das M\u00e4dchen in seinen Armen;", "tokens": ["H\u00e4lt", "das", "M\u00e4d\u00b7chen", "in", "sei\u00b7nen", "Ar\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Um sein braunes Gesicht", "tokens": ["Um", "sein", "brau\u00b7nes", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Wirrt sich ihr schwarzes Haar.", "tokens": ["Wirrt", "sich", "ihr", "schwar\u00b7zes", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "ADJA", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.12": {"text": "Er nennt sie:", "tokens": ["Er", "nennt", "sie", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Mein Ringelt\u00e4ubchen,", "tokens": ["Mein", "Rin\u00b7gel\u00b7t\u00e4ub\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Meine Eidechse,", "tokens": ["Mei\u00b7ne", "Ei\u00b7dech\u00b7se", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.15": {"text": "Meine Goldschlange!", "tokens": ["Mei\u00b7ne", "Gold\u00b7schlan\u00b7ge", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-++-", "measure": "unknown.measure.tri"}, "line.16": {"text": "Und erz\u00e4hlt ihr Geschichten,", "tokens": ["Und", "er\u00b7z\u00e4hlt", "ihr", "Ge\u00b7schich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.17": {"text": "M\u00e4rchen aus dem Morgenlande:", "tokens": ["M\u00e4r\u00b7chen", "aus", "dem", "Mor\u00b7gen\u00b7lan\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Vom K\u00f6nig Suleiman.", "tokens": ["Vom", "K\u00f6\u00b7nig", "Su\u00b7lei\u00b7man", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.19": {"text": "Erz\u00e4hlt ihr von seinen Kesseln und Fallen,", "tokens": ["Er\u00b7z\u00e4hlt", "ihr", "von", "sei\u00b7nen", "Kes\u00b7seln", "und", "Fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Und wie er heut Morgen", "tokens": ["Und", "wie", "er", "heut", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.21": {"text": "Eine Gans gestohlen habe.", "tokens": ["Ei\u00b7ne", "Gans", "ge\u00b7stoh\u00b7len", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Das alles erz\u00e4hlt er ihr", "tokens": ["Das", "al\u00b7les", "er\u00b7z\u00e4hlt", "er", "ihr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "VVFIN", "PPER", "PPER"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.23": {"text": "Lachend, ", "tokens": ["La\u00b7chend", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.24": {"text": "Und \u00fcber blinkernde Kieselsteine", "tokens": ["Und", "\u00fc\u00b7ber", "blin\u00b7kern\u00b7de", "Kie\u00b7sel\u00b7stei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "St\u00fcrzen die Quellen", "tokens": ["St\u00fcr\u00b7zen", "die", "Quel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.26": {"text": "In die schweigende Sommernacht...", "tokens": ["In", "die", "schwei\u00b7gen\u00b7de", "Som\u00b7mer\u00b7nacht", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Schon verblassen die Sterne", "tokens": ["Schon", "ver\u00b7blas\u00b7sen", "die", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "In den binsenumnickten Moorwassern,", "tokens": ["In", "den", "bin\u00b7sen\u00b7um\u00b7nick\u00b7ten", "Moor\u00b7was\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+--", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wo die Wildente schl\u00e4ft.", "tokens": ["Wo", "die", "Wil\u00b7den\u00b7te", "schl\u00e4ft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Durchs Gezweige", "tokens": ["Durchs", "Ge\u00b7zwei\u00b7ge"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Spielen gelbe und rote", "tokens": ["Spie\u00b7len", "gel\u00b7be", "und", "ro\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJA", "KON", "ADJA"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Und blaue Fr\u00fchlichter,", "tokens": ["Und", "blau\u00b7e", "Fr\u00fch\u00b7lich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.7": {"text": "Den Morgen wiegend.", "tokens": ["Den", "Mor\u00b7gen", "wie\u00b7gend", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Czico schleicht ans n\u00e4chste Dorf,", "tokens": ["Czi\u00b7co", "schleicht", "ans", "n\u00e4chs\u00b7te", "Dorf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Um wieder eine Gans zu stehlen;", "tokens": ["Um", "wie\u00b7der", "ei\u00b7ne", "Gans", "zu", "steh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und st\u00f6rt den Fuchs,", "tokens": ["Und", "st\u00f6rt", "den", "Fuchs", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Seinen Kumpan,", "tokens": ["Sei\u00b7nen", "Kum\u00b7pan", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Der auf denselben Wegen ist.", "tokens": ["Der", "auf", "den\u00b7sel\u00b7ben", "We\u00b7gen", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PDAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dann wird Tag.", "tokens": ["Dann", "wird", "Tag", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "G\u00e4hnend stehn die Bauern vor den T\u00fcren.", "tokens": ["G\u00e4h\u00b7nend", "stehn", "die", "Bau\u00b7ern", "vor", "den", "T\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Durch die Haide schleppen sich die Zigeuner,", "tokens": ["Durch", "die", "Hai\u00b7de", "schlep\u00b7pen", "sich", "die", "Zi\u00b7geu\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Braun und ungewaschen,", "tokens": ["Braun", "und", "un\u00b7ge\u00b7wa\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Braun wie die Haide.", "tokens": ["Braun", "wie", "die", "Hai\u00b7de", "."], "token_info":