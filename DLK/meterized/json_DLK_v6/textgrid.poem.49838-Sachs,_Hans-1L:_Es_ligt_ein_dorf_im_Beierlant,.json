 "-+--+--+-", "measure": "amphibrach.tri"}, "line.50": {"text": "der has wer in seim garten hinden", "tokens": ["der", "has", "wer", "in", "seim", "gar\u00b7ten", "hin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "PWS", "APPR", "NE", "VVFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "und het im gro\u00dfen schaden gtan", "tokens": ["und", "het", "im", "gro\u00b7\u00dfen", "scha\u00b7den", "gtan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "an seim kolkraut. dem edelman", "tokens": ["an", "seim", "kol\u00b7kraut", ".", "dem", "e\u00b7del\u00b7man"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "tet er ein simmer habrn versprechen,", "tokens": ["tet", "er", "ein", "sim\u00b7mer", "habrn", "ver\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "wenn er in tet am hasen rechen,", "tokens": ["wenn", "er", "in", "tet", "am", "ha\u00b7sen", "re\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.55": {"text": "von dem er wer beschedigt worn,", "tokens": ["von", "dem", "er", "wer", "be\u00b7sche\u00b7digt", "worn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "VVPP", "VAPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.56": {"text": "und het im a