{"textgrid.poem.49838": {"metadata": {"author": {"name": "Sachs, Hans", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es ligt ein dorf im Beierlant,", "genre": "verse", "period": "N.A.", "pub_year": 1563, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ligt ein dorf im Beierlant,", "tokens": ["Es", "ligt", "ein", "dorf", "im", "Bei\u00b7er\u00b7lant", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "dasselbig F\u00fcnsing ist genant,", "tokens": ["das\u00b7sel\u00b7big", "F\u00fcn\u00b7sing", "ist", "ge\u00b7nant", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "da sa\u00dfen leppisch bauren drin.", "tokens": ["da", "sa\u00b7\u00dfen", "lep\u00b7pisch", "bau\u00b7ren", "drin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "nun war auch einer under in,", "tokens": ["nun", "war", "auch", "ei\u00b7ner", "un\u00b7der", "in", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "KON", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "derselbig hie\u00df der Heinz Unru,", "tokens": ["der\u00b7sel\u00b7big", "hie\u00df", "der", "Heinz", "Un\u00b7ru", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "der het zu hadern immerzu", "tokens": ["der", "het", "zu", "ha\u00b7dern", "im\u00b7mer\u00b7zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "mit seinen nachbaurn hindn und vorn,", "tokens": ["mit", "sei\u00b7nen", "nach\u00b7baurn", "hindn", "und", "vorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "stak vol ungedult, rach und zorn,", "tokens": ["stak", "vol", "un\u00b7ge\u00b7dult", ",", "rach", "und", "zorn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "$,", "VVFIN", "KON", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "het gar ein eigensinning laun,", "tokens": ["het", "gar", "ein", "ei\u00b7gen\u00b7sin\u00b7ning", "laun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "brach oft ein hader von eim zaun,", "tokens": ["brach", "oft", "ein", "ha\u00b7der", "von", "eim", "zaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "auf das er nur zu zanken het;", "tokens": ["auf", "das", "er", "nur", "zu", "zan\u00b7ken", "het", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.12": {"text": "drumb iederman in fliehen tet,", "tokens": ["drumb", "ie\u00b7der\u00b7man", "in", "flie\u00b7hen", "tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "APPR", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.13": {"text": "hie\u00df auch darumb der Heinz Unru,", "tokens": ["hie\u00df", "auch", "da\u00b7rumb", "der", "Heinz", "Un\u00b7ru", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PAV", "ART", "NN", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.14": {"text": "das er vil haders richtet zu.", "tokens": ["das", "er", "vil", "ha\u00b7ders", "rich\u00b7tet", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "PIAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "der het ein garten an seim haus,", "tokens": ["der", "het", "ein", "gar\u00b7ten", "an", "seim", "haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VAFIN", "ART", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "darinnen het er \u00fcberaus", "tokens": ["da\u00b7rin\u00b7nen", "het", "er", "\u00fc\u00b7be\u00b7raus"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "wei\u00df r\u00fcben und das kumpaskraut,", "tokens": ["wei\u00df", "r\u00fc\u00b7ben", "und", "das", "kum\u00b7pas\u00b7kraut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVINF", "KON", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "auch korn und habern darin baut,", "tokens": ["auch", "korn", "und", "ha\u00b7bern", "da\u00b7rin", "baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "KON", "VAFIN", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "vil kreuter und w\u00fcrz mancher art,", "tokens": ["vil", "kreu\u00b7ter", "und", "w\u00fcrz", "man\u00b7cher", "art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "PIAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.20": {"text": "des er ser flei\u00dfig h\u00fcten wart.", "tokens": ["des", "er", "ser", "flei\u00b7\u00dfig", "h\u00fc\u00b7ten", "wart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "eins tags kam im ein has darein,", "tokens": ["eins", "tags", "kam", "im", "ein", "has", "da\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVFIN", "APPRART", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "der im darin das kolkraut sein", "tokens": ["der", "im", "da\u00b7rin", "das", "kol\u00b7kraut", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "PAV", "ART", "ADJD", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "an eim ort abgefre\u00dfen het;", "tokens": ["an", "eim", "ort", "ab\u00b7ge\u00b7fre\u00b7\u00dfen", "het", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "darob Heinz Unru z\u00fcrnen tet.", "tokens": ["da\u00b7rob", "Heinz", "Un\u00b7ru", "z\u00fcr\u00b7nen", "tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NE", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.25": {"text": "nicht anderst schalt und fluchet er,", "tokens": ["nicht", "an\u00b7derst", "schalt", "und", "flu\u00b7chet", "er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "sam der ganz gart verderbet wer.", "tokens": ["sam", "der", "ganz", "gart", "ver\u00b7der\u00b7bet", "wer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADV", "ADJD", "VVFIN", "PWS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "und als er den hasen ergucket,", "tokens": ["und", "als", "er", "den", "ha\u00b7sen", "er\u00b7gu\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.28": {"text": "im stadel er ein drischel zucket", "tokens": ["im", "sta\u00b7del", "er", "ein", "dri\u00b7schel", "zu\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "VVFIN", "PPER", "ART", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "und lof dem hasen grimmig nach,", "tokens": ["und", "lof", "dem", "ha\u00b7sen", "grim\u00b7mig", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "zu w\u00fcrgen in zu grimmer rach.", "tokens": ["zu", "w\u00fcr\u00b7gen", "in", "zu", "grim\u00b7mer", "rach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "der has aber war im zu glenk", "tokens": ["der", "has", "a\u00b7ber", "war", "im", "zu", "glenk"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VAFIN", "APPRART", "PTKZU", "VVFIN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.32": {"text": "und brauchet im lauf so vil renk,", "tokens": ["und", "brau\u00b7chet", "im", "lauf", "so", "vil", "renk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.33": {"text": "das der bauer oft fallen was", "tokens": ["das", "der", "bau\u00b7er", "oft", "fal\u00b7len", "was"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "ADV", "VVINF", "PWS"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.34": {"text": "und lag gestrecket in dem gras.", "tokens": ["und", "lag", "ge\u00b7stre\u00b7cket", "in", "dem", "gras", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "dem bauren tet gar we der spot", "tokens": ["dem", "bau\u00b7ren", "tet", "gar", "we", "der", "spot"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "und schwur dem armen hasn den tot:", "tokens": ["und", "schwur", "dem", "ar\u00b7men", "hasn", "den", "tot", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "du ragen\u00f6rlein, du must sterben,", "tokens": ["du", "ra\u00b7ge\u00b7n\u00f6r\u00b7lein", ",", "du", "must", "ster\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "kein man sol dir mein hult erwerben,", "tokens": ["kein", "man", "sol", "dir", "mein", "hult", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIS", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.39": {"text": "weil du mich so oft wirfst zu haufen.", "tokens": ["weil", "du", "mich", "so", "oft", "wirfst", "zu", "hau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.40": {"text": "ob ich dich gleich nit kan erlaufen,", "tokens": ["ob", "ich", "dich", "gleich", "nit", "kan", "er\u00b7lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "schick ich dir doch ein \u00fcbern hals,", "tokens": ["schick", "ich", "dir", "doch", "ein", "\u00fc\u00b7bern", "hals", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "der dich wol knicken kan nachmals!", "tokens": ["der", "dich", "wol", "kni\u00b7cken", "kan", "nach\u00b7mals", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "VMFIN", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.43": {"text": "und balt hin aus dem garten lof", "tokens": ["und", "balt", "hin", "aus", "dem", "gar\u00b7ten", "lof"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APP