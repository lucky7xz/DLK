{"dta.poem.20662": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Lied  \n an Se. F\u00fcrstl. Durchl.  \n  den  \n  jungen Prinzen von Anhalt,  \n Enkel  \n des regierenden Herrn Grafen von Wernigerode  \n  zum  \n Ersten Jahrstage .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Prinz, der von Seiner Mondenzahl                 ", "tokens": ["Prinz", ",", "der", "von", "Sei\u00b7ner", "Mon\u00b7den\u00b7zahl"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den ersten Umlauf hat vollendet,", "tokens": ["Den", "ers\u00b7ten", "Um\u00b7lauf", "hat", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und sich in Seiner Lieblingswahl", "tokens": ["Und", "sich", "in", "Sei\u00b7ner", "Lieb\u00b7lings\u00b7wahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu sch\u00f6nen Kleinigkeiten wendet,", "tokens": ["Zu", "sch\u00f6\u00b7nen", "Klei\u00b7nig\u00b7kei\u00b7ten", "wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O zarte Frucht von hohem Stamm,", "tokens": ["O", "zar\u00b7te", "Frucht", "von", "ho\u00b7hem", "Stamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Jezt spielst Du noch mit goldnen Pferden", "tokens": ["Jezt", "spielst", "Du", "noch", "mit", "gold\u00b7nen", "Pfer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und nimmst Dein kunstgemachtes Lamm,", "tokens": ["Und", "nimmst", "Dein", "kunst\u00b7ge\u00b7mach\u00b7tes", "Lamm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und willst ein Sch\u00e4fer werden.", "tokens": ["Und", "willst", "ein", "Sch\u00e4\u00b7fer", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bald aber, wenn Du gr\u00f6\u00dfer wirst,", "tokens": ["Bald", "a\u00b7ber", ",", "wenn", "Du", "gr\u00f6\u00b7\u00dfer", "wirst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bey vorgekeimeten Verstande,", "tokens": ["Bey", "vor\u00b7ge\u00b7kei\u00b7me\u00b7ten", "Ver\u00b7stan\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Begreift Dein Geist, da\u00df Du der F\u00fcrst,", "tokens": ["Be\u00b7greift", "Dein", "Geist", ",", "da\u00df", "Du", "der", "F\u00fcrst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Erbe bist von einem Lande,", "tokens": ["Der", "Er\u00b7be", "bist", "von", "ei\u00b7nem", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Worinnen Milch und Honig fleu\u00dft:", "tokens": ["Wo\u00b7rin\u00b7nen", "Milch", "und", "Ho\u00b7nig", "fleu\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dann werden Schmeichler Dich umgeben,", "tokens": ["Dann", "wer\u00b7den", "Schmeich\u00b7ler", "Dich", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Deinen witzbegabten Geist", "tokens": ["Die", "Dei\u00b7nen", "witz\u00b7be\u00b7gab\u00b7ten", "Geist"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bis an die Stern erheben.", "tokens": ["Bis", "an", "die", "Stern", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dann sey auf Deiner Hut, o Kind,", "tokens": ["Dann", "sey", "auf", "Dei\u00b7ner", "Hut", ",", "o", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bey dem Gesange der Sirenen,", "tokens": ["Bey", "dem", "Ge\u00b7san\u00b7ge", "der", "Si\u00b7re\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Die lieblich und bet\u00e4ubend sind,", "tokens": ["Die", "lieb\u00b7lich", "und", "be\u00b7t\u00e4u\u00b7bend", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und oftmals in den F\u00fcrstens\u00f6hnen", "tokens": ["Und", "oft\u00b7mals", "in", "den", "F\u00fcrs\u00b7ten\u00b7s\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die besten Seelen schlimm gemacht;", "tokens": ["Die", "bes\u00b7ten", "See\u00b7len", "schlimm", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie gleichen jener bunten Schlangen,", "tokens": ["Sie", "glei\u00b7chen", "je\u00b7ner", "bun\u00b7ten", "Schlan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die listig und mit Redepracht", "tokens": ["Die", "lis\u00b7tig", "und", "mit", "Re\u00b7de\u00b7pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das erste Weib gefangen.", "tokens": ["Das", "ers\u00b7te", "Weib", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "La\u00df nicht in Deiner jungen Brust", "tokens": ["La\u00df", "nicht", "in", "Dei\u00b7ner", "jun\u00b7gen", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Trieb zur stolzen Herrschsucht steigen!", "tokens": ["Den", "Trieb", "zur", "stol\u00b7zen", "Herrschsucht", "stei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sey sanft, und finde Deine Lust,", "tokens": ["Sey", "sanft", ",", "und", "fin\u00b7de", "Dei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn Du Dich f\u00fcrstlich kannst bezeigen,", "tokens": ["Wenn", "Du", "Dich", "f\u00fcrst\u00b7lich", "kannst", "be\u00b7zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gro\u00dfm\u00fcthig, z\u00e4rtlich, voll Gef\u00fchl", "tokens": ["Gro\u00df\u00b7m\u00fct\u00b7hig", ",", "z\u00e4rt\u00b7lich", ",", "voll", "Ge\u00b7f\u00fchl"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Mitleids und der Menschenliebe,", "tokens": ["Des", "Mit\u00b7leids", "und", "der", "Men\u00b7schen\u00b7lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "So da\u00df Dein Herz im Kinderspiel", "tokens": ["So", "da\u00df", "Dein", "Herz", "im", "Kin\u00b7der\u00b7spiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Schon fromme Pflichten \u00fcbe.", "tokens": ["Schon", "from\u00b7me", "Pflich\u00b7ten", "\u00fc\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Jedoch warum ermahnt mein Lied", "tokens": ["Je\u00b7doch", "wa\u00b7rum", "er\u00b7mahnt", "mein", "Lied"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich, der zu Tugenden geschaffen,", "tokens": ["Dich", ",", "der", "zu", "Tu\u00b7gen\u00b7den", "ge\u00b7schaf\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "NN", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Das gr\u00f6\u00dfte Beyspiel vor sich sieht:", "tokens": ["Das", "gr\u00f6\u00df\u00b7te", "Bey\u00b7spiel", "vor", "sich", "sieht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Vater, welcher einst in Waffen,", "tokens": ["Dein", "Va\u00b7ter", ",", "wel\u00b7cher", "einst", "in", "Waf\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und jetzt im Frieden als ein Christ", "tokens": ["Und", "jetzt", "im", "Frie\u00b7den", "als", "ein", "Christ"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Himmels Augen wohlgefallen,", "tokens": ["Des", "Him\u00b7mels", "Au\u00b7gen", "wohl\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Lehrt Dich, so bald Du denkend bist,", "tokens": ["Lehrt", "Dich", ",", "so", "bald", "Du", "den\u00b7kend", "bist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "ADV", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf ebnem Pfade wallen.", "tokens": ["Auf", "eb\u00b7nem", "Pfa\u00b7de", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}