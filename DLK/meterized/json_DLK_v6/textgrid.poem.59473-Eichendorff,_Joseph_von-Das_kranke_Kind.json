{"textgrid.poem.59473": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Das kranke Kind", "genre": "verse", "period": "N.A.", "pub_year": 1832, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Gegend lag so helle,", "tokens": ["Die", "Ge\u00b7gend", "lag", "so", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Sonne schien so warm,", "tokens": ["Die", "Son\u00b7ne", "schien", "so", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es sonnt sich auf der Schwelle", "tokens": ["Es", "sonnt", "sich", "auf", "der", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Kindlein krank und arm.", "tokens": ["Ein", "Kin\u00b7dlein", "krank", "und", "arm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Geputzt zum Sonntag heute", "tokens": ["Ge\u00b7putzt", "zum", "Sonn\u00b7tag", "heu\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPRART", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ziehn sie das Tal entlang,", "tokens": ["Ziehn", "sie", "das", "Tal", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Kind gr\u00fc\u00dft alle Leute,", "tokens": ["Das", "Kind", "gr\u00fc\u00dft", "al\u00b7le", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch niemand sagt ihm Dank.", "tokens": ["Doch", "nie\u00b7mand", "sagt", "ihm", "Dank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Viel Kinder jauchzen ferne,", "tokens": ["Viel", "Kin\u00b7der", "jauch\u00b7zen", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So sch\u00f6n ist's auf der Welt!", "tokens": ["So", "sch\u00f6n", "ist's", "auf", "der", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ging' auch spazieren gerne,", "tokens": ["Ging'", "auch", "spa\u00b7zie\u00b7ren", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch m\u00fcde st\u00fcrzt's im Feld.", "tokens": ["Doch", "m\u00fc\u00b7de", "st\u00fcrzt's", "im", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbach Vater, liebe Mutter,", "tokens": ["\u00bb", "ach", "Va\u00b7ter", ",", "lie\u00b7be", "Mut\u00b7ter", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "XY", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Helft mir in meiner Not! \u2013\u00ab", "tokens": ["Helft", "mir", "in", "mei\u00b7ner", "Not", "!", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du armes Kind! die ruhen", "tokens": ["Du", "ar\u00b7mes", "Kind", "!", "die", "ru\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$.", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ja unterm Grase tot.", "tokens": ["Ja", "un\u00b7term", "Gra\u00b7se", "tot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und so im Gras alleine", "tokens": ["Und", "so", "im", "Gras", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das kranke Kindlein blieb,", "tokens": ["Das", "kran\u00b7ke", "Kin\u00b7dlein", "blieb", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Frug keiner, was es weine,", "tokens": ["Frug", "kei\u00b7ner", ",", "was", "es", "wei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat jeder seins nur lieb.", "tokens": ["Hat", "je\u00b7der", "seins", "nur", "lieb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Abendglocken klangen", "tokens": ["Die", "A\u00b7bend\u00b7glo\u00b7cken", "klan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schon durch die stille Welt,", "tokens": ["Schon", "durch", "die", "stil\u00b7le", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Engel Gottes sangen", "tokens": ["Die", "En\u00b7gel", "Got\u00b7tes", "san\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und gingen \u00fcbers Feld.", "tokens": ["Und", "gin\u00b7gen", "\u00fc\u00b7bers", "Feld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und als die Nacht gekommen", "tokens": ["Und", "als", "die", "Nacht", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und alles das Kind verlie\u00df,", "tokens": ["Und", "al\u00b7les", "das", "Kind", "ver\u00b7lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie haben's mitgenommen,", "tokens": ["Sie", "ha\u00b7ben's", "mit\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nun spielt's im Paradies.", "tokens": ["Nun", "spielt's", "im", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}