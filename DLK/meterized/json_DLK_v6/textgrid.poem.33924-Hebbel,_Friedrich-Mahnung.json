{"textgrid.poem.33924": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Mahnung", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schilt nimmermehr die Stunde hart,", "tokens": ["Schilt", "nim\u00b7mer\u00b7mehr", "die", "Stun\u00b7de", "hart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die fort von dir was Theures rei\u00dft;", "tokens": ["Die", "fort", "von", "dir", "was", "Theu\u00b7res", "rei\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKVZ", "APPR", "PPER", "PIS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie schreitet durch die Gegenwart", "tokens": ["Sie", "schrei\u00b7tet", "durch", "die", "Ge\u00b7gen\u00b7wart"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als ferner Zukunft dunkler Geist;", "tokens": ["Als", "fer\u00b7ner", "Zu\u00b7kunft", "dunk\u00b7ler", "Geist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie will dich vorbereiten, ernst,", "tokens": ["Sie", "will", "dich", "vor\u00b7be\u00b7rei\u00b7ten", ",", "ernst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf das, was unabwendbar droht,", "tokens": ["Auf", "das", ",", "was", "un\u00b7ab\u00b7wend\u00b7bar", "droht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "----+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Damit du heut' entbehren lernst,", "tokens": ["Da\u00b7mit", "du", "heut'", "ent\u00b7beh\u00b7ren", "lernst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Was morgen sicher raubt der Tod.", "tokens": ["Was", "mor\u00b7gen", "si\u00b7cher", "raubt", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}