{"textgrid.poem.27826": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "41.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da, wo ich ietzund war, da war mir hertzlich wol;", "tokens": ["Da", ",", "wo", "ich", "ie\u00b7tzund", "war", ",", "da", "war", "mir", "hertz\u00b7lich", "wol", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "ADV", "VAFIN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wol wird mir wieder seyn, wohin ich kommen sol.", "tokens": ["Wol", "wird", "mir", "wie\u00b7der", "seyn", ",", "wo\u00b7hin", "ich", "kom\u00b7men", "sol", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VAINF", "$,", "PWAV", "PPER", "VVINF", "VMFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Gunst ohne Falsch war hier, dort ist Lieb ohne List;", "tokens": ["Gunst", "oh\u00b7ne", "Falsch", "war", "hier", ",", "dort", "ist", "Lieb", "oh\u00b7ne", "List", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VAFIN", "ADV", "$,", "ADV", "VAFIN", "NN", "APPR", "NN", "$."], "meter": "+--+-++-+--+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Hier ward ich sehr geehrt, dort werd ich sch\u00f6n gek\u00fcst.", "tokens": ["Hier", "ward", "ich", "sehr", "ge\u00b7ehrt", ",", "dort", "werd", "ich", "sch\u00f6n", "ge\u00b7k\u00fcst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,", "ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Beym Freunde war ich ietzt, zur Freundin kumm ich nun;", "tokens": ["Beym", "Freun\u00b7de", "war", "ich", "ietzt", ",", "zur", "Freun\u00b7din", "kumm", "ich", "nun", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADV", "$,", "APPRART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hier that der Tag mir guts, dort wird die Nacht es thun.", "tokens": ["Hier", "that", "der", "Tag", "mir", "guts", ",", "dort", "wird", "die", "Nacht", "es", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}