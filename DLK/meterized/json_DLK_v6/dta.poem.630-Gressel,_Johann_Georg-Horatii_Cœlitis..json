{"dta.poem.630": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Horatii C\u0153litis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Rom kunte nicht so viel als meine tapffre Hand/", "tokens": ["Rom", "kun\u00b7te", "nicht", "so", "viel", "als", "mei\u00b7ne", "tapf\u00b7fre", "Hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PTKNEG", "ADV", "PIAT", "KOKOM", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor den bewehrten Arm der Schwarm der Feinde stand/", "tokens": ["Vor", "den", "be\u00b7wehr\u00b7ten", "Arm", "der", "Schwarm", "der", "Fein\u00b7de", "stand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich wurde unversehrt ans Ufer hingetrieben/", "tokens": ["Ich", "wur\u00b7de", "un\u00b7ver\u00b7sehrt", "ans", "U\u00b7fer", "hin\u00b7ge\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn wer die G\u00f6tter ehrt/ den wollen sie auch lieben.", "tokens": ["Denn", "wer", "die", "G\u00f6t\u00b7ter", "ehrt", "/", "den", "wol\u00b7len", "sie", "auch", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$(", "ART", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}