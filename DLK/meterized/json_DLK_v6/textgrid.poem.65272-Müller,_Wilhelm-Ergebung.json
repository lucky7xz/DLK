{"textgrid.poem.65272": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Ergebung", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bin gefahren auf dem Wasser,", "tokens": ["Bin", "ge\u00b7fah\u00b7ren", "auf", "dem", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab' kein Ruder eingetaucht;", "tokens": ["Hab'", "kein", "Ru\u00b7der", "ein\u00b7ge\u00b7taucht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hab' das Lieben ausgelernet,", "tokens": ["Hab'", "das", "Lie\u00b7ben", "aus\u00b7ge\u00b7ler\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keinen Lehrer je gebraucht.", "tokens": ["Kei\u00b7nen", "Leh\u00b7rer", "je", "ge\u00b7braucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gestern fuhr ich auf dem Wasser,", "tokens": ["Ge\u00b7stern", "fuhr", "ich", "auf", "dem", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Heute sitz' ich auf dem Sand;", "tokens": ["Heu\u00b7te", "sitz'", "ich", "auf", "dem", "Sand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gestern hatt' ich noch ein Dirnel,", "tokens": ["Ge\u00b7stern", "hatt'", "ich", "noch", "ein", "Dir\u00b7nel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Heut' hat's mir den Korb gesandt.", "tokens": ["Heut'", "hat's", "mir", "den", "Korb", "ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und nun ich im Trocknen sitze,", "tokens": ["Und", "nun", "ich", "im", "Trock\u00b7nen", "sit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sing' ich mir ein Lied dazu,", "tokens": ["Sing'", "ich", "mir", "ein", "Lied", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "ART", "NN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und als ich mein Dirnel k\u00fc\u00dfte,", "tokens": ["Und", "als", "ich", "mein", "Dir\u00b7nel", "k\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hatt' ich zum Gesang nicht Ruh'.", "tokens": ["Hatt'", "ich", "zum", "Ge\u00b7sang", "nicht", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "PTKNEG", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Da\u00df es ist im Walde schattig,", "tokens": ["Da\u00df", "es", "ist", "im", "Wal\u00b7de", "schat\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seht, das macht der B\u00e4ume Laub,", "tokens": ["Seht", ",", "das", "macht", "der", "B\u00e4u\u00b7me", "Laub", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PDS", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und da\u00df ich ein Liedchen singe,", "tokens": ["Und", "da\u00df", "ich", "ein", "Lied\u00b7chen", "sin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seht, das macht, mein Schatz ist taub.", "tokens": ["Seht", ",", "das", "macht", ",", "mein", "Schatz", "ist", "taub", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PDS", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Willst nicht h\u00f6ren, wirst wohl f\u00fchlen,", "tokens": ["Willst", "nicht", "h\u00f6\u00b7ren", ",", "wirst", "wohl", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "VVINF", "$,", "VAFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn's zum \u00c4ndern ist zu spat.", "tokens": ["Wenn's", "zum", "\u00c4n\u00b7dern", "ist", "zu", "spat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "VAFIN", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kind, wach' auf, wach' auf und horche!", "tokens": ["Kind", ",", "wach'", "auf", ",", "wach'", "auf", "und", "hor\u00b7che", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcber Nacht k\u00f6mmt guter Rath.", "tokens": ["\u00dc\u00b7ber", "Nacht", "k\u00f6mmt", "gu\u00b7ter", "Rath", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Bin zu dir so oft gegangen", "tokens": ["Bin", "zu", "dir", "so", "oft", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPER", "ADV", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Nacht durch Eis und Schnee,", "tokens": ["In", "der", "Nacht", "durch", "Eis", "und", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hab' vor deiner Th\u00fcr gesungen,", "tokens": ["Hab'", "vor", "dei\u00b7ner", "Th\u00fcr", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wind und Wetter th\u00e4t nicht weh.", "tokens": ["Wind", "und", "Wet\u00b7ter", "th\u00e4t", "nicht", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Blieb das Fenster auch verschlossen,", "tokens": ["Blieb", "das", "Fens\u00b7ter", "auch", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat kein Lied mich doch gereut.", "tokens": ["Hat", "kein", "Lied", "mich", "doch", "ge\u00b7reut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Saiten sind gesprungen,", "tokens": ["Mei\u00b7ne", "Sai\u00b7ten", "sind", "ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "'s ist das letzte Liedel heut'.", "tokens": ["'", "s", "ist", "das", "letz\u00b7te", "Lie\u00b7del", "heut", "'", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}