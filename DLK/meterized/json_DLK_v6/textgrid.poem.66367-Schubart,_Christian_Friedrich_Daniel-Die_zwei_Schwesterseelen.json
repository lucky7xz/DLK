{"textgrid.poem.66367": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Die zwei Schwesterseelen", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n ist Ludovika's Seele,", "tokens": ["Sch\u00f6n", "ist", "Lu\u00b7do\u00b7vi\u00b7ka's", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Zauberin mit Farben;", "tokens": ["Der", "Zau\u00b7be\u00b7rin", "mit", "Far\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sch\u00f6n ist Regina's Seele,", "tokens": ["Sch\u00f6n", "ist", "Re\u00b7gi\u00b7na's", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NE", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Der Zauberin mit T\u00f6nen;", "tokens": ["Der", "Zau\u00b7be\u00b7rin", "mit", "T\u00f6\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zwei Flammen Gottes fuhren sie,", "tokens": ["Zwei", "Flam\u00b7men", "Got\u00b7tes", "fuh\u00b7ren", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Einander traulich umschlingend,", "tokens": ["Ein\u00b7an\u00b7der", "trau\u00b7lich", "um\u00b7schlin\u00b7gend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Aus der Hand des Menschenbilders,", "tokens": ["Aus", "der", "Hand", "des", "Men\u00b7schen\u00b7bil\u00b7ders", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Um der Erde Schmuck zu sein.", "tokens": ["Um", "der", "Er\u00b7de", "Schmuck", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ha, da sind sie nun \u2013", "tokens": ["Ha", ",", "da", "sind", "sie", "nun", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VAFIN", "PPER", "ADV", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Die zwei sich traulich umschlingenden", "tokens": ["Die", "zwei", "sich", "trau\u00b7lich", "um\u00b7schlin\u00b7gen\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "CARD", "PRF", "ADJD", "VVINF"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gottesflammen,", "tokens": ["Got\u00b7tes\u00b7flam\u00b7men", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Und zaubern mit Farben", "tokens": ["Und", "zau\u00b7bern", "mit", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und zaubern mit T\u00f6nen;", "tokens": ["Und", "zau\u00b7bern", "mit", "T\u00f6\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Aber was ist der Farben Zauberei,", "tokens": ["A\u00b7ber", "was", "ist", "der", "Far\u00b7ben", "Zau\u00b7be\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Der T\u00f6ne Zauberei", "tokens": ["Der", "T\u00f6\u00b7ne", "Zau\u00b7be\u00b7rei"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Gegen Ludovika's Madonnablick?", "tokens": ["Ge\u00b7gen", "Lu\u00b7do\u00b7vi\u00b7ka's", "Ma\u00b7don\u00b7nab\u00b7lick", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Gegen Regina's Herrscherblick?", "tokens": ["Ge\u00b7gen", "Re\u00b7gi\u00b7na's", "Herr\u00b7scher\u00b7blick", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Was Farbengemisch und Tonsturm", "tokens": ["Was", "Far\u00b7ben\u00b7ge\u00b7misch", "und", "Ton\u00b7sturm"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Gegen die Fl\u00e4mmchen voll Engelgef\u00fchl,", "tokens": ["Ge\u00b7gen", "die", "Fl\u00e4mm\u00b7chen", "voll", "En\u00b7gel\u00b7ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.12": {"text": "Auf Ludovika's Wange spielend?", "tokens": ["Auf", "Lu\u00b7do\u00b7vi\u00b7ka's", "Wan\u00b7ge", "spie\u00b7lend", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Gegen die himmlische Gluth,", "tokens": ["Ge\u00b7gen", "die", "himm\u00b7li\u00b7sche", "Gluth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.14": {"text": "Die Regina's Antlitz verkl\u00e4rt?", "tokens": ["Die", "Re\u00b7gi\u00b7na's", "Ant\u00b7litz", "ver\u00b7kl\u00e4rt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Wenn Regina liegt an Ludovika's Busen,", "tokens": ["Wenn", "Re\u00b7gi\u00b7na", "liegt", "an", "Lu\u00b7do\u00b7vi\u00b7ka's", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "APPR", "NE", "NE", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wenn Ludovika niederblickt", "tokens": ["Wenn", "Lu\u00b7do\u00b7vi\u00b7ka", "nie\u00b7der\u00b7blickt"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf ihre freundschaftathmende", "tokens": ["Auf", "ih\u00b7re", "freund\u00b7schaf\u00b7tath\u00b7men\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Engelschwester;", "tokens": ["En\u00b7gel\u00b7schwes\u00b7ter", ";"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Dann lieb\u00e4ugeln die Sterne,", "tokens": ["Dann", "lie\u00b7b\u00e4u\u00b7geln", "die", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und aus D\u00fcften des Monds blicken Geister des Himmels", "tokens": ["Und", "aus", "D\u00fcf\u00b7ten", "des", "Monds", "bli\u00b7cken", "Geis\u00b7ter", "des", "Him\u00b7mels"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ART", "NN", "VVFIN", "NN", "ART", "NN"], "meter": "--+--++-+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Und bel\u00e4cheln die Schwesterseelen.", "tokens": ["Und", "be\u00b7l\u00e4\u00b7cheln", "die", "Schwes\u00b7ter\u00b7see\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Gott aber, der Wonneschaffer,", "tokens": ["Gott", "a\u00b7ber", ",", "der", "Won\u00b7ne\u00b7schaf\u00b7fer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ART", "NN", "$,"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Thaut Segen auf sie und spricht:", "tokens": ["Thaut", "Se\u00b7gen", "auf", "sie", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "KON", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ludovika, Regina,", "tokens": ["Lu\u00b7do\u00b7vi\u00b7ka", ",", "Re\u00b7gi\u00b7na", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lebt miteinander,", "tokens": ["Lebt", "mi\u00b7tein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Sterbt miteinander!", "tokens": ["Sterbt", "mi\u00b7tein\u00b7an\u00b7der", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Einst einigt euch ewig mein Himmel!", "tokens": ["Einst", "ei\u00b7nigt", "euch", "e\u00b7wig", "mein", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "O Gl\u00fccklicher! dem Regina", "tokens": ["O", "Gl\u00fcck\u00b7li\u00b7cher", "!", "dem", "Re\u00b7gi\u00b7na"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Himmelsgedanken zuhaucht:", "tokens": ["Den", "Him\u00b7mels\u00b7ge\u00b7dan\u00b7ken", "zu\u00b7haucht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich liebe dich! ewig die Deine!", "tokens": ["Ich", "lie\u00b7be", "dich", "!", "e\u00b7wig", "die", "Dei\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "ADJD", "ART", "PPOSAT", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Beneide kein K\u00f6nigsdiadem,", "tokens": ["Be\u00b7nei\u00b7de", "kein", "K\u00f6\u00b7nigs\u00b7di\u00b7a\u00b7dem", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Keinen Kaiserthron,", "tokens": ["Kei\u00b7nen", "Kai\u00b7ser\u00b7thron", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Denn, Regina liebt dich!", "tokens": ["Denn", ",", "Re\u00b7gi\u00b7na", "liebt", "dich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NE", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}