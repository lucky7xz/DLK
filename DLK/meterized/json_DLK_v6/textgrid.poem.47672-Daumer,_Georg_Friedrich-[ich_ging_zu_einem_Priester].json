{"textgrid.poem.47672": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ich ging zu einem Priester]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ging zu einem Priester,", "tokens": ["Ich", "ging", "zu", "ei\u00b7nem", "Pries\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich warb mir einen Pathen", "tokens": ["Ich", "warb", "mir", "ei\u00b7nen", "Pa\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr mein zu tausend Kindelein", "tokens": ["F\u00fcr", "mein", "zu", "tau\u00b7send", "Kin\u00b7de\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "APPR", "CARD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und fischte zwei Dukaten.", "tokens": ["Und", "fischte", "zwei", "Du\u00b7ka\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "CARD", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Es ist nunmehr das zehnte Mal,", "tokens": ["Es", "ist", "nun\u00b7mehr", "das", "zehn\u00b7te", "Mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df es ein Christ geworden ist;", "tokens": ["Da\u00df", "es", "ein", "Christ", "ge\u00b7wor\u00b7den", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das ist ja wohl kein Schaden.", "tokens": ["Das", "ist", "ja", "wohl", "kein", "Scha\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}