{"dta.poem.2204": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Tirsanders Christenthum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es scheint der wilden Thiere Grimm gerechter noch,", "tokens": ["Es", "scheint", "der", "wil\u00b7den", "Thie\u00b7re", "Grimm", "ge\u00b7rech\u00b7ter", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NE", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "als unsre Wut,", "tokens": ["als", "uns\u00b7re", "Wut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sie spornt die Noth nur gegen andre, wir w\u00fcten gegen", "tokens": ["Sie", "spornt", "die", "Noth", "nur", "ge\u00b7gen", "and\u00b7re", ",", "wir", "w\u00fc\u00b7ten", "ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "PIS", "$,", "PPER", "VVFIN", "APPR"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "unser Blut,", "tokens": ["un\u00b7ser", "Blut", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Aus Hunger nicht, aus Geiz und Stolz. Sie sind auch", "tokens": ["Aus", "Hun\u00b7ger", "nicht", ",", "aus", "Geiz", "und", "Stolz", ".", "Sie", "sind", "auch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "PTKNEG", "$,", "APPR", "NN", "KON", "NN", "$.", "PPER", "VAFIN", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "in der That noch gr\u00f6\u00dfer,", "tokens": ["in", "der", "That", "noch", "gr\u00f6\u00b7\u00dfer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Den Vorzug, eine gr\u00f6\u00dfre Macht und Kraft schenkt ihnen", "tokens": ["Den", "Vor\u00b7zug", ",", "ei\u00b7ne", "gr\u00f6\u00df\u00b7re", "Macht", "und", "Kraft", "schenkt", "ih\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "KON", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "die Natur,", "tokens": ["die", "Na\u00b7tur", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "--+", "measure": "anapaest.init"}, "line.9": {"text": "Ein Reicher aber ist bey uns so wenig gr\u00f6\u00dfer als auch", "tokens": ["Ein", "Rei\u00b7cher", "a\u00b7ber", "ist", "bey", "uns", "so", "we\u00b7nig", "gr\u00f6\u00b7\u00dfer", "als", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VAFIN", "APPR", "PPER", "ADV", "ADV", "ADJD", "KOKOM", "ADV"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.10": {"text": "besser,", "tokens": ["bes\u00b7ser", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Und dennoch ist er m\u00e4chtiger: Man schmiegt sich vor", "tokens": ["Und", "den\u00b7noch", "ist", "er", "m\u00e4ch\u00b7ti\u00b7ger", ":", "Man", "schmiegt", "sich", "vor"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJA", "$.", "PIS", "VVFIN", "PRF", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "ihm \u00fcberall,", "tokens": ["ihm", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Um seiner eignen Gr\u00f6\u00dfe nicht: ein wenig gl\u00e4nzendes", "tokens": ["Um", "sei\u00b7ner", "eig\u00b7nen", "Gr\u00f6\u00b7\u00dfe", "nicht", ":", "ein", "we\u00b7nig", "gl\u00e4n\u00b7zen\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "PTKNEG", "$.", "ART", "PIAT", "ADJA"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "Metall,", "tokens": ["Me\u00b7tall", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "Das er besitzt, macht ihn nur gro\u00df. Verliert er die-", "tokens": ["Das", "er", "be\u00b7sitzt", ",", "macht", "ihn", "nur", "gro\u00df", ".", "Ver\u00b7liert", "er", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "ADJD", "$.", "VVFIN", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "ses, ist er klein", "tokens": ["ses", ",", "ist", "er", "klein"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["FM.fr", "$,", "VAFIN", "PPER", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "An Ehr und Gr\u00f6\u00df\u2019, er wird gleich dumm, ver\u00e4chtlich", "tokens": ["An", "Ehr", "und", "Gr\u00f6\u00df'", ",", "er", "wird", "gleich", "dumm", ",", "ver\u00b7\u00e4cht\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "und ein Narre seyn.", "tokens": ["und", "ein", "Nar\u00b7re", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAINF", "$."], "meter": "--+-+", "measure": "anapaest.init"}}}}}