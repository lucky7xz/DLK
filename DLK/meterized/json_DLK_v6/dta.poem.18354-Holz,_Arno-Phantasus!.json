{"dta.poem.18354": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Phantasus!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Ihm mit Staunen blickt' ich nach;", "tokens": ["Ihm", "mit", "Stau\u00b7nen", "blickt'", "ich", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch, wenn mir die Kraft gebrach,", "tokens": ["Doch", ",", "wenn", "mir", "die", "Kraft", "ge\u00b7brach", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Um ihm nachzuringen,", "tokens": ["Um", "ihm", "nach\u00b7zu\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVIZU", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Dacht' ich bang: genug! genug!", "tokens": ["Dacht'", "ich", "bang", ":", "ge\u00b7nug", "!", "ge\u00b7nug", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$.", "ADV", "$.", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Brechen m\u00fcssen bei dem Flug", "tokens": ["Bre\u00b7chen", "m\u00fcs\u00b7sen", "bei", "dem", "Flug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Endlich seine Schwingen.", "tokens": ["End\u00b7lich", "sei\u00b7ne", "Schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Und es kam, wie ich gedacht;", "tokens": ["Und", "es", "kam", ",", "wie", "ich", "ge\u00b7dacht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Um sein fr\u00fches Grab bei Nacht", "tokens": ["Um", "sein", "fr\u00fc\u00b7hes", "Grab", "bei", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Flattert die Phal\u00e4ne;", "tokens": ["Flat\u00b7tert", "die", "Pha\u00b7l\u00e4\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wo so oft er bei mir sa\u00df,", "tokens": ["Wo", "so", "oft", "er", "bei", "mir", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Blieb ich einsam, und ins Glas", "tokens": ["Blieb", "ich", "ein\u00b7sam", ",", "und", "ins", "Glas"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "KON", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Rieselt eine Thr\u00e4ne.", "tokens": ["Rie\u00b7selt", "ei\u00b7ne", "Thr\u00e4\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}