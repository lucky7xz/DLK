{"textgrid.poem.49116": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "Der Iltis und der Kater", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine Taube trug der Iltis", "tokens": ["Ei\u00b7ne", "Tau\u00b7be", "trug", "der", "Il\u00b7tis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber's Dach. Der Kater schreit:", "tokens": ["Ue\u00b7ber's", "Dach", ".", "Der", "Ka\u00b7ter", "schreit", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbei! Verruchter! Unschuldsm\u00f6rder!", "tokens": ["\u00bb", "ei", "!", "Ver\u00b7ruch\u00b7ter", "!", "Un\u00b7schulds\u00b7m\u00f6r\u00b7der", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "ITJ", "$.", "NN", "$.", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unerh\u00f6rte Grausamkeit!\u00ab \u2013", "tokens": ["Un\u00b7er\u00b7h\u00f6r\u00b7te", "Grau\u00b7sam\u00b7keit", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbschimpf nur, Heuchler!\u00ab gab's ihm jener", "tokens": ["\u00bb", "schimpf", "nur", ",", "Heuch\u00b7ler", "!", "\u00ab", "gab's", "ihm", "je\u00b7ner"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "ADJD", "ADV", "$,", "NN", "$.", "$(", "VVFIN", "PPER", "PDAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wieder, \u00bbwahrlich! 's steht dir gut.", "tokens": ["Wie\u00b7der", ",", "\u00bb", "wahr\u00b7lich", "!", "'s", "steht", "dir", "gut", "."], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "$(", "ADV", "$.", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Klebt doch frisch an deiner Schnauze", "tokens": ["Klebt", "doch", "frisch", "an", "dei\u00b7ner", "Schnau\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Erst erw\u00fcrgter Henne Blut!", "tokens": ["Erst", "er\u00b7w\u00fcrg\u00b7ter", "Hen\u00b7ne", "Blut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Du beschreiest mein Verbrechen", "tokens": ["Du", "be\u00b7schrei\u00b7est", "mein", "Ver\u00b7bre\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht aus Pflicht und Billigkeit.", "tokens": ["Nicht", "aus", "Pflicht", "und", "Bil\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aergres w\u00fcrdest du beginnen,", "tokens": ["A\u00b7er\u00b7gres", "w\u00fcr\u00b7dest", "du", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "B\u00f6t sich dir Gelegenheit.\u00ab", "tokens": ["B\u00f6t", "sich", "dir", "Ge\u00b7le\u00b7gen\u00b7heit", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PRF", "PPER", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}