{"textgrid.poem.26531": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fch' hab' ich deinen Boden schon betreten,", "tokens": ["Fr\u00fch'", "hab'", "ich", "dei\u00b7nen", "Bo\u00b7den", "schon", "be\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch eh' du meinem Geiste konntest frommen;", "tokens": ["Noch", "eh'", "du", "mei\u00b7nem", "Geis\u00b7te", "konn\u00b7test", "from\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch sahst du mich in Sehnsucht wiederkommen \u2013", "tokens": ["Doch", "sahst", "du", "mich", "in", "Sehn\u00b7sucht", "wie\u00b7der\u00b7kom\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und still den Mann zu deinen Wundern beten.", "tokens": ["Und", "still", "den", "Mann", "zu", "dei\u00b7nen", "Wun\u00b7dern", "be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O hehre Schauer, die mich da umwehten!", "tokens": ["O", "heh\u00b7re", "Schau\u00b7er", ",", "die", "mich", "da", "um\u00b7weh\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O heil'ge Gluthen, die mich da durchglommen,", "tokens": ["O", "heil'\u00b7ge", "Glu\u00b7then", ",", "die", "mich", "da", "durch\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als deine Sch\u00f6nheit ganz ich aufgenommen", "tokens": ["Als", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", "ganz", "ich", "auf\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Land und Stadt, in Meistern und Propheten!", "tokens": ["In", "Land", "und", "Stadt", ",", "in", "Meis\u00b7tern", "und", "Pro\u00b7phe\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch allgemach beim ernsten Gang der Zeiten,", "tokens": ["Doch", "all\u00b7ge\u00b7mach", "beim", "erns\u00b7ten", "Gang", "der", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo es zu siegen galt in Kampfesstunden,", "tokens": ["Wo", "es", "zu", "sie\u00b7gen", "galt", "in", "Kamp\u00b7fes\u00b7stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKZU", "VVINF", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sah ich dich ferner stets und ferner gleiten.", "tokens": ["Sah", "ich", "dich", "fer\u00b7ner", "stets", "und", "fer\u00b7ner", "glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADV", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und jetzt, da schon, zu Schw\u00e4rmen rings verbunden,", "tokens": ["Und", "jetzt", ",", "da", "schon", ",", "zu", "Schw\u00e4r\u00b7men", "rings", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "ADV", "$,", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Menschen eilen, laut dich zu beschreiten,", "tokens": ["Die", "Men\u00b7schen", "ei\u00b7len", ",", "laut", "dich", "zu", "be\u00b7schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bist du, verd\u00e4mmernd, meinem Blick entschwunden.", "tokens": ["Bist", "du", ",", "ver\u00b7d\u00e4m\u00b7mernd", ",", "mei\u00b7nem", "Blick", "ent\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "VVPP", "$,", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}