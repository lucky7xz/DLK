{"dta.poem.18143": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Pr\u00fcffung  der  Uebers etzung  \n von Horazens Dichtkunst.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So gehts den W\u00f6rtern auch; bald siehet man sie stehn", "tokens": ["So", "gehts", "den", "W\u00f6r\u00b7tern", "auch", ";", "bald", "sie\u00b7het", "man", "sie", "stehn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$.", "ADV", "VVFIN", "PIS", "PPER", "VVINF"], "meter": "-+-+-++-+--+", "measure": "iambic.hexa.chol"}, "line.2": {"text": "In ihrem Jugend Flor, bald abgelebt zerfallen.", "tokens": ["In", "ih\u00b7rem", "Ju\u00b7gend", "Flor", ",", "bald", "ab\u00b7ge\u00b7lebt", "zer\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "ADV", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Tod beh\u00e4lt sein Recht an uns so wohl als allen,", "tokens": ["Der", "Tod", "be\u00b7h\u00e4lt", "sein", "Recht", "an", "uns", "so", "wohl", "als", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "ADV", "ADV", "KOUS", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was immer unser hei\u00dft. ....", "tokens": ["Was", "im\u00b7mer", "un\u00b7ser", "hei\u00dft", ".", "...."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "ADV", "ADJD", "VVFIN", "$.", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}