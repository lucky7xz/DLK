{"dta.poem.12828": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Lob der einsamkeit.  \n G. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Einsamkeit mag immerhin", "tokens": ["Ein\u00b7sam\u00b7keit", "mag", "im\u00b7mer\u00b7hin"], "token_info": ["word", "word", "word"], "pos": ["NN", "VMFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schimpfflich ausgeh\u00f6net werden,", "tokens": ["Schimpf\u00b7flich", "aus\u00b7ge\u00b7h\u00f6\u00b7net", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nichts vergn\u00fcget mich auf erden,", "tokens": ["Nichts", "ver\u00b7gn\u00fc\u00b7get", "mich", "auf", "er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als wenn ich alleine bin.", "tokens": ["Als", "wenn", "ich", "al\u00b7lei\u00b7ne", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Stilles leben hilfft dem geist", "tokens": ["Stil\u00b7les", "le\u00b7ben", "hilfft", "dem", "geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf verborgne spur zu sinnen,", "tokens": ["Auf", "ver\u00b7borg\u00b7ne", "spur", "zu", "sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dasselbe zu gewinnen,", "tokens": ["Und", "das\u00b7sel\u00b7be", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was gerecht und edel heist.", "tokens": ["Was", "ge\u00b7recht", "und", "e\u00b7del", "heist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Giebt das ungl\u00fcck einen stich,", "tokens": ["Giebt", "das", "un\u00b7gl\u00fcck", "ei\u00b7nen", "stich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und verdoppelt mir die sorgen,", "tokens": ["Und", "ver\u00b7dop\u00b7pelt", "mir", "die", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bleibt es doch der welt verborgen,", "tokens": ["Bleibt", "es", "doch", "der", "welt", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die stille tr\u00f6stet mich.", "tokens": ["Und", "die", "stil\u00b7le", "tr\u00f6s\u00b7tet", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dieses lobet mein gedicht:", "tokens": ["Die\u00b7ses", "lo\u00b7bet", "mein", "ge\u00b7dicht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich mag ihr die schmertzen klagen,", "tokens": ["Ich", "mag", "ihr", "die", "schmert\u00b7zen", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "ADJA", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Oder meine freude sagen,", "tokens": ["O\u00b7der", "mei\u00b7ne", "freu\u00b7de", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einsamkeit verr\u00e4th mich nicht.", "tokens": ["Ein\u00b7sam\u00b7keit", "ver\u00b7r\u00e4\u00b7th", "mich", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}