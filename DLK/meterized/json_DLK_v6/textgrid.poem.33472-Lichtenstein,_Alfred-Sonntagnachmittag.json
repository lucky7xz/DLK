{"textgrid.poem.33472": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Sonntagnachmittag", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf faulen Stra\u00dfen lagern H\u00e4userrudel,", "tokens": ["Auf", "fau\u00b7len", "Stra\u00b7\u00dfen", "la\u00b7gern", "H\u00e4u\u00b7ser\u00b7ru\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um deren Buckel graue Sonne hellt.", "tokens": ["Um", "de\u00b7ren", "Bu\u00b7ckel", "grau\u00b7e", "Son\u00b7ne", "hellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein parf\u00fcmierter, halbverr\u00fcckter kleiner Pudel", "tokens": ["Ein", "par\u00b7f\u00fc\u00b7mier\u00b7ter", ",", "halb\u00b7ver\u00b7r\u00fcck\u00b7ter", "klei\u00b7ner", "Pu\u00b7del"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wirft w\u00fcste Augen in die gro\u00dfe Welt.", "tokens": ["Wirft", "w\u00fcs\u00b7te", "Au\u00b7gen", "in", "die", "gro\u00b7\u00dfe", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In einem Fenster f\u00e4ngt ein Junge Fliegen.", "tokens": ["In", "ei\u00b7nem", "Fens\u00b7ter", "f\u00e4ngt", "ein", "Jun\u00b7ge", "Flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein arg beschmiertes Baby \u00e4rgert sich.", "tokens": ["Ein", "arg", "be\u00b7schmier\u00b7tes", "Ba\u00b7by", "\u00e4r\u00b7gert", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Am Himmel f\u00e4hrt ein Zug, wo windge Wiesen liegen;", "tokens": ["Am", "Him\u00b7mel", "f\u00e4hrt", "ein", "Zug", ",", "wo", "wind\u00b7ge", "Wie\u00b7sen", "lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "PWAV", "VVFIN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Malt langsam einen langen dicken Strich.", "tokens": ["Malt", "lang\u00b7sam", "ei\u00b7nen", "lan\u00b7gen", "di\u00b7cken", "Strich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie Schreibmaschinen klappen Droschkenhufe.", "tokens": ["Wie", "Schreib\u00b7ma\u00b7schi\u00b7nen", "klap\u00b7pen", "Droschken\u00b7hu\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Und l\u00e4rmend kommt ein staubger Turnverein.", "tokens": ["Und", "l\u00e4r\u00b7mend", "kommt", "ein", "staub\u00b7ger", "Turn\u00b7ver\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus Kutscherkneipen st\u00fcrzen sich brutale Rufe.", "tokens": ["Aus", "Kut\u00b7scher\u00b7knei\u00b7pen", "st\u00fcr\u00b7zen", "sich", "bru\u00b7ta\u00b7le", "Ru\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch feine Glocken dringen auf sie ein.", "tokens": ["Doch", "fei\u00b7ne", "Glo\u00b7cken", "drin\u00b7gen", "auf", "sie", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "In Rummelpl\u00e4tzen, wo Athleten ringen,", "tokens": ["In", "Rum\u00b7mel\u00b7pl\u00e4t\u00b7zen", ",", "wo", "Ath\u00b7le\u00b7ten", "rin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PWAV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wird alles dunkler schon und ungenau.", "tokens": ["Wird", "al\u00b7les", "dunk\u00b7ler", "schon", "und", "un\u00b7ge\u00b7nau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJA", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Leierkasten heult und K\u00fcchenm\u00e4dchen singen.", "tokens": ["Ein", "Lei\u00b7er\u00b7kas\u00b7ten", "heult", "und", "K\u00fc\u00b7chen\u00b7m\u00e4d\u00b7chen", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Mann zertr\u00fcmmert eine morsche Frau.", "tokens": ["Ein", "Mann", "zer\u00b7tr\u00fcm\u00b7mert", "ei\u00b7ne", "mor\u00b7sche", "Frau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}