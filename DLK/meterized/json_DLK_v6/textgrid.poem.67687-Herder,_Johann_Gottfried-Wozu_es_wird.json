{"textgrid.poem.67687": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Wozu es wird", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Himmelstropfe sank ins Meer;", "tokens": ["Ein", "Him\u00b7mel\u00b7strop\u00b7fe", "sank", "ins", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schnell schwamm die Perlenmutter her", "tokens": ["Schnell", "schwamm", "die", "Per\u00b7len\u00b7mut\u00b7ter", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und trank ihn auf. Das Tr\u00f6pfchen Thau", "tokens": ["Und", "trank", "ihn", "auf", ".", "Das", "Tr\u00f6pf\u00b7chen", "Thau"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ward eine Perle, silbergrau.", "tokens": ["Ward", "ei\u00b7ne", "Per\u00b7le", ",", "sil\u00b7ber\u00b7grau", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein ganzer Himmelsregen schwamm", "tokens": ["Ein", "gan\u00b7zer", "Him\u00b7mels\u00b7re\u00b7gen", "schwamm"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf eines faulen Baumes Stamm,", "tokens": ["Auf", "ei\u00b7nes", "fau\u00b7len", "Bau\u00b7mes", "Stamm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der gierig ein ihn schlang.", "tokens": ["Der", "gie\u00b7rig", "ein", "ihn", "schlang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Was ward daraus? Aus ihm entsprang", "tokens": ["Was", "ward", "da\u00b7raus", "?", "Aus", "ihm", "ent\u00b7sprang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PAV", "$.", "APPR", "PPER", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Ein gelber, gift'ger Schwamm.", "tokens": ["Ein", "gel\u00b7ber", ",", "gift'\u00b7ger", "Schwamm", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Himmelstropfe ist das Evangelium;", "tokens": ["Der", "Him\u00b7mel\u00b7strop\u00b7fe", "ist", "das", "E\u00b7van\u00b7ge\u00b7li\u00b7um", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Himmelsregen ist das weite Christenthum;", "tokens": ["Der", "Him\u00b7mels\u00b7re\u00b7gen", "ist", "das", "wei\u00b7te", "Chris\u00b7ten\u00b7thum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es ist mit seiner stillen Pracht,", "tokens": ["Es", "ist", "mit", "sei\u00b7ner", "stil\u00b7len", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wozu es wird, wozu man's macht.", "tokens": ["Wo\u00b7zu", "es", "wird", ",", "wo\u00b7zu", "man's", "macht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}