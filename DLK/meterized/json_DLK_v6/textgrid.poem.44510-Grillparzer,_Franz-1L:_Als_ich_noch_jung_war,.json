{"textgrid.poem.44510": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als ich noch jung war,", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich noch jung war,", "tokens": ["Als", "ich", "noch", "jung", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Liebt ich zu klagen,", "tokens": ["Liebt", "ich", "zu", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "All, was dem Herzen leid,", "tokens": ["All", ",", "was", "dem", "Her\u00b7zen", "leid", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vielen zu sagen;", "tokens": ["Vie\u00b7len", "zu", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Nun da ich \u00e4lter,", "tokens": ["Nun", "da", "ich", "\u00e4l\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Hehl ich die Pein,", "tokens": ["Hehl", "ich", "die", "Pein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Schlie\u00dfe den Kummer", "tokens": ["Schlie\u00b7\u00dfe", "den", "Kum\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Im Innersten ein.", "tokens": ["Im", "In\u00b7ners\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Denn ich erfuhr es,", "tokens": ["Denn", "ich", "er\u00b7fuhr", "es", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Kalt ist die Welt,", "tokens": ["Kalt", "ist", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und nur der Anteil", "tokens": ["Und", "nur", "der", "An\u00b7teil"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Lindert, was qu\u00e4lt.", "tokens": ["Lin\u00b7dert", ",", "was", "qu\u00e4lt", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "So wie das V\u00f6glein,", "tokens": ["So", "wie", "das", "V\u00f6\u00b7glein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Jedermann kennts,", "tokens": ["Je\u00b7der\u00b7mann", "kennts", ","], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Das seine Liebe", "tokens": ["Das", "sei\u00b7ne", "Lie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["PDS", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Fl\u00f6tet im Lenz,", "tokens": ["Fl\u00f6\u00b7tet", "im", "Lenz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Aber vor\u00fcber", "tokens": ["A\u00b7ber", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Rosen und Brut,", "tokens": ["Ro\u00b7sen", "und", "Brut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Lautlos in Zweigen", "tokens": ["Laut\u00b7los", "in", "Zwei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPR", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "F\u00fcrder nur ruht:", "tokens": ["F\u00fcr\u00b7der", "nur", "ruht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "So meine Muse,", "tokens": ["So", "mei\u00b7ne", "Mu\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Also mein Herz,", "tokens": ["Al\u00b7so", "mein", "Herz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "War doch ihr Lied nur", "tokens": ["War", "doch", "ihr", "Lied", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "ADV"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Sehnsucht und Schmerz.", "tokens": ["Sehn\u00b7sucht", "und", "Schmerz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}