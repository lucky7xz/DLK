{"dta.poem.21717": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "\u00d6d und leer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Leere des Sinns und der Seele, wie wend' ich,", "tokens": ["Lee\u00b7re", "des", "Sinns", "und", "der", "See\u00b7le", ",", "wie", "wend'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ART", "NN", "$,", "PWAV", "PIS", "PPER", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Wie f\u00fcll' ich dich, \u00e4ngstendes, schauerndes Leer!", "tokens": ["Wie", "f\u00fcll'", "ich", "dich", ",", "\u00e4ngs\u00b7ten\u00b7des", ",", "schau\u00b7ern\u00b7des", "Leer", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Es treibt mich, es jagt mich, es hetzt mich un-", "tokens": ["Es", "treibt", "mich", ",", "es", "jagt", "mich", ",", "es", "hetzt", "mich", "un"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "TRUNC"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Sage mir,Himmel, wonach? Sagt es mir, Fluren", "tokens": ["Sa\u00b7ge", "mir", ",", "Him\u00b7mel", ",", "wo\u00b7nach", "?", "Sagt", "es", "mir", ",", "Flu\u00b7ren"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "PPER", "$,", "NN", "$,", "PWAV", "$.", "VVFIN", "PPER", "PPER", "$,", "NN"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "und Meer!", "tokens": ["und", "Meer", "!"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Nennt mir diess nimmer ers\u00e4ttigte Sehnen!", "tokens": ["Nennt", "mir", "diess", "nim\u00b7mer", "er\u00b7s\u00e4t\u00b7tig\u00b7te", "Seh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "ADV", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.7": {"text": "Nennt mir die w\u00fcrgende Ungeduld!", "tokens": ["Nennt", "mir", "die", "w\u00fcr\u00b7gen\u00b7de", "Un\u00b7ge\u00b7duld", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Deutet mir diese heissst\u00fcrzenden Thr\u00e4nen!", "tokens": ["Deu\u00b7tet", "mir", "die\u00b7se", "heiss\u00b7st\u00fcr\u00b7zen\u00b7den", "Thr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.9": {"text": "Lehrt mich, ach lehrt mich sie s\u00fchnen, die rastlos", "tokens": ["Lehrt", "mich", ",", "ach", "lehrt", "mich", "sie", "s\u00fch\u00b7nen", ",", "die", "rast\u00b7los"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ITJ", "VVFIN", "PPER", "PPER", "VVINF", "$,", "PRELS", "ADJD"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.10": {"text": "verfolgende Schuld!", "tokens": ["ver\u00b7fol\u00b7gen\u00b7de", "Schuld", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Funkelnder ", "tokens": ["Fun\u00b7keln\u00b7der"], "token_info": ["word"], "pos": ["NN"], "meter": "+--", "measure": "dactylic.init"}, "line.12": {"text": "Bergt ihr das Gut, das dem Lechzenden fehlt?", "tokens": ["Bergt", "ihr", "das", "Gut", ",", "das", "dem", "Lech\u00b7zen\u00b7den", "fehlt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.13": {"text": "Ergreift mich, St\u00fcrme der Nacht, und tragt mit", "tokens": ["Er\u00b7greift", "mich", ",", "St\u00fcr\u00b7me", "der", "Nacht", ",", "und", "tragt", "mit"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "NN", "ART", "NN", "$,", "KON", "VVFIN", "APPR"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "gewaltigem Fl\u00fcgel", "tokens": ["ge\u00b7wal\u00b7ti\u00b7gem", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Zu Welten mich empor, wo keine Sehnsucht", "tokens": ["Zu", "Wel\u00b7ten", "mich", "em\u00b7por", ",", "wo", "kei\u00b7ne", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "PPER", "PTKVZ", "$,", "PWAV", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "qu\u00e4lt!", "tokens": ["qu\u00e4lt", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.17": {"text": "Liegt in des Abgrundes Schooss es begraben,", "tokens": ["Liegt", "in", "des", "Ab\u00b7grun\u00b7des", "Scho\u00b7oss", "es", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+--+--+---+-", "measure": "dactylic.tri.plus"}, "line.18": {"text": "Was die schmachtende Seele letzet \u2014 Hinab,", "tokens": ["Was", "die", "schmach\u00b7ten\u00b7de", "See\u00b7le", "let\u00b7zet", "Hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$(", "ADV", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.19": {"text": "Hinab in chaotisches Graun! Hinab, es zu finden,", "tokens": ["Hin\u00b7ab", "in", "chao\u00b7ti\u00b7sches", "Graun", "!", "Hin\u00b7ab", ",", "es", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$.", "ADV", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "zu haben!", "tokens": ["zu", "ha\u00b7ben", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VAINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "Hinab in das grausige klaffende Grab!", "tokens": ["Hin\u00b7ab", "in", "das", "grau\u00b7si\u00b7ge", "klaf\u00b7fen\u00b7de", "Grab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Fasste dein Strudel mich, alte Charybde!", "tokens": ["Fass\u00b7te", "dein", "Stru\u00b7del", "mich", ",", "al\u00b7te", "Cha\u00b7ryb\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Braust' ich mit Ossians Geistern umher!", "tokens": ["Braust'", "ich", "mit", "Os\u00b7si\u00b7ans", "Geis\u00b7tern", "um\u00b7her", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "L\u00f6st' ich, ein B\u00fcsser, mein s\u00fchnend Gel\u00fcbde!", "tokens": ["L\u00f6st'", "ich", ",", "ein", "B\u00fcs\u00b7ser", ",", "mein", "s\u00fch\u00b7nend", "Ge\u00b7l\u00fcb\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "$,", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gl\u00fcht' ich im lybischen Sand! gefr\u00f6r' ich im arkti-", "tokens": ["Gl\u00fcht'", "ich", "im", "ly\u00b7bi\u00b7schen", "Sand", "!", "ge\u00b7fr\u00f6r'", "ich", "im", "ark\u00b7ti"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$.", "VVFIN", "PPER", "APPRART", "TRUNC"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Umstrickten mich nur der Begierden Empusen!", "tokens": ["Um\u00b7strick\u00b7ten", "mich", "nur", "der", "Be\u00b7gier\u00b7den", "Em\u00b7pu\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Sch\u00e4umte mir Wollust dein Kelch! Blinkte mir", "tokens": ["Sch\u00e4um\u00b7te", "mir", "Wol\u00b7lust", "dein", "Kelch", "!", "Blink\u00b7te", "mir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "PPOSAT", "NN", "$.", "VVFIN", "PPER"], "meter": "+--+-+++-+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Rache dein Stahl! \u2014", "tokens": ["Ra\u00b7che", "dein", "Stahl", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "PPOSAT", "NN", "$.", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Aber im Herzen der Frost! die schaudernde \u00d6de im", "tokens": ["A\u00b7ber", "im", "Her\u00b7zen", "der", "Frost", "!", "die", "schau\u00b7dern\u00b7de", "\u00d6\u00b7de", "im"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "$.", "ART", "ADJA", "NN", "APPRART"], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Busen! \u2014 \u2014", "tokens": ["Bu\u00b7sen", "!"], "token_info": ["word", "punct", "punct", "punct"], "pos": ["NN", "$.", "$(", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Acherontische, stygische, folternde Qual!", "tokens": ["A\u00b7cher\u00b7on\u00b7ti\u00b7sche", ",", "sty\u00b7gi\u00b7sche", ",", "fol\u00b7tern\u00b7de", "Qual", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}}, "stanza.3": {"line.1": {"text": "Wohl lockt vom Leukadischen Fels der Sprung", "tokens": ["Wohl", "lockt", "vom", "Leu\u00b7ka\u00b7di\u00b7schen", "Fels", "der", "Sprung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "in die sch\u00e4umenden Fluthen!", "tokens": ["in", "die", "sch\u00e4u\u00b7men\u00b7den", "Flut\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wohl b\u00f6t' ich die Deciusbrust den br\u00fcllenden", "tokens": ["Wohl", "b\u00f6t'", "ich", "die", "De\u00b7cius\u00b7brust", "den", "br\u00fcl\u00b7len\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Schl\u00fcnden der Schlacht!", "tokens": ["Schl\u00fcn\u00b7den", "der", "Schlacht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wohl sch\u00fcrt' ich alcidischen Sinns des Rogus ver-", "tokens": ["Wohl", "sch\u00fcrt'", "ich", "al\u00b7ci\u00b7di\u00b7schen", "Sinns", "des", "Ro\u00b7gus", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "ART", "NE", "TRUNC"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Aber wer harret dahinten? \u2014 \u2014 Erebische ewige", "tokens": ["A\u00b7ber", "wer", "har\u00b7ret", "da\u00b7hin\u00b7ten", "?", "E\u00b7re\u00b7bi\u00b7sche", "e\u00b7wi\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "PAV", "$.", "$(", "$(", "ADJA", "ADJA"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Nacht! \u2014 \u2014", "tokens": ["Nacht", "!"], "token_info": ["word", "punct", "punct", "punct"], "pos": ["NN", "$.", "$(", "$("], "meter": "+", "measure": "single.up"}, "line.8": {"text": "H\u00f6rst du sie rasseln, des Tartarus Ketten?", "tokens": ["H\u00f6rst", "du", "sie", "ras\u00b7seln", ",", "des", "Tar\u00b7ta\u00b7rus", "Ket\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVINF", "$,", "ART", "NN", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.9": {"text": "Siehst du die Larven im Schwefelgefild?", "tokens": ["Siehst", "du", "die", "Lar\u00b7ven", "im", "Schwe\u00b7fel\u00b7ge\u00b7fild", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.10": {"text": "Wohin denn sich bergen, sich fl\u00fcchten, sich retten?", "tokens": ["Wo\u00b7hin", "denn", "sich", "ber\u00b7gen", ",", "sich", "fl\u00fcch\u00b7ten", ",", "sich", "ret\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PRF", "VVINF", "$,", "PRF", "VVFIN", "$,", "PRF", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.11": {"text": "Dem keine Lethe rinnt, dem kein Nepenthe quillt!", "tokens": ["Dem", "kei\u00b7ne", "Le\u00b7the", "rinnt", ",", "dem", "kein", "Ne\u00b7pen\u00b7the", "quillt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+++-+", "measure": "unknown.measure.septa"}}}}}