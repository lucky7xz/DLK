{"dta.poem.2637": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mexicanischer Lebens-Baum/  \n Bey Beerdigung Fr. E. W. g. K. abgebildet/  \n den 12. Junii 1672.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jndem/ Hochw\u00fcrdiger/ sein Hau\u00df geht in der Klage/", "tokens": ["In\u00b7dem", "/", "Hoch\u00b7w\u00fcr\u00b7di\u00b7ger", "/", "sein", "Hau\u00df", "geht", "in", "der", "Kla\u00b7ge", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "NN", "$(", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "+-+-++-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Er selbst in Flor und Boy sich itzt verh\u00fcllen mu\u00df/", "tokens": ["Er", "selbst", "in", "Flor", "und", "Boy", "sich", "itzt", "ver\u00b7h\u00fcl\u00b7len", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "KON", "NE", "PRF", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "So \u00fcber ihn bestimmt de\u00df Allerh\u00f6chsten Schlu\u00df/", "tokens": ["So", "\u00fc\u00b7ber", "ihn", "be\u00b7stimmt", "de\u00df", "Al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "Schlu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Will ich den ", "tokens": ["Will", "ich", "den"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Den Neu Hispanien und Mexico geb\u00fcrt/", "tokens": ["Den", "Neu", "His\u00b7pa\u00b7ni\u00b7en", "und", "Me\u00b7xi\u00b7co", "ge\u00b7b\u00fcrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und dessen Eigenschasst und Namen zu erzehlen/", "tokens": ["Und", "des\u00b7sen", "Ei\u00b7gen\u00b7schasst", "und", "Na\u00b7men", "zu", "er\u00b7zeh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "KON", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den er in frembder Sprach Kokochiati f\u00fchrt.", "tokens": ["Den", "er", "in", "fremb\u00b7der", "Sprach", "Ko\u00b7ko\u00b7ch\u00b7i\u00b7a\u00b7ti", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "NE", "VVFIN", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Wenn eines Menschen Hand die Bl\u00e4tter nur betastet/", "tokens": ["Wenn", "ei\u00b7nes", "Men\u00b7schen", "Hand", "die", "Bl\u00e4t\u00b7ter", "nur", "be\u00b7tas\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So stehn sie wie betr\u00fcbt/ und in den Schlaff gesetzt.", "tokens": ["So", "stehn", "sie", "wie", "be\u00b7tr\u00fcbt", "/", "und", "in", "den", "Schlaff", "ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "VVPP", "$(", "KON", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Geschichts zum andernmal/ so wird der Baum entlastet/", "tokens": ["Ge\u00b7schichts", "zum", "an\u00b7dern\u00b7mal", "/", "so", "wird", "der", "Baum", "ent\u00b7las\u00b7tet", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADV", "$(", "ADV", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Von seiner Bl\u00e4tter Rey/ und scheint als wie verletzt.", "tokens": ["Von", "sei\u00b7ner", "Bl\u00e4t\u00b7ter", "Rey", "/", "und", "scheint", "als", "wie", "ver\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$(", "KON", "VVFIN", "KOUS", "KOKOM", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Erk\u00fchnt man ferner sich ein St\u00fcck darvon zu schneiden/", "tokens": ["Er\u00b7k\u00fchnt", "man", "fer\u00b7ner", "sich", "ein", "St\u00fcck", "dar\u00b7von", "zu", "schnei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PRF", "ART", "NN", "PAV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wird der ber\u00fchrte Ort gantz schwartz/ als wie verbrand/", "tokens": ["Wird", "der", "be\u00b7r\u00fchr\u00b7te", "Ort", "gantz", "schwartz", "/", "als", "wie", "ver\u00b7brand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "$(", "KOUS", "KOKOM", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und mehr/ so kan er nicht des Menschen Athem leiden/", "tokens": ["Und", "mehr", "/", "so", "kan", "er", "nicht", "des", "Men\u00b7schen", "A\u00b7them", "lei\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "ADV", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Er sch\u00fcttelt augenblicks die Bl\u00e4tter in den Sand.", "tokens": ["Er", "sch\u00fct\u00b7telt", "au\u00b7gen\u00b7blicks", "die", "Bl\u00e4t\u00b7ter", "in", "den", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Doch in Gujana siht ihn eine Viertelstunde/", "tokens": ["Doch", "in", "Gu\u00b7ja\u00b7na", "siht", "ihn", "ei\u00b7ne", "Vier\u00b7tel\u00b7stun\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Vom neuen wieder bl\u00fchn/ und gleichsam lebend seyn.", "tokens": ["Vom", "neu\u00b7en", "wie\u00b7der", "bl\u00fchn", "/", "und", "gleich\u00b7sam", "le\u00b7bend", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADV", "VVINF", "$(", "KON", "ADJD", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Steht unser Leben nicht auf eben solchem Grunde?", "tokens": ["Steht", "un\u00b7ser", "Le\u00b7ben", "nicht", "auf", "e\u00b7ben", "sol\u00b7chem", "Grun\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "APPR", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und trifft mit diesem Baum sein Eigenschafft nicht ein?", "tokens": ["Und", "trifft", "mit", "die\u00b7sem", "Baum", "sein", "Ei\u00b7gen\u00b7schafft", "nicht", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PDAT", "NN", "PPOSAT", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Hier macht des Menschen Hand da\u00df Bl\u00fcth und Bl\u00e4tter fallen:", "tokens": ["Hier", "macht", "des", "Men\u00b7schen", "Hand", "da\u00df", "Bl\u00fcth", "und", "Bl\u00e4t\u00b7ter", "fal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "KOUS", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Mit uns de\u00df Todes-Arm dergleichen Trauer-Spiel.", "tokens": ["Mit", "uns", "de\u00df", "To\u00b7des\u00b7Arm", "derg\u00b7lei\u00b7chen", "Trau\u00b7e\u00b7rSpiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "PIS", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die au\u00dfgestreckte Zeit/ die wir auff Erden wallen/", "tokens": ["Die", "au\u00df\u00b7ge\u00b7streck\u00b7te", "Zeit", "/", "die", "wir", "auff", "Er\u00b7den", "wal\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PRELS", "PPER", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Was ist sie? als nur blo\u00df der Eitelkeiten Ziel.", "tokens": ["Was", "ist", "sie", "?", "als", "nur", "blo\u00df", "der", "Ei\u00b7tel\u00b7kei\u00b7ten", "Ziel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$.", "KOUS", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wir tr\u00e4umen wie im Schlaff/ und irren mit Gedancken/", "tokens": ["Wir", "tr\u00e4u\u00b7men", "wie", "im", "Schlaff", "/", "und", "ir\u00b7ren", "mit", "Ge\u00b7dan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "APPRART", "NN", "$(", "KON", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Sind wie ein brennend Licht/ das selber sich verzehrt/", "tokens": ["Sind", "wie", "ein", "bren\u00b7nend", "Licht", "/", "das", "sel\u00b7ber", "sich", "ver\u00b7zehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "ADJD", "NN", "$(", "PDS", "ADV", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Hier wird durch K\u00fcmmern\u00fcs/ und dort durch stetes krancken/", "tokens": ["Hier", "wird", "durch", "K\u00fcm\u00b7mer\u00b7n\u00fcs", "/", "und", "dort", "durch", "ste\u00b7tes", "kran\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NE", "$(", "KON", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Fast St\u00fcckwei\u00df unser Leib in Asch und Grau\u00df gekehrt.", "tokens": ["Fast", "St\u00fcck\u00b7wei\u00df", "un\u00b7ser", "Leib", "in", "Asch", "und", "Grau\u00df", "ge\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPOSAT", "NN", "APPR", "NE", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Jedwede Stunde tr\u00e4gt auch was von uns zu Grabe/", "tokens": ["Jed\u00b7we\u00b7de", "Stun\u00b7de", "tr\u00e4gt", "auch", "was", "von", "uns", "zu", "Gra\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "PWS", "APPR", "PPER", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wir sterben da nicht erst/ wenn uns der Tod bef\u00e4llt/", "tokens": ["Wir", "ster\u00b7ben", "da", "nicht", "erst", "/", "wenn", "uns", "der", "Tod", "be\u00b7f\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ADV", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wann den entseelten Leib frist F\u00e4ulnis/ Wurm und Schabe/", "tokens": ["Wann", "den", "ent\u00b7seel\u00b7ten", "Leib", "frist", "F\u00e4ul\u00b7nis", "/", "Wurm", "und", "Scha\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADJD", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Nein; unsern Wiegen wird der Sarg schon beygesellt.", "tokens": ["Nein", ";", "un\u00b7sern", "Wie\u00b7gen", "wird", "der", "Sarg", "schon", "bey\u00b7ge\u00b7sellt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPOSAT", "NN", "VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.34": {"text": "Uns schw\u00e4rtzt der S\u00fcnden Nacht den nackten Mohren gleich.", "tokens": ["Uns", "schw\u00e4rtzt", "der", "S\u00fcn\u00b7den", "Nacht", "den", "nack\u00b7ten", "Moh\u00b7ren", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Mehr/ jener hat den Hauch der Menschen nicht erlitten:", "tokens": ["Mehr", "/", "je\u00b7ner", "hat", "den", "Hauch", "der", "Men\u00b7schen", "nicht", "er\u00b7lit\u00b7ten", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "$(", "PDS", "VAFIN", "ART", "NN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wir nicht die Warnungen aus GOttes Gnaden-Reich.", "tokens": ["Wir", "nicht", "die", "War\u00b7nun\u00b7gen", "aus", "Got\u00b7tes", "Gna\u00b7den\u00b7Reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ART", "NN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "So giebt der ", "tokens": ["So", "giebt", "der"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.38": {"text": "Sein st\u00fcndlich sterbend Stamm mahlt uns den Tod nur f\u00fcr:", "tokens": ["Sein", "st\u00fcnd\u00b7lich", "ster\u00b7bend", "Stamm", "mahlt", "uns", "den", "Tod", "nur", "f\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJD", "NN", "VVFIN", "PPER", "ART", "NN", "ADV", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "So vieler Bl\u00e4tter tr\u00e4gt/ so viel legt er zur\u00fccke/", "tokens": ["So", "vie\u00b7ler", "Bl\u00e4t\u00b7ter", "tr\u00e4gt", "/", "so", "viel", "legt", "er", "zu\u00b7r\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$(", "ADV", "ADV", "VVFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und wirfft wie Kleider weg des kalten Laubes Zier.", "tokens": ["Und", "wirfft", "wie", "Klei\u00b7der", "weg", "des", "kal\u00b7ten", "Lau\u00b7bes", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "NN", "ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Nichts anders gehts mit uns: Das Wonhau\u00df dieser Erden/", "tokens": ["Nichts", "an\u00b7ders", "gehts", "mit", "uns", ":", "Das", "Won\u00b7hau\u00df", "die\u00b7ser", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVFIN", "APPR", "PPER", "$.", "ART", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Wie gro\u00df und weit es ist/ steckt voller Roth und Tod/", "tokens": ["Wie", "gro\u00df", "und", "weit", "es", "ist", "/", "steckt", "vol\u00b7ler", "Roth", "und", "Tod", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD",