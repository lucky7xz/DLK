{"textgrid.poem.40970": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Mitternachtsszene", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "V\u00f6gel, die mit Wolken schifften,", "tokens": ["V\u00f6\u00b7gel", ",", "die", "mit", "Wol\u00b7ken", "schiff\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sanken in der W\u00e4lder Nacht,", "tokens": ["San\u00b7ken", "in", "der", "W\u00e4l\u00b7der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlummer liegt auf Wald und Triften,", "tokens": ["Schlum\u00b7mer", "liegt", "auf", "Wald", "und", "Trif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einsam nur der Hirte wacht.", "tokens": ["Ein\u00b7sam", "nur", "der", "Hir\u00b7te", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Freude macht es mir, zu lauschen,", "tokens": ["Freu\u00b7de", "macht", "es", "mir", ",", "zu", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie sich regt ein L\u00fcftchen dort,", "tokens": ["Wie", "sich", "regt", "ein", "L\u00fcft\u00b7chen", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie vom Baume Bl\u00e4tter rauschen", "tokens": ["Wie", "vom", "Bau\u00b7me", "Bl\u00e4t\u00b7ter", "rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ein B\u00e4chlein rieselt fort.", "tokens": ["Und", "ein", "B\u00e4ch\u00b7lein", "rie\u00b7selt", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Durch des Himmels Wolkenh\u00fclle", "tokens": ["Durch", "des", "Him\u00b7mels", "Wol\u00b7ken\u00b7h\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leise jetzt der Vollmond dringt,", "tokens": ["Lei\u00b7se", "jetzt", "der", "Voll\u00b7mond", "dringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und nun pl\u00f6tzlich in die stille", "tokens": ["Und", "nun", "pl\u00f6tz\u00b7lich", "in", "die", "stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mitternacht die Glocke singt.", "tokens": ["Mit\u00b7ter\u00b7nacht", "die", "Glo\u00b7cke", "singt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Weckest mich aus s\u00fc\u00dfen Tr\u00e4umen,", "tokens": ["We\u00b7ckest", "mich", "aus", "s\u00fc\u00b7\u00dfen", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alte Glocke! S\u00e4ngerin!", "tokens": ["Al\u00b7te", "Glo\u00b7cke", "!", "S\u00e4n\u00b7ge\u00b7rin", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich rufe nach den R\u00e4umen", "tokens": ["Und", "ich", "ru\u00b7fe", "nach", "den", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blauen Himmels zu dir hin:", "tokens": ["Blau\u00b7en", "Him\u00b7mels", "zu", "dir", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbtausende, die in den Hallen", "tokens": ["\u00bb", "tau\u00b7sen\u00b7de", ",", "die", "in", "den", "Hal\u00b7len"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADJA", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Lichten Tages laut gelebt,", "tokens": ["Lich\u00b7ten", "Ta\u00b7ges", "laut", "ge\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tausende von Nachtigallen,", "tokens": ["Tau\u00b7sen\u00b7de", "von", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die mit Sang die Nacht durchschwebt,", "tokens": ["Die", "mit", "Sang", "die", "Nacht", "durch\u00b7schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Schwanden aus des Lichtes Reichen,", "tokens": ["Schwan\u00b7den", "aus", "des", "Lich\u00b7tes", "Rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schweigen stumm, im Tod verbl\u00fcht,", "tokens": ["Schwei\u00b7gen", "stumm", ",", "im", "Tod", "ver\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du doch \u00fcber all den Leichen", "tokens": ["Du", "doch", "\u00fc\u00b7ber", "all", "den", "Lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "PIAT", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Singest fort das alte Lied.", "tokens": ["Sin\u00b7gest", "fort", "das", "al\u00b7te", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Erdens\u00e4nger kurz nur singen,", "tokens": ["Er\u00b7den\u00b7s\u00e4n\u00b7ger", "kurz", "nur", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bald zerrei\u00dft der Gram ihr Herz,", "tokens": ["Bald", "zer\u00b7rei\u00dft", "der", "Gram", "ihr", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Glocke! w\u00fcrdest du zerspringen,", "tokens": ["Glo\u00b7cke", "!", "w\u00fcr\u00b7dest", "du", "zer\u00b7sprin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Macht es nicht der Erde Schmerz.", "tokens": ["Macht", "es", "nicht", "der", "Er\u00b7de", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Denn du singst, ob Lust, ob Jammer,", "tokens": ["Denn", "du", "singst", ",", "ob", "Lust", ",", "ob", "Jam\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "NN", "$,", "KOUS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gleichen Ton stets durch die Luft,", "tokens": ["Glei\u00b7chen", "Ton", "stets", "durch", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob der Schlag von deinem Hammer", "tokens": ["Ob", "der", "Schlag", "von", "dei\u00b7nem", "Ham\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Br\u00e4ute oder Leichen ruft.", "tokens": ["Br\u00e4u\u00b7te", "o\u00b7der", "Lei\u00b7chen", "ruft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Und du, Mond! aus gleichen Erzen,", "tokens": ["Und", "du", ",", "Mond", "!", "aus", "glei\u00b7chen", "Er\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NN", "$.", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00c4nderst nie dein Angesicht,", "tokens": ["\u00c4n\u00b7derst", "nie", "dein", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob auch Tausende von Herzen", "tokens": ["Ob", "auch", "Tau\u00b7sen\u00b7de", "von", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unten bittres Leid zerbricht.", "tokens": ["Un\u00b7ten", "bitt\u00b7res", "Leid", "zer\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Glocke! singe! schwebt, Gestirne,", "tokens": ["Glo\u00b7cke", "!", "sin\u00b7ge", "!", "schwebt", ",", "Ge\u00b7stir\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "$.", "VVFIN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob der Erde Lust und Grab!", "tokens": ["Ob", "der", "Er\u00b7de", "Lust", "und", "Grab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hoch ob euch auf ew'ger Firne", "tokens": ["Hoch", "ob", "euch", "auf", "ew'\u00b7ger", "Fir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Schwingt ein Gott den Hirtenstab.\u00ab", "tokens": ["Schwingt", "ein", "Gott", "den", "Hir\u00b7ten\u00b7stab", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}