{"textgrid.poem.48227": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Es kribbelt und wibbelt weiter", "genre": "verse", "period": "N.A.", "pub_year": 1886, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Flut steigt bis an den Ararat,", "tokens": ["Die", "Flut", "steigt", "bis", "an", "den", "Ar\u00b7a\u00b7rat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und es hilft keine Rettungsleiter,", "tokens": ["Und", "es", "hilft", "kei\u00b7ne", "Ret\u00b7tungs\u00b7lei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da bringt die Taube Zweig und Blatt \u2013", "tokens": ["Da", "bringt", "die", "Tau\u00b7be", "Zweig", "und", "Blatt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und es kribbelt und wibbelt weiter.", "tokens": ["Und", "es", "krib\u00b7belt", "und", "wib\u00b7belt", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Es sicheln und m\u00e4hen von Ost nach West", "tokens": ["Es", "si\u00b7cheln", "und", "m\u00e4\u00b7hen", "von", "Ost", "nach", "West"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "ADJA", "APPR", "NE", "APPR", "NE"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die apokalyptischen Reiter,", "tokens": ["Die", "a\u00b7pok\u00b7a\u00b7lypt\u00b7i\u00b7schen", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Aber ob Hunger, ob Krieg, ob Pest,", "tokens": ["A\u00b7ber", "ob", "Hun\u00b7ger", ",", "ob", "Krieg", ",", "ob", "Pest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "$,", "KOUS", "NN", "$,", "KOUS", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Es kribbelt und wibbelt weiter.", "tokens": ["Es", "krib\u00b7belt", "und", "wib\u00b7belt", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ein Gott wird gekreuzigt auf Golgatha,", "tokens": ["Ein", "Gott", "wird", "ge\u00b7kreu\u00b7zigt", "auf", "Gol\u00b7ga\u00b7tha", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPR", "NE", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es brennen Millionen Scheiter,", "tokens": ["Es", "bren\u00b7nen", "Mil\u00b7lion\u00b7en", "Schei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "M\u00e4rtyrer hier und Hexen da,", "tokens": ["M\u00e4r\u00b7ty\u00b7rer", "hier", "und", "He\u00b7xen", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "NN", "PTKVZ", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Doch es kribbelt und wibbelt weiter.", "tokens": ["Doch", "es", "krib\u00b7belt", "und", "wib\u00b7belt", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "So banne dein Ich in dich zur\u00fcck", "tokens": ["So", "ban\u00b7ne", "dein", "Ich", "in", "dich", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "PPER", "APPR", "PPER", "PTKVZ"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und ergib dich und sei heiter;", "tokens": ["Und", "er\u00b7gib", "dich", "und", "sei", "hei\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "KON", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was liegt an dir und deinem Gl\u00fcck?", "tokens": ["Was", "liegt", "an", "dir", "und", "dei\u00b7nem", "Gl\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es kribbelt und wibbelt weiter.", "tokens": ["Es", "krib\u00b7belt", "und", "wib\u00b7belt", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}