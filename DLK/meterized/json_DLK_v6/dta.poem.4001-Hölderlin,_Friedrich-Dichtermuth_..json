{"dta.poem.4001": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Dichtermuth .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Sind denn Dir nicht verwandt alle Lebendigen?", "tokens": ["Sind", "denn", "Dir", "nicht", "ver\u00b7wandt", "al\u00b7le", "Le\u00b7ben\u00b7di\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "PTKNEG", "ADJD", "PIAT", "NN", "$."], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "N\u00e4hrt zum Dienste denn nicht selber die Parze", "tokens": ["N\u00e4hrt", "zum", "Diens\u00b7te", "denn", "nicht", "sel\u00b7ber", "die", "Par\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "ADV", "PTKNEG", "ADV", "ART", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Dich?", "tokens": ["Dich", "?"], "token_info": ["word", "punct"], "pos": ["PPER", "$."], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Drum! so wandle nur wehrlos", "tokens": ["Drum", "!", "so", "wand\u00b7le", "nur", "wehr\u00b7los"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "$.", "ADV", "VVFIN", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fort durch's Leben und sorge nicht!", "tokens": ["Fort", "durch's", "Le\u00b7ben", "und", "sor\u00b7ge", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "KON", "NN", "PTKNEG", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Was geschiehet, es sey alles gesegnet Dir,", "tokens": ["Was", "ge\u00b7schie\u00b7het", ",", "es", "sey", "al\u00b7les", "ge\u00b7seg\u00b7net", "Dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PPER", "VAFIN", "PIS", "VVFIN", "PPER", "$,"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Sey zur Freude gewandt! oder was k\u00f6nnte denn", "tokens": ["Sey", "zur", "Freu\u00b7de", "ge\u00b7wandt", "!", "o\u00b7der", "was", "k\u00f6nn\u00b7te", "denn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "NN", "VVPP", "$.", "KON", "PWS", "VMFIN", "ADV"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Dich beleidigen, Herz! was", "tokens": ["Dich", "be\u00b7lei\u00b7di\u00b7gen", ",", "Herz", "!", "was"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "NN", "$.", "PWS"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Da begegnen, wohin du sollst?", "tokens": ["Da", "be\u00b7geg\u00b7nen", ",", "wo\u00b7hin", "du", "sollst", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Dann, wie still am Gestad, oder in silberner", "tokens": ["Dann", ",", "wie", "still", "am", "Ge\u00b7stad", ",", "o\u00b7der", "in", "sil\u00b7ber\u00b7ner"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ADJD", "APPRART", "NN", "$,", "KON", "APPR", "NN"], "meter": "--+--+-+-+--", "measure": "anapaest.di.plus"}, "line.2": {"text": "Fernhint\u00f6nender Flut, oder auf schweigenden", "tokens": ["Fern\u00b7hin\u00b7t\u00f6\u00b7nen\u00b7der", "Flut", ",", "o\u00b7der", "auf", "schwei\u00b7gen\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "APPR", "ADJA"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wassertiefen der leichte", "tokens": ["Was\u00b7ser\u00b7tie\u00b7fen", "der", "leich\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "ADJA"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Schwimmer wandelt, so sind auch wir,", "tokens": ["Schwim\u00b7mer", "wan\u00b7delt", ",", "so", "sind", "auch", "wir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADV", "VAFIN", "ADV", "PPER", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Wir, die Dichter des Volks, gerne wo Lebendes", "tokens": ["Wir", ",", "die", "Dich\u00b7ter", "des", "Volks", ",", "ger\u00b7ne", "wo", "Le\u00b7ben\u00b7des"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "NN", "ART", "NN", "$,", "ADV", "PWAV", "NN"], "meter": "--+--+---+--", "measure": "anapaest.di.plus"}, "line.2": {"text": "Um uns athmet und wallt, freudig, und Jedem", "tokens": ["Um", "uns", "ath\u00b7met", "und", "wallt", ",", "freu\u00b7dig", ",", "und", "Je\u00b7dem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUI", "PPER", "VVFIN", "KON", "VVFIN", "$,", "ADJD", "$,", "KON", "PIAT"], "meter": "--+--++--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "hold,", "tokens": ["hold", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Jedoch trauend, wie s\u00e4ngen", "tokens": ["Je\u00b7doch", "trau\u00b7end", ",", "wie", "s\u00e4n\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "PWAV", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Sonst wir Jedem den eignen Gott?", "tokens": ["Sonst", "wir", "Je\u00b7dem", "den", "eig\u00b7nen", "Gott", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PIAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn die Woge denn auch Einen der Muthigen,", "tokens": ["Wenn", "die", "Wo\u00b7ge", "denn", "auch", "Ei\u00b7nen", "der", "Mut\u00b7hi\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "KON", "ADV", "ART", "ART", "NN", "$,"], "meter": "+-+-+-+--++-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Wo er treulich getraut, schmeichlend hinunter", "tokens": ["Wo", "er", "treu\u00b7lich", "ge\u00b7traut", ",", "schmeich\u00b7lend", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "VVPP", "$,", "ADJD", "PTKVZ"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "zieht,", "tokens": ["zieht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Und die Stimmen des S\u00e4ngers", "tokens": ["Und", "die", "Stim\u00b7men", "des", "S\u00e4n\u00b7gers"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Nun in blauender Halle schweigt;", "tokens": ["Nun", "in", "blau\u00b7en\u00b7der", "Hal\u00b7le", "schweigt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Freudig starb er und noch klagen die Einsamen,", "tokens": ["Freu\u00b7dig", "starb", "er", "und", "noch", "kla\u00b7gen", "die", "Ein\u00b7sa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-++--", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Seine Haine, den Fall ihres Geliebtesten;", "tokens": ["Sei\u00b7ne", "Hai\u00b7ne", ",", "den", "Fall", "ih\u00b7res", "Ge\u00b7lieb\u00b7tes\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Oefters t\u00f6net der Jungfrau", "tokens": ["Oef\u00b7ters", "t\u00f6\u00b7net", "der", "Jung\u00b7frau"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vom Gezweige sein freundlich Lied.", "tokens": ["Vom", "Ge\u00b7zwei\u00b7ge", "sein", "freund\u00b7lich", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "ADJD", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Wenn des Abends vorbei Einer der Unsern k\u00f6mmt,", "tokens": ["Wenn", "des", "A\u00b7bends", "vor\u00b7bei", "Ei\u00b7ner", "der", "Un\u00b7sern", "k\u00f6mmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Wo der Bruder ihm sank, denket er Manches", "tokens": ["Wo", "der", "Bru\u00b7der", "ihm", "sank", ",", "den\u00b7ket", "er", "Man\u00b7ches"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PIS"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "wohl", "tokens": ["wohl"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "An der warnenden Stelle,", "tokens": ["An", "der", "war\u00b7nen\u00b7den", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Schweigt und gehet getr\u00f6steter.", "tokens": ["Schweigt", "und", "ge\u00b7het", "ge\u00b7tr\u00f6s\u00b7te\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}