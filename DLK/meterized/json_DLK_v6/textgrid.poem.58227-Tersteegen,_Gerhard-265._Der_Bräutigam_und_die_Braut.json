{"textgrid.poem.58227": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "265. Der Br\u00e4utigam und die Braut", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meines Br\u00e4ut'gams Auge schaut", "tokens": ["Mei\u00b7nes", "Br\u00e4ut'\u00b7gams", "Au\u00b7ge", "schaut"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Stets auf mich als seine Braut,", "tokens": ["Stets", "auf", "mich", "als", "sei\u00b7ne", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "KOUS", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zeigt mir's, wo die Makel stecken.", "tokens": ["Zeigt", "mir's", ",", "wo", "die", "Ma\u00b7kel", "ste\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Er, mein Freund, ist selbst so sch\u00f6n,", "tokens": ["Er", ",", "mein", "Freund", ",", "ist", "selbst", "so", "sch\u00f6n", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Drum will er auch mich gern sehn", "tokens": ["Drum", "will", "er", "auch", "mich", "gern", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "PPER", "ADV", "VVINF"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ohne Runzeln, ohne Flecken.", "tokens": ["Oh\u00b7ne", "Run\u00b7zeln", ",", "oh\u00b7ne", "Fle\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}