{"textgrid.poem.25483": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das R\u00e4thsel, Freund, err\u00e4th sich bald,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das R\u00e4thsel, Freund, err\u00e4th sich bald,", "tokens": ["Das", "R\u00e4th\u00b7sel", ",", "Freund", ",", "er\u00b7r\u00e4\u00b7th", "sich", "bald", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Warum ich nichts, dem Scheine nach, empfinde,", "tokens": ["Wa\u00b7rum", "ich", "nichts", ",", "dem", "Schei\u00b7ne", "nach", ",", "emp\u00b7fin\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "$,", "ART", "NN", "PTKVZ", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und unter angenommener Gestalt,", "tokens": ["Und", "un\u00b7ter", "an\u00b7ge\u00b7nom\u00b7me\u00b7ner", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.4": {"text": "Dem Proteus gleich, gerade dann verschwinde,", "tokens": ["Dem", "Pro\u00b7teus", "gleich", ",", "ge\u00b7ra\u00b7de", "dann", "ver\u00b7schwin\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ADV", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wenn Lallagen, dem guten, sanften Kinde,", "tokens": ["Wenn", "Lal\u00b7la\u00b7gen", ",", "dem", "gu\u00b7ten", ",", "sanf\u00b7ten", "Kin\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von Hochgef\u00fchl das Herzchen \u00fcberwallt.", "tokens": ["Von", "Hoch\u00b7ge\u00b7f\u00fchl", "das", "Herz\u00b7chen", "\u00fc\u00b7berw\u00b7allt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ja freilich wird man durch das Herz", "tokens": ["Ja", "frei\u00b7lich", "wird", "man", "durch", "das", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "VAFIN", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Geselliger, zum Leben viel geschickter,", "tokens": ["Ge\u00b7sel\u00b7li\u00b7ger", ",", "zum", "Le\u00b7ben", "viel", "ge\u00b7schick\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Als blo\u00df durch Witz, Verstand und Scherz;", "tokens": ["Als", "blo\u00df", "durch", "Witz", ",", "Ver\u00b7stand", "und", "Scherz", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Denn jenes macht uns selbst und unsern Freund begl\u00fcckter.", "tokens": ["Denn", "je\u00b7nes", "macht", "uns", "selbst", "und", "un\u00b7sern", "Freund", "be\u00b7gl\u00fcck\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "ADV", "KON", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Doch m\u00fcssen denn nicht zwei bei jedem Handel seyn?", "tokens": ["Doch", "m\u00fcs\u00b7sen", "denn", "nicht", "zwei", "bei", "je\u00b7dem", "Han\u00b7del", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PTKNEG", "CARD", "APPR", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ihr Herz, ist Wachs; das meine, Eisenstein.", "tokens": ["Ihr", "Herz", ",", "ist", "Wachs", ";", "das", "mei\u00b7ne", ",", "Ei\u00b7sen\u00b7stein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "NN", "$.", "PDS", "VVFIN", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Kaum wird bei mir die Oberfl\u00e4che ritzen,", "tokens": ["Kaum", "wird", "bei", "mir", "die", "O\u00b7berf\u00b7l\u00e4\u00b7che", "rit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Was schnell bei Lallagen tief in das Innre dringt;", "tokens": ["Was", "schnell", "bei", "Lal\u00b7la\u00b7gen", "tief", "in", "das", "Inn\u00b7re", "dringt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPR", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Sie f\u00fchlet schon der Rosenbl\u00e4tter Spitzen,", "tokens": ["Sie", "f\u00fch\u00b7let", "schon", "der", "Ro\u00b7sen\u00b7bl\u00e4t\u00b7ter", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Wenn selbst der Dorn des Stiels mich kaum zu Seufzern zwingt.", "tokens": ["Wenn", "selbst", "der", "Dorn", "des", "Stiels", "mich", "kaum", "zu", "Seuf\u00b7zern", "zwingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Zwei solche Herzen schmelzen nie zusammen;", "tokens": ["Zwei", "sol\u00b7che", "Her\u00b7zen", "schmel\u00b7zen", "nie", "zu\u00b7sam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "PIAT", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Verzehrt ist l\u00e4ngst das eine von den Flammen,", "tokens": ["Ver\u00b7zehrt", "ist", "l\u00e4ngst", "das", "ei\u00b7ne", "von", "den", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "ART", "ART", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Wenn's andre warm zu werden kaum beginnt,", "tokens": ["Wenn's", "and\u00b7re", "warm", "zu", "wer\u00b7den", "kaum", "be\u00b7ginnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "PTKZU", "VAINF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Und sp\u00e4t im hohen Ofen erst zerrinnt.", "tokens": ["Und", "sp\u00e4t", "im", "ho\u00b7hen", "O\u00b7fen", "erst", "zer\u00b7rinnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Doch keins von beiden soll das andre drum verdammen.", "tokens": ["Doch", "keins", "von", "bei\u00b7den", "soll", "das", "and\u00b7re", "drum", "ver\u00b7dam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "APPR", "PIS", "VMFIN", "ART", "PIS", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da Herzen denn von Wachs und Eisen", "tokens": ["Da", "Her\u00b7zen", "denn", "von", "Wachs", "und", "Ei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Zusammen unverschmelzbar sind,", "tokens": ["Zu\u00b7sam\u00b7men", "un\u00b7ver\u00b7schmelz\u00b7bar", "sind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "So solltest du vielmehr mich gl\u00fccklich preisen,", "tokens": ["So", "soll\u00b7test", "du", "viel\u00b7mehr", "mich", "gl\u00fcck\u00b7lich", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Da\u00df Lallage mich nicht gewinnt.", "tokens": ["Da\u00df", "Lal\u00b7la\u00b7ge", "mich", "nicht", "ge\u00b7winnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}