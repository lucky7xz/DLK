{"textgrid.poem.64450": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "72. Antwort", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund, wenn ich im Vertrauen lache,", "tokens": ["Freund", ",", "wenn", "ich", "im", "Ver\u00b7trau\u00b7en", "la\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Warum verr\u00e4thst du mich leichtf\u00fc\u00df'ger Damen Rache?", "tokens": ["Wa\u00b7rum", "ver\u00b7r\u00e4thst", "du", "mich", "leicht\u00b7f\u00fc\u00df'\u00b7ger", "Da\u00b7men", "Ra\u00b7che", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zwar ihre Gunst mag ich mir nicht erwerben:", "tokens": ["Zwar", "ih\u00b7re", "Gunst", "mag", "ich", "mir", "nicht", "er\u00b7wer\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur send' ich Seufzer in die H\u00f6h',", "tokens": ["Nur", "send'", "ich", "Seuf\u00b7zer", "in", "die", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dort lebt mir noch ", "tokens": ["Dort", "lebt", "mir", "noch"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Doch will ich nicht wie ", "tokens": ["Doch", "will", "ich", "nicht", "wie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "KOKOM"], "meter": "+---+", "measure": "dactylic.init"}}}}}