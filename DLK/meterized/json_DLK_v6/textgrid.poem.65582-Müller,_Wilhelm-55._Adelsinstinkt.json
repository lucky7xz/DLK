{"textgrid.poem.65582": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "55. Adelsinstinkt", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wappen lie\u00df die edle Dame in des S\u00e4uglings Windeln n\u00e4hen,", "tokens": ["Wap\u00b7pen", "lie\u00df", "die", "ed\u00b7le", "Da\u00b7me", "in", "des", "S\u00e4ug\u00b7lings", "Win\u00b7deln", "n\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und das Kind starb an Verstopfung, eh' es noch sein Schild gesehen.", "tokens": ["Und", "das", "Kind", "starb", "an", "Ver\u00b7stop\u00b7fung", ",", "eh'", "es", "noch", "sein", "Schild", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN", "$,", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Lernt daraus, wie viel es hei\u00dfe, adelig geboren sein!", "tokens": ["Lernt", "da\u00b7raus", ",", "wie", "viel", "es", "hei\u00b7\u00dfe", ",", "a\u00b7de\u00b7lig", "ge\u00b7bo\u00b7ren", "sein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "$,", "PWAV", "PIS", "PPER", "VVFIN", "$,", "ADJD", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Nur aus noblem Bauchinstinkte hielt das Kind den Adel rein.", "tokens": ["Nur", "aus", "nob\u00b7lem", "Bau\u00b7ch\u00b7ins\u00b7tink\u00b7te", "hielt", "das", "Kind", "den", "A\u00b7del", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "--+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}}}}}