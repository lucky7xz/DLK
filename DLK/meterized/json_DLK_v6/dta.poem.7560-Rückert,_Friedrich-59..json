{"dta.poem.7560": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "59.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zum Tod bereite sich, wer nicht mehr kann genesen;", "tokens": ["Zum", "Tod", "be\u00b7rei\u00b7te", "sich", ",", "wer", "nicht", "mehr", "kann", "ge\u00b7ne\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "$,", "PWS", "PTKNEG", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was nicht weiter h\u00e4lt, mag auseinander wesen.", "tokens": ["Und", "was", "nicht", "wei\u00b7ter", "h\u00e4lt", ",", "mag", "aus\u00b7ein\u00b7an\u00b7der", "we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKNEG", "ADV", "VVFIN", "$,", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wol ists ein Trost zu sehn, da\u00df in Verwesung auch", "tokens": ["Wol", "ists", "ein", "Trost", "zu", "sehn", ",", "da\u00df", "in", "Ver\u00b7we\u00b7sung", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "$,", "KOUS", "APPR", "NN", "ADV"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Sich neues Leben regt, nur ists kein s\u00fc\u00dfer Hauch.", "tokens": ["Sich", "neu\u00b7es", "Le\u00b7ben", "regt", ",", "nur", "ists", "kein", "s\u00fc\u00b7\u00dfer", "Hauch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "NN", "VVFIN", "$,", "ADV", "VAFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Modersch\u00f6pfung ab wend' eilig deine Blicke,", "tokens": ["Der", "Mo\u00b7der\u00b7sch\u00f6p\u00b7fung", "ab", "wend'", "ei\u00b7lig", "dei\u00b7ne", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df rein des Lebens Bild dich Lebenden erquicke!", "tokens": ["Da\u00df", "rein", "des", "Le\u00b7bens", "Bild", "dich", "Le\u00b7ben\u00b7den", "er\u00b7qui\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "NN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}