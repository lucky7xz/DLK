{"textgrid.poem.67537": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Komm, o Du des Todes Bild,", "genre": "verse", "period": "N.A.", "pub_year": 1767, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm, o Du des Todes Bild,", "tokens": ["Komm", ",", "o", "Du", "des", "To\u00b7des", "Bild", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sanfter Schlaf, und breite", "tokens": ["Sanf\u00b7ter", "Schlaf", ",", "und", "brei\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Dein Gefieder \u00fcber mich!", "tokens": ["Dein", "Ge\u00b7fie\u00b7der", "\u00fc\u00b7ber", "mich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00fc\u00dfen Schlummers Beute", "tokens": ["S\u00fc\u00b7\u00dfen", "Schlum\u00b7mers", "Beu\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ist doch das ganze Leben!", "tokens": ["Ist", "doch", "das", "gan\u00b7ze", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist Traumwerk eitler Phantasie,", "tokens": ["Ist", "Traum\u00b7werk", "eit\u00b7ler", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die \u2013 ach, bald auch welket sie!", "tokens": ["Die", "\u2013", "ach", ",", "bald", "auch", "wel\u00b7ket", "sie", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "XY", "$,", "ADV", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sinkt mattem Schlummer zu!", "tokens": ["Sinkt", "mat\u00b7tem", "Schlum\u00b7mer", "zu", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "In sanfter Ohnmacht Ruh", "tokens": ["In", "sanf\u00b7ter", "Ohn\u00b7macht", "Ruh"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Schwimmen, schwinden hin der Seele Bilder!", "tokens": ["Schwim\u00b7men", ",", "schwin\u00b7den", "hin", "der", "See\u00b7le", "Bil\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Wie d\u00e4mmernder Quell,", "tokens": ["Wie", "d\u00e4m\u00b7mern\u00b7der", "Quell", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Alle Lebenswogen!", "tokens": ["Al\u00b7le", "Le\u00b7bens\u00b7wo\u00b7gen", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Wird's mir, wird es auch so sein", "tokens": ["Wird's", "mir", ",", "wird", "es", "auch", "so", "sein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "VAFIN", "PPER", "ADV", "ADV", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Im Todesschlummer?", "tokens": ["Im", "To\u00b7des\u00b7schlum\u00b7mer", "?"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wie von sp\u00e4ter, ferner Zeit", "tokens": ["Wie", "von", "sp\u00e4\u00b7ter", ",", "fer\u00b7ner", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "APPR", "ADJD", "$,", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommen dunkle Tr\u00e4ume", "tokens": ["Kom\u00b7men", "dunk\u00b7le", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Matt zur\u00fcck! In neue Welt", "tokens": ["Matt", "zu\u00b7r\u00fcck", "!", "In", "neu\u00b7e", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schatten Jugendb\u00e4ume", "tokens": ["Schat\u00b7ten", "Ju\u00b7gend\u00b7b\u00e4u\u00b7me"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Die stille Seel' hin\u00fcber!", "tokens": ["Die", "stil\u00b7le", "Seel'", "hin\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist's immer nicht dieselbe Welt,", "tokens": ["Ist's", "im\u00b7mer", "nicht", "die\u00b7sel\u00b7be", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKNEG", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die dem Schlummertraum gef\u00e4llt?", "tokens": ["Die", "dem", "Schlum\u00b7mer\u00b7traum", "ge\u00b7f\u00e4llt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wird's ewig auch so sein?", "tokens": ["Wird's", "e\u00b7wig", "auch", "so", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "ADV", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Wirst, erster Jugend Pein,", "tokens": ["Wirst", ",", "ers\u00b7ter", "Ju\u00b7gend", "Pein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Pein und Wonne, Du mir wiederkommen?", "tokens": ["Pein", "und", "Won\u00b7ne", ",", "Du", "mir", "wie\u00b7der\u00b7kom\u00b7men", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Zwar matter und sp\u00e4t,", "tokens": ["Zwar", "mat\u00b7ter", "und", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Sp\u00e4t und doch dieselbe?", "tokens": ["Sp\u00e4t", "und", "doch", "die\u00b7sel\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADV", "PDAT", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Sch\u00f6pfer! ahnet mir ein Traum", "tokens": ["Sch\u00f6p\u00b7fer", "!", "ah\u00b7net", "mir", "ein", "Traum"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Selbst Ewigkeiten?", "tokens": ["Selbst", "E\u00b7wig\u00b7kei\u00b7ten", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Sanfter Schlaf, der Dich erfand,", "tokens": ["Sanf\u00b7ter", "Schlaf", ",", "der", "Dich", "er\u00b7fand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Birgt auch diese Sorgen!", "tokens": ["Birgt", "auch", "die\u00b7se", "Sor\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PDAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Grauer Schleier h\u00fcllet sie.", "tokens": ["Grau\u00b7er", "Schlei\u00b7er", "h\u00fcl\u00b7let", "sie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und am sch\u00f6nen Morgen", "tokens": ["Und", "am", "sch\u00f6\u00b7nen", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ist selbst der Schlafgedanke", "tokens": ["Ist", "selbst", "der", "Schlaf\u00b7ge\u00b7dan\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie Traum! schon Traum mein Schlafgebet,", "tokens": ["Wie", "Traum", "!", "schon", "Traum", "mein", "Schlaf\u00b7ge\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$.", "ADV", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das \u2013 Du wei\u00dft es, was? \u2013 erfleht!", "tokens": ["Das", "\u2013", "Du", "wei\u00dft", "es", ",", "was", "?", "\u2013", "er\u00b7fleht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "$(", "PPER", "VVFIN", "PPER", "$,", "PWS", "$.", "$(", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Zu wiegen mich in Schlaf,", "tokens": ["Zu", "wie\u00b7gen", "mich", "in", "Schlaf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Zu w\u00e4hnen noch im Schlaf", "tokens": ["Zu", "w\u00e4h\u00b7nen", "noch", "im", "Schlaf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "ADV", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Theure, ferne Lebensfreunde. \u2013 Schirme,", "tokens": ["Theu\u00b7re", ",", "fer\u00b7ne", "Le\u00b7bens\u00b7freun\u00b7de", ".", "\u2013", "Schir\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$.", "$(", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Beschirme sie, Gott!", "tokens": ["Be\u00b7schir\u00b7me", "sie", ",", "Gott", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Schlaf und Lebenswachen", "tokens": ["Schlaf", "und", "Le\u00b7bens\u00b7wa\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Sendest Du der Menschenzeit,", "tokens": ["Sen\u00b7dest", "Du", "der", "Men\u00b7schen\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Ja, Alles Tr\u00e4ume!", "tokens": ["Ja", ",", "Al\u00b7les", "Tr\u00e4u\u00b7me", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}