{"textgrid.poem.56148": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: Blumen, ihr schlie\u00dflich den ordnenden H\u00e4nden verwandte,", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Blumen, ihr schlie\u00dflich den ordnenden H\u00e4nden verwandte,", "tokens": ["Blu\u00b7men", ",", "ihr", "schlie\u00df\u00b7lich", "den", "ord\u00b7nen\u00b7den", "H\u00e4n\u00b7den", "ver\u00b7wand\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.2": {"text": "(h\u00e4nden der M\u00e4dchen von einst und jetzt),", "tokens": ["(", "h\u00e4n\u00b7den", "der", "M\u00e4d\u00b7chen", "von", "einst", "und", "jetzt", ")", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "APPR", "ADV", "KON", "ADV", "$(", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "die auf dem Gartentisch oft von Kante zu Kante", "tokens": ["die", "auf", "dem", "Gar\u00b7ten\u00b7tisch", "oft", "von", "Kan\u00b7te", "zu", "Kan\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "APPR", "NN", "APPR", "NN"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "lagen, ermattet und sanft verletzt,", "tokens": ["la\u00b7gen", ",", "er\u00b7mat\u00b7tet", "und", "sanft", "ver\u00b7letzt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "KON", "ADJD", "VVPP", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "wartend des Wassers, das sie noch einmal erhole", "tokens": ["war\u00b7tend", "des", "Was\u00b7sers", ",", "das", "sie", "noch", "ein\u00b7mal", "er\u00b7ho\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVFIN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "aus dem begonnenen Tod \u2013, und nun", "tokens": ["aus", "dem", "be\u00b7gon\u00b7ne\u00b7nen", "Tod", "\u2013", ",", "und", "nun"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "$,", "KON", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "wieder erhobene zwischen die str\u00f6menden Pole", "tokens": ["wie\u00b7der", "er\u00b7ho\u00b7be\u00b7ne", "zwi\u00b7schen", "die", "str\u00f6\u00b7men\u00b7den", "Po\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.4": {"text": "f\u00fchlender Finger, die wohlzutun", "tokens": ["f\u00fch\u00b7len\u00b7der", "Fin\u00b7ger", ",", "die", "wohl\u00b7zu\u00b7tun"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "VVINF"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "mehr noch verm\u00f6gen, als ihr ahntet, ihr leichten,", "tokens": ["mehr", "noch", "ver\u00b7m\u00f6\u00b7gen", ",", "als", "ihr", "ahn\u00b7tet", ",", "ihr", "leich\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "wenn ihr euch wiederfandet im Krug,", "tokens": ["wenn", "ihr", "euch", "wie\u00b7der\u00b7fan\u00b7det", "im", "Krug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "langsam erk\u00fchlend und Warmes der M\u00e4dchen, wie Beichten,", "tokens": ["lang\u00b7sam", "er\u00b7k\u00fch\u00b7lend", "und", "War\u00b7mes", "der", "M\u00e4d\u00b7chen", ",", "wie", "Beich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "KON", "NN", "ART", "NN", "$,", "PWAV", "NN", "$,"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}}, "stanza.4": {"line.1": {"text": "von euch gebend, wie tr\u00fcbe erm\u00fcdende S\u00fcnden,", "tokens": ["von", "euch", "ge\u00b7bend", ",", "wie", "tr\u00fc\u00b7be", "er\u00b7m\u00fc\u00b7den\u00b7de", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "PWAV", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "die das Gepfl\u00fccktsein beging, als Bezug", "tokens": ["die", "das", "Ge\u00b7pfl\u00fcck\u00b7tsein", "be\u00b7ging", ",", "als", "Be\u00b7zug"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ART", "NN", "VVFIN", "$,", "KOUS", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "wieder zu ihnen, die sich euch bl\u00fchend verb\u00fcnden.", "tokens": ["wie\u00b7der", "zu", "ih\u00b7nen", ",", "die", "sich", "euch", "bl\u00fc\u00b7hend", "ver\u00b7b\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,", "PRELS", "PRF", "PPER", "ADJD", "VVINF", "$."], "meter": "+--+-++-+--+-", "measure": "iambic.hexa.invert"}}}}}