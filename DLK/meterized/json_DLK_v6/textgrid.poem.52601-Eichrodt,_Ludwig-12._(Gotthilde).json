{"textgrid.poem.52601": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "12. (Gotthilde)", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn nur ", "tokens": ["Wenn", "nur"], "token_info": ["word", "word"], "pos": ["KOUS", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Noch zu mir dich zieht,", "tokens": ["Noch", "zu", "mir", "dich", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Einer jener Triebe", "tokens": ["Ei\u00b7ner", "je\u00b7ner", "Trie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["PIS", "PDAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Deine Brust durchgl\u00fcht,", "tokens": ["Dei\u00b7ne", "Brust", "durch\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "O so k\u00fcsse, Theurer, mich denn noch einmal,", "tokens": ["O", "so", "k\u00fcs\u00b7se", ",", "Theu\u00b7rer", ",", "mich", "denn", "noch", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "$,", "ADJD", "$,", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "O so sende deiner Augen einen Strahl", "tokens": ["O", "so", "sen\u00b7de", "dei\u00b7ner", "Au\u00b7gen", "ei\u00b7nen", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "In mein armes krankes Herz!", "tokens": ["In", "mein", "ar\u00b7mes", "kran\u00b7kes", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schmerz um dich und wilde Qualen", "tokens": ["Schmerz", "um", "dich", "und", "wil\u00b7de", "Qua\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab ich hei\u00df gepflegt,", "tokens": ["Hab", "ich", "hei\u00df", "ge\u00b7pflegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Mich zu tausendmalen", "tokens": ["Mich", "zu", "tau\u00b7send\u00b7ma\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Schlummerlos gelegt.", "tokens": ["Schlum\u00b7mer\u00b7los", "ge\u00b7legt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "O verstehe du den ausgeweinten Blick,", "tokens": ["O", "ver\u00b7ste\u00b7he", "du", "den", "aus\u00b7ge\u00b7wein\u00b7ten", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "O nicht ganz zusammen brich mein zitternd Gl\u00fcck", "tokens": ["O", "nicht", "ganz", "zu\u00b7sam\u00b7men", "brich", "mein", "zit\u00b7ternd", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKNEG", "ADV", "ADV", "ADJD", "PPOSAT", "ADJD", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Und mein armes krankes Herz!", "tokens": ["Und", "mein", "ar\u00b7mes", "kran\u00b7kes", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Du nur kannst mir neubeleben", "tokens": ["Du", "nur", "kannst", "mir", "neu\u00b7be\u00b7le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den erloschnen Muth,", "tokens": ["Den", "er\u00b7lo\u00b7schnen", "Muth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Du nur wiedergeben", "tokens": ["Du", "nur", "wie\u00b7der\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Heitre Lebensglut.", "tokens": ["Heit\u00b7re", "Le\u00b7bens\u00b7glut", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "O so k\u00fcsse, Theurer, mich denn noch einmal,", "tokens": ["O", "so", "k\u00fcs\u00b7se", ",", "Theu\u00b7rer", ",", "mich", "denn", "noch", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "$,", "ADJD", "$,", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Nur dein Ku\u00df erweckt und nur dein Augenstrahl", "tokens": ["Nur", "dein", "Ku\u00df", "er\u00b7weckt", "und", "nur", "dein", "Au\u00b7gen\u00b7strahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "KON", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Dieses arme kranke Herz!", "tokens": ["Die\u00b7ses", "ar\u00b7me", "kran\u00b7ke", "Herz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}