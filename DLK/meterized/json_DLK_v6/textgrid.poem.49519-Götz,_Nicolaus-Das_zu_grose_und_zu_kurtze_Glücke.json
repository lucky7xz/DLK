{"textgrid.poem.49519": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Das zu grose und zu kurtze Gl\u00fccke", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Ma\u00fclgen hat mein Mund von Ihrem Mund geraubet,", "tokens": ["Ein", "Ma\u00b7\u00fcl\u00b7gen", "hat", "mein", "Mund", "von", "Ih\u00b7rem", "Mund", "ge\u00b7rau\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Ob sie mir gleich die Hand vor meine Lippen hielt.", "tokens": ["Ob", "sie", "mir", "gleich", "die", "Hand", "vor", "mei\u00b7ne", "Lip\u00b7pen", "hielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie es Sekund beschreibt, so hab ich es gef\u00fchlt,", "tokens": ["Wie", "es", "Se\u00b7kund", "be\u00b7schreibt", ",", "so", "hab", "ich", "es", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und mich gl\u00fcckseeliger, als K\u00f6nige, geglaubet.", "tokens": ["Und", "mich", "gl\u00fcck\u00b7see\u00b7li\u00b7ger", ",", "als", "K\u00f6\u00b7ni\u00b7ge", ",", "ge\u00b7glau\u00b7bet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "$,", "KOUS", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch meine Lust verschwand gleich einem Wa\u00dferschaum,", "tokens": ["Doch", "mei\u00b7ne", "Lust", "ver\u00b7schwand", "gleich", "ei\u00b7nem", "Wa\u00b7\u00dfer\u00b7schaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wie es mir ergieng, mu\u00df stets im Zweifel liegen.", "tokens": ["Und", "wie", "es", "mir", "er\u00b7gieng", ",", "mu\u00df", "stets", "im", "Zwei\u00b7fel", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPER", "VVFIN", "$,", "VMFIN", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mein Gl\u00fcck war zwar zu gros f\u00fcr einen leichten Traum,", "tokens": ["Mein", "Gl\u00fcck", "war", "zwar", "zu", "gros", "f\u00fcr", "ei\u00b7nen", "leich\u00b7ten", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ach! aber, auch zu kurtz, f\u00fcr ein warhaft Vergn\u00fcgen.", "tokens": ["Ach", "!", "a\u00b7ber", ",", "auch", "zu", "kurtz", ",", "f\u00fcr", "ein", "war\u00b7haft", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "$,", "ADV", "PTKA", "ADJD", "$,", "APPR", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}