{"textgrid.poem.55944": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Ein Frauen-Schicksal", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So wie der K\u00f6nig auf der Jagd ein Glas", "tokens": ["So", "wie", "der", "K\u00f6\u00b7nig", "auf", "der", "Jagd", "ein", "Glas"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ergreift, daraus zu trinken, irgendeines, \u2013", "tokens": ["er\u00b7greift", ",", "da\u00b7raus", "zu", "trin\u00b7ken", ",", "ir\u00b7gend\u00b7ei\u00b7nes", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "PAV", "PTKZU", "VVINF", "$,", "ADJA", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und wie hernach der welcher es besa\u00df", "tokens": ["und", "wie", "her\u00b7nach", "der", "wel\u00b7cher", "es", "be\u00b7sa\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "ART", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "es fortstellt und verwahrt als w\u00e4r es keines:", "tokens": ["es", "fort\u00b7stellt", "und", "ver\u00b7wahrt", "als", "w\u00e4r", "es", "kei\u00b7nes", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVPP", "KOKOM", "VAFIN", "PPER", "PIS", "$."], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "so hob vielleicht das Schicksal, durstig auch,", "tokens": ["so", "hob", "viel\u00b7leicht", "das", "Schick\u00b7sal", ",", "durs\u00b7tig", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "bisweilen Eine an den Mund und trank,", "tokens": ["bis\u00b7wei\u00b7len", "Ei\u00b7ne", "an", "den", "Mund", "und", "trank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "APPR", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "die dann ein kleines Leben, viel zu bang", "tokens": ["die", "dann", "ein", "klei\u00b7nes", "Le\u00b7ben", ",", "viel", "zu", "bang"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "$,", "ADV", "PTKA", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "sie zu zerbrechen, abseits vom Gebrauch", "tokens": ["sie", "zu", "zer\u00b7bre\u00b7chen", ",", "ab\u00b7seits", "vom", "Ge\u00b7brauch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "hinstellte in die \u00e4ngstliche Vitrine,", "tokens": ["hin\u00b7stell\u00b7te", "in", "die", "\u00e4ngst\u00b7li\u00b7che", "Vi\u00b7tri\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "in welcher seine Kostbarkeiten sind", "tokens": ["in", "wel\u00b7cher", "sei\u00b7ne", "Kost\u00b7bar\u00b7kei\u00b7ten", "sind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "(oder die Dinge, die f\u00fcr kostbar gelten).", "tokens": ["(", "o\u00b7der", "die", "Din\u00b7ge", ",", "die", "f\u00fcr", "kost\u00b7bar", "gel\u00b7ten", ")", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "ART", "NN", "$,", "PRELS", "APPR", "ADJD", "VVINF", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da stand sie fremd wie eine Fortgeliehne", "tokens": ["Da", "stand", "sie", "fremd", "wie", "ei\u00b7ne", "Fort\u00b7ge\u00b7lieh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und wurde einfach alt und wurde blind", "tokens": ["und", "wur\u00b7de", "ein\u00b7fach", "alt", "und", "wur\u00b7de", "blind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "KON", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und war nicht kostbar und war niemals selten.", "tokens": ["und", "war", "nicht", "kost\u00b7bar", "und", "war", "nie\u00b7mals", "sel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "ADJD", "KON", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}