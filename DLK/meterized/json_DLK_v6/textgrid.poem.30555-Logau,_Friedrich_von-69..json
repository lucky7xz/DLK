{"textgrid.poem.30555": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "69.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo kein Brunn, da kans nicht flissen;", "tokens": ["Wo", "kein", "Brunn", ",", "da", "kans", "nicht", "flis\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$,", "KOUS", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer viel redet, mu\u00df viel wissen.", "tokens": ["Wer", "viel", "re\u00b7det", ",", "mu\u00df", "viel", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "VMFIN", "ADV", "VVINF", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Veit sagt viel, wei\u00df nichts; er flicke,", "tokens": ["Veit", "sagt", "viel", ",", "wei\u00df", "nichts", ";", "er", "fli\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,", "VVFIN", "PIS", "$.", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "D\u00fcnckt mich, L\u00fcgen f\u00fcr die L\u00fccke.", "tokens": ["D\u00fcnckt", "mich", ",", "L\u00fc\u00b7gen", "f\u00fcr", "die", "L\u00fc\u00b7cke", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}