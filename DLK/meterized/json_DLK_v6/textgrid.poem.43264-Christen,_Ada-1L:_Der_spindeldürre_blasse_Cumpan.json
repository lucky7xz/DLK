{"textgrid.poem.43264": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der spindeld\u00fcrre blasse Cumpan", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der spindeld\u00fcrre blasse Cumpan", "tokens": ["Der", "spin\u00b7del\u00b7d\u00fcr\u00b7re", "blas\u00b7se", "Cum\u00b7pan"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Voll wohlgef\u00fcgter Reden", "tokens": ["Voll", "wohl\u00b7ge\u00b7f\u00fcg\u00b7ter", "Re\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schaut pr\u00fcfend sorgsam die Menschen an,", "tokens": ["Schaut", "pr\u00fc\u00b7fend", "sorg\u00b7sam", "die", "Men\u00b7schen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als examinirte er Jeden.", "tokens": ["Als", "e\u00b7xa\u00b7min\u00b7ir\u00b7te", "er", "Je\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PIAT", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Die Augen sind klein, das Stimmchen fein,", "tokens": ["Die", "Au\u00b7gen", "sind", "klein", ",", "das", "Stimm\u00b7chen", "fein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gezirkelt alle Geberden,", "tokens": ["Ge\u00b7zir\u00b7kelt", "al\u00b7le", "Ge\u00b7ber\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Man sagt, er sprach vor Jahren Latein", "tokens": ["Man", "sagt", ",", "er", "sprach", "vor", "Jah\u00b7ren", "La\u00b7tein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wollte auch Dichter werden.", "tokens": ["Und", "woll\u00b7te", "auch", "Dich\u00b7ter", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "NN", "VAINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Jetzt hat er oft Hunger ... immer Durst ...", "tokens": ["Jetzt", "hat", "er", "oft", "Hun\u00b7ger", "...", "im\u00b7mer", "Durst", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "NN", "$(", "ADV", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und dichtet nur epigrammatisch,", "tokens": ["Und", "dich\u00b7tet", "nur", "e\u00b7pig\u00b7ram\u00b7ma\u00b7tisch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Verwerthet f\u00fcr Wein als Wirthshaus-Hanswurst", "tokens": ["Ver\u00b7wert\u00b7het", "f\u00fcr", "Wein", "als", "Wirths\u00b7haus\u00b7Hans\u00b7wurst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "KOUS", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auch sein Talent dramatisch.", "tokens": ["Auch", "sein", "Ta\u00b7lent", "dra\u00b7ma\u00b7tisch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}