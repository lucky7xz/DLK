{"textgrid.poem.47364": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Winterlied", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die sch\u00f6ne Sommerzeit ist hin,", "tokens": ["Die", "sch\u00f6\u00b7ne", "Som\u00b7mer\u00b7zeit", "ist", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Winter ist nun da;", "tokens": ["Der", "Win\u00b7ter", "ist", "nun", "da", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir m\u00fcssen aus dem Garten fliehn,", "tokens": ["Wir", "m\u00fcs\u00b7sen", "aus", "dem", "Gar\u00b7ten", "fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der uns so fr\u00f6hlich sah.", "tokens": ["Der", "uns", "so", "fr\u00f6h\u00b7lich", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Busch ist kahl und abgelaubt,", "tokens": ["Der", "Busch", "ist", "kahl", "und", "ab\u00b7ge\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der uns im Schatten barg;", "tokens": ["Der", "uns", "im", "Schat\u00b7ten", "barg", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der alte kalte Nordwind schnaubt", "tokens": ["Der", "al\u00b7te", "kal\u00b7te", "Nord\u00b7wind", "schnaubt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und macht es gar zu arg.", "tokens": ["Und", "macht", "es", "gar", "zu", "arg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O M\u00e4dchen, komm, so weichen wir", "tokens": ["O", "M\u00e4d\u00b7chen", ",", "komm", ",", "so", "wei\u00b7chen", "wir"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVFIN", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und r\u00e4umen ihm das Feld;", "tokens": ["Und", "r\u00e4u\u00b7men", "ihm", "das", "Feld", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist nicht, o s\u00fc\u00dfes M\u00e4gdlein, dir", "tokens": ["Ist", "nicht", ",", "o", "s\u00fc\u00b7\u00dfes", "M\u00e4gd\u00b7lein", ",", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PTKNEG", "$,", "FM", "ADJA", "NN", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein H\u00fcttelein bestellt?", "tokens": ["Ein", "H\u00fct\u00b7tel\u00b7ein", "be\u00b7stellt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und bleibt mir fortan immer nur", "tokens": ["Und", "bleibt", "mir", "for\u00b7tan", "im\u00b7mer", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das H\u00fcttlein aufgethan;", "tokens": ["Das", "H\u00fctt\u00b7lein", "auf\u00b7ge\u00b7than", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So klag' ich nicht die \u00f6de Flur", "tokens": ["So", "klag'", "ich", "nicht", "die", "\u00f6\u00b7de", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nicht den Winter an.", "tokens": ["Und", "nicht", "den", "Win\u00b7ter", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}