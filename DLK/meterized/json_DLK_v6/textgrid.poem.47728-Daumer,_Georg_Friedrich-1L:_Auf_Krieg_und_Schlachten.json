{"textgrid.poem.47728": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf Krieg und Schlachten", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf Krieg und Schlachten", "tokens": ["Auf", "Krieg", "und", "Schlach\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Und gro\u00dfer Thaten", "tokens": ["Und", "gro\u00b7\u00dfer", "Tha\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Erhabne Prachten", "tokens": ["Er\u00b7hab\u00b7ne", "Prach\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Vernehmt ihr Leute", "tokens": ["Ver\u00b7nehmt", "ihr", "Leu\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Mein Preisgedicht!", "tokens": ["Mein", "Preis\u00b7ge\u00b7dicht", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Es kam ein H\u00e4schen", "tokens": ["Es", "kam", "ein", "H\u00e4\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Und hob das N\u00e4schen", "tokens": ["Und", "hob", "das", "N\u00e4\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und hob das Pf\u00f6tchen \u2013", "tokens": ["Und", "hob", "das", "Pf\u00f6t\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Es war kein Sp\u00e4\u00dfchen \u2013", "tokens": ["Es", "war", "kein", "Sp\u00e4\u00df\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Und traf den grausam", "tokens": ["Und", "traf", "den", "grau\u00b7sam"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Erschreckten Vater", "tokens": ["Er\u00b7schreck\u00b7ten", "Va\u00b7ter"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Mit hartem Schlage", "tokens": ["Mit", "har\u00b7tem", "Schla\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Und schonte nicht.", "tokens": ["Und", "schon\u00b7te", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Ich wollte retten,", "tokens": ["Ich", "woll\u00b7te", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Ich wollte r\u00e4chen,", "tokens": ["Ich", "woll\u00b7te", "r\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Es war ja Pflicht;", "tokens": ["Es", "war", "ja", "Pflicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Ich wollte brechen", "tokens": ["Ich", "woll\u00b7te", "bre\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Aus meinem Hage,", "tokens": ["Aus", "mei\u00b7nem", "Ha\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "So wie ein Krieger,", "tokens": ["So", "wie", "ein", "Krie\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Ein Held wohl bricht;", "tokens": ["Ein", "Held", "wohl", "bricht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Ich konnt' es aber,", "tokens": ["Ich", "konnt'", "es", "a\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "Ich arme Dirne \u2013", "tokens": ["Ich", "ar\u00b7me", "Dir\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Vor Lachen nicht.", "tokens": ["Vor", "La\u00b7chen", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}