{"textgrid.poem.52984": {"metadata": {"author": {"name": "Seume, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einsam durchirr' ich von Fels zu Fels", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einsam durchirr' ich von Fels zu Fels", "tokens": ["Ein\u00b7sam", "durch\u00b7irr'", "ich", "von", "Fels", "zu", "Fels"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "NE", "APPR", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Die Gruppen der Berge des Muldenthals;", "tokens": ["Die", "Grup\u00b7pen", "der", "Ber\u00b7ge", "des", "Mul\u00b7den\u00b7thals", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ruhe wallt sanft in dem Flu\u00df hin durch sie,", "tokens": ["Ru\u00b7he", "wallt", "sanft", "in", "dem", "Flu\u00df", "hin", "durch", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Und des Dorfs Herdengel\u00e4ut t\u00f6nt in der Schlucht.", "tokens": ["Und", "des", "Dorfs", "Her\u00b7den\u00b7ge\u00b7l\u00e4ut", "t\u00f6nt", "in", "der", "Schlucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "---+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "R\u00f6thlicher gl\u00fchet der Tempel dort,", "tokens": ["R\u00f6th\u00b7li\u00b7cher", "gl\u00fc\u00b7het", "der", "Tem\u00b7pel", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Und sp\u00e4ter noch fl\u00f6tet die Nachtigall;", "tokens": ["Und", "sp\u00e4\u00b7ter", "noch", "fl\u00f6\u00b7tet", "die", "Nach\u00b7ti\u00b7gall", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Schauerlich wirds in dem Hain, wenn das Lied", "tokens": ["Schau\u00b7er\u00b7lich", "wirds", "in", "dem", "Hain", ",", "wenn", "das", "Lied"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "APPR", "ART", "NN", "$,", "KOUS", "ART", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Nun verhallt, wird in des Thals Einsiedeley.", "tokens": ["Nun", "ver\u00b7hallt", ",", "wird", "in", "des", "Thals", "Ein\u00b7sie\u00b7de\u00b7ley", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "VAFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Freundlicher winken die Gruppen dir", "tokens": ["Freund\u00b7li\u00b7cher", "win\u00b7ken", "die", "Grup\u00b7pen", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "PPER"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Der Freunde die rund um dein Bildni\u00df stehn;", "tokens": ["Der", "Freun\u00b7de", "die", "rund", "um", "dein", "Bild\u00b7ni\u00df", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Jedes Herz, deinem verwandt, schl\u00e4gt dir zu,", "tokens": ["Je\u00b7des", "Herz", ",", "dei\u00b7nem", "ver\u00b7wandt", ",", "schl\u00e4gt", "dir", "zu", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PPOSAT", "VVPP", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-++--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und es schl\u00e4gt lieblicher als Nachtigallied.", "tokens": ["Und", "es", "schl\u00e4gt", "lieb\u00b7li\u00b7cher", "als", "Nach\u00b7ti\u00b7gal\u00b7lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "KOKOM", "NN", "$."], "meter": "+-++-+-+--+", "measure": "iambic.hexa.chol"}}, "stanza.4": {"line.1": {"text": "Gl\u00fccklich der Abend, der so vereint!", "tokens": ["Gl\u00fcck\u00b7lich", "der", "A\u00b7bend", ",", "der", "so", "ver\u00b7eint", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Er wird mir zur Feyer im Muldenthal:", "tokens": ["Er", "wird", "mir", "zur", "Fe\u00b7yer", "im", "Mul\u00b7den\u00b7thal", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und mein Fu\u00df pilgerte schnell hin zu euch,", "tokens": ["Und", "mein", "Fu\u00df", "pil\u00b7ger\u00b7te", "schnell", "hin", "zu", "euch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "ADV", "APPR", "PPER", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "W\u00e4r' er frey; aber doch frey pilgert das Herz.", "tokens": ["W\u00e4r'", "er", "frey", ";", "a\u00b7ber", "doch", "frey", "pil\u00b7gert", "das", "Herz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$.", "ADV", "ADV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "--++-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.5": {"line.1": {"text": "Kr\u00e4nze die G\u00f6ttinn die Schl\u00e4fe dir,", "tokens": ["Kr\u00e4n\u00b7ze", "die", "G\u00f6t\u00b7tinn", "die", "Schl\u00e4\u00b7fe", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "PPER", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Zu werden der Segen der Vaterstadt!", "tokens": ["Zu", "wer\u00b7den", "der", "Se\u00b7gen", "der", "Va\u00b7ter\u00b7stadt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und vielleicht, wallet mir einst Silberhaar", "tokens": ["Und", "viel\u00b7leicht", ",", "wal\u00b7let", "mir", "einst", "Sil\u00b7ber\u00b7haar"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "VVFIN", "PPER", "ADV", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Um das Haupt, komm' ich mit Dank freundlich zu dir.", "tokens": ["Um", "das", "Haupt", ",", "komm'", "ich", "mit", "Dank", "freund\u00b7lich", "zu", "dir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "VVFIN", "PPER", "APPR", "NN", "ADJD", "APPR", "PPER", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}}}}