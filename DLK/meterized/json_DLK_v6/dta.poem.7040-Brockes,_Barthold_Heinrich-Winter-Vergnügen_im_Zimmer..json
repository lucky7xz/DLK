{"dta.poem.7040": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Winter-Vergn\u00fcgen  \n im Zimmer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich sodann durchs Fenster seh\u2019,", "tokens": ["Wenn", "ich", "so\u00b7dann", "durchs", "Fens\u00b7ter", "seh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie draussen alles voller Schnee,", "tokens": ["Wie", "draus\u00b7sen", "al\u00b7les", "vol\u00b7ler", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie schwartz die Lufft, wie scharff und kalt,", "tokens": ["Wie", "schwartz", "die", "Lufft", ",", "wie", "scharff", "und", "kalt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "$,", "PWAV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dencke, wie bequem und gut der Aufenthalt,", "tokens": ["Und", "den\u00b7cke", ",", "wie", "be\u00b7quem", "und", "gut", "der", "Auf\u00b7ent\u00b7halt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ADJD", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den mir des Sch\u00f6pffers G\u00fcte g\u00f6nnet,", "tokens": ["Den", "mir", "des", "Sch\u00f6pf\u00b7fers", "G\u00fc\u00b7te", "g\u00f6n\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So danck\u2019 ich Jhm mit Recht: Ich denck\u2019 auch \u00f6ffters nach,", "tokens": ["So", "danck", "ich", "Jhm", "mit", "Recht", ":", "Ich", "denck", "auch", "\u00f6ff\u00b7ters", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "NN", "$.", "PPER", "VVFIN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie wahr es sey, was jener sprach:", "tokens": ["Wie", "wahr", "es", "sey", ",", "was", "je\u00b7ner", "sprach", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VAFIN", "$,", "PWS", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von einer warmen Stub\u2019 in kalter Winters-Zeit:", "tokens": ["Von", "ei\u00b7ner", "war\u00b7men", "Stub'", "in", "kal\u00b7ter", "Win\u00b7ter\u00b7sZeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da\u00df blo\u00df ein Scheiben-Glas der Unterscheid,", "tokens": ["Da\u00df", "blo\u00df", "ein", "Schei\u00b7ben\u00b7Glas", "der", "Un\u00b7ter\u00b7scheid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der gleichsam Africa von Nova Zembla trennet.", "tokens": ["Der", "gleich\u00b7sam", "A\u00b7fri\u00b7ca", "von", "No\u00b7va", "Zem\u00b7bla", "tren\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NE", "APPR", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}