{"textgrid.poem.47997": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Wohin?", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Vogel m\u00f6cht zum sonn'gen S\u00fcd,", "tokens": ["Der", "Vo\u00b7gel", "m\u00f6cht", "zum", "sonn'\u00b7gen", "S\u00fcd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "zu Gott des Menschen Seele fliehn \u2013", "tokens": ["zu", "Gott", "des", "Men\u00b7schen", "See\u00b7le", "fliehn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "sie wissen beide nicht den Weg", "tokens": ["sie", "wis\u00b7sen", "bei\u00b7de", "nicht", "den", "Weg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und beide finden ihn.\u2013", "tokens": ["und", "bei\u00b7de", "fin\u00b7den", "ihn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}