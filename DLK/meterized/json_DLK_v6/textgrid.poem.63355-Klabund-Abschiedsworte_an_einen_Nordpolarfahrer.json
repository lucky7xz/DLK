{"textgrid.poem.63355": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Abschiedsworte an einen Nordpolarfahrer", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lebe wohl, die Tr\u00e4ne h\u00e4ngt am Blicke,", "tokens": ["Le\u00b7be", "wohl", ",", "die", "Tr\u00e4\u00b7ne", "h\u00e4ngt", "am", "Bli\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Welcher dich von dannen gleiten sieht.", "tokens": ["Wel\u00b7cher", "dich", "von", "dan\u00b7nen", "glei\u00b7ten", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "PRF", "APPR", "ADV", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dir erf\u00fcllt der Horizont sich zum Geschicke,", "tokens": ["Dir", "er\u00b7f\u00fcllt", "der", "Ho\u00b7ri\u00b7zont", "sich", "zum", "Ge\u00b7schi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Und der M\u00f6wenruf zum Lied.", "tokens": ["Und", "der", "M\u00f6\u00b7wen\u00b7ruf", "zum", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Ewige Ewigkeiten bist du, Skage,", "tokens": ["E\u00b7wi\u00b7ge", "E\u00b7wig\u00b7kei\u00b7ten", "bist", "du", ",", "Ska\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die entmenschte Menschheit los.", "tokens": ["Die", "ent\u00b7menschte", "Menschheit", "los", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Unser Rattennest scheint dir nur eine Sage,", "tokens": ["Un\u00b7ser", "Rat\u00b7ten\u00b7nest", "scheint", "dir", "nur", "ei\u00b7ne", "Sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und die Zeitung dient als Brennstoff blo\u00df.", "tokens": ["Und", "die", "Zei\u00b7tung", "dient", "als", "Brenn\u00b7stoff", "blo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KOUS", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Ach, der Nordpol ist die einzige Gegend,", "tokens": ["Ach", ",", "der", "Nord\u00b7pol", "ist", "die", "ein\u00b7zi\u00b7ge", "Ge\u00b7gend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Wo die Parze Friedensstoffe webt,", "tokens": ["Wo", "die", "Par\u00b7ze", "Frie\u00b7dens\u00b7stof\u00b7fe", "webt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo man sich von hier nach dort bewegend", "tokens": ["Wo", "man", "sich", "von", "hier", "nach", "dort", "be\u00b7we\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "PRF", "APPR", "ADV", "APPR", "ADV", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Seiner Seele sch\u00f6nster Regung lebt.", "tokens": ["Sei\u00b7ner", "See\u00b7le", "sch\u00f6ns\u00b7ter", "Re\u00b7gung", "lebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Weder da\u00df man morgens zum Ersatztee", "tokens": ["We\u00b7der", "da\u00df", "man", "mor\u00b7gens", "zum", "Er\u00b7satz\u00b7tee"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Den Ersatzgeist aufgetischt bekommt \u2013", "tokens": ["Den", "Er\u00b7satz\u00b7geist", "auf\u00b7ge\u00b7tischt", "be\u00b7kommt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nein, der Eiskaffee ist hier am Platze,", "tokens": ["Nein", ",", "der", "Eis\u00b7kaf\u00b7fee", "ist", "hier", "am", "Plat\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und die kalte Schnauze ist's, die frommt.", "tokens": ["Und", "die", "kal\u00b7te", "Schnau\u00b7ze", "ist's", ",", "die", "frommt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "$,", "PRELS", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Denn der Eisb\u00e4r ist ein edler R\u00e4uber,", "tokens": ["Denn", "der", "Eis\u00b7b\u00e4r", "ist", "ein", "ed\u00b7ler", "R\u00e4u\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und ein stummer Bruder der Pinguin.", "tokens": ["Und", "ein", "stum\u00b7mer", "Bru\u00b7der", "der", "Pin\u00b7gu\u00b7in", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "M\u00f6wen sind die leichten Zeitvertreiber,", "tokens": ["M\u00f6\u00b7wen", "sind", "die", "leich\u00b7ten", "Zeit\u00b7ver\u00b7trei\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und ein biedrer Freund der Schneekamin.", "tokens": ["Und", "ein", "bie\u00b7drer", "Freund", "der", "Schnee\u00b7ka\u00b7min", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Kehrst nach manchen Jahren dann zur\u00fcck du \u2013", "tokens": ["Kehrst", "nach", "man\u00b7chen", "Jah\u00b7ren", "dann", "zu\u00b7r\u00fcck", "du", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "ADV", "PTKVZ", "PPER", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Liegt Europa brach von Menschen leer.", "tokens": ["Liegt", "Eu\u00b7ro\u00b7pa", "brach", "von", "Men\u00b7schen", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "VVFIN", "APPR", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Bleib in deinem wei\u00dfen Nordpolgl\u00fcck \u2013", "tokens": ["Bleib", "in", "dei\u00b7nem", "wei\u00b7\u00dfen", "Nord\u00b7pol\u00b7gl\u00fcck", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Du findest eine goldne Welt nicht mehr.", "tokens": ["Du", "fin\u00b7dest", "ei\u00b7ne", "gold\u00b7ne", "Welt", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}