{"textgrid.poem.38638": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Alpenwanderer", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Wand'rers Tritte wanken,", "tokens": ["Des", "Wan\u00b7d'\u00b7rers", "Trit\u00b7te", "wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auf schmaler Kieselbahn,", "tokens": ["Auf", "schma\u00b7ler", "Kie\u00b7sel\u00b7bahn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch wildverschlungne Ranken,", "tokens": ["Durch", "wild\u00b7ver\u00b7schlung\u00b7ne", "Ran\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Den Fichtenberg hinan.", "tokens": ["Den", "Fich\u00b7ten\u00b7berg", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie bebt des Waldstroms Br\u00fccke,", "tokens": ["Wie", "bebt", "des", "Wald\u00b7stroms", "Br\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der tosend sich ergeu\u00dft,", "tokens": ["Der", "to\u00b7send", "sich", "er\u00b7geu\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und B\u00e4um' und Felsenst\u00fccke", "tokens": ["Und", "B\u00e4um'", "und", "Fel\u00b7sen\u00b7st\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Jach in die Tiefe rei\u00dft!", "tokens": ["Jach", "in", "die", "Tie\u00b7fe", "rei\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Jezt flieht die Nacht der Wipfel;", "tokens": ["Jezt", "flieht", "die", "Nacht", "der", "Wip\u00b7fel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verkl\u00e4rt vom Sonnenstral,", "tokens": ["Ver\u00b7kl\u00e4rt", "vom", "Son\u00b7nen\u00b7stral", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gr\u00e4nzt an beschneite Gipfel", "tokens": ["Gr\u00e4nzt", "an", "be\u00b7schnei\u00b7te", "Gip\u00b7fel"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein gr\u00fcnes Zauberthal.", "tokens": ["Ein", "gr\u00fc\u00b7nes", "Zau\u00b7bert\u00b7hal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hier bliebe, wonnebebend,", "tokens": ["Hier", "blie\u00b7be", ",", "won\u00b7ne\u00b7be\u00b7bend", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Selbst Hallers Muse stumm.", "tokens": ["Selbst", "Hal\u00b7lers", "Mu\u00b7se", "stumm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie gro\u00df, wie seelenhebend!", "tokens": ["Wie", "gro\u00df", ",", "wie", "see\u00b7len\u00b7he\u00b7bend", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Hier ist Elysium!", "tokens": ["Hier", "ist", "E\u00b7ly\u00b7si\u00b7um", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hier wo ein rein'rer Aether", "tokens": ["Hier", "wo", "ein", "rein'\u00b7rer", "A\u00b7e\u00b7ther"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Um G\u00f6tterhaine flie\u00dft,", "tokens": ["Um", "G\u00f6t\u00b7ter\u00b7hai\u00b7ne", "flie\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aurorens Licht sich r\u00f6ther", "tokens": ["Au\u00b7ro\u00b7rens", "Licht", "sich", "r\u00f6\u00b7ther"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "PRF", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf hell'res Gr\u00fcn ergie\u00dft;", "tokens": ["Auf", "hell'\u00b7res", "Gr\u00fcn", "er\u00b7gie\u00dft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo Freiheit in den H\u00fctten", "tokens": ["Wo", "Frei\u00b7heit", "in", "den", "H\u00fct\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bei frommer Einfalt wohnt,", "tokens": ["Bei", "from\u00b7mer", "Ein\u00b7falt", "wohnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und Kraftgef\u00fchl die Sitten", "tokens": ["Und", "Kraft\u00b7ge\u00b7f\u00fchl", "die", "Sit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des goldnen Alters lohnt;", "tokens": ["Des", "gold\u00b7nen", "Al\u00b7ters", "lohnt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Hier wo die Heerde l\u00e4utend", "tokens": ["Hier", "wo", "die", "Heer\u00b7de", "l\u00e4u\u00b7tend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "ART", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Blumengrase geht,", "tokens": ["Im", "Blu\u00b7men\u00b7gra\u00b7se", "geht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und, Wohlgeruch verbreitend,", "tokens": ["Und", ",", "Wohl\u00b7ge\u00b7ruch", "ver\u00b7brei\u00b7tend", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Bergluft milder weht;", "tokens": ["Die", "Berg\u00b7luft", "mil\u00b7der", "weht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo, von der Enziane", "tokens": ["Wo", ",", "von", "der", "En\u00b7zi\u00b7a\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PWAV", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und Anemon' umbl\u00fcht,", "tokens": ["Und", "A\u00b7ne\u00b7mon'", "um\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Auf seidnem Rasenplane", "tokens": ["Auf", "seid\u00b7nem", "Ra\u00b7sen\u00b7pla\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Alpenrose gl\u00fcht;", "tokens": ["Die", "Al\u00b7pen\u00b7ro\u00b7se", "gl\u00fcht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Hier wo die Seele st\u00e4rker", "tokens": ["Hier", "wo", "die", "See\u00b7le", "st\u00e4r\u00b7ker"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "ART", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Fittigs H\u00fclle dehnt,", "tokens": ["Des", "Fit\u00b7tigs", "H\u00fcl\u00b7le", "dehnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hoch \u00fcber Erd' und Kerker", "tokens": ["Hoch", "\u00fc\u00b7ber", "Erd'", "und", "Ker\u00b7ker"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Emporzuschweben w\u00e4hnt,", "tokens": ["Em\u00b7por\u00b7zu\u00b7schwe\u00b7ben", "w\u00e4hnt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gel\u00e4uterter und freier", "tokens": ["Ge\u00b7l\u00e4u\u00b7ter\u00b7ter", "und", "frei\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Sinnenwelt entflieht,", "tokens": ["Der", "Sin\u00b7nen\u00b7welt", "ent\u00b7flieht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und schon, im Aetherschleier,", "tokens": ["Und", "schon", ",", "im", "A\u00b7e\u00b7ther\u00b7schlei\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "An Lethes Ufern kniet.", "tokens": ["An", "Le\u00b7thes", "U\u00b7fern", "kniet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Doch, ach! der Zauber schwindet,", "tokens": ["Doch", ",", "ach", "!", "der", "Zau\u00b7ber", "schwin\u00b7det", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ITJ", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Traumgotts Bildern gleich;", "tokens": ["Des", "Traum\u00b7gotts", "Bil\u00b7dern", "gleich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der enge Steinpfad windet", "tokens": ["Der", "en\u00b7ge", "Stein\u00b7pfad", "win\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich zwischen Felsgestr\u00e4uch;", "tokens": ["Sich", "zwi\u00b7schen", "Fels\u00b7ge\u00b7str\u00e4uch", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wild starren, matt vom Schimmer", "tokens": ["Wild", "star\u00b7ren", ",", "matt", "vom", "Schim\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Abendsonn' erhellt,", "tokens": ["Der", "A\u00b7bend\u00b7sonn'", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gest\u00fcrzter Berge Tr\u00fcmmer,", "tokens": ["Ge\u00b7st\u00fcrz\u00b7ter", "Ber\u00b7ge", "Tr\u00fcm\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie Tr\u00fcmmer einer Welt.", "tokens": ["Wie", "Tr\u00fcm\u00b7mer", "ei\u00b7ner", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Im hohen Raum der Blize", "tokens": ["Im", "ho\u00b7hen", "Raum", "der", "Bli\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "W\u00e4lzt die Lawine sich,", "tokens": ["W\u00e4lzt", "die", "La\u00b7wi\u00b7ne", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Es kreischt im Wolkensize", "tokens": ["Es", "kreischt", "im", "Wol\u00b7ken\u00b7si\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Adler f\u00fcrchterlich;", "tokens": ["Der", "Ad\u00b7ler", "f\u00fcrch\u00b7ter\u00b7lich", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dumpfdonnernd, wie die H\u00f6lle", "tokens": ["Dum\u00b7pf\u00b7don\u00b7nernd", ",", "wie", "die", "H\u00f6l\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In Aetnas Tiefen ras't,", "tokens": ["In", "A\u00b7et\u00b7nas", "Tie\u00b7fen", "ras't", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Kracht an des Bergstroms Quelle", "tokens": ["Kracht", "an", "des", "Berg\u00b7stroms", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Gletschers Eispallast.", "tokens": ["Des", "Glet\u00b7schers", "Eis\u00b7pal\u00b7last", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Hier d\u00e4mmern schwarze Gr\u00fcnde", "tokens": ["Hier", "d\u00e4m\u00b7mern", "schwar\u00b7ze", "Gr\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo nie ein Bl\u00fcmchen lacht,", "tokens": ["Wo", "nie", "ein", "Bl\u00fcm\u00b7chen", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dort bergen grause Schl\u00fcnde", "tokens": ["Dort", "ber\u00b7gen", "grau\u00b7se", "Schl\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Des Chaos alte Nacht;", "tokens": ["Des", "Chaos", "al\u00b7te", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und wilder, immer wilder", "tokens": ["Und", "wil\u00b7der", ",", "im\u00b7mer", "wil\u00b7der"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "ADJA", "$,", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Schwingt sich der Pfad empor;", "tokens": ["Schwingt", "sich", "der", "Pfad", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Bleich wallen Todesbilder", "tokens": ["Bleich", "wal\u00b7len", "To\u00b7des\u00b7bil\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVINF", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Aus jeder Kluft hervor.", "tokens": ["Aus", "je\u00b7der", "Kluft", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Kalt wehn des Grabes Schrecken,", "tokens": ["Kalt", "wehn", "des", "Gra\u00b7bes", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Wo dr\u00e4uend der Granit,", "tokens": ["Wo", "dr\u00e4u\u00b7end", "der", "Gra\u00b7nit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relax