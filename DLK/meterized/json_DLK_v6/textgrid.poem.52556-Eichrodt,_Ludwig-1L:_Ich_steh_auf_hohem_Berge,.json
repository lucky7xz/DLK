{"textgrid.poem.52556": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich steh auf hohem Berge,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich steh auf hohem Berge,", "tokens": ["Ich", "steh", "auf", "ho\u00b7hem", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Wind, tief unter mir", "tokens": ["Im", "Wind", ",", "tief", "un\u00b7ter", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "ADJD", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die rauschenden Buchenwipfel \u2013", "tokens": ["Die", "rau\u00b7schen\u00b7den", "Bu\u00b7chen\u00b7wip\u00b7fel", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie einsam ist es hier!", "tokens": ["Wie", "ein\u00b7sam", "ist", "es", "hier", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Sonn ist untergegangen,", "tokens": ["Die", "Sonn", "ist", "un\u00b7ter\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sogar das Abendroth", "tokens": ["So\u00b7gar", "das", "A\u00b7ben\u00b7droth"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In Wolkengr\u00fcfte gesunken,", "tokens": ["In", "Wol\u00b7ken\u00b7gr\u00fcf\u00b7te", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der sch\u00f6ne Tag ist todt.", "tokens": ["Der", "sch\u00f6\u00b7ne", "Tag", "ist", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Schwarz h\u00fcllt sich ein der Thalgrund", "tokens": ["Schwarz", "h\u00fcllt", "sich", "ein", "der", "Thal\u00b7grund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "ART", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In wallenden Nebelflor,", "tokens": ["In", "wal\u00b7len\u00b7den", "Ne\u00b7bel\u00b7flor", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Draus recket die dunkeln H\u00e4upter", "tokens": ["Draus", "re\u00b7cket", "die", "dun\u00b7keln", "H\u00e4up\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Das Hochgebirg empor.", "tokens": ["Das", "Hoch\u00b7ge\u00b7birg", "em\u00b7por", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und tief ins Herz erschrocken", "tokens": ["Und", "tief", "ins", "Herz", "er\u00b7schro\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Halt ich den Odem an,", "tokens": ["Halt", "ich", "den", "O\u00b7dem", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Furcht \u00fcberschleicht mein Wesen,", "tokens": ["Furcht", "\u00fc\u00b7bersc\u00b7hleicht", "mein", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die ich nicht meistern kann.", "tokens": ["Die", "ich", "nicht", "meis\u00b7tern", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So sanken finstre Schatten", "tokens": ["So", "san\u00b7ken", "finst\u00b7re", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auch in die Seele mir,", "tokens": ["Auch", "in", "die", "See\u00b7le", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So dunkelt es in mir selber", "tokens": ["So", "dun\u00b7kelt", "es", "in", "mir", "sel\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "ADV"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie einsam ist es hier.", "tokens": ["Wie", "ein\u00b7sam", "ist", "es", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}