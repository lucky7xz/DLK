{"textgrid.poem.67157": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Denn, wie wenn hoch von der herrlichgestimmten, der Orgel", "genre": "verse", "period": "N.A.", "pub_year": 1801, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denn, wie wenn hoch von der herrlichgestimmten, der Orgel", "tokens": ["Denn", ",", "wie", "wenn", "hoch", "von", "der", "herr\u00b7lich\u00b7ges\u00b7timm\u00b7ten", ",", "der", "Or\u00b7gel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "KOKOM", "KOUS", "ADJD", "APPR", "ART", "ADJA", "$,", "ART", "NN"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im heiligen Saal,", "tokens": ["Im", "hei\u00b7li\u00b7gen", "Saal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Reinquillend aus den unersch\u00f6pflichen R\u00f6hren,", "tokens": ["Re\u00b7in\u00b7quil\u00b7lend", "aus", "den", "un\u00b7er\u00b7sch\u00f6pf\u00b7li\u00b7chen", "R\u00f6h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Das Vorspiel, weckend, des Morgens beginnt", "tokens": ["Das", "Vor\u00b7spiel", ",", "we\u00b7ckend", ",", "des", "Mor\u00b7gens", "be\u00b7ginnt"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "ART", "ADV", "VVFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und weitumher, von Halle zu Halle,", "tokens": ["Und", "wei\u00b7tum\u00b7her", ",", "von", "Hal\u00b7le", "zu", "Hal\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "$,", "APPR", "NE", "APPR", "NE", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der erfrischende nun, der melodische Strom rinnt,", "tokens": ["Der", "er\u00b7fri\u00b7schen\u00b7de", "nun", ",", "der", "me\u00b7lo\u00b7di\u00b7sche", "Strom", "rinnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Bis in den kalten Schatten das Haus", "tokens": ["Bis", "in", "den", "kal\u00b7ten", "Schat\u00b7ten", "das", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Von Begeisterungen erf\u00fcllt,", "tokens": ["Von", "Be\u00b7geis\u00b7te\u00b7run\u00b7gen", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Nun aber erwacht ist, nun, aufsteigend ihr,", "tokens": ["Nun", "a\u00b7ber", "er\u00b7wacht", "ist", ",", "nun", ",", "auf\u00b7stei\u00b7gend", "ihr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VAFIN", "$,", "ADV", "$,", "VVPP", "PPER", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Der Sonne des Fests, antwortet", "tokens": ["Der", "Son\u00b7ne", "des", "Fests", ",", "ant\u00b7wor\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Der Chor der Gemeinde: so kam", "tokens": ["Der", "Chor", "der", "Ge\u00b7mein\u00b7de", ":", "so", "kam"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "ADV", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.12": {"text": "Das Wort aus Osten zu uns,", "tokens": ["Das", "Wort", "aus", "Os\u00b7ten", "zu", "uns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "PPER", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Und an Parnassos Felsen und am Kith\u00e4ron h\u00f6r ich,", "tokens": ["Und", "an", "Par\u00b7nas\u00b7sos", "Fel\u00b7sen", "und", "am", "Kit\u00b7h\u00e4\u00b7ron", "h\u00f6r", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "NN", "KON", "APPRART", "NN", "VVFIN", "PPER", "$,"], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.14": {"text": "O Asia, das Echo von dir und es bricht sich", "tokens": ["O", "A\u00b7sia", ",", "das", "E\u00b7cho", "von", "dir", "und", "es", "bricht", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "ART", "NN", "APPR", "PPER", "KON", "PPER", "VVFIN", "PRF"], "meter": "-+--+-+++-+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Am Kapitol und j\u00e4hlings herab von den Alpen", "tokens": ["Am", "Ka\u00b7pi\u00b7tol", "und", "j\u00e4h\u00b7lings", "her\u00b7ab", "von", "den", "Al\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Kommt eine Fremdlingin sie", "tokens": ["Kommt", "ei\u00b7ne", "Fremd\u00b7lin\u00b7gin", "sie"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPER"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zu uns, die Erweckerin,", "tokens": ["Zu", "uns", ",", "die", "Er\u00b7we\u00b7cke\u00b7rin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die menschenbildende Stimme.", "tokens": ["Die", "men\u00b7schen\u00b7bil\u00b7den\u00b7de", "Stim\u00b7me", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Da fa\u00dft' ein Staunen die Seele", "tokens": ["Da", "fa\u00dft'", "ein", "Stau\u00b7nen", "die", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der Getroffenen all und Nacht", "tokens": ["Der", "Ge\u00b7trof\u00b7fe\u00b7nen", "all", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "KON", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "War \u00fcber den Augen der Besten.", "tokens": ["War", "\u00fc\u00b7ber", "den", "Au\u00b7gen", "der", "Bes\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Denn vieles vermag", "tokens": ["Denn", "vie\u00b7les", "ver\u00b7mag"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIS", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Und die Flut und den Fels und Feuersgewalt auch", "tokens": ["Und", "die", "Flut", "und", "den", "Fels", "und", "Feu\u00b7ers\u00b7ge\u00b7walt", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "ART", "NN", "KON", "NN", "ADV"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Bezwinget mit Kunst der Mensch", "tokens": ["Be\u00b7zwin\u00b7get", "mit", "Kunst", "der", "Mensch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Und achtet, der Hochgesinnte, das Schwert", "tokens": ["Und", "ach\u00b7tet", ",", "der", "Hoch\u00b7ge\u00b7sinn\u00b7te", ",", "das", "Schwert"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Nicht, aber es steht", "tokens": ["Nicht", ",", "a\u00b7ber", "es", "steht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "$,", "KON", "PPER", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Vor G\u00f6ttlichem der Starke niedergeschlagen,", "tokens": ["Vor", "G\u00f6tt\u00b7li\u00b7chem", "der", "Star\u00b7ke", "nie\u00b7der\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Und gleichet dem Wild fast; das,", "tokens": ["Und", "glei\u00b7chet", "dem", "Wild", "fast", ";", "das", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NE", "ADV", "$.", "PDS", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von s\u00fc\u00dfer Jugend getrieben,", "tokens": ["Von", "s\u00fc\u00b7\u00dfer", "Ju\u00b7gend", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Schweift rastlos \u00fcber die Berg", "tokens": ["Schweift", "rast\u00b7los", "\u00fc\u00b7ber", "die", "Berg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und f\u00fchlet die eigene Kraft", "tokens": ["Und", "f\u00fch\u00b7let", "die", "ei\u00b7ge\u00b7ne", "Kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "In der Mittagshitze. Wenn aber", "tokens": ["In", "der", "Mit\u00b7tags\u00b7hit\u00b7ze", ".", "Wenn", "a\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "KOUS", "ADV"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Herabgef\u00fchrt, in spielenden L\u00fcften,", "tokens": ["Her\u00b7ab\u00b7ge\u00b7f\u00fchrt", ",", "in", "spie\u00b7len\u00b7den", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Das heilige Licht, und mit dem k\u00fchleren Strahl", "tokens": ["Das", "hei\u00b7li\u00b7ge", "Licht", ",", "und", "mit", "dem", "k\u00fch\u00b7le\u00b7ren", "Strahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Der freudige Geist kommt zu", "tokens": ["Der", "freu\u00b7di\u00b7ge", "Geist", "kommt", "zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Der seligen Erde, dann erliegt es, ungewohnt", "tokens": ["Der", "se\u00b7li\u00b7gen", "Er\u00b7de", ",", "dann", "er\u00b7liegt", "es", ",", "un\u00b7ge\u00b7wohnt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "$,", "ADJD"], "meter": "+---+-+-+-+-+", "measure": "dactylic.init"}, "line.10": {"text": "Des Sch\u00f6nsten, und schlummert wachenden Schlaf,", "tokens": ["Des", "Sch\u00f6ns\u00b7ten", ",", "und", "schlum\u00b7mert", "wa\u00b7chen\u00b7den", "Schlaf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Noch ehe Gestirn naht. So auch wir. Denn manchen erlosch", "tokens": ["Noch", "e\u00b7he", "Ge\u00b7stirn", "naht", ".", "So", "auch", "wir", ".", "Denn", "man\u00b7chen", "er\u00b7losch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOUS", "NN", "VVFIN", "$.", "ADV", "ADV", "PPER", "$.", "KON", "PIAT", "NN"], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.12": {"text": "Das Augenlicht schon vor den g\u00f6ttlichgesendeten Gaben,", "tokens": ["Das", "Au\u00b7gen\u00b7licht", "schon", "vor", "den", "g\u00f6tt\u00b7lich\u00b7ge\u00b7sen\u00b7de\u00b7ten", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.4": {"line.1": {"text": "Den freundlichen, die aus Ionien uns,", "tokens": ["Den", "freund\u00b7li\u00b7chen", ",", "die", "aus", "I\u00b7o\u00b7ni\u00b7en", "uns", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "APPR", "NE", "PPER", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auch aus Arabia kamen, und froh ward", "tokens": ["Auch", "aus", "A\u00b7ra\u00b7bia", "ka\u00b7men", ",", "und", "froh", "ward"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "VVFIN", "$,", "KON", "ADJD", "VAFIN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Der teuern Lehr und auch der holden Ges\u00e4nge", "tokens": ["Der", "teu\u00b7ern", "Lehr", "und", "auch", "der", "hol\u00b7den", "Ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die Seele jener Entschlafenen nie,", "tokens": ["Die", "See\u00b7le", "je\u00b7ner", "Ent\u00b7schla\u00b7fe\u00b7nen", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Doch einige wachten. Und sie wandelten oft", "tokens": ["Doch", "ei\u00b7ni\u00b7ge", "wach\u00b7ten", ".", "Und", "sie", "wan\u00b7del\u00b7ten", "oft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "$.", "KON", "PPER", "VVFIN", "ADV"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Zufrieden unter euch, ihr B\u00fcrger sch\u00f6ner St\u00e4dte,", "tokens": ["Zu\u00b7frie\u00b7den", "un\u00b7ter", "euch", ",", "ihr", "B\u00fcr\u00b7ger", "sch\u00f6\u00b7ner", "St\u00e4d\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Beim Kampfspiel, wo sonst unsichtbar der Heros", "tokens": ["Beim", "Kampf\u00b7spiel", ",", "wo", "sonst", "un\u00b7sicht\u00b7bar", "der", "He\u00b7ros"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "ADV", "ADJD", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Geheim bei Dichtern sa\u00df, die Ringer schaut' und l\u00e4chelnd", "tokens": ["Ge\u00b7heim", "bei", "Dich\u00b7tern", "sa\u00df", ",", "die", "Rin\u00b7ger", "schaut'", "und", "l\u00e4\u00b7chelnd"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Pries, der gepriesene, die m\u00fc\u00dfigernsten Kinder.", "tokens": ["Pries", ",", "der", "ge\u00b7prie\u00b7se\u00b7ne", ",", "die", "m\u00fc\u00b7\u00dfi\u00b7gerns\u00b7ten", "Kin\u00b7der", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+---+-+-+-", "measure": "dactylic.di.plus"}, "line.10": {"text": "Ein unaufh\u00f6rlich Lieben wars und ists.", "tokens": ["Ein", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "Lie\u00b7ben", "wars", "und", "ists", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "VAFIN", "KON", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und wohlgeschieden, aber darum denken", "tokens": ["Und", "wohl\u00b7ge\u00b7schie\u00b7den", ",", "a\u00b7ber", "da\u00b7rum", "den\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "KON", "PAV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "li