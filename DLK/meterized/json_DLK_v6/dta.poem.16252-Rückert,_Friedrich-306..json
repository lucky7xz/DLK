{"dta.poem.16252": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "306.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aufmerksamkeit, mein Sohn, ist was ich dir empfehle:", "tokens": ["Auf\u00b7merk\u00b7sam\u00b7keit", ",", "mein", "Sohn", ",", "ist", "was", "ich", "dir", "emp\u00b7feh\u00b7le", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "VAFIN", "PWS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bei dem, wobei du bist, zu seyn mit ganzer Seele.", "tokens": ["Bei", "dem", ",", "wo\u00b7bei", "du", "bist", ",", "zu", "seyn", "mit", "gan\u00b7zer", "See\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PWAV", "PPER", "VAFIN", "$,", "PTKZU", "VAINF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn du an andres denkst, als was dein Lehrer spricht,", "tokens": ["Wenn", "du", "an", "and\u00b7res", "denkst", ",", "als", "was", "dein", "Leh\u00b7rer", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIS", "VVFIN", "$,", "KOUS", "PIS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So h\u00f6rst du dis nur halb, und in dir haftets nicht.", "tokens": ["So", "h\u00f6rst", "du", "dis", "nur", "halb", ",", "und", "in", "dir", "haf\u00b7tets", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "ADV", "ADJD", "$,", "KON", "APPR", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du aber brauchst zum Gl\u00fcck an andres nicht zu denken,", "tokens": ["Du", "a\u00b7ber", "brauchst", "zum", "Gl\u00fcck", "an", "and\u00b7res", "nicht", "zu", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPRART", "NN", "APPR", "PIS", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und kanst Aufmerksamkeit mir ungetheilte schenken.", "tokens": ["Und", "kanst", "Auf\u00b7merk\u00b7sam\u00b7keit", "mir", "un\u00b7ge\u00b7theil\u00b7te", "schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "PPER", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das ist der Vorzug, den der Knabe hat vorm Mann,", "tokens": ["Das", "ist", "der", "Vor\u00b7zug", ",", "den", "der", "Kna\u00b7be", "hat", "vorm", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der eignen Denkens sich nicht mehr entschlagen kann.", "tokens": ["Der", "eig\u00b7nen", "Den\u00b7kens", "sich", "nicht", "mehr", "ent\u00b7schla\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}}, "stanza.5": {"line.1": {"text": "Er hat bei allem, was er h\u00f6rt, soviel zu denken,", "tokens": ["Er", "hat", "bei", "al\u00b7lem", ",", "was", "er", "h\u00f6rt", ",", "so\u00b7viel", "zu", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er kein voll Geh\u00f6r kann dem Geh\u00f6rten schenken.", "tokens": ["Da\u00df", "er", "kein", "voll", "Ge\u00b7h\u00f6r", "kann", "dem", "Ge\u00b7h\u00f6r\u00b7ten", "schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "ADJD", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}}}}