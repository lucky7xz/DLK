{"textgrid.poem.49592": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Sonnwendfeuer", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drunt'n is Nacht,", "tokens": ["Drunt'n", "is", "Nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "FM", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Alle Liachta ausg'macht,", "tokens": ["Al\u00b7le", "Liach\u00b7ta", "aus\u00b7g'\u00b7macht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Finsta und schwarz \u00fcberall'n.", "tokens": ["Fins\u00b7ta", "und", "schwarz", "\u00fc\u00b7ber\u00b7all'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["NE", "KON", "ADJD", "APPR", "NE"], "meter": "+--++-+", "measure": "dactylic.init"}, "line.4": {"text": "Buama, la\u00df ma's uns g'fall'n?", "tokens": ["Bu\u00b7a\u00b7ma", ",", "la\u00df", "ma's", "uns", "g'\u00b7fall'n", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "NE", "PPER", "VVINF", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Schaugt's umanand!", "tokens": ["Schaugt's", "u\u00b7ma\u00b7nand", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Herrgott, wia sch\u00f6 is d\u00f6s Land!", "tokens": ["Herr\u00b7gott", ",", "wia", "sch\u00f6", "is", "d\u00f6s", "Land", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "FM", "FM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00fca\u00df ma's de Schwarz'n verschreib'n?", "tokens": ["M\u00fc\u00b7a\u00df", "ma's", "de", "Schwa\u00b7rz'n", "ver\u00b7schreib'n", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Da\u00df uns de Freud au\u00dfi treib'n?", "tokens": ["Da\u00df", "uns", "de", "Freud", "au\u00b7\u00dfi", "treib'n", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "NE", "NE", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Hui! Sunnawend!", "tokens": ["Hui", "!", "Sun\u00b7na\u00b7wend", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["FM", "$.", "NN", "$."], "meter": "+---", "measure": "dactylic.init"}, "line.2": {"text": "Her mit de Scheiteln und brennt!", "tokens": ["Her", "mit", "de", "Schei\u00b7teln", "und", "brennt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "NN", "KON", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Leucht'n halt do no amal", "tokens": ["Leucht'n", "halt", "do", "no", "a\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Lustige Feuer ins Tal!", "tokens": ["Lus\u00b7ti\u00b7ge", "Feu\u00b7er", "ins", "Tal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Sehg'n sie an Schei',", "tokens": ["Seh\u00b7g'n", "sie", "an", "Schei'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Kunnt ja d\u00f6s aa'r amal sei,", "tokens": ["Kunnt", "ja", "d\u00f6s", "aa'r", "a\u00b7mal", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NE", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sie's spanna da drunt,", "tokens": ["Da\u00df", "sie's", "span\u00b7na", "da", "drunt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wia ma's heller hamm kunnt.", "tokens": ["Wia", "ma's", "hel\u00b7ler", "hamm", "kunnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}