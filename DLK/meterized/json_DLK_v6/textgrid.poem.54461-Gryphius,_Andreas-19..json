{"textgrid.poem.54461": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geh immer weil dich ja von mir gelust zu gehen", "tokens": ["Geh", "im\u00b7mer", "weil", "dich", "ja", "von", "mir", "ge\u00b7lust", "zu", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "KOUS", "PPER", "ADV", "APPR", "PPER", "VVPP", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd suche Preussens haubt die mehr den sch\u00f6ne stadt", "tokens": ["Vnd", "su\u00b7che", "Preus\u00b7sens", "haubt", "die", "mehr", "den", "sch\u00f6\u00b7ne", "stadt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ART", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die aller v\u00f6lcker zier vndt gaben in sich hatt/", "tokens": ["Die", "al\u00b7ler", "v\u00f6l\u00b7cker", "zier", "vndt", "ga\u00b7ben", "in", "sich", "hatt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "KON", "VVFIN", "APPR", "PRF", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd sprich dem zue durch den die grosse stadt kan stehen/", "tokens": ["Vnd", "sprich", "dem", "zue", "durch", "den", "die", "gros\u00b7se", "stadt", "kan", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "ART", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den tugend hatt bewehrt/ den trewe must erh\u00f6hen/", "tokens": ["Den", "tu\u00b7gend", "hatt", "be\u00b7wehrt", "/", "den", "tre\u00b7we", "must", "er\u00b7h\u00f6\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den redlikeit erkiest/ der stets gewissen raht", "tokens": ["Den", "red\u00b7li\u00b7keit", "er\u00b7kiest", "/", "der", "stets", "ge\u00b7wis\u00b7sen", "raht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In schweren f\u00e4llen findt/ vnd der sich in der thatt", "tokens": ["In", "schwe\u00b7ren", "f\u00e4l\u00b7len", "findt", "/", "vnd", "der", "sich", "in", "der", "thatt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "VVINF", "VVFIN", "$(", "KON", "ART", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Verst\u00e4ndig hatt erweist/ die blume der ", "tokens": ["Ver\u00b7st\u00e4n\u00b7dig", "hatt", "er\u00b7weist", "/", "die", "blu\u00b7me", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "VVPP", "$(", "ART", "NN", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ob schon ", "tokens": ["Ob", "schon"], "token_info": ["word", "word"], "pos": ["KOUS", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "Der so viel sprachen redt/ ob ihm schon ", "tokens": ["Der", "so", "viel", "spra\u00b7chen", "redt", "/", "ob", "ihm", "schon"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "VVFIN", "VVFIN", "$(", "KOUS", "PPER", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Ist seiner freundlikeit/ doch gar nichts gleich zue sch\u00e4tzen:", "tokens": ["Ist", "sei\u00b7ner", "freund\u00b7li\u00b7keit", "/", "doch", "gar", "nichts", "gleich", "zue", "sch\u00e4t\u00b7zen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$(", "ADV", "ADV", "PIS", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Er ists der mir viel ehr vnd mehr als ehr erweist/", "tokens": ["Er", "ists", "der", "mir", "viel", "ehr", "vnd", "mehr", "als", "ehr", "er\u00b7weist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "PPER", "PIAT", "NN", "KON", "ADV", "KOUS", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Er ists der mich nicht nur ins angesicht gepreist.", "tokens": ["Er", "ists", "der", "mich", "nicht", "nur", "ins", "an\u00b7ge\u00b7sicht", "ge\u00b7preist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "PPER", "PTKNEG", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wehn solt ich dir wol sonst zum freund vnd richter setzen.", "tokens": ["Wehn", "solt", "ich", "dir", "wol", "sonst", "zum", "freund", "vnd", "rich\u00b7ter", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "ADV", "ADV", "APPRART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}