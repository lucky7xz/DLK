{"textgrid.poem.56251": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Tragische Geschichte", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "'s war einer, dem's zu Herzen ging,", "tokens": ["'s", "war", "ei\u00b7ner", ",", "dem's", "zu", "Her\u00b7zen", "ging", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df ihm der Zopf so hinten hing,", "tokens": ["Da\u00df", "ihm", "der", "Zopf", "so", "hin\u00b7ten", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er wollt es anders haben.", "tokens": ["Er", "wollt", "es", "an\u00b7ders", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So denkt er denn: wie fang ich's an?", "tokens": ["So", "denkt", "er", "denn", ":", "wie", "fang", "ich's", "an", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "PWAV", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich dreh mich um, so ist's getan \u2013", "tokens": ["Ich", "dreh", "mich", "um", ",", "so", "ist's", "ge\u00b7tan", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Zopf, der h\u00e4ngt ihm hinten.", "tokens": ["Der", "Zopf", ",", "der", "h\u00e4ngt", "ihm", "hin\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da hat er flink sich umgedreht,", "tokens": ["Da", "hat", "er", "flink", "sich", "um\u00b7ge\u00b7dreht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wie es stund, es annoch steht \u2013", "tokens": ["Und", "wie", "es", "stund", ",", "es", "an\u00b7noch", "steht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Zopf, der h\u00e4ngt ihm hinten.", "tokens": ["Der", "Zopf", ",", "der", "h\u00e4ngt", "ihm", "hin\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da dreht er schnell sich anders 'rum,", "tokens": ["Da", "dreht", "er", "schnell", "sich", "an\u00b7ders", "'rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PRF", "ADV", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "'s wird aber noch nicht besser drum \u2013", "tokens": ["'s", "wird", "a\u00b7ber", "noch", "nicht", "bes\u00b7ser", "drum", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PTKNEG", "ADJD", "PAV", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Der Zopf, der h\u00e4ngt ihm hinten.", "tokens": ["Der", "Zopf", ",", "der", "h\u00e4ngt", "ihm", "hin\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er dreht sich links, er dreht sich rechts,", "tokens": ["Er", "dreht", "sich", "links", ",", "er", "dreht", "sich", "rechts", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "$,", "PPER", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es tut nichts Guts, es tut nichts Schlechts \u2013", "tokens": ["Es", "tut", "nichts", "Guts", ",", "es", "tut", "nichts", "Schlechts", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "NN", "$,", "PPER", "VVFIN", "PIS", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Zopf, der h\u00e4ngt ihm hinten.", "tokens": ["Der", "Zopf", ",", "der", "h\u00e4ngt", "ihm", "hin\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er dreht sich wie ein Kreisel fort,", "tokens": ["Er", "dreht", "sich", "wie", "ein", "Krei\u00b7sel", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "KOKOM", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es hilft zu nichts, in einem Wort \u2013", "tokens": ["Es", "hilft", "zu", "nichts", ",", "in", "ei\u00b7nem", "Wort", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIS", "$,", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Zopf, der h\u00e4ngt ihm hinten.", "tokens": ["Der", "Zopf", ",", "der", "h\u00e4ngt", "ihm", "hin\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und seht, er dreht sich immer noch,", "tokens": ["Und", "seht", ",", "er", "dreht", "sich", "im\u00b7mer", "noch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VVFIN", "PRF", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und denkt: es hilft am Ende doch \u2013", "tokens": ["Und", "denkt", ":", "es", "hilft", "am", "En\u00b7de", "doch", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VVFIN", "APPRART", "NN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Zopf, der h\u00e4ngt ihm hinten.", "tokens": ["Der", "Zopf", ",", "der", "h\u00e4ngt", "ihm", "hin\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}