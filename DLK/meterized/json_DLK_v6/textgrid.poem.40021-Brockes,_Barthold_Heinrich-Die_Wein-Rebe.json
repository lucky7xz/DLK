{"textgrid.poem.40021": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Wein-Rebe", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne stund schon in der g\u00fcld'nen Wage,", "tokens": ["Die", "Son\u00b7ne", "stund", "schon", "in", "der", "g\u00fcld'\u00b7nen", "Wa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als ich, an einem heitern Tage,", "tokens": ["Als", "ich", ",", "an", "ei\u00b7nem", "hei\u00b7tern", "Ta\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In einen Saal, der S\u00fcd-w\u00e4rts lage, trat,", "tokens": ["In", "ei\u00b7nen", "Saal", ",", "der", "S\u00fcd\u00b7w\u00e4rts", "la\u00b7ge", ",", "trat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den, weil ihm manches Reben-Blatt", "tokens": ["Den", ",", "weil", "ihm", "man\u00b7ches", "Re\u00b7ben\u00b7Blatt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die klaren Fenster gantz verh\u00fcllte,", "tokens": ["Die", "kla\u00b7ren", "Fens\u00b7ter", "gantz", "ver\u00b7h\u00fcll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein gr\u00fcner Schatten gantz erf\u00fcllte.", "tokens": ["Ein", "gr\u00fc\u00b7ner", "Schat\u00b7ten", "gantz", "er\u00b7f\u00fcll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es schmeichelte das halb gebroch'ne Licht,", "tokens": ["Es", "schmei\u00b7chel\u00b7te", "das", "halb", "ge\u00b7bro\u00b7ch'\u00b7ne", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Durch das begr\u00fcnte Laub gef\u00e4rbt, mir das Gesicht,", "tokens": ["Durch", "das", "be\u00b7gr\u00fcn\u00b7te", "Laub", "ge\u00b7f\u00e4rbt", ",", "mir", "das", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So da\u00df ich mich vergn\u00fcgt ans Fenster setzte,", "tokens": ["So", "da\u00df", "ich", "mich", "ver\u00b7gn\u00fcgt", "ans", "Fens\u00b7ter", "setz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PRF", "VVPP", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am sch\u00f6nen Sonnen-Schirm des Wein-Stocks mich ergetzte,", "tokens": ["Am", "sch\u00f6\u00b7nen", "Son\u00b7nen\u00b7Schirm", "des", "Wein\u00b7Stocks", "mich", "er\u00b7getz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Durch seinen Schmuck ger\u00fchrt, die Sinnen aufw\u00e4rts triebe,", "tokens": ["Durch", "sei\u00b7nen", "Schmuck", "ge\u00b7r\u00fchrt", ",", "die", "Sin\u00b7nen", "auf\u00b7w\u00e4rts", "trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und, zu des Sch\u00f6pfers Ruhm, Sein sch\u00f6n Gesch\u00f6pf beschriebe.", "tokens": ["Und", ",", "zu", "des", "Sch\u00f6p\u00b7fers", "Ruhm", ",", "Sein", "sch\u00f6n", "Ge\u00b7sch\u00f6pf", "be\u00b7schrie\u00b7be", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "NN", "$,", "PPOSAT", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Man kann auf Erden nichts so sch\u00f6n,", "tokens": ["Man", "kann", "auf", "Er\u00b7den", "nichts", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "NN", "PIS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als zartes Laub in solcher Stellung, sehn,", "tokens": ["Als", "zar\u00b7tes", "Laub", "in", "sol\u00b7cher", "Stel\u00b7lung", ",", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "PIAT", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da nemlich Licht und Tag durch sein Gewebe scheint,", "tokens": ["Da", "nem\u00b7lich", "Licht", "und", "Tag", "durch", "sein", "Ge\u00b7we\u00b7be", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und mit dem Gr\u00fcnen sich ein g\u00fcld'ner Strahl vereint,", "tokens": ["Und", "mit", "dem", "Gr\u00fc\u00b7nen", "sich", "ein", "g\u00fcld'\u00b7ner", "Strahl", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PRF", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wodurch man alle Pracht, die in den Bl\u00e4ttern stecket,", "tokens": ["Wo\u00b7durch", "man", "al\u00b7le", "Pracht", ",", "die", "in", "den", "Bl\u00e4t\u00b7tern", "ste\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PIAT", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In der Durchsichtigkeit am deutlichsten entdecket.", "tokens": ["In", "der", "Durch\u00b7sich\u00b7tig\u00b7keit", "am", "deut\u00b7lichs\u00b7ten", "ent\u00b7de\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "VVFIN", "$."], "meter": "--+--+-+---+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Wie wann des Nachts ein Licht", "tokens": ["Wie", "wann", "des", "Nachts", "ein", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PWAV", "ART", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Durch ein mit Oel getr\u00e4nckt Papier, das bunt bemalet,", "tokens": ["Durch", "ein", "mit", "O\u00b7el", "ge\u00b7tr\u00e4nckt", "Pa\u00b7pier", ",", "das", "bunt", "be\u00b7ma\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "NN", "VVPP", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Mit sch\u00f6n gef\u00e4rbtem Glantz und buntem Schimmer strahlet;", "tokens": ["Mit", "sch\u00f6n", "ge\u00b7f\u00e4rb\u00b7tem", "Glantz", "und", "bun\u00b7tem", "Schim\u00b7mer", "strah\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So strahl't, so drenget sich und bricht", "tokens": ["So", "strahl't", ",", "so", "dren\u00b7get", "sich", "und", "bricht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "PRF", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der hellen Sonnen Strahl, bey heiterm Wetter,", "tokens": ["Der", "hel\u00b7len", "Son\u00b7nen", "Strahl", ",", "bey", "hei\u00b7term", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Durch die so sch\u00f6n geformt- als sch\u00f6n gef\u00e4rbten Bl\u00e4tter.", "tokens": ["Durch", "die", "so", "sch\u00f6n", "ge\u00b7form\u00b7t", "als", "sch\u00f6n", "ge\u00b7f\u00e4rb\u00b7ten", "Bl\u00e4t\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJD", "TRUNC", "KOKOM", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Viel tausend Aederchen, die sonsten nicht zu sehn,", "tokens": ["Viel", "tau\u00b7send", "A\u00b7e\u00b7der\u00b7chen", ",", "die", "sons\u00b7ten", "nicht", "zu", "sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "$,", "PRELS", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Wodurch die Nahrungs-S\u00e4fte fliessen,", "tokens": ["Wo\u00b7durch", "die", "Nah\u00b7rungs\u00b7S\u00e4f\u00b7te", "flies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sieht man sodann in sch\u00f6nster Ordnung stehn.", "tokens": ["Sieht", "man", "so\u00b7dann", "in", "sch\u00f6ns\u00b7ter", "Ord\u00b7nung", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Es scheint, als ob ein jedes Blatt", "tokens": ["Es", "scheint", ",", "als", "ob", "ein", "je\u00b7des", "Blatt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "KOUS", "ART", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die Bildung eines Baumes hat.", "tokens": ["Die", "Bil\u00b7dung", "ei\u00b7nes", "Bau\u00b7mes", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der Stengel ist der Stamm; Aus diesem Stamm' entspriessen", "tokens": ["Der", "Sten\u00b7gel", "ist", "der", "Stamm", ";", "Aus", "die\u00b7sem", "Stamm'", "ent\u00b7spries\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$.", "APPR", "PDAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "F\u00fcnf Haupt-Zweig', und aus diesen Zweigen,", "tokens": ["F\u00fcnf", "Haup\u00b7tZweig'", ",", "und", "aus", "die\u00b7sen", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "KON", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "An deren jedem sich f\u00fcnf Neben-Aeste zeigen,", "tokens": ["An", "de\u00b7ren", "je\u00b7dem", "sich", "f\u00fcnf", "Ne\u00b7ben\u00b7Aes\u00b7te", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "PIS", "PRF", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Entsteht ein Bl\u00e4tter-Heer, die darin blo\u00df allein", "tokens": ["Ent\u00b7steht", "ein", "Bl\u00e4t\u00b7ter\u00b7Heer", ",", "die", "da\u00b7rin", "blo\u00df", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "PAV", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Von denen unterschieden seyn,", "tokens": ["Von", "de\u00b7nen", "un\u00b7ter\u00b7schie\u00b7den", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Womit sonst and're B\u00e4ume prangen,", "tokens": ["Wo\u00b7mit", "sonst", "an\u00b7d'\u00b7re", "B\u00e4u\u00b7me", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.18": {"text": "Da\u00df and're Bl\u00e4tter frey, da die zusammen hangen.", "tokens": ["Da\u00df", "an\u00b7d'\u00b7re", "Bl\u00e4t\u00b7ter", "frey", ",", "da", "die", "zu\u00b7sam\u00b7men", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PTKVZ", "$,", "KOUS", "ART", "ADV", "VVINF", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Je mehr man sie besieht, je mehr man sie betracht't,", "tokens": ["Je", "mehr", "man", "sie", "be\u00b7sieht", ",", "je", "mehr", "man", "sie", "be\u00b7tracht't", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "PPER", "VVFIN", "$,", "ADV", "ADV", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Je mehr vermehret sich die Lust in ihrer Pracht.", "tokens": ["Je", "mehr", "ver\u00b7meh\u00b7ret", "sich", "die", "Lust", "in", "ih\u00b7rer", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Zumahl an einigen, die aus der Maassen sch\u00f6n,", "tokens": ["Zu\u00b7mahl", "an", "ei\u00b7ni\u00b7gen", ",", "die", "aus", "der", "Maas\u00b7sen", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "$,", "PRELS", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Vermocht' mein Auge sich nicht satt zu sehn,", "tokens": ["Ver\u00b7mocht'", "mein", "Au\u00b7ge", "sich", "nicht", "satt", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PRF", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Die gantz Zinnober-roth, wie reines Schnecken-Blut:", "tokens": ["Die", "gantz", "Zin\u00b7no\u00b7ber\u00b7roth", ",", "wie", "rei\u00b7nes", "Schne\u00b7cken\u00b7Blut", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "$,", "PWAV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ja wie ein funckelnder Rubin,", "tokens": ["Ja", "wie", "ein", "fun\u00b7ckeln\u00b7der", "Ru\u00b7bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOKOM", "ART", "ADJA", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.25": {"text": "Da jedes von der Sonnen-Gluht,", "tokens": ["Da", "je\u00b7des", "von", "der", "Son\u00b7nen\u00b7Gluht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Die durch sie strahlt', ein funckelnd Licht erhielte:", "tokens": ["Die", "durch", "sie", "strahlt'", ",", "ein", "fun\u00b7ckelnd", "Licht", "er\u00b7hiel\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$,", "ART", "VVPP", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Und weil der Adern helles Gr\u00fcn", "tokens": ["Und", "weil", "der", "A\u00b7dern", "hel\u00b7les", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Smaragden-gleich,