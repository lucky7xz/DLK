{"textgrid.poem.67707": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die Vorsehung", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.85", "en:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und was soll mich Noth und Tod", "tokens": ["Und", "was", "soll", "mich", "Noth", "und", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VMFIN", "PPER", "NN", "KON", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Nur im Bilde schrecken?", "tokens": ["Nur", "im", "Bil\u00b7de", "schre\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Immer ja in Gottes Hand,", "tokens": ["Im\u00b7mer", "ja", "in", "Got\u00b7tes", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird sie stets mich decken,", "tokens": ["Wird", "sie", "stets", "mich", "de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wohin der Weg sich wende.", "tokens": ["Wo\u00b7hin", "der", "Weg", "sich", "wen\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wer war es, der auf diese Welt", "tokens": ["Wer", "war", "es", ",", "der", "auf", "die\u00b7se", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "$,", "PRELS", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mich, eh ich noch war, gestellt?", "tokens": ["Mich", ",", "eh", "ich", "noch", "war", ",", "ge\u00b7stellt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "PPER", "ADV", "VAFIN", "$,", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Der schon f\u00fcr mich gedacht,", "tokens": ["Der", "schon", "f\u00fcr", "mich", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Mich, was ich bin, gemacht,", "tokens": ["Mich", ",", "was", "ich", "bin", ",", "ge\u00b7macht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "PWS", "PPER", "VAFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Mich der Welt, die Welt f\u00fcr mich bereitet?", "tokens": ["Mich", "der", "Welt", ",", "die", "Welt", "f\u00fcr", "mich", "be\u00b7rei\u00b7tet", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Ein Vater, ein Gott!", "tokens": ["Ein", "Va\u00b7ter", ",", "ein", "Gott", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Ewiger Gedanke!", "tokens": ["E\u00b7wi\u00b7ger", "Ge\u00b7dan\u00b7ke", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+---+-", "measure": "dactylic.init"}, "line.13": {"text": "Vater, Gott, so bist es Du,", "tokens": ["Va\u00b7ter", ",", "Gott", ",", "so", "bist", "es", "Du", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADV", "VAFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Der stets mich leitet.", "tokens": ["Der", "stets", "mich", "lei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Einst in meiner Mutter Schoo\u00df,", "tokens": ["Einst", "in", "mei\u00b7ner", "Mut\u00b7ter", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der tiefen Fremde kam", "tokens": ["Aus", "der", "tie\u00b7fen", "Frem\u00b7de", "kam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich in Fremde. Weinen", "tokens": ["Ich", "in", "Frem\u00b7de", ".", "Wei\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "APPR", "NN", "$.", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "War meine erste Stimme.", "tokens": ["War", "mei\u00b7ne", "ers\u00b7te", "Stim\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "War nie gekannt und doch gekannt,", "tokens": ["War", "nie", "ge\u00b7kannt", "und", "doch", "ge\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schon geliebt und Kind genannt,", "tokens": ["Schon", "ge\u00b7liebt", "und", "Kind", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Fand vor mir Vaterarm,", "tokens": ["Fand", "vor", "mir", "Va\u00b7te\u00b7rarm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Fand vor mir Mutterbrust,", "tokens": ["Fand", "vor", "mir", "Mut\u00b7ter\u00b7brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Fand selbst Schmerzen mir als Liebesbande,", "tokens": ["Fand", "selbst", "Schmer\u00b7zen", "mir", "als", "Lie\u00b7bes\u00b7ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "PPER", "KOUS", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Als Bande ans Herz", "tokens": ["Als", "Ban\u00b7de", "ans", "Herz"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPRART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "V\u00e4terlich bereitet.", "tokens": ["V\u00e4\u00b7ter\u00b7lich", "be\u00b7rei\u00b7tet", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Schwachheit, Noth, die Thr\u00e4ne selbst", "tokens": ["Schwach\u00b7heit", ",", "Noth", ",", "die", "Thr\u00e4\u00b7ne", "selbst"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Ward Band der Liebe.", "tokens": ["Ward", "Band", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ewiger, der also mich", "tokens": ["E\u00b7wi\u00b7ger", ",", "der", "al\u00b7so", "mich"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJA", "$,", "PRELS", "ADV", "PPER"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Lie\u00df geboren werden,", "tokens": ["Lie\u00df", "ge\u00b7bo\u00b7ren", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "VAINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Du bist vor mir, leitest mich", "tokens": ["Du", "bist", "vor", "mir", ",", "lei\u00b7test", "mich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$,", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch zu bessern Erden,", "tokens": ["Auch", "zu", "bes\u00b7sern", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Hast meinen Tritt gez\u00e4hlet.", "tokens": ["Hast", "mei\u00b7nen", "Tritt", "ge\u00b7z\u00e4h\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Bl\u00fcthe reifet dort zur Frucht;", "tokens": ["Die", "Bl\u00fc\u00b7the", "rei\u00b7fet", "dort", "zur", "Frucht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dort find' ich, was mein Herz sich sucht", "tokens": ["Dort", "find'", "ich", ",", "was", "mein", "Herz", "sich", "sucht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "PPOSAT", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und hier nicht finden kann;", "tokens": ["Und", "hier", "nicht", "fin\u00b7den", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Du nimmst den Spr\u00f6\u00dfling an,", "tokens": ["Du", "nimmst", "den", "Spr\u00f6\u00df\u00b7ling", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Pflanzest weiter ihn auf Himmelsauen.", "tokens": ["Pflan\u00b7zest", "wei\u00b7ter", "ihn", "auf", "Him\u00b7mel\u00b7sau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Sei's Veilchen im Thal", "tokens": ["Sei's", "Veil\u00b7chen", "im", "Thal"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "APPRART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Oder Ceder Gottes, \u2013", "tokens": ["O\u00b7der", "Ce\u00b7der", "Got\u00b7tes", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "NN", "$,", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Alle, Alle bl\u00fchen wir", "tokens": ["Al\u00b7le", ",", "Al\u00b7le", "bl\u00fc\u00b7hen", "wir"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PIS", "$,", "PIS", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "In Gottes Reiche.", "tokens": ["In", "Got\u00b7tes", "Rei\u00b7che", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}