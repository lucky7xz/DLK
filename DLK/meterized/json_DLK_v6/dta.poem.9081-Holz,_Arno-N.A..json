{"dta.poem.9081": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In einem Garten", "tokens": ["In", "ei\u00b7nem", "Gar\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "unter dunklen B\u00e4umen", "tokens": ["un\u00b7ter", "dunk\u00b7len", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "erwarten wir die Fr\u00fchlingsnacht.", "tokens": ["er\u00b7war\u00b7ten", "wir", "die", "Fr\u00fch\u00b7lings\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Noch gl\u00e4nzt kein Stern.", "tokens": ["Noch", "gl\u00e4nzt", "kein", "Stern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Aus einem Fenster,", "tokens": ["Aus", "ei\u00b7nem", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "schwellend,", "tokens": ["schwel\u00b7lend", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "die T\u00f6ne einer Geige. . . . ", "tokens": ["die", "T\u00f6\u00b7ne", "ei\u00b7ner", "Gei\u00b7ge", ".", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "$.", "$.", "$.", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Goldregen blinkt,", "tokens": ["Der", "Gold\u00b7re\u00b7gen", "blinkt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "der Flieder duftet,", "tokens": ["der", "Flie\u00b7der", "duf\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "in unsern Herzen geht der Mond auf!", "tokens": ["in", "un\u00b7sern", "Her\u00b7zen", "geht", "der", "Mond", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}