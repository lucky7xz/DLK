{"textgrid.poem.39143": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es schlief ein Junker auf blumigem Grund", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es schlief ein Junker auf blumigem Grund", "tokens": ["Es", "schlief", "ein", "Jun\u00b7ker", "auf", "blu\u00b7mi\u00b7gem", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im schweigenden Waldesd\u00fcster,", "tokens": ["Im", "schwei\u00b7gen\u00b7den", "Wal\u00b7des\u00b7d\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es tanzten die Elfen auf gr\u00fcnem Rund", "tokens": ["Es", "tanz\u00b7ten", "die", "El\u00b7fen", "auf", "gr\u00fc\u00b7nem", "Rund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Mit neckischem Liebesgefl\u00fcster;", "tokens": ["Mit", "ne\u00b7cki\u00b7schem", "Lie\u00b7bes\u00b7ge\u00b7fl\u00fcs\u00b7ter", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Sie tanzten dahin im losen Spiel", "tokens": ["Sie", "tanz\u00b7ten", "da\u00b7hin", "im", "lo\u00b7sen", "Spiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Bei lauschigem Mondenscheine:", "tokens": ["Bei", "lau\u00b7schi\u00b7gem", "Mon\u00b7den\u00b7schei\u00b7ne", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die K\u00f6nigin auf die Nase fiel", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", "auf", "die", "Na\u00b7se", "fiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Wohl \u00fcber des Junkers Beine.", "tokens": ["Wohl", "\u00fc\u00b7ber", "des", "Jun\u00b7kers", "Bei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie hat sich am g\u00fcldenen Sporenrad", "tokens": ["Sie", "hat", "sich", "am", "g\u00fcl\u00b7de\u00b7nen", "Spo\u00b7ren\u00b7rad"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Spinnenweb'robe zerrissen,", "tokens": ["Die", "Spin\u00b7nen\u00b7we\u00b7b'\u00b7ro\u00b7be", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie hat in des Mehltaus frostigem Bad", "tokens": ["Sie", "hat", "in", "des", "Mehl\u00b7taus", "fros\u00b7ti\u00b7gem", "Bad"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "ADJA", "NE"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den Schnupfen sich holen m\u00fcssen,", "tokens": ["Den", "Schnup\u00b7fen", "sich", "ho\u00b7len", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "VMINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sie hat sich zerzaust die Wiener Frisur,", "tokens": ["Sie", "hat", "sich", "zer\u00b7zaust", "die", "Wie\u00b7ner", "Fri\u00b7sur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Da haben die Elfen mit hohem Schwur", "tokens": ["Da", "ha\u00b7ben", "die", "El\u00b7fen", "mit", "ho\u00b7hem", "Schwur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Dem T\u00e4ter Rache geschworen.", "tokens": ["Dem", "T\u00e4\u00b7ter", "Ra\u00b7che", "ge\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Den F\u00e4cher die K\u00f6nigin nahm geschwind,", "tokens": ["Den", "F\u00e4\u00b7cher", "die", "K\u00f6\u00b7ni\u00b7gin", "nahm", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aus M\u00fcckenfl\u00fcgeln geschnitten,", "tokens": ["Aus", "M\u00fc\u00b7cken\u00b7fl\u00fc\u00b7geln", "ge\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie schlug den Junker gar ungelind", "tokens": ["Sie", "schlug", "den", "Jun\u00b7ker", "gar", "un\u00b7ge\u00b7lind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wohl \u00fcber das Herze mitten,", "tokens": ["Wohl", "\u00fc\u00b7ber", "das", "Her\u00b7ze", "mit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDS", "VVFIN", "ADV", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und als er am Morgen erraffte sich,", "tokens": ["Und", "als", "er", "am", "Mor\u00b7gen", "er\u00b7raff\u00b7te", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "PRF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Da mu\u00dft' er die Folgen ermessen,", "tokens": ["Da", "mu\u00dft'", "er", "die", "Fol\u00b7gen", "er\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Weh tat ihm sein Herzlein gar bitterlich,", "tokens": ["Weh", "tat", "ihm", "sein", "Herz\u00b7lein", "gar", "bit\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ich glaube, er war besessen.", "tokens": ["Ich", "glau\u00b7be", ",", "er", "war", "be\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und wi\u00dft ihr, was ihn so sehr turbiert?", "tokens": ["Und", "wi\u00dft", "ihr", ",", "was", "ihn", "so", "sehr", "tur\u00b7biert", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Das will ich euch offenbaren,", "tokens": ["Das", "will", "ich", "euch", "of\u00b7fen\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mir ist die Geschichte schon oft passiert", "tokens": ["Mir", "ist", "die", "Ge\u00b7schich\u00b7te", "schon", "oft", "pas\u00b7siert"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADV", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "In meinen jungen Jahren:", "tokens": ["In", "mei\u00b7nen", "jun\u00b7gen", "Jah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Schlug Eine mich mit dem F\u00e4cher heut,", "tokens": ["Schlug", "Ei\u00b7ne", "mich", "mit", "dem", "F\u00e4\u00b7cher", "heut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PRF", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Da mu\u00dft' ich die Folgen sp\u00fcren,", "tokens": ["Da", "mu\u00dft'", "ich", "die", "Fol\u00b7gen", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Da tat man mich oft: nicht recht gescheut \u2013", "tokens": ["Da", "tat", "man", "mich", "oft", ":", "nicht", "recht", "ge\u00b7scheut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "ADV", "$.", "PTKNEG", "ADJD", "VVPP", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Oder gar: verliebt \u2013 titulieren.", "tokens": ["O\u00b7der", "gar", ":", "ver\u00b7liebt", "\u2013", "ti\u00b7tu\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "$.", "VVPP", "$(", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}