{"textgrid.poem.31114": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "228.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Schweine fressen Eicheln, so viel f\u00fcr ihnen liegen;", "tokens": ["Die", "Schwei\u00b7ne", "fres\u00b7sen", "Ei\u00b7cheln", ",", "so", "viel", "f\u00fcr", "ih\u00b7nen", "lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sie fressen ohne Sorgen und schaffen ihr Vergn\u00fcgen;", "tokens": ["Sie", "fres\u00b7sen", "oh\u00b7ne", "Sor\u00b7gen", "und", "schaf\u00b7fen", "ihr", "Ver\u00b7gn\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie lang es werde wehren, wo mehr sey her zunehmen,", "tokens": ["Wie", "lang", "es", "wer\u00b7de", "weh\u00b7ren", ",", "wo", "mehr", "sey", "her", "zu\u00b7neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VAFIN", "VVINF", "$,", "PWAV", "ADV", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Das ist nicht ihres Wesens. Sich herrlich nur bequemen,", "tokens": ["Das", "ist", "nicht", "ih\u00b7res", "We\u00b7sens", ".", "Sich", "herr\u00b7lich", "nur", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$.", "PRF", "ADJD", "ADV", "ADJA", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "In vollem sause leben, nur schlemmen, demmen, zehren,", "tokens": ["In", "vol\u00b7lem", "sau\u00b7se", "le\u00b7ben", ",", "nur", "schlem\u00b7men", ",", "dem\u00b7men", ",", "zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,", "ADV", "VVINF", "$,", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Ist hofem\u00e4ssig; sorgen, wo her es zu gewehren,", "tokens": ["Ist", "ho\u00b7fe\u00b7m\u00e4s\u00b7sig", ";", "sor\u00b7gen", ",", "wo", "her", "es", "zu", "ge\u00b7weh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$.", "VVFIN", "$,", "PWAV", "APZR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Damit sind ihre K\u00f6pffe mit nichte zu beschweren.", "tokens": ["Da\u00b7mit", "sind", "ih\u00b7re", "K\u00f6pf\u00b7fe", "mit", "nich\u00b7te", "zu", "be\u00b7schwe\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPOSAT", "NN", "APPR", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}