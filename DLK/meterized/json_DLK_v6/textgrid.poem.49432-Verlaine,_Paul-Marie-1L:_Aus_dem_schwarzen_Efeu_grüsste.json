{"textgrid.poem.49432": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus dem schwarzen Efeu gr\u00fcsste", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus dem schwarzen Efeu gr\u00fcsste", "tokens": ["Aus", "dem", "schwar\u00b7zen", "E\u00b7feu", "gr\u00fcs\u00b7ste"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Der Rosen leuchtendes Rot,", "tokens": ["Der", "Ro\u00b7sen", "leuch\u00b7ten\u00b7des", "Rot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Sobald du dich wendest, S\u00fcss'ste,", "tokens": ["So\u00b7bald", "du", "dich", "wen\u00b7dest", ",", "S\u00fcss'\u00b7ste", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Fasst mein Herz die alte Not.", "tokens": ["Fasst", "mein", "Herz", "die", "al\u00b7te", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mir waren die L\u00fcfte, die zarten,", "tokens": ["Mir", "wa\u00b7ren", "die", "L\u00fcf\u00b7te", ",", "die", "zar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Zu licht und die See zu gr\u00fcn.", "tokens": ["Zu", "licht", "und", "die", "See", "zu", "gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Furcht fasst mich und banges Erwarten,", "tokens": ["Furcht", "fasst", "mich", "und", "ban\u00b7ges", "Er\u00b7war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Du m\u00f6chtest mich grausam fliehn.", "tokens": ["Du", "m\u00f6ch\u00b7test", "mich", "grau\u00b7sam", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Mich lockt nicht der Bl\u00e4tter Gl\u00e4nzen", "tokens": ["Mich", "lockt", "nicht", "der", "Bl\u00e4t\u00b7ter", "Gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und des Buchsbaums schimmernde Zier,", "tokens": ["Und", "des", "Buchs\u00b7baums", "schim\u00b7mern\u00b7de", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Nicht das weite Land ohne Grenzen,", "tokens": ["Nicht", "das", "wei\u00b7te", "Land", "oh\u00b7ne", "Gren\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Und nichts mehr, nichts, ausser dir!", "tokens": ["Und", "nichts", "mehr", ",", "nichts", ",", "aus\u00b7ser", "dir", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "$,", "PIS", "$,", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}