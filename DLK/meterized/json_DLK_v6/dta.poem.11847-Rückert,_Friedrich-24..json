{"dta.poem.11847": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von Lebern aller Art m\u00f6cht' ich Jahraus Jahrein", "tokens": ["Von", "Le\u00b7bern", "al\u00b7ler", "Art", "m\u00f6cht'", "ich", "Ja\u00b7hraus", "Ja\u00b7hrein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PIAT", "NN", "VMFIN", "PPER", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am allerwenigsten ein Wirthshausleber seyn;", "tokens": ["Am", "al\u00b7ler\u00b7we\u00b7nigs\u00b7ten", "ein", "Wirths\u00b7haus\u00b7le\u00b7ber", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "PIS", "ART", "NN", "VAINF", "$."], "meter": "-+--+--+---+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Und noch viel weniger nur eins: ein Wirthshauswirth,", "tokens": ["Und", "noch", "viel", "we\u00b7ni\u00b7ger", "nur", "eins", ":", "ein", "Wirths\u00b7haus\u00b7wirth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "ADV", "PIS", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der schlechter selbst mir scheint als fremder Herden Hirt.", "tokens": ["Der", "schlech\u00b7ter", "selbst", "mir", "scheint", "als", "frem\u00b7der", "Her\u00b7den", "Hirt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "PPER", "VVFIN", "KOKOM", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er hat das ganze Jahr der G\u00e4ste Bauch zu weiden,", "tokens": ["Er", "hat", "das", "gan\u00b7ze", "Jahr", "der", "G\u00e4s\u00b7te", "Bauch", "zu", "wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die herzlos f\u00fcr ihr Geld fordern, und danklos scheiden.", "tokens": ["Die", "herz\u00b7los", "f\u00fcr", "ihr", "Geld", "for\u00b7dern", ",", "und", "dank\u00b7los", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}