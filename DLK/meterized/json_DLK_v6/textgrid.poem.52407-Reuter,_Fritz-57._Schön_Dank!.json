{"textgrid.poem.52407": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "57. Sch\u00f6n Dank!", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Dr\u00f6msacht lewt v\u00f6r Johr un Dag", "tokens": ["In", "Dr\u00f6m\u00b7sacht", "lewt", "v\u00f6r", "Johr", "un", "Dag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "En Kopmann B\u00e4uk; was ok \u00d6konomus", "tokens": ["En", "Kop\u00b7mann", "B\u00e4uk", ";", "was", "ok", "\u00d6\u00b7ko\u00b7no\u00b7mus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "NN", "$.", "PWS", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dor bi de Kirch, dat hei nah'n Rechten sach.", "tokens": ["Dor", "bi", "de", "Kirch", ",", "dat", "hei", "nah'n", "Rech\u00b7ten", "sach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "$,", "PRELS", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch nennt kein Deuwel em \u00bb\u00d6konomus\u00ab,", "tokens": ["Doch", "nennt", "kein", "Deu\u00b7wel", "em", "\u00bb", "\u00d6\u00b7ko\u00b7no\u00b7mus", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "ADJA", "$(", "FM.la", "$(", "$,"], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Kamm Bur un B\u00f6rger bi em taum Bes\u00e4uk,", "tokens": ["Kamm", "Bur", "un", "B\u00f6r\u00b7ger", "bi", "em", "taum", "Be\u00b7s\u00e4uk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Denn s\u00e4den s': \u00bbMorg'n, Herr Komes B\u00e4uk!\u00ab", "tokens": ["Denn", "s\u00e4\u00b7den", "s'", ":", "\u00bb", "Mor\u00b7g'n", ",", "Herr", "Ko\u00b7mes", "B\u00e4uk", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "NE", "$.", "$(", "NN", "$,", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Uns' Komes B\u00e4uk bedrew ok Ackeri,", "tokens": ["Un\u00b7s'", "Ko\u00b7mes", "B\u00e4uk", "be\u00b7drew", "ok", "A\u00b7cke\u00b7ri", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.8": {"text": "Un wil sin Hofrum knapp man wesen ded,", "tokens": ["Un", "wil", "sin", "Hof\u00b7rum", "knapp", "man", "we\u00b7sen", "ded", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.9": {"text": "F\u00fchrt hei sin Wagens f\u00f6r sin D\u00f6r", "tokens": ["F\u00fchrt", "hei", "sin", "Wa\u00b7gens", "f\u00f6r", "sin", "D\u00f6r"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Up apen Mark un let s' dor stahn.", "tokens": ["Up", "a\u00b7pen", "Mark", "un", "let", "s'", "dor", "stahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Dat wull'n sei eig'ntlich nich recht liden,", "tokens": ["Dat", "wull'n", "sei", "eig'\u00b7nt\u00b7lich", "nich", "recht", "li\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "ADV", "PTKNEG", "ADJD", "ADJA", "$,"], "meter": "+-+-+--+--", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "Doch in de ollen gauden Tiden", "tokens": ["Doch", "in", "de", "ol\u00b7len", "gau\u00b7den", "Ti\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Hett n\u00fcmms em dor\u00fcm grot wat dahn,", "tokens": ["Hett", "n\u00fcmms", "em", "do\u00b7r\u00fcm", "grot", "wat", "dahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Denn wenn de Herr'n von'n Magistrat", "tokens": ["Denn", "wenn", "de", "Herr'n", "von'n", "Ma\u00b7gist\u00b7rat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "NE", "NE", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Des Abends von den Keller kemen,", "tokens": ["Des", "A\u00b7bends", "von", "den", "Kel\u00b7ler", "ke\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Denn gung'n sei 'ne ganz anner Strat,", "tokens": ["Denn", "gung'n", "sei", "'ne", "ganz", "an\u00b7ner", "Strat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Wo sei meindag nich Schaden nemen,", "tokens": ["Wo", "sei", "mein\u00b7dag", "nich", "Scha\u00b7den", "ne\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "PTKNEG", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "De Knipers kregen ehren Snaps", "tokens": ["De", "Kni\u00b7pers", "kre\u00b7gen", "eh\u00b7ren", "Snaps"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Un nemen't denn nich so genau", "tokens": ["Un", "ne\u00b7men't", "denn", "nich", "so", "ge\u00b7nau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "PTKNEG", "ADV", "ADJD"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.20": {"text": "Un deden h\u00fcbsch de Ogen tau,", "tokens": ["Un", "de\u00b7den", "h\u00fcbsch", "de", "O\u00b7gen", "tau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.21": {"text": "Un lep en annern dummen Taps", "tokens": ["Un", "lep", "en", "an\u00b7nern", "dum\u00b7men", "Taps"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Des Abends up den Distel rup,", "tokens": ["Des", "A\u00b7bends", "up", "den", "Dis\u00b7tel", "rup", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Denn s\u00e4d uns Komes B\u00e4uk: \u00bbDauh Hei de Ogen up!\u00ab", "tokens": ["Denn", "s\u00e4d", "uns", "Ko\u00b7mes", "B\u00e4uk", ":", "\u00bb", "Dauh", "Hei", "de", "O\u00b7gen", "up", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "$.", "$(", "FM", "FM", "FM", "FM", "FM", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nu is't in Dr\u00f6msacht anners word'n:", "tokens": ["Nu", "is't", "in", "Dr\u00f6m\u00b7sacht", "an\u00b7ners", "word'n", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Fuhrwark un mit Wagens is dat dull,", "tokens": ["Mit", "Fuhr\u00b7wark", "un", "mit", "Wa\u00b7gens", "is", "dat", "dull", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "FM", "APPR", "NN", "FM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "De stahn nu unner streng Kuntrull,", "tokens": ["De", "stahn", "nu", "un\u00b7ner", "streng", "Kun\u00b7trull", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADJD", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nu ward'n an jeden S\u00fcnndagsmorg'n", "tokens": ["Nu", "ward'n", "an", "je\u00b7den", "S\u00fcnn\u00b7dags\u00b7mor\u00b7g'n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "De Straten spunnt mit Stricken un mit Keden,", "tokens": ["De", "Stra\u00b7ten", "spunnt", "mit", "Stri\