{"textgrid.poem.47426": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[gieb ihr, Ost, die Kunde meiner Leiden]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gieb ihr, Ost, die Kunde meiner Leiden,", "tokens": ["Gieb", "ihr", ",", "Ost", ",", "die", "Kun\u00b7de", "mei\u00b7ner", "Lei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aber gieb sie schwach! Es wird gen\u00fcgen.", "tokens": ["A\u00b7ber", "gieb", "sie", "schwach", "!", "Es", "wird", "ge\u00b7n\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADJD", "$.", "PPER", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Maltest du in vollen, \u00e4chten Z\u00fcgen \u2013", "tokens": ["Mal\u00b7test", "du", "in", "vol\u00b7len", ",", "\u00e4ch\u00b7ten", "Z\u00fc\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ach, du w\u00fcrdest ihr den Tod bereiten.", "tokens": ["Ach", ",", "du", "w\u00fcr\u00b7dest", "ihr", "den", "Tod", "be\u00b7rei\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}