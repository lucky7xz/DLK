{"textgrid.poem.46336": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Vitell", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vitell klagt den Verlust von seinem lieben Weibe,", "tokens": ["Vi\u00b7tell", "klagt", "den", "Ver\u00b7lust", "von", "sei\u00b7nem", "lie\u00b7ben", "Wei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Und schlie\u00dft sich abgeh\u00e4rmt, in seinem Keller ein,", "tokens": ["Und", "schlie\u00dft", "sich", "ab\u00b7ge\u00b7h\u00e4rmt", ",", "in", "sei\u00b7nem", "Kel\u00b7ler", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "VVPP", "$,", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und s\u00e4ufft sein letztes F\u00e4\u00dfchen Wein,", "tokens": ["Und", "s\u00e4ufft", "sein", "letz\u00b7tes", "F\u00e4\u00df\u00b7chen", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Damit kein Trost ihm \u00fcbrig bleibe.", "tokens": ["Da\u00b7mit", "kein", "Trost", "ihm", "\u00fcb\u00b7rig", "blei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}