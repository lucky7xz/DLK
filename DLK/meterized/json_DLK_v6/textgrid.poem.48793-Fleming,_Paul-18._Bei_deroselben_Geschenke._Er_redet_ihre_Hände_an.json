{"textgrid.poem.48793": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "18. Bei deroselben Geschenke. Er redet ihre H\u00e4nde an", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr schwesterliches Paar der kl\u00fcgsten K\u00fcnstlerinnen,", "tokens": ["Ihr", "schwes\u00b7ter\u00b7li\u00b7ches", "Paar", "der", "kl\u00fcgs\u00b7ten", "K\u00fcnst\u00b7le\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "so feurig sind an Witz' und vom Verstande hei\u00df,", "tokens": ["so", "feu\u00b7rig", "sind", "an", "Witz'", "und", "vom", "Ver\u00b7stan\u00b7de", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "APPR", "NN", "KON", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "dergleichen Gaben man an nicht viel Orten wei\u00df,", "tokens": ["derg\u00b7lei\u00b7chen", "Ga\u00b7ben", "man", "an", "nicht", "viel", "Or\u00b7ten", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "PIS", "APPR", "PTKNEG", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "habt Dank, habt, Edle, Dank f\u00fcr euer gut Gesinnen.", "tokens": ["habt", "Dank", ",", "habt", ",", "Ed\u00b7le", ",", "Dank", "f\u00fcr", "eu\u00b7er", "gut", "Ge\u00b7sin\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "VAFIN", "$,", "ADJA", "$,", "NN", "APPR", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "F\u00fcr dieses, das mir selbst die G\u00f6tter selbst mi\u00dfg\u00fcnnen,", "tokens": ["F\u00fcr", "die\u00b7ses", ",", "das", "mir", "selbst", "die", "G\u00f6t\u00b7ter", "selbst", "mi\u00df\u00b7g\u00fcn\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "$,", "PRELS", "PPER", "ADV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "habt hohen, gro\u00dfen Dank. Der euer weise Flai\u00df", "tokens": ["habt", "ho\u00b7hen", ",", "gro\u00b7\u00dfen", "Dank", ".", "Der", "eu\u00b7er", "wei\u00b7se", "Flai\u00df"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJA", "$,", "ADJA", "NN", "$.", "ART", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "verdienet euch bei mir f\u00fcr allen Meistern Preis,", "tokens": ["ver\u00b7die\u00b7net", "euch", "bei", "mir", "f\u00fcr", "al\u00b7len", "Meis\u00b7tern", "Preis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die ie gewesen sind, sein und sein werden k\u00fcnnen.", "tokens": ["die", "ie", "ge\u00b7we\u00b7sen", "sind", ",", "sein", "und", "sein", "wer\u00b7den", "k\u00fcn\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAPP", "VAFIN", "$,", "VAINF", "KON", "VAINF", "VAFIN", "VVINF", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Empfind' ich solche Lust von eurer sch\u00f6nen Kunst", "tokens": ["Emp\u00b7find'", "ich", "sol\u00b7che", "Lust", "von", "eu\u00b7rer", "sch\u00f6\u00b7nen", "Kunst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und macht mir euer Werk, das edle, solche Gunst,", "tokens": ["und", "macht", "mir", "eu\u00b7er", "Werk", ",", "das", "ed\u00b7le", ",", "sol\u00b7che", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wenn ich so weit von euch bin leiblich abgerissen", "tokens": ["wenn", "ich", "so", "weit", "von", "euch", "bin", "leib\u00b7lich", "ab\u00b7ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPR", "PPER", "VAFIN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "was, meinet ihr, mu\u00df da f\u00fcr Freude mir geschehn,", "tokens": ["was", ",", "mei\u00b7net", "ihr", ",", "mu\u00df", "da", "f\u00fcr", "Freu\u00b7de", "mir", "ge\u00b7schehn", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "VVFIN", "PPER", "$,", "VMFIN", "ADV", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wenn ihr euch, Zarte, mir nach Willen la\u00dft besehn,", "tokens": ["wenn", "ihr", "euch", ",", "Zar\u00b7te", ",", "mir", "nach", "Wil\u00b7len", "la\u00dft", "be\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "NN", "$,", "PPER", "APPR", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wenn ich euch lieben mag, mehr, wenn ich euch darf k\u00fcssen?", "tokens": ["wenn", "ich", "euch", "lie\u00b7ben", "mag", ",", "mehr", ",", "wenn", "ich", "euch", "darf", "k\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$,", "ADV", "$,", "KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}