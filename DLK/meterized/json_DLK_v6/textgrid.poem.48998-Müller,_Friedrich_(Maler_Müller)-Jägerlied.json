{"textgrid.poem.48998": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "J\u00e4gerlied", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf, r\u00fcstige Knaben,", "tokens": ["Auf", ",", "r\u00fcs\u00b7ti\u00b7ge", "Kna\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["APPR", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Eh' Lucifer sinkt!", "tokens": ["Eh'", "Lu\u00b7ci\u00b7fer", "sinkt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Auroren nun haben", "tokens": ["Au\u00b7ro\u00b7ren", "nun", "ha\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADV", "VAFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Stunden gewinkt!", "tokens": ["Die", "Stun\u00b7den", "ge\u00b7winkt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Schon blasen bei Netzen", "tokens": ["Schon", "bla\u00b7sen", "bei", "Net\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die J\u00e4ger im Wald,", "tokens": ["Die", "J\u00e4\u00b7ger", "im", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Zum Treiben und Hetzen", "tokens": ["Zum", "Trei\u00b7ben", "und", "Het\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Das Echo erschallt!", "tokens": ["Das", "E\u00b7cho", "er\u00b7schallt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Nach sausen die Lanzen", "tokens": ["Nach", "sau\u00b7sen", "die", "Lan\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Dem Wilde durch's Tal!", "tokens": ["Dem", "Wil\u00b7de", "durch's", "Tal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Am Abend, da tanzen", "tokens": ["Am", "A\u00b7bend", ",", "da", "tan\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "ADV", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wir lustig ums Mahl.", "tokens": ["Wir", "lus\u00b7tig", "ums", "Mahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Selbst Amor, der kleine,", "tokens": ["Selbst", "A\u00b7mor", ",", "der", "klei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NE", "$,", "ART", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Jauchzt mit ins Geschrei", "tokens": ["Jauchzt", "mit", "ins", "Ge\u00b7schrei"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "APPRART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und treibet uns feine", "tokens": ["Und", "trei\u00b7bet", "uns", "fei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Br\u00fcnetten herbei.", "tokens": ["Br\u00fc\u00b7net\u00b7ten", "her\u00b7bei", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Tallara! Taltara!", "tokens": ["Tal\u00b7la\u00b7ra", "!", "Tal\u00b7ta\u00b7ra", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Das Jagdhorn erschallt!", "tokens": ["Das", "Jagd\u00b7horn", "er\u00b7schallt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Taltara! Tallara!", "tokens": ["Tal\u00b7ta\u00b7ra", "!", "Tal\u00b7la\u00b7ra", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Der Dogen Laut hallt!", "tokens": ["Der", "Do\u00b7gen", "Laut", "hallt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Auf Rossen wir eilen", "tokens": ["Auf", "Ros\u00b7sen", "wir", "ei\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Gleich St\u00fcrmen dahin,", "tokens": ["Gleich", "St\u00fcr\u00b7men", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "PAV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Bepflanzen mit Pfeilen", "tokens": ["Be\u00b7pflan\u00b7zen", "mit", "Pfei\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Den Eber im Fliehn!", "tokens": ["Den", "E\u00b7ber", "im", "Fliehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Tallara! Taltara!", "tokens": ["Tal\u00b7la\u00b7ra", "!", "Tal\u00b7ta\u00b7ra", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Vom sch\u00e4umenden Quell,", "tokens": ["Vom", "sch\u00e4u\u00b7men\u00b7den", "Quell", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Taltara! Tallara!", "tokens": ["Tal\u00b7ta\u00b7ra", "!", "Tal\u00b7la\u00b7ra", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "St\u00fcrzt mutig Gebell!", "tokens": ["St\u00fcrzt", "mu\u00b7tig", "Ge\u00b7bell", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "NN", "$."], "meter": "+---+", "measure": "dactylic.init"}, "line.5": {"text": "Gebt, J\u00e4ger, die Spornen!", "tokens": ["Gebt", ",", "J\u00e4\u00b7ger", ",", "die", "Spor\u00b7nen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Auf, Hunde, hieher!", "tokens": ["Auf", ",", "Hun\u00b7de", ",", "hie\u00b7her", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "$,", "NN", "$,", "PAV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Schon w\u00e4lzt sich durch Dornen", "tokens": ["Schon", "w\u00e4lzt", "sich", "durch", "Dor\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Der zornige B\u00e4r!", "tokens": ["Der", "zor\u00b7ni\u00b7ge", "B\u00e4r", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Diana h\u00e4lt innen", "tokens": ["Di\u00b7a\u00b7na", "h\u00e4lt", "in\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die Drachen und blickt", "tokens": ["Die", "Dra\u00b7chen", "und", "blickt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Von wolkigen Zinnen,", "tokens": ["Von", "wol\u00b7ki\u00b7gen", "Zin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "In Jagdlust entz\u00fcckt;", "tokens": ["In", "Jagd\u00b7lust", "ent\u00b7z\u00fcckt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und l\u00e4\u00dft nun am Himmel", "tokens": ["Und", "l\u00e4\u00dft", "nun", "am", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Den Mondlauf verk\u00fcrzt,", "tokens": ["Den", "Mond\u00b7lauf", "ver\u00b7k\u00fcrzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und spornet den Schimmel,", "tokens": ["Und", "spor\u00b7net", "den", "Schim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Als J\u00fcngling gesch\u00fcrzt.", "tokens": ["Als", "J\u00fcng\u00b7ling", "ge\u00b7sch\u00fcrzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Wie lechzen die mutigen", "tokens": ["Wie", "lech\u00b7zen", "die", "mu\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Doggen! Wie eilt's", "tokens": ["Dog\u00b7gen", "!", "Wie", "eilt's"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "PWAV", "NE"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Dort \u00fcber die blutigen", "tokens": ["Dort", "\u00fc\u00b7ber", "die", "blu\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Klippen! Wie heult's!", "tokens": ["Klip\u00b7pen", "!", "Wie", "heult's", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "NE", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ha! Cynthiens m\u00e4chtiger", "tokens": ["Ha", "!", "Cyn\u00b7thiens", "m\u00e4ch\u00b7ti\u00b7ger"], "token_info": ["word", "punct", "word", "word"], "pos": ["ITJ", "$.", "NE", "ADJA"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ruf in den Klang:", "tokens": ["Ruf", "in", "den", "Klang", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Dem B\u00e4ren dem pr\u00e4chtiger", "tokens": ["Dem", "B\u00e4\u00b7ren", "dem", "pr\u00e4ch\u00b7ti\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Sterbegesang!", "tokens": ["Ster\u00b7be\u00b7ge\u00b7sang", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Tallara! Taltara!", "tokens": ["Tal\u00b7la\u00b7ra", "!", "Tal\u00b7ta\u00b7ra", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Juch, lieblich Get\u00f6n", "tokens": ["Juch", ",", "lieb\u00b7lich", "Ge\u00b7t\u00f6n"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJD", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Taltara! Tallara!", "tokens": ["Tal\u00b7ta\u00b7ra", "!", "Tal\u00b7la\u00b7ra", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Von bl\u00fchenden H\u00f6hn!", "tokens": ["Von", "bl\u00fc\u00b7hen\u00b7den", "H\u00f6hn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ei, seht doch, wie bieder", "tokens": ["Ei", ",", "seht", "doch", ",", "wie", "bie\u00b7der"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ADV", "$,", "PWAV", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Jagt Amor, der Mann!", "tokens": ["Jagt", "A\u00b7mor", ",", "der", "Mann", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Ihm treiben die Br\u00fcder", "tokens": ["Ihm", "trei\u00b7ben", "die", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Die M\u00e4dchen voran!", "tokens": ["Die", "M\u00e4d\u00b7chen", "vo\u00b7ran", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Schnell gibt er ein K\u00fc\u00dfchen", "tokens": ["Schnell", "gibt", "er", "ein", "K\u00fc\u00df\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der j\u00fcngsten, hihi!", "tokens": ["Der", "j\u00fcng\u00b7sten", ",", "hi\u00b7hi", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ITJ", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Entbl\u00f6\u00dfet ihr F\u00fc\u00dfchen", "tokens": ["Ent\u00b7bl\u00f6\u00b7\u00dfet", "ihr", "F\u00fc\u00df\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und w\u00e4chsernes Knie.", "tokens": ["Und", "w\u00e4ch\u00b7ser\u00b7nes", "Knie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Sie h\u00f6ren ihn lachen", "tokens": ["Sie", "h\u00f6\u00b7ren", "ihn", "la\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und schreien: ei, ei!", "tokens": ["Und", "schrei\u00b7en", ":", "ei", ",", "ei", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NE", "$,", "ITJ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und lachen und jagen", "tokens": ["Und", "la\u00b7chen", "und", "ja\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVINF", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Geschwinder vorbei!", "tokens": ["Ge\u00b7schwin\u00b7der", "vor\u00b7bei", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Auf, munter, ihr Sch\u00fctzen,", "tokens": ["Auf", ",", "mun\u00b7ter", ",", "ihr", "Sch\u00fct\u00b7zen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$,", "ADJD", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Zum sprudelnden Quell!", "tokens": ["Zum", "spru\u00b7deln\u00b7den", "Quell", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wir schm\u00fccken die M\u00fctzen", "tokens": ["Wir", "schm\u00fc\u00b7cken", "die", "M\u00fct\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mit Eichenlaub hell!", "tokens": ["Mit", "Ei\u00b7chen\u00b7laub", "hell", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Vorbei ist das Jagen!", "tokens": ["Vor\u00b7bei", "ist", "das", "Ja\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Dort reiten sie her,", "tokens": ["Dort", "rei\u00b7ten", "sie", "her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,"],