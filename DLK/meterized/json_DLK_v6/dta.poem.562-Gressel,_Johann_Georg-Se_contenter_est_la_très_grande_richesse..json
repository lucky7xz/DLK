{"dta.poem.562": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Se contenter est la tr\u00e8s grande richesse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin vergn\u00fcgt; ob gleich die holden Blicke", "tokens": ["Ich", "bin", "ver\u00b7gn\u00fcgt", ";", "ob", "gleich", "die", "hol\u00b7den", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Schicksahls nur ein falsches Blend-Werck seyn/", "tokens": ["Des", "Schick\u00b7sahls", "nur", "ein", "fal\u00b7sches", "Blen\u00b7d\u00b7Werck", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich kenne schon desselben schlaue T\u00fccke/", "tokens": ["Ich", "ken\u00b7ne", "schon", "des\u00b7sel\u00b7ben", "schlau\u00b7e", "T\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Drum acht\u2019 ich nichtes nicht den blossen Schein.", "tokens": ["Drum", "acht'", "ich", "nich\u00b7tes", "nicht", "den", "blos\u00b7sen", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PIS", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Den blossen Schein/ und sonsten nichts zu lieben/", "tokens": ["Den", "blos\u00b7sen", "Schein", "/", "und", "sons\u00b7ten", "nichts", "zu", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KON", "VVFIN", "PIS", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Heist Aepffel ihm von ", "tokens": ["Heist", "A\u00b7e\u00b7pf\u00b7fel", "ihm", "von"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "PPER", "APPR"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ein Mast der Wind und grimmer Wellen Treiben", "tokens": ["Ein", "Mast", "der", "Wind", "und", "grim\u00b7mer", "Wel\u00b7len", "Trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "KON", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Folgt/ mu\u00df gewi\u00df ohn Steur bald unter gehn.", "tokens": ["Folgt", "/", "mu\u00df", "ge\u00b7wi\u00df", "ohn", "Steur", "bald", "un\u00b7ter", "gehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VMFIN", "ADV", "APPR", "NN", "ADV", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Bald untergehn/ und in der Noht versincken", "tokens": ["Bald", "un\u00b7ter\u00b7gehn", "/", "und", "in", "der", "Noht", "ver\u00b7sin\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "$(", "KON", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kommt weibischen und bangen Hertzen zu.", "tokens": ["Kommt", "wei\u00b7bi\u00b7schen", "und", "ban\u00b7gen", "Hert\u00b7zen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer auch nicht kan die herben Myrrhen trincken", "tokens": ["Wer", "auch", "nicht", "kan", "die", "her\u00b7ben", "Myr\u00b7rhen", "trin\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "PTKNEG", "VMFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erlangt niemahls die stets gew\u00fcnschte Ruh.", "tokens": ["Er\u00b7langt", "nie\u00b7mahls", "die", "stets", "ge\u00b7w\u00fcnschte", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADV", "ADJA", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.4": {"line.1": {"text": "Gew\u00fcnschte Ruh/ und s\u00fcsse Lust geniessen/", "tokens": ["Ge\u00b7w\u00fcnschte", "Ruh", "/", "und", "s\u00fcs\u00b7se", "Lust", "ge\u00b7nies\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "KON", "ADJA", "NN", "VVPP", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ist nur f\u00fcr den/ der niemahls nnterliegt/", "tokens": ["Ist", "nur", "f\u00fcr", "den", "/", "der", "nie\u00b7mahls", "nn\u00b7ter\u00b7liegt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "$(", "ART", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Darum la\u00df ich mir keinen Sturm verdriessen/", "tokens": ["Da\u00b7rum", "la\u00df", "ich", "mir", "kei\u00b7nen", "Sturm", "ver\u00b7dries\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Denck-Spruch ist: Ich bin dennoch vergn\u00fcgt.", "tokens": ["Mein", "Den\u00b7ck\u00b7Spruch", "ist", ":", "Ich", "bin", "den\u00b7noch", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$.", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}}}}