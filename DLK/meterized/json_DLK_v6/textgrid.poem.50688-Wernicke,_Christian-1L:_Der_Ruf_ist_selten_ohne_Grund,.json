{"textgrid.poem.50688": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Ruf ist selten ohne Grund,", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Ruf ist selten ohne Grund,", "tokens": ["Der", "Ruf", "ist", "sel\u00b7ten", "oh\u00b7ne", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vergr\u00f6\u00dfert er gleich alle Sachen;", "tokens": ["Ver\u00b7gr\u00f6\u00b7\u00dfert", "er", "gleich", "al\u00b7le", "Sa\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die Wahrheit \u00f6ffnet ihm den Mund", "tokens": ["Die", "Wahr\u00b7heit", "\u00f6ff\u00b7net", "ihm", "den", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lehret ihn die L\u00fcgen machen;", "tokens": ["Und", "leh\u00b7ret", "ihn", "die", "L\u00fc\u00b7gen", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er setzt, um mehr uns zu betr\u00fcgen,", "tokens": ["Er", "setzt", ",", "um", "mehr", "uns", "zu", "be\u00b7tr\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUI", "ADV", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zur Finsterni\u00df ein wenig Klarheit,", "tokens": ["Zur", "Fins\u00b7ter\u00b7ni\u00df", "ein", "we\u00b7nig", "Klar\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Spricht keine Wahrheit ohne L\u00fcgen", "tokens": ["Spricht", "kei\u00b7ne", "Wahr\u00b7heit", "oh\u00b7ne", "L\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und keine L\u00fcgen ohne Wahrheit.", "tokens": ["Und", "kei\u00b7ne", "L\u00fc\u00b7gen", "oh\u00b7ne", "Wahr\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}