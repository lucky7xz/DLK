{"dta.poem.5998": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne herrscht, und ist, durch ihre rege Flammen,", "tokens": ["Die", "Son\u00b7ne", "herrscht", ",", "und", "ist", ",", "durch", "ih\u00b7re", "re\u00b7ge", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "KON", "VAFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der C\u00f6rperlichen Welt ihr Leben, Seel und Geist.", "tokens": ["Der", "C\u00f6r\u00b7per\u00b7li\u00b7chen", "Welt", "ihr", "Le\u00b7ben", ",", "Seel", "und", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es scheinet, als ob sie in ihrem Lauff zusammen,", "tokens": ["Es", "schei\u00b7net", ",", "als", "ob", "sie", "in", "ih\u00b7rem", "Lauff", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wol den Jahres-Creys, als Tages-Creys durchreis\u2019t:", "tokens": ["So", "wol", "den", "Jah\u00b7res\u00b7Creys", ",", "als", "Ta\u00b7ges\u00b7Creys", "durch\u00b7reis't", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nebst der Planeten Heer zeigt das Gestirn der Nacht", "tokens": ["Nebst", "der", "Pla\u00b7ne\u00b7ten", "Heer", "zeigt", "das", "Ge\u00b7stirn", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.6": {"text": "Sich stets ver\u00e4nderlich: indem am Firmament", "tokens": ["Sich", "stets", "ver\u00b7\u00e4n\u00b7der\u00b7lich", ":", "in\u00b7dem", "am", "Fir\u00b7ma\u00b7ment"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "ADV", "ADJD", "$.", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der festen Sterne Feur, in wunderbarer Pracht,", "tokens": ["Der", "fes\u00b7ten", "Ster\u00b7ne", "Feur", ",", "in", "wun\u00b7der\u00b7ba\u00b7rer", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als welche sonder Zahl, recht Majest\u00e4tisch brennt.", "tokens": ["Als", "wel\u00b7che", "son\u00b7der", "Zahl", ",", "recht", "Ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "brennt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAT", "ADJA", "NN", "$,", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}