{"textgrid.poem.38749": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "6. Das Licht", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon gr\u00fc\u00dft der Vater seinen Sohn,", "tokens": ["Schon", "gr\u00fc\u00dft", "der", "Va\u00b7ter", "sei\u00b7nen", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schon steht er an der alten Schwelle,", "tokens": ["Schon", "steht", "er", "an", "der", "al\u00b7ten", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihm winkt und lockt die liebe Helle", "tokens": ["Ihm", "winkt", "und", "lockt", "die", "lie\u00b7be", "Hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Licht dadrein, ein sanfter Ton.", "tokens": ["Das", "Licht", "da\u00b7drein", ",", "ein", "sanf\u00b7ter", "Ton", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hier klopft das Herz, die letzte Wand", "tokens": ["Hier", "klopft", "das", "Herz", ",", "die", "letz\u00b7te", "Wand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "H\u00e4lt Kind und Vater noch zur\u00fccke,", "tokens": ["H\u00e4lt", "Kind", "und", "Va\u00b7ter", "noch", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie ahnden schon die Liebesblicke,", "tokens": ["Sie", "ahn\u00b7den", "schon", "die", "Lie\u00b7bes\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Was sie getrennet sonst, verschwand.", "tokens": ["Was", "sie", "ge\u00b7tren\u00b7net", "sonst", ",", "ver\u00b7schwand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "ADV", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "So \u00f6ffne denn die letzte Th\u00fcr;", "tokens": ["So", "\u00f6ff\u00b7ne", "denn", "die", "letz\u00b7te", "Th\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Willst du noch immer weiter ziehen?", "tokens": ["Willst", "du", "noch", "im\u00b7mer", "wei\u00b7ter", "zie\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Entflieh hinein, sonst m\u00fc\u00dft du fliehen,", "tokens": ["Ent\u00b7flieh", "hin\u00b7ein", ",", "sonst", "m\u00fc\u00dft", "du", "flie\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Dir nach tritt, dem du kaum entgangen,", "tokens": ["Dir", "nach", "tritt", ",", "dem", "du", "kaum", "ent\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "VVFIN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Mit frischen Wangen", "tokens": ["Mit", "fri\u00b7schen", "Wan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Das falsche Verlangen:", "tokens": ["Das", "fal\u00b7sche", "Ver\u00b7lan\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Drum bleibe hier.", "tokens": ["Drum", "blei\u00b7be", "hier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "So schwinde was einst mein,", "tokens": ["So", "schwin\u00b7de", "was", "einst", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PWS", "ADV", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Ich werde nun mein eigen seyn", "tokens": ["Ich", "wer\u00b7de", "nun", "mein", "ei\u00b7gen", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "ADJD", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Im dreimalheilig-lichten Schein.", "tokens": ["Im", "drei\u00b7mal\u00b7hei\u00b7lig\u00b7lich\u00b7ten", "Schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}