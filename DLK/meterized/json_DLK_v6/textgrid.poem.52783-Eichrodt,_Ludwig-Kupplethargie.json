{"textgrid.poem.52783": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Kupplethargie", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebs Publikum, ich bin das Coupl\u00e9t,", "tokens": ["Liebs", "Pub\u00b7li\u00b7kum", ",", "ich", "bin", "das", "Cou\u00b7pl\u00e9t", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir ist vor mir selber ganz \u00fcbel, o weh!", "tokens": ["Mir", "ist", "vor", "mir", "sel\u00b7ber", "ganz", "\u00fc\u00b7bel", ",", "o", "weh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ADV", "ADV", "ADJD", "$,", "FM", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Weil ich witzig soll sein und bin gr\u00fcndlich fad,", "tokens": ["Weil", "ich", "wit\u00b7zig", "soll", "sein", "und", "bin", "gr\u00fcnd\u00b7lich", "fad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VMFIN", "VAINF", "KON", "VAFIN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Statt in Wahrheit ein \u2013 italien'scher Salat.", "tokens": ["Statt", "in", "Wahr\u00b7heit", "ein", "\u2013", "i\u00b7ta\u00b7li\u00b7en'\u00b7scher", "Sa\u00b7lat", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "$(", "ADJA", "NN", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.5": {"text": "Ach ich bin ein Bastard nur von Geist und Geschmier,", "tokens": ["Ach", "ich", "bin", "ein", "Bas\u00b7tard", "nur", "von", "Geist", "und", "Ge\u00b7schmier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VAFIN", "ART", "NE", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.6": {"text": "Aber \u2013 ich kann nichts daf\u00fcr.", "tokens": ["A\u00b7ber", "\u2013", "ich", "kann", "nichts", "da\u00b7f\u00fcr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PPER", "VMFIN", "PIS", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Von Ehedissidien, von st\u00e4dtischem Klatsch,", "tokens": ["Von", "E\u00b7he\u00b7dis\u00b7si\u00b7di\u00b7en", ",", "von", "st\u00e4d\u00b7ti\u00b7schem", "Klatsch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von ein wenig verschimmeltem Kladderadatsch,", "tokens": ["Von", "ein", "we\u00b7nig", "ver\u00b7schim\u00b7mel\u00b7tem", "Klad\u00b7de\u00b7ra\u00b7datsch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "ADJA", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Crinolinen und Chignons, von Schneidern, potz Blitz", "tokens": ["Cri\u00b7no\u00b7li\u00b7nen", "und", "Chig\u00b7nons", ",", "von", "Schnei\u00b7dern", ",", "potz", "Blitz"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NE", "$,", "APPR", "NN", "$,", "APPR", "NN"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Der \u00bbSchneider\u00ab das ist ja schon selber ein Witz,", "tokens": ["Der", "\u00bb", "Schnei\u00b7der", "\u00ab", "das", "ist", "ja", "schon", "sel\u00b7ber", "ein", "Witz", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "NE", "$(", "PDS", "VAFIN", "ADV", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Von dem soll ich n\u00e4hren mich armes Gethier,", "tokens": ["Von", "dem", "soll", "ich", "n\u00e4h\u00b7ren", "mich", "ar\u00b7mes", "Ge\u00b7thier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VMFIN", "PPER", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Aber \u2013 ich kann nichts daf\u00fcr!", "tokens": ["A\u00b7ber", "\u2013", "ich", "kann", "nichts", "da\u00b7f\u00fcr", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PPER", "VMFIN", "PIS", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}