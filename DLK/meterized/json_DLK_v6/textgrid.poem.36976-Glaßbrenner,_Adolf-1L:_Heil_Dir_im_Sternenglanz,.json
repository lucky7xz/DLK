{"textgrid.poem.36976": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Heil Dir im Sternenglanz,", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heil Dir im Sternenglanz,", "tokens": ["Heil", "Dir", "im", "Ster\u00b7nen\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Heil Dir im Bl\u00fcthenkranz,", "tokens": ["Heil", "Dir", "im", "Bl\u00fc\u00b7then\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Heil, Freiheit, Dir!", "tokens": ["Heil", ",", "Frei\u00b7heit", ",", "Dir", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Du, aller V\u00f6lker Gl\u00fcck,", "tokens": ["Du", ",", "al\u00b7ler", "V\u00f6l\u00b7ker", "Gl\u00fcck", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wende den milden Blick", "tokens": ["Wen\u00b7de", "den", "mil\u00b7den", "Blick"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Auf unser Vaterland!", "tokens": ["Auf", "un\u00b7ser", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Heil, Freiheit, Dir!", "tokens": ["Heil", ",", "Frei\u00b7heit", ",", "Dir", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wie t\u00f6nt Dein Name sch\u00f6n!", "tokens": ["Wie", "t\u00f6nt", "Dein", "Na\u00b7me", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Als ob zu Himmelsh\u00f6hn", "tokens": ["Als", "ob", "zu", "Him\u00b7mels\u00b7h\u00f6hn"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schwebt unser Geist.", "tokens": ["Schwebt", "un\u00b7ser", "Geist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Freiheit! Du s\u00fc\u00dfes Wort,", "tokens": ["Frei\u00b7heit", "!", "Du", "s\u00fc\u00b7\u00dfes", "Wort", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "T\u00f6ne von Ort zu Ort,", "tokens": ["T\u00f6\u00b7ne", "von", "Ort", "zu", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Bis Deine Melodie", "tokens": ["Bis", "Dei\u00b7ne", "Me\u00b7lo\u00b7die"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den Winter schmelzt.", "tokens": ["Den", "Win\u00b7ter", "schmelzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Leben ist Leben nicht,", "tokens": ["Le\u00b7ben", "ist", "Le\u00b7ben", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "PTKNEG", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Wo nicht Dein rosig Licht", "tokens": ["Wo", "nicht", "Dein", "ro\u00b7sig", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PTKNEG", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Alles umflie\u00dft;", "tokens": ["Al\u00b7les", "um\u00b7flie\u00dft", ";"], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Menschen sind Menschen nur,", "tokens": ["Men\u00b7schen", "sind", "Men\u00b7schen", "nur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Ringt aus der Creatur", "tokens": ["Ringt", "aus", "der", "Crea\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Liebend die Seele sich,", "tokens": ["Lie\u00b7bend", "die", "See\u00b7le", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PRF", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Freiheit, zu Dir!", "tokens": ["Frei\u00b7heit", ",", "zu", "Dir", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPER", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Geist, Muth und Kraft ergl\u00fcht,", "tokens": ["Geist", ",", "Muth", "und", "Kraft", "er\u00b7gl\u00fcht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sch\u00f6nheit und Tugend bl\u00fcht", "tokens": ["Sch\u00f6n\u00b7heit", "und", "Tu\u00b7gend", "bl\u00fcht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "An Deinem Thron!", "tokens": ["An", "Dei\u00b7nem", "Thron", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Stolzer schl\u00e4gt jedes Herz,", "tokens": ["Stol\u00b7zer", "schl\u00e4gt", "je\u00b7des", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Kleiner wird jeder Schmerz,", "tokens": ["Klei\u00b7ner", "wird", "je\u00b7der", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Es f\u00fchlt der Aermste sich", "tokens": ["Es", "f\u00fchlt", "der", "A\u00b7erms\u00b7te", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "PRF"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Gl\u00fccklich in Dir!", "tokens": ["Gl\u00fcck\u00b7lich", "in", "Dir", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Heil Dir im Bl\u00fcthenkranz,", "tokens": ["Heil", "Dir", "im", "Bl\u00fc\u00b7then\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Heil Dir im Sternenglanz,", "tokens": ["Heil", "Dir", "im", "Ster\u00b7nen\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Heil, Freiheit, Dir!", "tokens": ["Heil", ",", "Frei\u00b7heit", ",", "Dir", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "O Himmels-K\u00f6nigin,", "tokens": ["O", "Him\u00b7mels\u00b7K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "La\u00df' Deinen Heiland ziehn", "tokens": ["La\u00df'", "Dei\u00b7nen", "Hei\u00b7land", "ziehn"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Segnend durch's deutsche Land!", "tokens": ["Seg\u00b7nend", "durch's", "deut\u00b7sche", "Land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Heil, Freiheit, Dir!", "tokens": ["Heil", ",", "Frei\u00b7heit", ",", "Dir", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}