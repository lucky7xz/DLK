{"textgrid.poem.48729": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "16. Zuschrift an Braut und Br\u00e4utigam", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft di\u00df ein Zeichen sein, ihr wolgetrauten Beide,", "tokens": ["La\u00dft", "di\u00df", "ein", "Zei\u00b7chen", "sein", ",", "ihr", "wol\u00b7ge\u00b7trau\u00b7ten", "Bei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDS", "ART", "NN", "VAINF", "$,", "PPER", "VMFIN", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da\u00df euch mein Ph\u00f6bus liebt, der niemals Feind sein kan", "tokens": ["da\u00df", "euch", "mein", "Ph\u00f6\u00b7bus", "liebt", ",", "der", "nie\u00b7mals", "Feind", "sein", "kan"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,", "PRELS", "ADV", "NN", "VAINF", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "dem Volke, wie ihr seid! Die Braut geh\u00f6rt ihm an,", "tokens": ["dem", "Vol\u00b7ke", ",", "wie", "ihr", "seid", "!", "Die", "Braut", "ge\u00b7h\u00f6rt", "ihm", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VAFIN", "$.", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "der Br\u00e4utgamb ist sein Freund. Er freut sich euer Freude,", "tokens": ["der", "Br\u00e4ut\u00b7gamb", "ist", "sein", "Freund", ".", "Er", "freut", "sich", "eu\u00b7er", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "verehrt euch, was er hat, in diesem frei vom Neide,", "tokens": ["ver\u00b7ehrt", "euch", ",", "was", "er", "hat", ",", "in", "die\u00b7sem", "frei", "vom", "Nei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "PPER", "VAFIN", "$,", "APPR", "PDAT", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "da\u00df er nichts Hohes schenkt. Doch wei\u00df der gute Man,", "tokens": ["da\u00df", "er", "nichts", "Ho\u00b7hes", "schenkt", ".", "Doch", "wei\u00df", "der", "gu\u00b7te", "Man", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ADJA", "VVFIN", "$.", "KON", "VVFIN", "ART", "ADJA", "PIS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "da\u00df wenn er W\u00fcndsche giebt, so hab' er satt getan,", "tokens": ["da\u00df", "wenn", "er", "W\u00fcnd\u00b7sche", "giebt", ",", "so", "hab'", "er", "satt", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "NN", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und ihr seid auch vergn\u00fcgt. Sie spinnen Gold f\u00fcr Seide,", "tokens": ["und", "ihr", "seid", "auch", "ver\u00b7gn\u00fcgt", ".", "Sie", "spin\u00b7nen", "Gold", "f\u00fcr", "Sei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "VVPP", "$.", "PPER", "VVFIN", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "die Parzen \u00fcber euch. Des Jupiters Gemahl", "tokens": ["die", "Par\u00b7zen", "\u00fc\u00b7ber", "euch", ".", "Des", "Ju\u00b7pi\u00b7ters", "Ge\u00b7mahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "$.", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "f\u00fchrt euch das Lager auf. Der Hymen steckt den Saal", "tokens": ["f\u00fchrt", "euch", "das", "La\u00b7ger", "auf", ".", "Der", "Hy\u00b7men", "steckt", "den", "Saal"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "mit liechten Fackeln voll und l\u00e4\u00dft den Brauttanz machen,", "tokens": ["mit", "liech\u00b7ten", "Fa\u00b7ckeln", "voll", "und", "l\u00e4\u00dft", "den", "Braut\u00b7tanz", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "KON", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "den mein Apollo singt. Freit, tanzet, schlafet wol!", "tokens": ["den", "mein", "A\u00b7pol\u00b7lo", "singt", ".", "Freit", ",", "tan\u00b7zet", ",", "schla\u00b7fet", "wol", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NE", "VVFIN", "$.", "NN", "$,", "VVFIN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Schlu\u00df der G\u00f6tter steht; was drauf geschehen soll,", "tokens": ["Der", "Schlu\u00df", "der", "G\u00f6t\u00b7ter", "steht", ";", "was", "drauf", "ge\u00b7sche\u00b7hen", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$.", "PWS", "PAV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "das ist des Vatern Lust, der jungen Mutter Lachen.", "tokens": ["das", "ist", "des", "Va\u00b7tern", "Lust", ",", "der", "jun\u00b7gen", "Mut\u00b7ter", "La\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}