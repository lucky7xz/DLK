{"dta.poem.19650": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Lenore .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es stehn die Stern am Himmel,               ", "tokens": ["Es", "stehn", "die", "Stern", "am", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es scheint der Mond so hell,", "tokens": ["Es", "scheint", "der", "Mond", "so", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Todten reiten schnell:", "tokens": ["Die", "Tod\u00b7ten", "rei\u00b7ten", "schnell", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mach auf mein Schatz dein Fenster,", "tokens": ["Mach", "auf", "mein", "Schatz", "dein", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "La\u00df mich zu dir hinein,", "tokens": ["La\u00df", "mich", "zu", "dir", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kann nicht lang bey dir seyn;", "tokens": ["Kann", "nicht", "lang", "bey", "dir", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ADJD", "APPR", "PPER", "VAINF", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Der Hahn der th\u00e4t schon kr\u00e4hen,", "tokens": ["Der", "Hahn", "der", "th\u00e4t", "schon", "kr\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er singt uns an den Tag,", "tokens": ["Er", "singt", "uns", "an", "den", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht lang mehr bleiben mag.", "tokens": ["Nicht", "lang", "mehr", "blei\u00b7ben", "mag."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["PTKNEG", "ADJD", "ADV", "VVINF", "VMFIN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Weit bin ich her geritten,", "tokens": ["Weit", "bin", "ich", "her", "ge\u00b7rit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Zweihundert Meilen weit,", "tokens": ["Zwei\u00b7hun\u00b7dert", "Mei\u00b7len", "weit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mu\u00df ich noch reiten heut;", "tokens": ["Mu\u00df", "ich", "noch", "rei\u00b7ten", "heut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Herzallerliebste meine!", "tokens": ["Her\u00b7zal\u00b7ler\u00b7liebs\u00b7te", "mei\u00b7ne", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Komm setz dich auf mein Pferd,", "tokens": ["Komm", "setz", "dich", "auf", "mein", "Pferd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Weg ist reitens werth:", "tokens": ["Der", "Weg", "ist", "rei\u00b7tens", "werth", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dort drin im Ungerlande", "tokens": ["Dort", "drin", "im", "Un\u00b7ger\u00b7lan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hab ich ein kleines Haus,", "tokens": ["Hab", "ich", "ein", "klei\u00b7nes", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da geht mein Weg hinaus.", "tokens": ["Da", "geht", "mein", "Weg", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Auf einer gr\u00fcnen Haide,", "tokens": ["Auf", "ei\u00b7ner", "gr\u00fc\u00b7nen", "Hai\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da ist mein Haus gebaut,", "tokens": ["Da", "ist", "mein", "Haus", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr mich und meine Braut.", "tokens": ["F\u00fcr", "mich", "und", "mei\u00b7ne", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "La\u00df mich nicht lang mehr warten,", "tokens": ["La\u00df", "mich", "nicht", "lang", "mehr", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Komm Schatz zu mir herauf,", "tokens": ["Komm", "Schatz", "zu", "mir", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil fort geht unser Lauf.", "tokens": ["Weil", "fort", "geht", "un\u00b7ser", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKVZ", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Die Sternlein thun uns leuchten,", "tokens": ["Die", "Stern\u00b7lein", "thun", "uns", "leuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es scheint der Mond so hell,", "tokens": ["Es", "scheint", "der", "Mond", "so", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Todten reiten schnell.", "tokens": ["Die", "Tod\u00b7ten", "rei\u00b7ten", "schnell", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wo willst mich dann hinf\u00fchren?", "tokens": ["Wo", "willst", "mich", "dann", "hin\u00b7f\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ach Gott! was hast gedacht", "tokens": ["Ach", "Gott", "!", "was", "hast", "ge\u00b7dacht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "NN", "$.", "PWS", "VAFIN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohl in der finstern Nacht?", "tokens": ["Wohl", "in", "der", "fins\u00b7tern", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Mit dir kann ich nicht reiten,", "tokens": ["Mit", "dir", "kann", "ich", "nicht", "rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Dein Bettlein ist nicht breit,", "tokens": ["Dein", "Bet\u00b7tlein", "ist", "nicht", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Weg ist auch zu weit.", "tokens": ["Der", "Weg", "ist", "auch", "zu", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Allein leg du dich nieder,", "tokens": ["Al\u00b7lein", "leg", "du", "dich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Herzallerliebster schlaf!", "tokens": ["Her\u00b7zal\u00b7ler\u00b7liebs\u00b7ter", "schlaf", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis an den j\u00fcngsten Tag.", "tokens": ["Bis", "an", "den", "j\u00fcng\u00b7sten", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}