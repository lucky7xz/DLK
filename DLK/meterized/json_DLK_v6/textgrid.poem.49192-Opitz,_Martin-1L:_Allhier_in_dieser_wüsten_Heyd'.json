{"textgrid.poem.49192": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Allhier in dieser w\u00fcsten Heyd'", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Allhier in dieser w\u00fcsten Heyd'", "tokens": ["All\u00b7hier", "in", "die\u00b7ser", "w\u00fcs\u00b7ten", "Heyd'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist gar kein Mensch nicht weit und breit,", "tokens": ["Ist", "gar", "kein", "Mensch", "nicht", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "PTKNEG", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die wilden Thier allein", "tokens": ["Die", "wil\u00b7den", "Thier", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die seh' ich selbst Mitleyden tragen,", "tokens": ["Die", "seh'", "ich", "selbst", "Mit\u00b7ley\u00b7den", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die V\u00f6gel traurig seyn,", "tokens": ["Die", "V\u00f6\u00b7gel", "trau\u00b7rig", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und mich mit schwacher Stimme klagen,", "tokens": ["Und", "mich", "mit", "schwa\u00b7cher", "Stim\u00b7me", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die kalten Brunnen st\u00e4rcker fliessen,", "tokens": ["Die", "kal\u00b7ten", "Brun\u00b7nen", "st\u00e4r\u00b7cker", "flies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Viel Threnen gleichfalls zu vergiessen.", "tokens": ["Viel", "Thre\u00b7nen", "gleich\u00b7falls", "zu", "ver\u00b7gies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Stein, W\u00e4lder, Wiesen, Feld und Thal", "tokens": ["Stein", ",", "W\u00e4l\u00b7der", ",", "Wie\u00b7sen", ",", "Feld", "und", "Thal"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00f6r' ich beklagen meinen Fall.", "tokens": ["H\u00f6r'", "ich", "be\u00b7kla\u00b7gen", "mei\u00b7nen", "Fall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie f\u00fchlen meine Pein,", "tokens": ["Sie", "f\u00fch\u00b7len", "mei\u00b7ne", "Pein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Schafe wollen gar nichts weiden.", "tokens": ["Die", "Scha\u00b7fe", "wol\u00b7len", "gar", "nichts", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du, Delia, allein", "tokens": ["Du", ",", "De\u00b7lia", ",", "al\u00b7lein"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["PPER", "$,", "NE", "$,", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Wirst nicht beweget durch mein Leiden,", "tokens": ["Wirst", "nicht", "be\u00b7we\u00b7get", "durch", "mein", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Du Kron und Zier der Sch\u00e4fferinnen,", "tokens": ["Du", "Kron", "und", "Zier", "der", "Sch\u00e4f\u00b7fe\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Du strenge F\u00fcrstin meiner Sinnen.", "tokens": ["Du", "stren\u00b7ge", "F\u00fcrs\u00b7tin", "mei\u00b7ner", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "In dich hab' ich mein Ziel gericht,", "tokens": ["In", "dich", "hab'", "ich", "mein", "Ziel", "ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein einig All, meins Lebens Liecht;", "tokens": ["Mein", "ei\u00b7nig", "All", ",", "meins", "Le\u00b7bens", "Liecht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun hat de\u00df Gl\u00fcckes Neid", "tokens": ["Nun", "hat", "de\u00df", "Gl\u00fc\u00b7ckes", "Neid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Von deiner Seiten mich gerissen,", "tokens": ["Von", "dei\u00b7ner", "Sei\u00b7ten", "mich", "ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Drumb w\u00fcntsch' ich dieser Zeit", "tokens": ["Drumb", "w\u00fcnt\u00b7sch'", "ich", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht mehr de\u00df Lebens zu geniessen,", "tokens": ["Nicht", "mehr", "de\u00df", "Le\u00b7bens", "zu", "ge\u00b7nies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Vom Todte nur werd' ich bekommen", "tokens": ["Vom", "Tod\u00b7te", "nur", "werd'", "ich", "be\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Freyheit, so du mir genommen.", "tokens": ["Die", "Frey\u00b7heit", ",", "so", "du", "mir", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "La\u00df' ich gleich aber diese Welt,", "tokens": ["La\u00df'", "ich", "gleich", "a\u00b7ber", "die\u00b7se", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird meine Treu doch nicht gefellt;", "tokens": ["Wird", "mei\u00b7ne", "Treu", "doch", "nicht", "ge\u00b7fellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Liebe gegen dir", "tokens": ["Die", "Lie\u00b7be", "ge\u00b7gen", "dir"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hab' ich an manchem Baum geschnitten;", "tokens": ["Hab'", "ich", "an", "man\u00b7chem", "Baum", "ge\u00b7schnit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da sieht man f\u00fcr und f\u00fcr,", "tokens": ["Da", "sieht", "man", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Was ich f\u00fcr Angst und Pein erlitten;", "tokens": ["Was", "ich", "f\u00fcr", "Angst", "und", "Pein", "er\u00b7lit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So lang' Arcadia wird stehen", "tokens": ["So", "lang'", "Ar\u00b7ca\u00b7dia", "wird", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "NE", "VAFIN", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Soll auch mein Nahme nicht vergehen.", "tokens": ["Soll", "auch", "mein", "Nah\u00b7me", "nicht", "ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}}, "stanza.5": {"line.1": {"text": "Es trit Diana selber hin,", "tokens": ["Es", "trit", "Di\u00b7a\u00b7na", "sel\u00b7ber", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Grab zu machen in das Gr\u00fcn,", "tokens": ["Mein", "Grab", "zu", "ma\u00b7chen", "in", "das", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die G\u00f6ttin Flora geht", "tokens": ["Die", "G\u00f6t\u00b7tin", "Flo\u00b7ra", "geht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sich nach Violen umbzuschauen,", "tokens": ["Sich", "nach", "Vi\u00b7o\u00b7len", "um\u00b7bzu\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Leichstein ist erh\u00f6ht,", "tokens": ["Mein", "Leichs\u00b7tein", "ist", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Darein die Nymphen werden hauen:", "tokens": ["Da\u00b7rein", "die", "Nym\u00b7phen", "wer\u00b7den", "hau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Hier hat den Geist dahin gegeben", "tokens": ["Hier", "hat", "den", "Geist", "da\u00b7hin", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PAV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der seine Liebste bracht' umbs Leben.", "tokens": ["Der", "sei\u00b7ne", "Liebs\u00b7te", "bracht'", "umbs", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}