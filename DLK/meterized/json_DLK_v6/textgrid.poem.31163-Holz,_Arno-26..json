{"textgrid.poem.31163": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gr\u00fcndunkel wehn die Pinien,", "tokens": ["Gr\u00fcn\u00b7dun\u00b7kel", "wehn", "die", "Pi\u00b7ni\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Von Fern her blaut die See \u2013", "tokens": ["Von", "Fern", "her", "blaut", "die", "See", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APZR", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schau nicht so ernst in die sch\u00f6ne Welt,", "tokens": ["Schau", "nicht", "so", "ernst", "in", "die", "sch\u00f6\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Wie die Griechin Antigone!", "tokens": ["Wie", "die", "Grie\u00b7ch\u00b7in", "An\u00b7ti\u00b7go\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NE", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Dein Goldhaar flackert wie Feuer,", "tokens": ["Dein", "Gold\u00b7haar", "fla\u00b7ckert", "wie", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Micht packt ein wild Gel\u00fcst:", "tokens": ["Micht", "packt", "ein", "wild", "Ge\u00b7l\u00fcst", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denk lieber, du seist Cleopatra,", "tokens": ["Denk", "lie\u00b7ber", ",", "du", "seist", "Cleo\u00b7pat\u00b7ra", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "VAFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die ihren C\u00e4sar k\u00fcsst!", "tokens": ["Die", "ih\u00b7ren", "C\u00e4\u00b7sar", "k\u00fcsst", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Befiel, und der Erdball zerbl\u00e4ttert,", "tokens": ["Be\u00b7fiel", ",", "und", "der", "Erd\u00b7ball", "zer\u00b7bl\u00e4t\u00b7tert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "So wahr ich sein Herrscher bin,", "tokens": ["So", "wahr", "ich", "sein", "Herr\u00b7scher", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und zitternd vor deine", "tokens": ["Und", "zit\u00b7ternd", "vor", "dei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPOSAT"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "F\u00fcsse kniet Die ewige Roma hin!", "tokens": ["F\u00fcs\u00b7se", "kniet", "Die", "e\u00b7wi\u00b7ge", "Ro\u00b7ma", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Die V\u00f6lker bauen dir Tempel", "tokens": ["Die", "V\u00f6l\u00b7ker", "bau\u00b7en", "dir", "Tem\u00b7pel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von Susa bis Carchedoon \u2013", "tokens": ["Von", "Su\u00b7sa", "bis", "Car\u00b7che\u00b7do\u00b7on", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NE", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du aber, als Aphrodite, setzt", "tokens": ["Du", "a\u00b7ber", ",", "als", "A\u00b7phro\u00b7di\u00b7te", ",", "setzt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "ADV", "$,", "KOUS", "NN", "$,", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dich, l\u00e4chelnd, auf meinen Thron!", "tokens": ["Dich", ",", "l\u00e4\u00b7chelnd", ",", "auf", "mei\u00b7nen", "Thron", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJD", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}