{"textgrid.poem.39868": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gesundheit nehm' ich mit aus diesem Bade,", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gesundheit nehm' ich mit aus diesem Bade,", "tokens": ["Ge\u00b7sund\u00b7heit", "nehm'", "ich", "mit", "aus", "die\u00b7sem", "Ba\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Heil, das nicht blos dem Leibe widerfuhr,", "tokens": ["Heil", ",", "das", "nicht", "blos", "dem", "Lei\u00b7be", "wi\u00b7der\u00b7fuhr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PTKNEG", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nein, auch mein Sinn, dem sie des Lebens Spur", "tokens": ["Nein", ",", "auch", "mein", "Sinn", ",", "dem", "sie", "des", "Le\u00b7bens", "Spur"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Tief eingedr\u00fcckt, ist wieder hell und grade.", "tokens": ["Tief", "ein\u00b7ge\u00b7dr\u00fcckt", ",", "ist", "wie\u00b7der", "hell", "und", "gra\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "VAFIN", "ADV", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Liebe, die mich auf dem J\u00fcnglingspfade", "tokens": ["Die", "Lie\u00b7be", ",", "die", "mich", "auf", "dem", "J\u00fcng\u00b7lings\u00b7pfa\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geh\u00fctet, war nicht eitle T\u00e4uschung nur,", "tokens": ["Ge\u00b7h\u00fc\u00b7tet", ",", "war", "nicht", "eit\u00b7le", "T\u00e4u\u00b7schung", "nur", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VAFIN", "PTKNEG", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Hoffnung Gr\u00fcn ward ewiger Azur,", "tokens": ["Der", "Hoff\u00b7nung", "Gr\u00fcn", "ward", "e\u00b7wi\u00b7ger", "A\u00b7zur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der w\u00f6lbt sich \u00fcber uns als Schirm der Gnade.", "tokens": ["Der", "w\u00f6lbt", "sich", "\u00fc\u00b7ber", "uns", "als", "Schirm", "der", "Gna\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "PPER", "KOUS", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das Kind, das unter diesem Himmel ruht,", "tokens": ["Das", "Kind", ",", "das", "un\u00b7ter", "die\u00b7sem", "Him\u00b7mel", "ruht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hat Ohr und Wort in meiner Phantasie,", "tokens": ["Hat", "Ohr", "und", "Wort", "in", "mei\u00b7ner", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Mutter, die es tr\u00e4gt, hat Jugendsch\u00f6ne.", "tokens": ["Die", "Mut\u00b7ter", ",", "die", "es", "tr\u00e4gt", ",", "hat", "Ju\u00b7gend\u00b7sch\u00f6\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein duft'ger Vollmondschein verschwindet nie,", "tokens": ["Ein", "duft'\u00b7ger", "Voll\u00b7mond\u00b7schein", "ver\u00b7schwin\u00b7det", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wallt um das holde Paar wie Silberflut,", "tokens": ["Wallt", "um", "das", "hol\u00b7de", "Paar", "wie", "Sil\u00b7ber\u00b7flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und leise fl\u00f6tend klingen Liedest\u00f6ne.", "tokens": ["Und", "lei\u00b7se", "fl\u00f6\u00b7tend", "klin\u00b7gen", "Lie\u00b7des\u00b7t\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}