{"textgrid.poem.52673": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Entscheidung", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich in dein Herz voll hei\u00dfer Liebesfreud", "tokens": ["Als", "ich", "in", "dein", "Herz", "voll", "hei\u00b7\u00dfer", "Lie\u00b7bes\u00b7freud"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Warf einmal ein W\u00f6rtlein k\u00fchler N\u00fcchternheit,", "tokens": ["Warf", "ein\u00b7mal", "ein", "W\u00f6rt\u00b7lein", "k\u00fch\u00b7ler", "N\u00fcch\u00b7tern\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Da zerging die Rose deiner Wangen,", "tokens": ["Da", "zer\u00b7ging", "die", "Ro\u00b7se", "dei\u00b7ner", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Laut zu weinen hast du angefangen.", "tokens": ["Laut", "zu", "wei\u00b7nen", "hast", "du", "an\u00b7ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PTKZU", "VVINF", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Sprangest auf und wieder sankest du zur\u00fcck,", "tokens": ["Spran\u00b7gest", "auf", "und", "wie\u00b7der", "san\u00b7kest", "du", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Wie Verzeihung flehend drang in mich dein Blick,", "tokens": ["Wie", "Ver\u00b7zei\u00b7hung", "fle\u00b7hend", "drang", "in", "mich", "dein", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVPP", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Hast dich wild an meine Brust geschwungen,", "tokens": ["Hast", "dich", "wild", "an", "mei\u00b7ne", "Brust", "ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mich gek\u00fc\u00dft, umklammert und umschlungen.", "tokens": ["Mich", "ge\u00b7k\u00fc\u00dft", ",", "um\u00b7klam\u00b7mert", "und", "um\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "VVPP", "KON", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Aber ich \u2013 erbebte. Solche Schmerzenskraft", "tokens": ["A\u00b7ber", "ich", "\u2013", "er\u00b7beb\u00b7te", ".", "Sol\u00b7che", "Schmer\u00b7zen\u00b7skraft"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "$(", "VVFIN", "$.", "PIAT", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Wandelte die Liebe mir in Leidenschaft \u2013", "tokens": ["Wan\u00b7del\u00b7te", "die", "Lie\u00b7be", "mir", "in", "Lei\u00b7den\u00b7schaft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "APPR", "NN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Auf die sch\u00f6nen Augen schaut ich nieder,", "tokens": ["Auf", "die", "sch\u00f6\u00b7nen", "Au\u00b7gen", "schaut", "ich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Auf die F\u00fclle wonnereicher Glieder.", "tokens": ["Auf", "die", "F\u00fcl\u00b7le", "won\u00b7ne\u00b7rei\u00b7cher", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Nur ein Wort von mir, und heitrer Himmel bricht", "tokens": ["Nur", "ein", "Wort", "von", "mir", ",", "und", "hei\u00b7trer", "Him\u00b7mel", "bricht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "PPER", "$,", "KON", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Aus dem theuren Antlitz, o so lieb und licht.", "tokens": ["Aus", "dem", "theu\u00b7ren", "Ant\u00b7litz", ",", "o", "so", "lieb", "und", "licht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "FM", "ADV", "ADJD", "KON", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Ewigen Gru\u00df dir, wundervolle Stunde,", "tokens": ["E\u00b7wi\u00b7gen", "Gru\u00df", "dir", ",", "wun\u00b7der\u00b7vol\u00b7le", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Ich frohlocke dir mit Herz und Munde!", "tokens": ["Ich", "froh\u00b7lo\u00b7cke", "dir", "mit", "Herz", "und", "Mun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}