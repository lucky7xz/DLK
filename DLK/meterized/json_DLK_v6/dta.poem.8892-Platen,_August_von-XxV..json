{"dta.poem.8892": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.85", "af:0.14"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "In diesen L\u00fcften, die sich leise regen,", "tokens": ["In", "die\u00b7sen", "L\u00fcf\u00b7ten", ",", "die", "sich", "lei\u00b7se", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "PRF", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus jenen Hallen weht es mir entgegen,", "tokens": ["Aus", "je\u00b7nen", "Hal\u00b7len", "weht", "es", "mir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo Scherz und Jubel sonst gepflegt zu thronen.", "tokens": ["Wo", "Scherz", "und", "Ju\u00b7bel", "sonst", "ge\u00b7pflegt", "zu", "thro\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "ADV", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Venedig fiel, wiewohl's getrozt Aeonen,", "tokens": ["Ve\u00b7ne\u00b7dig", "fiel", ",", "wie\u00b7wohl's", "ge\u00b7trozt", "A\u00b7e\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PWAV", "VVPP", "NN", "$,"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Das Rad des Gl\u00fccks kann nichts zur\u00fcckbewegen:", "tokens": ["Das", "Rad", "des", "Gl\u00fccks", "kann", "nichts", "zu\u00b7r\u00fcck\u00b7be\u00b7we\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Oed' ist der Hafen, wen'ge Schiffe legen", "tokens": ["O\u00b7ed'", "ist", "der", "Ha\u00b7fen", ",", "wen'\u00b7ge", "Schif\u00b7fe", "le\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "PIAT", "NN", "VVINF"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sich an die sch\u00f6ne Riva der Sclavonen.", "tokens": ["Sich", "an", "die", "sch\u00f6\u00b7ne", "Ri\u00b7va", "der", "Scla\u00b7vo\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie hast du sonst, Venetia, geprahlet", "tokens": ["Wie", "hast", "du", "sonst", ",", "Ve\u00b7ne\u00b7tia", ",", "ge\u00b7prah\u00b7let"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$,", "NE", "$,", "VVPP"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als stolzes Weib mit goldenen Gew\u00e4ndern,", "tokens": ["Als", "stol\u00b7zes", "Weib", "mit", "gol\u00b7de\u00b7nen", "Ge\u00b7w\u00e4n\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So wie dich Paolo Veronese mahlet!", "tokens": ["So", "wie", "dich", "Pao\u00b7lo", "Ve\u00b7ro\u00b7ne\u00b7se", "mah\u00b7let", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "NE", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Nun steht ein Dichter an den Prachtgel\u00e4ndern", "tokens": ["Nun", "steht", "ein", "Dich\u00b7ter", "an", "den", "Pracht\u00b7ge\u00b7l\u00e4n\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Riesentreppe staunend und bezahlet", "tokens": ["Der", "Rie\u00b7sen\u00b7trep\u00b7pe", "stau\u00b7nend", "und", "be\u00b7zah\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Thr\u00e4nenzoll, der nichts vermag zu \u00e4ndern!", "tokens": ["Den", "Thr\u00e4\u00b7nen\u00b7zoll", ",", "der", "nichts", "ver\u00b7mag", "zu", "\u00e4n\u00b7dern", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}