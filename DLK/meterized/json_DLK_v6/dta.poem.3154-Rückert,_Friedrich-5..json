{"dta.poem.3154": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Tod ein Schauder und Entsetzen der Natur,", "tokens": ["Der", "Tod", "ein", "Schau\u00b7der", "und", "Ent\u00b7set\u00b7zen", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Anblick f\u00fcrchterlich, hold dem Gedanken nur.", "tokens": ["Dem", "An\u00b7blick", "f\u00fcrch\u00b7ter\u00b7lich", ",", "hold", "dem", "Ge\u00b7dan\u00b7ken", "nur", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ADJD", "ART", "NN", "ADV", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "S\u00fc\u00df ist Gestorbensein, und bitter nicht ist Sterben,", "tokens": ["S\u00fc\u00df", "ist", "Ge\u00b7stor\u00b7ben\u00b7sein", ",", "und", "bit\u00b7ter", "nicht", "ist", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,", "KON", "ADJD", "PTKNEG", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch Sterbensehen ist der Lebenslust Verderben.", "tokens": ["Doch", "Ster\u00b7ben\u00b7se\u00b7hen", "ist", "der", "Le\u00b7bens\u00b7lust", "Ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und um wie h\u00f6her steht schon auf der Stufenleiter", "tokens": ["Und", "um", "wie", "h\u00f6\u00b7her", "steht", "schon", "auf", "der", "Stu\u00b7fen\u00b7lei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PWAV", "ADJD", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Leben, um so mehr sind widerlich die Scheiter.", "tokens": ["Ein", "Le\u00b7ben", ",", "um", "so", "mehr", "sind", "wi\u00b7der\u00b7lich", "die", "Schei\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "ADV", "ADV", "VAFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Stein, lebendigtodt, ist drum sich immer gleich,", "tokens": ["Der", "Stein", ",", "le\u00b7ben\u00b7dig\u00b7todt", ",", "ist", "drum", "sich", "im\u00b7mer", "gleich", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "VAFIN", "PAV", "PRF", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihn macht der Tod nicht kalt, ihn macht der Tod nicht bleich.", "tokens": ["Ihn", "macht", "der", "Tod", "nicht", "kalt", ",", "ihn", "macht", "der", "Tod", "nicht", "bleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "ADJD", "$,", "PPER", "VVFIN", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die Blum' auch welket zwar, vom Stengel abgepfl\u00fcckt,", "tokens": ["Die", "Blum'", "auch", "wel\u00b7ket", "zwar", ",", "vom", "Sten\u00b7gel", "ab\u00b7ge\u00b7pfl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADV", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ist die welke noch mit Farb' und Duft geschm\u00fcckt.", "tokens": ["Doch", "ist", "die", "wel\u00b7ke", "noch", "mit", "Fa\u00b7rb'", "und", "Duft", "ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+---+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Und jene Bl\u00fcthe, die an keinem Stiel darf rasten,", "tokens": ["Und", "je\u00b7ne", "Bl\u00fc\u00b7the", ",", "die", "an", "kei\u00b7nem", "Stiel", "darf", "ras\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "PRELS", "APPR", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Schmetterling ist sch\u00f6n noch in des Sammlers Kasten.", "tokens": ["Der", "Schmet\u00b7ter\u00b7ling", "ist", "sch\u00f6n", "noch", "in", "des", "Samm\u00b7lers", "Kas\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Der Vogel, dem das Herz nicht unter'm Flaum mehr klopft,", "tokens": ["Der", "Vo\u00b7gel", ",", "dem", "das", "Herz", "nicht", "un\u00b7ter'm", "Flaum", "mehr", "klopft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "ART", "NN", "PTKNEG", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und steif den Fittig h\u00e4ngt, ist artig ausgestopft.", "tokens": ["Und", "steif", "den", "Fit\u00b7tig", "h\u00e4ngt", ",", "ist", "ar\u00b7tig", "aus\u00b7ge\u00b7stopft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVFIN", "$,", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Die gr\u00f6\u00dfern Thiere, die n\u00e4chst an den Menschen reichen,", "tokens": ["Die", "gr\u00f6\u00b7\u00dfern", "Thie\u00b7re", ",", "die", "n\u00e4chst", "an", "den", "Men\u00b7schen", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind widerw\u00e4rtiger, je gr\u00f6\u00dfer ihre Leichen.", "tokens": ["Sind", "wi\u00b7der\u00b7w\u00e4r\u00b7ti\u00b7ger", ",", "je", "gr\u00f6\u00b7\u00dfer", "ih\u00b7re", "Lei\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Doch nur den Menschen, weil er ist des Lebens Krone,", "tokens": ["Doch", "nur", "den", "Men\u00b7schen", ",", "weil", "er", "ist", "des", "Le\u00b7bens", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Macht v\u00f6llig schauderhaft das Leben, das entfloh'ne.", "tokens": ["Macht", "v\u00f6l\u00b7lig", "schau\u00b7der\u00b7haft", "das", "Le\u00b7ben", ",", "das", "ent\u00b7floh'\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Darum verh\u00fcllte, den der Freunde Dolch erstach,", "tokens": ["Da\u00b7rum", "ver\u00b7h\u00fcll\u00b7te", ",", "den", "der", "Freun\u00b7de", "Dolch", "er\u00b7stach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "PRELS", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein Haupt vor'm Himmelsaug', eh' ihm das Auge brach.", "tokens": ["Sein", "Haupt", "vor'm", "Him\u00b7mels\u00b7aug'", ",", "eh'", "ihm", "das", "Au\u00b7ge", "brach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Wie auf Naturgehei\u00df die Thier' auch, wenn sie siechen", "tokens": ["Wie", "auf", "Na\u00b7tur\u00b7ge\u00b7hei\u00df", "die", "Thier'", "auch", ",", "wenn", "sie", "sie\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "ART", "NN", "ADV", "$,", "KOUS", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am letzten Weh, in Schl\u00fcft' und H\u00f6hlen sich verkriechen.", "tokens": ["Am", "letz\u00b7ten", "Weh", ",", "in", "Schl\u00fcft'", "und", "H\u00f6h\u00b7len", "sich", "ver\u00b7krie\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Und ein mit Sch\u00f6nheitsinn begabtes Volk bedeckt", "tokens": ["Und", "ein", "mit", "Sch\u00f6n\u00b7hei\u00b7tsinn", "be\u00b7gab\u00b7tes", "Volk", "be\u00b7deckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "NN", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Sarg mit Blumen, da\u00df sein Anblick minder schreckt,", "tokens": ["Den", "Sarg", "mit", "Blu\u00b7men", ",", "da\u00df", "sein", "An\u00b7blick", "min\u00b7der", "schreckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Nachahmend der Natur, die, \u00fcberall erf\u00fcllt", "tokens": ["Nac\u00b7hah\u00b7mend", "der", "Na\u00b7tur", ",", "die", ",", "\u00fc\u00b7be\u00b7rall", "er\u00b7f\u00fcllt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "$,", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Gr\u00e4bern, jedes Grab in Blumenteppich h\u00fcllt.", "tokens": ["Von", "Gr\u00e4\u00b7bern", ",", "je\u00b7des", "Grab", "in", "Blu\u00b7men\u00b7tep\u00b7pich", "h\u00fcllt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PIAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}