{"textgrid.poem.27764": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "79.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die neue Welt ist from und fr\u00f6mer als die alte;", "tokens": ["Die", "neu\u00b7e", "Welt", "ist", "from", "und", "fr\u00f6\u00b7mer", "als", "die", "al\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "KON", "ADJD", "KOKOM", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie darff nur acht Gebot, die sie im Leben halte;", "tokens": ["Sie", "darff", "nur", "acht", "Ge\u00b7bot", ",", "die", "sie", "im", "Le\u00b7ben", "hal\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "CARD", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn Ehbruch, Diebstal bleibt; man hanet nur die Leute", "tokens": ["Denn", "Eh\u00b7bruch", ",", "Diebs\u00b7tal", "bleibt", ";", "man", "ha\u00b7net", "nur", "die", "Leu\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "NN", "VVFIN", "$.", "PIS", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und macht, was uns gef\u00e4llt, nach Krieges Art zur Beute.", "tokens": ["Und", "macht", ",", "was", "uns", "ge\u00b7f\u00e4llt", ",", "nach", "Krie\u00b7ges", "Art", "zur", "Beu\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "PPER", "VVPP", "$,", "APPR", "NN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}