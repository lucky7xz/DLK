{"dta.poem.2439": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "An den reichen Mann und armen  \n Lazarus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Du hast mit tausend Weh dein Leiden \u00fcberwunden/", "tokens": ["Du", "hast", "mit", "tau\u00b7send", "Weh", "dein", "Lei\u00b7den", "\u00fc\u00b7berw\u00b7un\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "CARD", "NN", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und vor dein Thr\u00e4nenhau\u00df das Himmelreich gefun-", "tokens": ["Und", "vor", "dein", "Thr\u00e4\u00b7nen\u00b7hau\u00df", "das", "Him\u00b7mel\u00b7reich", "ge\u00b7fun"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "den/", "tokens": ["den", "/"], "token_info": ["word", "punct"], "pos": ["ART", "$("], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Du armer Lazarus; Du aber reicher Mann/", "tokens": ["Du", "ar\u00b7mer", "La\u00b7za\u00b7rus", ";", "Du", "a\u00b7ber", "rei\u00b7cher", "Mann", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "PPER", "ADV", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Lebst in dem hellschen Pfuhl da man nicht retten kan/", "tokens": ["Lebst", "in", "dem", "hell\u00b7schen", "Pfuhl", "da", "man", "nicht", "ret\u00b7ten", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "KOUS", "PIS", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da kein\u2019 Erl\u00f6sung ist. Auch ich wil gerne leiden/", "tokens": ["Da", "kein'", "Er\u00b7l\u00f6\u00b7sung", "ist", ".", "Auch", "ich", "wil", "ger\u00b7ne", "lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "$.", "ADV", "PPER", "VMFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Was ich nur leiden kan/ \u00fcm jene Himmelsfreuden.", "tokens": ["Was", "ich", "nur", "lei\u00b7den", "kan", "/", "\u00fcm", "je\u00b7ne", "Him\u00b7mels\u00b7freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVINF", "VMFIN", "$(", "ADV", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Hat ungerechtes Gut zu Lohn die hellsche Pein/", "tokens": ["Hat", "un\u00b7ge\u00b7rech\u00b7tes", "Gut", "zu", "Lohn", "die", "hell\u00b7sche", "Pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "APPR", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So wil ich lieber noch ein armer Betler seyn.", "tokens": ["So", "wil", "ich", "lie\u00b7ber", "noch", "ein", "ar\u00b7mer", "Bet\u00b7ler", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}