{"textgrid.poem.43168": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie bist Du bl\u00fchend sch\u00f6n und hold,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie bist Du bl\u00fchend sch\u00f6n und hold,", "tokens": ["Wie", "bist", "Du", "bl\u00fc\u00b7hend", "sch\u00f6n", "und", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Augen blau, die Flechten gold,", "tokens": ["Die", "Au\u00b7gen", "blau", ",", "die", "Flech\u00b7ten", "gold", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein weiches, liebliches Gesicht", "tokens": ["Dein", "wei\u00b7ches", ",", "lieb\u00b7li\u00b7ches", "Ge\u00b7sicht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein frommes, r\u00fchrendes Gedicht!", "tokens": ["Ein", "from\u00b7mes", ",", "r\u00fch\u00b7ren\u00b7des", "Ge\u00b7dicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie bist Du keusch und engelrein,", "tokens": ["Wie", "bist", "Du", "keusch", "und", "en\u00b7gel\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gleich einem milden Strahlenschein;", "tokens": ["Gleich", "ei\u00b7nem", "mil\u00b7den", "Strah\u00b7len\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Unschuld Zauber Dich umflie\u00dft,", "tokens": ["Der", "Un\u00b7schuld", "Zau\u00b7ber", "Dich", "um\u00b7flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein ganzes Wesen \u00fcbergie\u00dft.....", "tokens": ["Dein", "gan\u00b7zes", "We\u00b7sen", "\u00fc\u00b7berg\u00b7ie\u00dft", "....."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schau' ich dich wieder \u00fcber's Jahr,", "tokens": ["Schau'", "ich", "dich", "wie\u00b7der", "\u00fc\u00b7ber's", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bist Du des s\u00fc\u00dfen Zaubers bar \u2013", "tokens": ["Bist", "Du", "des", "s\u00fc\u00b7\u00dfen", "Zau\u00b7bers", "bar", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Heut' z\u00e4hlt ja Deine Mutter schon", "tokens": ["Heut'", "z\u00e4hlt", "ja", "Dei\u00b7ne", "Mut\u00b7ter", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr Zukunftsschmach erfeilschten Lohn!", "tokens": ["F\u00fcr", "Zu\u00b7kunfts\u00b7schmach", "er\u00b7feilschten", "Lohn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}