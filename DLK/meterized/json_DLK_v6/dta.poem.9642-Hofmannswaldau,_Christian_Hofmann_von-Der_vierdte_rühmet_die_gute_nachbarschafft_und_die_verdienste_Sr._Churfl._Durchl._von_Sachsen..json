{"dta.poem.9642": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der vierdte r\u00fchmet die gute nachbarschafft  \n und die verdienste Sr. Churfl. Durchl. von  \n Sachsen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es mu\u00df auch unser gl\u00fccke wachsen", "tokens": ["Es", "mu\u00df", "auch", "un\u00b7ser", "gl\u00fc\u00b7cke", "wach\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch unsre gute nachbarschafft/", "tokens": ["Durch", "uns\u00b7re", "gu\u00b7te", "nach\u00b7bar\u00b7schafft", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil der erw\u00fcnschte Held der Sachsen", "tokens": ["Weil", "der", "er\u00b7w\u00fcnschte", "Held", "der", "Sach\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit uns vereinigt seine krafft.", "tokens": ["Mit", "uns", "ver\u00b7ei\u00b7nigt", "sei\u00b7ne", "krafft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er hat als Printz so grosse proben", "tokens": ["Er", "hat", "als", "Printz", "so", "gros\u00b7se", "pro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von seiner tugend abgelegt/", "tokens": ["Von", "sei\u00b7ner", "tu\u00b7gend", "ab\u00b7ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df/ da ihn GOtt zur Chur erhoben/", "tokens": ["Da\u00df", "/", "da", "ihn", "Gott", "zur", "Chur", "er\u00b7ho\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "KOUS", "PPER", "NN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gantz Teutschland ihn auff h\u00e4nden tr\u00e4gt.", "tokens": ["Gantz", "Teutschland", "ihn", "auff", "h\u00e4n\u00b7den", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPER", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}