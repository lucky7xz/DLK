{"textgrid.poem.52506": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "26. Am Quell vom Pfeffers", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh! hier aus Kl\u00fcften gro\u00df und wild", "tokens": ["Sieh", "!", "hier", "aus", "Kl\u00fcf\u00b7ten", "gro\u00df", "und", "wild"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "APPR", "NN", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entstr\u00f6mt ein Quell voll Kraft, doch wild,", "tokens": ["Ent\u00b7str\u00f6mt", "ein", "Quell", "voll", "Kraft", ",", "doch", "wild", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erst schwer erforscht, doch hell;", "tokens": ["Erst", "schwer", "er\u00b7forscht", ",", "doch", "hell", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nicht allzu warm, nicht widrig lau,", "tokens": ["Nicht", "all\u00b7zu", "warm", ",", "nicht", "wid\u00b7rig", "lau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKA", "ADJD", "$,", "PTKNEG", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im Wohlthun unerm\u00fcdlich; schau!", "tokens": ["Im", "Wohl\u00b7thun", "un\u00b7er\u00b7m\u00fcd\u00b7lich", ";", "schau", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$.", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und lern's vom edlen Quell!", "tokens": ["Und", "lern's", "vom", "ed\u00b7len", "Quell", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}