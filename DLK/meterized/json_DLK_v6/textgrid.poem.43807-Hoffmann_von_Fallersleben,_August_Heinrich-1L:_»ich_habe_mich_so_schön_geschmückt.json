{"textgrid.poem.43807": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbich habe mich so sch\u00f6n geschm\u00fcckt", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbich habe mich so sch\u00f6n geschm\u00fcckt", "tokens": ["\u00bb", "ich", "ha\u00b7be", "mich", "so", "sch\u00f6n", "ge\u00b7schm\u00fcckt"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "PPER", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit einem bunten Kranz:", "tokens": ["Mit", "ei\u00b7nem", "bun\u00b7ten", "Kranz", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie w\u00e4r' ich doch so hoch begl\u00fcckt,", "tokens": ["Wie", "w\u00e4r'", "ich", "doch", "so", "hoch", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "K\u00e4m' Einer jetzt zum Tanz", "tokens": ["K\u00e4m'", "Ei\u00b7ner", "jetzt", "zum", "Tanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wollte springen einen Reih'n", "tokens": ["Und", "woll\u00b7te", "sprin\u00b7gen", "ei\u00b7nen", "Reih'n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "VVINF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Beim Klang der Pfeifen und Schalmei'n,", "tokens": ["Beim", "Klang", "der", "Pfei\u00b7fen", "und", "Schal\u00b7mei'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Juchhe, juchhe! mit mir!\u00ab", "tokens": ["Juch\u00b7he", ",", "juch\u00b7he", "!", "mit", "mir", "!", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "VVFIN", "$.", "APPR", "PPER", "$.", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "So singt die Gans und h\u00fcpft und springt", "tokens": ["So", "singt", "die", "Gans", "und", "h\u00fcpft", "und", "springt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wirft sich in die Brust;", "tokens": ["Und", "wirft", "sich", "in", "die", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch weil sie weiter nichts erringt,", "tokens": ["Doch", "weil", "sie", "wei\u00b7ter", "nichts", "er\u00b7ringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist halb nur ihre Lust:", "tokens": ["Ist", "halb", "nur", "ih\u00b7re", "Lust", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie tanzet ja im Sonnenschein", "tokens": ["Sie", "tan\u00b7zet", "ja", "im", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit ihrem Schatten nur allein,", "tokens": ["Mit", "ih\u00b7rem", "Schat\u00b7ten", "nur", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O weh, o weh! allein.", "tokens": ["O", "weh", ",", "o", "weh", "!", "al\u00b7lein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "FM", "PTKVZ", "$.", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da huscht ein F\u00fcchslein aus dem Hag,", "tokens": ["Da", "huscht", "ein", "F\u00fcchs\u00b7lein", "aus", "dem", "Hag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Eilt nach dem G\u00e4nslein hin", "tokens": ["Eilt", "nach", "dem", "G\u00e4ns\u00b7lein", "hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und spricht: \u00bbei, sch\u00f6nen guten Tag!", "tokens": ["Und", "spricht", ":", "\u00bb", "ei", ",", "sch\u00f6\u00b7nen", "gu\u00b7ten", "Tag", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "ITJ", "$,", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gr\u00fc\u00df Gott, Frau Nachbarin!", "tokens": ["Gr\u00fc\u00df", "Gott", ",", "Frau", "Nach\u00b7ba\u00b7rin", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie steht so sch\u00f6n doch dir der Kranz!", "tokens": ["Wie", "steht", "so", "sch\u00f6n", "doch", "dir", "der", "Kranz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ADJD", "ADV", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Willst du nicht machen einen Tanz,", "tokens": ["Willst", "du", "nicht", "ma\u00b7chen", "ei\u00b7nen", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Juchhe, juchhe! mit mir?\u00ab", "tokens": ["Juch\u00b7he", ",", "juch\u00b7he", "!", "mit", "mir", "?", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "VVFIN", "$.", "APPR", "PPER", "$.", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Sie springen hin, sie springen her,", "tokens": ["Sie", "sprin\u00b7gen", "hin", ",", "sie", "sprin\u00b7gen", "her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gar lustig geht der Tanz:", "tokens": ["Gar", "lus\u00b7tig", "geht", "der", "Tanz", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbach, wenn ich so geschm\u00fcckt doch w\u00e4r'!", "tokens": ["\u00bb", "ach", ",", "wenn", "ich", "so", "ge\u00b7schm\u00fcckt", "doch", "w\u00e4r'", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "KOUS", "PPER", "ADV", "VVPP", "ADV", "VAFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Komm, reich mir deinen Kranz!\u00ab", "tokens": ["Komm", ",", "reich", "mir", "dei\u00b7nen", "Kranz", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "ADJD", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch als sie ihren Kranz ihm bot,", "tokens": ["Doch", "als", "sie", "ih\u00b7ren", "Kranz", "ihm", "bot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da bei\u00dft der Fuchs das G\u00e4nslein todt,", "tokens": ["Da", "bei\u00dft", "der", "Fuchs", "das", "G\u00e4ns\u00b7lein", "todt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O weh, o weh! die Gans. \u2013", "tokens": ["O", "weh", ",", "o", "weh", "!", "die", "Gans", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "PTKVZ", "$,", "FM", "PTKVZ", "$.", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Drum wisse, was und wo du bist,", "tokens": ["Drum", "wis\u00b7se", ",", "was", "und", "wo", "du", "bist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "PWS", "KON", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Begehrst du froh zu sein,", "tokens": ["Be\u00b7gehrst", "du", "froh", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und was mal deines Amts nicht ist,", "tokens": ["Und", "was", "mal", "dei\u00b7nes", "Amts", "nicht", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "PPOSAT", "NN", "PTKNEG", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da stell den F\u00fcrwitz ein,", "tokens": ["Da", "stell", "den", "F\u00fcr\u00b7witz", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sonst kann es leider auch gescheh'n,", "tokens": ["Sonst", "kann", "es", "lei\u00b7der", "auch", "ge\u00b7scheh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wie der Gans so wird's ergeh'n,", "tokens": ["Und", "wie", "der", "Gans", "so", "wird's", "er\u00b7geh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O weh, o weh! auch dir!", "tokens": ["O", "weh", ",", "o", "weh", "!", "auch", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "FM", "PTKVZ", "$.", "ADV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}