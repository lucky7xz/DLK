{"textgrid.poem.58848": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "67. Ein stilles Herz", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer ein recht stilles Herz begehret,", "tokens": ["Wer", "ein", "recht", "stil\u00b7les", "Herz", "be\u00b7ge\u00b7hret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Lass' sanfte fallen, was ihn st\u00f6ret,", "tokens": ["Lass'", "sanf\u00b7te", "fal\u00b7len", ",", "was", "ihn", "st\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "VVINF", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und kehr' in Jesu Herz hinein;", "tokens": ["Und", "kehr'", "in", "Je\u00b7su", "Herz", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da kann das Herze stille sein.", "tokens": ["Da", "kann", "das", "Her\u00b7ze", "stil\u00b7le", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PDS", "VVFIN", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}