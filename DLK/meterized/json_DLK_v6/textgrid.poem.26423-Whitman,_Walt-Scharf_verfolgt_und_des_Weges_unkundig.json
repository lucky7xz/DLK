{"textgrid.poem.26423": {"metadata": {"author": {"name": "Whitman, Walt", "birth": "N.A.", "death": "N.A."}, "title": "Scharf verfolgt und des Weges unkundig", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf dem Marsch, hart bedr\u00e4ngt und des Weges unkundig;", "tokens": ["Auf", "dem", "Marsch", ",", "hart", "be\u00b7dr\u00e4ngt", "und", "des", "We\u00b7ges", "un\u00b7kun\u00b7dig", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "VVPP", "KON", "ART", "NN", "ADJD", "$."], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Durch dichten Wald, mit ged\u00e4mpften Schritten im Dunkeln;", "tokens": ["Durch", "dich\u00b7ten", "Wald", ",", "mit", "ge\u00b7d\u00e4mpf\u00b7ten", "Schrit\u00b7ten", "im", "Dun\u00b7keln", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Unsere Armee geschlagen bei schweren Verlusten, und der m\u00fcrrische Rest auf dem R\u00fcckzug,", "tokens": ["Un\u00b7se\u00b7re", "Ar\u00b7mee", "ge\u00b7schla\u00b7gen", "bei", "schwe\u00b7ren", "Ver\u00b7lus\u00b7ten", ",", "und", "der", "m\u00fcr\u00b7ri\u00b7sche", "Rest", "auf", "dem", "R\u00fcck\u00b7zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "APPR", "ADJA", "NN", "$,", "KON", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+--+-+-+--+--+-", "measure": "dactylic.tetra.plus"}, "line.4": {"text": "Bis nach Mitternacht uns Lichter entgegenflimmern von einem tr\u00fcberleuchteten Hause.", "tokens": ["Bis", "nach", "Mit\u00b7ter\u00b7nacht", "uns", "Lich\u00b7ter", "ent\u00b7ge\u00b7gen\u00b7flim\u00b7mern", "von", "ei\u00b7nem", "tr\u00fc\u00b7berl\u00b7euch\u00b7te\u00b7ten", "Hau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NN", "PPER", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+--+-+--+-+-+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Wir kommen an eine Lichtung und halten beim tr\u00fcberleuchteten Hause:", "tokens": ["Wir", "kom\u00b7men", "an", "ei\u00b7ne", "Lich\u00b7tung", "und", "hal\u00b7ten", "beim", "tr\u00fc\u00b7berl\u00b7euch\u00b7te\u00b7ten", "Hau\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-++-+--+--+-+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "Eine alte ger\u00e4umige Kirche, wo die Wege sich kreuzen \u2013 ein improvisiertes Spital.", "tokens": ["Ei\u00b7ne", "al\u00b7te", "ge\u00b7r\u00e4u\u00b7mi\u00b7ge", "Kir\u00b7che", ",", "wo", "die", "We\u00b7ge", "sich", "kreu\u00b7zen", "\u2013", "ein", "im\u00b7pro\u00b7vi\u00b7sier\u00b7tes", "Spi\u00b7tal", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "PWAV", "ART", "NN", "PRF", "VVINF", "$(", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+-+-+--+-+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.7": {"text": "Einen Augenblick trete ich ein, da seh ich ein Bild weit \u00fcber alles was jemals gemalt und gedichtet:", "tokens": ["Ei\u00b7nen", "Au\u00b7gen\u00b7blick", "tre\u00b7te", "ich", "ein", ",", "da", "seh", "ich", "ein", "Bild", "weit", "\u00fc\u00b7ber", "al\u00b7les", "was", "je\u00b7mals", "ge\u00b7malt", "und", "ge\u00b7dich\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "ADJD", "APPR", "PIS", "PWS", "ADV", "VVPP", "KON", "VVPP", "$."], "meter": "+-+-+---+-+--+-+-+--+--+--+-", "measure": "trochaic.octa.plus.relaxed"}, "line.8": {"text": "Schatten von tiefstem Schwarz, sp\u00e4rlich erhellt von beweglichen Kerzen und Lampen,", "tokens": ["Schat\u00b7ten", "von", "tiefs\u00b7tem", "Schwarz", ",", "sp\u00e4r\u00b7lich", "er\u00b7hellt", "von", "be\u00b7weg\u00b7li\u00b7chen", "Ker\u00b7zen", "und", "Lam\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+--+-+---+--+--+--+-", "measure": "iambic.septa.invert"}, "line.9": {"text": "Und eine Pechfackel, festgesteckt, mit roter flackernder Flamme und Wolken von Qualm.", "tokens": ["Und", "ei\u00b7ne", "Pech\u00b7fa\u00b7ckel", ",", "fest\u00b7ge\u00b7steckt", ",", "mit", "ro\u00b7ter", "fla\u00b7ckern\u00b7der", "Flam\u00b7me", "und", "Wol\u00b7ken", "von", "Qualm", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "$,", "APPR", "ADJA", "ADJA", "NN", "KON", "NN", "APPR", "NE", "$."], "meter": "-+-+--+-+-+-+--+--+--+", "measure": "iambic.octa.plus.relaxed"}, "line.10": {"text": "Gedr\u00e4nge und Gestalten undeutlich am Boden, einige hingestreckt in den Kirchenst\u00fchlen;", "tokens": ["Ge\u00b7dr\u00e4n\u00b7ge", "und", "Ge\u00b7stal\u00b7ten", "un\u00b7deut\u00b7lich", "am", "Bo\u00b7den", ",", "ei\u00b7ni\u00b7ge", "hin\u00b7ge\u00b7streckt", "in", "den", "Kir\u00b7chen\u00b7st\u00fch\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "APPRART", "NN", "$,", "PIS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+--+-+--+-+--+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.11": {"text": "Zu meinen F\u00fc\u00dfen erkennbar ein Soldat, fast noch ein Knabe,", "tokens": ["Zu", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen", "er\u00b7kenn\u00b7bar", "ein", "Sol\u00b7dat", ",", "fast", "noch", "ein", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "ART", "NN", "$,", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "In Gefahr zu verbluten (in den Unterleib ist er getroffen).", "tokens": ["In", "Ge\u00b7fahr", "zu", "ver\u00b7blu\u00b7ten", "(", "in", "den", "Un\u00b7ter\u00b7leib", "ist", "er", "ge\u00b7trof\u00b7fen", ")", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$(", "APPR", "ART", "NN", "VAFIN", "PPER", "VVPP", "$(", "$."], "meter": "+-+--+-+-+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.13": {"text": "Ich stille zeitweilig das Blut (lilienwei\u00df ist des J\u00fcnglings Gesicht).", "tokens": ["Ich", "stil\u00b7le", "zeit\u00b7wei\u00b7lig", "das", "Blut", "(", "li\u00b7li\u00b7en\u00b7wei\u00df", "ist", "des", "J\u00fcng\u00b7lings", "Ge\u00b7sicht", ")", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$(", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$(", "$."], "meter": "-+--+--+-+-+--+--+", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Dann beim Aufbruch la\u00df ich die Augen \u00fcber das Bild hingleiten, alles zu fassen bem\u00fcht,", "tokens": ["Dann", "beim", "Auf\u00b7bruch", "la\u00df", "ich", "die", "Au\u00b7gen", "\u00fc\u00b7ber", "das", "Bild", "hing\u00b7lei\u00b7ten", ",", "al\u00b7les", "zu", "fas\u00b7sen", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVIMP", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,", "PIS", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "--+-+--+-+--+-+-+--+--+", "measure": "iambic.octa.plus.relaxed"}, "line.15": {"text": "Gesichter und Stellungen, nicht zu beschreiben; die meisten im Dunkeln, einige tot,", "tokens": ["Ge\u00b7sich\u00b7ter", "und", "Stel\u00b7lun\u00b7gen", ",", "nicht", "zu", "be\u00b7schrei\u00b7ben", ";", "die", "meis\u00b7ten", "im", "Dun\u00b7keln", ",", "ei\u00b7ni\u00b7ge", "tot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PTKNEG", "PTKZU", "VVINF", "$.", "ART", "VVFIN", "APPRART", "ADJA", "$,", "PIAT", "NN", "$,"], "meter": "-+--+--+--+--+--+-+--+", "measure": "amphibrach.penta.plus"}, "line.16": {"text": "Wund\u00e4rzte operieren, Geh\u00fclfen halten Lichter, der Geruch des \u00c4thers, der Dunst des Bluts,", "tokens": ["Wun\u00b7d\u00e4rz\u00b7te", "o\u00b7pe\u00b7rie\u00b7ren", ",", "Ge\u00b7h\u00fcl\u00b7fen", "hal\u00b7ten", "Lich\u00b7ter", ",", "der", "Ge\u00b7ruch", "des", "\u00c4\u00b7thers", ",", "der", "Dunst", "des", "Bluts", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+--+-+-+-+-+-+--+-+", "measure": "iambic.octa.plus.invert"}, "line.17": {"text": "Drau\u00dfen auch, im Hof, auf der Erde, auf Brettern und Tragbahren; einige liegen im Todesschwei\u00df;", "tokens": ["Drau\u00b7\u00dfen", "auch", ",", "im", "Hof", ",", "auf", "der", "Er\u00b7de", ",", "auf", "Bret\u00b7tern", "und", "Trag\u00b7bah\u00b7ren", ";", "ei\u00b7ni\u00b7ge", "lie\u00b7gen", "im", "To\u00b7des\u00b7schwei\u00df", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPRART", "NN", "$,", "APPR", "ART", "NN", "$,", "APPR", "NN", "KON", "NN", "$.", "PIS", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+--+--+--++-+--+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.18": {"text": "Hin und wieder ein Aufschrei oder Wehruf; des Doktors laute Befehle,", "tokens": ["Hin", "und", "wie\u00b7der", "ein", "Auf\u00b7schrei", "o\u00b7der", "We\u00b7hruf", ";", "des", "Dok\u00b7tors", "lau\u00b7te", "Be\u00b7feh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "ART", "NN", "KON", "NN", "$.", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+--+-+--+-+--+-", "measure": "trochaic.octa.plus.relaxed"}, "line.19": {"text": "Und das Blinken der kleinen Stahlinstrumente im Fackelschein.", "tokens": ["Und", "das", "Blin\u00b7ken", "der", "klei\u00b7nen", "Stah\u00b7linst\u00b7ru\u00b7men\u00b7te", "im", "Fa\u00b7ckel\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "--+--+-+-+---+-+", "measure": "anapaest.di.plus"}, "line.20": {"text": "Alles sehe ich vor mir, sehe wieder die Formen, rieche den Dunst,", "tokens": ["Al\u00b7les", "se\u00b7he", "ich", "vor", "mir", ",", "se\u00b7he", "wie\u00b7der", "die", "For\u00b7men", ",", "rie\u00b7che", "den", "Dunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "PPER", "$,", "VVFIN", "ADV", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+-+--+-+--+", "measure": "trochaic.octa.plus.relaxed"}, "line.21": {"text": "H\u00f6re drau\u00dfen das Kommando: \u00bbAntreten Leute, antreten!\u00ab", "tokens": ["H\u00f6\u00b7re", "drau\u00b7\u00dfen", "das", "Kom\u00b7man\u00b7do", ":", "\u00bb", "An\u00b7tre\u00b7ten", "Leu\u00b7te", ",", "an\u00b7tre\u00b7ten", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$.", "$(", "ADJA", "NN", "$,", "VVFIN", "$.", "$("], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.22": {"text": "Beuge mich erst zu dem sterbenden Knaben \u2013 mit aufgeschlagenen Augen l\u00e4chelt er leise mir zu;", "tokens": ["Beu\u00b7ge", "mich", "erst", "zu", "dem", "ster\u00b7ben\u00b7den", "Kna\u00b7ben", "\u2013", "mit", "auf\u00b7ge\u00b7schla\u00b7ge\u00b7nen", "Au\u00b7gen", "l\u00e4\u00b7chelt", "er", "lei\u00b7se", "mir", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$(", "APPR", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+--+--+-+--+-+--+--+", "measure": "trochaic.octa.plus.relaxed"}, "line.23": {"text": "Dann schlie\u00dft er die Augen, ruhig. \u2013 Ich eile hinaus in die Nacht,", "tokens": ["Dann", "schlie\u00dft", "er", "die", "Au\u00b7gen", ",", "ru\u00b7hig", ".", "\u2013", "Ich", "ei\u00b7le", "hin\u00b7aus", "in", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ADJD", "$.", "$(", "PPER", "VVFIN", "APZR", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.24": {"text": "Trete wieder in Reih und Glied und marschiere vorw\u00e4rts im Dunkeln,", "tokens": ["Tre\u00b7te", "wie\u00b7der", "in", "Reih", "und", "Glied", "und", "mar\u00b7schie\u00b7re", "vor\u00b7w\u00e4rts", "im", "Dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "KON", "NN", "KON", "VVFIN", "ADV", "APPRART", "ADJA", "$,"], "meter": "+-+--+-+-++-+--+-", "measure": "trochaic.octa.plus.relaxed"}, "line.25": {"text": "Noch immer des Weges unkundig.", "tokens": ["Noch", "im\u00b7mer", "des", "We\u00b7ges", "un\u00b7kun\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}