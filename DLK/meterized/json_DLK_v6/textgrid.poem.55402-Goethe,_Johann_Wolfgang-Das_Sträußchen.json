{"textgrid.poem.55402": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Das Str\u00e4u\u00dfchen", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wehet ein L\u00fcftchen", "tokens": ["We\u00b7het", "ein", "L\u00fcft\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Aus f\u00fcrstlichen W\u00e4ldern;", "tokens": ["Aus", "f\u00fcrst\u00b7li\u00b7chen", "W\u00e4l\u00b7dern", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Da l\u00e4ufet das M\u00e4dchen,", "tokens": ["Da", "l\u00e4u\u00b7fet", "das", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Da l\u00e4uft es zum Bach,", "tokens": ["Da", "l\u00e4uft", "es", "zum", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Sch\u00f6pft in beschlagne", "tokens": ["Sch\u00f6pft", "in", "be\u00b7schlag\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Eimer das Wasser.", "tokens": ["Ei\u00b7mer", "das", "Was\u00b7ser", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Vorsichtig, bed\u00e4chtig", "tokens": ["Vor\u00b7sich\u00b7tig", ",", "be\u00b7d\u00e4ch\u00b7tig"], "token_info": ["word", "punct", "word"], "pos": ["ADJD", "$,", "ADJD"], "meter": "+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Versteht sie zu sch\u00f6pfen.", "tokens": ["Ver\u00b7steht", "sie", "zu", "sch\u00f6p\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Am Flusse zum M\u00e4dchen", "tokens": ["Am", "Flus\u00b7se", "zum", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Schwimmet ein Str\u00e4u\u00dfchen,", "tokens": ["Schwim\u00b7met", "ein", "Str\u00e4u\u00df\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ein duftiges Str\u00e4u\u00dfchen", "tokens": ["Ein", "duf\u00b7ti\u00b7ges", "Str\u00e4u\u00df\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Von Veilchen und Rosen.", "tokens": ["Von", "Veil\u00b7chen", "und", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bbwenn ich, du holdes", "tokens": ["\u00bb", "wenn", "ich", ",", "du", "hol\u00b7des"], "token_info": ["punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "KOUS", "PPER", "$,", "PPER", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Bl\u00fcmchen, es w\u00fc\u00dfte,", "tokens": ["Bl\u00fcm\u00b7chen", ",", "es", "w\u00fc\u00df\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wer dich gepflanzet", "tokens": ["Wer", "dich", "ge\u00b7pflan\u00b7zet"], "token_info": ["word", "word", "word"], "pos": ["PWS", "PPER", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "In lockeren Boden,", "tokens": ["In", "lo\u00b7cke\u00b7ren", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wahrlich! dem g\u00e4b ich", "tokens": ["Wahr\u00b7lich", "!", "dem", "g\u00e4b", "ich"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$.", "ART", "VVFIN", "PPER"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ein goldenes Ringlein.", "tokens": ["Ein", "gol\u00b7de\u00b7nes", "Rin\u00b7glein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Wenn ich, du holdes", "tokens": ["Wenn", "ich", ",", "du", "hol\u00b7des"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "$,", "PPER", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Str\u00e4u\u00dfchen, es w\u00fc\u00dfte,", "tokens": ["Str\u00e4u\u00df\u00b7chen", ",", "es", "w\u00fc\u00df\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wer dich mit zartem", "tokens": ["Wer", "dich", "mit", "zar\u00b7tem"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Baste gebunden,", "tokens": ["Bas\u00b7te", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wahrlich! dem g\u00e4b ich", "tokens": ["Wahr\u00b7lich", "!", "dem", "g\u00e4b", "ich"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$.", "ART", "VVFIN", "PPER"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die Nadel vom Haare.", "tokens": ["Die", "Na\u00b7del", "vom", "Haa\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Wenn ich, du holdes", "tokens": ["Wenn", "ich", ",", "du", "hol\u00b7des"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "$,", "PPER", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Bl\u00fcmchen, es w\u00fc\u00dfte,", "tokens": ["Bl\u00fcm\u00b7chen", ",", "es", "w\u00fc\u00df\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wer in den k\u00fchlen", "tokens": ["Wer", "in", "den", "k\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Bach dich geworfen,", "tokens": ["Bach", "dich", "ge\u00b7wor\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wahrlich! dem g\u00e4b ich", "tokens": ["Wahr\u00b7lich", "!", "dem", "g\u00e4b", "ich"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$.", "ART", "VVFIN", "PPER"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Mein Kr\u00e4nzlein vom Haupte.\u00ab", "tokens": ["Mein", "Kr\u00e4nz\u00b7lein", "vom", "Haup\u00b7te", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Und so verfolgt sie", "tokens": ["Und", "so", "ver\u00b7folgt", "sie"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Das eilende Str\u00e4u\u00dfchen,", "tokens": ["Das", "ei\u00b7len\u00b7de", "Str\u00e4u\u00df\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sie eilet vorauf ihm,", "tokens": ["Sie", "ei\u00b7let", "vor\u00b7auf", "ihm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Versucht es zu fangen:", "tokens": ["Ver\u00b7sucht", "es", "zu", "fan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Da f\u00e4llt, ach! da f\u00e4llt sie", "tokens": ["Da", "f\u00e4llt", ",", "ach", "!", "da", "f\u00e4llt", "sie"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ITJ", "$.", "ADV", "VVFIN", "PPER"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ins k\u00fchlige Wasser.", "tokens": ["Ins", "k\u00fch\u00b7li\u00b7ge", "Was\u00b7ser", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}