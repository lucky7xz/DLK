{"textgrid.poem.64200": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Lied des Heimgekehrten", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch Donner des Todes,", "tokens": ["Durch", "Don\u00b7ner", "des", "To\u00b7des", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Durch Schl\u00e4ge der Schlacht", "tokens": ["Durch", "Schl\u00e4\u00b7ge", "der", "Schlacht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Hast du mich geleitet", "tokens": ["Hast", "du", "mich", "ge\u00b7lei\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PRF", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Mit schirmender Macht.", "tokens": ["Mit", "schir\u00b7men\u00b7der", "Macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Wie von Schwingen der Schw\u00e4ne", "tokens": ["Wie", "von", "Schwin\u00b7gen", "der", "Schw\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Deckte Rauschen mich zu:", "tokens": ["Deck\u00b7te", "Rau\u00b7schen", "mich", "zu", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dein waren die Fl\u00fcgel,", "tokens": ["Dein", "wa\u00b7ren", "die", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Walk\u00fcre, du.", "tokens": ["Wal\u00b7k\u00fc\u00b7re", ",", "du", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Rings sanken die K\u00e4mpfer", "tokens": ["Rings", "san\u00b7ken", "die", "K\u00e4mp\u00b7fer"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ins blut'ge Gefild:", "tokens": ["Ins", "blut'\u00b7ge", "Ge\u00b7fild", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mir hieltest du, Holde,", "tokens": ["Mir", "hiel\u00b7test", "du", ",", "Hol\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zu H\u00e4upten den Schild:", "tokens": ["Zu", "H\u00e4up\u00b7ten", "den", "Schild", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "\u00bbdu schwirrende Kugel,", "tokens": ["\u00bb", "du", "schwir\u00b7ren\u00b7de", "Ku\u00b7gel", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Such anderen Pfad,", "tokens": ["Such", "an\u00b7de\u00b7ren", "Pfad", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Du Pest mit dem Gifthauch, \u2013", "tokens": ["Du", "Pest", "mit", "dem", "Gift\u00b7hauch", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "NN", "APPR", "ART", "NN", "$,", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mein Liebling naht.", "tokens": ["Mein", "Lieb\u00b7ling", "naht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Schlang einst um die Schl\u00e4fe", "tokens": ["Schlang", "einst", "um", "die", "Schl\u00e4\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ihm Rosen der Ruh': \u2013", "tokens": ["Ihm", "Ro\u00b7sen", "der", "Ruh'", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Heut' leg' ich den Lorbeer", "tokens": ["Heut'", "leg'", "ich", "den", "Lor\u00b7beer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Der Schlachten dazu.\u00ab", "tokens": ["Der", "Schlach\u00b7ten", "da\u00b7zu", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}