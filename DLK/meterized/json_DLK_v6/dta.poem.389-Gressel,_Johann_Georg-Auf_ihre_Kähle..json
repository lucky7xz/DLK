{"dta.poem.389": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf ihre K\u00e4hle.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr zarten Perlen falt vor ihrer Marmor K\u00e4hle/", "tokens": ["Ihr", "zar\u00b7ten", "Per\u00b7len", "falt", "vor", "ih\u00b7rer", "Mar\u00b7mor", "K\u00e4h\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die wie Crystallen-Glas fein und durchsichtig ist.", "tokens": ["Die", "wie", "Cry\u00b7stal\u00b7len\u00b7Glas", "fein", "und", "durch\u00b7sich\u00b7tig", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "NE", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Der s\u00fcsse Athen-Dufft die gantze Lufft vers\u00fc\u00dft/", "tokens": ["Der", "s\u00fcs\u00b7se", "A\u00b7then\u00b7Dufft", "die", "gant\u00b7ze", "Lufft", "ver\u00b7s\u00fc\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Liljen Geruch ernehrt die krancke Seele.", "tokens": ["Der", "Lil\u00b7jen", "Ge\u00b7ruch", "er\u00b7nehrt", "die", "kran\u00b7cke", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Die Wollust hat ihr Milch im Uberflu\u00df geschencket/", "tokens": ["Die", "Wol\u00b7lust", "hat", "ihr", "Milch", "im", "U\u00b7ber\u00b7flu\u00df", "ge\u00b7schen\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So da\u00df ein grosser Brunn da lauter Zucker quillt", "tokens": ["So", "da\u00df", "ein", "gros\u00b7ser", "Brunn", "da", "lau\u00b7ter", "Zu\u00b7cker", "quillt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "ADV", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mit welchen dis Gef\u00e4\u00df der Liebe angef\u00fcllt.", "tokens": ["Mit", "wel\u00b7chen", "dis", "Ge\u00b7f\u00e4\u00df", "der", "Lie\u00b7be", "an\u00b7ge\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PDS", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ja der wird truncken seyn den sie die Sinnen tr\u00e4ncket.", "tokens": ["Ja", "der", "wird", "trun\u00b7cken", "seyn", "den", "sie", "die", "Sin\u00b7nen", "tr\u00e4n\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "VAFIN", "ADJD", "VAINF", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}