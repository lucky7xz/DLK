{"textgrid.poem.47381": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Ausdruck der Empfindung", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Manches hab' ich wohl empfunden,", "tokens": ["Man\u00b7ches", "hab'", "ich", "wohl", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als es lebend vor mir stand,", "tokens": ["Als", "es", "le\u00b7bend", "vor", "mir", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch den rechten Sinn gefunden", "tokens": ["Doch", "den", "rech\u00b7ten", "Sinn", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Erst, als ich die Worte fand.", "tokens": ["Erst", ",", "als", "ich", "die", "Wor\u00b7te", "fand", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Darum auch ist Weltverkl\u00e4rung,", "tokens": ["Da\u00b7rum", "auch", "ist", "Welt\u00b7ver\u00b7kl\u00e4\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VAFIN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Poesie, dein Zauberstrahl,", "tokens": ["Poe\u00b7sie", ",", "dein", "Zau\u00b7ber\u00b7strahl", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Weil ich ohne dein' Erkl\u00e4rung", "tokens": ["Weil", "ich", "oh\u00b7ne", "dein'", "Er\u00b7kl\u00e4\u00b7rung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht mich selbst verst\u00e4nd' einmal.", "tokens": ["Nicht", "mich", "selbst", "ver\u00b7st\u00e4nd'", "ein\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "ADV", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}