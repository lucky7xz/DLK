{"textgrid.poem.54255": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ists m\u00f6glich, da\u00df ich noch kan Hand und Feder r\u00fchren?", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ists m\u00f6glich, da\u00df ich noch kan Hand und Feder r\u00fchren?", "tokens": ["Ists", "m\u00f6g\u00b7lich", ",", "da\u00df", "ich", "noch", "kan", "Hand", "und", "Fe\u00b7der", "r\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "ADV", "VMFIN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du schreibst, Philander, mir vielleicht nur in dem Traum,", "tokens": ["Du", "schreibst", ",", "Phi\u00b7lan\u00b7der", ",", "mir", "viel\u00b7leicht", "nur", "in", "dem", "Traum", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie? solt ich dich so schnell und unverhofft verliehren,", "tokens": ["Wie", "?", "solt", "ich", "dich", "so", "schnell", "und", "un\u00b7ver\u00b7hofft", "ver\u00b7lieh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VMFIN", "PPER", "PRF", "ADV", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach treibe doch mit mir nicht Schertz, ich glaub es kaum.", "tokens": ["Ach", "trei\u00b7be", "doch", "mit", "mir", "nicht", "Schertz", ",", "ich", "glaub", "es", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "APPR", "PPER", "PTKNEG", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jedoch was Traum und Schertz? dein \u00e4ngstliches Bezeigen", "tokens": ["Je\u00b7doch", "was", "Traum", "und", "Schertz", "?", "dein", "\u00e4ngst\u00b7li\u00b7ches", "Be\u00b7zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PWS", "NN", "KON", "NN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gibt, liebster Hertzens-Freund, mir leider! zu verstehn,", "tokens": ["Gibt", ",", "liebs\u00b7ter", "Hert\u00b7zens\u00b7Freund", ",", "mir", "lei\u00b7der", "!", "zu", "ver\u00b7stehn", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "PPER", "ADV", "$.", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df sich ein Zorn-Comet am Liebes-Himmel zeigen", "tokens": ["Da\u00df", "sich", "ein", "Zorn\u00b7Co\u00b7met", "am", "Lie\u00b7bes\u00b7Him\u00b7mel", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "APPRART", "NN", "VVINF"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.8": {"text": "Und meine Sonne will nun mehr zu r\u00fcste gehn.", "tokens": ["Und", "mei\u00b7ne", "Son\u00b7ne", "will", "nun", "mehr", "zu", "r\u00fcs\u00b7te", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "ADV", "ADV", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Kein grauses Beben kan das Erdreich so ersch\u00fcttern,", "tokens": ["Kein", "grau\u00b7ses", "Be\u00b7ben", "kan", "das", "Er\u00b7dreich", "so", "er\u00b7sch\u00fct\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Kein Keil bet\u00e4ubt uns so, so hart er immer klingt,", "tokens": ["Kein", "Keil", "be\u00b7t\u00e4ubt", "uns", "so", ",", "so", "hart", "er", "im\u00b7mer", "klingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "$,", "ADV", "ADJD", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Als meine Seele mu\u00df vor Furcht und Schrecken zittern,", "tokens": ["Als", "mei\u00b7ne", "See\u00b7le", "mu\u00df", "vor", "Furcht", "und", "Schre\u00b7cken", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VMFIN", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da deine Feder mir ein Lied zum Abschied singt.", "tokens": ["Da", "dei\u00b7ne", "Fe\u00b7der", "mir", "ein", "Lied", "zum", "Ab\u00b7schied", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Philander, deine Flucht und dein so j\u00e4hes Reisen", "tokens": ["Phi\u00b7lan\u00b7der", ",", "dei\u00b7ne", "Flucht", "und", "dein", "so", "j\u00e4\u00b7hes", "Rei\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Macht mir, ich schw\u00f6r es dir, nunmehr die Welt verhast;", "tokens": ["Macht", "mir", ",", "ich", "schw\u00f6r", "es", "dir", ",", "nun\u00b7mehr", "die", "Welt", "ver\u00b7hast", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "VVFIN", "PPER", "PPER", "$,", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Denn da mein Augen-Trost von mir entfernt soll heissen,", "tokens": ["Denn", "da", "mein", "Au\u00b7gen\u00b7Trost", "von", "mir", "ent\u00b7fernt", "soll", "heis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "APPR", "PPER", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So wird mir alles das, was um mich ist, zur Last.", "tokens": ["So", "wird", "mir", "al\u00b7les", "das", ",", "was", "um", "mich", "ist", ",", "zur", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "PDS", "$,", "PRELS", "APPR", "PPER", "VAFIN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Entmenschte Sylvia! du bist gantz ausser Sinnen,", "tokens": ["Ent\u00b7menschte", "Syl\u00b7via", "!", "du", "bist", "gantz", "aus\u00b7ser", "Sin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$.", "PPER", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Das Schrecken raubet dir Vernunfft und die Gestalt,", "tokens": ["Das", "Schre\u00b7cken", "rau\u00b7bet", "dir", "Ver\u00b7nunfft", "und", "die", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Du weist nicht was du thust noch was du solst beginnen,", "tokens": ["Du", "weist", "nicht", "was", "du", "thust", "noch", "was", "du", "solst", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWS", "PPER", "VVFIN", "ADV", "PWS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Dein Hertze wird dir welck, das Blut in Adern kalt.", "tokens": ["Dein", "Hert\u00b7ze", "wird", "dir", "welck", ",", "das", "Blut", "in", "A\u00b7dern", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VAFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wie z\u00e4rtlich hab ich dich, erweg es selbst, geliebet?", "tokens": ["Wie", "z\u00e4rt\u00b7lich", "hab", "ich", "dich", ",", "er\u00b7weg", "es", "selbst", ",", "ge\u00b7lie\u00b7bet", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "PRF", "$,", "VVFIN", "PPER", "ADV", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Was aber hab ich denn daf\u00fcr zu Lohn und Danck?", "tokens": ["Was", "a\u00b7ber", "hab", "ich", "denn", "da\u00b7f\u00fcr", "zu", "Lohn", "und", "Danck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "PPER", "ADV", "PAV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So viel, da\u00df mir dein Kiel den Scheide-Brief nun giebet,", "tokens": ["So", "viel", ",", "da\u00df", "mir", "dein", "Kiel", "den", "Schei\u00b7de\u00b7Brief", "nun", "gie\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "PPOSAT", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der mir im Lesen recht durch Marck und Seele drang.", "tokens": ["Der", "mir", "im", "Le\u00b7sen", "recht", "durch", "Marck", "und", "See\u00b7le", "drang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "ADJD", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Welch unbarmhertziges und widriges Geschicke", "tokens": ["Welch", "un\u00b7barm\u00b7hert\u00b7zi\u00b7ges", "und", "wid\u00b7ri\u00b7ges", "Ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Rufft dich, durch j\u00e4hen Winck, von mir in frembde Lufft?", "tokens": ["Rufft", "dich", ",", "durch", "j\u00e4\u00b7hen", "Win\u00b7ck", ",", "von", "mir", "in", "fremb\u00b7de", "Lufft", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.27": {"text": "Philander, folg ihm nicht, ach! bleibe doch zur\u00fccke,", "tokens": ["Phi\u00b7lan\u00b7der", ",", "folg", "ihm", "nicht", ",", "ach", "!", "blei\u00b7be", "doch", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PTKNEG", "$,", "ITJ", "$.", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Du st\u00fcrtzst mich, gehst du fort, wahrhafftig in die Grufft.", "tokens": ["Du", "st\u00fcrtz\u00b7st", "mich", ",", "gehst", "du", "fort", ",", "wahr\u00b7haff\u00b7tig", "in", "die", "Grufft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "ADJD", "APPR", "ART", "NN", "$."], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}, "line.29": {"text": "Was soll bey dem Verlust vor Trost mir \u00fcbrig bleiben?", "tokens": ["Was", "soll", "bey", "dem", "Ver\u00b7lust", "vor", "Trost", "mir", "\u00fcb\u00b7rig", "blei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPR", "ART", "NN", "APPR", "NN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Was vor ein Unterpfand wird mir wohl zugestellt?", "tokens": ["Was", "vor", "ein", "Un\u00b7ter\u00b7pfand", "wird", "mir", "wohl", "zu\u00b7ge\u00b7stellt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Wer will als B\u00fcrge sich, Geliebter, unterschreiben,", "tokens": ["Wer", "will", "als", "B\u00fcr\u00b7ge", "sich", ",", "Ge\u00b7lieb\u00b7ter", ",", "un\u00b7ter\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "VMFIN", "KOUS", "NN", "PRF", "$,", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da\u00df deine Sylvia ihr Recht an dir beh\u00e4lt?", "tokens": ["Da\u00df", "dei\u00b7ne", "Syl\u00b7via", "ihr", "Recht", "an", "dir", "be\u00b7h\u00e4lt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NE", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "Zwar will dein Abschieds-Brief mir viel Versichrung geben,", "tokens": ["Zwar", "will", "dein", "Ab\u00b7schieds\u00b7Brief", "mir", "viel", "Ver\u00b7sich\u00b7rung", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Du schw\u00f6rst mir heilig zu, dein Auge w\u00fcrde nicht", "tokens": ["Du", "schw\u00f6rst", "mir", "hei\u00b7lig", "zu", ",", "dein", "Au\u00b7ge", "w\u00fcr\u00b7de", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "PPOSAT", "NN", "VAFIN", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "An einen frembden Strahl und Antlitz bleiben kleben;", "tokens": ["An", "ei\u00b7nen", "fremb\u00b7den", "Strahl", "und", "Ant\u00b7litz", "blei\u00b7ben", "kle\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Allein behertzge wohl, was deine Feder spricht.", "tokens": ["Al\u00b7lein", "be\u00b7hertz\u00b7ge", "wohl", ",", "was", "dei\u00b7ne", "Fe\u00b7der", "spricht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Zeit und Entfernung wischt ein Bild leicht aus den Hertzen,", "tokens": ["Zeit", "und", "Ent\u00b7fer\u00b7nung", "wischt", "ein", "Bild", "leicht", "aus", "den", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.38": {"text": "Und w\u00e4r auch selbiges mit Blut gleich eingepr\u00e4gt.", "tokens": ["Und", "w\u00e4r", "auch", "sel\u00b7bi\u00b7ges", "mit", "Blut", "gleich", "ein\u00b7ge\u00b7pr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ach! dieser Kummer macht mir hundert tausend Schmertzen,", "tokens": ["Ach", "!", "die\u00b7ser", "Kum\u00b7mer", "macht", "mir", "hun\u00b7dert", "tau\u00b7send", "Schmert\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PDAT", "NN", "VVFIN", "PPER", "CARD", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Mit dem sich Sylvia nunmehr best\u00e4ndig tr\u00e4gt.", "tokens": ["Mit", "dem", "sich", "Syl\u00b7via", "nun\u00b7mehr", "be\u00b7st\u00e4n\u00b7dig", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "NE", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.41": {"text": "Das m\u00e4nnliche Geschlecht, wie die Erfahrung lehret,", "tokens": ["Das", "m\u00e4nn\u00b7li\u00b7che", "Ge\u00b7schlecht", ",", "wie", "die", "Er\u00b7fah\u00b7rung", "leh\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Ist leider allzusehr dem Wechsel zugethan.", "tokens": ["Ist", "lei\u00b7der", "all\u00b7zu\u00b7sehr", "dem", "Wech\u00b7sel", "zu\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Wie leichte wird ihr Hertz durch frembden Blick beth\u00f6ret?", "tokens": ["Wie", "leich\u00b7te", "wird", "ihr", "Hertz", "durch", "fremb\u00b7den", "Blick", "be\u00b7th\u00f6\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "VAFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Wie balde z\u00fcndet es ein Strahl von neuen an?", "tokens": ["Wie", "bal\u00b7de", "z\u00fcn\u00b7det", "es", "ein", "Strahl", "von", "neu\u00b7en", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "w