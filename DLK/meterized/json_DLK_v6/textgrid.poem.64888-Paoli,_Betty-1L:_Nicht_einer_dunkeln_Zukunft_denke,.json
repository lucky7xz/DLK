{"textgrid.poem.64888": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht einer dunkeln Zukunft denke,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht einer dunkeln Zukunft denke,", "tokens": ["Nicht", "ei\u00b7ner", "dun\u00b7keln", "Zu\u00b7kunft", "den\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn dir die jetz'ge Stunde lacht;", "tokens": ["Wenn", "dir", "die", "jetz'\u00b7ge", "Stun\u00b7de", "lacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Genie\u00dfe sorglos die Geschenke,", "tokens": ["Ge\u00b7nie\u00b7\u00dfe", "sorg\u00b7los", "die", "Ge\u00b7schen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die dir die Gegenwart gebracht.", "tokens": ["Die", "dir", "die", "Ge\u00b7gen\u00b7wart", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Morgen ist ein b\u00f6ser Riese,", "tokens": ["Das", "Mor\u00b7gen", "ist", "ein", "b\u00f6\u00b7ser", "Rie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der drohend seinen Bogen spannt", "tokens": ["Der", "dro\u00b7hend", "sei\u00b7nen", "Bo\u00b7gen", "spannt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und der in seine Nachtverlie\u00dfe", "tokens": ["Und", "der", "in", "sei\u00b7ne", "Nacht\u00b7ver\u00b7lie\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gar gern die heitre Freude bannt.", "tokens": ["Gar", "gern", "die", "heit\u00b7re", "Freu\u00b7de", "bannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wir wollen trotzen seinem W\u00fcthen!", "tokens": ["Wir", "wol\u00b7len", "trot\u00b7zen", "sei\u00b7nem", "W\u00fct\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die jetz'ge Stunde sei so sch\u00f6n,", "tokens": ["Die", "jetz'\u00b7ge", "Stun\u00b7de", "sei", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df als Erinn'rung ihre Bl\u00fcthen", "tokens": ["Da\u00df", "als", "Er\u00b7inn'\u00b7rung", "ih\u00b7re", "Bl\u00fc\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In's Morgen noch hin\u00fcberwehn!", "tokens": ["In's", "Mor\u00b7gen", "noch", "hin\u00b7\u00fc\u00b7berw\u00b7ehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}