{"textgrid.poem.60315": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Klostergarten steht ein steinern Bild,", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Klostergarten steht ein steinern Bild,", "tokens": ["Im", "Klos\u00b7ter\u00b7gar\u00b7ten", "steht", "ein", "stei\u00b7nern", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Kruzifix so ernst, vers\u00f6hnungsmild.", "tokens": ["Ein", "Kru\u00b7zi\u00b7fix", "so", "ernst", ",", "ver\u00b7s\u00f6h\u00b7nungs\u00b7mild", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Oft in der Nacht, der ungest\u00f6rten, sp\u00e4ten,", "tokens": ["Oft", "in", "der", "Nacht", ",", "der", "un\u00b7ge\u00b7st\u00f6r\u00b7ten", ",", "sp\u00e4\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Geht Schwester Heloise hin, zu beten.", "tokens": ["Geht", "Schwes\u00b7ter", "He\u00b7loi\u00b7se", "hin", ",", "zu", "be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "NN", "PTKVZ", "$,", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Auch heute kniet sie dort am Marmorstamme", "tokens": ["Auch", "heu\u00b7te", "kniet", "sie", "dort", "am", "Mar\u00b7mors\u00b7tam\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und fleht um K\u00fchlung ihrer Herzensflamme:", "tokens": ["Und", "fleht", "um", "K\u00fch\u00b7lung", "ih\u00b7rer", "Her\u00b7zens\u00b7flam\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "\u00bbo Gott! nachdem du hast f\u00fcr uns gelitten,", "tokens": ["\u00bb", "o", "Gott", "!", "nach\u00b7dem", "du", "hast", "f\u00fcr", "uns", "ge\u00b7lit\u00b7ten", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$.", "KOUS", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Geklagt, geweint, empfangen Todeswunden,", "tokens": ["Ge\u00b7klagt", ",", "ge\u00b7weint", ",", "emp\u00b7fan\u00b7gen", "To\u00b7des\u00b7wun\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wird ungl\u00fcckliche Liebe noch gefunden?", "tokens": ["Wird", "un\u00b7gl\u00fcck\u00b7li\u00b7che", "Lie\u00b7be", "noch", "ge\u00b7fun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Hat sie nicht ausgeweint und ausgestritten?", "tokens": ["Hat", "sie", "nicht", "aus\u00b7ge\u00b7weint", "und", "aus\u00b7ge\u00b7strit\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Hilf! rette mich aus diesen Finsternissen", "tokens": ["Hilf", "!", "ret\u00b7te", "mich", "aus", "die\u00b7sen", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "VVFIN", "PRF", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Der Zweifel, die mein blutend Herz umnachten!", "tokens": ["Der", "Zwei\u00b7fel", ",", "die", "mein", "blu\u00b7tend", "Herz", "um\u00b7nach\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Nach Ihm, nach Ihm nur mu\u00df ich ewig schmachten,", "tokens": ["Nach", "Ihm", ",", "nach", "Ihm", "nur", "mu\u00df", "ich", "e\u00b7wig", "schmach\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "ADV", "VMFIN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "O Gott! hier liegt mein Herz vor dir zerrissen!", "tokens": ["O", "Gott", "!", "hier", "liegt", "mein", "Herz", "vor", "dir", "zer\u00b7ris\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Umsonst, da\u00df ich empfing den frommen Schleier,", "tokens": ["Um\u00b7sonst", ",", "da\u00df", "ich", "emp\u00b7fing", "den", "from\u00b7men", "Schlei\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Da\u00df ich zum strengen Orden mich bekannte,", "tokens": ["Da\u00df", "ich", "zum", "stren\u00b7gen", "Or\u00b7den", "mich", "be\u00b7kann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Noch immer seh ich meinen s\u00fc\u00dfen Freier,", "tokens": ["Noch", "im\u00b7mer", "seh", "ich", "mei\u00b7nen", "s\u00fc\u00b7\u00dfen", "Frei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Wie er beim letzten Lebewohl sich wandte.", "tokens": ["Wie", "er", "beim", "letz\u00b7ten", "Le\u00b7be\u00b7wohl", "sich", "wand\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Du selbst hast ihn zum Gatten mir erkoren;", "tokens": ["Du", "selbst", "hast", "ihn", "zum", "Gat\u00b7ten", "mir", "er\u00b7ko\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Oft, wenn ich Wort' und K\u00fcsse mit ihm tauschte,", "tokens": ["Oft", ",", "wenn", "ich", "Wort'", "und", "K\u00fcs\u00b7se", "mit", "ihm", "tauschte", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "NN", "KON", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "War mir, ob Himmelsbeifall uns umrauschte;", "tokens": ["War", "mir", ",", "ob", "Him\u00b7mels\u00b7bei\u00b7fall", "uns", "um\u00b7rauschte", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "KOUS", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Kannst du mich tr\u00f6sten, da\u00df ich ihn verloren?", "tokens": ["Kannst", "du", "mich", "tr\u00f6s\u00b7ten", ",", "da\u00df", "ich", "ihn", "ver\u00b7lo\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "$,", "KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Du kannst es nicht, mu\u00df zitternd ich bekennen,", "tokens": ["Du", "kannst", "es", "nicht", ",", "mu\u00df", "zit\u00b7ternd", "ich", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "$,", "VMFIN", "VVPP", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Ich sterbe hin in meiner Leidenschaft,", "tokens": ["Ich", "ster\u00b7be", "hin", "in", "mei\u00b7ner", "Lei\u00b7den\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Es mu\u00df mein Herz mit seiner letzten Kraft,", "tokens": ["Es", "mu\u00df", "mein", "Herz", "mit", "sei\u00b7ner", "letz\u00b7ten", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Dir abgewandt, in dieser Glut verbrennen.", "tokens": ["Dir", "ab\u00b7ge\u00b7wandt", ",", "in", "die\u00b7ser", "Glut", "ver\u00b7bren\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Und wenn ich das Verlerne und Vers\u00e4umte,", "tokens": ["Und", "wenn", "ich", "das", "Ver\u00b7ler\u00b7ne", "und", "Ver\u00b7s\u00e4um\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Als ", "tokens": ["Als"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.29": {"text": "Vergib, mein Gott! da\u00df ich in meinen Schrecken,", "tokens": ["Ver\u00b7gib", ",", "mein", "Gott", "!", "da\u00df", "ich", "in", "mei\u00b7nen", "Schre\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PPOSAT", "NN", "$.", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Wenn kalt die Schwestern mich zur Hora wecken,", "tokens": ["Wenn", "kalt", "die", "Schwes\u00b7tern", "mich", "zur", "Ho\u00b7ra", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Nach Truggestalten strecke meine H\u00e4nde,", "tokens": ["Nach", "Trug\u00b7ge\u00b7stal\u00b7ten", "stre\u00b7cke", "mei\u00b7ne", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Verg\u00f6tternd mich zu meinen Tr\u00e4umen wende.", "tokens": ["Ver\u00b7g\u00f6t\u00b7ternd", "mich", "zu", "mei\u00b7nen", "Tr\u00e4u\u00b7men", "wen\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Verzeih, wenn ich oft knieend am Altare", "tokens": ["Ver\u00b7zeih", ",", "wenn", "ich", "oft", "kni\u00b7e\u00b7end", "am", "Al\u00b7ta\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "VVPP", "APPRART", "NN"], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.34": {"text": "Zu knieen mein' an meiner Freudenbahre,", "tokens": ["Zu", "kni\u00b7e\u00b7en", "mein'", "an", "mei\u00b7ner", "Freu\u00b7den\u00b7bah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPOSAT", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.35": {"text": "Und da\u00df in mir verlornes Muttergl\u00fcck", "tokens": ["Und", "da\u00df", "in", "mir", "ver\u00b7lor\u00b7nes", "Mut\u00b7ter\u00b7gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Aufschreit: gib mir den Br\u00e4utigam zur\u00fcck!", "tokens": ["Auf\u00b7schreit", ":", "gib", "mir", "den", "Br\u00e4u\u00b7ti\u00b7gam", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVIMP", "PPER", "ART", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Im Mondlicht seh ich hier dein Antlitz schimmern,", "tokens": ["Im", "Mond\u00b7licht", "seh", "ich", "hier", "dein", "Ant\u00b7litz", "schim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Die Winde seufzen durch den Bl\u00fctenstrauch;", "tokens": ["Die", "Win\u00b7de", "seuf\u00b7zen", "durch", "den", "Bl\u00fc\u00b7ten\u00b7strauch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Ich kam zu beten, doch im Windeshauch", "tokens": ["Ich", "kam", "zu", "be\u00b7ten", ",", "doch", "im", "Win\u00b7des\u00b7hauch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKZU", "VVINF", "$,", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "H\u00f6r ich mein unempfangnes Kindlein wimmern.", "tokens": ["H\u00f6r", "ich", "mein", "un\u00b7em\u00b7pfang\u00b7nes", "Kin\u00b7dlein", "wim\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Ich bin so arm, verlassen und beraubt,", "tokens": ["Ich", "bin", "so", "arm", ",", "ver\u00b7las\u00b7sen", "und", "be\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Nichts kann ich mehr zum Opfer und Geschenke", "tokens": ["Nichts", "kann", "ich", "mehr", "zum", "Op\u00b7fer", "und", "Ge\u00b7schen\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "APPRART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Dir bringen, Gott! als da\u00df mein m\u00fcdes Haupt", "tokens": ["Dir", "brin\u00b7gen", ",", "Gott", "!", "als", "da\u00df", "mein", "m\u00fc\u00b7des", "Haupt"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "$,", "NN", "$.", "KOKOM", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Ich hier zu deinem heilgen Kreuze senke,", "tokens": ["Ich", "hier", "zu", "dei\u00b7nem", "heil\u00b7gen", "Kreu\u00b7ze", "sen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Da\u00df ich die Wange k\u00fchl' an deinem Steine,", "tokens": ["Da\u00df", "ich", "die", "Wan\u00b7ge", "k\u00fchl'", "an", "dei\u00b7nem", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Wenn ich die Nacht um Ab\u00e4lard verweine.\u00ab", "tokens": ["Wenn", "ich", "die", "Nacht", "um", "A\u00b7b\u00e4\u00b7lard", "ver\u00b7wei\u00b7ne", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NE", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}