{"textgrid.poem.23950": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Dem Tage", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Breit h\u00e4ngt vom Himmel die Fahne der Freude,", "tokens": ["Breit", "h\u00e4ngt", "vom", "Him\u00b7mel", "die", "Fah\u00b7ne", "der", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dunkelblau, unbewegt, sonnendurchprunkt;", "tokens": ["Dun\u00b7kel\u00b7blau", ",", "un\u00b7be\u00b7wegt", ",", "son\u00b7nen\u00b7durch\u00b7prunkt", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "VVFIN", "$."], "meter": "+-+---+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Hurra, die Herzen hoch, hurra dem Heute,", "tokens": ["Hur\u00b7ra", ",", "die", "Her\u00b7zen", "hoch", ",", "hur\u00b7ra", "dem", "Heu\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "ADJD", "$,", "NE", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Was auch das m\u00fcrrische Morgen uns unkt.", "tokens": ["Was", "auch", "das", "m\u00fcr\u00b7ri\u00b7sche", "Mor\u00b7gen", "uns", "unkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "PPER", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Morgen der Tod, aber heute das Leben,", "tokens": ["Mor\u00b7gen", "der", "Tod", ",", "a\u00b7ber", "heu\u00b7te", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Leben und Liebe zu allem, das bl\u00fcht;", "tokens": ["Le\u00b7ben", "und", "Lie\u00b7be", "zu", "al\u00b7lem", ",", "das", "bl\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PIS", "$,", "PDS", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "La\u00dft uns die Herzen zur Sonne erheben,", "tokens": ["La\u00dft", "uns", "die", "Her\u00b7zen", "zur", "Son\u00b7ne", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die wie ein Heilandsherz g\u00fctevoll gl\u00fcht.", "tokens": ["Die", "wie", "ein", "Hei\u00b7lands\u00b7herz", "g\u00fc\u00b7te\u00b7voll", "gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "Schielt Tante Mors mit der silbernen Glatze", "tokens": ["Schielt", "Tan\u00b7te", "Mors", "mit", "der", "sil\u00b7ber\u00b7nen", "Glat\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "NE", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Heute zur Nacht wieder \u00fcber die Welt,", "tokens": ["Heu\u00b7te", "zur", "Nacht", "wie\u00b7der", "\u00fc\u00b7ber", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Lachen wir ihr in die bleichkalte Fratze,", "tokens": ["La\u00b7chen", "wir", "ihr", "in", "die", "bleich\u00b7kal\u00b7te", "Frat\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--++-++-+-", "measure": "dactylic.init"}, "line.4": {"text": "Denen das Herz G\u00f6ttin Sonne erhellt.", "tokens": ["De\u00b7nen", "das", "Herz", "G\u00f6t\u00b7tin", "Son\u00b7ne", "er\u00b7hellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "NN", "NN", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}