{"textgrid.poem.31518": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Jetzt l\u00e4sst die Gottheits-Sonn ein Wesens-Strahle fallen.", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzt l\u00e4sst die Gottheits-Sonn ein Wesens-Strahle fallen.", "tokens": ["Jetzt", "l\u00e4sst", "die", "Gott\u00b7heits\u00b7Sonn", "ein", "We\u00b7sens\u00b7Strah\u00b7le", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jetzt schickt's ein Donner-Wort aus ihren Himmel-Mund.", "tokens": ["Jetzt", "schickt's", "ein", "Don\u00b7ner\u00b7Wort", "aus", "ih\u00b7ren", "Him\u00b7mel\u00b7Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jetzt macht sie / da\u00df sie sey das Wort und Warheit / kund.", "tokens": ["Jetzt", "macht", "sie", "/", "da\u00df", "sie", "sey", "das", "Wort", "und", "War\u00b7heit", "/", "kund", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PPER", "VAFIN", "ART", "NN", "KON", "NN", "$(", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie l\u00e4sst den Engel-Thon / das klar Bek\u00e4ntnu\u00df / schallen.", "tokens": ["Sie", "l\u00e4sst", "den", "En\u00b7gel\u00b7Thon", "/", "das", "klar", "Be\u00b7k\u00e4nt\u00b7nu\u00df", "/", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "ART", "ADJD", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich bins: der Ewig Gott / der alles ist in allen;", "tokens": ["Ich", "bins", ":", "der", "E\u00b7wig", "Gott", "/", "der", "al\u00b7les", "ist", "in", "al\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$.", "ART", "ADJA", "NN", "$(", "ART", "PIS", "VAFIN", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "der Schrifft und Bilder Ziel; des Heils und Segens Grund /", "tokens": ["der", "Schrifft", "und", "Bil\u00b7der", "Ziel", ";", "des", "Heils", "und", "Se\u00b7gens", "Grund", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NN", "$.", "ART", "NN", "KON", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "auf dem das ganz Geb\u00e4u der Welt-Erl\u00f6sung stund'", "tokens": ["auf", "dem", "das", "ganz", "Ge\u00b7b\u00e4u", "der", "Welt\u00b7Er\u00b7l\u00f6\u00b7sung", "stund'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "ADV", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "wie k\u00f6nte doch die Sonn der Warheit heller strahlen?", "tokens": ["wie", "k\u00f6n\u00b7te", "doch", "die", "Sonn", "der", "War\u00b7heit", "hel\u00b7ler", "strah\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "ART", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "die Warheit / die sich Gott vor Menschen hie bekennt /", "tokens": ["die", "War\u00b7heit", "/", "die", "sich", "Gott", "vor", "Men\u00b7schen", "hie", "be\u00b7kennt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PRF", "NN", "APPR", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "wird dort die Menschen vor der Gottheit auch bekennen.", "tokens": ["wird", "dort", "die", "Men\u00b7schen", "vor", "der", "Got\u00b7theit", "auch", "be\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ach la\u00df' auch vor der Welt dein Dienerin mich nennen /", "tokens": ["Ach", "la\u00df'", "auch", "vor", "der", "Welt", "dein", "Die\u00b7ne\u00b7rin", "mich", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "APPR", "ART", "NN", "PPOSAT", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "beherzt in Noht und Tod / da\u00df nichts von dir mich trennt.", "tokens": ["be\u00b7herzt", "in", "Noht", "und", "Tod", "/", "da\u00df", "nichts", "von", "dir", "mich", "trennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$(", "KOUS", "PIS", "APPR", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Gib da\u00df ich keck bekenn / vor aller Menschen Macht /", "tokens": ["Gib", "da\u00df", "ich", "keck", "be\u00b7kenn", "/", "vor", "al\u00b7ler", "Men\u00b7schen", "Macht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KOUS", "PPER", "ADJD", "VVFIN", "$(", "APPR", "PIAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "da\u00df Glaub' in dein Verdienst allein uns Seelig macht.", "tokens": ["da\u00df", "Glaub'", "in", "dein", "Ver\u00b7dienst", "al\u00b7lein", "uns", "See\u00b7lig", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN", "ADV", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}