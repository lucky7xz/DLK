un\u00b7ten", "Straus", "oh\u00b7ne", "da\u00df", "das", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "APPR", "KOUS", "ART", "NN"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.131": {"text": "bedenket", "tokens": ["be\u00b7den\u00b7ket"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.132": {"text": "Was wir dem noch schuldig sein, der uns diese Lust", "tokens": ["Was", "wir", "dem", "noch", "schul\u00b7dig", "sein", ",", "der", "uns", "die\u00b7se", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ART", "ADV", "ADJD", "VAINF", "$,", "PRELS", "PPER", "PDAT", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.133": {"text": "geschenket.", "tokens": ["ge\u00b7schen\u00b7ket", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.134": {"text": "Als ich dieses \u00fcberdacht und der Nelken Schmuk er-", "tokens": ["Als", "ich", "die\u00b7ses", "\u00fc\u00b7berd\u00b7acht", "und", "der", "Nel\u00b7ken", "Schmuk", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "KON", "ART", "NN", "NE", "TRUNC"], "meter": "+-+----+-+-+-", "measure": "unknown.measure.penta"}, "line.135": {"text": "wegt,", "tokens": ["wegt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+", "measure": "single.up"}, "line.136": {"text": "Ward des Herzens Andachts Trieb durch die Augen-", "tokens": ["Ward", "des", "Her\u00b7zens", "An\u00b7dachts", "Trieb", "durch", "die", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN", "APPR", "ART", "TRUNC"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.137": {"text": "lust erregt;", "tokens": ["lust", "er\u00b7regt", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.138": {"text": "Darum lies ich alsobald, zu des weisen Sch\u00f6pfers", "tokens": ["Da\u00b7rum", "lies", "ich", "al\u00b7so\u00b7bald", ",", "zu", "des", "wei\u00b7sen", "Sch\u00f6p\u00b7fers"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.139": {"text": "Ehren,", "tokens": ["Eh\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.140": {"text": "F\u00fcr die sch\u00f6ne Nelkenflor dieses frohe Danklied h\u00f6-", "tokens": ["F\u00fcr", "die", "sch\u00f6\u00b7ne", "Nel\u00b7ken\u00b7flor", "die\u00b7ses", "fro\u00b7he", "Dank\u00b7lied", "h\u00f6"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PDAT", "ADJA", "NN", "TRUNC"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.141": {"text": "ren:", "tokens": ["ren", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}}}}}