{"dta.poem.18158": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die sch\u00f6ne Nelkenflor.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als ich j\u00fcngstens bei dem Schein einer war-", "tokens": ["Als", "ich", "j\u00fcng\u00b7stens", "bei", "dem", "Schein", "ei\u00b7ner", "wa\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "ART", "TRUNC"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "men Fr\u00fchlings Zeit,", "tokens": ["men", "Fr\u00fch\u00b7lings", "Zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mich vollkommen frei gemacht von des", "tokens": ["Mich", "voll\u00b7kom\u00b7men", "frei", "ge\u00b7macht", "von", "des"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "ADJD", "VVPP", "APPR", "ART"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Amtes Tag Arbeit,", "tokens": ["Am\u00b7tes", "Tag", "Ar\u00b7beit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ging ich aus zu einen Freund, meinen", "tokens": ["Ging", "ich", "aus", "zu", "ei\u00b7nen", "Freund", ",", "mei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "ART", "NN", "$,", "PPOSAT"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Sch\u00f6pfer in dem Gr\u00fcnen,", "tokens": ["Sch\u00f6p\u00b7fer", "in", "dem", "Gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Durch and\u00e4chtiges Beschaun holder Blumen; noch", "tokens": ["Durch", "an\u00b7d\u00e4ch\u00b7ti\u00b7ges", "Be\u00b7schaun", "hol\u00b7der", "Blu\u00b7men", ";", "noch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$.", "ADV"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "zu dienen.", "tokens": ["zu", "die\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Ich fand eine Nelkenflor, deren sch\u00f6n geschm\u00fckte", "tokens": ["Ich", "fand", "ei\u00b7ne", "Nel\u00b7ken\u00b7flor", ",", "de\u00b7ren", "sch\u00f6n", "ge\u00b7schm\u00fck\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELAT", "ADJD", "VVFIN"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Pracht,", "tokens": ["Pracht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Damahls in den Blumen Reich erst zu unsrer Lust", "tokens": ["Da\u00b7mahls", "in", "den", "Blu\u00b7men", "Reich", "erst", "zu", "uns\u00b7rer", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "erwacht.", "tokens": ["er\u00b7wacht", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "Es gefiel mir alles woll, ihre Ordnung wie sie stun-", "tokens": ["Es", "ge\u00b7fiel", "mir", "al\u00b7les", "woll", ",", "ih\u00b7re", "Ord\u00b7nung", "wie", "sie", "stun"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "ADV", "$,", "PPOSAT", "NN", "KOKOM", "PPER", "TRUNC"], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "den.", "tokens": ["den", "."], "token_info": ["word", "punct"], "pos": ["ART", "$."], "meter": "-", "measure": "single.down"}, "line.15": {"text": "In dem T\u00f6pfen aufgesetzt, in der Erd am Stok ge-", "tokens": ["In", "dem", "T\u00f6p\u00b7fen", "auf\u00b7ge\u00b7setzt", ",", "in", "der", "Erd", "am", "Stok", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "APPR", "ART", "NN", "APPRART", "NN", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "bunden.", "tokens": ["bun\u00b7den", "."], "token_info": ["word", "punct"], "pos": ["ADJA", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Ich gab auf die Farben acht, deren liebliches Ge-", "tokens": ["Ich", "gab", "auf", "die", "Far\u00b7ben", "acht", ",", "de\u00b7ren", "lieb\u00b7li\u00b7ches", "Ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "CARD", "$,", "PRELAT", "ADJA", "TRUNC"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "spiel,", "tokens": ["spiel", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.19": {"text": "Mir noch besser als die Ordnung in die frohen Sin-", "tokens": ["Mir", "noch", "bes\u00b7ser", "als", "die", "Ord\u00b7nung", "in", "die", "fro\u00b7hen", "Sin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJD", "KOKOM", "ART", "NN", "APPR", "ART", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.20": {"text": "nen fiel,", "tokens": ["nen", "fiel", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.21": {"text": "Und gedachte gleich dabei, was der Sch\u00f6pfer einge-", "tokens": ["Und", "ge\u00b7dach\u00b7te", "gleich", "da\u00b7bei", ",", "was", "der", "Sch\u00f6p\u00b7fer", "ein\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PAV", "$,", "PRELS", "ART", "NN", "TRUNC"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "richtet,", "tokens": ["rich\u00b7tet", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Uebertrift doch tausendmahl da\u00df, was unser Witz er-", "tokens": ["Ue\u00b7bert\u00b7rift", "doch", "tau\u00b7send\u00b7mahl", "da\u00df", ",", "was", "un\u00b7ser", "Witz", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "KOUS", "$,", "PRELS", "PPOSAT", "NN", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "dichtet.", "tokens": ["dich\u00b7tet", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.25": {"text": "Mein Freund hatte jeden Stok, der die Nelke hielt,", "tokens": ["Mein", "Freund", "hat\u00b7te", "je\u00b7den", "Stok", ",", "der", "die", "Nel\u00b7ke", "hielt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "vermahlt,", "tokens": ["ver\u00b7mahlt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.27": {"text": "Und die Farben so gemischt, wie sie an den Blumen", "tokens": ["Und", "die", "Far\u00b7ben", "so", "ge\u00b7mischt", ",", "wie", "sie", "an", "den", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "VVPP", "$,", "PWAV", "PPER", "APPR", "ART", "NN"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "strahlt;", "tokens": ["strahlt", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.29": {"text": "Es war hie Natur und Kunst: Aber diese muste weichen,", "tokens": ["Es", "war", "hie", "Na\u00b7tur", "und", "Kunst", ":", "A\u00b7ber", "die\u00b7se", "mus\u00b7te", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "KON", "NN", "$.", "KON", "PDS", "VMFIN", "VVINF", "$,"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "Und gleich wie ein Schattenschein, vor des Urbilds", "tokens": ["Und", "gleich", "wie", "ein", "Schat\u00b7ten\u00b7schein", ",", "vor", "des", "Ur\u00b7bilds"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "KOKOM", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+--+-++-+-", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Licht erbleichen", "tokens": ["Licht", "er\u00b7blei\u00b7chen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.32": {"text": "An den Nelken sah ich nichts, als ein sch\u00f6nes Roth", "tokens": ["An", "den", "Nel\u00b7ken", "sah", "ich", "nichts", ",", "als", "ein", "sch\u00f6\u00b7nes", "Roth"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PIS", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.33": {"text": "und Weis,", "tokens": ["und", "Weis", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.34": {"text": "Da\u00df die spielende Natur, durch, den uns verborg-", "tokens": ["Da\u00df", "die", "spie\u00b7len\u00b7de", "Na\u00b7tur", ",", "durch", ",", "den", "uns", "ver\u00b7bor\u00b7g"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "APPR", "$,", "PRELS", "PPER", "TRUNC"], "meter": "+-+---+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.35": {"text": "nen Fleis,", "tokens": ["nen", "Fleis", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.36": {"text": "Wunderbarlich durchgesprengt, so gestrichelt da\u00df die", "tokens": ["Wun\u00b7der\u00b7bar\u00b7lich", "durch\u00b7ge\u00b7sprengt", ",", "so", "ge\u00b7stri\u00b7chelt", "da\u00df", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "ADV", "VVPP", "KOUS", "ART"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.37": {"text": "Farben,", "tokens": ["Far\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.38": {"text": "In gemischter Aenderung mancherlei Gestalt erwar-", "tokens": ["In", "ge\u00b7mischter", "A\u00b7en\u00b7de\u00b7rung", "man\u00b7cher\u00b7lei", "Ge\u00b7stalt", "er\u00b7wa\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PIAT", "NN", "TRUNC"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.39": {"text": "ben.", "tokens": ["ben", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-", "measure": "single.down"}, "line.40": {"text": "Diese schien wie Milch und Blut das geronnen", "tokens": ["Die\u00b7se", "schien", "wie", "Milch", "und", "Blut", "das", "ge\u00b7ron\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KOKOM", "NN", "KON", "NN", "ART", "ADJA"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.41": {"text": "sich vermischt,", "tokens": ["sich", "ver\u00b7mischt", ","], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.42": {"text": "Jene als wenn nur ein Tropf auf ein weisses Tuch", "tokens": ["Je\u00b7ne", "als", "wenn", "nur", "ein", "Tropf", "auf", "ein", "weis\u00b7ses", "Tuch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "KOKOM", "KOUS", "ADV", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-----+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.43": {"text": "gewischt,", "tokens": ["ge\u00b7wischt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.44": {"text": "Der durch den erhobnen Glanz, als wen er dar-", "tokens": ["Der", "durch", "den", "er\u00b7hob\u00b7nen", "Glanz", ",", "als", "wen", "er", "dar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "$,", "KOUS", "PWS", "PPER", "TRUNC"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.45": {"text": "auf gesprenget", "tokens": ["auf", "ge\u00b7spren\u00b7get"], "token_info": ["word", "word"], "pos": ["APPR", "NE"], "meter": "+-+-", "measure": "trochaic.di"}, "line.46": {"text": "An dem eingekerbten Blat, wie ein fliessend Tr\u00f6pf-", "tokens": ["An", "dem", "ein\u00b7ge\u00b7kerb\u00b7ten", "Blat", ",", "wie", "ein", "flies\u00b7send", "Tr\u00f6pf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PWAV", "ART", "ADJD", "TRUNC"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.47": {"text": "gen henget.", "tokens": ["gen", "hen\u00b7get", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.48": {"text": "Diese glom in Purpur Tracht; jene schien als wie", "tokens": ["Die\u00b7se", "glom", "in", "Pur\u00b7pur", "Tracht", ";", "je\u00b7ne", "schien", "als", "wie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "NN", "NN", "$.", "PDS", "VVFIN", "KOUS", "KOKOM"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.49": {"text": "ein Gold", "tokens": ["ein", "Gold"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.50": {"text": "Das auf einen runden Kneul, wie in Schichten", "tokens": ["Das", "auf", "ei\u00b7nen", "run\u00b7den", "Kneul", ",", "wie", "in", "Schich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "$,", "PWAV", "APPR", "NN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.51": {"text": "aufgerollt.", "tokens": ["auf\u00b7ge\u00b7rollt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.52": {"text": "Wen man sie von ferne sah; Eine andre sah ich", "tokens": ["Wen", "man", "sie", "von", "fer\u00b7ne", "sah", ";", "Ei\u00b7ne", "and\u00b7re", "sah", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PIS", "PPER", "APPR", "ADV", "VVFIN", "$.", "ART", "PIS", "VVFIN", "PPER"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.53": {"text": "blizzen", "tokens": ["bliz\u00b7zen"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.54": {"text": "Die mir in die Augen schien, als gebrochne Silber-", "tokens": ["Die", "mir", "in", "die", "Au\u00b7gen", "schien", ",", "als", "ge\u00b7broch\u00b7ne", "Sil\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN", "$,", "KOUS", "ADJA", "TRUNC"], "meter": "----+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.55": {"text": "spizzen.", "tokens": ["spiz\u00b7zen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.56": {"text": "Diese war ein Violet das get\u00fcpfelt, wie gedr\u00fckt;", "tokens": ["Die\u00b7se", "war", "ein", "Vi\u00b7o\u00b7let", "das", "ge\u00b7t\u00fcp\u00b7felt", ",", "wie", "ge\u00b7dr\u00fckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "VVPP", "$,", "PWAV", "VVPP", "$."], "meter": "----+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.57": {"text": "Jene war als wie ein Flor, der aus gelblicht Garn", "tokens": ["Je\u00b7ne", "war", "als", "wie", "ein", "Flor", ",", "der", "aus", "gelb\u00b7licht", "Garn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "KOUS", "KOKOM", "ART", "NN", "$,", "PRELS", "APPR", "NN", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.58": {"text": "gestrikt:", "tokens": ["ge\u00b7strikt", ":"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.59": {"text": "Andre waren wiederum durch so manchen Schein", "tokens": ["A