{"textgrid.poem.52995": {"metadata": {"author": {"name": "Seume, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich jauchze dir, ich jauchze dir entgegen,", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich jauchze dir, ich jauchze dir entgegen,", "tokens": ["Ich", "jauch\u00b7ze", "dir", ",", "ich", "jauch\u00b7ze", "dir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der du in deine Fluth mich tauchst,", "tokens": ["Der", "du", "in", "dei\u00b7ne", "Fluth", "mich", "tauchst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir, Genius des Tags, der du auf meinen Wegen", "tokens": ["Dir", ",", "Ge\u00b7nius", "des", "Tags", ",", "der", "du", "auf", "mei\u00b7nen", "We\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Jetzt Paradiese hauchst.", "tokens": ["Jetzt", "Pa\u00b7ra\u00b7die\u00b7se", "hauchst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie ist vergl\u00fcht die Weltensaat der Sterne", "tokens": ["Sie", "ist", "ver\u00b7gl\u00fcht", "die", "Wel\u00b7ten\u00b7saat", "der", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor deines Goldes erstem Glanz,", "tokens": ["Vor", "dei\u00b7nes", "Gol\u00b7des", "ers\u00b7tem", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und aus dem Feuermeer ziehst an der Berge Ferne", "tokens": ["Und", "aus", "dem", "Feu\u00b7er\u00b7meer", "ziehst", "an", "der", "Ber\u00b7ge", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du deinen Strahlenkranz.", "tokens": ["Du", "dei\u00b7nen", "Strah\u00b7len\u00b7kranz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was schlief erwacht, wie sich die Spitzen r\u00f6then,", "tokens": ["Was", "schlief", "er\u00b7wacht", ",", "wie", "sich", "die", "Spit\u00b7zen", "r\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "VVPP", "$,", "PWAV", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und in dem Hain wird alles Chor", "tokens": ["Und", "in", "dem", "Hain", "wird", "al\u00b7les", "Chor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu deinem Feyergru\u00df, und Philomelen fl\u00f6ten", "tokens": ["Zu", "dei\u00b7nem", "Fey\u00b7er\u00b7gru\u00df", ",", "und", "Phi\u00b7lo\u00b7me\u00b7len", "fl\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Begeisterter empor.", "tokens": ["Be\u00b7geis\u00b7ter\u00b7ter", "em\u00b7por", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dein Balsam weht in lieblichem Gemische", "tokens": ["Dein", "Bal\u00b7sam", "weht", "in", "lieb\u00b7li\u00b7chem", "Ge\u00b7mi\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Lebenskraft von Thal zu Thal", "tokens": ["Mit", "Le\u00b7bens\u00b7kraft", "von", "Thal", "zu", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf Blumenteppichen, und deines Odems Frische", "tokens": ["Auf", "Blu\u00b7men\u00b7tep\u00b7pi\u00b7chen", ",", "und", "dei\u00b7nes", "O\u00b7dems", "Fri\u00b7sche"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gibt allen Nerven Stahl.", "tokens": ["Gibt", "al\u00b7len", "Ner\u00b7ven", "Stahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Erhebt den Herrn! t\u00f6nts rund von jedem H\u00fcgel,", "tokens": ["Er\u00b7hebt", "den", "Herrn", "!", "t\u00f6nts", "rund", "von", "je\u00b7dem", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "VVFIN", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo eines Pflanzers H\u00fctte steht,", "tokens": ["Wo", "ei\u00b7nes", "Pflan\u00b7zers", "H\u00fct\u00b7te", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und aus der Bergschlucht hallts, und jedes L\u00fcftchens Fl\u00fcgel", "tokens": ["Und", "aus", "der", "Berg\u00b7schlucht", "hallts", ",", "und", "je\u00b7des", "L\u00fcft\u00b7chens", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,", "KON", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Empf\u00e4ngt ein Dankgebeth.", "tokens": ["Emp\u00b7f\u00e4ngt", "ein", "Dank\u00b7ge\u00b7beth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dort gie\u00dft die Gluth sich von dem Felsen nieder,", "tokens": ["Dort", "gie\u00dft", "die", "Gluth", "sich", "von", "dem", "Fel\u00b7sen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Lichtstaub, der von Gott sich schwingt:", "tokens": ["Ein", "Licht\u00b7staub", ",", "der", "von", "Gott", "sich", "schwingt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir trinken von dem Quell, und stammeln nur die Lieder,", "tokens": ["Wir", "trin\u00b7ken", "von", "dem", "Quell", ",", "und", "stam\u00b7meln", "nur", "die", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die kaum der Seraph singt.", "tokens": ["Die", "kaum", "der", "Se\u00b7raph", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Das Weitzenfeld ist deiner Wohlthat trunken", "tokens": ["Das", "Weit\u00b7zen\u00b7feld", "ist", "dei\u00b7ner", "Wohlt\u00b7hat", "trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und badet sich in Perlenthau;", "tokens": ["Und", "ba\u00b7det", "sich", "in", "Per\u00b7lent\u00b7hau", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und alle Farben blitzt in diamantnen Funken", "tokens": ["Und", "al\u00b7le", "Far\u00b7ben", "blitzt", "in", "di\u00b7a\u00b7mant\u00b7nen", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Feyerkleid der Au.", "tokens": ["Das", "Fe\u00b7yer\u00b7kleid", "der", "Au", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Am H\u00fcgel hin wogt Hygieens Weben", "tokens": ["Am", "H\u00fc\u00b7gel", "hin", "wogt", "Hy\u00b7gi\u00b7eens", "We\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "NE", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Apfelb\u00e4ume Bl\u00fcthenduft,", "tokens": ["Der", "Ap\u00b7fel\u00b7b\u00e4u\u00b7me", "Bl\u00fc\u00b7then\u00b7duft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und alles haucht mit Kraft das neugeschaffne Leben", "tokens": ["Und", "al\u00b7les", "haucht", "mit", "Kraft", "das", "neu\u00b7ge\u00b7schaff\u00b7ne", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In der gew\u00fcrzten Luft.", "tokens": ["In", "der", "ge\u00b7w\u00fcrz\u00b7ten", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "In Gruppen steigt die Gegend auf und nieder;", "tokens": ["In", "Grup\u00b7pen", "steigt", "die", "Ge\u00b7gend", "auf", "und", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Flu\u00df rollt rauchend durch das Thal,", "tokens": ["Der", "Flu\u00df", "rollt", "rau\u00b7chend", "durch", "das", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verbirgt sich hier im Wald, und dort erscheint er wieder,", "tokens": ["Ver\u00b7birgt", "sich", "hier", "im", "Wald", ",", "und", "dort", "er\u00b7scheint", "er", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPRART", "NN", "$,", "KON", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und gl\u00fcht im Sonnenstrahl.", "tokens": ["Und", "gl\u00fcht", "im", "Son\u00b7nen\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Die Dorfschalmey ruft laut schon Muth und Freude,", "tokens": ["Die", "Dorf\u00b7schal\u00b7mey", "ruft", "laut", "schon", "Muth", "und", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Herdenglocken t\u00f6nt der Zug;", "tokens": ["Von", "Her\u00b7den\u00b7glo\u00b7cken", "t\u00f6nt", "der", "Zug", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mit dem Morgenlied hebt an dem Saum der Heide", "tokens": ["Und", "mit", "dem", "Mor\u00b7gen\u00b7lied", "hebt", "an", "dem", "Saum", "der", "Hei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Pfl\u00fcger seinen Pflug.", "tokens": ["Der", "Pfl\u00fc\u00b7ger", "sei\u00b7nen", "Pflug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Verschlaft im Flaum, ihr modischen Gerippe,", "tokens": ["Ver\u00b7schlaft", "im", "Flaum", ",", "ihr", "mo\u00b7di\u00b7schen", "Ge\u00b7rip\u00b7pe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der jungen Horen Reihentanz!", "tokens": ["Der", "jun\u00b7gen", "Ho\u00b7ren", "Rei\u00b7hen\u00b7tanz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Euch reicht kein Felsenquell die Fluth der Aganippe", "tokens": ["Euch", "reicht", "kein", "Fel\u00b7sen\u00b7quell", "die", "Fluth", "der", "A\u00b7gan\u00b7ip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Kein Lenz den Bl\u00fcthenkranz.", "tokens": ["Kein", "Lenz", "den", "Bl\u00fc\u00b7then\u00b7kranz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ich rei\u00dfe mich aus meinem dumpfen Kerker", "tokens": ["Ich", "rei\u00b7\u00dfe", "mich", "aus", "mei\u00b7nem", "dum\u00b7pfen", "Ker\u00b7ker"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf in die Arme der Natur,", "tokens": ["Auf", "in", "die", "Ar\u00b7me", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und werde fr\u00f6hlicher, lebendiger und st\u00e4rker", "tokens": ["Und", "wer\u00b7de", "fr\u00f6h\u00b7li\u00b7cher", ",", "le\u00b7ben\u00b7di\u00b7ger", "und", "st\u00e4r\u00b7ker"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADJD", "$,", "ADJA", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In der erwachten Flur.", "tokens": ["In", "der", "er\u00b7wach\u00b7ten", "Flur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Elastisch hebt der Fu\u00df, wie eine Feder,", "tokens": ["E\u00b7las\u00b7tisch", "hebt", "der", "Fu\u00df", ",", "wie", "ei\u00b7ne", "Fe\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den K\u00f6rper, den er rhythmisch tr\u00e4gt,", "tokens": ["Den", "K\u00f6r\u00b7per", ",", "den", "er", "rhyth\u00b7misch", "tr\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Inde\u00df das leichte Blut zum Tanz durch das Ge\u00e4der", "tokens": ["In\u00b7de\u00df", "das", "leich\u00b7te", "Blut", "zum", "Tanz", "durch", "das", "Ge\u00b7\u00e4\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["