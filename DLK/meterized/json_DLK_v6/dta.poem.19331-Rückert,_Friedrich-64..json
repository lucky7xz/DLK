{"dta.poem.19331": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "64.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Esel ist bekannt, der hungernd stehen bliebe,", "tokens": ["Der", "E\u00b7sel", "ist", "be\u00b7kannt", ",", "der", "hun\u00b7gernd", "ste\u00b7hen", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PRELS", "ADJD", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil zu zwei B\u00fcndeln Heu ihn gleicher Hunger triebe.", "tokens": ["Weil", "zu", "zwei", "B\u00fcn\u00b7deln", "Heu", "ihn", "glei\u00b7cher", "Hun\u00b7ger", "trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "CARD", "NN", "NN", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Bekannt ist auch, da\u00df er nicht wirklich stehn wird bleiben,", "tokens": ["Be\u00b7kannt", "ist", "auch", ",", "da\u00df", "er", "nicht", "wirk\u00b7lich", "stehn", "wird", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "$,", "KOUS", "PPER", "PTKNEG", "ADJD", "VVINF", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil von zwei Treibenden mu\u00df eines st\u00e4rker treiben.", "tokens": ["Weil", "von", "zwei", "Trei\u00b7ben\u00b7den", "mu\u00df", "ei\u00b7nes", "st\u00e4r\u00b7ker", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "CARD", "NN", "VMFIN", "ART", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und bleibt er gleichwol stehn, so ists aus Eselei;", "tokens": ["Und", "bleibt", "er", "gleich\u00b7wol", "stehn", ",", "so", "ists", "aus", "E\u00b7se\u00b7lei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVINF", "$,", "ADV", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Zweifel wohnt ihm selbst, doch nicht der Sache bei.", "tokens": ["Der", "Zwei\u00b7fel", "wohnt", "ihm", "selbst", ",", "doch", "nicht", "der", "Sa\u00b7che", "bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "ADV", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ein Esel ist wer nicht kommt zum Entschlu\u00df deswegen,", "tokens": ["Ein", "E\u00b7sel", "ist", "wer", "nicht", "kommt", "zum", "Ent\u00b7schlu\u00df", "des\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PWS", "PTKNEG", "VVFIN", "APPRART", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil F\u00fcr und Wider er nicht kann aufs Loth abw\u00e4gen.", "tokens": ["Weil", "F\u00fcr", "und", "Wi\u00b7der", "er", "nicht", "kann", "aufs", "Loth", "ab\u00b7w\u00e4\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "PTKNEG", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}