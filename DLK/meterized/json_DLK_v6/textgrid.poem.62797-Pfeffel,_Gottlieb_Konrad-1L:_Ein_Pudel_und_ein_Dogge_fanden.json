{"textgrid.poem.62797": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Pudel und ein Dogge fanden", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Pudel und ein Dogge fanden", "tokens": ["Ein", "Pu\u00b7del", "und", "ein", "Dog\u00b7ge", "fan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als Waller sich in fremden Landen.", "tokens": ["Als", "Wal\u00b7ler", "sich", "in", "frem\u00b7den", "Lan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nachdem man, wie es \u00fcblich ist,", "tokens": ["Nach\u00b7dem", "man", ",", "wie", "es", "\u00fcb\u00b7lich", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PWAV", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich fein berochen und gek\u00fc\u00dft,", "tokens": ["Sich", "fein", "be\u00b7ro\u00b7chen", "und", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVINF", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So fieng man an sich allerhand zu sagen.", "tokens": ["So", "fi\u00b7eng", "man", "an", "sich", "al\u00b7ler\u00b7hand", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "PRF", "PIS", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Der Pudel, ein Genie, sprach im Posaunenton", "tokens": ["Der", "Pu\u00b7del", ",", "ein", "Ge\u00b7nie", ",", "sprach", "im", "Po\u00b7sau\u00b7nen\u00b7ton"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Von seiner eigenen Person;", "tokens": ["Von", "sei\u00b7ner", "ei\u00b7ge\u00b7nen", "Per\u00b7son", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die\u00df ist der Modestyl in unsern Tagen.", "tokens": ["Die\u00df", "ist", "der", "Mo\u00b7des\u00b7tyl", "in", "un\u00b7sern", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Herr Vetter, fieng er l\u00e4chelnd an,", "tokens": ["Herr", "Vet\u00b7ter", ",", "fi\u00b7eng", "er", "l\u00e4\u00b7chelnd", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Sie sollten mich nur einmal sehen,", "tokens": ["Sie", "soll\u00b7ten", "mich", "nur", "ein\u00b7mal", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Was ich f\u00fcr Schw\u00e4nke machen kann:", "tokens": ["Was", "ich", "f\u00fcr", "Schw\u00e4n\u00b7ke", "ma\u00b7chen", "kann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Es ist ein Spiel f\u00fcr mich auf einem Seil zu gehen,", "tokens": ["Es", "ist", "ein", "Spiel", "f\u00fcr", "mich", "auf", "ei\u00b7nem", "Seil", "zu", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und wie ein steifer Fl\u00fcgelmann", "tokens": ["Und", "wie", "ein", "stei\u00b7fer", "Fl\u00fc\u00b7gel\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Mit einem Spie\u00df im Schilderhaus zu stehen.", "tokens": ["Mit", "ei\u00b7nem", "Spie\u00df", "im", "Schil\u00b7der\u00b7haus", "zu", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Ich tanze, besser tanzt der gro\u00dfe Vestris nicht.", "tokens": ["Ich", "tan\u00b7ze", ",", "bes\u00b7ser", "tanzt", "der", "gro\u00b7\u00dfe", "Vest\u00b7ris", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "VVFIN", "ART", "ADJA", "NE", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ich lasse mich zu todte schie\u00dfen", "tokens": ["Ich", "las\u00b7se", "mich", "zu", "tod\u00b7te", "schie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Und bin flugs wieder auf den F\u00fc\u00dfen,", "tokens": ["Und", "bin", "flugs", "wie\u00b7der", "auf", "den", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Wenn man ein Wort vom Henker spricht.", "tokens": ["Wenn", "man", "ein", "Wort", "vom", "Hen\u00b7ker", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Noch mehr, ich kann mit unerh\u00f6rten Spr\u00fcngen,", "tokens": ["Noch", "mehr", ",", "ich", "kann", "mit", "un\u00b7er\u00b7h\u00f6r\u00b7ten", "Spr\u00fcn\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPER", "VMFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Bald \u00fcber einen Stock, bald durch den Reif mich schwingen", "tokens": ["Bald", "\u00fc\u00b7ber", "ei\u00b7nen", "Stock", ",", "bald", "durch", "den", "Reif", "mich", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und ... g\u00e4hnen Sie? Hier brach der Redner ab.", "tokens": ["Und", "...", "g\u00e4h\u00b7nen", "Sie", "?", "Hier", "brach", "der", "Red\u00b7ner", "ab", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "VVFIN", "PPER", "$.", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Der Dogge sprach, soll ich mich auch erheben?", "tokens": ["Der", "Dog\u00b7ge", "sprach", ",", "soll", "ich", "mich", "auch", "er\u00b7he\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Ich sch\u00fctze meinem Herrn das Leben", "tokens": ["Ich", "sch\u00fct\u00b7ze", "mei\u00b7nem", "Herrn", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Und gehe mit ihm bis ins Grab.", "tokens": ["Und", "ge\u00b7he", "mit", "ihm", "bis", "ins", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}