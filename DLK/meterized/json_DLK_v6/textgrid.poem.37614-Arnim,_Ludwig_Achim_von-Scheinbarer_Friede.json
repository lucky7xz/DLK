{"textgrid.poem.37614": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Scheinbarer Friede", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, die Welt wird \u00f6de,", "tokens": ["Ja", ",", "die", "Welt", "wird", "\u00f6\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ja, die Welt wird leer,", "tokens": ["Ja", ",", "die", "Welt", "wird", "leer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und mein Aug' so schn\u00f6de,", "tokens": ["Und", "mein", "Aug'", "so", "schn\u00f6\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Gr\u00fc\u00dft das Licht nicht mehr;", "tokens": ["Gr\u00fc\u00dft", "das", "Licht", "nicht", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Was in Abendr\u00f6the", "tokens": ["Was", "in", "A\u00b7bend\u00b7r\u00f6\u00b7the"], "token_info": ["word", "word", "word"], "pos": ["PWS", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Schien gewitternd her,", "tokens": ["Schien", "ge\u00b7wit\u00b7ternd", "her", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Jetzt in Morgenr\u00f6the", "tokens": ["Jetzt", "in", "Mor\u00b7gen\u00b7r\u00f6\u00b7the"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Macht die Luft so schwer;", "tokens": ["Macht", "die", "Luft", "so", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Ferne Donner rollen,", "tokens": ["Fer\u00b7ne", "Don\u00b7ner", "rol\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Und die Arbeit g\u00e4hnt,", "tokens": ["Und", "die", "Ar\u00b7beit", "g\u00e4hnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Ferne Feinde grollen,", "tokens": ["Fer\u00b7ne", "Fein\u00b7de", "grol\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Friede wird gew\u00e4hnt.", "tokens": ["Frie\u00b7de", "wird", "ge\u00b7w\u00e4hnt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}