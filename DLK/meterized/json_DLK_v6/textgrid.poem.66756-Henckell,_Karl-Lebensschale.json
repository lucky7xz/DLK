{"textgrid.poem.66756": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Lebensschale", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So magst du unersch\u00fcttert schweben", "tokens": ["So", "magst", "du", "un\u00b7er\u00b7sch\u00fct\u00b7tert", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und reichgef\u00fcllt im Gleichma\u00df ruhn,", "tokens": ["Und", "reich\u00b7ge\u00b7f\u00fcllt", "im", "Gleich\u00b7ma\u00df", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du Schale, die mir Gott gegeben,", "tokens": ["Du", "Scha\u00b7le", ",", "die", "mir", "Gott", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "All Lust und Last hineinzutun.", "tokens": ["All", "Lust", "und", "Last", "hin\u00b7ein\u00b7zu\u00b7tun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wild schwanktest du im Ungewissen,", "tokens": ["Wild", "schwank\u00b7test", "du", "im", "Un\u00b7ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hast dich zum Abgrund j\u00e4h geneigt \u2013", "tokens": ["Hast", "dich", "zum", "Ab\u00b7grund", "j\u00e4h", "ge\u00b7neigt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun sei in Licht und Finsternissen,", "tokens": ["Nun", "sei", "in", "Licht", "und", "Fins\u00b7ter\u00b7nis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die nimmer st\u00fcrzt noch schwindelnd steigt!", "tokens": ["Die", "nim\u00b7mer", "st\u00fcrzt", "noch", "schwin\u00b7delnd", "steigt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}