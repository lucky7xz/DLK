{"dta.poem.746": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxXII.  An Frawen Marien Richterin.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Schawt Gott/ wie er denn schawt/ von seiner Him\u0303el feste ", "tokens": ["Schawt", "Gott", "/", "wie", "er", "denn", "schawt", "/", "von", "sei\u00b7ner", "Him\u0303el", "fes\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "$(", "PWAV", "PPER", "ADV", "VVFIN", "$(", "APPR", "PPOSAT", "NN", "ADJA"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auff di\u00df was heilig ist/ vnd einig auff jhn h\u00e4lt;", "tokens": ["Auff", "di\u00df", "was", "hei\u00b7lig", "ist", "/", "vnd", "ei\u00b7nig", "auff", "jhn", "h\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "PRELS", "ADJD", "VAFIN", "$(", "KON", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie da\u00df sein donnerstrall denn jmmer auff euch f\u00e4lt", "tokens": ["Wie", "da\u00df", "sein", "don\u00b7ner\u00b7strall", "denn", "jm\u00b7mer", "auff", "euch", "f\u00e4lt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "PPOSAT", "NN", "KON", "ADV", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jhr sch\u00f6nstes tugendt reis/ vnd schont der d\u00fcrren \u00e4ste?", "tokens": ["Ihr", "sch\u00f6ns\u00b7tes", "tu\u00b7gendt", "reis", "/", "vnd", "schont", "der", "d\u00fcr\u00b7ren", "\u00e4s\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$(", "KON", "VVFIN", "ART", "ADJA", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Auff welche schl\u00e4gt sein ", "tokens": ["Auff", "wel\u00b7che", "schl\u00e4gt", "sein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIS", "VVFIN", "PPOSAT"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Gantz truncken voll von lust sich breiten in der Welt", "tokens": ["Gantz", "trun\u00b7cken", "voll", "von", "lust", "sich", "brei\u00b7ten", "in", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJD", "APPR", "VVFIN", "PRF", "ADJA", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist pest/ ist flam\u0303 vnd Todt/ denn nur auff euch bestelt/", "tokens": ["Ist", "pest", "/", "ist", "flam\u0303", "vnd", "Todt", "/", "denn", "nur", "auff", "euch", "be\u00b7stelt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "$(", "VAFIN", "VVINF", "KON", "NN", "$(", "KON", "ADV", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.4": {"text": "Da doch der b\u00f6sen lo\u00df st\u00e4ts f\u00e4lt auffs allerbeste?", "tokens": ["Da", "doch", "der", "b\u00f6\u00b7sen", "lo\u00df", "st\u00e4ts", "f\u00e4lt", "auffs", "al\u00b7ler\u00b7bes\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "ADV", "VVFIN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was sag ich? nein f\u00fcrwar/ weil jhr in diesem leidt/", "tokens": ["Was", "sag", "ich", "?", "nein", "f\u00fcr\u00b7war", "/", "weil", "jhr", "in", "die\u00b7sem", "leidt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "PTKANT", "ADJD", "$(", "KOUS", "PPER", "APPR", "PDAT", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Spiegel der gedult/ vnd Bild der Hoffnung seidt/", "tokens": ["Ein", "Spie\u00b7gel", "der", "ge\u00b7dult", "/", "vnd", "Bild", "der", "Hoff\u00b7nung", "seidt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "KON", "NN", "ART", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mu\u00df dieser wetter Sturm den starcken Geist bewehren.", "tokens": ["Mu\u00df", "die\u00b7ser", "wet\u00b7ter", "Sturm", "den", "star\u00b7cken", "Geist", "be\u00b7weh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dem/ weil er mehr denn trew\u2019 in vngemeiner noth/", "tokens": ["Dem", "/", "weil", "er", "mehr", "denn", "tre\u00b7w'", "in", "vn\u00b7ge\u00b7mei\u00b7ner", "noth", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Bey Christus Blutfahn h\u00e4lt der ewigtrewe GOtt", "tokens": ["Bey", "Chris\u00b7tus", "Blut\u00b7fahn", "h\u00e4lt", "der", "e\u00b7wig\u00b7tre\u00b7we", "Gott"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mehr denn gemeinen lohn vnd Frewde wird bescheren.", "tokens": ["Mehr", "denn", "ge\u00b7mei\u00b7nen", "lohn", "vnd", "Frew\u00b7de", "wird", "be\u00b7sche\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "KON", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}