{"textgrid.poem.46075": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[die Abendwinde wehen]", "genre": "verse", "period": "N.A.", "pub_year": 1825, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Abendwinde wehen,", "tokens": ["Die", "Ab\u00b7end\u00b7win\u00b7de", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich mu\u00df zur Linde gehen,", "tokens": ["Ich", "mu\u00df", "zur", "Lin\u00b7de", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mu\u00df einsam weinend stehen,", "tokens": ["Mu\u00df", "ein\u00b7sam", "wei\u00b7nend", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es kommt kein Sternenschein;", "tokens": ["Es", "kommt", "kein", "Ster\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die kleinen V\u00f6glein sehen", "tokens": ["Die", "klei\u00b7nen", "V\u00f6\u00b7glein", "se\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Betr\u00fcbt zu mir und flehen,", "tokens": ["Be\u00b7tr\u00fcbt", "zu", "mir", "und", "fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "KON", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und wenn sie schlafen gehen,", "tokens": ["Und", "wenn", "sie", "schla\u00b7fen", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dann wein' ich ganz allein!", "tokens": ["Dann", "wein'", "ich", "ganz", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "\u00bbich h\u00f6r' ein Sichlein rauschen,", "tokens": ["\u00bb", "ich", "h\u00f6r'", "ein", "Sic\u00b7hlein", "rau\u00b7schen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Woll rauschen durch den Klee,", "tokens": ["Woll", "rau\u00b7schen", "durch", "den", "Klee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Ich h\u00f6r' ein M\u00e4gdlein klagen", "tokens": ["Ich", "h\u00f6r'", "ein", "M\u00e4gd\u00b7lein", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Von Weh, von bitterm Weh!\u00ab", "tokens": ["Von", "Weh", ",", "von", "bit\u00b7term", "Weh", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich soll ein Lied dir singen,", "tokens": ["Ich", "soll", "ein", "Lied", "dir", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich mu\u00df die H\u00e4nde ringen,", "tokens": ["Ich", "mu\u00df", "die", "H\u00e4n\u00b7de", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Herz will mir zerspringen", "tokens": ["Das", "Herz", "will", "mir", "zer\u00b7sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In bittrer Tr\u00e4nenflut,", "tokens": ["In", "bit\u00b7trer", "Tr\u00e4\u00b7nen\u00b7flut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich sing' und m\u00f6chte weinen,", "tokens": ["Ich", "sing'", "und", "m\u00f6ch\u00b7te", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So lang der Mond mag scheinen,", "tokens": ["So", "lang", "der", "Mond", "mag", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sehn' ich mich nach der Einen,", "tokens": ["Sehn'", "ich", "mich", "nach", "der", "Ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bei der mein Leiden ruht!", "tokens": ["Bei", "der", "mein", "Lei\u00b7den", "ruht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "\u00bbich h\u00f6r' ein Sichlein rauschen,", "tokens": ["\u00bb", "ich", "h\u00f6r'", "ein", "Sic\u00b7hlein", "rau\u00b7schen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Wohl rauschen durch den Klee,", "tokens": ["Wohl", "rau\u00b7schen", "durch", "den", "Klee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Ich h\u00f6r' ein M\u00e4gdlein klagen", "tokens": ["Ich", "h\u00f6r'", "ein", "M\u00e4gd\u00b7lein", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Von Weh, von bitterm Weh!\u00ab", "tokens": ["Von", "Weh", ",", "von", "bit\u00b7term", "Weh", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mein Herz mu\u00df nun vollenden,", "tokens": ["Mein", "Herz", "mu\u00df", "nun", "voll\u00b7en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da sich die Zeit will wenden,", "tokens": ["Da", "sich", "die", "Zeit", "will", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es f\u00e4llt mir aus den H\u00e4nden", "tokens": ["Es", "f\u00e4llt", "mir", "aus", "den", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der letzte Lebenstraum.", "tokens": ["Der", "letz\u00b7te", "Le\u00b7ben\u00b7straum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Entsetzliches Verschwenden", "tokens": ["Ent\u00b7setz\u00b7li\u00b7ches", "Ver\u00b7schwen\u00b7den"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In allen Elementen,", "tokens": ["In", "al\u00b7len", "E\u00b7le\u00b7men\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.7": {"text": "Mu\u00dft' ich den Geist verpf\u00e4nden,", "tokens": ["Mu\u00dft'", "ich", "den", "Geist", "ver\u00b7pf\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und alles war nur Schaum!", "tokens": ["Und", "al\u00b7les", "war", "nur", "Schaum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "\u00bbich h\u00f6r' ein Sichlein rauschen,", "tokens": ["\u00bb", "ich", "h\u00f6r'", "ein", "Sic\u00b7hlein", "rau\u00b7schen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Wohl rauschen durch den Klee,", "tokens": ["Wohl", "rau\u00b7schen", "durch", "den", "Klee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Ich h\u00f6r' ein M\u00e4gdlein klagen", "tokens": ["Ich", "h\u00f6r'", "ein", "M\u00e4gd\u00b7lein", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Von Weh, von bitterm Weh!\u00ab", "tokens": ["Von", "Weh", ",", "von", "bit\u00b7term", "Weh", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Was du mir hast gegeben,", "tokens": ["Was", "du", "mir", "hast", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gen\u00fcgt ein ganzes Leben", "tokens": ["Ge\u00b7n\u00fcgt", "ein", "gan\u00b7zes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zum Himmel zu erheben;", "tokens": ["Zum", "Him\u00b7mel", "zu", "er\u00b7he\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O sage, ich sei dein!", "tokens": ["O", "sa\u00b7ge", ",", "ich", "sei", "dein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPER", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da kehrt sie sich mit Schweigen", "tokens": ["Da", "kehrt", "sie", "sich", "mit", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und gibt kein Lebenszeichen,", "tokens": ["Und", "gibt", "kein", "Le\u00b7bens\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da mu\u00dfte ich erbleichen,", "tokens": ["Da", "mu\u00df\u00b7te", "ich", "er\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein Herz ward wie ein Stein.", "tokens": ["Mein", "Herz", "ward", "wie", "ein", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "\u00bbich h\u00f6r' ein Sichlein rauschen,", "tokens": ["\u00bb", "ich", "h\u00f6r'", "ein", "Sic\u00b7hlein", "rau\u00b7schen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Wohl rauschen durch den Klee,", "tokens": ["Wohl", "rau\u00b7schen", "durch", "den", "Klee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Ich h\u00f6r' ein M\u00e4gdlein klagen", "tokens": ["Ich", "h\u00f6r'", "ein", "M\u00e4gd\u00b7lein", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Von Weh, von bitterm Weh!\u00ab", "tokens": ["Von", "Weh", ",", "von", "bit\u00b7term", "Weh", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Heb Fr\u00fchling jetzt die Schwingen,", "tokens": ["Heb", "Fr\u00fch\u00b7ling", "jetzt", "die", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "La\u00df kleine V\u00f6glein singen,", "tokens": ["La\u00df", "klei\u00b7ne", "V\u00f6\u00b7glein", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df Bl\u00fcmlein aufw\u00e4rts dringen,", "tokens": ["La\u00df", "Bl\u00fcm\u00b7lein", "auf\u00b7w\u00e4rts", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "S\u00fc\u00df Lieb geht durch den Hain.", "tokens": ["S\u00fc\u00df", "Lieb", "geht", "durch", "den", "Hain", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich mu\u00dft' mein Herz bezwingen,", "tokens": ["Ich", "mu\u00dft'", "mein", "Herz", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mu\u00df alles niederringen,", "tokens": ["Mu\u00df", "al\u00b7les", "nie\u00b7der\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Darf nichts zu Tage bringen,", "tokens": ["Darf", "nichts", "zu", "Ta\u00b7ge", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wir waren nicht allein!", "tokens": ["Wir", "wa\u00b7ren", "nicht", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "\u00bbich h\u00f6r' ein Sichlein rauschen,", "tokens": ["\u00bb", "ich", "h\u00f6r'", "ein", "Sic\u00b7hlein", "rau\u00b7schen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Wohl rauschen durch den Klee,", "tokens": ["Wohl", "rau\u00b7schen", "durch", "den", "Klee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Ich h\u00f6r' ein M\u00e4gdlein klagen", "tokens": ["Ich", "h\u00f6r'", "ein", "M\u00e4gd\u00b7lein", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Von Weh, von bitterm Weh!\u00ab", "tokens": ["Von", "Weh", ",", "von", "bit\u00b7term", "Weh", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wie soll ich mich im Freien", "tokens": ["Wie", "soll", "ich", "mich", "im", "Frei\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWA