{"textgrid.poem.40251": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ueber unsre Liebe h\u00e4ngt", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ueber unsre Liebe h\u00e4ngt", "tokens": ["Ue\u00b7ber", "uns\u00b7re", "Lie\u00b7be", "h\u00e4ngt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "eine tiefe Trauerweide.", "tokens": ["ei\u00b7ne", "tie\u00b7fe", "Trau\u00b7er\u00b7wei\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nacht und Schatten um uns beide;", "tokens": ["Nacht", "und", "Schat\u00b7ten", "um", "uns", "bei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PPER", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "unsre Stirnen sind gesenkt.", "tokens": ["uns\u00b7re", "Stir\u00b7nen", "sind", "ge\u00b7senkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wortlos sitzen wir im Dunkeln;", "tokens": ["Wort\u00b7los", "sit\u00b7zen", "wir", "im", "Dun\u00b7keln", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "einstmals rauschte hier ein Strom,", "tokens": ["einst\u00b7mals", "rauschte", "hier", "ein", "Strom", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "einstmals sahn wir Sterne funkeln ...", "tokens": ["einst\u00b7mals", "sahn", "wir", "Ster\u00b7ne", "fun\u00b7keln", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ist denn Alles tot und tr\u00fcbe? \u2013", "tokens": ["Ist", "denn", "Al\u00b7les", "tot", "und", "tr\u00fc\u00b7be", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "PIS", "ADJD", "KON", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Horch: ein ferner Mund! vom Dom!", "tokens": ["Horch", ":", "ein", "fer\u00b7ner", "Mund", "!", "vom", "Dom", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "ADJA", "NN", "$.", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Glockench\u00f6re ... Nacht ... und Liebe ...", "tokens": ["Glo\u00b7cken\u00b7ch\u00f6\u00b7re", "...", "Nacht", "...", "und", "Lie\u00b7be", "..."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$(", "NN", "$(", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}