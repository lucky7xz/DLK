{"textgrid.poem.57824": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Totenblumen", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es bl\u00fchten Tulpen und Narzissen,", "tokens": ["Es", "bl\u00fch\u00b7ten", "Tul\u00b7pen", "und", "Nar\u00b7zis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie bl\u00fchten dir, sie bl\u00fchten mir;", "tokens": ["Sie", "bl\u00fch\u00b7ten", "dir", ",", "sie", "bl\u00fch\u00b7ten", "mir", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sind verwelkt, sie sind verdorret,", "tokens": ["Sie", "sind", "ver\u00b7welkt", ",", "sie", "sind", "ver\u00b7dor\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn heute mu\u00df ich fort von dir.", "tokens": ["Denn", "heu\u00b7te", "mu\u00df", "ich", "fort", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PTKVZ", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der blaue und der wei\u00dfe Flieder", "tokens": ["Der", "blau\u00b7e", "und", "der", "wei\u00b7\u00dfe", "Flie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der hat verloren seine Zier;", "tokens": ["Der", "hat", "ver\u00b7lo\u00b7ren", "sei\u00b7ne", "Zier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er wird uns niemals wieder bl\u00fchen,", "tokens": ["Er", "wird", "uns", "nie\u00b7mals", "wie\u00b7der", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn heute mu\u00df ich fort von dir.", "tokens": ["Denn", "heu\u00b7te", "mu\u00df", "ich", "fort", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PTKVZ", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die roten und die wei\u00dfen Rosen", "tokens": ["Die", "ro\u00b7ten", "und", "die", "wei\u00b7\u00dfen", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die bl\u00fchen weder dir noch mir;", "tokens": ["Die", "bl\u00fc\u00b7hen", "we\u00b7der", "dir", "noch", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "PPER", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie m\u00fcssen ungepfl\u00fcckt verwelken,", "tokens": ["Sie", "m\u00fcs\u00b7sen", "un\u00b7ge\u00b7pfl\u00fcckt", "ver\u00b7wel\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn heute mu\u00df ich fort von dir.", "tokens": ["Denn", "heu\u00b7te", "mu\u00df", "ich", "fort", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PTKVZ", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Astern und Reseden bl\u00fchen,", "tokens": ["Die", "As\u00b7tern", "und", "Re\u00b7se\u00b7den", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Was hilft es dir, was hilft es mir;", "tokens": ["Was", "hilft", "es", "dir", ",", "was", "hilft", "es", "mir", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "$,", "PWS", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein andrer wird sie beide brechen,", "tokens": ["Ein", "an\u00b7drer", "wird", "sie", "bei\u00b7de", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn heute mu\u00df ich fort von dir.", "tokens": ["Denn", "heu\u00b7te", "mu\u00df", "ich", "fort", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PTKVZ", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die allerletzten gelben Blumen,", "tokens": ["Die", "al\u00b7ler\u00b7letz\u00b7ten", "gel\u00b7ben", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Ringelblumen, pfl\u00fcck' ich mir;", "tokens": ["Die", "Rin\u00b7gel\u00b7blu\u00b7men", ",", "pfl\u00fcck", "ich", "mir", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie bl\u00fchen auf dem Grab der Liebe,", "tokens": ["Sie", "bl\u00fc\u00b7hen", "auf", "dem", "Grab", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn heute mu\u00df ich fort von dir.", "tokens": ["Denn", "heu\u00b7te", "mu\u00df", "ich", "fort", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PTKVZ", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}