{"textgrid.poem.59794": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sing' ich ein Lied, h\u00fcpft freudereich", "tokens": ["Sing'", "ich", "ein", "Lied", ",", "h\u00fcpft", "freu\u00b7de\u00b7reich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Herz der jungen M\u00e4dchen,", "tokens": ["Das", "Herz", "der", "jun\u00b7gen", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn Perlen sind die Worte gleich,", "tokens": ["Denn", "Per\u00b7len", "sind", "die", "Wor\u00b7te", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gereiht auf seidnen F\u00e4dchen!", "tokens": ["Ge\u00b7reiht", "auf", "seid\u00b7nen", "F\u00e4d\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und D\u00fcfte steigen auf daraus,", "tokens": ["Und", "D\u00fcf\u00b7te", "stei\u00b7gen", "auf", "da\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Houris' Hauch getr\u00e4nkte \u2013", "tokens": ["Von", "Hou\u00b7ris'", "Hauch", "ge\u00b7tr\u00e4nk\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gleichwie aus jenem Blumenstrau\u00df,", "tokens": ["Gleich\u00b7wie", "aus", "je\u00b7nem", "Blu\u00b7men\u00b7strau\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den mir Zul\u00e9ikha schenkte.", "tokens": ["Den", "mir", "Zul\u00e9ik\u00b7ha", "schenk\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NE", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Erstaunt nicht, da\u00df des S\u00e4ngers Mund", "tokens": ["Er\u00b7staunt", "nicht", ",", "da\u00df", "des", "S\u00e4n\u00b7gers", "Mund"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "PTKNEG", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So Herrliches vollbringe,", "tokens": ["So", "Herr\u00b7li\u00b7ches", "voll\u00b7brin\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und da\u00df die Weisheit hier den Bund", "tokens": ["Und", "da\u00df", "die", "Weis\u00b7heit", "hier", "den", "Bund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Jugendtollheit schlinge!", "tokens": ["Mit", "Ju\u00b7gend\u00b7toll\u00b7heit", "schlin\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wi\u00dft ihr, wer mir die Weisheit gab?", "tokens": ["Wi\u00dft", "ihr", ",", "wer", "mir", "die", "Weis\u00b7heit", "gab", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie kam vom rechten Orte,", "tokens": ["Sie", "kam", "vom", "rech\u00b7ten", "Or\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich las sie ", "tokens": ["Ich", "las", "sie"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Und h\u00fcllte sie in Worte!", "tokens": ["Und", "h\u00fcll\u00b7te", "sie", "in", "Wor\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was Wunder, wenn so anmutvoll", "tokens": ["Was", "Wun\u00b7der", ",", "wenn", "so", "an\u00b7mut\u00b7voll"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "NN", "$,", "KOUS", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Euch meine Lieder t\u00f6nen,", "tokens": ["Euch", "mei\u00b7ne", "Lie\u00b7der", "t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ist doch, was meinem Mund entquoll,", "tokens": ["Ist", "doch", ",", "was", "mei\u00b7nem", "Mund", "ent\u00b7quoll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "PRELS", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Abglanz nur der Sch\u00f6nen!", "tokens": ["Ein", "Ab\u00b7glanz", "nur", "der", "Sch\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie ist dem Becher Oshemschid", "tokens": ["Sie", "ist", "dem", "Be\u00b7cher", "Os\u00b7hem\u00b7schid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Quell der Offenbarung,", "tokens": ["Ein", "Quell", "der", "Of\u00b7fen\u00b7ba\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der mir erschlie\u00dft ein Zauberreich", "tokens": ["Der", "mir", "er\u00b7schlie\u00dft", "ein", "Zau\u00b7ber\u00b7reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Weisheit und Erfahrung.", "tokens": ["Der", "Weis\u00b7heit", "und", "Er\u00b7fah\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und sagt: erklingt nicht mein Gesang", "tokens": ["Und", "sagt", ":", "er\u00b7klingt", "nicht", "mein", "Ge\u00b7sang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "VVFIN", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von wunderbaren T\u00f6nen?", "tokens": ["Von", "wun\u00b7der\u00b7ba\u00b7ren", "T\u00f6\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ist nicht meines Liedes Gang", "tokens": ["Und", "ist", "nicht", "mei\u00b7nes", "Lie\u00b7des", "Gang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PTKNEG", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Leicht wie der Gong der Sch\u00f6nen?", "tokens": ["Leicht", "wie", "der", "Gong", "der", "Sch\u00f6\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}