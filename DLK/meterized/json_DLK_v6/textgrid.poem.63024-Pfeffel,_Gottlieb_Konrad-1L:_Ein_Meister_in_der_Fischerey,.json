{"textgrid.poem.63024": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Meister in der Fischerey,", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Meister in der Fischerey,", "tokens": ["Ein", "Meis\u00b7ter", "in", "der", "Fi\u00b7sche\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Reiger, welcher nur Forellen", "tokens": ["Ein", "Rei\u00b7ger", ",", "wel\u00b7cher", "nur", "Fo\u00b7rel\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und Aale fra\u00df, stund einst im May,", "tokens": ["Und", "Aa\u00b7le", "fra\u00df", ",", "stund", "einst", "im", "May", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Itzt Wonnemond, um Wild zu f\u00e4llen,", "tokens": ["Itzt", "Won\u00b7ne\u00b7mond", ",", "um", "Wild", "zu", "f\u00e4l\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "KOUI", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vor Ph\u00f6bus auf. Der Sybarit", "tokens": ["Vor", "Ph\u00f6\u00b7bus", "auf", ".", "Der", "Sy\u00b7ba\u00b7rit"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "PTKVZ", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sah zwanzig Hechte, Karpfen, Schleyen", "tokens": ["Sah", "zwan\u00b7zig", "Hech\u00b7te", ",", "Karp\u00b7fen", ",", "Schle\u00b7yen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "CARD", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.7": {"text": "Vor\u00fcber ziehn: sein Appetit", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "ziehn", ":", "sein", "Ap\u00b7pe\u00b7tit"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVINF", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Verschm\u00e4hte diese Lumpereyen.", "tokens": ["Ver\u00b7schm\u00e4h\u00b7te", "die\u00b7se", "Lum\u00b7pe\u00b7rey\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Inzwischen treibt der Sonne Lauf", "tokens": ["I\u00b7nzwi\u00b7schen", "treibt", "der", "Son\u00b7ne", "Lauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Der Fische Schwarm in ihre Zellen", "tokens": ["Der", "Fi\u00b7sche", "Schwarm", "in", "ih\u00b7re", "Zel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und der Corsar bringt statt Forellen,", "tokens": ["Und", "der", "Cor\u00b7sar", "bringt", "statt", "Fo\u00b7rel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Zwo Kressen und ein Krebschen auf.", "tokens": ["Zwo", "Kres\u00b7sen", "und", "ein", "Kreb\u00b7schen", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "KON", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Er wirft sie weg, w\u00fchlt einem Aale,", "tokens": ["Er", "wirft", "sie", "weg", ",", "w\u00fchlt", "ei\u00b7nem", "Aa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Den er erblickt, im lauen Bach", "tokens": ["Den", "er", "er\u00b7blickt", ",", "im", "lau\u00b7en", "Bach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "VVPP", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Erbost mit seinem Schnabel nach;", "tokens": ["Er\u00b7bost", "mit", "sei\u00b7nem", "Schna\u00b7bel", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Allein umsonst, vom Mittagsstrahle", "tokens": ["Al\u00b7lein", "um\u00b7sonst", ",", "vom", "Mit\u00b7tags\u00b7strah\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Verbrannt, vor Schw\u00e4che halb entseelt", "tokens": ["Ver\u00b7brannt", ",", "vor", "Schw\u00e4\u00b7che", "halb", "ent\u00b7seelt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und von des Hungers Wuth gequ\u00e4lt", "tokens": ["Und", "von", "des", "Hun\u00b7gers", "Wuth", "ge\u00b7qu\u00e4lt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Mu\u00df er am Ende sich bequemen,", "tokens": ["Mu\u00df", "er", "am", "En\u00b7de", "sich", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "PRF", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Mit einem Frosch vorlieb zu nehmen.", "tokens": ["Mit", "ei\u00b7nem", "Frosch", "vor\u00b7lieb", "zu", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}