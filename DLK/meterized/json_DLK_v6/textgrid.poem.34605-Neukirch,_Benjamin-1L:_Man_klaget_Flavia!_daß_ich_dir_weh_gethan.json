{"textgrid.poem.34605": {"metadata": {"author": {"name": "Neukirch, Benjamin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Man klaget/ Flavia! da\u00df ich dir weh gethan/", "genre": "verse", "period": "N.A.", "pub_year": 1697, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man klaget/ Flavia! da\u00df ich dir weh gethan/", "tokens": ["Man", "kla\u00b7get", "/", "Fla\u00b7via", "!", "da\u00df", "ich", "dir", "weh", "ge\u00b7than", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$(", "NE", "$.", "KOUS", "PPER", "PPER", "ADV", "VVPP", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und redet \u00f6ffentlich/ du werdest bald verscheiden/", "tokens": ["Und", "re\u00b7det", "\u00f6f\u00b7fent\u00b7lich", "/", "du", "wer\u00b7dest", "bald", "ver\u00b7schei\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$(", "PPER", "VAFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du selber zeigest es durch deine schwachheit an;", "tokens": ["Du", "sel\u00b7ber", "zei\u00b7gest", "es", "durch", "dei\u00b7ne", "schwach\u00b7heit", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So dencke nun/ was ich mu\u00df in der seele leiden.", "tokens": ["So", "den\u00b7cke", "nun", "/", "was", "ich", "mu\u00df", "in", "der", "see\u00b7le", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$(", "PWS", "PPER", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich komme/ liebes kind/ mir als ein teuffel f\u00fcr/", "tokens": ["Ich", "kom\u00b7me", "/", "lie\u00b7bes", "kind", "/", "mir", "als", "ein", "teuf\u00b7fel", "f\u00fcr", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADJA", "NN", "$(", "PPER", "KOUS", "ART", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und bin so mir als dir zu einem hencker worden.", "tokens": ["Und", "bin", "so", "mir", "als", "dir", "zu", "ei\u00b7nem", "hen\u00b7cker", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PPER", "KOUS", "PPER", "APPR", "ART", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dann alle meine lust bestehet blo\u00df in dir/", "tokens": ["Dann", "al\u00b7le", "mei\u00b7ne", "lust", "be\u00b7ste\u00b7het", "blo\u00df", "in", "dir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "PPOSAT", "NN", "VVFIN", "ADV", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und darumb kan ich dich nicht ohne mich ermorden.", "tokens": ["Und", "da\u00b7rumb", "kan", "ich", "dich", "nicht", "oh\u00b7ne", "mich", "er\u00b7mor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VMFIN", "PPER", "PRF", "PTKNEG", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "O himmel! wende doch die schulden von mir ab/", "tokens": ["O", "him\u00b7mel", "!", "wen\u00b7de", "doch", "die", "schul\u00b7den", "von", "mir", "ab", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "ADV", "ART", "ADJA", "APPR", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Hilff meiner Flavia/ und lege mich ins grab!", "tokens": ["Hilff", "mei\u00b7ner", "Fla\u00b7via", "/", "und", "le\u00b7ge", "mich", "ins", "grab", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NE", "$(", "KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+----+-+-+", "measure": "dactylic.init"}}}}}