{"dta.poem.9341": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Anbind-gedichte/ als Adelheit den 16.  \n Christm\u00f6nat jhren Nahmentstag be-  \n gieng", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Oauszug aller schaam/ o bild der Sch\u00f6nen/", "tokens": ["O\u00b7a\u00b7us\u00b7zug", "al\u00b7ler", "schaam", "/", "o", "bild", "der", "Sch\u00f6\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$(", "FM", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "von der die Venus selbst mu\u00df schmuck entlehnen/", "tokens": ["von", "der", "die", "Ve\u00b7nus", "selbst", "mu\u00df", "schmuck", "ent\u00b7leh\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "ADV", "VMFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "o spiegel aller zucht/ o Adelheit/", "tokens": ["o", "spie\u00b7gel", "al\u00b7ler", "zucht", "/", "o", "A\u00b7del\u00b7heit", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["FM", "NN", "PIAT", "NN", "$(", "FM", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "o Demant aller zier/ o Licht der zeit/", "tokens": ["o", "De\u00b7mant", "al\u00b7ler", "zier", "/", "o", "Licht", "der", "zeit", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "NN", "PIAT", "NN", "$(", "FM", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "was ist dis f\u00fcr ein gru\u00df und wundsch? von wannen?", "tokens": ["was", "ist", "dis", "f\u00fcr", "ein", "gru\u00df", "und", "wund\u00b7sch", "?", "von", "wan\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "APPR", "ART", "NN", "KON", "ADJD", "$.", "APPR", "ADV", "$."], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Wer hatt hier aufgesteckt die gr\u00fcnen Dannen?", "tokens": ["Wer", "hatt", "hier", "auf\u00b7ge\u00b7steckt", "die", "gr\u00fc\u00b7nen", "Dan\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Was ist di\u00df f\u00fcr ein spiel/ der s\u00fc\u00dfe toon?", "tokens": ["Was", "ist", "di\u00df", "f\u00fcr", "ein", "spiel", "/", "der", "s\u00fc\u00b7\u00dfe", "toon", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "APPR", "ART", "VVFIN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wer tritt ", "tokens": ["Wer", "tritt"], "token_info": ["word", "word"], "pos": ["PWS", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Wie! oder ist dein Licht und Tag erschienen/", "tokens": ["Wie", "!", "o\u00b7der", "ist", "dein", "Licht", "und", "Tag", "er\u00b7schie\u00b7nen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KON", "VAFIN", "PPOSAT", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "da\u00df dier zu ehren jtzt so viel aufdienen?", "tokens": ["da\u00df", "dier", "zu", "eh\u00b7ren", "jtzt", "so", "viel", "auf\u00b7die\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKZU", "VVINF", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "F\u00fcrwar es ist also: hier ist das band/", "tokens": ["F\u00fcr\u00b7war", "es", "ist", "al\u00b7so", ":", "hier", "ist", "das", "band", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "$.", "ADV", "VAFIN", "PDS", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "damit ich binden will die zahrte hand:", "tokens": ["da\u00b7mit", "ich", "bin\u00b7den", "will", "die", "zahr\u00b7te", "hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Hier ist der g\u00fcldne Krantz dich zubekr\u00e4ntzen/", "tokens": ["Hier", "ist", "der", "g\u00fcld\u00b7ne", "Krantz", "dich", "zu\u00b7be\u00b7kr\u00b7\u00e4nt\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "den Demant ziert und schm\u00fcckt mit solchen gl\u00e4ntzen/", "tokens": ["den", "De\u00b7mant", "ziert", "und", "schm\u00fcckt", "mit", "sol\u00b7chen", "gl\u00e4nt\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "da\u00df mann verbl\u00e4ndet wird bey sp\u00e4ter nacht:", "tokens": ["da\u00df", "mann", "ver\u00b7bl\u00e4n\u00b7det", "wird", "bey", "sp\u00e4\u00b7ter", "nacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVPP", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Was wird er kriegen wohl vor glantz und pracht/", "tokens": ["Was", "wird", "er", "krie\u00b7gen", "wohl", "vor", "glantz", "und", "pracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "wenn Jhn dein g\u00fcldnes haar erst wird auszieren/", "tokens": ["wenn", "Jhn", "dein", "g\u00fcld\u00b7nes", "haar", "erst", "wird", "aus\u00b7zie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "ADV", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "und mit verwunderung den glantz duplieren?", "tokens": ["und", "mit", "ver\u00b7wun\u00b7de\u00b7rung", "den", "glantz", "du\u00b7plie\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Vor dich wer er zu schlecht/ ", "tokens": ["Vor", "dich", "wer", "er", "zu", "schlecht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PWS", "PPER", "PTKA", "ADJD", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.20": {"text": "wo er von deinem glantz nicht w\u00fcrd\u2019 erf\u00fcllt:", "tokens": ["wo", "er", "von", "dei\u00b7nem", "glantz", "nicht", "w\u00fcrd'", "er\u00b7f\u00fcllt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN", "PTKNEG", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "\u00fcnd solches wei\u00df ich wohl/ dr\u00fcmb nim vor willen/", "tokens": ["\u00fcnd", "sol\u00b7ches", "wei\u00df", "ich", "wohl", "/", "dr\u00fcmb", "nim", "vor", "wil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "ADV", "$(", "ADV", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "weil dein Belieben wird nichts bessers stillen/", "tokens": ["weil", "dein", "Be\u00b7lie\u00b7ben", "wird", "nichts", "bes\u00b7sers", "stil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "PIS", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "als dieses edle pfand der sesten Treu/", "tokens": ["als", "die\u00b7ses", "ed\u00b7le", "pfand", "der", "ses\u00b7ten", "Treu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "die durch die Feder dier versprochen sey.", "tokens": ["die", "durch", "die", "Fe\u00b7der", "dier", "ver\u00b7spro\u00b7chen", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Hier mit gehab dich wohl und leb\u2019 in freuden/", "tokens": ["Hier", "mit", "ge\u00b7hab", "dich", "wohl", "und", "leb'", "in", "freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "nim hin den g\u00fcldnen krantz/ das band von seiden/", "tokens": ["nim", "hin", "den", "g\u00fcld\u00b7nen", "krantz", "/", "das", "band", "von", "sei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN", "$(", "PDS", "VVFIN", "APPR", "PPOSAT", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "der Himmel friste dier die Lebenszeit/", "tokens": ["der", "Him\u00b7mel", "fris\u00b7te", "dier", "die", "Le\u00b7bens\u00b7zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "und la\u00dfe dich gesund ohn\u2019 alles leid.", "tokens": ["und", "la\u00b7\u00dfe", "dich", "ge\u00b7sund", "ohn'", "al\u00b7les", "leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}