{"dta.poem.892": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "TrAUM DES B\u00d6SEN", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Verhallend eines Gongs braungoldne Kl\u00e4nge \u2014", "tokens": ["Ver\u00b7hal\u00b7lend", "ei\u00b7nes", "Gongs", "braun\u00b7gold\u00b7ne", "Kl\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Liebender erwacht in schwarzen Zimmern", "tokens": ["Ein", "Lie\u00b7ben\u00b7der", "er\u00b7wacht", "in", "schwar\u00b7zen", "Zim\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Wang\u2019 an Flammen, die im Fenster flimmern.", "tokens": ["Die", "Wang'", "an", "Flam\u00b7men", ",", "die", "im", "Fens\u00b7ter", "flim\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am Strome blitzen Segel, Masten, Str\u00e4nge.", "tokens": ["Am", "Stro\u00b7me", "blit\u00b7zen", "Se\u00b7gel", ",", "Mas\u00b7ten", ",", "Str\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein M\u00f6nch, ein schwangres Weib dort im Gedr\u00e4nge.", "tokens": ["Ein", "M\u00f6nch", ",", "ein", "schwang\u00b7res", "Weib", "dort", "im", "Ge\u00b7dr\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Guitarren klimpern, rote Kittel schimmern.", "tokens": ["Gui\u00b7tar\u00b7ren", "klim\u00b7pern", ",", "ro\u00b7te", "Kit\u00b7tel", "schim\u00b7mern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kastanien schw\u00fcl in goldnem Glanz verk\u00fcmmern;", "tokens": ["Kas\u00b7ta\u00b7ni\u00b7en", "schw\u00fcl", "in", "gold\u00b7nem", "Glanz", "ver\u00b7k\u00fcm\u00b7mern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Schwarz ragt der Kirchen trauriges Gepr\u00e4nge.", "tokens": ["Schwarz", "ragt", "der", "Kir\u00b7chen", "trau\u00b7ri\u00b7ges", "Ge\u00b7pr\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Aus bleichen Masken schaut der Geist des B\u00f6sen.", "tokens": ["Aus", "blei\u00b7chen", "Mas\u00b7ken", "schaut", "der", "Geist", "des", "B\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Platz verd\u00e4mmert grauenvoll und d\u00fcster;", "tokens": ["Ein", "Platz", "ver\u00b7d\u00e4m\u00b7mert", "grau\u00b7en\u00b7voll", "und", "d\u00fcs\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am Abend regt auf Inseln sich Gefl\u00fcster.", "tokens": ["Am", "A\u00b7bend", "regt", "auf", "In\u00b7seln", "sich", "Ge\u00b7fl\u00fcs\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "NN", "PRF", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Des Vogelfluges wirre Zeichen lesen", "tokens": ["Des", "Vo\u00b7gel\u00b7flu\u00b7ges", "wir\u00b7re", "Zei\u00b7chen", "le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auss\u00e4tzige, die zur Nacht vielleicht verwesen.", "tokens": ["Aus\u00b7s\u00e4t\u00b7zi\u00b7ge", ",", "die", "zur", "Nacht", "viel\u00b7leicht", "ver\u00b7we\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im Park erblicken zitternd sich Geschwister.", "tokens": ["Im", "Park", "er\u00b7bli\u00b7cken", "zit\u00b7ternd", "sich", "Ge\u00b7schwis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "VVFIN", "PRF", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}