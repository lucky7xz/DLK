{"textgrid.poem.37845": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der vortreffliche Stallbruder", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn der Sch\u00e4fer scheeren will,", "tokens": ["Wenn", "der", "Sch\u00e4\u00b7fer", "schee\u00b7ren", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Stellt er sich hinter die Hecke,", "tokens": ["Stellt", "er", "sich", "hin\u00b7ter", "die", "He\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Scheert dem Schaaf die Wolle ab,", "tokens": ["Scheert", "dem", "Schaaf", "die", "Wol\u00b7le", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Steckt sie in die S\u00e4cke.", "tokens": ["Steckt", "sie", "in", "die", "S\u00e4\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "F\u00e4ngt zu tanzen an, zu singen,", "tokens": ["F\u00e4ngt", "zu", "tan\u00b7zen", "an", ",", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKZU", "VVINF", "PTKVZ", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00e4st auf seinem Dudeldu:", "tokens": ["Bl\u00e4st", "auf", "sei\u00b7nem", "Du\u00b7del\u00b7du", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bblieber Bruder dir ichs bringe,", "tokens": ["\u00bb", "lie\u00b7ber", "Bru\u00b7der", "dir", "ichs", "brin\u00b7ge", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NN", "PPER", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieber Bruder trink dir's zu.", "tokens": ["Lie\u00b7ber", "Bru\u00b7der", "trink", "dir's", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was ich trag auf H\u00e4nden mein,", "tokens": ["Was", "ich", "trag", "auf", "H\u00e4n\u00b7den", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "NN", "PPOSAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist ein Gl\u00e4\u00dflein k\u00fchlen Wein;", "tokens": ["Ist", "ein", "Gl\u00e4\u00df\u00b7lein", "k\u00fch\u00b7len", "Wein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fl\u00f6g doch ein V\u00f6glein \u00fcbern Rhein,", "tokens": ["Fl\u00f6g", "doch", "ein", "V\u00f6\u00b7glein", "\u00fc\u00b7bern", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Br\u00e4cht's dem lieben Stallbruder mein.", "tokens": ["Br\u00e4cht's", "dem", "lie\u00b7ben", "Stall\u00b7bru\u00b7der", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "PPOSAT", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Stallbruder mein, du bist wohl werth,", "tokens": ["Stall\u00b7bru\u00b7der", "mein", ",", "du", "bist", "wohl", "werth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df man dich auf'm Altar verehrt,", "tokens": ["Da\u00df", "man", "dich", "auf'm", "Al\u00b7tar", "ver\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hast ein Paar W\u00e4ngelein", "tokens": ["Hast", "ein", "Paar", "W\u00e4n\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie ein Rubin,", "tokens": ["Wie", "ein", "Ru\u00b7bin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NE", "$,"], "meter": "--+-", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Augen wie Schwarzenstein,", "tokens": ["Au\u00b7gen", "wie", "Schwar\u00b7zen\u00b7stein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Z\u00e4hne wie Elfenbein,", "tokens": ["Z\u00e4h\u00b7ne", "wie", "El\u00b7fen\u00b7bein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Bist gar ein kluger Kerl,", "tokens": ["Bist", "gar", "ein", "klu\u00b7ger", "Kerl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie ich es bin.\u00ab", "tokens": ["Wie", "ich", "es", "bin", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "PPER", "VAFIN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}}}}