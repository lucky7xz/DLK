{"textgrid.poem.37941": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Die fromme Magd", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine fromme Magd von gutem Stand,", "tokens": ["Ei\u00b7ne", "from\u00b7me", "Magd", "von", "gu\u00b7tem", "Stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Geht ihrer Frauen fein zur Hand,", "tokens": ["Geht", "ih\u00b7rer", "Frau\u00b7en", "fein", "zur", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00e4lt Sch\u00fcssel, Tisch und Teller weis,", "tokens": ["H\u00e4lt", "Sch\u00fcs\u00b7sel", ",", "Tisch", "und", "Tel\u00b7ler", "weis", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu ihrem und der Frauen Prei\u00df.", "tokens": ["Zu", "ih\u00b7rem", "und", "der", "Frau\u00b7en", "Prei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie tr\u00e4gt und bringt kein neue M\u00e4hr,", "tokens": ["Sie", "tr\u00e4gt", "und", "bringt", "kein", "neu\u00b7e", "M\u00e4hr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geht still in ihrer Arbeit her,", "tokens": ["Geht", "still", "in", "ih\u00b7rer", "Ar\u00b7beit", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist treu und eines keuschen Muths,", "tokens": ["Ist", "treu", "und", "ei\u00b7nes", "keu\u00b7schen", "Muths", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und thut den Kindern alles Guts.", "tokens": ["Und", "thut", "den", "Kin\u00b7dern", "al\u00b7les", "Guts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie ist auch munter, hurtig, frisch,", "tokens": ["Sie", "ist", "auch", "mun\u00b7ter", ",", "hur\u00b7tig", ",", "frisch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verbringet ihr Gesch\u00e4fte risch,", "tokens": ["Ver\u00b7brin\u00b7get", "ihr", "Ge\u00b7sch\u00e4f\u00b7te", "risch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und h\u00e4lts der Frauen wohl zu gut,", "tokens": ["Und", "h\u00e4lts", "der", "Frau\u00b7en", "wohl", "zu", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sie um Schaden reden thut.", "tokens": ["Wenn", "sie", "um", "Scha\u00b7den", "re\u00b7den", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie hat dazu ein fein Geberd,", "tokens": ["Sie", "hat", "da\u00b7zu", "ein", "fein", "Ge\u00b7berd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00e4lt alles sauber an dem Heerd,", "tokens": ["H\u00e4lt", "al\u00b7les", "sau\u00b7ber", "an", "dem", "Heerd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verwahrt das Feuer und das Licht,", "tokens": ["Ver\u00b7wahrt", "das", "Feu\u00b7er", "und", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schlummert in der Kirche nicht.", "tokens": ["Und", "schlum\u00b7mert", "in", "der", "Kir\u00b7che", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}