{"dta.poem.1915": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Die Frau/ die dich zu Tisch und Bette soll erg\u00f6tzen/", "tokens": ["Die", "Frau", "/", "die", "dich", "zu", "Tisch", "und", "Bet\u00b7te", "soll", "er\u00b7g\u00f6t\u00b7zen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PRF", "APPR", "NN", "KON", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mu\u00df nicht nach Redners Art die langen Worte setzen/", "tokens": ["Mu\u00df", "nicht", "nach", "Red\u00b7ners", "Art", "die", "lan\u00b7gen", "Wor\u00b7te", "set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "APPR", "NN", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mu\u00df nicht nach kluger Kunst die rundten Schl\u00fcsse drehen/", "tokens": ["Mu\u00df", "nicht", "nach", "klu\u00b7ger", "Kunst", "die", "rund\u00b7ten", "Schl\u00fcs\u00b7se", "dre\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht wissen/ was f\u00fcr ihr und \u00fcberall geschehen.", "tokens": ["Nicht", "wis\u00b7sen", "/", "was", "f\u00fcr", "ihr", "und", "\u00fc\u00b7be\u00b7rall", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$(", "PWS", "APPR", "PPER", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Aus B\u00fcchern steht ihr frey da\u00df sie ein wenig wisse/", "tokens": ["Aus", "B\u00fc\u00b7chern", "steht", "ihr", "frey", "da\u00df", "sie", "ein", "we\u00b7nig", "wis\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJD", "KOUS", "PPER", "ART", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch da\u00df sie den Verstand bey dir erholen m\u00fcsse.", "tokens": ["Doch", "da\u00df", "sie", "den", "Ver\u00b7stand", "bey", "dir", "er\u00b7ho\u00b7len", "m\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}