{"textgrid.poem.24358": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Als Dieselben mit der Arm\u00e9e in ein fremd Land r\u00fcckten", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sage selbst der Feind/ wie er dich hat gefunden:", "tokens": ["Es", "sa\u00b7ge", "selbst", "der", "Feind", "/", "wie", "er", "dich", "hat", "ge\u00b7fun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$(", "PWAV", "PPER", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Kriege Helden gleich/ und tapfer in der Schlacht/", "tokens": ["Im", "Krie\u00b7ge", "Hel\u00b7den", "gleich", "/", "und", "tap\u00b7fer", "in", "der", "Schlacht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "ADV", "$(", "KON", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Im Ungl\u00fcck Gro\u00dfmuths voll/ die ward nie \u00fcberwunden/", "tokens": ["Im", "Un\u00b7gl\u00fcck", "Gro\u00df\u00b7muths", "voll", "/", "die", "ward", "nie", "\u00fc\u00b7berw\u00b7un\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "ADJD", "$(", "ART", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Klug/ standhafft/ da der Feind dein Land in Noth gebracht/", "tokens": ["Klug", "/", "stand\u00b7hafft", "/", "da", "der", "Feind", "dein", "Land", "in", "Noth", "ge\u00b7bracht", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVFIN", "$(", "KOUS", "ART", "NN", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der gr\u00f6sten Cronen wehrt/ da er sie wolte rauben.", "tokens": ["Der", "gr\u00f6s\u00b7ten", "Cro\u00b7nen", "wehrt", "/", "da", "er", "sie", "wol\u00b7te", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und endlich ist der ", "tokens": ["Und", "end\u00b7lich", "ist", "der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Da\u00df sich die Tugenden/ die \u2013 \u2013 nicht will glauben/", "tokens": ["Da\u00df", "sich", "die", "Tu\u00b7gen\u00b7den", "/", "die", "\u2013", "\u2013", "nicht", "will", "glau\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "$(", "ART", "$(", "$(", "PTKNEG", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In wahrer G\u00fctigkeit den Feinden liessen sehn.", "tokens": ["In", "wah\u00b7rer", "G\u00fc\u00b7tig\u00b7keit", "den", "Fein\u00b7den", "lies\u00b7sen", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}