{"dta.poem.14380": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "G\u00f6tterd\u00e4mmerung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ist's Liebe, ist's Andacht,", "tokens": ["Ist's", "Lie\u00b7be", ",", "ist's", "An\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Was so Dich begl\u00fcckt?", "tokens": ["Was", "so", "Dich", "be\u00b7gl\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Rings Fr\u00fchling Dich anlacht,", "tokens": ["Rings", "Fr\u00fch\u00b7ling", "Dich", "an\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du sinnest entz\u00fcckt. \u2014", "tokens": ["Du", "sin\u00b7nest", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}