{"textgrid.poem.35347": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was haben Dichterworte zu bedeuten,", "tokens": ["Was", "ha\u00b7ben", "Dich\u00b7ter\u00b7wor\u00b7te", "zu", "be\u00b7deu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was soll ein Lied, das keiner Laune fr\u00f6hnt?", "tokens": ["Was", "soll", "ein", "Lied", ",", "das", "kei\u00b7ner", "Lau\u00b7ne", "fr\u00f6hnt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und, wenn es weder schmeichelt noch verh\u00f6hnt,", "tokens": ["Und", ",", "wenn", "es", "we\u00b7der", "schmei\u00b7chelt", "noch", "ver\u00b7h\u00f6hnt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "KON", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie findet es den Weg zu fremden Leuten?", "tokens": ["Wie", "fin\u00b7det", "es", "den", "Weg", "zu", "frem\u00b7den", "Leu\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In diesen Tagen, den gewitterschw\u00fclen,", "tokens": ["In", "die\u00b7sen", "Ta\u00b7gen", ",", "den", "ge\u00b7wit\u00b7ter\u00b7schw\u00fc\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In dieser Zeit, der bangen, \u00fcberreifen,", "tokens": ["In", "die\u00b7ser", "Zeit", ",", "der", "ban\u00b7gen", ",", "\u00fc\u00b7berr\u00b7ei\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "ADJD", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wer wird, um seine Sorgen wegzusp\u00fclen,", "tokens": ["Wer", "wird", ",", "um", "sei\u00b7ne", "Sor\u00b7gen", "weg\u00b7zu\u00b7sp\u00fc\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "KOUI", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nach dem Pokale des Poeten greifen?", "tokens": ["Nach", "dem", "Po\u00b7ka\u00b7le", "des", "Po\u00b7et\u00b7en", "grei\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ein deutsches M\u00e4gdlein mag von Bechern nippen,", "tokens": ["Ein", "deut\u00b7sches", "M\u00e4gd\u00b7lein", "mag", "von", "Be\u00b7chern", "nip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die fader Maitrank f\u00fcllt; seit meine Lenze", "tokens": ["Die", "fa\u00b7der", "Mai\u00b7trank", "f\u00fcllt", ";", "seit", "mei\u00b7ne", "Len\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Von dannen flogen, ist, was ich kredenze,", "tokens": ["Von", "dan\u00b7nen", "flo\u00b7gen", ",", "ist", ",", "was", "ich", "kre\u00b7den\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "$,", "VAFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Kein Honigseim f\u00fcr jungfr\u00e4uliche Lippen.", "tokens": ["Kein", "Ho\u00b7ni\u00b7gseim", "f\u00fcr", "jung\u00b7fr\u00e4u\u00b7li\u00b7che", "Lip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Ich komme nicht als ungest\u00fcmer Dr\u00e4nger,", "tokens": ["Ich", "kom\u00b7me", "nicht", "als", "un\u00b7ge\u00b7st\u00fc\u00b7mer", "Dr\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Als Waffenherold oder Minnes\u00e4nger,", "tokens": ["Als", "Waf\u00b7fen\u00b7he\u00b7rold", "o\u00b7der", "Min\u00b7ne\u00b7s\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Verlasse selten mein bequemes Zelt.", "tokens": ["Ver\u00b7las\u00b7se", "sel\u00b7ten", "mein", "be\u00b7que\u00b7mes", "Zelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Wohl sah ich einst, aus hoher Fensterbr\u00fcstung,", "tokens": ["Wohl", "sah", "ich", "einst", ",", "aus", "ho\u00b7her", "Fens\u00b7ter\u00b7br\u00fcs\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "In Jugend\u00fcbermut, in voller R\u00fcstung,", "tokens": ["In", "Ju\u00b7gend\u00b7\u00fc\u00b7ber\u00b7mut", ",", "in", "vol\u00b7ler", "R\u00fcs\u00b7tung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Durch Morgennebel in die weite Welt;", "tokens": ["Durch", "Mor\u00b7gen\u00b7ne\u00b7bel", "in", "die", "wei\u00b7te", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.19": {"text": "Jetzt aber ist es innerste Betrachtung,", "tokens": ["Jetzt", "a\u00b7ber", "ist", "es", "in\u00b7ners\u00b7te", "Be\u00b7trach\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Die mir allein geziemt; von mir entfernt", "tokens": ["Die", "mir", "al\u00b7lein", "ge\u00b7ziemt", ";", "von", "mir", "ent\u00b7fernt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "VVPP", "$.", "APPR", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Ist des Genusses Kelch; was ich gelernt:", "tokens": ["Ist", "des", "Ge\u00b7nus\u00b7ses", "Kelch", ";", "was", "ich", "ge\u00b7lernt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$.", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Entsagung; Selbstbezwingung, Selbstverachtung,", "tokens": ["Ent\u00b7sa\u00b7gung", ";", "Selbst\u00b7be\u00b7zwin\u00b7gung", ",", "Selbst\u00b7ver\u00b7ach\u00b7tung", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Was ich erhofft, erfleht, was ich gewonnen,", "tokens": ["Was", "ich", "er\u00b7hofft", ",", "er\u00b7fleht", ",", "was", "ich", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVFIN", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Hat sich in der Gedanken Feueresse,", "tokens": ["Hat", "sich", "in", "der", "Ge\u00b7dan\u00b7ken", "Feu\u00b7e\u00b7res\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.25": {"text": "In meines tiefsten Wesens Flammenbronnen", "tokens": ["In", "mei\u00b7nes", "tiefs\u00b7ten", "We\u00b7sens", "Flam\u00b7men\u00b7bron\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Langsam geformt zu einer Totenmesse.", "tokens": ["Lang\u00b7sam", "ge\u00b7formt", "zu", "ei\u00b7ner", "To\u00b7ten\u00b7mes\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.27": {"text": "Nicht ganz erloschen war, was einst so m\u00e4chtig", "tokens": ["Nicht", "ganz", "er\u00b7lo\u00b7schen", "war", ",", "was", "einst", "so", "m\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VVPP", "VAFIN", "$,", "PRELS", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "In mir gebrannt, was sich als Glut bew\u00e4hrte,", "tokens": ["In", "mir", "ge\u00b7brannt", ",", "was", "sich", "als", "Glut", "be\u00b7w\u00e4hr\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "PRELS", "PRF", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Die keiner sch\u00fcren wollte, keiner n\u00e4hrte,", "tokens": ["Die", "kei\u00b7ner", "sch\u00fc\u00b7ren", "woll\u00b7te", ",", "kei\u00b7ner", "n\u00e4hr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIS", "VVINF", "VMFIN", "$,", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Und was ich selbst entfachte \u2013 doch bed\u00e4chtig.", "tokens": ["Und", "was", "ich", "selbst", "ent\u00b7fach\u00b7te", "\u2013", "doch", "be\u00b7d\u00e4ch\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "$(", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Poetenherz! aus deiner Asche spr\u00fchn", "tokens": ["Poe\u00b7ten\u00b7herz", "!", "aus", "dei\u00b7ner", "A\u00b7sche", "spr\u00fchn"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.32": {"text": "Die Funken hoch empor \u2013 es sind nicht viele \u2013;", "tokens": ["Die", "Fun\u00b7ken", "hoch", "em\u00b7por", "\u2013", "es", "sind", "nicht", "vie\u00b7le", "\u2013", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$(", "PPER", "VAFIN", "PTKNEG", "PIAT", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Der Winter naht, wir stehen bald am Ziele,", "tokens": ["Der", "Win\u00b7ter", "naht", ",", "wir", "ste\u00b7hen", "bald", "am", "Zie\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Und mich bed\u00fcnkt, da\u00df eitel mein Bem\u00fchn,", "tokens": ["Und", "mich", "be\u00b7d\u00fcnkt", ",", "da\u00df", "ei\u00b7tel", "mein", "Be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Da\u00df mich die Au\u00dfenwelt, die glatte, kalte,", "tokens": ["Da\u00df", "mich", "die", "Au\u00b7\u00dfen\u00b7welt", ",", "die", "glat\u00b7te", ",", "kal\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "PRELS", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Verdammen mu\u00df, weil du zu rasch geschlagen.", "tokens": ["Ver\u00b7dam\u00b7men", "mu\u00df", ",", "weil", "du", "zu", "rasch", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "KOUS", "PPER", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "O Herz, ich h\u00f6re Stimmen, die dir sagen:", "tokens": ["O", "Herz", ",", "ich", "h\u00f6\u00b7re", "Stim\u00b7men", ",", "die", "dir", "sa\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN", "NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Du bist das gleiche noch, du bist das alte!", "tokens": ["Du", "bist", "das", "glei\u00b7che", "noch", ",", "du", "bist", "das", "al\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADV", "$,", "PPER", "VAFIN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Wohlan, verloren sei mein letzter Pfeil!", "tokens": ["Wo\u00b7hlan", ",", "ver\u00b7lo\u00b7ren", "sei", "mein", "letz\u00b7ter", "Pfeil", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVPP", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Verklingen m\u00f6gen meine Melodien,", "tokens": ["Ver\u00b7klin\u00b7gen", "m\u00f6\u00b7gen", "mei\u00b7ne", "Me\u00b7lo\u00b7dien", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Wenn siegesstarke S\u00e4nger f\u00fcr das Heil", "tokens": ["Wenn", "sie\u00b7ges\u00b7star\u00b7ke", "S\u00e4n\u00b7ger", "f\u00fcr", "das", "Heil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Der Menschen vor der Wahrheit niederknien!", "tokens": ["Der", "Men\u00b7schen", "vor", "der", "Wahr\u00b7heit", "nie\u00b7der\u00b7kni\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Tod! du ersch\u00fctterst meines Wissens Schranken;", "tokens": ["Tod", "!", "du", "er\u00b7sch\u00fct\u00b7terst", "mei\u00b7nes", "Wis\u00b7sens", "Schran\u00b7ken", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.44": {"text": "Doch ungestillte Sehnsucht rei\u00dft mich fort;", "tokens": ["Doch", "un\u00b7ge\u00b7still\u00b7te", "Sehn\u00b7sucht", "rei\u00dft", "mich", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Nicht mir geb\u00fchrt das priesterliche Wort,", "tokens": ["Nicht", "mir", "ge\u00b7b\u00fchrt", "das", "pries\u00b7ter\u00b7li\u00b7che", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Das rechte Wort f\u00fcr z\u00fcndende Gedanken;", "tokens": ["Das", "rech\u00b7te", "Wort", "f\u00fcr", "z\u00fcn\u00b7den\u00b7de", "Ge\u00b7dan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "Ich beuge willig mich vor Geistesf\u00fcrsten,", "tokens": ["Ich", "beu\u00b7ge", "wil\u00b7lig", "mich", "vor", "Geis\u00b7tes\u00b7f\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.48": {"text": "Und ich ersehne ihren G\u00f6tterwein", "tokens": ["Und", "ich", "er\u00b7seh\u00b7ne", "ih\u00b7ren", "G\u00f6t\u00b7ter\u00b7wein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.49": {"text": "F\u00fcr alle, die in ihrer Herzenspein", "tokens": ["F\u00fcr", "al\u00b7le", ",", "die", "in", "ih\u00b7rer", "Her\u00b7zen\u00b7spein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.50": {"text": "Nach Himmelsnektar, nach Erkenntnis d\u00fcrsten.", "tokens": ["Nach", "Him\u00b7mels\u00b7nek\u00b7tar", ",", "nach", "Er\u00b7kennt\u00b7nis", "d\u00fcrs\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}