{"textgrid.poem.34284": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1. Archiplagiarius", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr kennt mich nicht: ich weis die Kniffe!", "tokens": ["Ihr", "kennt", "mich", "nicht", ":", "ich", "weis", "die", "Knif\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "PTKVZ", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wes man mich zeyht, das heft' ich andern an.", "tokens": ["Wes", "man", "mich", "zeyht", ",", "das", "heft'", "ich", "an\u00b7dern", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "PIS", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Kein einziger der Autorgriffe", "tokens": ["Kein", "ein\u00b7zi\u00b7ger", "der", "Au\u00b7tor\u00b7grif\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "ART", "NN"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Entwischte mir, das machte mich zum Mann.", "tokens": ["Ent\u00b7wischte", "mir", ",", "das", "mach\u00b7te", "mich", "zum", "Mann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PDS", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "So z\u00fcndte Archimed der R\u00f6mer Schiffe", "tokens": ["So", "z\u00fcnd\u00b7te", "Ar\u00b7chi\u00b7med", "der", "R\u00f6\u00b7mer", "Schif\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit aufgefangnem Lichte an.", "tokens": ["Mit", "auf\u00b7ge\u00b7fang\u00b7nem", "Lich\u00b7te", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}