{"textgrid.poem.57839": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Der goldene Hahn", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hatte einen sch\u00f6nen Traum", "tokens": ["Ich", "hat\u00b7te", "ei\u00b7nen", "sch\u00f6\u00b7nen", "Traum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von einem gr\u00fcnen Buchenbaum;", "tokens": ["Von", "ei\u00b7nem", "gr\u00fc\u00b7nen", "Bu\u00b7chen\u00b7baum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Traum, der war so lang und breit,", "tokens": ["Der", "Traum", ",", "der", "war", "so", "lang", "und", "breit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie eine kleine Ewigkeit.", "tokens": ["Wie", "ei\u00b7ne", "klei\u00b7ne", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich ging allein im gr\u00fcnen Wald,", "tokens": ["Ich", "ging", "al\u00b7lein", "im", "gr\u00fc\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Viel Brommelbeeren fand ich bald;", "tokens": ["Viel", "Brom\u00b7mel\u00b7bee\u00b7ren", "fand", "ich", "bald", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich hab' mich auf und ab geb\u00fcckt,", "tokens": ["Ich", "hab'", "mich", "auf", "und", "ab", "ge\u00b7b\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Brommelbeeren abgepfl\u00fcckt.", "tokens": ["Die", "Brom\u00b7mel\u00b7bee\u00b7ren", "ab\u00b7ge\u00b7pfl\u00fcckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mein Herz auf einmal stille stand,", "tokens": ["Mein", "Herz", "auf", "ein\u00b7mal", "stil\u00b7le", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADV", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das K\u00f6rblein fiel mir aus der Hand;", "tokens": ["Das", "K\u00f6rb\u00b7lein", "fiel", "mir", "aus", "der", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich h\u00f6rte singen den gold'nen Hahn,", "tokens": ["Ich", "h\u00f6r\u00b7te", "sin\u00b7gen", "den", "gold'\u00b7nen", "Hahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der k\u00fcndet junges Sterben an.", "tokens": ["Der", "k\u00fcn\u00b7det", "jun\u00b7ges", "Ster\u00b7ben", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Was fang' ich an in meiner Not?", "tokens": ["Was", "fang'", "ich", "an", "in", "mei\u00b7ner", "Not", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich h\u00f6re meinen eig'nen Tod;", "tokens": ["Ich", "h\u00f6\u00b7re", "mei\u00b7nen", "eig'\u00b7nen", "Tod", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer den gold'nen Hahn h\u00f6rt ganz allein,", "tokens": ["Wer", "den", "gold'\u00b7nen", "Hahn", "h\u00f6rt", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Sein Grab wird bald gegraben sein.", "tokens": ["Sein", "Grab", "wird", "bald", "ge\u00b7gra\u00b7ben", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du junges, junges J\u00e4gerblut,", "tokens": ["Du", "jun\u00b7ges", ",", "jun\u00b7ges", "J\u00e4\u00b7ger\u00b7blut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nimm mich in deine treue Hut;", "tokens": ["Nimm", "mich", "in", "dei\u00b7ne", "treu\u00b7e", "Hut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Brommelbeeren im K\u00f6rbelein,", "tokens": ["Die", "Brom\u00b7mel\u00b7bee\u00b7ren", "im", "K\u00f6r\u00b7be\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die soll'n dir nicht verwehret sein.", "tokens": ["Die", "soll'n", "dir", "nicht", "ver\u00b7weh\u00b7ret", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Brommelbeeren will ich nicht,", "tokens": ["Die", "Brom\u00b7mel\u00b7bee\u00b7ren", "will", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du allerliebstes Angesicht;", "tokens": ["Du", "al\u00b7ler\u00b7liebs\u00b7tes", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Will k\u00fcssen deinen roten Mund", "tokens": ["Will", "k\u00fcs\u00b7sen", "dei\u00b7nen", "ro\u00b7ten", "Mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im gr\u00fcnen Wald eine Viertelstund'.", "tokens": ["Im", "gr\u00fc\u00b7nen", "Wald", "ei\u00b7ne", "Vier\u00b7tel\u00b7stund'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Eine Viertelstund' ist nicht lang noch breit,", "tokens": ["Ei\u00b7ne", "Vier\u00b7tel\u00b7stund'", "ist", "nicht", "lang", "noch", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADJD", "ADV", "ADJD", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Es ist ja keine Ewigkeit;", "tokens": ["Es", "ist", "ja", "kei\u00b7ne", "E\u00b7wig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "K\u00fc\u00df ihn ein St\u00fcndlein oder zwei,", "tokens": ["K\u00fc\u00df", "ihn", "ein", "St\u00fcnd\u00b7lein", "o\u00b7der", "zwei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wenn du willst, noch lieber drei.", "tokens": ["Und", "wenn", "du", "willst", ",", "noch", "lie\u00b7ber", "drei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VMFIN", "$,", "ADV", "ADV", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Da stand ein gr\u00fcner Buchenbaum,", "tokens": ["Da", "stand", "ein", "gr\u00fc\u00b7ner", "Bu\u00b7chen\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da hatt' ich einen sch\u00f6nen Traum;", "tokens": ["Da", "hatt'", "ich", "ei\u00b7nen", "sch\u00f6\u00b7nen", "Traum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drei St\u00fcndlein lang, drei St\u00fcndlein breit,", "tokens": ["Drei", "St\u00fcnd\u00b7lein", "lang", ",", "drei", "St\u00fcnd\u00b7lein", "breit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "$,", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und durch und durch voll S\u00fc\u00dfigkeit.", "tokens": ["Und", "durch", "und", "durch", "voll", "S\u00fc\u00b7\u00dfig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "KON", "APPR", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Im gr\u00fcnen Wald der goldne Hahn,", "tokens": ["Im", "gr\u00fc\u00b7nen", "Wald", "der", "gold\u00b7ne", "Hahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der singt und singt, soviel er kann;", "tokens": ["Der", "singt", "und", "singt", ",", "so\u00b7viel", "er", "kann", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "$,", "KOUS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sing' du nur hin, sing' du nur her,", "tokens": ["Sing'", "du", "nur", "hin", ",", "sing'", "du", "nur", "her", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKVZ", "$,", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich f\u00fcrchte mich kein bi\u00dfchen mehr.", "tokens": ["Ich", "f\u00fcrch\u00b7te", "mich", "kein", "bi\u00df\u00b7chen", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}