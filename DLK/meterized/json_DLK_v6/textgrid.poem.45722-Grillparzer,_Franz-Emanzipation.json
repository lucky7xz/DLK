{"textgrid.poem.45722": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Emanzipation", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sp\u00e4t ward man billig eurem Geschlechte,", "tokens": ["Sp\u00e4t", "ward", "man", "bil\u00b7lig", "eu\u00b7rem", "Ge\u00b7schlech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PIS", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das Ha\u00df und Rachsucht mit Schmach beluden,", "tokens": ["Das", "Ha\u00df", "und", "Rach\u00b7sucht", "mit", "Schmach", "be\u00b7lu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ihr habt nun alle B\u00fcrgerrechte,", "tokens": ["Ihr", "habt", "nun", "al\u00b7le", "B\u00fcr\u00b7ger\u00b7rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur freilich bleibt ihr immer Juden.", "tokens": ["Nur", "frei\u00b7lich", "bleibt", "ihr", "im\u00b7mer", "Ju\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}