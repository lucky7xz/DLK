{"textgrid.poem.42458": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf die Terrasse war ich hinbefohlen,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf die Terrasse war ich hinbefohlen,", "tokens": ["Auf", "die", "Ter\u00b7ras\u00b7se", "war", "ich", "hin\u00b7be\u00b7foh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der jugendlichen, sch\u00f6nen, geistvollen,", "tokens": ["Der", "ju\u00b7gend\u00b7li\u00b7chen", ",", "sch\u00f6\u00b7nen", ",", "geist\u00b7vol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Holdseligen Prinzessin vorzulesen.", "tokens": ["Hold\u00b7se\u00b7li\u00b7gen", "Prin\u00b7zes\u00b7sin", "vor\u00b7zu\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVIZU", "$."], "meter": "+---+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ich w\u00e4hlte Tasso.", "tokens": ["Ich", "w\u00e4hl\u00b7te", "Tas\u00b7so", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Durch den Sommerabend", "tokens": ["Durch", "den", "Som\u00b7mer\u00b7a\u00b7bend"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Umschwirrt uns schon das erste Nachtinsekt.", "tokens": ["Um\u00b7schwirrt", "uns", "schon", "das", "ers\u00b7te", "Nach\u00b7tin\u00b7sekt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Sonne war gesunken. Rot Gew\u00f6lk", "tokens": ["Die", "Son\u00b7ne", "war", "ge\u00b7sun\u00b7ken", ".", "Rot", "Ge\u00b7w\u00f6lk"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$.", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Stand hellget\u00f6nt, mit Blau vermischt, im Westen.", "tokens": ["Stand", "hell\u00b7ge\u00b7t\u00f6nt", ",", "mit", "Blau", "ver\u00b7mischt", ",", "im", "Wes\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "APPR", "NN", "VVPP", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der Garten vor uns, tief gelegen, h\u00fcllt", "tokens": ["Der", "Gar\u00b7ten", "vor", "uns", ",", "tief", "ge\u00b7le\u00b7gen", ",", "h\u00fcllt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "ADJD", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Sich ein in dunkle Schatten mehr und mehr.", "tokens": ["Sich", "ein", "in", "dunk\u00b7le", "Schat\u00b7ten", "mehr", "und", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "APPR", "ADJA", "NN", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und eine Nachtigall beginnt.", "tokens": ["Und", "ei\u00b7ne", "Nach\u00b7ti\u00b7gall", "be\u00b7ginnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der Diener", "tokens": ["Der", "Die\u00b7ner"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Setzt auf den Tisch die Lampen, deren Licht", "tokens": ["Setzt", "auf", "den", "Tisch", "die", "Lam\u00b7pen", ",", "de\u00b7ren", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Nicht durch den schw\u00e4chsten Zug ins Flackern kommt.", "tokens": ["Nicht", "durch", "den", "schw\u00e4chs\u00b7ten", "Zug", "ins", "Fla\u00b7ckern", "kommt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Von unten, aus dem Dorfe, klingt Musik,", "tokens": ["Von", "un\u00b7ten", ",", "aus", "dem", "Dor\u00b7fe", ",", "klingt", "Mu\u00b7sik", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "APPR", "ART", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und deutlich aus der Finsternis heraus,", "tokens": ["Und", "deut\u00b7lich", "aus", "der", "Fins\u00b7ter\u00b7nis", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Leuchtstriche, blitzen eines Tanzsaals Fenster.", "tokens": ["Leucht\u00b7stri\u00b7che", ",", "blit\u00b7zen", "ei\u00b7nes", "Tanz\u00b7saals", "Fens\u00b7ter", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Die Paare huschen schnell vorbei in ihnen.", "tokens": ["Die", "Paa\u00b7re", "hu\u00b7schen", "schnell", "vor\u00b7bei", "in", "ih\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Zuweilen, wenn die Th\u00fcr ge\u00f6ffnet steht,", "tokens": ["Zu\u00b7wei\u00b7len", ",", "wenn", "die", "Th\u00fcr", "ge\u00b7\u00f6ff\u00b7net", "steht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Erschallt Gestampf, der Brummba\u00df, Kreischen, Jauchzen.", "tokens": ["Er\u00b7schallt", "Ge\u00b7stampf", ",", "der", "Brumm\u00b7ba\u00df", ",", "Krei\u00b7schen", ",", "Jauch\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "ART", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Unb\u00e4ndig scheint die Freude dort zu herrschen.", "tokens": ["Un\u00b7b\u00e4n\u00b7dig", "scheint", "die", "Freu\u00b7de", "dort", "zu", "herr\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Ich trage unterdessen weiter vor,", "tokens": ["Ich", "tra\u00b7ge", "un\u00b7ter\u00b7des\u00b7sen", "wei\u00b7ter", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Wie fl\u00fcchtige Bilder, unbewu\u00dft, den Trubel", "tokens": ["Wie", "fl\u00fcch\u00b7ti\u00b7ge", "Bil\u00b7der", ",", "un\u00b7be\u00b7wu\u00dft", ",", "den", "Tru\u00b7bel"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "$,", "ADJD", "$,", "ART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Im Thal an mir vor\u00fcberziehen lassend,", "tokens": ["Im", "Thal", "an", "mir", "vor\u00b7\u00fc\u00b7ber\u00b7zie\u00b7hen", "las\u00b7send", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Und jene Verse hab' ich grad getroffen:", "tokens": ["Und", "je\u00b7ne", "Ver\u00b7se", "hab'", "ich", "grad", "ge\u00b7trof\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "\u00bbbeschr\u00e4nkt der Rand des Bechers einen Wein,", "tokens": ["\u00bb", "be\u00b7schr\u00e4nkt", "der", "Rand", "des", "Be\u00b7chers", "ei\u00b7nen", "Wein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "Der sch\u00e4umend wallt und brausend \u00fcberquillt?\u00ab,", "tokens": ["Der", "sch\u00e4u\u00b7mend", "wallt", "und", "brau\u00b7send", "\u00fc\u00b7berq\u00b7uillt", "?", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJD", "VVFIN", "KON", "ADJD", "VVPP", "$.", "$(", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Als ich die Lider hob und die Prinze\u00df,", "tokens": ["Als", "ich", "die", "Li\u00b7der", "hob", "und", "die", "Prin\u00b7ze\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Die s\u00e4umig ihre Linke dem Gel\u00e4nder", "tokens": ["Die", "s\u00e4u\u00b7mig", "ih\u00b7re", "Lin\u00b7ke", "dem", "Ge\u00b7l\u00e4n\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Hin\u00fcber ruhen l\u00e4\u00dft, erblicke, wie sie,", "tokens": ["Hin\u00b7\u00fc\u00b7ber", "ru\u00b7hen", "l\u00e4\u00dft", ",", "er\u00b7bli\u00b7cke", ",", "wie", "sie", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "$,", "VVFIN", "$,", "PWAV", "PPER", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.31": {"text": "Nicht meiner Lesung achtend, niederschaut,", "tokens": ["Nicht", "mei\u00b7ner", "Le\u00b7sung", "ach\u00b7tend", ",", "nie\u00b7der\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Das braune Auge tr\u00e4umerisch, sehns\u00fcchtig", "tokens": ["Das", "brau\u00b7ne", "Au\u00b7ge", "tr\u00e4u\u00b7me\u00b7risch", ",", "sehn\u00b7s\u00fcch\u00b7tig"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Hinuntersendet auf den fr\u00f6hlichen L\u00e4ndler.", "tokens": ["Hin\u00b7un\u00b7ter\u00b7sen\u00b7det", "auf", "den", "fr\u00f6h\u00b7li\u00b7chen", "L\u00e4nd\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbwie w\u00e4r' es, f\u00e4nden wohl Durchlaucht Vergn\u00fcgen,", "tokens": ["\u00bb", "wie", "w\u00e4r'", "es", ",", "f\u00e4n\u00b7den", "wohl", "Durch\u00b7laucht", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "VAFIN", "PPER", "$,", "VVFIN", "ADV", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem frohen Reigen dort sich anzuschlie\u00dfen?\u00ab", "tokens": ["Dem", "fro\u00b7hen", "Rei\u00b7gen", "dort", "sich", "an\u00b7zu\u00b7schlie\u00b7\u00dfen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PRF", "VVIZU", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sie, ein Seufzer: \u00bbAch, ich th\u00e4t's so gern.\u00ab", "tokens": ["Und", "sie", ",", "ein", "Seuf\u00b7zer", ":", "\u00bb", "Ach", ",", "ich", "th\u00e4t's", "so", "gern", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "$.", "$(", "ITJ", "$,", "PPER", "VVFIN", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wenn ich's nur bringen k\u00f6nnte, wiedergeben,", "tokens": ["Wenn", "ich's", "nur", "brin\u00b7gen", "k\u00f6nn\u00b7te", ",", "wie\u00b7der\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVINF", "VMFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie jenes Wort von ihr gesprochen ward,", "tokens": ["Wie", "je\u00b7nes", "Wort", "von", "ihr", "ge\u00b7spro\u00b7chen", "ward", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "APPR", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das \u00bbso\u00ab, das \u00bbgern\u00ab, wenn ich's nur treffen k\u00f6nnte,", "tokens": ["Das", "\u00bb", "so", "\u00ab", ",", "das", "\u00bb", "gern", "\u00ab", ",", "wenn", "ich's", "nur", "tref\u00b7fen", "k\u00f6nn\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ADV", "$(", "$,", "PRELS", "$(", "ADV", "$(", "$,", "KOUS", "PIS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}