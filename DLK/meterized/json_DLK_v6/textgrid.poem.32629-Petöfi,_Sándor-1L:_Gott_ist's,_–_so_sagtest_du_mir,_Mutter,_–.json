{"textgrid.poem.32629": {"metadata": {"author": {"name": "Pet\u00f6fi, S\u00e1ndor", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gott ist's, \u2013 so sagtest du mir, Mutter, \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott ist's, \u2013 so sagtest du mir, Mutter, \u2013", "tokens": ["Gott", "ist's", ",", "\u2013", "so", "sag\u00b7test", "du", "mir", ",", "Mut\u00b7ter", ",", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "$,", "$(", "ADV", "VVFIN", "PPER", "PPER", "$,", "NN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der n\u00e4chtlich unsre Tr\u00e4ume schm\u00fcckt,", "tokens": ["Der", "n\u00e4cht\u00b7lich", "uns\u00b7re", "Tr\u00e4u\u00b7me", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Damit im Traum, wie durch ein Fenster,", "tokens": ["Da\u00b7mit", "im", "Traum", ",", "wie", "durch", "ein", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPRART", "NN", "$,", "PWAV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Bild der Zukunft man erblickt.", "tokens": ["Das", "Bild", "der", "Zu\u00b7kunft", "man", "er\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun tr\u00e4umte mir. Erkl\u00e4re, Mutter", "tokens": ["Nun", "tr\u00e4um\u00b7te", "mir", ".", "Er\u00b7kl\u00e4\u00b7re", ",", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir, was bedeutet wohl mein Traum?", "tokens": ["Mir", ",", "was", "be\u00b7deu\u00b7tet", "wohl", "mein", "Traum", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir wuchsen Schwingen, da durchflog ich", "tokens": ["Mir", "wuch\u00b7sen", "Schwin\u00b7gen", ",", "da", "durch\u00b7flog", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Luft, \u2013 ein Ende nahm es kaum!", "tokens": ["Die", "Luft", ",", "\u2013", "ein", "En\u00b7de", "nahm", "es", "kaum", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "$(", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbmein Kind, der Traum bedeutet Gutes,", "tokens": ["\u00bb", "mein", "Kind", ",", "der", "Traum", "be\u00b7deu\u00b7tet", "Gu\u00b7tes", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein froh Verhei\u00dfen schlie\u00dft er ein:", "tokens": ["Ein", "froh", "Ver\u00b7hei\u00b7\u00dfen", "schlie\u00dft", "er", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er k\u00fcndet dir ein langes Leben,", "tokens": ["Er", "k\u00fcn\u00b7det", "dir", "ein", "lan\u00b7ges", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Gl\u00fcck wird dir beschieden sein!\u00ab ...", "tokens": ["Und", "Gl\u00fcck", "wird", "dir", "be\u00b7schie\u00b7den", "sein", "!", "\u00ab", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "VVPP", "VAINF", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Knabe wuchs heran, und Flammen", "tokens": ["Der", "Kna\u00b7be", "wuchs", "he\u00b7ran", ",", "und", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durchgl\u00fchten bald des J\u00fcnglings Brust,", "tokens": ["Durch\u00b7gl\u00fch\u00b7ten", "bald", "des", "J\u00fcng\u00b7lings", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und drohten die ihn zu verzehren,", "tokens": ["Und", "droh\u00b7ten", "die", "ihn", "zu", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}}, "stanza.5": {"line.1": {"text": "Die Laute schlie\u00dft er in die Arme,", "tokens": ["Die", "Lau\u00b7te", "schlie\u00dft", "er", "in", "die", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die alles, was sein Herz bewegt,", "tokens": ["Die", "al\u00b7les", ",", "was", "sein", "Herz", "be\u00b7wegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf goldnen Fl\u00fcgeln des Gesanges", "tokens": ["Auf", "gold\u00b7nen", "Fl\u00fc\u00b7geln", "des", "Ge\u00b7san\u00b7ges"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hinaus in alle Welten tr\u00e4gt.", "tokens": ["Hin\u00b7aus", "in", "al\u00b7le", "Wel\u00b7ten", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Gen Himmel flogen seine Lieder,", "tokens": ["Gen", "Him\u00b7mel", "flo\u00b7gen", "sei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie holten ihm des Ruhmes Glanz,", "tokens": ["Sie", "hol\u00b7ten", "ihm", "des", "Ruh\u00b7mes", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wanden aus der Sonne Strahlen", "tokens": ["Und", "wan\u00b7den", "aus", "der", "Son\u00b7ne", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm um die Stirn' den Dichterkranz.", "tokens": ["Ihm", "um", "die", "Stirn'", "den", "Dich\u00b7ter\u00b7kranz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch ist der Honigseim des Liedes", "tokens": ["Doch", "ist", "der", "Ho\u00b7ni\u00b7gseim", "des", "Lie\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Gift von t\u00f6tender Gewalt,", "tokens": ["Ein", "Gift", "von", "t\u00f6\u00b7ten\u00b7der", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und jedes Lied, mit einem Tage", "tokens": ["Und", "je\u00b7des", "Lied", ",", "mit", "ei\u00b7nem", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des eignen Lebens wird's bezahlt!", "tokens": ["Des", "eig\u00b7nen", "Le\u00b7bens", "wird's", "be\u00b7zahlt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Zur H\u00f6llenglut ward ihm die Flamme", "tokens": ["Zur", "H\u00f6l\u00b7len\u00b7glut", "ward", "ihm", "die", "Flam\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und als ihr Opfer sank er hin,", "tokens": ["Und", "als", "ihr", "Op\u00b7fer", "sank", "er", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir sehn am Baume seines Lebens", "tokens": ["Wir", "sehn", "am", "Bau\u00b7me", "sei\u00b7nes", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das letzte Zweiglein still verbl\u00fchn! ...", "tokens": ["Das", "letz\u00b7te", "Zwei\u00b7glein", "still", "ver\u00b7bl\u00fchn", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Nun liegt er auf dem Sterbebette.", "tokens": ["Nun", "liegt", "er", "auf", "dem", "Ster\u00b7be\u00b7bet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bald ist's vorbei! \u2013 Die Mutter steht", "tokens": ["Bald", "ist's", "vor\u00b7bei", "!", "\u2013", "Die", "Mut\u00b7ter", "steht"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PTKVZ", "$.", "$(", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor ihm, mit Tr\u00e4nen in den Augen,", "tokens": ["Vor", "ihm", ",", "mit", "Tr\u00e4\u00b7nen", "in", "den", "Au\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Durchw\u00fchlt von Gram; sie klagt und fleht:", "tokens": ["Durch\u00b7w\u00fchlt", "von", "Gram", ";", "sie", "klagt", "und", "fleht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "$.", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "\u00bbo Tod! Du willst ihn mir entrei\u00dfen \u2013", "tokens": ["\u00bb", "o", "Tod", "!", "Du", "willst", "ihn", "mir", "ent\u00b7rei\u00b7\u00dfen", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$.", "PPER", "VMFIN", "PPER", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Gott selbst verhie\u00df ihm langes Leben,", "tokens": ["Gott", "selbst", "ver\u00b7hie\u00df", "ihm", "lan\u00b7ges", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und kann denn l\u00fcgen auch ein Traum?\u00ab", "tokens": ["Und", "kann", "denn", "l\u00fc\u00b7gen", "auch", "ein", "Traum", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VMFIN", "ADV", "VVFIN", "ADV", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "\u00bbdie Tr\u00e4ume, Mutter, l\u00fcgen nimmer, \u2013", "tokens": ["\u00bb", "die", "Tr\u00e4u\u00b7me", ",", "Mut\u00b7ter", ",", "l\u00fc\u00b7gen", "nim\u00b7mer", ",", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "$,", "NN", "$,", "VVFIN", "ADV", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denn, wenn man jetzt mich auch begr\u00e4bt,", "tokens": ["Denn", ",", "wenn", "man", "jetzt", "mich", "auch", "be\u00b7gr\u00e4bt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PIS", "ADV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Sohn z\u00e4hlt niemals zu den Toten,", "tokens": ["Dein", "Sohn", "z\u00e4hlt", "nie\u00b7mals", "zu", "den", "To\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil ruhmbegl\u00e4nzt sein Name lebt!\u00ab", "tokens": ["Weil", "ruhm\u00b7be\u00b7gl\u00e4nzt", "sein", "Na\u00b7me", "lebt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "VVFIN", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}