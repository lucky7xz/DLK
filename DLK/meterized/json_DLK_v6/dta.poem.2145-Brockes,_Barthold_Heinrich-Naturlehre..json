{"dta.poem.2145": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Naturlehre.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die\u00df wird von der Natur gelehrt:", "tokens": ["Die\u00df", "wird", "von", "der", "Na\u00b7tur", "ge\u00b7lehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Mensch mu\u00df im Gesch\u00f6pf den Sch\u00f6pfer, und", "tokens": ["Der", "Mensch", "mu\u00df", "im", "Ge\u00b7sch\u00f6pf", "den", "Sch\u00f6p\u00b7fer", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VMFIN", "APPRART", "NN", "ART", "NN", "$,", "KON"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gott in seinen Werken ehren,", "tokens": ["Gott", "in", "sei\u00b7nen", "Wer\u00b7ken", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und so, von der Vernunft erkl\u00e4rt:", "tokens": ["Und", "so", ",", "von", "der", "Ver\u00b7nunft", "er\u00b7kl\u00e4rt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df, unter g\u00f6ttlichen Gesch\u00f6pfen, auch alle Menschen", "tokens": ["Da\u00df", ",", "un\u00b7ter", "g\u00f6tt\u00b7li\u00b7chen", "Ge\u00b7sch\u00f6p\u00b7fen", ",", "auch", "al\u00b7le", "Men\u00b7schen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "$,", "APPR", "ADJA", "NN", "$,", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "mit geh\u00f6ren.", "tokens": ["mit", "ge\u00b7h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Wir sehn aus dieser kurzen Lehre des gro\u00dfen Sch\u00f6pfers", "tokens": ["Wir", "sehn", "aus", "die\u00b7ser", "kur\u00b7zen", "Leh\u00b7re", "des", "gro\u00b7\u00dfen", "Sch\u00f6p\u00b7fers"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "gn\u00e4digs Wollen,", "tokens": ["gn\u00e4\u00b7digs", "Wol\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Da\u00df wir, in Werken, ihn bewundern, und unsern N\u00e4ch-", "tokens": ["Da\u00df", "wir", ",", "in", "Wer\u00b7ken", ",", "ihn", "be\u00b7wun\u00b7dern", ",", "und", "un\u00b7sern", "N\u00e4ch"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "NN", "$,", "PPER", "VVINF", "$,", "KON", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "sten lieben sollen.", "tokens": ["sten", "lie\u00b7ben", "sol\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "VVINF", "VMFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}