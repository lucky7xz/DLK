{"dta.poem.16157": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "211.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Oft war ich so gebeugt, wenn alles mir gegl\u00fcckt,", "tokens": ["Oft", "war", "ich", "so", "ge\u00b7beugt", ",", "wenn", "al\u00b7les", "mir", "ge\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,", "KOUS", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und so erhoben oft, wenn alles mich gedr\u00fcckt.", "tokens": ["Und", "so", "er\u00b7ho\u00b7ben", "oft", ",", "wenn", "al\u00b7les", "mich", "ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "$,", "KOUS", "PIS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Aus etwas anderm als Gelingen und Mislingen", "tokens": ["Aus", "et\u00b7was", "an\u00b7derm", "als", "Ge\u00b7lin\u00b7gen", "und", "Mis\u00b7lin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PIS", "KOKOM", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Au\u00dfendinge mu\u00df mein Wohl und Weh entspringen.", "tokens": ["Der", "Au\u00b7\u00dfen\u00b7din\u00b7ge", "mu\u00df", "mein", "Wohl", "und", "Weh", "ent\u00b7sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexan