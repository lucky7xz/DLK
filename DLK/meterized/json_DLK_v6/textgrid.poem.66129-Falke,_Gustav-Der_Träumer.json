{"textgrid.poem.66129": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der Tr\u00e4umer", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich, du und die mich schelten,", "tokens": ["Ich", ",", "du", "und", "die", "mich", "schel\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "KON", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sind Bl\u00fcten an Einem Baum,", "tokens": ["Sind", "Bl\u00fc\u00b7ten", "an", "Ei\u00b7nem", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gott und die rollenden Welten,", "tokens": ["Gott", "und", "die", "rol\u00b7len\u00b7den", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Wir alle sind Ein Traum.", "tokens": ["Wir", "al\u00b7le", "sind", "Ein", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr scheltet meine Tr\u00e4ume,", "tokens": ["Ihr", "schel\u00b7tet", "mei\u00b7ne", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn auch mit mildem Wort,", "tokens": ["Wenn", "auch", "mit", "mil\u00b7dem", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich das Hier vers\u00e4ume", "tokens": ["Da\u00df", "ich", "das", "Hier", "ver\u00b7s\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDS", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Um ein erdichtetes Dort.", "tokens": ["Um", "ein", "er\u00b7dich\u00b7te\u00b7tes", "Dort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Wohl bleib ich fern den Toren,", "tokens": ["Wohl", "bleib", "ich", "fern", "den", "To\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was auch ihr Tun beginnt,", "tokens": ["Was", "auch", "ihr", "Tun", "be\u00b7ginnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die da nach Quellen bohren,", "tokens": ["Die", "da", "nach", "Quel\u00b7len", "boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo keine Quelle rinnt.", "tokens": ["Wo", "kei\u00b7ne", "Quel\u00b7le", "rinnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich suche mir das Wasser,", "tokens": ["Ich", "su\u00b7che", "mir", "das", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dessen meine Seele bedarf,", "tokens": ["Des\u00b7sen", "mei\u00b7ne", "See\u00b7le", "be\u00b7darf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Den Quell, in den kein Hasser,", "tokens": ["Den", "Quell", ",", "in", "den", "kein", "Has\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein Neidling Steine warf.", "tokens": ["Kein", "Neid\u00b7ling", "Stei\u00b7ne", "warf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und meine Eimer steigen", "tokens": ["Und", "mei\u00b7ne", "Ei\u00b7mer", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hinab, herauf in Ruh,", "tokens": ["Hin\u00b7ab", ",", "her\u00b7auf", "in", "Ruh", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Tiefe wird mein Eigen,", "tokens": ["Die", "Tie\u00b7fe", "wird", "mein", "Ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Leben flie\u00dft Leben zu.", "tokens": ["Le\u00b7ben", "flie\u00dft", "Le\u00b7ben", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Und wenn es steigt und flutet", "tokens": ["Und", "wenn", "es", "steigt", "und", "flu\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und f\u00fcllt die Seele ganz,", "tokens": ["Und", "f\u00fcllt", "die", "See\u00b7le", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und auf der F\u00fclle glutet", "tokens": ["Und", "auf", "der", "F\u00fcl\u00b7le", "glu\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von oben her ein Glanz \u2013", "tokens": ["Von", "o\u00b7ben", "her", "ein", "Glanz", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APZR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da hebt von selbst zu t\u00f6nen", "tokens": ["Da", "hebt", "von", "selbst", "zu", "t\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die volle Tiefe an,", "tokens": ["Die", "vol\u00b7le", "Tie\u00b7fe", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das la\u00df ich mir nicht h\u00f6hnen,", "tokens": ["Das", "la\u00df", "ich", "mir", "nicht", "h\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Meine Seligkeit h\u00e4ngt daran.", "tokens": ["Mei\u00b7ne", "Se\u00b7lig\u00b7keit", "h\u00e4ngt", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PAV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "Wollt ihr um andres schm\u00e4len,", "tokens": ["Wollt", "ihr", "um", "and\u00b7res", "schm\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da l\u00e4chle ich nur still,", "tokens": ["Da", "l\u00e4ch\u00b7le", "ich", "nur", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mag jeder sein R\u00f6\u00dflein w\u00e4hlen", "tokens": ["Mag", "je\u00b7der", "sein", "R\u00f6\u00df\u00b7lein", "w\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "PPOSAT", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und reiten, wie er will.", "tokens": ["Und", "rei\u00b7ten", ",", "wie", "er", "will", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Sitz er nur fest im B\u00fcgel", "tokens": ["Sitz", "er", "nur", "fest", "im", "B\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "ADJD", "APPRART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Und wisse, wohin es geht:", "tokens": ["Und", "wis\u00b7se", ",", "wo\u00b7hin", "es", "geht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nach einem kleinen H\u00fcgel,", "tokens": ["Nach", "ei\u00b7nem", "klei\u00b7nen", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dar\u00fcber Vergessen weht.", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "Ver\u00b7ges\u00b7sen", "weht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Genug, wenn eine Platte", "tokens": ["Ge\u00b7nug", ",", "wenn", "ei\u00b7ne", "Plat\u00b7te"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit einem Spr\u00fcchlein drin", "tokens": ["Mit", "ei\u00b7nem", "Spr\u00fcch\u00b7lein", "drin"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Grab mir deckt: Er hatte", "tokens": ["Das", "Grab", "mir", "deckt", ":", "Er", "hat\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Herz und gab es hin.", "tokens": ["Ein", "Herz", "und", "gab", "es", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}