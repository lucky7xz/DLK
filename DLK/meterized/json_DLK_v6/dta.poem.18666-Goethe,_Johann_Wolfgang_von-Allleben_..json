{"dta.poem.18666": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Allleben .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Staub ist eins der Elemente", "tokens": ["Staub", "ist", "eins", "der", "E\u00b7le\u00b7men\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PIS", "ART", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Das du gar geschickt bezwingest", "tokens": ["Das", "du", "gar", "ge\u00b7schickt", "be\u00b7zwin\u00b7gest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hafis, wenn zu Liebchens Ehren,", "tokens": ["Ha\u00b7fis", ",", "wenn", "zu", "Lieb\u00b7chens", "Eh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du ein zierlich Liedchen singest.", "tokens": ["Du", "ein", "zier\u00b7lich", "Lied\u00b7chen", "sin\u00b7gest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn der Staub auf ihrer Schwelle", "tokens": ["Denn", "der", "Staub", "auf", "ih\u00b7rer", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist dem Teppich vorzuziehen,", "tokens": ["Ist", "dem", "Tep\u00b7pich", "vor\u00b7zu\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dessen goldgewirkte Blumen", "tokens": ["Des\u00b7sen", "gold\u00b7ge\u00b7wirk\u00b7te", "Blu\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mahmuds G\u00fcnstlinge beknieen.", "tokens": ["Mah\u00b7muds", "G\u00fcnst\u00b7lin\u00b7ge", "be\u00b7kni\u00b7e\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Treibt der Wind von ihrer Pforte", "tokens": ["Treibt", "der", "Wind", "von", "ih\u00b7rer", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wolken Staubs behend vor\u00fcber,", "tokens": ["Wol\u00b7ken", "Staubs", "be\u00b7hend", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mehr als Moschus sind die D\u00fcfte", "tokens": ["Mehr", "als", "Mo\u00b7schus", "sind", "die", "D\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "KOKOM", "NE", "VAFIN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Und als Rosen\u00f6l dir lieber.", "tokens": ["Und", "als", "Ro\u00b7sen\u00b7\u00f6l", "dir", "lie\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Staub den hab\u2019 ich l\u00e4ngst entbehret", "tokens": ["Staub", "den", "hab'", "ich", "l\u00e4ngst", "ent\u00b7beh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "VAFIN", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem stets umh\u00fcllten Norden,", "tokens": ["In", "dem", "stets", "um\u00b7h\u00fcll\u00b7ten", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber in dem heissen S\u00fcden", "tokens": ["A\u00b7ber", "in", "dem", "heis\u00b7sen", "S\u00fc\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist er mir genugsam worden.", "tokens": ["Ist", "er", "mir", "ge\u00b7nug\u00b7sam", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADJD", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch schon l\u00e4ngst das liebe Pforten", "tokens": ["Doch", "schon", "l\u00e4ngst", "das", "lie\u00b7be", "Pfor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir auf ihren Angeln schwiegen!", "tokens": ["Mir", "auf", "ih\u00b7ren", "An\u00b7geln", "schwie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Heile mich Gewitterregen,", "tokens": ["Hei\u00b7le", "mich", "Ge\u00b7wit\u00b7ter\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lass mich dass es grunelt riechen!", "tokens": ["Lass", "mich", "dass", "es", "gru\u00b7nelt", "rie\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KOUS", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wenn jetzt alle Donner rollen", "tokens": ["Wenn", "jetzt", "al\u00b7le", "Don\u00b7ner", "rol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der ganze Himmel leuchtet,", "tokens": ["Und", "der", "gan\u00b7ze", "Him\u00b7mel", "leuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird der wilde Staub des Windes", "tokens": ["Wird", "der", "wil\u00b7de", "Staub", "des", "Win\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach dem Boden hingefeuchtet.", "tokens": ["Nach", "dem", "Bo\u00b7den", "hin\u00b7ge\u00b7feuch\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und sogleich entspringt ein Leben,", "tokens": ["Und", "sog\u00b7leich", "ent\u00b7springt", "ein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schwillt ein heilig, heimlich Wirken,", "tokens": ["Schwillt", "ein", "hei\u00b7lig", ",", "heim\u00b7lich", "Wir\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "$,", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es grunelt und es gr\u00fcnet", "tokens": ["Und", "es", "gru\u00b7nelt", "und", "es", "gr\u00fc\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KON", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den irdischen Bezirken.", "tokens": ["In", "den", "ir\u00b7di\u00b7schen", "Be\u00b7zir\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}