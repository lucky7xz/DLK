{"dta.poem.1091": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "\u03a6.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Al\u00df lang mir dein Hertz war kund/", "tokens": ["Al\u00df", "lang", "mir", "dein", "Hertz", "war", "kund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "PPOSAT", "NN", "VAFIN", "PTKVZ", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das sein s\u00fcsser Rosen-mund", "tokens": ["Das", "sein", "s\u00fcs\u00b7ser", "Ro\u00b7sen\u00b7mund"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Niemand dan mich wolt erlaben;", "tokens": ["Nie\u00b7mand", "dan", "mich", "wolt", "er\u00b7la\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "War ich so seelig vnd reich/", "tokens": ["War", "ich", "so", "see\u00b7lig", "vnd", "reich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Das ich K\u00f6nig in Franckreich", "tokens": ["Das", "ich", "K\u00f6\u00b7nig", "in", "Fran\u00b7ck\u00b7reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "NN", "APPR", "NE"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Zusein nicht gew\u00fcnscht wolt haben.", "tokens": ["Zu\u00b7sein", "nicht", "ge\u00b7w\u00fcnscht", "wolt", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVPP", "VMFIN", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}