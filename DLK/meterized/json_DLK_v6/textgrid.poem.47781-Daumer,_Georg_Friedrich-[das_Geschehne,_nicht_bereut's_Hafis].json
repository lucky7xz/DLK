{"textgrid.poem.47781": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[das Geschehne, nicht bereut's Hafis]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Geschehne, nicht bereut's Hafis,", "tokens": ["Das", "Ge\u00b7scheh\u00b7ne", ",", "nicht", "be\u00b7reut's", "Ha\u00b7fis", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKNEG", "NE", "NE", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Er bedauert, was er unterlie\u00df.", "tokens": ["Er", "be\u00b7dau\u00b7ert", ",", "was", "er", "un\u00b7ter\u00b7lie\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Schmerzlich ist ihm jeglicher Moment,", "tokens": ["Schmerz\u00b7lich", "ist", "ihm", "jeg\u00b7li\u00b7cher", "Mo\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Den er in ascetischem Verlies,", "tokens": ["Den", "er", "in", "as\u00b7ce\u00b7ti\u00b7schem", "Ver\u00b7lies", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Den er nicht bei'm Klange des Pokals,", "tokens": ["Den", "er", "nicht", "bei'm", "Klan\u00b7ge", "des", "Po\u00b7kals", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Den er nicht im Glanze des Genies,", "tokens": ["Den", "er", "nicht", "im", "Glan\u00b7ze", "des", "Ge\u00b7nies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Den er nicht an holder Liebe Brust,", "tokens": ["Den", "er", "nicht", "an", "hol\u00b7der", "Lie\u00b7be", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Den er nicht gelebt im Paradies.", "tokens": ["Den", "er", "nicht", "ge\u00b7lebt", "im", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VVPP", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}