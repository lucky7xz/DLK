{"dta.poem.17257": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "107.  Die n\u00fctzlichsten Gedancken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.71", "af:0.28"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "D\u00e4nk an den Tod/ mein Krist: was d\u00e4nkstu anders viel?", "tokens": ["D\u00e4nk", "an", "den", "Tod", "/", "mein", "Krist", ":", "was", "d\u00e4nks\u00b7tu", "an\u00b7ders", "viel", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$(", "PPOSAT", "NN", "$.", "PWS", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man d\u00e4nkt nichts n\u00fctzlichers als wie man sterben wil.", "tokens": ["Man", "d\u00e4nkt", "nichts", "n\u00fctz\u00b7li\u00b7chers", "als", "wie", "man", "ster\u00b7ben", "wil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIS", "ADV", "KOUS", "PWAV", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}