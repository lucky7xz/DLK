{"dta.poem.19945": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Anna V\u00f6gtly .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.56", "da:0.43"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo dem Spalt geborstner Felsen", "tokens": ["Wo", "dem", "Spalt", "ge\u00b7borst\u00b7ner", "Fel\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In endloser Wildni\u00df Grausen,", "tokens": ["In", "end\u00b7lo\u00b7ser", "Wild\u00b7ni\u00df", "Grau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Recht wie aus der H\u00f6lle Grund", "tokens": ["Recht", "wie", "aus", "der", "H\u00f6l\u00b7le", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hei\u00dfe Wasser wild entbrausen.", "tokens": ["Hei\u00b7\u00dfe", "Was\u00b7ser", "wild", "ent\u00b7brau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aus dem alten Born zu Pfeffers", "tokens": ["Aus", "dem", "al\u00b7ten", "Born", "zu", "Pfef\u00b7fers"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NE", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hob sich oft des Abgrunds Meister,", "tokens": ["Hob", "sich", "oft", "des", "Ab\u00b7grunds", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Warb zu seiner H\u00f6lle Dienst", "tokens": ["Warb", "zu", "sei\u00b7ner", "H\u00f6l\u00b7le", "Dienst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Listig s\u00fcnd'ger Menschen Geister.", "tokens": ["Lis\u00b7tig", "s\u00fcn\u00b7d'\u00b7ger", "Men\u00b7schen", "Geis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Anna V\u00f6gtly! Anna V\u00f6gtly!", "tokens": ["An\u00b7na", "V\u00f6gt\u00b7ly", "!", "An\u00b7na", "V\u00f6gt\u00b7ly", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "NE", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wahre fest dein s\u00fcnd'ges Herze!", "tokens": ["Wah\u00b7re", "fest", "dein", "s\u00fcn\u00b7d'\u00b7ges", "Her\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VVFIN", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Geh' nicht Zauberkr\u00e4uter suchend", "tokens": ["Geh'", "nicht", "Zau\u00b7ber\u00b7kr\u00e4u\u00b7ter", "su\u00b7chend"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PTKNEG", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mitternachts mit mag'scher Kerze!", "tokens": ["Mit\u00b7ter\u00b7nachts", "mit", "mag'\u00b7scher", "Ker\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ja! bey solchem H\u00f6llenspiel", "tokens": ["Ja", "!", "bey", "sol\u00b7chem", "H\u00f6l\u00b7len\u00b7spiel"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$.", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist er keck vor dich getreten.", "tokens": ["Ist", "er", "keck", "vor", "dich", "ge\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Anna V\u00f6gtly! Anna V\u00f6gtly!", "tokens": ["An\u00b7na", "V\u00f6gt\u00b7ly", "!", "An\u00b7na", "V\u00f6gt\u00b7ly", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "NE", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lehrte Mutter dich nicht beten?", "tokens": ["Lehr\u00b7te", "Mut\u00b7ter", "dich", "nicht", "be\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Durch den Graus der Mitternacht", "tokens": ["Durch", "den", "Graus", "der", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bist du leuchtend vorgeschritten,", "tokens": ["Bist", "du", "leuch\u00b7tend", "vor\u00b7ge\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Raubtest, weh! den heil'gen Leib", "tokens": ["Raub\u00b7test", ",", "weh", "!", "den", "heil'\u00b7gen", "Leib"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "PTKVZ", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus der Waldkapelle Mitten.", "tokens": ["Aus", "der", "Wald\u00b7ka\u00b7pel\u00b7le", "Mit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wild Gel\u00e4chter man vernommen,", "tokens": ["Wild", "Ge\u00b7l\u00e4ch\u00b7ter", "man", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PIS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ries'ge Felsen wiederhallten,", "tokens": ["Ries'\u00b7ge", "Fel\u00b7sen", "wie\u00b7der\u00b7hall\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6ll'sche Masken, scheuslich grinsend", "tokens": ["H\u00f6ll'\u00b7sche", "Mas\u00b7ken", ",", "scheus\u00b7lich", "grin\u00b7send"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Funkelten aus ihren Spalten.", "tokens": ["Fun\u00b7kel\u00b7ten", "aus", "ih\u00b7ren", "Spal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}}, "stanza.7": {"line.1": {"text": "B\u00e4ume schwankten auf und nieder,", "tokens": ["B\u00e4u\u00b7me", "schwank\u00b7ten", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aechzend wie von Sturmes Zorne,", "tokens": ["A\u00b7ech\u00b7zend", "wie", "von", "Stur\u00b7mes", "Zor\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und die Hostie wirfst du zitternd", "tokens": ["Und", "die", "Hos\u00b7tie", "wirfst", "du", "zit\u00b7ternd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "VVPP"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "In der grausen Wildni\u00df Dorne.", "tokens": ["In", "der", "grau\u00b7sen", "Wild\u00b7ni\u00df", "Dor\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Eine Rose silberhelle", "tokens": ["Ei\u00b7ne", "Ro\u00b7se", "sil\u00b7ber\u00b7hel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist sogleich empor gesprossen.", "tokens": ["Ist", "sog\u00b7leich", "em\u00b7por", "ge\u00b7spros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKVZ", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Solche h\u00e4lt mit sieben Bl\u00e4ttern", "tokens": ["Sol\u00b7che", "h\u00e4lt", "mit", "sie\u00b7ben", "Bl\u00e4t\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "VVFIN", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fest das Heiligthum umschlossen.", "tokens": ["Fest", "das", "Hei\u00b7lig\u00b7thum", "um\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Als der N\u00e4chte Graus verschwunden,", "tokens": ["Als", "der", "N\u00e4ch\u00b7te", "Graus", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Goldne Tagesstrahlen siegten,", "tokens": ["Gold\u00b7ne", "Ta\u00b7ges\u00b7strah\u00b7len", "sieg\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "V\u00f6gel sich auf schlankem Zweig", "tokens": ["V\u00f6\u00b7gel", "sich", "auf", "schlan\u00b7kem", "Zweig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Singend \u00fcber'm Abgrund wiegten.", "tokens": ["Sin\u00b7gend", "\u00fc\u00b7ber'm", "Ab\u00b7grund", "wieg\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Eine Sch\u00e4f'rinn f\u00e4hrt zu Thal,", "tokens": ["Ei\u00b7ne", "Sch\u00e4f'\u00b7rinn", "f\u00e4hrt", "zu", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaut der Silberrose Funkel", "tokens": ["Schaut", "der", "Sil\u00b7ber\u00b7ro\u00b7se", "Fun\u00b7kel"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie spricht: f\u00fcrwahr ein Stern", "tokens": ["Und", "sie", "spricht", ":", "f\u00fcr\u00b7wahr", "ein", "Stern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Blieb in dieses Waldes Dunkel!", "tokens": ["Blieb", "in", "die\u00b7ses", "Wal\u00b7des", "Dun\u00b7kel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ihre treue Sch\u00e4flein z\u00f6gern", "tokens": ["Ih\u00b7re", "treu\u00b7e", "Sch\u00e4f\u00b7lein", "z\u00f6\u00b7gern"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An den nahen Born zu gehen,", "tokens": ["An", "den", "na\u00b7hen", "Born", "zu", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Neigen alle sich zur Erd'", "tokens": ["Nei\u00b7gen", "al\u00b7le", "sich", "zur", "Erd'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PIAT", "PRF", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als so seel'gen Glanz sie sehen.", "tokens": ["Als", "so", "seel'\u00b7gen", "Glanz", "sie", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Aufgewacht vom Felsenlager", "tokens": ["Auf\u00b7ge\u00b7wacht", "vom", "Fel\u00b7sen\u00b7la\u00b7ger"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt ein gir'ger Wolf geschritten,", "tokens": ["Kommt", "ein", "gir'\u00b7ger", "Wolf", "ge\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NE", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Sieht der Gottesblume Licht,", "tokens": ["Sieht", "der", "Got\u00b7tes\u00b7blu\u00b7me", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Legt sich in der Sch\u00e4flein Mitten.", "tokens": ["Legt", "sich", "in", "der", "Sch\u00e4f\u00b7lein", "Mit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Und die Hirtin thut es kund,", "tokens": ["Und", "die", "Hir\u00b7tin", "thut", "es", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Volk und Priester eilt zur Stelle,", "tokens": ["Volk", "und", "Pries\u00b7ter", "eilt", "zur", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Pflanzen diese Gottesblume", "tokens": ["Pflan\u00b7zen", "die\u00b7se", "Got\u00b7tes\u00b7blu\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["NN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf den Altar der Kapelle.", "tokens": ["Auf", "den", "Al\u00b7tar", "der", "Ka\u00b7pel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.14": {"line.1": {"text": "Helle Glocken, Prei\u00dfges\u00e4nge", "tokens": ["Hel\u00b7le", "Glo\u00b7cken", ",", "Prei\u00df\u00b7ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "punct", "word"], "pos": ["NE", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hallen durch die Waldesstille,", "tokens": ["Hal\u00b7len", "durch", "die", "Wal\u00b7des\u00b7stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ueber Land und Meere zieh'n", "tokens": ["Ue\u00b7ber", "Land", "und", "Mee\u00b7re", "zieh'n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fromme Pilgrime die F\u00fclle.", "tokens": ["From\u00b7me", "Pil\u00b7gri\u00b7me", "die", "F\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}}, "stanza.15": {"line.1": {"text": "Ettiswyl, nennt sich die St\u00e4tte,", "tokens": ["Et\u00b7tis\u00b7wyl", ",", "nennt", "sich", "die", "St\u00e4t\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Wo in dunkler Waldkapelle", "tokens": ["Wo", "in", "dunk\u00b7ler", "Wald\u00b7ka\u00b7pel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jene Gottesblum' erbl\u00fcht", "tokens": ["Je\u00b7ne", "Got\u00b7tes\u00b7blum'", "er\u00b7bl\u00fcht"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Silbern mit des Mondeshelle.", "tokens": ["Sil\u00b7bern", "mit", "des", "Mon\u