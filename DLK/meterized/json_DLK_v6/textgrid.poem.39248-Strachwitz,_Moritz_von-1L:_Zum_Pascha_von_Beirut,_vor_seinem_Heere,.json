{"textgrid.poem.39248": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zum Pascha von Beirut, vor seinem Heere,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum Pascha von Beirut, vor seinem Heere,", "tokens": ["Zum", "Pa\u00b7scha", "von", "Bei\u00b7rut", ",", "vor", "sei\u00b7nem", "Hee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als just die Schaufel hielt der B\u00fcgelhalter,", "tokens": ["Als", "just", "die", "Schau\u00b7fel", "hielt", "der", "B\u00fc\u00b7gel\u00b7hal\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Her trat ein Araber vom Roten Meere.", "tokens": ["Her", "trat", "ein", "A\u00b7ra\u00b7ber", "vom", "Ro\u00b7ten", "Mee\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+---+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Es war ein brauner Scheich in r\u00fcst'gem Alter,", "tokens": ["Es", "war", "ein", "brau\u00b7ner", "Scheich", "in", "r\u00fcst'\u00b7gem", "Al\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wei\u00df war sein Kleid, an dunkelroter Schleife", "tokens": ["Wei\u00df", "war", "sein", "Kleid", ",", "an", "dun\u00b7kel\u00b7ro\u00b7ter", "Schlei\u00b7fe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da hing sein Perserschwert, sein Turbanspalter.", "tokens": ["Da", "hing", "sein", "Per\u00b7ser\u00b7schwert", ",", "sein", "Tur\u00b7ban\u00b7spal\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es flo\u00df der Bart in vollster Schw\u00e4rz' und Reife", "tokens": ["Es", "flo\u00df", "der", "Bart", "in", "volls\u00b7ter", "Schw\u00e4rz'", "und", "Rei\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf unsres Emirs bronzebraune B\u00fcste,", "tokens": ["Auf", "uns\u00b7res", "E\u00b7mirs", "bron\u00b7ze\u00b7brau\u00b7ne", "B\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er hielt ein Pferd am schmalen Z\u00fcgelstreife.", "tokens": ["Er", "hielt", "ein", "Pferd", "am", "schma\u00b7len", "Z\u00fc\u00b7gel\u00b7strei\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dreimal die Erde schlug sein Mund und gr\u00fc\u00dfte", "tokens": ["Drei\u00b7mal", "die", "Er\u00b7de", "schlug", "sein", "Mund", "und", "gr\u00fc\u00df\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Pascha, der hinschielend nach der Stute", "tokens": ["Den", "Pa\u00b7scha", ",", "der", "hin\u00b7schie\u00b7lend", "nach", "der", "Stu\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "APPR", "ART", "NN"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Gar gn\u00e4dig sprach: \u00bbSteh auf, o Sohn der W\u00fcste!\u00ab", "tokens": ["Gar", "gn\u00e4\u00b7dig", "sprach", ":", "\u00bb", "Steh", "auf", ",", "o", "Sohn", "der", "W\u00fcs\u00b7te", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$.", "$(", "VVFIN", "PTKVZ", "$,", "FM", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Darauf der Scheich mit schmerzbewegtem Mute:", "tokens": ["Da\u00b7rauf", "der", "Scheich", "mit", "schmerz\u00b7be\u00b7weg\u00b7tem", "Mu\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbich bringe Dir ein Ro\u00df, o Herr, zu Kaufe", "tokens": ["\u00bb", "ich", "brin\u00b7ge", "Dir", "ein", "Ro\u00df", ",", "o", "Herr", ",", "zu", "Kau\u00b7fe"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ART", "NN", "$,", "FM", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von der Koylani allerreinstem Blute.\u00ab", "tokens": ["Von", "der", "Ko\u00b7yla\u00b7ni", "al\u00b7ler\u00b7reins\u00b7tem", "Blu\u00b7te", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "\u00bbein fl\u00fccht'ger Staub der W\u00fcste ist's im Laufe,", "tokens": ["\u00bb", "ein", "fl\u00fccht'\u00b7ger", "Staub", "der", "W\u00fcs\u00b7te", "ist's", "im", "Lau\u00b7fe", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch fest wie Sinai, der Wolkentr\u00e4ger,", "tokens": ["Doch", "fest", "wie", "Si\u00b7nai", ",", "der", "Wol\u00b7ken\u00b7tr\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "NE", "$,", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So steht's in des Gefechtes Feuertaufe.\u00ab", "tokens": ["So", "steht's", "in", "des", "Ge\u00b7fech\u00b7tes", "Feu\u00b7er\u00b7tau\u00b7fe", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "\u00bbkennst Du den S'mum, den todbeschwingten J\u00e4ger?", "tokens": ["\u00bb", "kennst", "Du", "den", "S'\u00b7mum", ",", "den", "tod\u00b7be\u00b7schwing\u00b7ten", "J\u00e4\u00b7ger", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Oft hab' ich ihn ereilt im tollsten Jagen", "tokens": ["Oft", "hab'", "ich", "ihn", "er\u00b7eilt", "im", "tolls\u00b7ten", "Ja\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ihn beim Bart gezaust, den Steppenfeger.\u00ab", "tokens": ["Und", "ihn", "beim", "Bart", "ge\u00b7zaust", ",", "den", "Step\u00b7pen\u00b7fe\u00b7ger", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VVPP", "$,", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "\u00bbnimm hin das Ro\u00df, ich w\u00fcrd' ihm nie entsagen,", "tokens": ["\u00bb", "nimm", "hin", "das", "Ro\u00df", ",", "ich", "w\u00fcrd'", "ihm", "nie", "ent\u00b7sa\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ADV", "ART", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn nicht der Hunger mir, der Markzerfresser,", "tokens": ["Wenn", "nicht", "der", "Hun\u00b7ger", "mir", ",", "der", "Mark\u00b7zer\u00b7fres\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die d\u00fcrren Krallen in das Fleisch geschlagen.\u00ab", "tokens": ["Die", "d\u00fcr\u00b7ren", "Kral\u00b7len", "in", "das", "Fleisch", "ge\u00b7schla\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "\u00bbnimm hin das Ro\u00df, nie ritt ein F\u00fcrst es besser,", "tokens": ["\u00bb", "nimm", "hin", "das", "Ro\u00df", ",", "nie", "ritt", "ein", "F\u00fcrst", "es", "bes\u00b7ser", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ADV", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nie trank ein edles Tier mit Durstgel\u00fcste", "tokens": ["Nie", "trank", "ein", "ed\u00b7les", "Tier", "mit", "Durst\u00b7ge\u00b7l\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der W\u00fcstenquelle heiliges Gew\u00e4sser.\u00ab", "tokens": ["Der", "W\u00fcs\u00b7ten\u00b7quel\u00b7le", "hei\u00b7li\u00b7ges", "Ge\u00b7w\u00e4s\u00b7ser", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "\u00bbich g\u00e4b' es nicht um Ormus Perlenk\u00fcste!", "tokens": ["\u00bb", "ich", "g\u00e4b'", "es", "nicht", "um", "Or\u00b7mus", "Per\u00b7len\u00b7k\u00fcs\u00b7te", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch! In der \u00d6de hungern meine Knaben,", "tokens": ["Doch", "!", "In", "der", "\u00d6\u00b7de", "hun\u00b7gern", "mei\u00b7ne", "Kna\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$.", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und meinem Weibe dorrt der Quell der Br\u00fcste!\u00ab", "tokens": ["Und", "mei\u00b7nem", "Wei\u00b7be", "dorrt", "der", "Quell", "der", "Br\u00fcs\u00b7te", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Also der Mann, und in die Runde traben", "tokens": ["Al\u00b7so", "der", "Mann", ",", "und", "in", "die", "Run\u00b7de", "tra\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "KON", "APPR", "ART", "NN", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Lie\u00df er am Seil den vielgepries'nen Renner:", "tokens": ["Lie\u00df", "er", "am", "Seil", "den", "viel\u00b7ge\u00b7pries'\u00b7nen", "Ren\u00b7ner", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbf\u00fcr tausend Tamans sollst das Tier Du haben.\u00ab", "tokens": ["\u00bb", "f\u00fcr", "tau\u00b7send", "Ta\u00b7mans", "sollst", "das", "Tier", "Du", "ha\u00b7ben", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "CARD", "NN", "VMFIN", "ART", "NN", "PPER", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Der T\u00fcrke schmunzelte, er war ein Kenner,", "tokens": ["Der", "T\u00fcr\u00b7ke", "schmun\u00b7zel\u00b7te", ",", "er", "war", "ein", "Ken\u00b7ner", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Stute war von wundervollem Baue,", "tokens": ["Die", "Stu\u00b7te", "war", "von", "wun\u00b7der\u00b7vol\u00b7lem", "Bau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und schnurrbartstreichend staunten seine M\u00e4nner.", "tokens": ["Und", "schnurr\u00b7bart\u00b7strei\u00b7chend", "staun\u00b7ten", "sei\u00b7ne", "M\u00e4n\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Es schimmerte das Fell im reinsten Graue,", "tokens": ["Es", "schim\u00b7mer\u00b7te", "das", "Fell", "im", "reins\u00b7ten", "Grau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gleich mattem Silber oder wei\u00dfem Samte,", "tokens": ["Gleich", "mat\u00b7tem", "Sil\u00b7ber", "o\u00b7der", "wei\u00b7\u00dfem", "Sam\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gestrichen von der Hand der sch\u00f6nsten Fraue.", "tokens": ["Ge\u00b7stri\u00b7chen", "von", "der", "Hand", "der", "sch\u00f6ns\u00b7ten", "Frau\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Schaumwellen glich die M\u00e4hn', das Auge flammte,", "tokens": ["Schaum\u00b7wel\u00b7len", "glich", "die", "M\u00e4hn'", ",", "das", "Au\u00b7ge", "flamm\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Im Bogen flog der Schweif, wild schnob die Nase,", "tokens": ["Im", "Bo\u00b7gen", "flog", "der", "Schweif", ",", "wild", "schnob", "die", "Na\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn sich das Bein zum Niedersetzen strammte.", "tokens": ["Wenn", "sich", "das", "Bein", "zum", "Nie\u00b7der\u00b7set\u00b7zen", "stramm\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Nicht eine Wunde schlug der Huf im Grase,", "tokens": ["Nicht", "ei\u00b7ne", "Wun\u00b7de", "schlug", "der", "Huf", "im", "Gra\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So, selbstgef\u00e4llig an dem seid'nen Stricke,", "tokens": ["So", ",", "selbst\u00b7ge\u00b7f\u00e4l\u00b7lig", "an", "dem", "sei\u00b7d'\u00b7nen", "Stri\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+----+-", "measure": "unknown.me