, "laut\u00b7los", ",", "oh\u00b7ne", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "KON", "ADJD", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sah er sein k\u00f6stlich Tier zusammenknicken,", "tokens": ["Sah", "er", "sein", "k\u00f6st\u00b7lich", "Tier", "zu\u00b7sam\u00b7men\u00b7kni\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das blickt' ihn an, als ob's noch Dank ihm sage.", "tokens": ["Das", "blickt'", "ihn", "an", ",", "als", "ob's", "noch", "Dank", "ihm", "sa\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "NE", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.31": {"line.1": {"text": "\u00bbdich wird f\u00fcrwahr kein fremder Sattel dr\u00fccken,", "tokens": ["\u00bb", "dich", "wird", "f\u00fcr\u00b7wahr", "kein", "frem\u00b7der", "Sat\u00b7tel", "dr\u00fc\u00b7cken", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kein fremder Daumen wird Dein Kammhaar fassen,", "tokens": ["Kein", "frem\u00b7der", "Dau\u00b7men", "wird", "Dein", "Kamm\u00b7haar", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kein fremder Sporn die Flanke Dir zerst\u00fccken! 