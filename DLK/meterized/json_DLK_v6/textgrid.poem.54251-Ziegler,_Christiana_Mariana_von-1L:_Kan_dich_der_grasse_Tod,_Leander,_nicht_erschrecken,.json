{"textgrid.poem.54251": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kan dich der grasse Tod, Leander, nicht erschrecken,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kan dich der grasse Tod, Leander, nicht erschrecken,", "tokens": ["Kan", "dich", "der", "gras\u00b7se", "Tod", ",", "Le\u00b7an\u00b7der", ",", "nicht", "er\u00b7schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "ADJA", "NN", "$,", "NE", "$,", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der doch das schrecklichste von allen Dingen heist,", "tokens": ["Der", "doch", "das", "schreck\u00b7lichs\u00b7te", "von", "al\u00b7len", "Din\u00b7gen", "heist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Darhinter mu\u00df gewi\u00df ein gro\u00df Geheimni\u00df stecken,", "tokens": ["Dar\u00b7hin\u00b7ter", "mu\u00df", "ge\u00b7wi\u00df", "ein", "gro\u00df", "Ge\u00b7heim\u00b7ni\u00df", "ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ADV", "ART", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df dir dein Wunsch so fr\u00fch die Sterbens-Bahne weist.", "tokens": ["Da\u00df", "dir", "dein", "Wunsch", "so", "fr\u00fch", "die", "Ster\u00b7bens\u00b7Bah\u00b7ne", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein schlechter Wurm wind sich sein Leben zu erretten,", "tokens": ["Ein", "schlech\u00b7ter", "Wurm", "wind", "sich", "sein", "Le\u00b7ben", "zu", "er\u00b7ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn er von ohngef\u00e4hr in di\u00df Geschick verf\u00e4llt;", "tokens": ["Wenn", "er", "von", "ohn\u00b7ge\u00b7f\u00e4hr", "in", "di\u00df", "Ge\u00b7schick", "ver\u00b7f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJD", "APPR", "PDS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein Sclave freuet sich bey Wasser, Brod und Ketten,", "tokens": ["Ein", "Scla\u00b7ve", "freu\u00b7et", "sich", "bey", "Was\u00b7ser", ",", "Brod", "und", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wann selbger nur dabey sein Leben noch erh\u00e4lt,", "tokens": ["Wann", "selb\u00b7ger", "nur", "da\u00b7bey", "sein", "Le\u00b7ben", "noch", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "PAV", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Heist ihm sein Schicksal gleich dabey das Elend bauen,", "tokens": ["Heist", "ihm", "sein", "Schick\u00b7sal", "gleich", "da\u00b7bey", "das", "E\u00b7lend", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "ADV", "PAV", "ART", "NN", "VVINF", "$,"], "meter": "+-++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.10": {"text": "So wird doch Frohn und Last ihm recht zu lauter Lust,", "tokens": ["So", "wird", "doch", "Frohn", "und", "Last", "ihm", "recht", "zu", "lau\u00b7ter", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "KON", "NN", "PPER", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und kan er Sarg und Grufft von sich entfernt nur schauen,", "tokens": ["Und", "kan", "er", "Sarg", "und", "Grufft", "von", "sich", "ent\u00b7fernt", "nur", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "NN", "KON", "NN", "APPR", "PRF", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "N\u00e4hrt doch ein Lebens-Trieb noch die beschwerte Brust.", "tokens": ["N\u00e4hrt", "doch", "ein", "Le\u00b7bens\u00b7Trieb", "noch", "die", "be\u00b7schwer\u00b7te", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Leander, du bist frey, dein eigen, nicht gefangen,", "tokens": ["Le\u00b7an\u00b7der", ",", "du", "bist", "frey", ",", "dein", "ei\u00b7gen", ",", "nicht", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "ADJD", "$,", "PPOSAT", "ADJD", "$,", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und bist, ich wundre mich, doch deines Lebens satt.", "tokens": ["Und", "bist", ",", "ich", "wund\u00b7re", "mich", ",", "doch", "dei\u00b7nes", "Le\u00b7bens", "satt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "PPER", "VVFIN", "PPER", "$,", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Du klagst, wie vielmahl bin ich schon dem Todt entgangen?", "tokens": ["Du", "klagst", ",", "wie", "viel\u00b7mahl", "bin", "ich", "schon", "dem", "Todt", "ent\u00b7gan\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "O sage was dein Mund zu klagen n\u00f6thig hat?", "tokens": ["O", "sa\u00b7ge", "was", "dein", "Mund", "zu", "kla\u00b7gen", "n\u00f6\u00b7thig", "hat", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "PPOSAT", "NN", "PTKZU", "VVINF", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Soll denn ein kalter Stein dein liebstes Wohnhau\u00df heissen,", "tokens": ["Soll", "denn", "ein", "kal\u00b7ter", "Stein", "dein", "liebs\u00b7tes", "Wohn\u00b7hau\u00df", "heis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ein finstres Toden Hau\u00df, dein sch\u00f6nstes Paradie\u00df,", "tokens": ["Ein", "finst\u00b7res", "To\u00b7den", "Hau\u00df", ",", "dein", "sch\u00f6ns\u00b7tes", "Pa\u00b7ra\u00b7die\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wilst du dich mit Gewalt und Macht nach etwas reissen,", "tokens": ["Wilst", "du", "dich", "mit", "Ge\u00b7walt", "und", "Macht", "nach", "et\u00b7was", "reis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "NN", "KON", "NN", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Das schon der ersten Welt mehr als zu furchtbar hie\u00df.", "tokens": ["Das", "schon", "der", "ers\u00b7ten", "Welt", "mehr", "als", "zu", "furcht\u00b7bar", "hie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "ADJA", "NN", "PIAT", "KOKOM", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wir waren anfangs zwar zum Sterben nicht erkohren,", "tokens": ["Wir", "wa\u00b7ren", "an\u00b7fangs", "zwar", "zum", "Ster\u00b7ben", "nicht", "er\u00b7koh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPRART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Doch Evens Apffel-Bi\u00df und Adams blinder Wahn,", "tokens": ["Doch", "E\u00b7vens", "Apf\u00b7fel\u00b7Bi\u00df", "und", "A\u00b7dams", "blin\u00b7der", "Wahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "KON", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Hat uns so Sarg als Grufft zur Strafe zugeschworen,", "tokens": ["Hat", "uns", "so", "Sarg", "als", "Grufft", "zur", "Stra\u00b7fe", "zu\u00b7ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NN", "KOUS", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die ieder gerne flieht, so lang er immer kan.", "tokens": ["Die", "ie\u00b7der", "ger\u00b7ne", "flieht", ",", "so", "lang", "er", "im\u00b7mer", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "VVFIN", "$,", "ADV", "ADJD", "PPER", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wie kanst du, sage mir, so nach dem Tode ringen?", "tokens": ["Wie", "kanst", "du", ",", "sa\u00b7ge", "mir", ",", "so", "nach", "dem", "To\u00b7de", "rin\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Da doch ein iedermann um Aufschub bitten will;", "tokens": ["Da", "doch", "ein", "ie\u00b7der\u00b7mann", "um", "Auf\u00b7schub", "bit\u00b7ten", "will", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADV", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Es w\u00e4re viel zu fr\u00fch ein Grab-Lied dir zu singen,", "tokens": ["Es", "w\u00e4\u00b7re", "viel", "zu", "fr\u00fch", "ein", "Grab\u00b7Lied", "dir", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKA", "ADJD", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Dein Wunsch ist ungerecht, erwarte doch dein Ziel.", "tokens": ["Dein", "Wunsch", "ist", "un\u00b7ge\u00b7recht", ",", "er\u00b7war\u00b7te", "doch", "dein", "Ziel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Dein Seufftzen, dein Gebet, dein \u00e4ngstlich H\u00e4nderingen,", "tokens": ["Dein", "Seufft\u00b7zen", ",", "dein", "Ge\u00b7bet", ",", "dein", "\u00e4ngst\u00b7lich", "H\u00e4n\u00b7de\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Bezeugen, da\u00df dein Hertz Verdru\u00df und Unruh hegt.", "tokens": ["Be\u00b7zeu\u00b7gen", ",", "da\u00df", "dein", "Hertz", "Ver\u00b7dru\u00df", "und", "Un\u00b7ruh", "hegt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPOSAT", "NN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Du kanst das Schicksal doch dadurch nicht eher zwingen,", "tokens": ["Du", "kanst", "das", "Schick\u00b7sal", "doch", "da\u00b7durch", "nicht", "e\u00b7her", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "PAV", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Als bi\u00df dir selbiges den Gr\u00e4ntz-Stein selber legt.", "tokens": ["Als", "bi\u00df", "dir", "sel\u00b7bi\u00b7ges", "den", "Gr\u00e4nt\u00b7zStein", "sel\u00b7ber", "legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ADJA", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Vielleicht hat dir das Gl\u00fcck, und di\u00df in wenig Jahren,", "tokens": ["Viel\u00b7leicht", "hat", "dir", "das", "Gl\u00fcck", ",", "und", "di\u00df", "in", "we\u00b7nig", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$,", "KON", "PDS", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ein angenehmes Kind zur Tr\u00f6sterin ersehn,", "tokens": ["Ein", "an\u00b7ge\u00b7neh\u00b7mes", "Kind", "zur", "Tr\u00f6s\u00b7te\u00b7rin", "er\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Da wirst du Zucker-Lust, Leander, einst erfahren,", "tokens": ["Da", "wirst", "du", "Zu\u00b7cke\u00b7rLust", ",", "Le\u00b7an\u00b7der", ",", "einst", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "NE", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ich sehe schon voraus im Geist, als w\u00e4rs geschehn.", "tokens": ["Ich", "se\u00b7he", "schon", "vo\u00b7raus", "im", "Geist", ",", "als", "w\u00e4rs", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPRART", "NN", "$,", "KOKOM", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Da wirst du, hoff ich, wohl nicht mehr an Tod gedencken,", "tokens": ["Da", "wirst", "du", ",", "hoff", "ich", ",", "wohl", "nicht", "mehr", "an", "Tod", "ge\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "ADV", "PTKNEG", "ADV", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Der, artger Mensch, dir ietzt stets auf der Zunge sitzt;", "tokens": ["Der", ",", "art\u00b7ger", "Mensch", ",", "dir", "ietzt", "stets", "auf", "der", "Zun\u00b7ge", "sitzt", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJA", "NN", "$,", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die Liebe wird so dann mit Lebens-Safft dich tr\u00e4ncken,", "tokens": ["Die", "Lie\u00b7be", "wird", "so", "dann", "mit", "Le\u00b7bens\u00b7\u00b7S\u00b7afft", "dich", "tr\u00e4n\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "li