{"textgrid.poem.47762": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[nur R\u00e4uber und M\u00f6rder schl\u00e4gt man in Bande sonst]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur R\u00e4uber und M\u00f6rder schl\u00e4gt man in Bande sonst;", "tokens": ["Nur", "R\u00e4u\u00b7ber", "und", "M\u00f6r\u00b7der", "schl\u00e4gt", "man", "in", "Ban\u00b7de", "sonst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVFIN", "PIS", "APPR", "NN", "ADV", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich bin beraubt, gemordet und \u00fcberdies", "tokens": ["Ich", "bin", "be\u00b7raubt", ",", "ge\u00b7mor\u00b7det", "und", "\u00fc\u00b7ber\u00b7dies"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "VVPP", "KON", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Geschlagen in Lockenbande wunderlich,", "tokens": ["Ge\u00b7schla\u00b7gen", "in", "Lo\u00b7cken\u00b7ban\u00b7de", "wun\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und frei umher geht jene, die Th\u00e4terin!", "tokens": ["Und", "frei", "um\u00b7her", "geht", "je\u00b7ne", ",", "die", "Th\u00e4\u00b7te\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKVZ", "VVFIN", "PDS", "$,", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}