{"dta.poem.3165": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kein Schaden kann dich je betreffen in der Nacht,", "tokens": ["Kein", "Scha\u00b7den", "kann", "dich", "je", "be\u00b7tref\u00b7fen", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den nicht zu Nutzen sich gemeine Habgier macht.", "tokens": ["Den", "nicht", "zu", "Nut\u00b7zen", "sich", "ge\u00b7mei\u00b7ne", "Hab\u00b7gier", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "NN", "PRF", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wo sie nur eine Leich' erwittern in dem Hause,", "tokens": ["Wo", "sie", "nur", "ei\u00b7ne", "Leich'", "er\u00b7wit\u00b7tern", "in", "dem", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da sammeln alsogleich die Raben sich zum Schmause.", "tokens": ["Da", "sam\u00b7meln", "al\u00b7so\u00b7gleich", "die", "Ra\u00b7ben", "sich", "zum", "Schmau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Erleichtern wollen sie dir recht die Weltentschlagung;", "tokens": ["Er\u00b7leich\u00b7tern", "wol\u00b7len", "sie", "dir", "recht", "die", "Wel\u00b7tent\u00b7schla\u00b7gung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch den Brahmanen ziemt gem\u00e4\u00dfigte Entsagung:", "tokens": ["Doch", "den", "Brah\u00b7ma\u00b7nen", "ziemt", "ge\u00b7m\u00e4\u00b7\u00dfig\u00b7te", "Ent\u00b7sa\u00b7gung", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nur die Begierden, nicht die Kleider auszuziehn,", "tokens": ["Nur", "die", "Be\u00b7gier\u00b7den", ",", "nicht", "die", "Klei\u00b7der", "aus\u00b7zu\u00b7ziehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil ich noch nicht gereift zum nackten B\u00fc\u00dfer bin.", "tokens": ["Weil", "ich", "noch", "nicht", "ge\u00b7reift", "zum", "nack\u00b7ten", "B\u00fc\u00b7\u00dfer", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "ADV", "APPRART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}