{"textgrid.poem.39292": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es war eine Nacht voll Grimm und Graus,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war eine Nacht voll Grimm und Graus,", "tokens": ["Es", "war", "ei\u00b7ne", "Nacht", "voll", "Grimm", "und", "Graus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "NE", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In Tr\u00e4nen die Wolken schwammen,", "tokens": ["In", "Tr\u00e4\u00b7nen", "die", "Wol\u00b7ken", "schwam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In der Ferne stand Deines Vaters Haus,", "tokens": ["In", "der", "Fer\u00b7ne", "stand", "Dei\u00b7nes", "Va\u00b7ters", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Drum tanzten die Blitzesflammen.", "tokens": ["Drum", "tanz\u00b7ten", "die", "Blit\u00b7zes\u00b7flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Erde schauderte bis zum Kern,", "tokens": ["Die", "Er\u00b7de", "schau\u00b7der\u00b7te", "bis", "zum", "Kern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es \u00e4chzte der Wald im Leide,", "tokens": ["Es", "\u00e4chz\u00b7te", "der", "Wald", "im", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie eines Verzweifelnden Augenstern", "tokens": ["Wie", "ei\u00b7nes", "Ver\u00b7zwei\u00b7feln\u00b7den", "Au\u00b7gens\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "So stierte mich an die Heide.", "tokens": ["So", "stier\u00b7te", "mich", "an", "die", "Hei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und auf der Heide \u00f6d' und kalt,", "tokens": ["Und", "auf", "der", "Hei\u00b7de", "\u00f6d'", "und", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O k\u00f6nnt' ich's endlich vergessen \u2013", "tokens": ["O", "k\u00f6nnt'", "ich's", "end\u00b7lich", "ver\u00b7ges\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PIS", "ADV", "VVPP", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da hat eine wei\u00dfe Frauengestalt", "tokens": ["Da", "hat", "ei\u00b7ne", "wei\u00b7\u00dfe", "Frau\u00b7en\u00b7ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "An meinem Wege gesessen.", "tokens": ["An", "mei\u00b7nem", "We\u00b7ge", "ge\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und wie ich entsetzt, halb unbewu\u00dft,", "tokens": ["Und", "wie", "ich", "ent\u00b7setzt", ",", "halb", "un\u00b7be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVPP", "$,", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Von dem stutzenden Rosse schwang mich,", "tokens": ["Von", "dem", "stut\u00b7zen\u00b7den", "Ros\u00b7se", "schwang", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Da f\u00fchlt' ich, wie mit bebender Lust", "tokens": ["Da", "f\u00fchlt'", "ich", ",", "wie", "mit", "be\u00b7ben\u00b7der", "Lust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Dein zittender Arm umschlang mich.", "tokens": ["Dein", "zit\u00b7ten\u00b7der", "Arm", "um\u00b7schlang", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Ich hab' Dich verlassen wie ein Wicht,", "tokens": ["Ich", "hab'", "Dich", "ver\u00b7las\u00b7sen", "wie", "ein", "Wicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "O k\u00f6nnt' ich das Wort versenken!", "tokens": ["O", "k\u00f6nnt'", "ich", "das", "Wort", "ver\u00b7sen\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stets mu\u00df ich, beim hellsten Tageslicht,", "tokens": ["Stets", "mu\u00df", "ich", ",", "beim", "hells\u00b7ten", "Ta\u00b7ges\u00b7licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "An jene Nacht gedenken.", "tokens": ["An", "je\u00b7ne", "Nacht", "ge\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Stets seh' ich Dich, verratenes Weib,", "tokens": ["Stets", "seh'", "ich", "Dich", ",", "ver\u00b7ra\u00b7te\u00b7nes", "Weib", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Beim Donnerschlage Dich b\u00fccken,", "tokens": ["Beim", "Don\u00b7ner\u00b7schla\u00b7ge", "Dich", "b\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Stets seh' ich Deinen z\u00e4rtlichen Leib", "tokens": ["Stets", "seh'", "ich", "Dei\u00b7nen", "z\u00e4rt\u00b7li\u00b7chen", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Im Sturm zusammenknicken.", "tokens": ["Im", "Sturm", "zu\u00b7sam\u00b7men\u00b7kni\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Du gingst durch der Nacht zornbr\u00fcllende Wut,", "tokens": ["Du", "gingst", "durch", "der", "Nacht", "zorn\u00b7br\u00fcl\u00b7len\u00b7de", "Wut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Um mich einmal zu umfassen,", "tokens": ["Um", "mich", "ein\u00b7mal", "zu", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich aber hab' Dich mit leichtem Mut", "tokens": ["Ich", "a\u00b7ber", "hab'", "Dich", "mit", "leich\u00b7tem", "Mut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Drei Tage darauf verlassen.", "tokens": ["Drei", "Ta\u00b7ge", "da\u00b7rauf", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "PAV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}