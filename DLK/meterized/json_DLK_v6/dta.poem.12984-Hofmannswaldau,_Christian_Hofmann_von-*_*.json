{"dta.poem.12984": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "* *", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du bist nicht mehr so geil, drum willst du heilig heissen:", "tokens": ["Du", "bist", "nicht", "mehr", "so", "geil", ",", "drum", "willst", "du", "hei\u00b7lig", "heis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "ADV", "ADJD", "$,", "PAV", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum denckst du bey dir selbst: nun hab\u2019 ich mich be-", "tokens": ["Drum", "denckst", "du", "bey", "dir", "selbst", ":", "nun", "hab'", "ich", "mich", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "PPER", "ADV", "$.", "ADV", "VAFIN", "PPER", "PRF", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "kehrt;", "tokens": ["kehrt", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Nun witz und tugend mir der keuschdeit schatz gewehrt,", "tokens": ["Nun", "witz", "und", "tu\u00b7gend", "mir", "der", "keuschdeit", "schatz", "ge\u00b7wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "NN", "PPER", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Den soll kein liebes-blitz mir aus dem hertzen reissen.", "tokens": ["Den", "soll", "kein", "lie\u00b7bes\u00b7blitz", "mir", "aus", "dem", "hert\u00b7zen", "reis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PIAT", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denck\u2019 aber nicht zu viel, dein dencken ist ein wahn.", "tokens": ["Denck", "a\u00b7ber", "nicht", "zu", "viel", ",", "dein", "den\u00b7cken", "ist", "ein", "wahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PTKNEG", "PTKA", "PIS", "$,", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dein hochmuth, den du liebst, l\u00e4st dich es nicht erwegen;", "tokens": ["Dein", "hoch\u00b7muth", ",", "den", "du", "liebst", ",", "l\u00e4st", "dich", "es", "nicht", "er\u00b7we\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denn dieser sucht in dir der tugend beyzulegen,", "tokens": ["Denn", "die\u00b7ser", "sucht", "in", "dir", "der", "tu\u00b7gend", "bey\u00b7zu\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "APPR", "PPER", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was doch des alters macht und nicht dein witz gethan.", "tokens": ["Was", "doch", "des", "al\u00b7ters", "macht", "und", "nicht", "dein", "witz", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "VVFIN", "KON", "PTKNEG", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}