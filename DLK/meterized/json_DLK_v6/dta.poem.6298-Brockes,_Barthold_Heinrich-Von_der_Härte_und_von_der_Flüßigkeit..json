{"dta.poem.6298": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der H\u00e4rte und von der  \n Fl\u00fc\u00dfigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn wir zwey Dinge, welche fl\u00fcssen,", "tokens": ["Wenn", "wir", "zwey", "Din\u00b7ge", ",", "wel\u00b7che", "fl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "CARD", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zusammen g\u00fcssen;", "tokens": ["Zu\u00b7sam\u00b7men", "g\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wird offt ein harter C\u00f6rper draus,", "tokens": ["Wird", "offt", "ein", "har\u00b7ter", "C\u00f6r\u00b7per", "draus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das sieht nun dunckel zwar, und wunderbarlich aus.", "tokens": ["Das", "sieht", "nun", "dun\u00b7ckel", "zwar", ",", "und", "wun\u00b7der\u00b7bar\u00b7lich", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "ADV", "$,", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Allein f\u00fcr uns ist dies nicht dunckel mehr,", "tokens": ["Al\u00b7lein", "f\u00fcr", "uns", "ist", "dies", "nicht", "dun\u00b7ckel", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VAFIN", "PDS", "PTKNEG", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wir schl\u00fcssen leicht: es komme dies daher,", "tokens": ["Wir", "schl\u00fcs\u00b7sen", "leicht", ":", "es", "kom\u00b7me", "dies", "da\u00b7her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "PPER", "VVFIN", "PDS", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Weil eine Feuchtigkeit in eine andre dringet,", "tokens": ["Weil", "ei\u00b7ne", "Feuch\u00b7tig\u00b7keit", "in", "ei\u00b7ne", "and\u00b7re", "drin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit solcher eigenen Beschaffenheit,", "tokens": ["Mit", "sol\u00b7cher", "ei\u00b7ge\u00b7nen", "Be\u00b7schaf\u00b7fen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Da\u00df es die kleinen Theil um ihre Wallung bringet,", "tokens": ["Da\u00df", "es", "die", "klei\u00b7nen", "Theil", "um", "ih\u00b7re", "Wal\u00b7lung", "brin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und um die Fl\u00fc\u00dfigkeit,", "tokens": ["Und", "um", "die", "Fl\u00fc\u00b7\u00dfig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Da eins dem andern wehr\u2019t, und sein Bewegen stillt:", "tokens": ["Da", "eins", "dem", "an\u00b7dern", "wehr't", ",", "und", "sein", "Be\u00b7we\u00b7gen", "stillt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Was leer war, das wird voll, die L\u00f6cherchen erf\u00fcllt,", "tokens": ["Was", "leer", "war", ",", "das", "wird", "voll", ",", "die", "L\u00f6\u00b7cher\u00b7chen", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "PDS", "VAFIN", "ADJD", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Daraus wird eine Massa dann,", "tokens": ["Da\u00b7raus", "wird", "ei\u00b7ne", "Mas\u00b7sa", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und h\u00e4nget eins dem andern an.", "tokens": ["Und", "h\u00e4n\u00b7get", "eins", "dem", "an\u00b7dern", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}