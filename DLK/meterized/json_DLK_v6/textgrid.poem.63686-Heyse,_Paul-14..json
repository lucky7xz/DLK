{"textgrid.poem.63686": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun treibt ein graugespenst'ger Nebelspuk", "tokens": ["Nun", "treibt", "ein", "grau\u00b7ge\u00b7spenst'\u00b7ger", "Ne\u00b7bels\u00b7puk"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sein frostig Spiel in diesen H\u00f6hn und Tiefen.", "tokens": ["Sein", "fros\u00b7tig", "Spiel", "in", "die\u00b7sen", "H\u00f6hn", "und", "Tie\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "APPR", "PDAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Wald die regenschweren Wipfel triefen,", "tokens": ["Im", "Wald", "die", "re\u00b7gen\u00b7schwe\u00b7ren", "Wip\u00b7fel", "trie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und \u00fcber ihnen kreischt ein Kr\u00e4henflug.", "tokens": ["Und", "\u00fc\u00b7ber", "ih\u00b7nen", "kreischt", "ein", "Kr\u00e4\u00b7hen\u00b7flug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Als ob Berggeister, die, vom Fremdenzug", "tokens": ["Als", "ob", "Berg\u00b7geis\u00b7ter", ",", "die", ",", "vom", "Frem\u00b7den\u00b7zug"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "KOUS", "NN", "$,", "PRELS", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Versch\u00fcchtert, sommerlang geduldig schliefen,", "tokens": ["Ver\u00b7sch\u00fcch\u00b7tert", ",", "som\u00b7mer\u00b7lang", "ge\u00b7dul\u00b7dig", "schlie\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erwachten und den Eindringlingen riefen:", "tokens": ["Er\u00b7wach\u00b7ten", "und", "den", "Ein\u00b7dring\u00b7lin\u00b7gen", "rie\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es naht der Herbst! Fort ihr! Nun ist's genug!", "tokens": ["Es", "naht", "der", "Herbst", "!", "Fort", "ihr", "!", "Nun", "ist's", "ge\u00b7nug", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "NN", "PPER", "$.", "ADV", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So werd' auch ich bald weichen der Gewalt,", "tokens": ["So", "werd'", "auch", "ich", "bald", "wei\u00b7chen", "der", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denn nicht mehr lockt mit sonnenhellen Reizen", "tokens": ["Denn", "nicht", "mehr", "lockt", "mit", "son\u00b7nen\u00b7hel\u00b7len", "Rei\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In seine Tiefen mein geliebter Wald.", "tokens": ["In", "sei\u00b7ne", "Tie\u00b7fen", "mein", "ge\u00b7lieb\u00b7ter", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nun wird der J\u00e4ger diese Wege kreuzen,", "tokens": ["Nun", "wird", "der", "J\u00e4\u00b7ger", "die\u00b7se", "We\u00b7ge", "kreu\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ich, wenn lustig seine B\u00fcchse knallt,", "tokens": ["Und", "ich", ",", "wenn", "lus\u00b7tig", "sei\u00b7ne", "B\u00fcch\u00b7se", "knallt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "KOUS", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sitz' still im Haus und mu\u00df den Ofen heizen.", "tokens": ["Sitz'", "still", "im", "Haus", "und", "mu\u00df", "den", "O\u00b7fen", "hei\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "KON", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}