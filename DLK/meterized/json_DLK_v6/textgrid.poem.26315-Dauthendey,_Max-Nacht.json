{"textgrid.poem.26315": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aufmerksam an der Wegecke ein Laternenlicht sich dreht,", "tokens": ["Auf\u00b7merk\u00b7sam", "an", "der", "We\u00b7ge\u00b7cke", "ein", "La\u00b7ter\u00b7nen\u00b7licht", "sich", "dreht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+---+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mutterseelenallein ein warmer Wind \u00fcber die Stra\u00dfe geht,", "tokens": ["Mut\u00b7ter\u00b7see\u00b7len\u00b7al\u00b7lein", "ein", "war\u00b7mer", "Wind", "\u00fc\u00b7ber", "die", "Stra\u00b7\u00dfe", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+---+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Eine wei\u00dfe Hauswand leuchtend in der Nacht steht.", "tokens": ["Ei\u00b7ne", "wei\u00b7\u00dfe", "Haus\u00b7wand", "leuch\u00b7tend", "in", "der", "Nacht", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Unruhig ein Palmenschatten am Wegrand weht.", "tokens": ["Un\u00b7ru\u00b7hig", "ein", "Pal\u00b7men\u00b7schat\u00b7ten", "am", "Weg\u00b7rand", "weht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Meine Augen schreiben auf die Wand ein Gebet,", "tokens": ["Mei\u00b7ne", "Au\u00b7gen", "schrei\u00b7ben", "auf", "die", "Wand", "ein", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Ein Gebet meines leeren Armes, der nach der Liebsten fleht.", "tokens": ["Ein", "Ge\u00b7bet", "mei\u00b7nes", "lee\u00b7ren", "Ar\u00b7mes", ",", "der", "nach", "der", "Liebs\u00b7ten", "fleht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}}}}