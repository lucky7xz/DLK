{"dta.poem.18495": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines Nachtw\u00e4chters.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn andre schlieffen wol/ so must ich f\u00fcr sie wachen/", "tokens": ["Wenn", "and\u00b7re", "schlief\u00b7fen", "wol", "/", "so", "must", "ich", "f\u00fcr", "sie", "wa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "ADV", "$(", "ADV", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und fleissig haben acht auf all ihr hab und gut:", "tokens": ["Und", "fleis\u00b7sig", "ha\u00b7ben", "acht", "auf", "all", "ihr", "hab", "und", "gut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "CARD", "APPR", "PIAT", "PPER", "VAFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jtzt bin ich durch den todt genommen von der hut.", "tokens": ["Jtzt", "bin", "ich", "durch", "den", "todt", "ge\u00b7nom\u00b7men", "von", "der", "hut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "ADJD", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nimmt er die w\u00e4chter weg/ wie wird ers schl\u00e4ffern machen?", "tokens": ["Nimmt", "er", "die", "w\u00e4ch\u00b7ter", "weg", "/", "wie", "wird", "ers", "schl\u00e4f\u00b7fern", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$(", "KOKOM", "VAFIN", "PIS", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}