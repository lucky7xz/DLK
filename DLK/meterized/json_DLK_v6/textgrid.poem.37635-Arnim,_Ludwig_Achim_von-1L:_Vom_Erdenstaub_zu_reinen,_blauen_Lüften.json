{"textgrid.poem.37635": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vom Erdenstaub zu reinen, blauen L\u00fcften", "genre": "verse", "period": "N.A.", "pub_year": 1825, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Erdenstaub zu reinen, blauen L\u00fcften", "tokens": ["Vom", "Er\u00b7dens\u00b7taub", "zu", "rei\u00b7nen", ",", "blau\u00b7en", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dringt weit der Blick in ersten Fr\u00fchlingstagen,", "tokens": ["Dringt", "weit", "der", "Blick", "in", "ers\u00b7ten", "Fr\u00fch\u00b7lings\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und h\u00f6her steigt der m\u00e4cht'ge Sonnenwagen,", "tokens": ["Und", "h\u00f6\u00b7her", "steigt", "der", "m\u00e4cht'\u00b7ge", "Son\u00b7nen\u00b7wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Erde sehnt nach Bl\u00e4ttern sich und D\u00fcften,", "tokens": ["Die", "Er\u00b7de", "sehnt", "nach", "Bl\u00e4t\u00b7tern", "sich", "und", "D\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "PRF", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und heilige Geschichten uns dann sagen", "tokens": ["Und", "hei\u00b7li\u00b7ge", "Ge\u00b7schich\u00b7ten", "uns", "dann", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was sich geahnet in des Herzens Kl\u00fcften.", "tokens": ["Was", "sich", "ge\u00b7ah\u00b7net", "in", "des", "Her\u00b7zens", "Kl\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Er ist erstanden aus den Todesgr\u00fcften,", "tokens": ["Er", "ist", "er\u00b7stan\u00b7den", "aus", "den", "To\u00b7des\u00b7gr\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und wie vergebens war der Menschen Zagen,", "tokens": ["Und", "wie", "ver\u00b7ge\u00b7bens", "war", "der", "Men\u00b7schen", "Za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ja so ersteht die Welt der Himmelsgaben", "tokens": ["Ja", "so", "er\u00b7steht", "die", "Welt", "der", "Him\u00b7mels\u00b7ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Mit jedem Jahre neu, die Knospen brechen,", "tokens": ["Mit", "je\u00b7dem", "Jah\u00b7re", "neu", ",", "die", "Knos\u00b7pen", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und nichts ist unsrer Liebe zu erhaben,", "tokens": ["Und", "nichts", "ist", "uns\u00b7rer", "Lie\u00b7be", "zu", "er\u00b7ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Sie giebt uns alles in den Wonneb\u00e4chen,", "tokens": ["Sie", "giebt", "uns", "al\u00b7les", "in", "den", "Won\u00b7ne\u00b7b\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Die nach dem Eisgang Flur und Aug' durchgraben,", "tokens": ["Die", "nach", "dem", "Eis\u00b7gang", "Flur", "und", "Aug'", "durch\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Das Unsichtbarste will zum Lichte sprechen.", "tokens": ["Das", "Un\u00b7sicht\u00b7bars\u00b7te", "will", "zum", "Lich\u00b7te", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}