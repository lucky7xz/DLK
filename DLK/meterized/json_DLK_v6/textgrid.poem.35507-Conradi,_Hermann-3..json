{"textgrid.poem.35507": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Winternot und Wintergram", "tokens": ["Von", "Win\u00b7ter\u00b7not", "und", "Win\u00b7ter\u00b7gram"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wollt' ich ein Lied im Lenze singen \u2013", "tokens": ["Wollt'", "ich", "ein", "Lied", "im", "Len\u00b7ze", "sin\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Im Lenz, der von der Seele nahm", "tokens": ["Im", "Lenz", ",", "der", "von", "der", "See\u00b7le", "nahm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das wilde Suchen, tolle Ueberspringen ...", "tokens": ["Das", "wil\u00b7de", "Su\u00b7chen", ",", "tol\u00b7le", "Ue\u00b7ber\u00b7sprin\u00b7gen", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der mir mit seinem Drang zum Licht,", "tokens": ["Der", "mir", "mit", "sei\u00b7nem", "Drang", "zum", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit seinem Schwellen, seinem Treiben,", "tokens": ["Mit", "sei\u00b7nem", "Schwel\u00b7len", ",", "sei\u00b7nem", "Trei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur schweigendes Erstaunen gab", "tokens": ["Nur", "schwei\u00b7gen\u00b7des", "Er\u00b7stau\u00b7nen", "gab"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und tiefergriffnes Stehenbleiben ...", "tokens": ["Und", "tie\u00b7fer\u00b7griff\u00b7nes", "Ste\u00b7hen\u00b7blei\u00b7ben", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}