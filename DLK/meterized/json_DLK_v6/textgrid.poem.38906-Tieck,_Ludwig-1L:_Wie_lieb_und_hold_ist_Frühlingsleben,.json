{"textgrid.poem.38906": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie lieb und hold ist Fr\u00fchlingsleben,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie lieb und hold ist Fr\u00fchlingsleben,", "tokens": ["Wie", "lieb", "und", "hold", "ist", "Fr\u00fch\u00b7lings\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn alle Nachtigallen singen,", "tokens": ["Wenn", "al\u00b7le", "Nach\u00b7ti\u00b7gal\u00b7len", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie die T\u00f6n' in B\u00e4umen klingen", "tokens": ["Und", "wie", "die", "T\u00f6n'", "in", "B\u00e4u\u00b7men", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Wonne Laub und Bl\u00fcthen beben.", "tokens": ["In", "Won\u00b7ne", "Laub", "und", "Bl\u00fc\u00b7then", "be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie sch\u00f6n im goldnen Mondenscheine", "tokens": ["Wie", "sch\u00f6n", "im", "gold\u00b7nen", "Mon\u00b7den\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Spiel der lauen Abendl\u00fcfte,", "tokens": ["Das", "Spiel", "der", "lau\u00b7en", "A\u00b7ben\u00b7dl\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die, auf den Fl\u00fcgeln Lindend\u00fcfte,", "tokens": ["Die", ",", "auf", "den", "Fl\u00fc\u00b7geln", "Lin\u00b7den\u00b7d\u00fcf\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich jagen durch die stillen Haine.", "tokens": ["Sich", "ja\u00b7gen", "durch", "die", "stil\u00b7len", "Hai\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie herrlich gl\u00e4nzt die Rosenpracht,", "tokens": ["Wie", "herr\u00b7lich", "gl\u00e4nzt", "die", "Ro\u00b7sen\u00b7pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Liebreiz rings die Felder schm\u00fccket,", "tokens": ["Wenn", "Lieb\u00b7reiz", "rings", "die", "Fel\u00b7der", "schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Lieb' aus tausend Rosen blicket,", "tokens": ["Die", "Lieb'", "aus", "tau\u00b7send", "Ro\u00b7sen", "bli\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Sternen ihrer Wonne-Nacht.", "tokens": ["Aus", "Ster\u00b7nen", "ih\u00b7rer", "Won\u00b7ne\u00b7Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch sch\u00f6ner d\u00fcnkt mir, holder, lieber,", "tokens": ["Doch", "sch\u00f6\u00b7ner", "d\u00fcnkt", "mir", ",", "hol\u00b7der", ",", "lie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$,", "ADJA", "$,", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des kleinen Lichtleins bla\u00df Geflimmer,", "tokens": ["Des", "klei\u00b7nen", "Licht\u00b7leins", "bla\u00df", "Ge\u00b7flim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn sie sich zeigt im engen Zimmer,", "tokens": ["Wenn", "sie", "sich", "zeigt", "im", "en\u00b7gen", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sp\u00e4h' ich in Nacht zu ihr hin\u00fcber,", "tokens": ["Sp\u00e4h'", "ich", "in", "Nacht", "zu", "ihr", "hin\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "APPR", "PPOSAT", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie sie die Flechten l\u00f6\u00dft und bindet,", "tokens": ["Wie", "sie", "die", "Flech\u00b7ten", "l\u00f6\u00dft", "und", "bin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sie im Schwung der wei\u00dfen Hand", "tokens": ["Wie", "sie", "im", "Schwung", "der", "wei\u00b7\u00dfen", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Anschmiegt dem Leibe hell Gewand,", "tokens": ["An\u00b7schmiegt", "dem", "Lei\u00b7be", "hell", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Kr\u00e4nz' in braune Locken windet.", "tokens": ["Und", "Kr\u00e4nz'", "in", "brau\u00b7ne", "Lo\u00b7cken", "win\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie sie die Laute l\u00e4\u00dft erklingen,", "tokens": ["Wie", "sie", "die", "Lau\u00b7te", "l\u00e4\u00dft", "er\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und T\u00f6ne, aufgejagt, erwachen,", "tokens": ["Und", "T\u00f6\u00b7ne", ",", "auf\u00b7ge\u00b7jagt", ",", "er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "$,", "VVPP", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ber\u00fchrt von zarten Fingern lachen,", "tokens": ["Be\u00b7r\u00fchrt", "von", "zar\u00b7ten", "Fin\u00b7gern", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und scherzend durch die Saiten springen;", "tokens": ["Und", "scher\u00b7zend", "durch", "die", "Sai\u00b7ten", "sprin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Sie einzufangen schickt sie Kl\u00e4nge", "tokens": ["Sie", "ein\u00b7zu\u00b7fan\u00b7gen", "schickt", "sie", "Kl\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVIZU", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gesanges fort, da flieht mit Scherzen", "tokens": ["Ge\u00b7san\u00b7ges", "fort", ",", "da", "flieht", "mit", "Scher\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Ton, sucht Schirm in meinem Herzen,", "tokens": ["Der", "Ton", ",", "sucht", "Schirm", "in", "mei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dahin verfolgen die Ges\u00e4nge.", "tokens": ["Da\u00b7hin", "ver\u00b7fol\u00b7gen", "die", "Ge\u00b7s\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "O la\u00dft mich doch, ihr B\u00f6sen, frei!", "tokens": ["O", "la\u00dft", "mich", "doch", ",", "ihr", "B\u00f6\u00b7sen", ",", "frei", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie riegeln sich dort ein und sprechen:", "tokens": ["Sie", "rie\u00b7geln", "sich", "dort", "ein", "und", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "PTKVZ", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht weichen wir, bis dies wird brechen,", "tokens": ["Nicht", "wei\u00b7chen", "wir", ",", "bis", "dies", "wird", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "$,", "KOUS", "PDS", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Damit du wei\u00dft, was Lieben sey.", "tokens": ["Da\u00b7mit", "du", "wei\u00dft", ",", "was", "Lie\u00b7ben", "sey", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PWS", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}