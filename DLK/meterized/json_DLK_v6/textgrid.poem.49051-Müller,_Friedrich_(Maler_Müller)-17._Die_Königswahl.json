{"textgrid.poem.49051": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "17. Die K\u00f6nigswahl", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Buhler um die Krone,", "tokens": ["Ihr", "Buh\u00b7ler", "um", "die", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was l\u00e4rmt ihr, rief der Adler?", "tokens": ["Was", "l\u00e4rmt", "ihr", ",", "rief", "der", "Ad\u00b7ler", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dort oben an die Sonne", "tokens": ["Dort", "o\u00b7ben", "an", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat Zeus sie aufgehangen,", "tokens": ["Hat", "Zeus", "sie", "auf\u00b7ge\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Frisch aufw\u00e4rts schwing' sich jeder,", "tokens": ["Frisch", "auf\u00b7w\u00e4rts", "schwing'", "sich", "je\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PRF", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wer Macht hat, sie zu holen.", "tokens": ["Wer", "Macht", "hat", ",", "sie", "zu", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}