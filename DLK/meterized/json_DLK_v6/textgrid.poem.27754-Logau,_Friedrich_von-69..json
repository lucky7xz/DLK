{"textgrid.poem.27754": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "69.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin zwar wol kein Rath aus Gottes Cantzeley;", "tokens": ["Ich", "bin", "zwar", "wol", "kein", "Rath", "aus", "Got\u00b7tes", "Cant\u00b7ze\u00b7ley", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PIAT", "NN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch wei\u00df ich, was daselbst ietzund im Werke sey.", "tokens": ["Doch", "wei\u00df", "ich", ",", "was", "da\u00b7selbst", "ie\u00b7tzund", "im", "Wer\u00b7ke", "sey", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PRELS", "PAV", "ADV", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es wird der kranken Welt ihr Leichendienst bestellet,", "tokens": ["Es", "wird", "der", "kran\u00b7ken", "Welt", "ihr", "Lei\u00b7chen\u00b7dienst", "be\u00b7stel\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil sie ie mehr und mehr in schwere Seuchen f\u00e4llet.", "tokens": ["Weil", "sie", "ie", "mehr", "und", "mehr", "in", "schwe\u00b7re", "Seu\u00b7chen", "f\u00e4l\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "KON", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie hat sich offt kasteit durch Hunger und purgirt", "tokens": ["Sie", "hat", "sich", "offt", "kas\u00b7teit", "durch", "Hun\u00b7ger", "und", "pur\u00b7girt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "ADJD", "APPR", "NN", "KON", "VVFIN"], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Durch Pest; Mars hat ihr auch viel \u00fcbrig Blut entf\u00fchrt:", "tokens": ["Durch", "Pest", ";", "Mars", "hat", "ihr", "auch", "viel", "\u00fcb\u00b7rig", "Blut", "ent\u00b7f\u00fchrt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "NN", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Noch dennoch ists umbsonst. Drum der sich ihr vertrauet,", "tokens": ["Noch", "den\u00b7noch", "ists", "um\u00b7bsonst", ".", "Drum", "der", "sich", "ihr", "ver\u00b7trau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ADV", "$.", "PAV", "ART", "PRF", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Hat f\u00fcr ein sch\u00f6nes Bild ein stinckend Aas erschauet.", "tokens": ["Hat", "f\u00fcr", "ein", "sch\u00f6\u00b7nes", "Bild", "ein", "stin\u00b7ckend", "Aas", "er\u00b7schau\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}