{"textgrid.poem.37744": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Wiegenlied im Freyen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Goldne Wiegen schwingen", "tokens": ["Gold\u00b7ne", "Wie\u00b7gen", "schwin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und die M\u00fccken singen,", "tokens": ["Und", "die", "M\u00fc\u00b7cken", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Blumen sind die Wiegen,", "tokens": ["Blu\u00b7men", "sind", "die", "Wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Kinder drinnen liegen,", "tokens": ["Kin\u00b7der", "drin\u00b7nen", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Auf und nieder geht der Wind,", "tokens": ["Auf", "und", "nie\u00b7der", "geht", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "PTKVZ", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Geht sich warm und geht gelind.", "tokens": ["Geht", "sich", "warm", "und", "geht", "ge\u00b7lind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "KON", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie viel Kinder wiegen?", "tokens": ["Wie", "viel", "Kin\u00b7der", "wie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wie viel soll ich kriegen?", "tokens": ["Wie", "viel", "soll", "ich", "krie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Eins und zwey und dreye,", "tokens": ["Eins", "und", "zwey", "und", "drey\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "CARD", "KON", "CARD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und ich z\u00e4hl auf's neue,", "tokens": ["Und", "ich", "z\u00e4hl", "auf's", "neu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Auf und nieder geht der Wind,", "tokens": ["Auf", "und", "nie\u00b7der", "geht", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "PTKVZ", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Tausend Blumen wiegt geschwind.", "tokens": ["Tau\u00b7send", "Blu\u00b7men", "wiegt", "ge\u00b7schwind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was zu viel, das ist zu viel,", "tokens": ["Was", "zu", "viel", ",", "das", "ist", "zu", "viel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKA", "PIS", "$,", "PDS", "VAFIN", "PTKA", "PIS", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich frage nicht zum Spiel.", "tokens": ["Und", "ich", "fra\u00b7ge", "nicht", "zum", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dr\u00fcben auf den Aesten wiegen", "tokens": ["Dr\u00fc\u00b7ben", "auf", "den", "A\u00b7es\u00b7ten", "wie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Sich zwey Aepfel ganz allein", "tokens": ["Sich", "zwey", "A\u00b7e\u00b7pfel", "ganz", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "CARD", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich zu mir so freundlich biegen", "tokens": ["Sich", "zu", "mir", "so", "freund\u00b7lich", "bie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "PPER", "ADV", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "R\u00f6then ihre Backen klein", "tokens": ["R\u00f6\u00b7then", "ih\u00b7re", "Ba\u00b7cken", "klein"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ich les' in ihren Z\u00fcgen", "tokens": ["Und", "ich", "les'", "in", "ih\u00b7ren", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schuldlos K\u00fcssen, mein Vergn\u00fcgen.", "tokens": ["Schuld\u00b7los", "K\u00fcs\u00b7sen", ",", "mein", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und ich brech mit heiterm Singen", "tokens": ["Und", "ich", "brech", "mit", "hei\u00b7term", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diese Aepfel mir zum Schmaus", "tokens": ["Die\u00b7se", "A\u00b7e\u00b7pfel", "mir", "zum", "Schmaus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "PPER", "APPRART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und aus meinen Wangen dringen", "tokens": ["Und", "aus", "mei\u00b7nen", "Wan\u00b7gen", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch zwey Kinderk\u00f6pf heraus,", "tokens": ["Auch", "zwey", "Kin\u00b7der\u00b7k\u00f6pf", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Werd so viele Kinder bringen", "tokens": ["Werd", "so", "vie\u00b7le", "Kin\u00b7der", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Als hier rothe Aepfel hingen.", "tokens": ["Als", "hier", "ro\u00b7the", "A\u00b7e\u00b7pfel", "hin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}}}}