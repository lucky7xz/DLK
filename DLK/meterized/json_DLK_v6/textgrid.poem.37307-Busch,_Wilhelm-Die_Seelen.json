{"textgrid.poem.37307": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Seelen", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der F\u00e4hrmann lag in seinem Schiff", "tokens": ["Der", "F\u00e4hr\u00b7mann", "lag", "in", "sei\u00b7nem", "Schiff"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beim Schein des Mondenlichts,", "tokens": ["Beim", "Schein", "des", "Mon\u00b7den\u00b7lichts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als etwas kam und rief und pfiff;", "tokens": ["Als", "et\u00b7was", "kam", "und", "rief", "und", "pfiff", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch sehen tat er nichts.", "tokens": ["Doch", "se\u00b7hen", "tat", "er", "nichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "PPER", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihm war, als stiegen hundert ein.", "tokens": ["Ihm", "war", ",", "als", "stie\u00b7gen", "hun\u00b7dert", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "ADJA", "CARD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Schifflein wurde schwer.", "tokens": ["Das", "Schif\u00b7flein", "wur\u00b7de", "schwer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Flink, F\u00e4hrmann, fahr uns \u00fcbern Rhein,", "tokens": ["Flink", ",", "F\u00e4hr\u00b7mann", ",", "fahr", "uns", "\u00fc\u00b7bern", "Rhein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "PPER", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Zahlung folgt nachher.", "tokens": ["Die", "Zah\u00b7lung", "folgt", "nach\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und als er seine Pflicht getan,", "tokens": ["Und", "als", "er", "sei\u00b7ne", "Pflicht", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da ging es klinglingling,", "tokens": ["Da", "ging", "es", "kling\u00b7ling\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da warf ein Goldst\u00fcck in den Kahn", "tokens": ["Da", "warf", "ein", "Gold\u00b7st\u00fcck", "in", "den", "Kahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jedwedes Geisterding.", "tokens": ["Jed\u00b7we\u00b7des", "Geis\u00b7ter\u00b7ding", "."], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Husch, weg und weiter zog die Schar.", "tokens": ["Husch", ",", "weg", "und", "wei\u00b7ter", "zog", "die", "Schar", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verwundert steht der Mann:", "tokens": ["Ver\u00b7wun\u00b7dert", "steht", "der", "Mann", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So Seelen sind zwar unsichtbar,", "tokens": ["So", "See\u00b7len", "sind", "zwar", "un\u00b7sicht\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und doch ist etwas dran.", "tokens": ["Und", "doch", "ist", "et\u00b7was", "dran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}