{"textgrid.poem.46360": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Der Sch\u00e4fer zu dem B\u00fcrger", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du schl\u00e4fst auf weichen Betten, ich schlaf auf weichem Klee,", "tokens": ["Du", "schl\u00e4fst", "auf", "wei\u00b7chen", "Bet\u00b7ten", ",", "ich", "schlaf", "auf", "wei\u00b7chem", "Klee", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Du siehest dich im Spiegel, ich mich in stiller See;", "tokens": ["Du", "sie\u00b7hest", "dich", "im", "Spie\u00b7gel", ",", "ich", "mich", "in", "stil\u00b7ler", "See", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,", "PPER", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Du trittst auf Fu\u00dftapeten, ich tret' auf sanftes Gra\u00df;", "tokens": ["Du", "trittst", "auf", "Fu\u00df\u00b7ta\u00b7pe\u00b7ten", ",", "ich", "tret'", "auf", "sanf\u00b7tes", "Gra\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dich tr\u00e4nken theure Weine, mich tr\u00e4nkt ein wohlfeil Na\u00df;", "tokens": ["Dich", "tr\u00e4n\u00b7ken", "theu\u00b7re", "Wei\u00b7ne", ",", "mich", "tr\u00e4nkt", "ein", "wohl\u00b7feil", "Na\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Du wohnst in bangen Mauren, ich wohn auf freyer Flur;", "tokens": ["Du", "wohnst", "in", "ban\u00b7gen", "Mau\u00b7ren", ",", "ich", "wohn", "auf", "frey\u00b7er", "Flur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "F\u00fcr dich mahlt Pesn' und Hempel, f\u00fcr mich mahlt die Natur;", "tokens": ["F\u00fcr", "dich", "mahlt", "Pesn'", "und", "Hem\u00b7pel", ",", "f\u00fcr", "mich", "mahlt", "die", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "NN", "KON", "NN", "$,", "APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.7": {"text": "Du bist oft siech f\u00fcr Wollust, ich bleibe stets gesund;", "tokens": ["Du", "bist", "oft", "siech", "f\u00fcr", "Wol\u00b7lust", ",", "ich", "blei\u00b7be", "stets", "ge\u00b7sund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "NN", "$,", "PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Dich sch\u00fctzt f\u00fcr Geld ein Schweizer, mich sch\u00fctzt mein treuer Hund;", "tokens": ["Dich", "sch\u00fctzt", "f\u00fcr", "Geld", "ein", "Schwei\u00b7zer", ",", "mich", "sch\u00fctzt", "mein", "treu\u00b7er", "Hund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Du schlummerst ein bey Sayten, ich bey dem Wasserfall;", "tokens": ["Du", "schlum\u00b7merst", "ein", "bey", "Say\u00b7ten", ",", "ich", "bey", "dem", "Was\u00b7ser\u00b7fall", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "APPR", "NN", "$,", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Du h\u00f6rst Castrat und Geiger, ich Lerch und Nachtigal;", "tokens": ["Du", "h\u00f6rst", "Cas\u00b7trat", "und", "Gei\u00b7ger", ",", "ich", "Lerch", "und", "Nach\u00b7ti\u00b7gal", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "KON", "NN", "$,", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Dein Auge sieht oft finster, das meine bleibet hell;", "tokens": ["Dein", "Au\u00b7ge", "sieht", "oft", "fins\u00b7ter", ",", "das", "mei\u00b7ne", "blei\u00b7bet", "hell", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "$,", "PRELS", "PPOSAT", "VVFIN", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Dein M\u00e4dchen gl\u00e4nzt von Schminke, mein M\u00e4dchen gl\u00e4nzt vom Quell!", "tokens": ["Dein", "M\u00e4d\u00b7chen", "gl\u00e4nzt", "von", "Schmin\u00b7ke", ",", "mein", "M\u00e4d\u00b7chen", "gl\u00e4nzt", "vom", "Quell", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}