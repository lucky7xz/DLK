{"textgrid.poem.49020": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "Der Ritt von drei\u00dfig Meilen", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ritt ein Pfalzgraf \u00fcber den Rhein;", "tokens": ["Es", "ritt", "ein", "Pfalz\u00b7graf", "\u00fc\u00b7ber", "den", "Rhein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NE", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Er sang f\u00fcr sich ein Liedelein;", "tokens": ["Er", "sang", "f\u00fcr", "sich", "ein", "Lie\u00b7de\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sang's lieblich auf dreierlei Stimmen, ja Stimmen,", "tokens": ["Sang's", "lieb\u00b7lich", "auf", "drei\u00b7er\u00b7lei", "Stim\u00b7men", ",", "ja", "Stim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "PIAT", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Da\u00df Berg und Tal es erklingen.", "tokens": ["Da\u00df", "Berg", "und", "Tal", "es", "er\u00b7klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Das h\u00f6rt des Markgrafen T\u00f6chterlein", "tokens": ["Das", "h\u00f6rt", "des", "Mark\u00b7gra\u00b7fen", "T\u00f6ch\u00b7ter\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In ihres Vaters Lustk\u00e4mmerlein.", "tokens": ["In", "ih\u00b7res", "Va\u00b7ters", "Lust\u00b7k\u00e4m\u00b7mer\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Sie flocht ihr H\u00e4rlein in Seiden, ja Seiden,", "tokens": ["Sie", "flocht", "ihr", "H\u00e4r\u00b7lein", "in", "Sei\u00b7den", ",", "ja", "Sei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NE", "$,", "ADV", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit dem Rittersmann will sie gern reiten.", "tokens": ["Mit", "dem", "Rit\u00b7ters\u00b7mann", "will", "sie", "gern", "rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Der Ritter bog sich herab vom Ro\u00df,", "tokens": ["Der", "Rit\u00b7ter", "bog", "sich", "her\u00b7ab", "vom", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wohl ihren schlanken Leib umschlo\u00df,", "tokens": ["Wohl", "ih\u00b7ren", "schlan\u00b7ken", "Leib", "um\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie ritten in schnellem Eilen, ja Eilen,", "tokens": ["Sie", "rit\u00b7ten", "in", "schnel\u00b7lem", "Ei\u00b7len", ",", "ja", "Ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den Weg von drei\u00dfig Meilen.", "tokens": ["Den", "Weg", "von", "drei\u00b7\u00dfig", "Mei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}