{"dta.poem.17578": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "200.  Was man liebt/ in das verwandelt  \n man sich. au\u00df   S. Augustino.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.85", "af:0.14"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Mensch was du liebst in das wirstu verwandelt werden/", "tokens": ["Mensch", "was", "du", "liebst", "in", "das", "wirs\u00b7tu", "ver\u00b7wan\u00b7delt", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "PPER", "VVFIN", "APPR", "PDS", "VAFIN", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gott wirstu liebstu GOtt/ und Erde liebstu Erden", "tokens": ["Gott", "wirs\u00b7tu", "liebs\u00b7tu", "Gott", "/", "und", "Er\u00b7de", "liebs\u00b7tu", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "VVFIN", "NN", "$(", "KON", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}