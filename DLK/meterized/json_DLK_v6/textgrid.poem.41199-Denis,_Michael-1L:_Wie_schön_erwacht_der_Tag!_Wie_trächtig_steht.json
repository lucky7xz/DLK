{"textgrid.poem.41199": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie sch\u00f6n erwacht der Tag! Wie tr\u00e4chtig steht", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sch\u00f6n erwacht der Tag! Wie tr\u00e4chtig steht", "tokens": ["Wie", "sch\u00f6n", "er\u00b7wacht", "der", "Tag", "!", "Wie", "tr\u00e4ch\u00b7tig", "steht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$.", "PWAV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von bunten Morgentropfen Laub und Gras!", "tokens": ["Von", "bun\u00b7ten", "Mor\u00b7gen\u00b7trop\u00b7fen", "Laub", "und", "Gras", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie zeichnet Sined's Fu\u00df den Pfad ins Thal", "tokens": ["Wie", "zeich\u00b7net", "Si\u00b7ne\u00b7d's", "Fu\u00df", "den", "Pfad", "ins", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "NE", "NN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Willkommen, Thalbach! der du gestern noch", "tokens": ["Will\u00b7kom\u00b7men", ",", "Thal\u00b7bach", "!", "der", "du", "ge\u00b7stern", "noch"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$.", "ART", "PPER", "ADV", "ADV"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Mir Joseph lispeltest, und o gegr\u00fc\u00dft,", "tokens": ["Mir", "Jo\u00b7se\u00b7ph", "lis\u00b7pel\u00b7test", ",", "und", "o", "ge\u00b7gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "$,", "KON", "FM", "VVPP", "$,"], "meter": "-+--+---+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ihr Weiden um den Thalbach! Sonnenhell", "tokens": ["Ihr", "Wei\u00b7den", "um", "den", "Thal\u00b7bach", "!", "Son\u00b7nen\u00b7hell"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$.", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sind schon die Schwestern alle, derer Haupt", "tokens": ["Sind", "schon", "die", "Schwes\u00b7tern", "al\u00b7le", ",", "de\u00b7rer", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "PIS", "$,", "PDS", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Von Bergen rings umher in's Blaue ragt.", "tokens": ["Von", "Ber\u00b7gen", "rings", "um\u00b7her", "in's", "Blau\u00b7e", "ragt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PTKVZ", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Nur unter euren Zweigen br\u00fctet noch", "tokens": ["Nur", "un\u00b7ter", "eu\u00b7ren", "Zwei\u00b7gen", "br\u00fc\u00b7tet", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ein n\u00e4chtlich K\u00fchl und D\u00e4mm'rung. Aber bald,", "tokens": ["Ein", "n\u00e4cht\u00b7lich", "K\u00fchl", "und", "D\u00e4m\u00b7m'\u00b7rung", ".", "A\u00b7ber", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$.", "KON", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Bald stralet auch auf eu're Niedrigkeit", "tokens": ["Bald", "stra\u00b7let", "auch", "auf", "eu'\u00b7re", "Nied\u00b7rig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Der Sonne Blick. Denn was verbirgt sich ihr?", "tokens": ["Der", "Son\u00b7ne", "Blick", ".", "Denn", "was", "ver\u00b7birgt", "sich", "ihr", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$.", "KON", "PWS", "VVFIN", "PRF", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie geht, wie Joseph. \u2013 Ist es der Durst nach Ruhm,", "tokens": ["Sie", "geht", ",", "wie", "Jo\u00b7se\u00b7ph", ".", "\u2013", "Ist", "es", "der", "Durst", "nach", "Ruhm", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NE", "$.", "$(", "VAFIN", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist's Liebe zu dem Volke, zur Arbeit Lust,", "tokens": ["Ist's", "Lie\u00b7be", "zu", "dem", "Vol\u00b7ke", ",", "zur", "Ar\u00b7beit", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "$,", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Was in des Herrschers hoher Seele", "tokens": ["Was", "in", "des", "Herr\u00b7schers", "ho\u00b7her", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Un\u00fcberstimmlicher, als des Himmels", "tokens": ["Un\u00b7\u00fc\u00b7bers\u00b7timm\u00b7li\u00b7cher", ",", "als", "des", "Him\u00b7mels"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "KOUS", "ART", "NN"], "meter": "+--+-++-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Vereinte Donner, rufet? \u2013 Noch eines ist,", "tokens": ["Ver\u00b7ein\u00b7te", "Don\u00b7ner", ",", "ru\u00b7fet", "?", "\u2013", "Noch", "ei\u00b7nes", "ist", ","], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$.", "$(", "ADV", "PIS", "VAFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "O Sohn Theresen's! eines der V\u00f6lker ist", "tokens": ["O", "Sohn", "The\u00b7re\u00b7sen's", "!", "ei\u00b7nes", "der", "V\u00f6l\u00b7ker", "ist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "NE", "$.", "ART", "ART", "NN", "VAFIN"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Noch unbesuchet, die Dir dienen!", "tokens": ["Noch", "un\u00b7be\u00b7su\u00b7chet", ",", "die", "Dir", "die\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf, und besuche die Deinen alle! \u2013", "tokens": ["Auf", ",", "und", "be\u00b7su\u00b7che", "die", "Dei\u00b7nen", "al\u00b7le", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "$,", "KON", "VVFIN", "ART", "PPOSAT", "PIAT", "$.", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "O Durst nach Ruhm! o Liebe zum Volk'; o Lust", "tokens": ["O", "Durst", "nach", "Ruhm", "!", "o", "Lie\u00b7be", "zum", "Vol\u00b7k'", ";", "o", "Lust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "APPR", "NN", "$.", "FM", "NN", "APPRART", "NN", "$.", "FM", "NN"], "meter": "-+-+-+--++-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zur Arbeit! ihr, ihr seid es vereinet, ihr,", "tokens": ["Zur", "Ar\u00b7beit", "!", "ihr", ",", "ihr", "seid", "es", "ver\u00b7ei\u00b7net", ",", "ihr", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$.", "PPER", "$,", "PPER", "VAFIN", "PPER", "VVFIN", "$,", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die in des Herrschers hoher Seele", "tokens": ["Die", "in", "des", "Herr\u00b7schers", "ho\u00b7her", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Un\u00fcberstimmlicher, als des Himmels", "tokens": ["Un\u00b7\u00fc\u00b7bers\u00b7timm\u00b7li\u00b7cher", ",", "als", "des", "Him\u00b7mels"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "KOUS", "ART", "NN"], "meter": "+--+-++-+-", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Vereinte Donner, rufen! Er h\u00f6ret euch.", "tokens": ["Ver\u00b7ein\u00b7te", "Don\u00b7ner", ",", "ru\u00b7fen", "!", "Er", "h\u00f6\u00b7ret", "euch", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nun bald ist Habsburg's weit sich erstreckendes,", "tokens": ["Nun", "bald", "ist", "Habs\u00b7bur\u00b7g's", "weit", "sich", "er\u00b7stre\u00b7cken\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "NE", "ADJD", "PRF", "PIS", "$,"], "meter": "-+-+--+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bewohnerreiches Erb' ersch\u00f6pfet,", "tokens": ["Be\u00b7woh\u00b7ner\u00b7rei\u00b7ches", "Erb'", "er\u00b7sch\u00f6p\u00b7fet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Keine der Gegenden unbesuchet,", "tokens": ["Kei\u00b7ne", "der", "Ge\u00b7gen\u00b7den", "un\u00b7be\u00b7su\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "ADJD", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Wo Joseph's Mutter herrschet. \u2013 Ha seh't ihr ihn,", "tokens": ["Wo", "Jo\u00b7se\u00b7ph's", "Mut\u00b7ter", "herr\u00b7schet", ".", "\u2013", "Ha", "seh't", "ihr", "ihn", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "VVFIN", "$.", "$(", "ITJ", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Fernen von den Freuden der Kaiserstadt,", "tokens": ["Den", "Fer\u00b7nen", "von", "den", "Freu\u00b7den", "der", "Kai\u00b7ser\u00b7stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den Sonnendulder, im Geleite", "tokens": ["Den", "Son\u00b7nen\u00b7dul\u00b7der", ",", "im", "Ge\u00b7lei\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weniger Ed'len, auf steilen H\u00f6hen,", "tokens": ["We\u00b7ni\u00b7ger", "Ed'\u00b7len", ",", "auf", "stei\u00b7len", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "In unwirthbaren Hainen! Ha seh't ihr ihn", "tokens": ["In", "un\u00b7wirth\u00b7ba\u00b7ren", "Hai\u00b7nen", "!", "Ha", "seh't", "ihr", "ihn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "NE", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf unerme\u00df'nen, d\u00fcrstenden Fl\u00e4chen itzt,", "tokens": ["Auf", "un\u00b7er\u00b7me\u00df'\u00b7nen", ",", "d\u00fcrs\u00b7ten\u00b7den", "Fl\u00e4\u00b7chen", "itzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Itzt unter strohbedeckten H\u00fctten,", "tokens": ["Itzt", "un\u00b7ter", "stroh\u00b7be\u00b7deck\u00b7ten", "H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie er in Mitte des Staubgew\u00f6lkes", "tokens": ["Wie", "er", "in", "Mit\u00b7te", "des", "Staub\u00b7ge\u00b7w\u00f6l\u00b7kes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "NN", "ART", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Die H\u00e4nde zu den Bitten der Seinen streckt,", "tokens": ["Die", "H\u00e4n\u00b7de", "zu", "den", "Bit\u00b7ten", "der", "Sei\u00b7nen", "streckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "PPOSS", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und unerm\u00fcdlich forschet, und h\u00f6rt, und lernt,", "tokens": ["Und", "un\u00b7er\u00b7m\u00fcd\u00b7lich", "for\u00b7schet", ",", "und", "h\u00f6rt", ",", "und", "lernt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und Rath und Recht, und Trost und Lohnung,", "tokens": ["Und", "Rath", "und", "Recht", ",", "und", "Trost", "und", "Loh\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie der Gebieter des Himmels, austheilt!", "tokens": ["Wie", "der", "Ge\u00b7bie\u00b7ter", "des", "Him\u00b7mels", ",", "aust\u00b7heilt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$,", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.9": {"line.1": {"text": "Bet\u00e4ubet steht, und glaubet dem Auge kaum", "tokens": ["Be\u00b7t\u00e4u\u00b7bet", "steht", ",", "und", "glau\u00b7bet", "dem", "Au\u00b7ge", "kaum"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "KON", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der pelzumgeb'ne Dacier, und der Theil", "tokens": ["Der", "pel\u00b7zum\u00b7ge\u00b7b'\u00b7ne", "Da\u00b7cier", ",", "und", "der", "Theil"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Der Kinder Lech's, dem itzt Theresien's", "tokens": ["Der", "Kin\u00b7der", "Lech's", ",", "dem", "itzt", "The\u00b7re\u00b7si\u00b7en's"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NE", "$,", "PRELS", "ADV", "NE"], "meter": "-+-+-+++-+", "measure": "zehnsilber"}, "line.4": {"text": "M\u00e4chtige Fittige wieder schatten.", "tokens": ["M\u00e4ch\u00b7ti\u00b7ge", "Fit\u00b7ti\u00b7ge", "wie\u00b7der", "schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "O F\u00fcrst der F\u00fcrsten, Kenner des hohen Zweck's,", "tokens": ["O", "F\u00fcrst", "der", "F\u00fcrs\u00b7ten", ",", "Ken\u00b7ner", "des", "ho\u00b7hen", "Zweck's", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nach dem Allvater einst die Gewaltigen", "tokens": ["Nach", "dem", "All\u00b7va\u00b7ter", "einst", "die", "Ge\u00b7wal\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "ART", "NN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Der Erde richtet! o des hohen,", "tokens": ["Der", "Er\u00b7de", "rich\u00b7tet", "!", "o", "des", "ho\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "FM", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aber nicht immer erf\u00fcllten Zweckes:", "tokens": ["A\u00b7ber", "nicht", "im\u00b7mer", "er\u00b7f\u00fcll\u00b7ten", "Zw