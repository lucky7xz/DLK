{"textgrid.poem.48524": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "27. Eine Andere", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor kunt' ich stehen gleichwie du;", "tokens": ["Vor", "kunt'", "ich", "ste\u00b7hen", "gleich\u00b7wie", "du", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VMFIN", "PPER", "VVINF", "KON", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "nun lieg' ich in der langen Ruh'.", "tokens": ["nun", "lieg'", "ich", "in", "der", "lan\u00b7gen", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denk, Leser, der du itzt kanst stehn,", "tokens": ["Denk", ",", "Le\u00b7ser", ",", "der", "du", "itzt", "kanst", "stehn", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PRELS", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "da\u00df du auch mu\u00dft so liegen gehn!", "tokens": ["da\u00df", "du", "auch", "mu\u00dft", "so", "lie\u00b7gen", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "ADV", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}