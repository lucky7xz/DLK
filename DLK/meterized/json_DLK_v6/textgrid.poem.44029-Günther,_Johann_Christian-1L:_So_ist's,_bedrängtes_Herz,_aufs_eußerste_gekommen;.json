AV", "$,", "PPOSAT", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und endlich auch der Welt die Pflichten zu erweisen,", "tokens": ["Und", "end\u00b7lich", "auch", "der", "Welt", "die", "Pflich\u00b7ten", "zu", "er\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Worzu mir die Natur ein gutes Pfund verliehn.", "tokens": ["Wor\u00b7zu", "mir", "die", "Na\u00b7tur", "ein", "gu\u00b7tes", "Pfund", "ver\u00b7liehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}