{"textgrid.poem.44029": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: So ist's, bedr\u00e4ngtes Herz, aufs eu\u00dferste gekommen;", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So ist's, bedr\u00e4ngtes Herz, aufs eu\u00dferste gekommen;", "tokens": ["So", "ist's", ",", "be\u00b7dr\u00e4ng\u00b7tes", "Herz", ",", "aufs", "eu\u00b7\u00dfers\u00b7te", "ge\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "ADJA", "NN", "$,", "APPRART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Das Elend hat den Lenz des Alters mitgenommen,", "tokens": ["Das", "E\u00b7lend", "hat", "den", "Lenz", "des", "Al\u00b7ters", "mit\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Schmach, Armuth, Schmerz und M\u00fch gebiehrt noch keine Ruh,", "tokens": ["Schmach", ",", "Ar\u00b7muth", ",", "Schmerz", "und", "M\u00fch", "ge\u00b7biehrt", "noch", "kei\u00b7ne", "Ruh", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein Erbtheil ist verraucht, die G\u00f6nner sind verblichen,", "tokens": ["Mein", "E\u00b7rbtheil", "ist", "ver\u00b7raucht", ",", "die", "G\u00f6n\u00b7ner", "sind", "ver\u00b7bli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Eltern Herz verstockt, der beste Freund gewichen,", "tokens": ["Der", "El\u00b7tern", "Herz", "ver\u00b7stockt", ",", "der", "bes\u00b7te", "Freund", "ge\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wo mein Jammer klopft, da schl\u00e4gt die Th\u00fcre zu.", "tokens": ["Und", "wo", "mein", "Jam\u00b7mer", "klopft", ",", "da", "schl\u00e4gt", "die", "Th\u00fc\u00b7re", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der P\u00f6bel \u00e4rgert sich an diesem leichten Kleide;", "tokens": ["Der", "P\u00f6\u00b7bel", "\u00e4r\u00b7gert", "sich", "an", "die\u00b7sem", "leich\u00b7ten", "Klei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein Armer ist so schlecht, er hat noch eine Freude,", "tokens": ["Kein", "Ar\u00b7mer", "ist", "so", "schlecht", ",", "er", "hat", "noch", "ei\u00b7ne", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nur mich erquickt und st\u00e4rckt kein Augenblick voll Lust.", "tokens": ["Nur", "mich", "er\u00b7quickt", "und", "st\u00e4rckt", "kein", "Au\u00b7gen\u00b7blick", "voll", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVPP", "KON", "VVFIN", "PIAT", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich habe keinen Ort, wohin mein Haupt sich lege,", "tokens": ["Ich", "ha\u00b7be", "kei\u00b7nen", "Ort", ",", "wo\u00b7hin", "mein", "Haupt", "sich", "le\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PWAV", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.5": {"text": "Bin nirgends angenehm und \u00fcberall im Wege", "tokens": ["Bin", "nir\u00b7gends", "an\u00b7ge\u00b7nehm", "und", "\u00fc\u00b7be\u00b7rall", "im", "We\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJD", "KON", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und finde, wo ich fall, kein Mitleid treuer Brust.", "tokens": ["Und", "fin\u00b7de", ",", "wo", "ich", "fall", ",", "kein", "Mit\u00b7leid", "treu\u00b7er", "Brust", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "NN", "$,", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zehl jemand, wo er kan, den Grie\u00df am Oderstrande,", "tokens": ["Zehl", "je\u00b7mand", ",", "wo", "er", "kan", ",", "den", "Grie\u00df", "am", "O\u00b7der\u00b7stran\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "$,", "PWAV", "PPER", "VMFIN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Schuppen in der See, die Gr\u00e4ser auf dem Lande,", "tokens": ["Die", "Schup\u00b7pen", "in", "der", "See", ",", "die", "Gr\u00e4\u00b7ser", "auf", "dem", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So hat er gleich das Maas der Seufzer meiner Pein,", "tokens": ["So", "hat", "er", "gleich", "das", "Maas", "der", "Seuf\u00b7zer", "mei\u00b7ner", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Seufzer, die es stets so treu und redlich meinen,", "tokens": ["Der", "Seuf\u00b7zer", ",", "die", "es", "stets", "so", "treu", "und", "red\u00b7lich", "mei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Jugend Schwachheitsschuld erkennen und beweinen", "tokens": ["Der", "Ju\u00b7gend", "Schwach\u00b7heits\u00b7schuld", "er\u00b7ken\u00b7nen", "und", "be\u00b7wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und doch so lang umsonst nach Trost und H\u00fclfe schreyn.", "tokens": ["Und", "doch", "so", "lang", "um\u00b7sonst", "nach", "Trost", "und", "H\u00fcl\u00b7fe", "schreyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "ADV", "APPR", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie wenig, liebster Gott, bed\u00fcrft ich, mich zu retten,", "tokens": ["Wie", "we\u00b7nig", ",", "liebs\u00b7ter", "Gott", ",", "be\u00b7d\u00fcrft", "ich", ",", "mich", "zu", "ret\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn Argwohn, List und Wahn an mir kein Greuel h\u00e4tten", "tokens": ["Wenn", "Arg\u00b7wohn", ",", "List", "und", "Wahn", "an", "mir", "kein", "Greu\u00b7el", "h\u00e4t\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN", "APPR", "PPER", "PIAT", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und Glimpf und Billigkeit bey einem Richter w\u00e4r!", "tokens": ["Und", "Glimpf", "und", "Bil\u00b7lig\u00b7keit", "bey", "ei\u00b7nem", "Rich\u00b7ter", "w\u00e4r", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So aber w\u00fctet man mit Donner, Bliz und Flammen,", "tokens": ["So", "a\u00b7ber", "w\u00fc\u00b7tet", "man", "mit", "Don\u00b7ner", ",", "Bliz", "und", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Verst\u00f6\u00dft mich ungeh\u00f6rt und macht mir durch Verdammen", "tokens": ["Ver\u00b7st\u00f6\u00dft", "mich", "un\u00b7ge\u00b7h\u00f6rt", "und", "macht", "mir", "durch", "Ver\u00b7dam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den Weg zur Be\u00dferung auf allen Seiten schwer.", "tokens": ["Den", "Weg", "zur", "Be\u00b7\u00dfe\u00b7rung", "auf", "al\u00b7len", "Sei\u00b7ten", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Man misbraucht Gottes Wort, um mich nur recht zu qu\u00e4len,", "tokens": ["Man", "mis\u00b7braucht", "Got\u00b7tes", "Wort", ",", "um", "mich", "nur", "recht", "zu", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "NN", "$,", "KOUI", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man sucht mir das Vertraun auf de\u00dfen Huld zu stehlen,", "tokens": ["Man", "sucht", "mir", "das", "Ver\u00b7traun", "auf", "de\u00b7\u00dfen", "Huld", "zu", "steh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Der auch den \u00e4rgsten Mensch mit Lieb und Trost bekehrt:", "tokens": ["Der", "auch", "den", "\u00e4rgs\u00b7ten", "Mensch", "mit", "Lieb", "und", "Trost", "be\u00b7kehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da schreyt die Heucheley den G\u00f6nnern in die Ohren,", "tokens": ["Da", "schreyt", "die", "Heu\u00b7che\u00b7ley", "den", "G\u00f6n\u00b7nern", "in", "die", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich als verworfnes Kind sey w\u00fcrcklich schon verloren", "tokens": ["Ich", "als", "ver\u00b7worf\u00b7nes", "Kind", "sey", "w\u00fcrck\u00b7lich", "schon", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "KOUS", "ADJA", "NN", "VAFIN", "ADJD", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und folglich keiner Gunst noch H\u00fclfe weiter werth.", "tokens": ["Und", "folg\u00b7lich", "kei\u00b7ner", "Gunst", "noch", "H\u00fcl\u00b7fe", "wei\u00b7ter", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "ADV", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wer glaubt die Grausamkeit von v\u00e4terlichen H\u00e4nden?", "tokens": ["Wer", "glaubt", "die", "Grau\u00b7sam\u00b7keit", "von", "v\u00e4\u00b7ter\u00b7li\u00b7chen", "H\u00e4n\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er z\u00fcrnt, er l\u00e4st es auch dabey noch nicht bewenden,", "tokens": ["Er", "z\u00fcrnt", ",", "er", "l\u00e4st", "es", "auch", "da\u00b7bey", "noch", "nicht", "be\u00b7wen\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "PAV", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df er allein vor sich sein Fleisch und Blut verschm\u00e4ht,", "tokens": ["Da\u00df", "er", "al\u00b7lein", "vor", "sich", "sein", "Fleisch", "und", "Blut", "ver\u00b7schm\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PRF", "PPOSAT", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er zeugt mir fremden Ha\u00df, l\u00e4st andre Feuer blasen,", "tokens": ["Er", "zeugt", "mir", "frem\u00b7den", "Ha\u00df", ",", "l\u00e4st", "and\u00b7re", "Feu\u00b7er", "bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "$,", "VVFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Weckt selber meinen Feind, ist's Einfalt oder Rasen?", "tokens": ["Weckt", "sel\u00b7ber", "mei\u00b7nen", "Feind", ",", "ist's", "Ein\u00b7falt", "o\u00b7der", "Ra\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$,", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sprecht, sagt es frey heraus, die ihr das Recht verdreht!", "tokens": ["Sprecht", ",", "sagt", "es", "frey", "he\u00b7raus", ",", "die", "ihr", "das", "Recht", "ver\u00b7dreht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Gedult, geplagtes Herz, Gedult, betr\u00fcbte Sinnen;", "tokens": ["Ge\u00b7dult", ",", "ge\u00b7plag\u00b7tes", "Herz", ",", "Ge\u00b7dult", ",", "be\u00b7tr\u00fcb\u00b7te", "Sin\u00b7nen", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "NN", "$,", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vielleicht wird Zeit und Flei\u00df dem Unstern abgewinnen,", "tokens": ["Viel\u00b7leicht", "wird", "Zeit", "und", "Flei\u00df", "dem", "Uns\u00b7tern", "ab\u00b7ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Ehrbegierde kommt und steckt die Geister an;", "tokens": ["Die", "Ehr\u00b7be\u00b7gier\u00b7de", "kommt", "und", "steckt", "die", "Geis\u00b7ter", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nimm allen Trost von dir und von des Himmels G\u00fcte", "tokens": ["Nimm", "al\u00b7len", "Trost", "von", "dir", "und", "von", "des", "Him\u00b7mels", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PIAT", "NN", "APPR", "PPER", "KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und opfre seiner Huld mit feurigem Gem\u00fcthe,", "tokens": ["Und", "opf\u00b7re", "sei\u00b7ner", "Huld", "mit", "feu\u00b7ri\u00b7gem", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du weist ja, da\u00df er dich nicht ewig la\u00dfen kan.", "tokens": ["Du", "weist", "ja", ",", "da\u00df", "er", "dich", "nicht", "e\u00b7wig", "la\u00b7\u00dfen", "kan", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Dein Zustand kan gar bald ein ander Aussehn kriegen,", "tokens": ["Dein", "Zu\u00b7stand", "kan", "gar", "bald", "ein", "an\u00b7der", "Aus\u00b7sehn", "krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es heist ja die Gefahr der Anfang zu dem Siegen", "tokens": ["Es", "heist", "ja", "die", "Ge\u00b7fahr", "der", "An\u00b7fang", "zu", "dem", "Sie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und diese deine Noth der Grund vom Wohlergehn.", "tokens": ["Und", "die\u00b7se", "dei\u00b7ne", "Noth", "der", "Grund", "vom", "Woh\u00b7ler\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PPOSAT", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gebeth, Verstand und Wiz und Klugheit, sich zu schicken,", "tokens": ["Ge\u00b7beth", ",", "Ver\u00b7stand", "und", "Wiz", "und", "Klug\u00b7heit", ",", "sich", "zu", "schi\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "KON", "NN", "$,", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Verm\u00f6gen allzeit mehr als die, so h\u00e4misch dr\u00fccken", "tokens": ["Ver\u00b7m\u00f6\u00b7gen", "all\u00b7zeit", "mehr", "als", "die", ",", "so", "h\u00e4\u00b7misch", "dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "PIAT", "KOKOM", "ART", "$,", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und ohne Fug und Recht der Unschuld widerstehn.", "tokens": ["Und", "oh\u00b7ne", "Fug", "und", "Recht", "der", "Un\u00b7schuld", "wi\u00b7der\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Auch darauf kommt's nicht an, da\u00df viel von deinen Jahren", "tokens": ["Auch", "da\u00b7rauf", "kommt's", "nicht", "an", ",", "da\u00df", "viel", "von", "dei\u00b7nen", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PAV", "VVFIN", "PTKNEG", "PTKVZ", "$,", "KOUS", "PIS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir unter fauler Angst wie Traum und Strom entfahren.", "tokens": ["Dir", "un\u00b7ter", "fau\u00b7ler", "Angst", "wie", "Traum", "und", "Strom", "ent\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "KOKOM", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was weg ist, gilt nicht mehr, der Ernst bringt alles ein,", "tokens": ["Was", "weg", "ist", ",", "gilt", "nicht", "mehr", ",", "der", "Ernst", "bringt", "al\u00b7les", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "$,", "VVFIN", "PTKNEG", "ADV", "$,", "ART", "NN", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Hofnung st\u00e4rckt den Muth, erleichtert Schmerz und B\u00fcrde.", "tokens": ["Die", "Hof\u00b7nung", "st\u00e4rckt", "den", "Muth", ",", "er\u00b7leich\u00b7tert", "Schmerz", "und", "B\u00fcr\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "VVPP", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Erlangstu nur einmahl der Wei\u00dfheit halbe W\u00fcrde,", "tokens": ["Er\u00b7langs\u00b7tu", "nur", "ein\u00b7mahl", "der", "Wei\u00df\u00b7heit", "hal\u00b7be", "W\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So kan dein Reichthum dir noch mehr als Cronen seyn.", "tokens": ["So", "kan", "dein", "Reicht\u00b7hum", "dir", "noch", "mehr", "als", "Cro\u00b7nen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "PPER", "ADV", "ADV", "KOUS", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Mein Vater und mein Schuz, du Brunnquell reiner Liebe,", "tokens": ["Mein", "Va\u00b7ter", "und", "mein", "Schuz", ",", "du", "Brunn\u00b7quell", "rei\u00b7ner", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,", "PPER", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In de\u00dfen Warheit ich Verstand und Sinnen \u00fcbe,", "tokens": ["In", "de\u00b7\u00dfen", "War\u00b7heit", "ich", "Ver\u00b7stand", "und", "Sin\u00b7nen", "\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach sprich doch auch in mir: Es werde wieder Licht!", "tokens": ["Ach", "sprich", "doch", "auch", "in", "mir", ":", "Es", "wer\u00b7de", "wie\u00b7der", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ADV", "ADV", "APPR", "PPER", "$.", "PPER", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dich kennen ist allein die Wei\u00dfheit, so ich suche;", "tokens": ["Dich", "ken\u00b7nen", "ist", "al\u00b7lein", "die", "Wei\u00df\u00b7heit", ",", "so", "ich", "su\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VAFIN", "ADV", "ART", "NN", "$,", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich seh sie in der Welt, in diesem gro\u00dfen Buche,", "tokens": ["Ich", "seh", "sie", "in", "der", "Welt", ",", "in", "die\u00b7sem", "gro\u00b7\u00dfen", "Bu\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Worinnen jeder Punct von deiner Gr\u00f6\u00dfe spricht.", "tokens": ["Wo\u00b7rin\u00b7nen", "je\u00b7der", "Punct", "von", "dei\u00b7ner", "Gr\u00f6\u00b7\u00dfe", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "So deutlich und so wahr ich jezt mich selbst empfinde,", "tokens": ["So", "deut\u00b7lich", "und", "so", "wahr", "ich", "jezt", "mich", "selbst", "emp\u00b7fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "PPER", "ADV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So fest versprech ich mir Vergebung jeder S\u00fcnde,", "tokens": ["So", "fest", "ver\u00b7sprech", "ich", "mir", "Ver\u00b7ge\u00b7bung", "je\u00b7der", "S\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PPER", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und so gewis du bist, so starck ist mein Vertraun.", "tokens": ["Und", "so", "ge\u00b7wis", "du", "bist", ",", "so", "starck", "ist", "mein", "Ver\u00b7traun", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PPER", "VAFIN", "$,", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du hast mich l\u00e4ngst erwehlt, ach hilf mich jezt bekehren,", "tokens": ["Du", "hast", "mich", "l\u00e4ngst", "er\u00b7wehlt", ",", "ach", "hilf", "mich", "jezt", "be\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,", "XY", "XY", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Damit ich w\u00fcrdig sey, die Mittel zu begehren,", "tokens": ["Da\u00b7mit", "ich", "w\u00fcr\u00b7dig", "sey", ",", "die", "Mit\u00b7tel", "zu", "be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die inn- und eu\u00dferlich mein Gl\u00fccke fester baun.", "tokens": ["Die", "in\u00b7n", "und", "eu\u00b7\u00dfer\u0