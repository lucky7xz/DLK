{"textgrid.poem.63316": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Schwinds\u00fcchtige", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie m\u00fcssen ruh'n und ruh'n und wieder ruh'n,", "tokens": ["Sie", "m\u00fcs\u00b7sen", "ruh'n", "und", "ruh'n", "und", "wie\u00b7der", "ruh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "KON", "VVFIN", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Teils auf den patentierten Liegest\u00fchlen", "tokens": ["Teils", "auf", "den", "pa\u00b7ten\u00b7tier\u00b7ten", "Lie\u00b7ge\u00b7st\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sieht man in Wolle ", "tokens": ["Sieht", "man", "in", "Wol\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "NE"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Teils haben sie im Bette Kur zu tun.", "tokens": ["Teils", "ha\u00b7ben", "sie", "im", "Bet\u00b7te", "Kur", "zu", "tun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPRART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nur mittags hocken kr\u00f6tig sie bei Tisch", "tokens": ["Nur", "mit\u00b7tags", "ho\u00b7cken", "kr\u00f6\u00b7tig", "sie", "bei", "Tisch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "ADJD", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und schlingen Speisen: fett und s\u00fc\u00df und zahlreich.", "tokens": ["Und", "schlin\u00b7gen", "Spei\u00b7sen", ":", "fett", "und", "s\u00fc\u00df", "und", "zahl\u00b7reich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$.", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf einmal klingt ein Frauenlachen, qualreich,", "tokens": ["Auf", "ein\u00b7mal", "klingt", "ein", "Frau\u00b7en\u00b7la\u00b7chen", ",", "qual\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie eine Aeolsharfe zauberisch.", "tokens": ["Wie", "ei\u00b7ne", "A\u00b7e\u00b7ols\u00b7har\u00b7fe", "zau\u00b7be\u00b7risch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Vielleicht, da\u00df einer dann zum Gehn sich wendet,", "tokens": ["Viel\u00b7leicht", ",", "da\u00df", "ei\u00b7ner", "dann", "zum", "Gehn", "sich", "wen\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "ADV", "APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u2013 Er ist am n\u00e4chsten Tage nicht mehr da \u2013", "tokens": ["\u2013", "Er", "ist", "am", "n\u00e4chs\u00b7ten", "Ta\u00b7ge", "nicht", "mehr", "da", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "APPRART", "ADJA", "NN", "PTKNEG", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und seine Stumpfheit mit dem Browning endet...", "tokens": ["Und", "sei\u00b7ne", "Stumpf\u00b7heit", "mit", "dem", "Brow\u00b7ning", "en\u00b7det", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein andrer macht sich dick und rund und rot.", "tokens": ["Ein", "an\u00b7drer", "macht", "sich", "dick", "und", "rund", "und", "rot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PRF", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die \u00c4rzte wiehern stolz: Halleluja!", "tokens": ["Die", "\u00c4rz\u00b7te", "wie\u00b7hern", "stolz", ":", "Hal\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$.", "ITJ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er ward gesund! (und ward ein Halbidiot...)", "tokens": ["Er", "ward", "ge\u00b7sund", "!", "(", "und", "ward", "ein", "Hal\u00b7bi\u00b7di\u00b7ot", "...", ")"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "$(", "KON", "VAFIN", "ART", "NN", "$(", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}