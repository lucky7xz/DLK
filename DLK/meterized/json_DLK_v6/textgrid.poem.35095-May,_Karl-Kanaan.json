{"textgrid.poem.35095": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Kanaan", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verla\u00df mich nicht! Ich steh im dunkeln Land.", "tokens": ["Ver\u00b7la\u00df", "mich", "nicht", "!", "Ich", "steh", "im", "dun\u00b7keln", "Land", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fchr mich zur Wahrheit, Herr, an deiner Hand.", "tokens": ["F\u00fchr", "mich", "zur", "Wahr\u00b7heit", ",", "Herr", ",", "an", "dei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich sehne mich nach deinem Licht;", "tokens": ["Ich", "seh\u00b7ne", "mich", "nach", "dei\u00b7nem", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verla\u00df mich nicht, o Herr, verla\u00df mich nicht!", "tokens": ["Ver\u00b7la\u00df", "mich", "nicht", ",", "o", "Herr", ",", "ver\u00b7la\u00df", "mich", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "$,", "FM", "NN", "$,", "VVIMP", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Verla\u00df mich nicht! Herr, h\u00f6r mein Flehen an.", "tokens": ["Ver\u00b7la\u00df", "mich", "nicht", "!", "Herr", ",", "h\u00f6r", "mein", "Fle\u00b7hen", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "$.", "NN", "$,", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hin\u00fcber schaut mein Aug nach Kanaan.", "tokens": ["Hin\u00b7\u00fc\u00b7ber", "schaut", "mein", "Aug", "nach", "Ka\u00b7naan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NE", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Gieb mir, was dein Prophet verspricht;", "tokens": ["Gieb", "mir", ",", "was", "dein", "Pro\u00b7phet", "ver\u00b7spricht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verla\u00df mich nicht, o Herr, verla\u00df mich nicht!", "tokens": ["Ver\u00b7la\u00df", "mich", "nicht", ",", "o", "Herr", ",", "ver\u00b7la\u00df", "mich", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "$,", "FM", "NN", "$,", "VVIMP", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Verla\u00df mich nicht! Es winkt mir Zion schon.", "tokens": ["Ver\u00b7la\u00df", "mich", "nicht", "!", "Es", "winkt", "mir", "Zi\u00b7on", "schon", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "PPER", "NE", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich seh den Himmelsglanz um deinen Thron.", "tokens": ["Ich", "seh", "den", "Him\u00b7mels\u00b7glanz", "um", "dei\u00b7nen", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es leuchtet mir dein Angesicht;", "tokens": ["Es", "leuch\u00b7tet", "mir", "dein", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verla\u00df mich nicht, o Herr, verla\u00df mich nicht!", "tokens": ["Ver\u00b7la\u00df", "mich", "nicht", ",", "o", "Herr", ",", "ver\u00b7la\u00df", "mich", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "$,", "FM", "NN", "$,", "VVIMP", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}