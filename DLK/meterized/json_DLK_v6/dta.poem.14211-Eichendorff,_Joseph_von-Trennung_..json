{"dta.poem.14211": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Trennung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ob Du die Mutter auch belogst, betr\u00fcbtest,", "tokens": ["Ob", "Du", "die", "Mut\u00b7ter", "auch", "be\u00b7logst", ",", "be\u00b7tr\u00fcb\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was andre Leute dr\u00fcber deuten, sagen \u2014", "tokens": ["Was", "and\u00b7re", "Leu\u00b7te", "dr\u00fc\u00b7ber", "deu\u00b7ten", ",", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "PAV", "VVINF", "$,", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sonst scheu \u2014 heut mocht'st Du nichts nach allem", "tokens": ["Sonst", "scheu", "heut", "mocht'st", "Du", "nichts", "nach", "al\u00b7lem"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$(", "ADV", "VMFIN", "PPER", "PIS", "APPR", "PIS"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "fragen,", "tokens": ["fra\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Mir einzig zeigen nur, wie Du mich liebtest.", "tokens": ["Mir", "ein\u00b7zig", "zei\u00b7gen", "nur", ",", "wie", "Du", "mich", "lieb\u00b7test", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "ADV", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und aus dem Hause heimlich so entwichen,", "tokens": ["Und", "aus", "dem", "Hau\u00b7se", "heim\u00b7lich", "so", "ent\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Gabst Du in's Feld mir schweigend das Geleite,", "tokens": ["Gabst", "Du", "in's", "Feld", "mir", "schwei\u00b7gend", "das", "Ge\u00b7lei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Vor uns das Thal, das hoffnungsreiche, weite,", "tokens": ["Vor", "uns", "das", "Thal", ",", "das", "hoff\u00b7nungs\u00b7rei\u00b7che", ",", "wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und hinter uns kam grau die Nacht geschlichen.", "tokens": ["Und", "hin\u00b7ter", "uns", "kam", "grau", "die", "Nacht", "ge\u00b7schli\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}