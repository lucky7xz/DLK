{"textgrid.poem.25278": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Marie gebenedeite,", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Marie gebenedeite,", "tokens": ["Ma\u00b7rie", "ge\u00b7be\u00b7ne\u00b7dei\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Kind und Myrtenkrone,", "tokens": ["Mit", "Kind", "und", "Myr\u00b7ten\u00b7kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verbleib nicht in der Weite", "tokens": ["Ver\u00b7bleib", "nicht", "in", "der", "Wei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf hehrem Sternenthrone!", "tokens": ["Auf", "heh\u00b7rem", "Ster\u00b7nen\u00b7thro\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Kehr in die H\u00fctten ein", "tokens": ["Kehr", "in", "die", "H\u00fct\u00b7ten", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und mir im Busen wohne!", "tokens": ["Und", "mir", "im", "Bu\u00b7sen", "woh\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es hat das Reich der Himmel", "tokens": ["Es", "hat", "das", "Reich", "der", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hienieden allen Raum.", "tokens": ["Hien\u00b7ie\u00b7den", "al\u00b7len", "Raum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df fern im Morgenland", "tokens": ["Da\u00df", "fern", "im", "Mor\u00b7gen\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein Eden bl\u00fcht, ist Traum.", "tokens": ["Ein", "E\u00b7den", "bl\u00fcht", ",", "ist", "Traum", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die wache Seele fand", "tokens": ["Die", "wa\u00b7che", "See\u00b7le", "fand"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In sich den Lebensbaum.", "tokens": ["In", "sich", "den", "Le\u00b7bens\u00b7baum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sei, Seele, du Marie,", "tokens": ["Sei", ",", "See\u00b7le", ",", "du", "Ma\u00b7rie", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NN", "$,", "PPER", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die keusche Gottesmaid,", "tokens": ["Die", "keu\u00b7sche", "Got\u00b7tes\u00b7maid", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom Licht aus Vaterscho\u00df", "tokens": ["Vom", "Licht", "aus", "Va\u00b7ter\u00b7scho\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Umflutet und umfreit,", "tokens": ["Um\u00b7flu\u00b7tet", "und", "um\u00b7freit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In Minne makellos", "tokens": ["In", "Min\u00b7ne", "ma\u00b7kel\u00b7los"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Zur Mutterschaft geweiht.", "tokens": ["Zur", "Mut\u00b7ter\u00b7schaft", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Zu Bethlehem die Krippe", "tokens": ["Zu", "Beth\u00b7le\u00b7hem", "die", "Krip\u00b7pe"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist jedes Herzens Schrein.", "tokens": ["Ist", "je\u00b7des", "Her\u00b7zens", "Schrein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Soll mich und meine Sippe", "tokens": ["Soll", "mich", "und", "mei\u00b7ne", "Sip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Gottessohn befrein,", "tokens": ["Der", "Got\u00b7tes\u00b7sohn", "be\u00b7fr\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er mu\u00df aus Menschengrunde,", "tokens": ["Er", "mu\u00df", "aus", "Men\u00b7schen\u00b7grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Aus mir geboren sein.", "tokens": ["Aus", "mir", "ge\u00b7bo\u00b7ren", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}