{"dta.poem.23552": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Salvator mundi.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des deutschen Kaisers Kammerknechte", "tokens": ["Des", "deut\u00b7schen", "Kai\u00b7sers", "Kam\u00b7mer\u00b7knech\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sind jetzt Europas Kammerherrn.", "tokens": ["Sind", "jetzt", "Eu\u00b7ro\u00b7pas", "Kam\u00b7mer\u00b7herrn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am Himmel aller Erdenm\u00e4chte,", "tokens": ["Am", "Him\u00b7mel", "al\u00b7ler", "Er\u00b7den\u00b7m\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O Israel, wie gl\u00e4nzt dein Stern!", "tokens": ["O", "Is\u00b7rael", ",", "wie", "gl\u00e4nzt", "dein", "Stern", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Es ward die Zeit wohl immer b\u00f6ser", "tokens": ["Es", "ward", "die", "Zeit", "wohl", "im\u00b7mer", "b\u00f6\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und immer h\u00f6her stieg die Schuld,", "tokens": ["Und", "im\u00b7mer", "h\u00f6\u00b7her", "stieg", "die", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da spro\u00df aus dir uns der Erl\u00f6ser,", "tokens": ["Da", "spro\u00df", "aus", "dir", "uns", "der", "Er\u00b7l\u00f6\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "PRF", "ART", "NN", "$,"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Und Rothschild kam in Gnad' und Huld.", "tokens": ["Und", "Roth\u00b7schild", "kam", "in", "Gnad'", "und", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja, er ist der Erl\u00f6ser worden", "tokens": ["Ja", ",", "er", "ist", "der", "Er\u00b7l\u00f6\u00b7ser", "wor\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "NN", "VAPP"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "F\u00fcr diese schuldenvolle Welt,", "tokens": ["F\u00fcr", "die\u00b7se", "schul\u00b7den\u00b7vol\u00b7le", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geschm\u00fcckt mit dem Erl\u00f6serorden", "tokens": ["Ge\u00b7schm\u00fcckt", "mit", "dem", "Er\u00b7l\u00f6\u00b7se\u00b7ror\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hat er vergossen all sein Blut.", "tokens": ["Hat", "er", "ver\u00b7gos\u00b7sen", "all", "sein", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}