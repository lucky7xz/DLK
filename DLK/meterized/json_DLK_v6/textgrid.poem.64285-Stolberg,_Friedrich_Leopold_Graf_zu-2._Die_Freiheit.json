{"textgrid.poem.64285": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "2. Die Freiheit", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freiheit! Der H\u00f6fling kennt den Gedanken nicht!", "tokens": ["Frei\u00b7heit", "!", "Der", "H\u00f6f\u00b7ling", "kennt", "den", "Ge\u00b7dan\u00b7ken", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Der Sklave! Ketten rasseln ihm Silberton!", "tokens": ["Der", "Skla\u00b7ve", "!", "Ket\u00b7ten", "ras\u00b7seln", "ihm", "Sil\u00b7ber\u00b7ton", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NN", "VVFIN", "PPER", "NE", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Gebeugt das Knie, gebeugt die Seele,", "tokens": ["Ge\u00b7beugt", "das", "Knie", ",", "ge\u00b7beugt", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Reicht er dem Joch den erschlafften Nacken!", "tokens": ["Reicht", "er", "dem", "Joch", "den", "er\u00b7schlaff\u00b7ten", "Na\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Uns, uns ein hoher seelenverkl\u00e4render", "tokens": ["Uns", ",", "uns", "ein", "ho\u00b7her", "see\u00b7len\u00b7ver\u00b7kl\u00e4\u00b7ren\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gedanke! Freiheit! Freiheit! wir f\u00fchlen dich!", "tokens": ["Ge\u00b7dan\u00b7ke", "!", "Frei\u00b7heit", "!", "Frei\u00b7heit", "!", "wir", "f\u00fch\u00b7len", "dich", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Du Wort, du Kraft, du Lohn von Gott uns!", "tokens": ["Du", "Wort", ",", "du", "Kraft", ",", "du", "Lohn", "von", "Gott", "uns", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "NN", "$,", "PPER", "NN", "APPR", "NN", "PPER", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O! wo noch voller ins Herz der Helden", "tokens": ["O", "!", "wo", "noch", "vol\u00b7ler", "ins", "Herz", "der", "Hel\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PWAV", "ADV", "ADJD", "APPRART", "NN", "ART", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Dein Nektar str\u00f6mte, jener, an deren Grab", "tokens": ["Dein", "Nek\u00b7tar", "str\u00f6m\u00b7te", ",", "je\u00b7ner", ",", "an", "de\u00b7ren", "Grab"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PDS", "$,", "APPR", "PRELAT", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Nachwelten staunen; str\u00f6m'! o entflamm' uns ganz!", "tokens": ["Nach\u00b7wel\u00b7ten", "stau\u00b7nen", ";", "str\u00f6m'", "!", "o", "ent\u00b7flamm'", "uns", "ganz", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "VVFIN", "$.", "FM", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Denn sieh', in deutscher Sklaven H\u00e4nden", "tokens": ["Denn", "sieh'", ",", "in", "deut\u00b7scher", "Skla\u00b7ven", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "$,", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rostet der Stahl, ist entnervt die Harfe!", "tokens": ["Ros\u00b7tet", "der", "Stahl", ",", "ist", "ent\u00b7nervt", "die", "Har\u00b7fe", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VAFIN", "ADJD", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Nur Freiheitsharf' ist Harfe des Vaterlands!", "tokens": ["Nur", "Frei\u00b7heits\u00b7ha\u00b7rf'", "ist", "Har\u00b7fe", "des", "Va\u00b7ter\u00b7lands", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "NN", "ART", "NN", "$."], "meter": "-+-++-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wer Freiheitsharfe schl\u00e4gt, ist wie Nachtorkan", "tokens": ["Wer", "Frei\u00b7heits\u00b7har\u00b7fe", "schl\u00e4gt", ",", "ist", "wie", "Nach\u00b7tor\u00b7kan"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "NN", "VVFIN", "$,", "VAFIN", "KOKOM", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vor Donnerwettern! Donnre! Schlachtruf!", "tokens": ["Vor", "Don\u00b7ner\u00b7wet\u00b7tern", "!", "Donn\u00b7re", "!", "Schlach\u00b7truf", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$.", "NE", "$.", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Schwerter, fliegt auf, dem Gesandten Gottes!", "tokens": ["Schwer\u00b7ter", ",", "fliegt", "auf", ",", "dem", "Ge\u00b7sand\u00b7ten", "Got\u00b7tes", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKVZ", "$,", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Nur Freiheitsschwert ist Schwert f\u00fcr das Vaterland!", "tokens": ["Nur", "Frei\u00b7heits\u00b7schwert", "ist", "Schwert", "f\u00fcr", "das", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wer Freiheitsschwert hebt, flammt durch das Schlachtgew\u00fchl,", "tokens": ["Wer", "Frei\u00b7heits\u00b7schwert", "hebt", ",", "flammt", "durch", "das", "Schlacht\u00b7ge\u00b7w\u00fchl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wie Blitz des Nachtsturms! St\u00fcrzt, Pal\u00e4ste!", "tokens": ["Wie", "Blitz", "des", "Nachts\u00b7turms", "!", "St\u00fcrzt", ",", "Pa\u00b7l\u00e4s\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "$.", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "St\u00fcrze, Tyrann, dem Verderber Gottes!", "tokens": ["St\u00fcr\u00b7ze", ",", "Ty\u00b7rann", ",", "dem", "Ver\u00b7der\u00b7ber", "Got\u00b7tes", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "O Namen! Namen! festlich, wie Siegsgesang!", "tokens": ["O", "Na\u00b7men", "!", "Na\u00b7men", "!", "fest\u00b7lich", ",", "wie", "Siegs\u00b7ge\u00b7sang", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "NN", "$.", "ADJD", "$,", "PWAV", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Tell! Hermann! Klopstock! Brutus! Timoleon!", "tokens": ["Tell", "!", "Her\u00b7mann", "!", "Klops\u00b7tock", "!", "Bru\u00b7tus", "!", "Ti\u00b7mo\u00b7le\u00b7on", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NE", "$.", "NN", "$.", "NE", "$.", "NE", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "O ihr, wem freie Seele Gott gab,", "tokens": ["O", "ihr", ",", "wem", "frei\u00b7e", "See\u00b7le", "Gott", "gab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "$,", "PWS", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Flammend ins eherne Herz gegraben!", "tokens": ["Flam\u00b7mend", "ins", "e\u00b7her\u00b7ne", "Herz", "ge\u00b7gra\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}