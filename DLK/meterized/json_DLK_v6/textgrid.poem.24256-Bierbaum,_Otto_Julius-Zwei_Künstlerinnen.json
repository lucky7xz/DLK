{"textgrid.poem.24256": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Zwei K\u00fcnstlerinnen", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die heilige C\u00e4cilie versteht sich, wie man wei\u00df,", "tokens": ["Die", "hei\u00b7li\u00b7ge", "C\u00e4\u00b7ci\u00b7lie", "ver\u00b7steht", "sich", ",", "wie", "man", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "PRF", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sehr wohl auf das Harmonium", "tokens": ["Sehr", "wohl", "auf", "das", "Har\u00b7mo\u00b7ni\u00b7um"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und spielt dem lieben Gott zum Preis", "tokens": ["Und", "spielt", "dem", "lie\u00b7ben", "Gott", "zum", "Preis"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sehr sch\u00f6n darauf herum.", "tokens": ["Sehr", "sch\u00f6n", "da\u00b7rauf", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PAV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch ist sie mehr des Zarten froh", "tokens": ["Doch", "ist", "sie", "mehr", "des", "Zar\u00b7ten", "froh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und liebt das Pianissimo", "tokens": ["Und", "liebt", "das", "Pi\u00b7a\u00b7nis\u00b7si\u00b7mo"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und schmelzende Andante,", "tokens": ["Und", "schmel\u00b7zen\u00b7de", "An\u00b7dan\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.8": {"text": "Weil sie, wie jede K\u00fcnstlerin,", "tokens": ["Weil", "sie", ",", "wie", "je\u00b7de", "K\u00fcnst\u00b7le\u00b7rin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mit feinem und erfahrnem Sinn", "tokens": ["Mit", "fei\u00b7nem", "und", "er\u00b7fahr\u00b7nem", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Erkannte:", "tokens": ["Er\u00b7kann\u00b7te", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Dies enchantiert mein Publikum,", "tokens": ["Dies", "en\u00b7chan\u00b7tiert", "mein", "Pub\u00b7li\u00b7kum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Engel und Anverwandte.", "tokens": ["En\u00b7gel", "und", "An\u00b7ver\u00b7wand\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Bellona h\u00f6rte lange schon", "tokens": ["Bel\u00b7lo\u00b7na", "h\u00f6r\u00b7te", "lan\u00b7ge", "schon"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der Hymnen und Chor\u00e4le Ton", "tokens": ["Der", "Hym\u00b7nen", "und", "Cho\u00b7r\u00e4\u00b7le", "Ton"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Mit vielem Mi\u00dfbehagen.", "tokens": ["Mit", "vie\u00b7lem", "Mi\u00df\u00b7be\u00b7ha\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Darum begann", "tokens": ["Da\u00b7rum", "be\u00b7gann"], "token_info": ["word", "word"], "pos": ["PAV", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Sie dann und wann", "tokens": ["Sie", "dann", "und", "wann"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "KON", "PWAV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Die Pauke schon zu schlagen.", "tokens": ["Die", "Pau\u00b7ke", "schon", "zu", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bald war sie dort, bald war sie da", "tokens": ["Bald", "war", "sie", "dort", ",", "bald", "war", "sie", "da"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "ADV", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit ihrer groben Musika.", "tokens": ["Mit", "ih\u00b7rer", "gro\u00b7ben", "Mu\u00b7si\u00b7ka", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Seis auf den Philippinen,", "tokens": ["Seis", "auf", "den", "Phil\u00b7ip\u00b7pi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Seis in S\u00fcdafrika,", "tokens": ["Seis", "in", "S\u00fcd\u00b7af\u00b7ri\u00b7ka", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Wo sie mit frohen Mienen", "tokens": ["Wo", "sie", "mit", "fro\u00b7hen", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Schon viele H\u00f6rer sah.", "tokens": ["Schon", "vie\u00b7le", "H\u00f6\u00b7rer", "sah", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Jedoch, das Rechte ward es nie.", "tokens": ["Je\u00b7doch", ",", "das", "Rech\u00b7te", "ward", "es", "nie", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Bellona zog die Stirne kraus", "tokens": ["Bel\u00b7lo\u00b7na", "zog", "die", "Stir\u00b7ne", "kraus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.15": {"text": "Und murmelte verdrie\u00dflich:", "tokens": ["Und", "mur\u00b7mel\u00b7te", "ver\u00b7drie\u00df\u00b7lich", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "So eine kleine Sinfonie", "tokens": ["So", "ei\u00b7ne", "klei\u00b7ne", "Sin\u00b7fo\u00b7nie"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Kann schlie\u00dflich", "tokens": ["Kann", "schlie\u00df\u00b7lich"], "token_info": ["word", "word"], "pos": ["VMFIN", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.18": {"text": "Auch Doktor Richard Strau\u00df.", "tokens": ["Auch", "Dok\u00b7tor", "Ric\u00b7hard", "Strau\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Ich brauch noch viel mehr Blech und Krach,", "tokens": ["Ich", "brauch", "noch", "viel", "mehr", "Blech", "und", "Krach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Bei dem Gewimmer wird mir schwach;", "tokens": ["Bei", "dem", "Ge\u00b7wim\u00b7mer", "wird", "mir", "schwach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Oh, h\u00e4tt ich Massen, Massen,", "tokens": ["Oh", ",", "h\u00e4tt", "ich", "Mas\u00b7sen", ",", "Mas\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Mein ganzes Seelenungest\u00fcm", "tokens": ["Mein", "gan\u00b7zes", "See\u00b7le\u00b7nun\u00b7ge\u00b7st\u00fcm"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "In einem Fugenunget\u00fcm", "tokens": ["In", "ei\u00b7nem", "Fu\u00b7ge\u00b7nun\u00b7ge\u00b7t\u00fcm"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Gewaltig loszulassen.", "tokens": ["Ge\u00b7wal\u00b7tig", "los\u00b7zu\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Indessen zog C\u00e4cilia", "tokens": ["In\u00b7des\u00b7sen", "zog", "C\u00e4\u00b7ci\u00b7lia"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Mit Inbrunst die Harmonika", "tokens": ["Mit", "In\u00b7brunst", "die", "Har\u00b7mo\u00b7ni\u00b7ka"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und fand (bei ausverkauftem Haus)", "tokens": ["Und", "fand", "(", "bei", "aus\u00b7ver\u00b7kauf\u00b7tem", "Haus", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Himmel und Erden viel Applaus,", "tokens": ["Auf", "Him\u00b7mel", "und", "Er\u00b7den", "viel", "Ap\u00b7plaus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PIAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wobei der Zar", "tokens": ["Wo\u00b7bei", "der", "Zar"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Der allerbegeistertste Klatscher war.", "tokens": ["Der", "al\u00b7ler\u00b7be\u00b7geis\u00b7terts\u00b7te", "Klat\u00b7scher", "war", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Das wurmte Bellonen,", "tokens": ["Das", "wurm\u00b7te", "Bel\u00b7lo\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Es ist nicht zu sagen,", "tokens": ["Es", "ist", "nicht", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Wie sehr.", "tokens": ["Wie", "sehr", "."], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "ADV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Sie schleppte Kanonen", "tokens": ["Sie", "schlepp\u00b7te", "Ka\u00b7no\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und Pulverwagen", "tokens": ["Und", "Pul\u00b7ver\u00b7wa\u00b7gen"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Daher.", "tokens": ["Da\u00b7her", "."], "token_info": ["word", "punct"], "pos": ["PAV", "$."], "meter": "--", "measure": "unknown.measure.zero"}, "line.7": {"text": "Und pr\u00fcfte die Z\u00fcnder", "tokens": ["Und", "pr\u00fcf\u00b7te", "die", "Z\u00fcn\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Und putzte die Schl\u00fcnder", "tokens": ["Und", "putz\u00b7te", "die", "Schl\u00fcn\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Und fand:", "tokens": ["Und", "fand", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "Es war das Orchester", "tokens": ["Es", "war", "das", "Or\u00b7ches\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.11": {"text": "Der Monsterballester", "tokens": ["Der", "Mons\u00b7ter\u00b7bal\u00b7les\u00b7ter"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.12": {"text": "Im trefflichsten Stand.", "tokens": ["Im", "treff\u00b7lichs\u00b7ten", "Stand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Und blies dem Zaren ins Ohr:", "tokens": ["Und", "blies", "dem", "Za\u00b7ren", "ins", "Ohr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Du Tor!", "tokens": ["Du", "Tor", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Was sitzst du im Parkette", "tokens": ["Was", "sitz\u00b7st", "du", "im", "Par\u00b7ket\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Und lauschst den S\u00e4uselein", "tokens": ["Und", "lauschst", "den", "S\u00e4u\u00b7sel\u00b7ein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von Geigen und Schalmein", "tokens": ["Von", "Gei\u00b7gen", "und", "Schal\u00b7mein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der himmlischen Motette!", "tokens": ["Der", "himm\u00b7li\u00b7schen", "Mo\u00b7tet\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.7": {"text": "Dabei schl\u00e4fst du noch ein,", "tokens": ["Da\u00b7bei", "schl\u00e4fst", "du", "noch", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und k\u00f6nntst doch selbst der Geister", "tokens": ["Und", "k\u00f6nntst", "doch", "selbst", "der", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Der Welt-Kapellenmeister sein.", "tokens": ["Der", "Welt\u00b7Ka\u00b7pel\u00b7len\u00b7meis\u00b7ter", "sein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Das ist das Amt des Zaren!", "tokens": ["Das", "ist", "das", "Amt", "des", "Za\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die ehmals der Tataren", "tokens": ["Die", "eh\u00b7mals", "der", "Ta\u00b7ta\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Blutge Bezwinger waren,", "tokens": ["Blut\u00b7ge", "Be\u00b7zwin\u00b7ger", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Sind deine ", "tokens": ["Sind", "dei\u00b7ne"], "token_info": ["word", "word"], "pos": ["VAFIN", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Du sollst, wie sie, dich strecken,", "tokens": ["Du", "sollst", ",", "wie", "sie", ",", "dich", "stre\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PWAV", "PPER", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ostw\u00e4rts die Pranken recken,", "tokens": ["Ost\u00b7w\u00e4rts", "die", "Pran\u00b7ken", "re\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Ganz Asien soll bedecken", "tokens": ["Ganz", "A\u00b7sien", "soll", "be\u00b7de\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NE", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit seinem Fl\u00fcgelpaar", "tokens": ["Mit", "sei\u00b7nem", "Fl\u00fc\u00b7gel\u00b7paar"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Moskowiens Doppelaar.", "tokens": ["Mos\u00b7ko\u00b7wi\u00b7ens", "Dop\u00b7pe\u00b7laar", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Es ist bei den Mongolen", "tokens": ["Es", "ist", "bei", "den", "Mon\u00b7go\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.11": {"text": "Noch viel f\u00fcr dich zu holen;", "tokens": ["Noch", "viel", "f\u00fcr", "dich", "zu", "ho\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure":