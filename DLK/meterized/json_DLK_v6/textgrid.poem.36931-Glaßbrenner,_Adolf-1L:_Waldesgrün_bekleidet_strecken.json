{"textgrid.poem.36931": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Waldesgr\u00fcn bekleidet strecken", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Waldesgr\u00fcn bekleidet strecken", "tokens": ["Wal\u00b7des\u00b7gr\u00fcn", "be\u00b7klei\u00b7det", "stre\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stolz ihr Haupt, empor zum Himmel", "tokens": ["Stolz", "ihr", "Haupt", ",", "em\u00b7por", "zum", "Him\u00b7mel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "PTKVZ", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jene riesig hohen Berge!", "tokens": ["Je\u00b7ne", "rie\u00b7sig", "ho\u00b7hen", "Ber\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schaun ", "tokens": ["Schaun"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Auf das \u00e4msige Gewimmel", "tokens": ["Auf", "das", "\u00e4m\u00b7si\u00b7ge", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dort in D\u00f6rfern und in Flecken.", "tokens": ["Dort", "in", "D\u00f6r\u00b7fern", "und", "in", "Fle\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn sie bieten reichen Segen", "tokens": ["Denn", "sie", "bie\u00b7ten", "rei\u00b7chen", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An Metallen, Holz und Steinen,", "tokens": ["An", "Me\u00b7tal\u00b7len", ",", "Holz", "und", "Stei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bieten Fl\u00fcsse, Felder, Fr\u00fcchte,", "tokens": ["Bie\u00b7ten", "Fl\u00fcs\u00b7se", ",", "Fel\u00b7der", ",", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und doch sehn sie das Gez\u00fcchte", "tokens": ["Und", "doch", "sehn", "sie", "das", "Ge\u00b7z\u00fcch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Jammern, hungern, betteln, weinen", "tokens": ["Jam\u00b7mern", ",", "hun\u00b7gern", ",", "bet\u00b7teln", ",", "wei\u00b7nen"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "VVINF", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Drunten auf den bl\u00fch'nden Wegen.", "tokens": ["Drun\u00b7ten", "auf", "den", "bl\u00fch'n\u00b7den", "We\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mehr als Thau auf goldnen Auen", "tokens": ["Mehr", "als", "Thau", "auf", "gold\u00b7nen", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "KOKOM", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blitzen hier der Armuth Thr\u00e4nen,", "tokens": ["Blit\u00b7zen", "hier", "der", "Ar\u00b7muth", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und inmitten \u00fcpp'ger Th\u00e4ler,", "tokens": ["Und", "in\u00b7mit\u00b7ten", "\u00fcpp'\u00b7ger", "Th\u00e4\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur zur Wollust ihrer Qu\u00e4ler,", "tokens": ["Nur", "zur", "Wol\u00b7lust", "ih\u00b7rer", "Qu\u00e4\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nackte Menschen unter Thr\u00e4nen", "tokens": ["Nack\u00b7te", "Men\u00b7schen", "un\u00b7ter", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gier an troknem Brote kauen.", "tokens": ["Gier", "an", "tro\u00b7knem", "Bro\u00b7te", "kau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Soll ich nun zu schildern trachten,", "tokens": ["Soll", "ich", "nun", "zu", "schil\u00b7dern", "trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie in Weh mein Herz geschmolzen", "tokens": ["Wie", "in", "Weh", "mein", "Herz", "ge\u00b7schmol\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Euer elend, elend Leben?", "tokens": ["Eu\u00b7er", "e\u00b7lend", ",", "e\u00b7lend", "Le\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "$,", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nein, mag mir es Gott vergeben!", "tokens": ["Nein", ",", "mag", "mir", "es", "Gott", "ver\u00b7ge\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VMFIN", "PPER", "PPER", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie die Riesen dort, die stolzen,", "tokens": ["Wie", "die", "Rie\u00b7sen", "dort", ",", "die", "stol\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "$,", "PRELS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sclaven! mu\u00df ich euch verachten.", "tokens": ["Scla\u00b7ven", "!", "mu\u00df", "ich", "euch", "ver\u00b7ach\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VMFIN", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}