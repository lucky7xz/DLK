{"textgrid.poem.32029": {"metadata": {"author": {"name": "Angelus Silesius", "birth": "N.A.", "death": "N.A."}, "title": "52. Die Liebe hats erfunden", "genre": "verse", "period": "N.A.", "pub_year": 1650, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df Gott gekreuzigt wird, da\u00df man ihn kann verwunden,", "tokens": ["Da\u00df", "Gott", "ge\u00b7kreu\u00b7zigt", "wird", ",", "da\u00df", "man", "ihn", "kann", "ver\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVPP", "VAFIN", "$,", "KOUS", "PIS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er die Schmach vertr\u00e4gt, die man ihm angetan,", "tokens": ["Da\u00df", "er", "die", "Schmach", "ver\u00b7tr\u00e4gt", ",", "die", "man", "ihm", "an\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "PRELS", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df er solch Angst aussteht und da\u00df er sterben kann,", "tokens": ["Da\u00df", "er", "solch", "Angst", "aus\u00b7steht", "und", "da\u00df", "er", "ster\u00b7ben", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "KON", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verwundere dich nicht: die Liebe hats erfunden. ", "tokens": ["Ver\u00b7wun\u00b7de\u00b7re", "dich", "nicht", ":", "die", "Lie\u00b7be", "hats", "er\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$.", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}