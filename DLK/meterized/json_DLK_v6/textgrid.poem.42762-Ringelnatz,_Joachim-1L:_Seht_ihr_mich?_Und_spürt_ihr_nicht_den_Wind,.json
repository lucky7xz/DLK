{"textgrid.poem.42762": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seht ihr mich? Und sp\u00fcrt ihr nicht den Wind,", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht ihr mich? Und sp\u00fcrt ihr nicht den Wind,", "tokens": ["Seht", "ihr", "mich", "?", "Und", "sp\u00fcrt", "ihr", "nicht", "den", "Wind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$.", "KON", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Den ich mache? Ja, das ist gef\u00e4hrlich!", "tokens": ["Den", "ich", "ma\u00b7che", "?", "Ja", ",", "das", "ist", "ge\u00b7f\u00e4hr\u00b7lich", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "$.", "PTKANT", "$,", "PDS", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Aber mir, dem alten Seemann, sind", "tokens": ["A\u00b7ber", "mir", ",", "dem", "al\u00b7ten", "See\u00b7mann", ",", "sind"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "$,", "ART", "ADJA", "NN", "$,", "VAFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Riesenwellen eben unentbehrlich.", "tokens": ["Rie\u00b7sen\u00b7wel\u00b7len", "e\u00b7ben", "un\u00b7ent\u00b7behr\u00b7lich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "K\u00e4me mir jetzt einer in die Speichen", "tokens": ["K\u00e4\u00b7me", "mir", "jetzt", "ei\u00b7ner", "in", "die", "Spei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "(w\u00e4r es auch ein Riese aus Granit),", "tokens": ["(", "w\u00e4r", "es", "auch", "ein", "Rie\u00b7se", "aus", "Gra\u00b7nit", ")", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "PPER", "ADV", "ART", "NN", "APPR", "NE", "$(", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "W\u00fcrde er doch damit nur erreichen,", "tokens": ["W\u00fcr\u00b7de", "er", "doch", "da\u00b7mit", "nur", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PAV", "ADV", "VVINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Da\u00df ich ihn in d\u00fcnne Scheiben schnitt.", "tokens": ["Da\u00df", "ich", "ihn", "in", "d\u00fcn\u00b7ne", "Schei\u00b7ben", "schnitt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Aber nicht die Herstellung von Scheiben", "tokens": ["A\u00b7ber", "nicht", "die", "Her\u00b7stel\u00b7lung", "von", "Schei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ART", "NN", "APPR", "NN"], "meter": "+-+-++--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Denk ich mir als Lebenszweck. O, nein!", "tokens": ["Denk", "ich", "mir", "als", "Le\u00b7bens\u00b7zweck", ".", "O", ",", "nein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "KOUS", "NN", "$.", "NE", "$,", "PTKANT", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Eine S\u00e4gem\u00fchle m\u00f6cht' ich treiben,", "tokens": ["Ei\u00b7ne", "S\u00e4\u00b7ge\u00b7m\u00fch\u00b7le", "m\u00f6cht'", "ich", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "M\u00f6cht' ein Schwungrad f\u00fcr Dynamo sein.", "tokens": ["M\u00f6cht'", "ein", "Schwun\u00b7grad", "f\u00fcr", "Dy\u00b7na\u00b7mo", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPR", "NE", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Wenn ich pl\u00f6tzlich jetzt die H\u00e4nde strecke", "tokens": ["Wenn", "ich", "pl\u00f6tz\u00b7lich", "jetzt", "die", "H\u00e4n\u00b7de", "stre\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "(und ich habe \u00c4hnliches im Sinn),", "tokens": ["(", "und", "ich", "ha\u00b7be", "\u00c4hn\u00b7li\u00b7ches", "im", "Sinn", ")", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "PPER", "VAFIN", "NN", "APPRART", "NN", "$(", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ja dann \u2013 splittert augenblicks die Decke,", "tokens": ["Ja", "dann", "\u2013", "split\u00b7tert", "au\u00b7gen\u00b7blicks", "die", "De\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$(", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und der Wellenriese \u2013 ist dahin.", "tokens": ["Und", "der", "Wel\u00b7len\u00b7rie\u00b7se", "\u2013", "ist", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$(", "VAFIN", "PAV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}