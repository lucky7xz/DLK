{"textgrid.poem.39791": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Heloise an Abelard", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ruhe sei dem Abgem\u00fchten,", "tokens": ["Ru\u00b7he", "sei", "dem", "Ab\u00b7ge\u00b7m\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dem von Liebespein Durchgl\u00fchten;", "tokens": ["Dem", "von", "Lie\u00b7be\u00b7spein", "Durch\u00b7gl\u00fch\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bei den Seligen zu sein", "tokens": ["Bei", "den", "Se\u00b7li\u00b7gen", "zu", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKZU", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Flehend bat er,", "tokens": ["Fle\u00b7hend", "bat", "er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Und jetzt trat er", "tokens": ["Und", "jetzt", "trat", "er"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "In des Heilands Tempel ein.", "tokens": ["In", "des", "Hei\u00b7lands", "Tem\u00b7pel", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In des Grabes finstrer Zelle", "tokens": ["In", "des", "Gra\u00b7bes", "finst\u00b7rer", "Zel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird's dem Frommen sternenhelle,", "tokens": ["Wird's", "dem", "From\u00b7men", "ster\u00b7nen\u00b7hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und bald selber wie ein Stern", "tokens": ["Und", "bald", "sel\u00b7ber", "wie", "ein", "Stern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "KOKOM", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus dem Dunkeln", "tokens": ["Aus", "dem", "Dun\u00b7keln"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.5": {"text": "Wird er funkeln,", "tokens": ["Wird", "er", "fun\u00b7keln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Angestralt vom Glanz des Herrn.", "tokens": ["An\u00b7ge\u00b7stralt", "vom", "Glanz", "des", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Heil dir! Sieger mit der Krone,", "tokens": ["Heil", "dir", "!", "Sie\u00b7ger", "mit", "der", "Kro\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Br\u00e4utigam in heller Zone!", "tokens": ["Br\u00e4u\u00b7ti\u00b7gam", "in", "hel\u00b7ler", "Zo\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die dir solchen Gru\u00df gebracht", "tokens": ["Die", "dir", "sol\u00b7chen", "Gru\u00df", "ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PIAT", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wankt im Sehnen", "tokens": ["Wankt", "im", "Seh\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Hei\u00dfer Thr\u00e4nen,", "tokens": ["Hei\u00b7\u00dfer", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Deine Wittwe, durch die Nacht.", "tokens": ["Dei\u00b7ne", "Witt\u00b7we", ",", "durch", "die", "Nacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mein ist er, obgleich entnommen", "tokens": ["Mein", "ist", "er", ",", "ob\u00b7gleich", "ent\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "VAFIN", "PPER", "$,", "KOUS", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zur Genossenschaft der Frommen,", "tokens": ["Zur", "Ge\u00b7nos\u00b7sen\u00b7schaft", "der", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcrdiger jetzt lieb' ich ihn.", "tokens": ["W\u00fcr\u00b7di\u00b7ger", "jetzt", "lieb'", "ich", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deinem Fehle,", "tokens": ["Dei\u00b7nem", "Feh\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Kranke Seele,", "tokens": ["Kran\u00b7ke", "See\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Ward durch seinen Tod verziehn.", "tokens": ["Ward", "durch", "sei\u00b7nen", "Tod", "ver\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Mit dir theilt' ich deinen Kummer,", "tokens": ["Mit", "dir", "theilt'", "ich", "dei\u00b7nen", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df mich theilen deinen Schlummer,", "tokens": ["La\u00df", "mich", "thei\u00b7len", "dei\u00b7nen", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df auch mich in Zion sein.", "tokens": ["La\u00df", "auch", "mich", "in", "Zi\u00b7on", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPER", "APPR", "NE", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00f6s' aus Leide,", "tokens": ["L\u00f6s'", "aus", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "F\u00fchr' in Freude", "tokens": ["F\u00fchr'", "in", "Freu\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Mein beschwertes Leben ein.", "tokens": ["Mein", "be\u00b7schwer\u00b7tes", "Le\u00b7ben", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "F\u00f6rdert, Heilige! mein Beten,", "tokens": ["F\u00f6r\u00b7dert", ",", "Hei\u00b7li\u00b7ge", "!", "mein", "Be\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "$.", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Woll', o Tr\u00f6ster, mich vertreten! \u2013", "tokens": ["Woll'", ",", "o", "Tr\u00f6s\u00b7ter", ",", "mich", "ver\u00b7tre\u00b7ten", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "FM", "NN", "$,", "PPER", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welche Wonne trifft mein Ohr?", "tokens": ["Wel\u00b7che", "Won\u00b7ne", "trifft", "mein", "Ohr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Welches Sch\u00fcttern", "tokens": ["Wel\u00b7ches", "Sch\u00fct\u00b7tern"], "token_info": ["word", "word"], "pos": ["PWAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "S\u00fc\u00dfer Zithern?", "tokens": ["S\u00fc\u00b7\u00dfer", "Zi\u00b7thern", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Ja, es ist der Engel Chor.", "tokens": ["Ja", ",", "es", "ist", "der", "En\u00b7gel", "Chor", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}