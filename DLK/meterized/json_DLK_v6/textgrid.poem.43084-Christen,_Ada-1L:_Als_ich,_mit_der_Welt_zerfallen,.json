{"textgrid.poem.43084": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als ich, mit der Welt zerfallen,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich, mit der Welt zerfallen,", "tokens": ["Als", "ich", ",", "mit", "der", "Welt", "zer\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schweigend ging umher,", "tokens": ["Schwei\u00b7gend", "ging", "um\u00b7her", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da fragten die lieben Menschen:", "tokens": ["Da", "frag\u00b7ten", "die", "lie\u00b7ben", "Men\u00b7schen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Was qu\u00e4let dich so sehr?", "tokens": ["Was", "qu\u00e4\u00b7let", "dich", "so", "sehr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich sagte ihnen die Wahrheit;", "tokens": ["Ich", "sag\u00b7te", "ih\u00b7nen", "die", "Wahr\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie haben sich fortgedr\u00fcckt", "tokens": ["Sie", "ha\u00b7ben", "sich", "fort\u00b7ge\u00b7dr\u00fcckt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "VVPP"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und hinter meinem R\u00fccken", "tokens": ["Und", "hin\u00b7ter", "mei\u00b7nem", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erkl\u00e4rt, ich sei verr\u00fcckt.", "tokens": ["Er\u00b7kl\u00e4rt", ",", "ich", "sei", "ver\u00b7r\u00fcckt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}