{"textgrid.poem.47067": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "64.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir tr\u00e4umt', ich s\u00e4\u00dfe droben an der Eiche,", "tokens": ["Mir", "tr\u00e4umt'", ",", "ich", "s\u00e4\u00b7\u00dfe", "dro\u00b7ben", "an", "der", "Ei\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu der ich Sommers lenkte meine Bahnen,", "tokens": ["Zu", "der", "ich", "Som\u00b7mers", "lenk\u00b7te", "mei\u00b7ne", "Bah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo ich, ein K\u00f6nig ohne Unterthanen,", "tokens": ["Wo", "ich", ",", "ein", "K\u00f6\u00b7nig", "oh\u00b7ne", "Un\u00b7ter\u00b7tha\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Oft blickt' ins Thal auf meine Liebesreiche.", "tokens": ["Oft", "blickt'", "ins", "Thal", "auf", "mei\u00b7ne", "Lie\u00b7bes\u00b7rei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und pl\u00f6tzlich war es jetzt, als \u00fcberschleiche", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "war", "es", "jetzt", ",", "als", "\u00fc\u00b7bersc\u00b7hlei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ADV", "$,", "KOUS", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den starren Winter laues Fr\u00fchlingsahnen;", "tokens": ["Den", "star\u00b7ren", "Win\u00b7ter", "lau\u00b7es", "Fr\u00fch\u00b7lin\u00b7gsah\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vom Turme drunten knatterten die Fahnen,", "tokens": ["Vom", "Tur\u00b7me", "drun\u00b7ten", "knat\u00b7ter\u00b7ten", "die", "Fah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dr\u00fcben krachte dumpf das Eis vom Teiche.", "tokens": ["Und", "dr\u00fc\u00b7ben", "krach\u00b7te", "dumpf", "das", "Eis", "vom", "Tei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADJD", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und als ich niederschaute nach dem Hause,", "tokens": ["Und", "als", "ich", "nie\u00b7der\u00b7schau\u00b7te", "nach", "dem", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von L\u00e4den gr\u00fcn und rosenbla\u00df von W\u00e4nden,", "tokens": ["Von", "L\u00e4\u00b7den", "gr\u00fcn", "und", "ro\u00b7sen\u00b7bla\u00df", "von", "W\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da stieg der dunkle Rauch vom Giebel wieder.", "tokens": ["Da", "stieg", "der", "dunk\u00b7le", "Rauch", "vom", "Gie\u00b7bel", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Anfa\u00dft' es mich als wie mit Sturmwindsbrause;", "tokens": ["An\u00b7fa\u00dft'", "es", "mich", "als", "wie", "mit", "Sturm\u00b7winds\u00b7brau\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "KOUS", "KOKOM", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Am Eichstamm hielt ich mich mit beiden H\u00e4nden,", "tokens": ["Am", "Eichs\u00b7tamm", "hielt", "ich", "mich", "mit", "bei\u00b7den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sonst ri\u00df der Wirbel mich zum Hause nieder.", "tokens": ["Sonst", "ri\u00df", "der", "Wir\u00b7bel", "mich", "zum", "Hau\u00b7se", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}