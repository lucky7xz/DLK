{"textgrid.poem.65976": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zu ihr! zu ihr! Es schl\u00e4gt das Herz", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu ihr! zu ihr! Es schl\u00e4gt das Herz", "tokens": ["Zu", "ihr", "!", "zu", "ihr", "!", "Es", "schl\u00e4gt", "das", "Herz"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$.", "APPR", "PPOSAT", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit dreifach schnellen Schl\u00e4gen.", "tokens": ["Mit", "drei\u00b7fach", "schnel\u00b7len", "Schl\u00e4\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O h\u00e4tten Schwalbenfl\u00fcgel doch", "tokens": ["O", "h\u00e4t\u00b7ten", "Schwal\u00b7ben\u00b7fl\u00fc\u00b7gel", "doch"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sohlen auch, die tr\u00e4gen.", "tokens": ["Die", "Soh\u00b7len", "auch", ",", "die", "tr\u00e4\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zu ihr! zu ihr! Schon bin ich da.", "tokens": ["Zu", "ihr", "!", "zu", "ihr", "!", "Schon", "bin", "ich", "da", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "APPR", "PPOSAT", "$.", "ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird sie wohl meiner warten?", "tokens": ["Wird", "sie", "wohl", "mei\u00b7ner", "war\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich sp\u00e4he um das Haus herum", "tokens": ["Ich", "sp\u00e4\u00b7he", "um", "das", "Haus", "he\u00b7rum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und durch den ganzen Garten.", "tokens": ["Und", "durch", "den", "gan\u00b7zen", "Gar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zu ihr! zu ihr! Um Busch und Beet", "tokens": ["Zu", "ihr", "!", "zu", "ihr", "!", "Um", "Busch", "und", "Beet"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$.", "APPR", "PPOSAT", "$.", "KOUI", "NN", "KON", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mach' ich mich auf die Suche.", "tokens": ["Mach'", "ich", "mich", "auf", "die", "Su\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Deckt meinen Schelm mit ihrem Stamm", "tokens": ["Deckt", "mei\u00b7nen", "Schelm", "mit", "ih\u00b7rem", "Stamm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die junge Fr\u00fchlingsbuche?", "tokens": ["Die", "jun\u00b7ge", "Fr\u00fch\u00b7lings\u00b7bu\u00b7che", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dort in der H\u00fctte, regt sich's nicht?", "tokens": ["Dort", "in", "der", "H\u00fct\u00b7te", ",", "regt", "sich's", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "VVFIN", "PIS", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gefunden, ja gefunden!", "tokens": ["Ge\u00b7fun\u00b7den", ",", "ja", "ge\u00b7fun\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schon hat sie den verliebten Narr'n", "tokens": ["Schon", "hat", "sie", "den", "ver\u00b7lieb\u00b7ten", "Narr'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit weichem Arm umwunden.", "tokens": ["Mit", "wei\u00b7chem", "Arm", "um\u00b7wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Schon herzt sie mich, schon k\u00fcsst sie mich.", "tokens": ["Schon", "herzt", "sie", "mich", ",", "schon", "k\u00fcsst", "sie", "mich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O M\u00e4dchen, dies Entz\u00fccken,", "tokens": ["O", "M\u00e4d\u00b7chen", ",", "dies", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PDS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von deinem sechzehnj\u00e4hrigen Mund", "tokens": ["Von", "dei\u00b7nem", "sech\u00b7zehn\u00b7j\u00e4h\u00b7ri\u00b7gen", "Mund"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "So Kuss um Kuss zu pfl\u00fccken.", "tokens": ["So", "Kuss", "um", "Kuss", "zu", "pfl\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}