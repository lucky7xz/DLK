{"textgrid.poem.57910": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun mu\u00df der L\u00e4rchenberg dran glauben,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun mu\u00df der L\u00e4rchenberg dran glauben,", "tokens": ["Nun", "mu\u00df", "der", "L\u00e4r\u00b7chen\u00b7berg", "dran", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er pa\u00dft in unsere Zeit nicht mehr.", "tokens": ["Er", "pa\u00dft", "in", "un\u00b7se\u00b7re", "Zeit", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Graf-Waldersee-Stra\u00dfe soll er hei\u00dfen,", "tokens": ["Graf\u00b7Wal\u00b7der\u00b7see\u00b7Stra\u00b7\u00dfe", "soll", "er", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das ist bedeutend vornehmerer.", "tokens": ["Das", "ist", "be\u00b7deu\u00b7tend", "vor\u00b7neh\u00b7me\u00b7rer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbam L\u00e4rchenberg\u00ab, das klingt nach Gr\u00fcnem,", "tokens": ["\u00bb", "am", "L\u00e4r\u00b7chen\u00b7berg", "\u00ab", ",", "das", "klingt", "nach", "Gr\u00fc\u00b7nem", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "$(", "$,", "PDS", "VVFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Gr\u00fcn, das ist doch ordin\u00e4r,", "tokens": ["Und", "Gr\u00fcn", ",", "das", "ist", "doch", "or\u00b7di\u00b7n\u00e4r", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PDS", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die neue Bezeichnung, liebe Leute,", "tokens": ["Die", "neu\u00b7e", "Be\u00b7zeich\u00b7nung", ",", "lie\u00b7be", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die ist bedeutend sch\u00f6nerer.", "tokens": ["Die", "ist", "be\u00b7deu\u00b7tend", "sch\u00f6\u00b7ne\u00b7rer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbam L\u00e4rchenberg\u00ab ist viel zu kurz auch,", "tokens": ["\u00bb", "am", "L\u00e4r\u00b7chen\u00b7berg", "\u00ab", "ist", "viel", "zu", "kurz", "auch", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "$(", "VAFIN", "ADV", "PTKA", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und K\u00fcrze, die mag man nicht mehr,", "tokens": ["Und", "K\u00fcr\u00b7ze", ",", "die", "mag", "man", "nicht", "mehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "VMFIN", "PIS", "PTKNEG", "ADV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Graf-Waldersee-Stra\u00dfe, das ist besser,", "tokens": ["Graf\u00b7Wal\u00b7der\u00b7see\u00b7Stra\u00b7\u00dfe", ",", "das", "ist", "bes\u00b7ser", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VAFIN", "ADJD", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Denn das ist ja viel l\u00e4ngerer.", "tokens": ["Denn", "das", "ist", "ja", "viel", "l\u00e4n\u00b7ge\u00b7rer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Was h\u00fcbsch und alt ist in Hannover,", "tokens": ["Was", "h\u00fcbsch", "und", "alt", "ist", "in", "Han\u00b7no\u00b7ver", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "VAFIN", "APPR", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Will man am gr\u00fcnen Tisch nicht mehr,", "tokens": ["Will", "man", "am", "gr\u00fc\u00b7nen", "Tisch", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPRART", "ADJA", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und flugs erfindet man was Neues,", "tokens": ["Und", "flugs", "er\u00b7fin\u00b7det", "man", "was", "Neu\u00b7es", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "PWS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch das ist meist viel d\u00e4mlicherer.", "tokens": ["Doch", "das", "ist", "meist", "viel", "d\u00e4m\u00b7li\u00b7che\u00b7rer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "PIAT", "ADJA", "$."], "meter": "+-++-+-+-", "measure": "unknown.measure.penta"}}}}}