{"dta.poem.2255": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Lobgedichte  \n auf die Pamela.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Tugend war den Sterblichen, doch nur dem Na-", "tokens": ["Die", "Tu\u00b7gend", "war", "den", "Sterb\u00b7li\u00b7chen", ",", "doch", "nur", "dem", "Na"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "ADV", "ADV", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "men nach, bekannt,", "tokens": ["men", "nach", ",", "be\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Bis sie, uns Menschen zu begl\u00fccken, beschlo\u00df, zu uns", "tokens": ["Bis", "sie", ",", "uns", "Men\u00b7schen", "zu", "be\u00b7gl\u00fc\u00b7cken", ",", "be\u00b7schlo\u00df", ",", "zu", "uns"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "$,", "PPER", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "$,", "APPR", "PPER"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "herab zu steigen,", "tokens": ["her\u00b7ab", "zu", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Und uns, mit allem ihren Reiz, sich in der Pamela zu", "tokens": ["Und", "uns", ",", "mit", "al\u00b7lem", "ih\u00b7ren", "Reiz", ",", "sich", "in", "der", "Pa\u00b7me\u00b7la", "zu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "APPR", "PIS", "PPOSAT", "NN", "$,", "PRF", "APPR", "ART", "NE", "APPR"], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "zeigen.", "tokens": ["zei\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Doch leider! es war diese M\u00fche von ihr vergebens an-", "tokens": ["Doch", "lei\u00b7der", "!", "es", "war", "die\u00b7se", "M\u00fc\u00b7he", "von", "ihr", "ver\u00b7ge\u00b7bens", "an"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "PPER", "VAFIN", "PDAT", "NN", "APPR", "PPER", "ADV", "TRUNC"], "meter": "-+--+--+--+-+-+", "measure": "amphibrach.tetra.plus"}, "line.8": {"text": "gewandt.", "tokens": ["ge\u00b7wandt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Die Pamela ward spr\u00f6d und kalt, als ein gemeines Buch,", "tokens": ["Die", "Pa\u00b7me\u00b7la", "ward", "spr\u00f6d", "und", "kalt", ",", "als", "ein", "ge\u00b7mei\u00b7nes", "Buch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ADJD", "KON", "ADJD", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "gelesen;", "tokens": ["ge\u00b7le\u00b7sen", ";"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Ein jeder, wenig ausgenommen, blieb das, so er vorher", "tokens": ["Ein", "je\u00b7der", ",", "we\u00b7nig", "aus\u00b7ge\u00b7nom\u00b7men", ",", "blieb", "das", ",", "so", "er", "vor\u00b7her"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIS", "$,", "ADV", "VVPP", "$,", "VVFIN", "PDS", "$,", "ADV", "PPER", "ADV"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.12": {"text": "gewesen.", "tokens": ["ge\u00b7we\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["VAPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Jhr Sittenlehrer, k\u00f6nnt denn k\u00fcnftig all\u2019 eure Lehren sicher", "tokens": ["Ihr", "Sit\u00b7ten\u00b7leh\u00b7rer", ",", "k\u00f6nnt", "denn", "k\u00fcnf\u00b7tig", "all'", "eu\u00b7re", "Leh\u00b7ren", "si\u00b7cher"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ADV", "ADJD", "PIAT", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "sparen;", "tokens": ["spa\u00b7ren", ";"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Die Menschen werden immer seyn so gut und b\u00f6se, wie", "tokens": ["Die", "Men\u00b7schen", "wer\u00b7den", "im\u00b7mer", "seyn", "so", "gut", "und", "b\u00f6\u00b7se", ",", "wie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "PPOSAT", "ADV", "ADJD", "KON", "ADJD", "$,", "PWAV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.16": {"text": "sie waren.", "tokens": ["sie", "wa\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VAFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}