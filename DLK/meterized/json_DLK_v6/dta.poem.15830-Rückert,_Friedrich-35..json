{"dta.poem.15830": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "35.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr meine Nachbarn einst, nicht meine Nachbarn mehr,", "tokens": ["Ihr", "mei\u00b7ne", "Nach\u00b7barn", "einst", ",", "nicht", "mei\u00b7ne", "Nach\u00b7barn", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADV", "$,", "PTKNEG", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus eurer Nachbarschaft weht noch ein Duft mir her.", "tokens": ["Aus", "eu\u00b7rer", "Nach\u00b7bar\u00b7schaft", "weht", "noch", "ein", "Duft", "mir", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein Duft der Herzlichkeit, ein Duft der Lebenstreue;", "tokens": ["Ein", "Duft", "der", "Herz\u00b7lich\u00b7keit", ",", "ein", "Duft", "der", "Le\u00b7ben\u00b7streu\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Alte wird nie alt, es wird nur alt das Neue.", "tokens": ["Das", "Al\u00b7te", "wird", "nie", "alt", ",", "es", "wird", "nur", "alt", "das", "Neu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADJD", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie sollt' ich B\u00fcndnisse im Alter neue schlie\u00dfen,", "tokens": ["Wie", "sollt'", "ich", "B\u00fcnd\u00b7nis\u00b7se", "im", "Al\u00b7ter", "neu\u00b7e", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "NN", "APPRART", "NN", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ich die Jugend sah in euerm Bund verflie\u00dfen!", "tokens": ["Da", "ich", "die", "Ju\u00b7gend", "sah", "in", "eu\u00b7erm", "Bund", "ver\u00b7flie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Zerflossen ist der Thau in scharfer Morgenluft,", "tokens": ["Zer\u00b7flos\u00b7sen", "ist", "der", "Thau", "in", "schar\u00b7fer", "Mor\u00b7gen\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nur aus euerm Gau weht der Erinnrung Duft.", "tokens": ["Und", "nur", "aus", "eu\u00b7erm", "Gau", "weht", "der", "E\u00b7rinn\u00b7rung", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}