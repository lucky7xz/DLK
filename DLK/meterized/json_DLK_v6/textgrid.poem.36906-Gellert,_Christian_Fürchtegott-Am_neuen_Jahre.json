{"textgrid.poem.36906": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Am neuen Jahre", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er ruft der Sonn und schafft den Mond,", "tokens": ["Er", "ruft", "der", "Sonn", "und", "schafft", "den", "Mond", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Jahr darnach zu teilen;", "tokens": ["Das", "Jahr", "dar\u00b7nach", "zu", "tei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er schafft es, da\u00df man sicher wohnt,", "tokens": ["Er", "schafft", "es", ",", "da\u00df", "man", "si\u00b7cher", "wohnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hei\u00dft die Zeiten eilen;", "tokens": ["Und", "hei\u00dft", "die", "Zei\u00b7ten", "ei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er ordnet Jahre, Tag und Nacht;", "tokens": ["Er", "ord\u00b7net", "Jah\u00b7re", ",", "Tag", "und", "Nacht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf! la\u00dft uns ihm, dem Gott der Macht,", "tokens": ["Auf", "!", "la\u00dft", "uns", "ihm", ",", "dem", "Gott", "der", "Macht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVIMP", "PPER", "PPER", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ruhm, Preis und Dank erteilen.", "tokens": ["Ruhm", ",", "Preis", "und", "Dank", "er\u00b7tei\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Herr, der da ist, und der da war!", "tokens": ["Herr", ",", "der", "da", "ist", ",", "und", "der", "da", "war", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "VAFIN", "$,", "KON", "ART", "ADV", "VAFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Von dankerf\u00fcllten Zungen", "tokens": ["Von", "dan\u00b7ker\u00b7f\u00fcll\u00b7ten", "Zun\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sei dir f\u00fcr das verflo\u00dfne Jahr", "tokens": ["Sei", "dir", "f\u00fcr", "das", "ver\u00b7flo\u00df\u00b7ne", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein heilig Lied gesungen;", "tokens": ["Ein", "hei\u00b7lig", "Lied", "ge\u00b7sun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fcr Leben, Wohlfahrt, Trost und Rat,", "tokens": ["F\u00fcr", "Le\u00b7ben", ",", "Wohl\u00b7fahrt", ",", "Trost", "und", "Rat", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr Fried und Ruh, f\u00fcr jede Tat,", "tokens": ["F\u00fcr", "Fried", "und", "Ruh", ",", "f\u00fcr", "je\u00b7de", "Tat", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die uns durch dich gelungen.", "tokens": ["Die", "uns", "durch", "dich", "ge\u00b7lun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "La\u00df auch dies Jahr gesegnet sein,", "tokens": ["La\u00df", "auch", "dies", "Jahr", "ge\u00b7seg\u00b7net", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PDS", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das du uns neu gegeben.", "tokens": ["Das", "du", "uns", "neu", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verleih uns Kraft, die Kraft ist dein,", "tokens": ["Ver\u00b7leih", "uns", "Kraft", ",", "die", "Kraft", "ist", "dein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,", "ART", "NN", "VAFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In deiner Furcht zu leben.", "tokens": ["In", "dei\u00b7ner", "Furcht", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du sch\u00fctzest uns, und du vermehrst", "tokens": ["Du", "sch\u00fct\u00b7zest", "uns", ",", "und", "du", "ver\u00b7mehrst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Menschen Gl\u00fcck, wenn sie zuerst", "tokens": ["Der", "Men\u00b7schen", "Gl\u00fcck", ",", "wenn", "sie", "zu\u00b7erst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nach deinem Reiche streben.", "tokens": ["Nach", "dei\u00b7nem", "Rei\u00b7che", "stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NE", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Gib mir, wofern es dir gef\u00e4llt,", "tokens": ["Gib", "mir", ",", "wo\u00b7fern", "es", "dir", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Lebens Ruh und Freuden.", "tokens": ["Des", "Le\u00b7bens", "Ruh", "und", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch schadet mir das Gl\u00fcck der Welt:", "tokens": ["Doch", "scha\u00b7det", "mir", "das", "Gl\u00fcck", "der", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So gib mir Kreuz und Leiden.", "tokens": ["So", "gib", "mir", "Kreuz", "und", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nur st\u00e4rke mit Geduld mein Herz,", "tokens": ["Nur", "st\u00e4r\u00b7ke", "mit", "Ge\u00b7duld", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und la\u00df mich nicht in Not und Schmerz", "tokens": ["Und", "la\u00df", "mich", "nicht", "in", "Not", "und", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "PTKNEG", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Gl\u00fccklichern beneiden.", "tokens": ["Die", "Gl\u00fcck\u00b7li\u00b7chern", "be\u00b7nei\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Hilf deinem Volke v\u00e4terlich", "tokens": ["Hilf", "dei\u00b7nem", "Vol\u00b7ke", "v\u00e4\u00b7ter\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In diesem Jahre wieder.", "tokens": ["In", "die\u00b7sem", "Jah\u00b7re", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Erbarme der Verla\u00dfnen dich,", "tokens": ["Er\u00b7bar\u00b7me", "der", "Ver\u00b7la\u00df\u00b7nen", "dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und der bedr\u00e4ngten Glieder.", "tokens": ["Und", "der", "be\u00b7dr\u00e4ng\u00b7ten", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gib Gl\u00fcck zu jeder guten Tat,", "tokens": ["Gib", "Gl\u00fcck", "zu", "je\u00b7der", "gu\u00b7ten", "Tat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und la\u00df dich, Gott, mit Heil und Rat", "tokens": ["Und", "la\u00df", "dich", ",", "Gott", ",", "mit", "Heil", "und", "Rat"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "$,", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf unsern F\u00fcrsten nieder;", "tokens": ["Auf", "un\u00b7sern", "F\u00fcrs\u00b7ten", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da\u00df Weisheit und Gerechtigkeit", "tokens": ["Da\u00df", "Weis\u00b7heit", "und", "Ge\u00b7rech\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf seinem Stuhle throne;", "tokens": ["Auf", "sei\u00b7nem", "Stuh\u00b7le", "thro\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df Tugend und Zufriedenheit", "tokens": ["Da\u00df", "Tu\u00b7gend", "und", "Zu\u00b7frie\u00b7den\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In unserm Lande wohne;", "tokens": ["In", "un\u00b7serm", "Lan\u00b7de", "woh\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df Treu und Liebe bei uns sei;", "tokens": ["Da\u00df", "Treu", "und", "Lie\u00b7be", "bei", "uns", "sei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dies, lieber Vater, dies verleih", "tokens": ["Dies", ",", "lie\u00b7ber", "Va\u00b7ter", ",", "dies", "ver\u00b7leih"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PDS", "$,", "ADV", "NN", "$,", "PDS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In Christo, deinem Sohne!", "tokens": ["In", "Chris\u00b7to", ",", "dei\u00b7nem", "Soh\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}