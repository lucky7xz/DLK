{"textgrid.poem.41498": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der malerische Lenz kann nichts so sinnreich bilden,", "tokens": ["Der", "ma\u00b7le\u00b7ri\u00b7sche", "Lenz", "kann", "nichts", "so", "sinn\u00b7reich", "bil\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PIS", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als jene Gegenden von Hainen und Gefilden;", "tokens": ["Als", "je\u00b7ne", "Ge\u00b7gen\u00b7den", "von", "Hai\u00b7nen", "und", "Ge\u00b7fil\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Anmuth Ueberflu\u00df erquickt dort Aug' und Brust:", "tokens": ["Der", "An\u00b7muth", "Ue\u00b7berf\u00b7lu\u00df", "er\u00b7quickt", "dort", "Aug'", "und", "Brust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "O Licht der weiten Felder!", "tokens": ["O", "Licht", "der", "wei\u00b7ten", "Fel\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O Nacht der stillen W\u00e4lder!", "tokens": ["O", "Nacht", "der", "stil\u00b7len", "W\u00e4l\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "O Vaterland der ersten Lust!", "tokens": ["O", "Va\u00b7ter\u00b7land", "der", "ers\u00b7ten", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dort l\u00e4\u00dft sich wiederum, in gr\u00fcnenden Troph\u00e4en,", "tokens": ["Dort", "l\u00e4\u00dft", "sich", "wie\u00b7de\u00b7rum", ",", "in", "gr\u00fc\u00b7nen\u00b7den", "Trop\u00b7h\u00e4\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Winters Untergang, der Flor des Fr\u00fchlings sehen;", "tokens": ["Des", "Win\u00b7ters", "Un\u00b7ter\u00b7gang", ",", "der", "Flor", "des", "Fr\u00fch\u00b7lings", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sein schmeichelnder Triumph begl\u00fccket jede Flur:", "tokens": ["Sein", "schmei\u00b7cheln\u00b7der", "Tri\u00b7umph", "be\u00b7gl\u00fc\u00b7cket", "je\u00b7de", "Flur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die frohen Lerchen fliegen", "tokens": ["Die", "fro\u00b7hen", "Ler\u00b7chen", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und singen von den Siegen", "tokens": ["Und", "sin\u00b7gen", "von", "den", "Sie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der t\u00e4glich sch\u00f6neren Natur.", "tokens": ["Der", "t\u00e4g\u00b7lich", "sch\u00f6\u00b7ne\u00b7ren", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Bach, den Eis verschlo\u00df und Sonn' und West entsiegeln,", "tokens": ["Der", "Bach", ",", "den", "Eis", "ver\u00b7schlo\u00df", "und", "Sonn'", "und", "West", "ent\u00b7sie\u00b7geln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "KON", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dem sich Luft und Baum und Hirt' und Heerde spiegeln,", "tokens": ["In", "dem", "sich", "Luft", "und", "Baum", "und", "Hirt'", "und", "Heer\u00b7de", "spie\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "NN", "KON", "NE", "KON", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Befruchtet und erfrischt das aufgelebte Land.", "tokens": ["Be\u00b7fruch\u00b7tet", "und", "er\u00b7frischt", "das", "auf\u00b7ge\u00b7leb\u00b7te", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dort l\u00e4\u00dft sich alles sehen,", "tokens": ["Dort", "l\u00e4\u00dft", "sich", "al\u00b7les", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was Flaccus in den H\u00f6hen", "tokens": ["Was", "Flac\u00b7cus", "in", "den", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des quellenreichen Tiburs fand.", "tokens": ["Des", "quel\u00b7len\u00b7rei\u00b7chen", "Ti\u00b7burs", "fand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Fast jeder Vogel singt; es schweigen Nord und Klage!", "tokens": ["Fast", "je\u00b7der", "Vo\u00b7gel", "singt", ";", "es", "schwei\u00b7gen", "Nord", "und", "Kla\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$.", "PPER", "VVFIN", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie sch\u00f6n verbinden sich, zum Muster guter Tage,", "tokens": ["Wie", "sch\u00f6n", "ver\u00b7bin\u00b7den", "sich", ",", "zum", "Mus\u00b7ter", "gu\u00b7ter", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PRF", "$,", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Hoffnung k\u00fcnft'ger Lust, der jetzige Genu\u00df!", "tokens": ["Die", "Hoff\u00b7nung", "k\u00fcnft'\u00b7ger", "Lust", ",", "der", "jet\u00b7zi\u00b7ge", "Ge\u00b7nu\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ihr stolzen, g\u00fcldnen Zeiten!", "tokens": ["Ihr", "stol\u00b7zen", ",", "g\u00fcld\u00b7nen", "Zei\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sagt, ob, an Fr\u00f6hlichkeiten,", "tokens": ["Sagt", ",", "ob", ",", "an", "Fr\u00f6h\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auch diese Zeit euch weichen mu\u00df.", "tokens": ["Auch", "die\u00b7se", "Zeit", "euch", "wei\u00b7chen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "An Reizung kann mir nichts den holden Stunden gleichen,", "tokens": ["An", "Rei\u00b7zung", "kann", "mir", "nichts", "den", "hol\u00b7den", "Stun\u00b7den", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PIS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da bei dem reinen Quell und in belaubten Str\u00e4uchen", "tokens": ["Da", "bei", "dem", "rei\u00b7nen", "Quell", "und", "in", "be\u00b7laub\u00b7ten", "Str\u00e4u\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die alte Freundschaft scherzt, die junge Liebe lacht.", "tokens": ["Die", "al\u00b7te", "Freund\u00b7schaft", "scherzt", ",", "die", "jun\u00b7ge", "Lie\u00b7be", "lacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Am Morgen keimt die Wonne", "tokens": ["Am", "Mor\u00b7gen", "keimt", "die", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und steiget mit der Sonne", "tokens": ["Und", "stei\u00b7get", "mit", "der", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und bl\u00fcht auch in der k\u00fchlen Nacht.", "tokens": ["Und", "bl\u00fcht", "auch", "in", "der", "k\u00fch\u00b7len", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Es spielen Luft und Laub; es spielen Wind und B\u00e4che;", "tokens": ["Es", "spie\u00b7len", "Luft", "und", "Laub", ";", "es", "spie\u00b7len", "Wind", "und", "B\u00e4\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$.", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dort duften Blum' und Gras; hier gr\u00fcnen Berg und Fl\u00e4che;", "tokens": ["Dort", "duf\u00b7ten", "Blum'", "und", "Gras", ";", "hier", "gr\u00fc\u00b7nen", "Berg", "und", "Fl\u00e4\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$.", "ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das muntre Landvolk tanzt; der Sch\u00e4fer singt und ruht:", "tokens": ["Das", "mun\u00b7tre", "Land\u00b7volk", "tanzt", ";", "der", "Sch\u00e4\u00b7fer", "singt", "und", "ruht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "ART", "NE", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die sichern Schafe weiden,", "tokens": ["Die", "si\u00b7chern", "Scha\u00b7fe", "wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und allgemeine Freuden", "tokens": ["Und", "all\u00b7ge\u00b7mei\u00b7ne", "Freu\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Erweitern gleichfalls mir den Muth.", "tokens": ["Er\u00b7wei\u00b7tern", "gleich\u00b7falls", "mir", "den", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Es soll den Wald ein Lied von Phyllis Ruhm erfreuen;", "tokens": ["Es", "soll", "den", "Wald", "ein", "Lied", "von", "Phyl\u00b7lis", "Ruhm", "er\u00b7freu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "NN", "APPR", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Fr\u00fchling will ich ihr und sie dem Fr\u00fchling weihen.", "tokens": ["Den", "Fr\u00fch\u00b7ling", "will", "ich", "ihr", "und", "sie", "dem", "Fr\u00fch\u00b7ling", "wei\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PPER", "KON", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie sind einander gleich, an Bl\u00fct' und Lieblichkeit.", "tokens": ["Sie", "sind", "ein\u00b7an\u00b7der", "gleich", ",", "an", "Bl\u00fct'", "und", "Lieb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihr frohnen meine Triebe,", "tokens": ["Ihr", "froh\u00b7nen", "mei\u00b7ne", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ihr schw\u00f6r' ich meine Liebe,", "tokens": ["Ihr", "schw\u00f6r'", "ich", "mei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fcr's erste bis zur Sommerszeit.", "tokens": ["F\u00fcr's", "ers\u00b7te", "bis", "zur", "Som\u00b7mers\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}