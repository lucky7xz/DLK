{"dta.poem.23715": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Tr\u00f6ster.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kleist du must, wenn ich einst sterbe,", "tokens": ["Kleist", "du", "must", ",", "wenn", "ich", "einst", "ster\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VMFIN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mein getreues M\u00e4dchen tr\u00f6sten.", "tokens": ["Mein", "ge\u00b7treu\u00b7es", "M\u00e4d\u00b7chen", "tr\u00f6s\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als ich nach volbrachter Bitte,", "tokens": ["Als", "ich", "nach", "vol\u00b7brach\u00b7ter", "Bit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wieder nach dem M\u00e4dchen sahe,", "tokens": ["Wie\u00b7der", "nach", "dem", "M\u00e4d\u00b7chen", "sa\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sah ich noch die Tr\u00e4nen fliessen;", "tokens": ["Sah", "ich", "noch", "die", "Tr\u00e4\u00b7nen", "flies\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ich stahl den Weisen Gr\u00fcnde,", "tokens": ["Und", "ich", "stahl", "den", "Wei\u00b7sen", "Gr\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ich sprach mit Trauerminen:", "tokens": ["Und", "ich", "sprach", "mit", "Trau\u00b7er\u00b7mi\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Weine nicht, gebeugtes M\u00e4dchen,", "tokens": ["Wei\u00b7ne", "nicht", ",", "ge\u00b7beug\u00b7tes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Weine nicht um deinen Liebling.", "tokens": ["Wei\u00b7ne", "nicht", "um", "dei\u00b7nen", "Lieb\u00b7ling", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Lebt er doch anitzt im Himmel,", "tokens": ["Lebt", "er", "doch", "a\u00b7nitzt", "im", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "G\u00f6nn ihm doch das Gl\u00fckk der Engel,", "tokens": ["G\u00f6nn", "ihm", "doch", "das", "Gl\u00fckk", "der", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Murre nicht mit dem Geschikke!", "tokens": ["Mur\u00b7re", "nicht", "mit", "dem", "Ge\u00b7schik\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Aber das gebeugte M\u00e4dchen", "tokens": ["A\u00b7ber", "das", "ge\u00b7beug\u00b7te", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Murrte doch mit dem Geschikke;", "tokens": ["Murr\u00b7te", "doch", "mit", "dem", "Ge\u00b7schik\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Denn von den erbla\u00dften Wangen", "tokens": ["Denn", "von", "den", "er\u00b7bla\u00df\u00b7ten", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.16": {"text": "Flossen noch viel heisse Tr\u00e4nen,", "tokens": ["Flos\u00b7sen", "noch", "viel", "heis\u00b7se", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Als ich ausgetr\u00f6stet hatte.", "tokens": ["Als", "ich", "aus\u00b7ge\u00b7tr\u00f6s\u00b7tet", "hat\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Ich verlie\u00df hierauf das M\u00e4dchen,", "tokens": ["Ich", "ver\u00b7lie\u00df", "hier\u00b7auf", "das", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Und begleitete die Leiche,", "tokens": ["Und", "be\u00b7glei\u00b7te\u00b7te", "die", "Lei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Ihres Lieblings in den Tempel.", "tokens": ["Ih\u00b7res", "Lieb\u00b7lings", "in", "den", "Tem\u00b7pel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Und nach zwanzig Todtenseufzern,", "tokens": ["Und", "nach", "zwan\u00b7zig", "Tod\u00b7ten\u00b7seuf\u00b7zern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Welche mich ein Redner lehrte,", "tokens": ["Wel\u00b7che", "mich", "ein", "Red\u00b7ner", "lehr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Ging ich wieder zu dem M\u00e4dchen.", "tokens": ["Ging", "ich", "wie\u00b7der", "zu", "dem", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Und ich tr\u00f6stete von neuen,", "tokens": ["Und", "ich", "tr\u00f6s\u00b7te\u00b7te", "von", "neu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}