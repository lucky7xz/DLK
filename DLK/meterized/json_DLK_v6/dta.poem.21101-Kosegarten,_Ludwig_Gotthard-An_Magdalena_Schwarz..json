{"dta.poem.21101": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "An  \n Magdalena Schwarz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Meine Freundin, es ist der Tag der Pfingsten               ", "tokens": ["Mei\u00b7ne", "Freun\u00b7din", ",", "es", "ist", "der", "Tag", "der", "Pfings\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "erschienen.", "tokens": ["er\u00b7schie\u00b7nen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Ihn umarmet der Tag, welcher ins Leben", "tokens": ["Ihn", "um\u00b7ar\u00b7met", "der", "Tag", ",", "wel\u00b7cher", "ins", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "--+--++--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "dich rief \u2014", "tokens": ["dich", "rief"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "In diess d\u00e4mmernde Leben, das einst ein Traum", "tokens": ["In", "diess", "d\u00e4m\u00b7mern\u00b7de", "Le\u00b7ben", ",", "das", "einst", "ein", "Traum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "PRELS", "ADV", "ART", "NN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "uns gemahnet,", "tokens": ["uns", "ge\u00b7mah\u00b7net", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Wann den entkerkerten Geist Wahrheit und", "tokens": ["Wann", "den", "ent\u00b7ker\u00b7ker\u00b7ten", "Geist", "Wahr\u00b7heit", "und"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "NN", "KON"], "meter": "+--+--++-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Freyheit umstrahlt;", "tokens": ["Frey\u00b7heit", "um\u00b7strahlt", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "In diess seufzende Rund, das nur als Schwelle des", "tokens": ["In", "diess", "seuf\u00b7zen\u00b7de", "Rund", ",", "das", "nur", "als", "Schwel\u00b7le", "des"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "PRELS", "ADV", "KOUS", "NN", "ART"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Himmels", "tokens": ["Him\u00b7mels"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Unsre Liebe verdient; in diess polarische", "tokens": ["Uns\u00b7re", "Lie\u00b7be", "ver\u00b7dient", ";", "in", "diess", "po\u00b7la\u00b7ri\u00b7sche"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP", "$.", "APPR", "PDAT", "ADJA"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "Land,", "tokens": ["Land", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Dessen N\u00e4chte nur sparsam der Meinungen Nordlicht", "tokens": ["Des\u00b7sen", "N\u00e4ch\u00b7te", "nur", "spar\u00b7sam", "der", "Mei\u00b7nun\u00b7gen", "Nord\u00b7licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "ADV", "ADJD", "ART", "NN", "NN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "durchflimmert,", "tokens": ["durch\u00b7flim\u00b7mert", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Dessen ewiges Eis, Liebe, dein Athem", "tokens": ["Des\u00b7sen", "e\u00b7wi\u00b7ges", "Eis", ",", "Lie\u00b7be", ",", "dein", "A\u00b7them"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "$,", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "nicht schmelzt;", "tokens": ["nicht", "schmelzt", ";"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "In diess Siberien, wo, gleich einem fl\u00fcchtigen", "tokens": ["In", "diess", "Si\u00b7be\u00b7ri\u00b7en", ",", "wo", ",", "gleich", "ei\u00b7nem", "fl\u00fcch\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PWAV", "$,", "ADV", "ART", "ADJA"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Freunde,", "tokens": ["Freun\u00b7de", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Sch\u00fcchtern die Freud' uns besucht, schnell", "tokens": ["Sch\u00fcch\u00b7tern", "die", "Freud'", "uns", "be\u00b7sucht", ",", "schnell"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "ART", "NN", "PPER", "VVPP", "$,", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "uns umhalset und flieht;", "tokens": ["uns", "um\u00b7hal\u00b7set", "und", "flieht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "\u201ewo die Gegenwart Wund' ist, und die Vergan-", "tokens": ["\u201e", "wo", "die", "Ge\u00b7gen\u00b7wart", "Wund'", "ist", ",", "und", "die", "Ver\u00b7gan"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PWAV", "ART", "NN", "NN", "VAFIN", "$,", "KON", "ART", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "genheit Narbe;\u201c ", "tokens": ["ge\u00b7nheit", "Nar\u00b7be", ";", "\u201c"], "token_info": ["word", "word", "punct", "punct"], "pos": ["APPR", "NN", "$.", "$("], "meter": "--+-", "measure": "anapaest.init"}, "line.7": {"text": "Wo die Besten von uns Fr\u00fcchte nicht tra-", "tokens": ["Wo", "die", "Bes\u00b7ten", "von", "uns", "Fr\u00fcch\u00b7te", "nicht", "tra"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "PPER", "NN", "PTKNEG", "TRUNC"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "gen, nur Laub;", "tokens": ["gen", ",", "nur", "Laub", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "NN", "$."], "meter": "--+", "measure": "anapaest.init"}, "line.9": {"text": "Wo wir klimmen auf staubigen Stufen der Thorheit", "tokens": ["Wo", "wir", "klim\u00b7men", "auf", "stau\u00b7bi\u00b7gen", "Stu\u00b7fen", "der", "Thor\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.10": {"text": "und S\u00fcnde,", "tokens": ["und", "S\u00fcn\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Zu der Vollkommenheit leuchtendem Tempel", "tokens": ["Zu", "der", "Voll\u00b7kom\u00b7men\u00b7heit", "leuch\u00b7ten\u00b7dem", "Tem\u00b7pel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "---+--+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "hinan.", "tokens": ["hi\u00b7nan", "."], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "O+", "measure": "unknown.measure.single"}}, "stanza.3": {"line.1": {"text": "Meine Freundin, ich wollte zum fr\u00f6hlichen Tage", "tokens": ["Mei\u00b7ne", "Freun\u00b7din", ",", "ich", "woll\u00b7te", "zum", "fr\u00f6h\u00b7li\u00b7chen", "Ta\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VMFIN", "APPRART", "ADJA", "NN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "der Pfingsten", "tokens": ["der", "Pfings\u00b7ten"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Dir ein fr\u00f6hliches Lied dichten; ich wollte", "tokens": ["Dir", "ein", "fr\u00f6h\u00b7li\u00b7ches", "Lied", "dich\u00b7ten", ";", "ich", "woll\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ART", "ADJA", "NN", "ADJA", "$.", "PPER", "VMFIN"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "dein Lob,", "tokens": ["dein", "Lob", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Das schon lange den Busen mir w\u00e4rmet, mit Einfalt", "tokens": ["Das", "schon", "lan\u00b7ge", "den", "Bu\u00b7sen", "mir", "w\u00e4r\u00b7met", ",", "mit", "Ein\u00b7falt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "ADV", "ADV", "ART", "NN", "PPER", "VAFIN", "$,", "APPR", "NN"], "meter": "--+--+--+-++-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "der Wahrheit,", "tokens": ["der", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Nicht mit der Dichtungen Schmuck einmal", "tokens": ["Nicht", "mit", "der", "Dich\u00b7tun\u00b7gen", "Schmuck", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "NN", "NN", "ADV"], "meter": "+--++-+-+", "measure": "dactylic.init"}, "line.8": {"text": "nur singen, und nie.", "tokens": ["nur", "sin\u00b7gen", ",", "und", "nie", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "KON", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Aber es h\u00fcllt mir die Seel' ein unausw\u00f6lkbares", "tokens": ["A\u00b7ber", "es", "h\u00fcllt", "mir", "die", "Seel'", "ein", "un\u00b7aus\u00b7w\u00f6lk\u00b7ba\u00b7res"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.10": {"text": "Dunkel;", "tokens": ["Dun\u00b7kel", ";"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Regengedanken umwehn meinen umnach-", "tokens": ["Re\u00b7gen\u00b7ge\u00b7dan\u00b7ken", "um\u00b7wehn", "mei\u00b7nen", "um\u00b7nach"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "TRUNC"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.12": {"text": "teten Geist;", "tokens": ["te\u00b7ten", "Geist", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "--+", "measure": "anapaest.init"}, "line.13": {"text": "Grauer Schatten umflort den weissen Brautschmuck", "tokens": ["Grau\u00b7er", "Schat\u00b7ten", "um\u00b7flort", "den", "weis\u00b7sen", "Brautsc\u00b7hmuck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "des Fr\u00fchlings;", "tokens": ["des", "Fr\u00fch\u00b7lings", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Feuchter Nebelduft h\u00fcllet das freundliche", "tokens": ["Feuch\u00b7ter", "Ne\u00b7bel\u00b7duft", "h\u00fcl\u00b7let", "das", "freund\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.16": {"text": "Gr\u00fcn.", "tokens": ["Gr\u00fcn", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.17": {"text": "Diese bl\u00fchende Welt und jener lasurene Him-", "tokens": ["Die\u00b7se", "bl\u00fc\u00b7hen\u00b7de", "Welt", "und", "je\u00b7ner", "la\u00b7su\u00b7re\u00b7ne", "Him"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "KON", "PDAT", "ADJA", "TRUNC"], "meter": "+-+--+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "mel,", "tokens": ["mel", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-", "measure": "single.down"}, "line.19": {"text": "Dieses prangende All d\u00e4ucht mir ein w\u00f6l-", "tokens": ["Die\u00b7ses", "pran\u00b7gen\u00b7de", "All", "d\u00e4ucht", "mir", "ein", "w\u00f6l"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "VVFIN", "PPER", "ART", "TRUNC"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "bendes Grab,", "tokens": ["ben\u00b7des", "Grab", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "--+", "measure": "anapaest.init"}, "line.21": {"text": "Drinnen tausendmal tausend geborstene Herzen ver-", "tokens": ["Drin\u00b7nen", "tau\u00b7send\u00b7mal", "tau\u00b7send", "ge\u00b7bors\u00b7te\u00b7ne", "Her\u00b7zen", "ver"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "CARD", "ADJA", "NN", "TRUNC"], "meter": "+-+--+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "wesen", "tokens": ["we\u00b7sen"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "(ach, sie schwollen so voll einst von Ent-", "tokens": ["(", "ach", ",", "sie", "schwol\u00b7len", "so", "voll", "einst", "von", "Ent"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ITJ", "$,", "PPER", "VVFIN", "ADV", "ADJD", "ADV", "APPR", "TRUNC"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.24": {"text": "z\u00fccken und Schmerz);", "tokens": ["z\u00fc\u00b7cken", "und", "Schmerz", ")", ";"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "KON", "NN", "$(", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.25": {"text": "Drinnen zusammengesunken in wenige stiebende", "tokens": ["Drin\u00b7nen", "zu\u00b7sam\u00b7men\u00b7ge\u00b7sun\u00b7ken", "in", "we\u00b7ni\u00b7ge", "stie\u00b7ben\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "ADJA"], "meter": "+--+--+--+--+--", "measure": "diphilius"}, "line.26": {"text": "Asche", "tokens": ["A\u00b7sche"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.27": {"text": "An des Br\u00e4utigams Brust ruhet die schlum-", "tokens": ["An", "des", "Br\u00e4u\u00b7ti\u00b7gams", "Brust", "ru\u00b7het", "die", "schlum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "NN", "VVFIN", "ART", "TRUNC"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.28": {"text": "mernde Braut.", "tokens": ["mern\u00b7de", "Braut", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "--+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Die du uns tr\u00e4gst und begr\u00e4bst, wer z\u00e4hlet, o Erde,", "tokens": ["Die", "du", "uns", "tr\u00e4gst", "und", "be\u00b7gr\u00e4bst", ",", "wer", "z\u00e4h\u00b7let", ",", "o", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "KON", "VVFIN", "$,", "PWS", "VVFIN", "$,", "FM", "NN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "die Stummen,", "tokens": ["die", "Stum\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Welche schlafen in dir, welche kein Hahnen-", "tokens": ["Wel\u00b7che", "schla\u00b7fen", "in", "dir", ",", "wel\u00b7che", "kein", "Hah\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "PPER", "$,", "PRELS", "PIAT", "TRUNC"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "ruf weckt!", "tokens": ["ruf", "weckt", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "--", "measure": "unknown.measure.zero"}}}}}