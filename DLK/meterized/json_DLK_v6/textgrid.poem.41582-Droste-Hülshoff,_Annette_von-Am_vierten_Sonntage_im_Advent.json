{"textgrid.poem.41582": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am vierten Sonntage im Advent", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fragst du mich, wer ich bin? Ich berg' es nicht:", "tokens": ["Fragst", "du", "mich", ",", "wer", "ich", "bin", "?", "Ich", "ber\u00b7g'", "es", "nicht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "PWS", "PPER", "VAFIN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Ein Wesen bin ich sonder Farb' und Licht,", "tokens": ["Ein", "We\u00b7sen", "bin", "ich", "son\u00b7der", "Fa\u00b7rb'", "und", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Schau mich nicht an; dann wendet sich dein Sinn;", "tokens": ["Schau", "mich", "nicht", "an", ";", "dann", "wen\u00b7det", "sich", "dein", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "PTKVZ", "$.", "ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Doch h\u00f6re! h\u00f6re! h\u00f6re! denn ich bin", "tokens": ["Doch", "h\u00f6\u00b7re", "!", "h\u00f6\u00b7re", "!", "h\u00f6\u00b7re", "!", "denn", "ich", "bin"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "VVFIN", "$.", "VVFIN", "$.", "KON", "PPER", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Des Rufers in der W\u00fcste Stimme.", "tokens": ["Des", "Ru\u00b7fers", "in", "der", "W\u00fcs\u00b7te", "Stim\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In N\u00e4chten voller Pein kam mir das Wort", "tokens": ["In", "N\u00e4ch\u00b7ten", "vol\u00b7ler", "Pein", "kam", "mir", "das", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von ihm, der Balsam s\u00e4t an Sumpfes Bord,", "tokens": ["Von", "ihm", ",", "der", "Bal\u00b7sam", "s\u00e4t", "an", "Sump\u00b7fes", "Bord", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ART", "NN", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Skorpion der Heilung \u00d6l gelegt,", "tokens": ["Im", "Skor\u00b7pi\u00b7on", "der", "Hei\u00b7lung", "\u00d6l", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem auch der wilde Dorn die Rose tr\u00e4gt,", "tokens": ["Dem", "auch", "der", "wil\u00b7de", "Dorn", "die", "Ro\u00b7se", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das faule Holz entz\u00fcndet sein Geglimme.", "tokens": ["Das", "fau\u00b7le", "Holz", "ent\u00b7z\u00fcn\u00b7det", "sein", "Ge\u00b7glim\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So senke deine Augen und vernimm", "tokens": ["So", "sen\u00b7ke", "dei\u00b7ne", "Au\u00b7gen", "und", "ver\u00b7nimm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "KON", "XY"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Von seinem Herold deines Herren Grimm,", "tokens": ["Von", "sei\u00b7nem", "He\u00b7rold", "dei\u00b7nes", "Her\u00b7ren", "Grimm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und seine Gnade sei dir auch bekannt,", "tokens": ["Und", "sei\u00b7ne", "Gna\u00b7de", "sei", "dir", "auch", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Wunde Heil, so wie der schwarze Brand,", "tokens": ["Der", "Wun\u00b7de", "Heil", ",", "so", "wie", "der", "schwar\u00b7ze", "Brand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADV", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn seiner Adern Bluten hemmt der Schlimme.", "tokens": ["Wenn", "sei\u00b7ner", "A\u00b7dern", "Blu\u00b7ten", "hemmt", "der", "Schlim\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Merk auf! ich wei\u00df es, da\u00df in h\u00e4rtster Brust", "tokens": ["Merk", "auf", "!", "ich", "wei\u00df", "es", ",", "da\u00df", "in", "h\u00e4rts\u00b7ter", "Brust"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch schlummert das Gewissen unbewu\u00dft;", "tokens": ["Doch", "schlum\u00b7mert", "das", "Ge\u00b7wis\u00b7sen", "un\u00b7be\u00b7wu\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Merk auf, wenn es erwacht, und seinen Schrei", "tokens": ["Merk", "auf", ",", "wenn", "es", "er\u00b7wacht", ",", "und", "sei\u00b7nen", "Schrei"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$,", "KOUS", "PPER", "VVPP", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ersticke nicht, wie M\u00fctter sonder Treu'", "tokens": ["Er\u00b7sti\u00b7cke", "nicht", ",", "wie", "M\u00fct\u00b7ter", "son\u00b7der", "Treu'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Des Bastards Wimmern und sein matt Gekr\u00fcmme!", "tokens": ["Des", "Bas\u00b7tards", "Wim\u00b7mern", "und", "sein", "matt", "Ge\u00b7kr\u00fcm\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich wei\u00df es auch, da\u00df in der ganzen Welt", "tokens": ["Ich", "wei\u00df", "es", "auch", ",", "da\u00df", "in", "der", "gan\u00b7zen", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem Teufel die Alt\u00e4re sind gestellt,", "tokens": ["Dem", "Teu\u00b7fel", "die", "Al\u00b7t\u00e4\u00b7re", "sind", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df mancher kniet, dem\u00fctig nicht gebeugt,", "tokens": ["Da\u00df", "man\u00b7cher", "kniet", ",", "de\u00b7m\u00fc\u00b7tig", "nicht", "ge\u00b7beugt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "ADJD", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und \u00fcberm Sumpfe, engelgleich und leicht", "tokens": ["Und", "\u00fc\u00b7berm", "Sump\u00b7fe", ",", "en\u00b7gel\u00b7gleich", "und", "leicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der wei\u00dfe Lotos wie ein Kindlein schwimme.", "tokens": ["Der", "wei\u00b7\u00dfe", "Lo\u00b7tos", "wie", "ein", "Kin\u00b7dlein", "schwim\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Es tobt des tollen Strudels Ungest\u00fcm", "tokens": ["Es", "tobt", "des", "tol\u00b7len", "Stru\u00b7dels", "Un\u00b7ge\u00b7st\u00fcm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und zitternd fliehen wir das Unget\u00fcm,", "tokens": ["Und", "zit\u00b7ternd", "flie\u00b7hen", "wir", "das", "Un\u00b7ge\u00b7t\u00fcm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Still liegt der Sumpf und lauert wie ein Dieb,", "tokens": ["Still", "liegt", "der", "Sumpf", "und", "lau\u00b7ert", "wie", "ein", "Dieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "KON", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wir pfl\u00fccken Blumen und es ist uns lieb", "tokens": ["Wir", "pfl\u00fc\u00b7cken", "Blu\u00b7men", "und", "es", "ist", "uns", "lieb"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "PPER", "VAFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zu schaun des Irrlichts tanzendes Geflimme.", "tokens": ["Zu", "schaun", "des", "Irr\u00b7lichts", "tan\u00b7zen\u00b7des", "Ge\u00b7flim\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Drum nicht vor dem Verruchten sei gewarnt;", "tokens": ["Drum", "nicht", "vor", "dem", "Ver\u00b7ruch\u00b7ten", "sei", "ge\u00b7warnt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKNEG", "APPR", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch wenn dich s\u00fc\u00dfer Unschuld Schein umgarnt,", "tokens": ["Doch", "wenn", "dich", "s\u00fc\u00b7\u00dfer", "Un\u00b7schuld", "Schein", "um\u00b7garnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJA", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann l\u00e4chelt der Vampyr, dann fahr zur\u00fcck", "tokens": ["Dann", "l\u00e4\u00b7chelt", "der", "Vam\u00b7pyr", ",", "dann", "fahr", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und senke tief, o tief in dich den Blick,", "tokens": ["Und", "sen\u00b7ke", "tief", ",", "o", "tief", "in", "dich", "den", "Blick", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "FM", "ADJD", "APPR", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ob leise quellend die Verwesung klimme!", "tokens": ["Ob", "lei\u00b7se", "quel\u00b7lend", "die", "Ver\u00b7we\u00b7sung", "klim\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVPP", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Ja, wo dein Aug' sich schaudernd wenden mag,", "tokens": ["Ja", ",", "wo", "dein", "Aug'", "sich", "schau\u00b7dernd", "wen\u00b7den", "mag", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWAV", "PPOSAT", "NN", "PRF", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da bist du sicher mindstens diesen Tag,", "tokens": ["Da", "bist", "du", "si\u00b7cher", "minds\u00b7tens", "die\u00b7sen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch gift'ger \u00f6fters ist ein Druck der Hand,", "tokens": ["Doch", "gift'\u00b7ger", "\u00f6f\u00b7ters", "ist", "ein", "Druck", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die weiche Tr\u00e4ne und der stille Brand,", "tokens": ["Die", "wei\u00b7che", "Tr\u00e4\u00b7ne", "und", "der", "stil\u00b7le", "Brand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den Lorbeer treibend aus Vulkanes Grimme.", "tokens": ["Den", "Lor\u00b7beer", "trei\u00b7bend", "aus", "Vul\u00b7ka\u00b7nes", "Grim\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NE", "NE", "$."], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}}, "stanza.9": {"line.1": {"text": "Ich bin ein Hauch nur, achtet nicht wie Tand", "tokens": ["Ich", "bin", "ein", "Hauch", "nur", ",", "ach\u00b7tet", "nicht", "wie", "Tand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$,", "VVFIN", "PTKNEG", "KOKOM", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein schwaches Wehn, um des der mich gesandt.", "tokens": ["Mein", "schwa\u00b7ches", "Wehn", ",", "um", "des", "der", "mich", "ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KOUI", "ART", "ART", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erwacht! erwacht! ihr steht in seinem Reich;", "tokens": ["Er\u00b7wacht", "!", "er\u00b7wacht", "!", "ihr", "steht", "in", "sei\u00b7nem", "Reich", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVPP", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Denn sehet, er ist mitten unter euch,", "tokens": ["Denn", "se\u00b7het", ",", "er", "ist", "mit\u00b7ten", "un\u00b7ter", "euch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den ihr verkennt, und ich bin seine Stimme!", "tokens": ["Den", "ihr", "ver\u00b7kennt", ",", "und", "ich", "bin", "sei\u00b7ne", "Stim\u00b7me", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "KON", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}