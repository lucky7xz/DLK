{"textgrid.poem.54052": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Nur", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": " dies singt eine Dame im Dreivierteltakt", "tokens": ["dies", "singt", "ei\u00b7ne", "Da\u00b7me", "im", "Drei\u00b7vier\u00b7tel\u00b7takt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Manchmal auf B\u00e4llen und Festen", "tokens": ["Manch\u00b7mal", "auf", "B\u00e4l\u00b7len", "und", "Fes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "tritt in den Saal ein freundlicher Mann,", "tokens": ["tritt", "in", "den", "Saal", "ein", "freund\u00b7li\u00b7cher", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "an Geist und Kultur von den Besten . . .", "tokens": ["an", "Geist", "und", "Kul\u00b7tur", "von", "den", "Bes\u00b7ten", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "ART", "NN", "$.", "$.", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "und macht sich an die Frauen heran.", "tokens": ["und", "macht", "sich", "an", "die", "Frau\u00b7en", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Doch schon nach wenigen Minuten", "tokens": ["Doch", "schon", "nach", "we\u00b7ni\u00b7gen", "Mi\u00b7nu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "ist alles zersprungen wie Glas \u2013", "tokens": ["ist", "al\u00b7les", "zer\u00b7sprun\u00b7gen", "wie", "Glas", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVINF", "KOKOM", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Von Geist keine Spur,", "tokens": ["Von", "Geist", "kei\u00b7ne", "Spur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "nichts mehr von Kultur:", "tokens": ["nichts", "mehr", "von", "Kul\u00b7tur", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Nur \u2013 nur \u2013 das.", "tokens": ["Nur", "\u2013", "nur", "\u2013", "das", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "$(", "ADV", "$(", "PDS", "$."], "meter": "+--", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Ber\u00fchmtheit ist ja kein Einwand", "tokens": ["Be\u00b7r\u00fchmt\u00b7heit", "ist", "ja", "kein", "Ein\u00b7wand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "gegen M\u00e4nner, die in den Filmen stehn.", "tokens": ["ge\u00b7gen", "M\u00e4n\u00b7ner", ",", "die", "in", "den", "Fil\u00b7men", "stehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ich l\u00fcpfte neulich die Leinwand,", "tokens": ["Ich", "l\u00fcpf\u00b7te", "neu\u00b7lich", "die", "Lein\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "ich wollt mal einen n\u00e4her sehn.", "tokens": ["ich", "wollt", "mal", "ei\u00b7nen", "n\u00e4\u00b7her", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach, war das eine Entt\u00e4uschung!", "tokens": ["Ach", ",", "war", "das", "ei\u00b7ne", "Ent\u00b7t\u00e4u\u00b7schung", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PDS", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ich bekam einen k\u00e4ltenden Ha\u00df \u2013", "tokens": ["Ich", "be\u00b7kam", "ei\u00b7nen", "k\u00e4l\u00b7ten\u00b7den", "Ha\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Von Herz keine Spur,", "tokens": ["Von", "Herz", "kei\u00b7ne", "Spur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "eine Karikatur . . .", "tokens": ["ei\u00b7ne", "Ka\u00b7ri\u00b7ka\u00b7tur", ".", ".", "."], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "$.", "$.", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "Und", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.10": {"text": "nur \u2013 nur \u2013 das.", "tokens": ["nur", "\u2013", "nur", "\u2013", "das", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "$(", "ADV", "$(", "PDS", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Ich nahm den Tee und den Kuchen", "tokens": ["Ich", "nahm", "den", "Tee", "und", "den", "Ku\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "in Berlin und Frohnau und mal hier und mal dort.", "tokens": ["in", "Ber\u00b7lin", "und", "Fro\u00b7hnau", "und", "mal", "hier", "und", "mal", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NE", "KON", "ADV", "ADV", "KON", "ADV", "ADV", "$."], "meter": "-++-+---+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Nun, dacht ich, willst mal versuchen", "tokens": ["Nun", ",", "dacht", "ich", ",", "willst", "mal", "ver\u00b7su\u00b7chen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VMFIN", "ADV", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "eine Freundschaft mit einem Herrn vom Sport.", "tokens": ["ei\u00b7ne", "Freund\u00b7schaft", "mit", "ei\u00b7nem", "Herrn", "vom", "Sport", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Der bricht das eigne Training \u2013", "tokens": ["Der", "bricht", "das", "eig\u00b7ne", "Trai\u00b7ning", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "auf wen ist denn heut noch Verla\u00df . . . ?", "tokens": ["auf", "wen", "ist", "denn", "heut", "noch", "Ver\u00b7la\u00df", ".", ".", ".", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["APPR", "PWS", "VAFIN", "ADV", "ADV", "ADV", "NN", "$.", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von Hirn keine Spur,", "tokens": ["Von", "Hirn", "kei\u00b7ne", "Spur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "eine h\u00fcbsche Figur \u2013", "tokens": ["ei\u00b7ne", "h\u00fcb\u00b7sche", "Fi\u00b7gur", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "aber sonst", "tokens": ["a\u00b7ber", "sonst"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "nur \u2013 nur \u2013 das.", "tokens": ["nur", "\u2013", "nur", "\u2013", "das", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "$(", "ADV", "$(", "PDS", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Wie kann man Frauen so verkennen?", "tokens": ["Wie", "kann", "man", "Frau\u00b7en", "so", "ver\u00b7ken\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PIS", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Gott, sie sind ja gar nicht so!", "tokens": ["Mein", "Gott", ",", "sie", "sind", "ja", "gar", "nicht", "so", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "ADV", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gewi\u00df, es will jede entbrennen . . .", "tokens": ["Ge\u00b7wi\u00df", ",", "es", "will", "je\u00b7de", "ent\u00b7bren\u00b7nen", ".", ".", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "PIAT", "VVINF", "$.", "$.", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "aber doch nicht stets und irgendwo!", "tokens": ["a\u00b7ber", "doch", "nicht", "stets", "und", "ir\u00b7gend\u00b7wo", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "ADV", "KON", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Auf Harfen kann jedermann klimpern,", "tokens": ["Auf", "Har\u00b7fen", "kann", "je\u00b7der\u00b7mann", "klim\u00b7pern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "es fragt sich nur: Wer spielt \u2013 und was . . .", "tokens": ["es", "fragt", "sich", "nur", ":", "Wer", "spielt", "\u2013", "und", "was", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "$.", "PWS", "VVFIN", "$(", "KON", "PWS", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und spielt er dann nur nach unsrer Natur \u2013:", "tokens": ["Und", "spielt", "er", "dann", "nur", "nach", "uns\u00b7rer", "Na\u00b7tur", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$(", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Dann gern", "tokens": ["Dann", "gern"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "auch das.", "tokens": ["auch", "das", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PDS", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}