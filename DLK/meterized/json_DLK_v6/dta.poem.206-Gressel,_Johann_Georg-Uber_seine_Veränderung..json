{"dta.poem.206": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Uber seine Ver\u00e4nderung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df die Ursach nicht im Hertzen zu ergr\u00fcnden/", "tokens": ["Ich", "wei\u00df", "die", "Ur\u00b7sach", "nicht", "im", "Hert\u00b7zen", "zu", "er\u00b7gr\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "APPRART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Warum/ wenn ", "tokens": ["Wa\u00b7rum", "/", "wenn"], "token_info": ["word", "punct", "word"], "pos": ["PWAV", "$(", "KOUS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Ich bald bin Purpur-roht/ bald blasser wie der Schnee/", "tokens": ["Ich", "bald", "bin", "Pur\u00b7pur\u00b7roht", "/", "bald", "blas\u00b7ser", "wie", "der", "Schnee", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "NN", "$(", "ADV", "ADJD", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und doch vor s\u00fcsser Lust auf Zucker-Rosen geh.", "tokens": ["Und", "doch", "vor", "s\u00fcs\u00b7ser", "Lust", "auf", "Zu\u00b7cke\u00b7rRo\u00b7sen", "geh."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "APPR", "NN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vielleichte wird die Gluht mir im Gesicht\u2019 erreget", "tokens": ["Viel\u00b7leich\u00b7te", "wird", "die", "Gluht", "mir", "im", "Ge\u00b7sicht'", "er\u00b7re\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von ihrer Augen-Blitz? der Feuer in sich heget/", "tokens": ["Von", "ih\u00b7rer", "Au\u00b7gen\u00b7Blitz", "?", "der", "Feu\u00b7er", "in", "sich", "he\u00b7get", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ART", "NN", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie! aber kan das Feur ernehren auch das Ey\u00df/", "tokens": ["Wie", "!", "a\u00b7ber", "kan", "das", "Feur", "er\u00b7neh\u00b7ren", "auch", "das", "Ey\u00df", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "ADV", "VMFIN", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und macht die Hitze auch die rohten Glieder wei\u00df?", "tokens": ["Und", "macht", "die", "Hit\u00b7ze", "auch", "die", "roh\u00b7ten", "Glie\u00b7der", "wei\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nein! diese Bl\u00e4sse kommt von denen ", "tokens": ["Nein", "!", "die\u00b7se", "Bl\u00e4s\u00b7se", "kommt", "von", "de\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "PDAT", "NN", "VVFIN", "APPR", "PRELS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wo man den Liebes-Schnee in heisser Gluht sieht wallen/", "tokens": ["Wo", "man", "den", "Lie\u00b7bes\u00b7Schnee", "in", "heis\u00b7ser", "Gluht", "sieht", "wal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und wo der Feur-Rubin auf Lust-", "tokens": ["Und", "wo", "der", "Feur\u00b7Ru\u00b7bin", "auf", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der sich mit stoltzem Schmuck bald neiget/ bald erh\u00f6ht.", "tokens": ["Der", "sich", "mit", "stolt\u00b7zem", "Schmuck", "bald", "nei\u00b7get", "/", "bald", "er\u00b7h\u00f6ht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ADJA", "NN", "ADV", "VVFIN", "$(", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich halt die ", "tokens": ["Ich", "halt", "die"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Ist es so beyderley mir im Gesicht erwecket/", "tokens": ["Ist", "es", "so", "bey\u00b7der\u00b7ley", "mir", "im", "Ge\u00b7sicht", "er\u00b7we\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sie ist es die erregt die heisse Liebes-Gluht/", "tokens": ["Sie", "ist", "es", "die", "er\u00b7regt", "die", "heis\u00b7se", "Lie\u00b7bes\u00b7Gluht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und die in Ey\u00df verkehrt das auffgewalte Blut.", "tokens": ["Und", "die", "in", "Ey\u00df", "ver\u00b7kehrt", "das", "auff\u00b7ge\u00b7wal\u00b7te", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}