{"textgrid.poem.48742": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "29. Auf Mons. Lyon Bernulli, F\u00fcrstl. Holsteinischer Gesandten Hofejunkern Namenstag, vor Kolumna auf der Moskaw gehalten, 1636 Juni 28.", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nim dieses Brieflein an, Freund einer meiner besten,", "tokens": ["Nim", "die\u00b7ses", "Brie\u00b7flein", "an", ",", "Freund", "ei\u00b7ner", "mei\u00b7ner", "bes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "PTKVZ", "$,", "NN", "ART", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die mir der ferne Weg nach Morgen zugesellt.", "tokens": ["die", "mir", "der", "fer\u00b7ne", "Weg", "nach", "Mor\u00b7gen", "zu\u00b7ge\u00b7sellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nims an und setze dich mit mir hin in das Zelt,", "tokens": ["Nims", "an", "und", "set\u00b7ze", "dich", "mit", "mir", "hin", "in", "das", "Zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "KON", "VVFIN", "PRF", "APPR", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "darinnen uns erquickt ein k\u00fchler Hauch von Westen,", "tokens": ["da\u00b7rin\u00b7nen", "uns", "er\u00b7quickt", "ein", "k\u00fch\u00b7ler", "Hauch", "von", "Wes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "doch mehr ein frischer Trunk, den du uns, deinen G\u00e4sten,", "tokens": ["doch", "mehr", "ein", "fri\u00b7scher", "Trunk", ",", "den", "du", "uns", ",", "dei\u00b7nen", "G\u00e4s\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "auf heute setzest f\u00fcr. Tu, was dir wolgef\u00e4llt,", "tokens": ["auf", "heu\u00b7te", "set\u00b7zest", "f\u00fcr", ".", "Tu", ",", "was", "dir", "wol\u00b7ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "APPR", "$.", "NE", "$,", "PWS", "PPER", "VMFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "was uns bei reicher Lust, bei Ehren dich erh\u00e4lt,", "tokens": ["was", "uns", "bei", "rei\u00b7cher", "Lust", ",", "bei", "Eh\u00b7ren", "dich", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ADJD", "NN", "$,", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "was man von Alters hat getan bei solchen Festen.", "tokens": ["was", "man", "von", "Al\u00b7ters", "hat", "ge\u00b7tan", "bei", "sol\u00b7chen", "Fes\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPR", "NN", "VAFIN", "VVPP", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Schau, wie die frohe Schaar der Moschischen Najaden", "tokens": ["Schau", ",", "wie", "die", "fro\u00b7he", "Schaar", "der", "Mo\u00b7schi\u00b7schen", "Na\u00b7ja\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "den Schwestern rufet zu, die in der ", "tokens": ["den", "Schwes\u00b7tern", "ru\u00b7fet", "zu", ",", "die", "in", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PRELS", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Schau, wie sie lustig sein und scherzen mit der Flut.", "tokens": ["Schau", ",", "wie", "sie", "lus\u00b7tig", "sein", "und", "scher\u00b7zen", "mit", "der", "Flut", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "ADJD", "VAINF", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Die angestrichne Zunft der farbichten Napeen", "tokens": ["Die", "an\u00b7ge\u00b7strich\u00b7ne", "Zunft", "der", "far\u00b7bich\u00b7ten", "Na\u00b7peen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "flicht einen bunten Kranz, der dir, Freund, zu soll stehen,", "tokens": ["flicht", "ei\u00b7nen", "bun\u00b7ten", "Kranz", ",", "der", "dir", ",", "Freund", ",", "zu", "soll", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "$,", "NN", "$,", "PTKA", "ADJD", "VVINF", "$,"], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "sie schrein gesamt mit uns: Wol dir, du tapfres Blut!", "tokens": ["sie", "schrein", "ge\u00b7samt", "mit", "uns", ":", "Wol", "dir", ",", "du", "tapf\u00b7res", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "APPR", "PPER", "$.", "ADV", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}