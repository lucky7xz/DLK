{"textgrid.poem.39439": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was tr\u00fcbte dir, o Rhein, den klaren Spiegel?", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was tr\u00fcbte dir, o Rhein, den klaren Spiegel?", "tokens": ["Was", "tr\u00fcb\u00b7te", "dir", ",", "o", "Rhein", ",", "den", "kla\u00b7ren", "Spie\u00b7gel", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "FM", "NE", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie welkten eure Kr\u00e4nze, Rebenh\u00fcgel?", "tokens": ["Wie", "welk\u00b7ten", "eu\u00b7re", "Kr\u00e4n\u00b7ze", ",", "Re\u00b7ben\u00b7h\u00fc\u00b7gel", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Warum verh\u00fcllt die Landschaft rings ein Flor?", "tokens": ["Wa\u00b7rum", "ver\u00b7h\u00fcllt", "die", "Land\u00b7schaft", "rings", "ein", "Flor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So fragt' ich, und ein Fl\u00fcstern traf mein Ohr:", "tokens": ["So", "fragt'", "ich", ",", "und", "ein", "Fl\u00fcs\u00b7tern", "traf", "mein", "Ohr", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbverkleidet wandeln in den Erdbezirken", "tokens": ["\u00bb", "ver\u00b7klei\u00b7det", "wan\u00b7deln", "in", "den", "Erd\u00b7be\u00b7zir\u00b7ken"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVPP", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Himmlischen, doch sie verr\u00e4th ihr Wirken,", "tokens": ["Die", "Himm\u00b7li\u00b7schen", ",", "doch", "sie", "ver\u00b7r\u00e4\u00b7th", "ihr", "Wir\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Weil ihrer N\u00e4h' geheimni\u00dfvolle Kraft", "tokens": ["Weil", "ih\u00b7rer", "N\u00e4h'", "ge\u00b7heim\u00b7ni\u00df\u00b7vol\u00b7le", "Kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In der Natur ein sch\u00f6n'res Leben schafft.", "tokens": ["In", "der", "Na\u00b7tur", "ein", "sch\u00f6n'\u00b7res", "Le\u00b7ben", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein milder Blick hat oft auf uns verweilet,", "tokens": ["Ein", "mil\u00b7der", "Blick", "hat", "oft", "auf", "uns", "ver\u00b7wei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Ein leiser Fu\u00dftritt unsre Flur durcheilet;", "tokens": ["Ein", "lei\u00b7ser", "Fu\u00df\u00b7tritt", "uns\u00b7re", "Flur", "durc\u00b7hei\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir prangten, da uns diese Sonne schien:", "tokens": ["Wir", "prang\u00b7ten", ",", "da", "uns", "die\u00b7se", "Son\u00b7ne", "schien", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die fremde Zierde war uns nur geliehn.\u00ab", "tokens": ["Die", "frem\u00b7de", "Zier\u00b7de", "war", "uns", "nur", "ge\u00b7liehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}