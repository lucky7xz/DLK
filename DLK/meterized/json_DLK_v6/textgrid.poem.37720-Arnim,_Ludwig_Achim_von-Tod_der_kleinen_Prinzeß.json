{"textgrid.poem.37720": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Tod der kleinen Prinze\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wunderbar nenn' ich den Tod und g\u00fctig m\u00f6cht ihr ihn preisen,", "tokens": ["Wun\u00b7der\u00b7bar", "nenn'", "ich", "den", "Tod", "und", "g\u00fc\u00b7tig", "m\u00f6cht", "ihr", "ihn", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "KON", "ADJD", "VMFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Der das f\u00fcrstliche Kind, eingesungen so fr\u00fch,", "tokens": ["Der", "das", "f\u00fcrst\u00b7li\u00b7che", "Kind", ",", "ein\u00b7ge\u00b7sun\u00b7gen", "so", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "$,", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Von der traurigen Zeit erkannt' es nur wechselnde Farben,", "tokens": ["Von", "der", "trau\u00b7ri\u00b7gen", "Zeit", "er\u00b7kannt'", "es", "nur", "wech\u00b7seln\u00b7de", "Far\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Fl\u00fcchtend am \u00f6den Gestad', lacht es ins schwankende Meer.", "tokens": ["Fl\u00fcch\u00b7tend", "am", "\u00f6\u00b7den", "Ge\u00b7stad'", ",", "lacht", "es", "ins", "schwan\u00b7ken\u00b7de", "Meer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Vernstein gab ihm das Meer zum Spielen mit lieblichem Weihrauch,", "tokens": ["Verns\u00b7tein", "gab", "ihm", "das", "Meer", "zum", "Spie\u00b7len", "mit", "lieb\u00b7li\u00b7chem", "Weih\u00b7rauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Steine der Krone die fiel, legt es zerschellet dabei,", "tokens": ["Stei\u00b7ne", "der", "Kro\u00b7ne", "die", "fiel", ",", "legt", "es", "zer\u00b7schel\u00b7let", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "VVFIN", "$,", "VVFIN", "PPER", "VVFIN", "PAV", "$,"], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Also erblickte es froh das Ende des \u00f6desten Landes,", "tokens": ["Al\u00b7so", "er\u00b7blick\u00b7te", "es", "froh", "das", "En\u00b7de", "des", "\u00f6\u00b7des\u00b7ten", "Lan\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Sah das Jenseit der Welt, sch\u00f6n von Fluthen umtanzt.", "tokens": ["Sah", "das", "Jen\u00b7seit", "der", "Welt", ",", "sch\u00f6n", "von", "Flut\u00b7hen", "um\u00b7tanzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,", "ADJD", "APPR", "NN", "VVPP", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.9": {"text": "Sah Paradiese von fern, so nennen nur Preu\u00dfen die \u00d6de,", "tokens": ["Sah", "Pa\u00b7ra\u00b7die\u00b7se", "von", "fern", ",", "so", "nen\u00b7nen", "nur", "Preu\u00b7\u00dfen", "die", "\u00d6\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ADJD", "$,", "ADV", "VVFIN", "ADV", "NN", "ART", "NN", "$,"], "meter": "-+-+----+--+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Ihm erschien es so sch\u00f6n auf dem schwebenden Sarg,", "tokens": ["Ihm", "er\u00b7schien", "es", "so", "sch\u00f6n", "auf", "dem", "schwe\u00b7ben\u00b7den", "Sarg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.11": {"text": "Denn im Schiffe da blies ein Engel auf eilendem Sturmwind", "tokens": ["Denn", "im", "Schif\u00b7fe", "da", "blies", "ein", "En\u00b7gel", "auf", "ei\u00b7len\u00b7dem", "Sturm\u00b7wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Seine Seele ihm aus und das Kindlein verschied.", "tokens": ["Sei\u00b7ne", "See\u00b7le", "ihm", "aus", "und", "das", "Kin\u00b7dlein", "ver\u00b7schied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "PTKVZ", "KON", "ART", "NN", "VVFIN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.13": {"text": "Lieblich l\u00e4chelt es noch, als s\u00e4he es Himmelsgefilde,", "tokens": ["Lieb\u00b7lich", "l\u00e4\u00b7chelt", "es", "noch", ",", "als", "s\u00e4\u00b7he", "es", "Him\u00b7mels\u00b7ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "$,", "KOKOM", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.14": {"text": "H\u00f6rte Sirenen im Meer, gl\u00fcckliche V\u00f6lker am Land,", "tokens": ["H\u00f6r\u00b7te", "Si\u00b7re\u00b7nen", "im", "Meer", ",", "gl\u00fcck\u00b7li\u00b7che", "V\u00f6l\u00b7ker", "am", "Land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+---+---+--+", "measure": "iambic.penta.chol"}, "line.15": {"text": "Rauh wir finden dies Land, umst\u00fcrmet umfluthet von Schrecken,", "tokens": ["Rauh", "wir", "fin\u00b7den", "dies", "Land", ",", "um\u00b7st\u00fcr\u00b7met", "um\u00b7flut\u00b7het", "von", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "PDS", "NN", "$,", "VVFIN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Es schlief ruhig davor, wie der Schiffer am Mast,", "tokens": ["Es", "schlief", "ru\u00b7hig", "da\u00b7vor", ",", "wie", "der", "Schif\u00b7fer", "am", "Mast", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PAV", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Es schl\u00e4ft ruhig am Land, wenn dr\u00fcber ihr Feinde auch tobet,", "tokens": ["Es", "schl\u00e4ft", "ru\u00b7hig", "am", "Land", ",", "wenn", "dr\u00fc\u00b7ber", "ihr", "Fein\u00b7de", "auch", "to\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "$,", "KOUS", "PAV", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Habt ihr selber nicht Lust, so zu ruhn wie das Kind?", "tokens": ["Habt", "ihr", "sel\u00b7ber", "nicht", "Lust", ",", "so", "zu", "ruhn", "wie", "das", "Kind", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "NN", "$,", "ADV", "PTKZU", "VVINF", "KOKOM", "ART", "NN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.19": {"text": "Einz'ge Bewohner des Strands, ihr ewig klagenden V\u00f6gel,", "tokens": ["Einz'\u00b7ge", "Be\u00b7woh\u00b7ner", "des", "Strands", ",", "ihr", "e\u00b7wig", "kla\u00b7gen\u00b7den", "V\u00f6\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.20": {"text": "St\u00fcrzt euch ins st\u00fcrmende Meer, nicht erweckt es der Schrei.", "tokens": ["St\u00fcrzt", "euch", "ins", "st\u00fcr\u00b7men\u00b7de", "Meer", ",", "nicht", "er\u00b7weckt", "es", "der", "Schrei", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,", "PTKNEG", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}}}}