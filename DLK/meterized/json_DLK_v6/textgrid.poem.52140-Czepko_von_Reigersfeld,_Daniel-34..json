{"textgrid.poem.52140": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "34.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Erde zur Artzney, wie sie gesiegelt ist,", "tokens": ["Die", "Er\u00b7de", "zur", "Artz\u00b7ney", ",", "wie", "sie", "ge\u00b7sie\u00b7gelt", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "PWAV", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+--++-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Nehmt lieber Ritterfurt, so gut ich sie erkiest:", "tokens": ["Nehmt", "lie\u00b7ber", "Rit\u00b7ter\u00b7furt", ",", "so", "gut", "ich", "sie", "er\u00b7kiest", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NE", "$,", "ADV", "ADJD", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zum Siegel meiner Treu und Freundschafft setzt sie ein,", "tokens": ["Zum", "Sie\u00b7gel", "mei\u00b7ner", "Treu", "und", "Freund\u00b7schafft", "setzt", "sie", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "KON", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es ist zwar schlecht, doch denckt, da\u00df wir auch Erde seyn.", "tokens": ["Es", "ist", "zwar", "schlecht", ",", "doch", "denckt", ",", "da\u00df", "wir", "auch", "Er\u00b7de", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADV", "VVFIN", "$,", "KOUS", "PPER", "ADV", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}