{"textgrid.poem.36527": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "26. Der Sperling und der Dichter", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Sperling sa\u00df auf einem Dach,", "tokens": ["Ein", "Sper\u00b7ling", "sa\u00df", "auf", "ei\u00b7nem", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und h\u00f6rte meine Stubenlerche singen;", "tokens": ["Und", "h\u00f6r\u00b7te", "mei\u00b7ne", "Stu\u00b7ben\u00b7ler\u00b7che", "sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und lauschte, lauschte; sang ihr nach;", "tokens": ["Und", "lauschte", ",", "lauschte", ";", "sang", "ihr", "nach", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "$.", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Allein es wollt' ihm nicht gelingen,", "tokens": ["Al\u00b7lein", "es", "wollt'", "ihm", "nicht", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er traf nicht einen rechten Ton!", "tokens": ["Er", "traf", "nicht", "ei\u00b7nen", "rech\u00b7ten", "Ton", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So, dacht' ich, lern' ich auch nicht singen,", "tokens": ["So", ",", "dacht'", "ich", ",", "lern'", "ich", "auch", "nicht", "sin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ein Anakreon!", "tokens": ["Wie", "ein", "A\u00b7nak\u00b7re\u00b7on", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}