{"textgrid.poem.32306": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Die Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ohne Liebe", "tokens": ["Oh\u00b7ne", "Lie\u00b7be"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Lebe, wer da kann.", "tokens": ["Le\u00b7be", ",", "wer", "da", "kann", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWS", "ADV", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn er auch ein Mensch schon bliebe,", "tokens": ["Wenn", "er", "auch", "ein", "Mensch", "schon", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleibt er doch kein Mann.", "tokens": ["Bleibt", "er", "doch", "kein", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "S\u00fc\u00dfe Liebe,", "tokens": ["S\u00fc\u00b7\u00dfe", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Mach' mein Leben s\u00fc\u00df!", "tokens": ["Mach'", "mein", "Le\u00b7ben", "s\u00fc\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Stille nie die regen Triebe", "tokens": ["Stil\u00b7le", "nie", "die", "re\u00b7gen", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sonder Hindernis.", "tokens": ["Son\u00b7der", "Hin\u00b7der\u00b7nis", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Schmachten lassen", "tokens": ["Schmach\u00b7ten", "las\u00b7sen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Sei der Sch\u00f6nen Pflicht!", "tokens": ["Sei", "der", "Sch\u00f6\u00b7nen", "Pflicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Nur uns ewig schmachten lassen,", "tokens": ["Nur", "uns", "e\u00b7wig", "schmach\u00b7ten", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJD", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieses sei sie nicht.", "tokens": ["Die\u00b7ses", "sei", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}