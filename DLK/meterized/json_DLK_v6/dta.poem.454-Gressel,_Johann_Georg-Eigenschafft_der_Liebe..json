{"dta.poem.454": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Eigenschafft der Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Wer liebet/ findt vor sich ein Hoffen/ das betriegt/", "tokens": ["Wer", "lie\u00b7bet", "/", "findt", "vor", "sich", "ein", "Hof\u00b7fen", "/", "das", "be\u00b7triegt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "VVFIN", "APPR", "PRF", "ART", "NN", "$(", "PDS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein F\u00fcrchten/ so stets daurt/ und Lust/ die bald verfliegt/", "tokens": ["Ein", "F\u00fcrch\u00b7ten", "/", "so", "stets", "daurt", "/", "und", "Lust", "/", "die", "bald", "ver\u00b7fliegt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ADV", "VVFIN", "$(", "KON", "NN", "$(", "PRELS", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Versanrte Fr\u00f6lichkeit und einen s\u00fcssen Schmertz/", "tokens": ["Ver\u00b7sanr\u00b7te", "Fr\u00f6\u00b7lich\u00b7keit", "und", "ei\u00b7nen", "s\u00fcs\u00b7sen", "Schmertz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn bey der Liebe findt man Kummer und auch Schertz.", "tokens": ["Denn", "bey", "der", "Lie\u00b7be", "findt", "man", "Kum\u00b7mer", "und", "auch", "Scher