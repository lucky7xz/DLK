{"textgrid.poem.63469": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "He st\u00fcrw", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nu, Meister, schruw He to dat Sark!", "tokens": ["Nu", ",", "Meis\u00b7ter", ",", "schruw", "He", "to", "dat", "Sark", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "FM", "FM", "FM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nu, Moder, Moder, mak di stark", "tokens": ["Nu", ",", "Mo\u00b7der", ",", "Mo\u00b7der", ",", "mak", "di", "stark"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "$,", "NN", "$,", "NE", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "un dr\u00f6g di af din Tran!", "tokens": ["un", "dr\u00f6g", "di", "af", "din", "Tran", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "De Wag de f\u00fchr nu v\u00f6r, Johann,", "tokens": ["De", "Wag", "de", "f\u00fchr", "nu", "v\u00f6r", ",", "Jo\u00b7hann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "un Schritt f\u00f6r Schritt lettst du din Spann", "tokens": ["un", "Schritt", "f\u00f6r", "Schritt", "lettst", "du", "din", "Spann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "NN", "ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "hen nah den Kirchhoff gahn.", "tokens": ["hen", "nah", "den", "Kirch\u00b7hoff", "gahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wi, Kinner, wi gahn mit to Fot;", "tokens": ["Wi", ",", "Kin\u00b7ner", ",", "wi", "gahn", "mit", "to", "Fot", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PWAV", "VVFIN", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "de Weg to Gott is \u00fcmmer got,", "tokens": ["de", "Weg", "to", "Gott", "is", "\u00fcm\u00b7mer", "got", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NE", "NN", "FM", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "un is't sonn sworen Gang,", "tokens": ["un", "is't", "sonn", "swo\u00b7ren", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "KON", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "so swor as ick em gahn nu m\u00f6t,", "tokens": ["so", "swor", "as", "ick", "em", "gahn", "nu", "m\u00f6t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "mi dr\u00e4gen doch sacht hen min F\u00f6t,", "tokens": ["mi", "dr\u00e4\u00b7gen", "doch", "sacht", "hen", "min", "F\u00f6t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "wir he noch eens so lang.", "tokens": ["wir", "he", "noch", "e\u00b7ens", "so", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "+-++--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Wat freugt ick mi, as Gott em gew \u2013", "tokens": ["Wat", "freugt", "ick", "mi", ",", "as", "Gott", "em", "gew", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "sonn eenzigst Kind hett een so leef,", "tokens": ["sonn", "e\u00b7en\u00b7zigst", "Kind", "hett", "e\u00b7en", "so", "leef", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ART", "ADV", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "wat helpt dat alltosam!", "tokens": ["wat", "helpt", "dat", "all\u00b7to\u00b7sam", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "He st\u00fcrw; ick bring em nah sin Graff,", "tokens": ["He", "st\u00fcrw", ";", "ick", "bring", "em", "nah", "sin", "Graff", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gott's Husd\u00f6r is't, dor sett'ck em af \u2013", "tokens": ["Gott's", "Hus\u00b7d\u00f6r", "is't", ",", "dor", "sett'ck", "em", "af", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "+----+-+", "measure": "dactylic.init"}, "line.6": {"text": "f\u00fchr to, in Jesu Nam!", "tokens": ["f\u00fchr", "to", ",", "in", "Je\u00b7su", "Nam", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "APPR", "NE", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}