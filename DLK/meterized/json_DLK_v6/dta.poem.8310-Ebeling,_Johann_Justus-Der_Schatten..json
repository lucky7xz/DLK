{"dta.poem.8310": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Der Schatten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Schatten folget allgemach,", "tokens": ["Der", "Schat\u00b7ten", "fol\u00b7get", "all\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jm Sonnenschein den K\u00f6rper nach:", "tokens": ["Jm", "Son\u00b7nen\u00b7schein", "den", "K\u00f6r\u00b7per", "nach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er kan uns Menschen von dem Leben", "tokens": ["Er", "kan", "uns", "Men\u00b7schen", "von", "dem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Manch lehrreich Bild zu lesen geben.", "tokens": ["Manch", "lehr\u00b7reich", "Bild", "zu", "le\u00b7sen", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So lang die Gl\u00fckkessonne strahlt,", "tokens": ["So", "lang", "die", "Gl\u00fck\u00b7kes\u00b7son\u00b7ne", "strahlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sind wir mit Ehr und Ruhm be-", "tokens": ["Sind", "wir", "mit", "Ehr", "und", "Ruhm", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und kaum ist diese untergangen;", "tokens": ["Und", "kaum", "ist", "die\u00b7se", "un\u00b7ter\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So endigt sich auch unser Prangen.", "tokens": ["So", "en\u00b7digt", "sich", "auch", "un\u00b7ser", "Pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der Schatten der vom Leib entsteht,", "tokens": ["Der", "Schat\u00b7ten", "der", "vom", "Leib", "ent\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und seinem K\u00f6rper stets nachgeht,", "tokens": ["Und", "sei\u00b7nem", "K\u00f6r\u00b7per", "stets", "nach\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Scheint bei dem schr\u00e4gen Stand vom Lichte,", "tokens": ["Scheint", "bei", "dem", "schr\u00e4\u00b7gen", "Stand", "vom", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Oft gr\u00f6sser uns ins Angesichte,", "tokens": ["Oft", "gr\u00f6s\u00b7ser", "uns", "ins", "An\u00b7ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Als wie der K\u00f6rper selbsten ist:", "tokens": ["Als", "wie", "der", "K\u00f6r\u00b7per", "selbs\u00b7ten", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wenn man von Wundern, Thaten liest,", "tokens": ["Wenn", "man", "von", "Wun\u00b7dern", ",", "Tha\u00b7ten", "liest", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Die dieser oder der verrichtet;", "tokens": ["Die", "die\u00b7ser", "o\u00b7der", "der", "ver\u00b7rich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "KON", "ART", "VVPP", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.16": {"text": "So ist es \u00f6fters nur erdichtet,", "tokens": ["So", "ist", "es", "\u00f6f\u00b7ters", "nur", "er\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Ist gros des Menschen Ehrenstand,", "tokens": ["Ist", "gros", "des", "Men\u00b7schen", "Eh\u00b7ren\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "So wird die That auch gros genannt,", "tokens": ["So", "wird", "die", "That", "auch", "gros", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Der Ohnmacht werden grosse St\u00e4rke,", "tokens": ["Der", "Ohn\u00b7macht", "wer\u00b7den", "gros\u00b7se", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Und kleinen Geistern, Wunderwerke", "tokens": ["Und", "klei\u00b7nen", "Geis\u00b7tern", ",", "Wun\u00b7der\u00b7wer\u00b7ke"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "ADJA", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Zu ihrem Ruhme beigelegt:", "tokens": ["Zu", "ih\u00b7rem", "Ruh\u00b7me", "bei\u00b7ge\u00b7legt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Doch wenn mans in der N\u00e4h erwegt", "tokens": ["Doch", "wenn", "mans", "in", "der", "N\u00e4h", "er\u00b7wegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "So sind es oft nur Heldenthaten,", "tokens": ["So", "sind", "es", "oft", "nur", "Hel\u00b7den\u00b7tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Die nicht zu sonderbar gerathen.", "tokens": ["Die", "nicht", "zu", "son\u00b7der\u00b7bar", "ge\u00b7ra\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Verdienst und Ruhm sind zwar vereint,", "tokens": ["Ver\u00b7dienst", "und", "Ruhm", "sind", "zwar", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Doch wenn das Licht verkehret scheint,", "tokens": ["Doch", "wenn", "das", "Licht", "ver\u00b7keh\u00b7ret", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "So kan sich ein recht grosser Schatten", "tokens": ["So", "kan", "sich", "ein", "recht", "gros\u00b7ser", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PRF", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Mit einem kleinen K\u00f6rper gatten:", "tokens": ["Mit", "ei\u00b7nem", "klei\u00b7nen", "K\u00f6r\u00b7per", "gat\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Und das geschiehet in der Welt,", "tokens": ["Und", "das", "ge\u00b7schie\u00b7het", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Nachdem das blind Ger\u00fcchte f\u00e4llt;", "tokens": ["Nach\u00b7dem", "das", "blind", "Ge\u00b7r\u00fcch\u00b7te", "f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "So werden ofte kleine Proben,", "tokens": ["So", "wer\u00b7den", "of\u00b7te", "klei\u00b7ne", "Pro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Weit \u00fcber das Verdienst erhoben.", "tokens": ["Weit", "\u00fc\u00b7ber", "das", "Ver\u00b7dienst", "er\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Man merket auch beim Schatten-Schein,", "tokens": ["Man", "mer\u00b7ket", "auch", "beim", "Schat\u00b7ten\u00b7Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Da\u00df solche Theile dunkler seyn,", "tokens": ["Da\u00df", "sol\u00b7che", "Thei\u00b7le", "dunk\u00b7ler", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADJA", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Die an den K\u00f6rpern nahe stehen;", "tokens": ["Die", "an", "den", "K\u00f6r\u00b7pern", "na\u00b7he", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Daran ist diese Lehr zu sehen:", "tokens": ["Da\u00b7ran", "ist", "die\u00b7se", "Lehr", "zu", "se\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Der Schein der Ehre ist sehr schwach,", "tokens": ["Der", "Schein", "der", "Eh\u00b7re", "ist", "sehr", "schwach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Den einer in der N\u00e4he hat,", "tokens": ["Den", "ei\u00b7ner", "in", "der", "N\u00e4\u00b7he", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Ja! mancher ist an einem Orte,", "tokens": ["Ja", "!", "man\u00b7cher", "ist", "an", "ei\u00b7nem", "Or\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PIS", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Da man oft kaum mit einem Worte,", "tokens": ["Da", "man", "oft", "kaum", "mit", "ei\u00b7nem", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Das Lob, das ihm geb\u00fchrt, erhebt,", "tokens": ["Das", "Lob", ",", "das", "ihm", "ge\u00b7b\u00fchrt", ",", "er\u00b7hebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Man sieht ihn nicht, weil er da lebt.", "tokens": ["Man", "sieht", "ihn", "nicht", ",", "weil", "er", "da", "lebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Der Schatten ist am \u00e4usren Rande", "tokens": ["Der", "Schat\u00b7ten", "ist", "am", "\u00e4us\u00b7ren", "Ran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Viel heller als in nahen Stande:", "tokens": ["Viel", "hel\u00b7ler", "als", "in", "na\u00b7hen", "Stan\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Das ist ein Bild von Kunst und Fleis,", "tokens": ["Das", "ist", "ein", "Bild", "von", "Kunst", "und", "Fleis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Die haben da mehr Ruhm und Preis,", "tokens": ["Die", "ha\u00b7ben", "da", "mehr", "Ruhm", "und", "Preis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Von ihren flei\u00dfigen Bestreben,", "tokens": ["Von", "ih\u00b7ren", "flei\u00b7\u00dfi\u00b7gen", "Be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Wo sie ganz weit entfernet, leben.", "tokens": ["Wo", "sie", "ganz", "weit", "ent\u00b7fer\u00b7net", ",", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "So bald der K\u00f6rper sich verkriecht,", "tokens": ["So", "bald", "der", "K\u00f6r\u00b7per", "sich", "ver\u00b7kriecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Sieht man wie auch der Schatten fliegt,", "tokens": ["Sieht", "man", "wie", "auch", "der", "Schat\u00b7ten", "fliegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KOKOM", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Der sich im Augenblik zertrennet,", "tokens": ["Der", "sich", "im", "Au\u00b7gen\u00b7blik", "zer\u00b7tren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "So bald der Mensch ist weggerennt.", "tokens": ["So", "bald", "der", "Mensch", "ist", "weg\u00b7ge\u00b7rennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Die Ehre Pracht und \u00e4usre Zier", "tokens": ["Die", "Eh\u00b7re", "Pracht", "und", "\u00e4us\u00b7re", "Zier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Kommt mir wie Dunst und Schatten f\u00fcr,", "tokens": ["Kommt", "mir", "wie", "Dunst", "und", "Schat\u00b7ten", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KOKOM", "NN", "KON", "NN", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Kaum sterben wir, bei dem Erbleichen,", "tokens": ["Kaum", "ster\u00b7ben", "wir", ",", "bei", "dem", "Er\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.56": {"text": "Sicht man die Ehre von uns weichen.", "tokens": ["Sicht", "man", "die", "Eh\u00b7re", "von", "uns", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.57": {"text": "Da unser Thun der ganzen Welt,", "tokens": ["Da", "un\u00b7ser", "Thun", "der", "gan\u00b7zen", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.58": {"text": "So lang wir leben woll gef\u00e4llt;", "tokens": ["So", "lang", "wir", "le\u00b7ben", "woll", "ge\u00b7f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.59": {"text": "So ist doch in die d\u00fcstren Bogen", "tokens": ["So", "ist", "doch", "in", "die", "d\u00fcst\u00b7ren", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.60": {"text": "Des Grabes, aller Ruhm gezogen:", "tokens": ["Des", "Gra\u00b7bes", ",", "al\u00b7ler", "Ruhm", "ge\u00b7zo\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.61": {"text": "Wer jetzo nichts als loben kan,", "tokens": ["Wer", "jet\u00b7zo", "nichts", "als", "lo\u00b7ben", "kan", ","], "token_info": ["word", "word", "word", "word", "wor