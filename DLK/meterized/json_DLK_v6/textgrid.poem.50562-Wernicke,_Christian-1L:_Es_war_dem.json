{"textgrid.poem.50562": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es war dem ", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war dem ", "tokens": ["Es", "war", "dem"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Dass ", "tokens": ["Dass"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Als ein ", "tokens": ["Als", "ein"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Der ein Gewerb vom Printzen brachte,", "tokens": ["Der", "ein", "Ge\u00b7werb", "vom", "Print\u00b7zen", "brach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So fragt' er ihn: Wie kommt's, dass ", "tokens": ["So", "fragt'", "er", "ihn", ":", "Wie", "kommt's", ",", "dass"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$.", "PWAV", "VVFIN", "$,", "KOUS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So ", "tokens": ["So"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Es m\u00f6cht', antwortet er, sich neigend biss zur Erden,", "tokens": ["Es", "m\u00f6cht'", ",", "ant\u00b7wor\u00b7tet", "er", ",", "sich", "nei\u00b7gend", "biss", "zur", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "VVFIN", "PPER", "$,", "PRF", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mein Herr auch gern' als ihr, ", "tokens": ["Mein", "Herr", "auch", "gern'", "als", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "KOUS", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}