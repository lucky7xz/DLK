{"textgrid.poem.30671": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "85.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Liebste lebt in dir; nun ist sie dir gestorben;", "tokens": ["Die", "Liebs\u00b7te", "lebt", "in", "dir", ";", "nun", "ist", "sie", "dir", "ge\u00b7stor\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$.", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein andrer hat sie ihm zum Leben auch erworben.", "tokens": ["Ein", "an\u00b7drer", "hat", "sie", "ihm", "zum", "Le\u00b7ben", "auch", "er\u00b7wor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PPER", "PPER", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}