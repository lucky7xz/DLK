{"textgrid.poem.62856": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Amynth, der sich mit W\u00fcnschen qu\u00e4lte,", "genre": "verse", "period": "N.A.", "pub_year": 1781, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Amynth, der sich mit W\u00fcnschen qu\u00e4lte,", "tokens": ["A\u00b7mynth", ",", "der", "sich", "mit", "W\u00fcn\u00b7schen", "qu\u00e4l\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weil ihm zu seinem Zeitvertreib", "tokens": ["Weil", "ihm", "zu", "sei\u00b7nem", "Zeit\u00b7ver\u00b7treib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Regiment, ein sch\u00f6nes Weib", "tokens": ["Ein", "Re\u00b7gi\u00b7ment", ",", "ein", "sch\u00f6\u00b7nes", "Weib"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und eine Tonne Golds noch fehlte,", "tokens": ["Und", "ei\u00b7ne", "Ton\u00b7ne", "Golds", "noch", "fehl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Lag einst bey d\u00fcsterm Sternenlicht", "tokens": ["Lag", "einst", "bey", "d\u00fcs\u00b7term", "Ster\u00b7nen\u00b7licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sann auf gl\u00e4nzende Projecte,", "tokens": ["Und", "sann", "auf", "gl\u00e4n\u00b7zen\u00b7de", "Pro\u00b7jec\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als ihn ein mystisches Gesicht", "tokens": ["Als", "ihn", "ein", "mys\u00b7ti\u00b7sches", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Aus seinen Phantasien weckte.", "tokens": ["Aus", "sei\u00b7nen", "Phan\u00b7ta\u00b7si\u00b7en", "weck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text": "Ihm rief ein sanfter Ton: Amynth!", "tokens": ["Ihm", "rief", "ein", "sanf\u00b7ter", "Ton", ":", "A\u00b7mynth", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$.", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und als er aus dem Bette blickte,", "tokens": ["Und", "als", "er", "aus", "dem", "Bet\u00b7te", "blick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Sah er das sch\u00f6nste G\u00f6tterkind,", "tokens": ["Sah", "er", "das", "sch\u00f6ns\u00b7te", "G\u00f6t\u00b7ter\u00b7kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Das je des Barden Aug entz\u00fcckte.", "tokens": ["Das", "je", "des", "Bar\u00b7den", "Aug", "ent\u00b7z\u00fcck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Sie hielt in ihrer rechten Hand", "tokens": ["Sie", "hielt", "in", "ih\u00b7rer", "rech\u00b7ten", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Ein F\u00fcllhorn und ein Ordensband", "tokens": ["Ein", "F\u00fcll\u00b7horn", "und", "ein", "Or\u00b7dens\u00b7band"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und schien ihm freundlich mit der Linken", "tokens": ["Und", "schien", "ihm", "freund\u00b7lich", "mit", "der", "Lin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "An ihre wei\u00dfe Brust zu winken.", "tokens": ["An", "ih\u00b7re", "wei\u00b7\u00dfe", "Brust", "zu", "win\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Wer bist du? rief er, durch die Macht", "tokens": ["Wer", "bist", "du", "?", "rief", "er", ",", "durch", "die", "Macht"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "$.", "VVFIN", "PPER", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Des Winks von seiner Angst entbunden.", "tokens": ["Des", "Winks", "von", "sei\u00b7ner", "Angst", "ent\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Allein, itzt schlug es Mitternacht,", "tokens": ["Al\u00b7lein", ",", "itzt", "schlug", "es", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und schnell war die Gestalt verschwunden.", "tokens": ["Und", "schnell", "war", "die", "Ge\u00b7stalt", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Allm\u00e4hlig, wie der Tag, erwacht", "tokens": ["All\u00b7m\u00e4h\u00b7lig", ",", "wie", "der", "Tag", ",", "er\u00b7wacht"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Amynth und trocknet sich die Stirne.", "tokens": ["A\u00b7mynth", "und", "trock\u00b7net", "sich", "die", "Stir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Wo bist du, rief er, holde Dirne?", "tokens": ["Wo", "bist", "du", ",", "rief", "er", ",", "hol\u00b7de", "Dir\u00b7ne", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Schuf Morpheus dich zu meiner Quaal?", "tokens": ["Schuf", "Mor\u00b7pheus", "dich", "zu", "mei\u00b7ner", "Qua\u00b7al", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Doch nein \u2013 ein leeres Ideal", "tokens": ["Doch", "nein", "\u2013", "ein", "lee\u00b7res", "I\u00b7deal"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PTKANT", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Kann keine solche Glut entz\u00fcnden.", "tokens": ["Kann", "kei\u00b7ne", "sol\u00b7che", "Glut", "ent\u00b7z\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Wohlan, mir bleibt nur eine Wahl,", "tokens": ["Wo\u00b7hlan", ",", "mir", "bleibt", "nur", "ei\u00b7ne", "Wahl", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Zu sterben oder dich zu finden.", "tokens": ["Zu", "ster\u00b7ben", "o\u00b7der", "dich", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Er sattelt seinen Bucephal,", "tokens": ["Er", "sat\u00b7telt", "sei\u00b7nen", "Bu\u00b7ce\u00b7phal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Trabt r\u00fcstig \u00fcber Berg und Thal,", "tokens": ["Trabt", "r\u00fcs\u00b7tig", "\u00fc\u00b7ber", "Berg", "und", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Und kurz, nachdem er einen Blinden", "tokens": ["Und", "kurz", ",", "nach\u00b7dem", "er", "ei\u00b7nen", "Blin\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Und ein paar Kinder umgerannt,", "tokens": ["Und", "ein", "paar", "Kin\u00b7der", "um\u00b7ge\u00b7rannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Warf ihn der wilde Gaul vom R\u00fccken", "tokens": ["Warf", "ihn", "der", "wil\u00b7de", "Gaul", "vom", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Und floh, Gott wei\u00df in welches Land.", "tokens": ["Und", "floh", ",", "Gott", "wei\u00df", "in", "wel\u00b7ches", "Land", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "NN", "VVFIN", "APPR", "PWAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Ein Held mu\u00df sich in alles schicken;", "tokens": ["Ein", "Held", "mu\u00df", "sich", "in", "al\u00b7les", "schi\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Er setzt zu Fu\u00df die Reise fort", "tokens": ["Er", "setzt", "zu", "Fu\u00df", "die", "Rei\u00b7se", "fort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Und glaubt entz\u00fcckt, bald hie, bald dort,", "tokens": ["Und", "glaubt", "ent\u00b7z\u00fcckt", ",", "bald", "hie", ",", "bald", "dort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVPP", "$,", "ADV", "ADV", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Den trauten Irrwisch zu erblicken;", "tokens": ["Den", "trau\u00b7ten", "Irr\u00b7wisch", "zu", "er\u00b7bli\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Doch wenn er ihm entgegenl\u00e4uft,", "tokens": ["Doch", "wenn", "er", "ihm", "ent\u00b7ge\u00b7gen\u00b7l\u00e4uft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Zerflie\u00dft er, wie die Seifenblase,", "tokens": ["Zer\u00b7flie\u00dft", "er", ",", "wie", "die", "Sei\u00b7fen\u00b7bla\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Nach der ein rascher Knabe greift.", "tokens": ["Nach", "der", "ein", "ra\u00b7scher", "Kna\u00b7be", "greift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.42": {"text": "Itzt winkt ihm eine stille Stra\u00dfe", "tokens": ["Itzt", "winkt", "ihm", "ei\u00b7ne", "stil\u00b7le", "Stra\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "In einen dunkeln Palmenhayn;", "tokens": ["In", "ei\u00b7nen", "dun\u00b7keln", "Pal\u00b7men\u00b7hayn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Hier schlummert er im weichen Grase", "tokens": ["Hier", "schlum\u00b7mert", "er", "im", "wei\u00b7chen", "Gra\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Vom langen Zug ermattet ein,", "tokens": ["Vom", "lan\u00b7gen", "Zug", "er\u00b7mat\u00b7tet", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Und sieht die fl\u00fcchtige Sylphide", "tokens": ["Und", "sieht", "die", "fl\u00fcch\u00b7ti\u00b7ge", "Syl\u00b7phi\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "In einem silbernen Talar", "tokens": ["In", "ei\u00b7nem", "sil\u00b7ber\u00b7nen", "Ta\u00b7lar"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Mit wei\u00dfen Rosen in dem Haar;", "tokens": ["Mit", "wei\u00b7\u00dfen", "Ro\u00b7sen", "in", "dem", "Haar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Ihr Gru\u00df war ernste Huld und Friede.", "tokens": ["Ihr", "Gru\u00df", "war", "erns\u00b7te", "Huld", "und", "Frie\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "Er rafft sich auf, sie schwirrt davon;", "tokens": ["Er", "rafft", "sich", "auf", ",", "sie", "schwirrt", "da\u00b7von", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$,", "PPER", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Er jagt sie, wie Latonens Sohn", "tokens": ["Er", "jagt", "sie", ",", "wie", "La\u00b7to\u00b7nens", "Sohn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Einst Daphnen, \u00fcber Stock und Stengel;", "tokens": ["Einst", "Daph\u00b7nen", ",", "\u00fc\u00b7ber", "Stock", "und", "Sten\u00b7gel", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "Itzt hascht er sie; doch schnell zerrinnt", "tokens": ["Itzt", "hascht", "er", "sie", ";", "doch", "schnell", "zer\u00b7rinnt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$.", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Das holde Traumbild, und Amynth \u2013", "tokens": ["Das", "hol\u00b7de", "Traum\u00b7bild", ",", "und", "A\u00b7mynth", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "NE", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.55": {"text": "Umarmet einen Todesengel.", "tokens": ["Um\u00b7ar\u00b7met", "ei\u00b7nen", "To\u00b7de\u00b7sen\u00b7gel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}