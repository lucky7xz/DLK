{"textgrid.poem.61487": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hei\u00dft dein Herz dich Gutes tun,", "tokens": ["Hei\u00dft", "dein", "Herz", "dich", "Gu\u00b7tes", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tu es rein um deinetwillen;", "tokens": ["Tu", "es", "rein", "um", "dei\u00b7net\u00b7wil\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4\u00dft das Sch\u00f6ne dich nicht ruhn,", "tokens": ["L\u00e4\u00dft", "das", "Sch\u00f6\u00b7ne", "dich", "nicht", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bild' es, deinen Trieb zu stillen;", "tokens": ["Bild'", "es", ",", "dei\u00b7nen", "Trieb", "zu", "stil\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch das lasse dich ungeirrt,", "tokens": ["Doch", "das", "las\u00b7se", "dich", "un\u00b7ge\u00b7irrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Was die Welt dazu sagen wird.", "tokens": ["Was", "die", "Welt", "da\u00b7zu", "sa\u00b7gen", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PAV", "VVINF", "VAFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}