{"textgrid.poem.54748": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "62. Auf den Buscus", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der karge ", "tokens": ["Der", "kar\u00b7ge"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Und Darr und Lungensucht schreckt mit der schwartzen Baar/", "tokens": ["Und", "Darr", "und", "Lun\u00b7gen\u00b7sucht", "schreckt", "mit", "der", "schwart\u00b7zen", "Baar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "KON", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Entsetzt sich vor dem Tod/ nicht da\u00df er w\u00fcndscht zu leben:", "tokens": ["Ent\u00b7setzt", "sich", "vor", "dem", "Tod", "/", "nicht", "da\u00df", "er", "w\u00fcnd\u00b7scht", "zu", "le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$(", "PTKNEG", "KOUS", "PPER", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Nur da\u00df er vor ein Grab soll virtzig Creutzer geben.", "tokens": ["Nur", "da\u00df", "er", "vor", "ein", "Grab", "soll", "virt\u00b7zig", "Creut\u00b7zer", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPR", "ART", "NN", "VMFIN", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}