{"textgrid.poem.43863": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was ich in Gedancken k\u00fc\u00dfe,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ich in Gedancken k\u00fc\u00dfe,", "tokens": ["Was", "ich", "in", "Ge\u00b7dan\u00b7cken", "k\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Macht mir M\u00fch und Leben s\u00fc\u00dfe", "tokens": ["Macht", "mir", "M\u00fch", "und", "Le\u00b7ben", "s\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und vertreibt so Gram als Zeit;", "tokens": ["Und", "ver\u00b7treibt", "so", "Gram", "als", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NE", "KOUS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Niemand soll es auch erfahren,", "tokens": ["Nie\u00b7mand", "soll", "es", "auch", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Niemand will ich's ofenbahren", "tokens": ["Nie\u00b7mand", "will", "ich's", "o\u00b7fen\u00b7bah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Als der stummen Einsamkeit.", "tokens": ["Als", "der", "stum\u00b7men", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ob ich gleich nun, sch\u00f6ne Seele,", "tokens": ["Ob", "ich", "gleich", "nun", ",", "sch\u00f6\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nahmen, Brand und Schmerz verheele,", "tokens": ["Nah\u00b7men", ",", "Brand", "und", "Schmerz", "ver\u00b7hee\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcrd es doch mein Gl\u00fccke seyn,", "tokens": ["W\u00fcrd", "es", "doch", "mein", "Gl\u00fc\u00b7cke", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPOSAT", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn du selbst errathen solltest", "tokens": ["Wenn", "du", "selbst", "er\u00b7ra\u00b7then", "soll\u00b7test"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und nur einmahl forschen wolltest,", "tokens": ["Und", "nur", "ein\u00b7mahl", "for\u00b7schen", "woll\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wem sich meine Flammen weihn.", "tokens": ["Wem", "sich", "mei\u00b7ne", "Flam\u00b7men", "weihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Merckstu nichts aus Wort und Blicken,", "tokens": ["Mercks\u00b7tu", "nichts", "aus", "Wort", "und", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die viel Sehnsuchtszeichen schicken?", "tokens": ["Die", "viel", "Sehn\u00b7suchts\u00b7zei\u00b7chen", "schi\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Siehstu mir kein Feuer an,", "tokens": ["Sieh\u00b7stu", "mir", "kein", "Feu\u00b7er", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wenn mein z\u00e4rtliches Gem\u00fcthe", "tokens": ["Wenn", "mein", "z\u00e4rt\u00b7li\u00b7ches", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bey der Wallung im Gebl\u00fcte", "tokens": ["Bey", "der", "Wal\u00b7lung", "im", "Ge\u00b7bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Diesen Trieb nicht bergen kan?", "tokens": ["Die\u00b7sen", "Trieb", "nicht", "ber\u00b7gen", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Freylich mach ich \u00f6fters Grillen,", "tokens": ["Frey\u00b7lich", "mach", "ich", "\u00f6f\u00b7ters", "Gril\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber alles doch im Stillen", "tokens": ["A\u00b7ber", "al\u00b7les", "doch", "im", "Stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dabey nicht ohne Lust,", "tokens": ["Und", "da\u00b7bey", "nicht", "oh\u00b7ne", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "PTKNEG", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil du allzeit meine Sinnen", "tokens": ["Weil", "du", "all\u00b7zeit", "mei\u00b7ne", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch dein artiges Beginnen", "tokens": ["Durch", "dein", "ar\u00b7ti\u00b7ges", "Be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auch entfernt erg\u00f6zen must.", "tokens": ["Auch", "ent\u00b7fernt", "er\u00b7g\u00f6\u00b7zen", "must", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Will ich mich gleich selber zwingen,", "tokens": ["Will", "ich", "mich", "gleich", "sel\u00b7ber", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein Ged\u00e4chtn\u00fc\u00df wegzubringen,", "tokens": ["Dein", "Ge\u00b7d\u00e4cht\u00b7n\u00fc\u00df", "weg\u00b7zu\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchl ich in mir Widerstand;", "tokens": ["F\u00fchl", "ich", "in", "mir", "Wi\u00b7der\u00b7stand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn ich glaube, dich zu lieben,", "tokens": ["Denn", "ich", "glau\u00b7be", ",", "dich", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "War mir schon ins Blut geschrieben,", "tokens": ["War", "mir", "schon", "ins", "Blut", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eh ich noch die Wiege fand.", "tokens": ["Eh", "ich", "noch", "die", "Wie\u00b7ge", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch was hilft ins Blut geschrieben,", "tokens": ["Doch", "was", "hilft", "ins", "Blut", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn mir dies getreue Lieben", "tokens": ["Wenn", "mir", "dies", "ge\u00b7treu\u00b7e", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weder Frucht noch Hofnung zieht?", "tokens": ["We\u00b7der", "Frucht", "noch", "Hof\u00b7nung", "zieht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Krancke m\u00f6gen sich beklagen,", "tokens": ["Kran\u00b7cke", "m\u00f6\u00b7gen", "sich", "be\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur mein Herz soll garnichts sagen,", "tokens": ["Nur", "mein", "Herz", "soll", "gar\u00b7nichts", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ob es noch so heftig gl\u00fcht.", "tokens": ["Ob", "es", "noch", "so", "hef\u00b7tig", "gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "O du ungemeines Leiden,", "tokens": ["O", "du", "un\u00b7ge\u00b7mei\u00b7nes", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6ne Fr\u00fcchte sehn und meiden", "tokens": ["Sch\u00f6\u00b7ne", "Fr\u00fcch\u00b7te", "sehn", "und", "mei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVINF", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und bey Quellen d\u00fcrsten stehn!", "tokens": ["Und", "bey", "Quel\u00b7len", "d\u00fcrs\u00b7ten", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn die Hauptperson nur w\u00fcste,", "tokens": ["Wenn", "die", "Haupt\u00b7per\u00b7son", "nur", "w\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was vor Seufzer sanfter L\u00fcste", "tokens": ["Was", "vor", "Seuf\u00b7zer", "sanf\u00b7ter", "L\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihrer Sch\u00f6nheit opfern gehn!", "tokens": ["Ih\u00b7rer", "Sch\u00f6n\u00b7heit", "op\u00b7fern", "gehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch du ungemeines Leiden", "tokens": ["Doch", "du", "un\u00b7ge\u00b7mei\u00b7nes", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bist auch warlich zu beneiden,", "tokens": ["Bist", "auch", "war\u00b7lich", "zu", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil dich die Person erweckt,", "tokens": ["Weil", "dich", "die", "Per\u00b7son", "er\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Die vom Sch\u00f6nsten auf der Erden", "tokens": ["Die", "vom", "Sch\u00f6ns\u00b7ten", "auf", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Selbst verdient geehrt zu werden", "tokens": ["Selbst", "ver\u00b7dient", "ge\u00b7ehrt", "zu", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP", "PTKZU", "VAINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und schon manches angesteckt.", "tokens": ["Und", "schon", "man\u00b7ches", "an\u00b7ge\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Durch ein ehrerbietig Schweigen", "tokens": ["Durch", "ein", "ehr\u00b7er\u00b7bie\u00b7tig", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will ich mich gela\u00dfen zeigen,", "tokens": ["Will", "ich", "mich", "ge\u00b7la\u00b7\u00dfen", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis vielleicht ein Tag erscheint,", "tokens": ["Bis", "viel\u00b7leicht", "ein", "Tag", "er\u00b7scheint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da die Flammen heller brennen", "tokens": ["Da", "die", "Flam\u00b7men", "hel\u00b7ler", "bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der Welt entdecken k\u00f6nnen,", "tokens": ["Und", "der", "Welt", "ent\u00b7de\u00b7cken", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "VMINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie ich es so treu gemeint.", "tokens": ["Wie", "ich", "es", "so", "treu", "ge\u00b7meint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Sollt auch dieser Wunsch betriegen,", "tokens": ["Sollt", "auch", "die\u00b7ser", "Wunsch", "be\u00b7trie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Find ich dennoch mein Vergn\u00fcgen", "tokens": ["Find", "ich", "den\u00b7noch", "mein", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die gr\u00f6ste Lust daran,", "tokens": ["Und", "die", "gr\u00f6s\u00b7te", "Lust", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich nach der klugen Lehre", "tokens": ["Da\u00df", "ich", "nach", "der", "klu\u00b7gen", "Leh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses Bild geheim verehre,", "tokens": ["Die\u00b7ses", "Bild", "ge\u00b7heim", "ver\u00b7eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was ich nicht besizen kan.", "tokens": ["Was", "ich", "nicht", "be\u00b7si\u00b7zen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}