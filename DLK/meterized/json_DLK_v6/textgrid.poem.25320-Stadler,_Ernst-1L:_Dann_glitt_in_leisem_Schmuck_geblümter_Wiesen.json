{"textgrid.poem.25320": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dann glitt in leisem Schmuck gebl\u00fcmter Wiesen", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dann glitt in leisem Schmuck gebl\u00fcmter Wiesen", "tokens": ["Dann", "glitt", "in", "lei\u00b7sem", "Schmuck", "ge\u00b7bl\u00fcm\u00b7ter", "Wie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "der Fr\u00fchling \u00fcbers Land rieselnd von Sonne", "tokens": ["der", "Fr\u00fch\u00b7ling", "\u00fc\u00b7bers", "Land", "rie\u00b7selnd", "von", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und schwer vom Sehnen fr\u00fcher Sternenn\u00e4chte.", "tokens": ["und", "schwer", "vom", "Seh\u00b7nen", "fr\u00fc\u00b7her", "Ster\u00b7nen\u00b7n\u00e4ch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Abend kam geh\u00fcllt in weiches Licht", "tokens": ["Ein", "A\u00b7bend", "kam", "ge\u00b7h\u00fcllt", "in", "wei\u00b7ches", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "beperlter B\u00fcsche. Matter Fr\u00fchlingsregen", "tokens": ["be\u00b7perl\u00b7ter", "B\u00fc\u00b7sche", ".", "Mat\u00b7ter", "Fr\u00fch\u00b7lings\u00b7re\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "war sanft verronnen in den braunen D\u00e4mmer", "tokens": ["war", "sanft", "ver\u00b7ron\u00b7nen", "in", "den", "brau\u00b7nen", "D\u00e4m\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "der hinter den Zypressenst\u00e4mmen aufglomm.", "tokens": ["der", "hin\u00b7ter", "den", "Zyp\u00b7res\u00b7sen\u00b7st\u00e4m\u00b7men", "auf\u00b7glomm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Ich stand an dem Magnolienstrauch und sog", "tokens": ["Ich", "stand", "an", "dem", "Mag\u00b7no\u00b7li\u00b7en\u00b7strauch", "und", "sog"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "den starken Duft und schmiegte meine Lippen", "tokens": ["den", "star\u00b7ken", "Duft", "und", "schmieg\u00b7te", "mei\u00b7ne", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "tief in den warmen feuchten Flaum der Bl\u00fcten.", "tokens": ["tief", "in", "den", "war\u00b7men", "feuch\u00b7ten", "Flaum", "der", "Bl\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Er kam von hinten. Fa\u00dfte mich am Arm. Ich schrak", "tokens": ["Er", "kam", "von", "hin\u00b7ten", ".", "Fa\u00df\u00b7te", "mich", "am", "Arm", ".", "Ich", "schrak"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "$.", "VVFIN", "PPER", "APPRART", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "zusammen. Doch er war so sch\u00f6n", "tokens": ["zu\u00b7sam\u00b7men", ".", "Doch", "er", "war", "so", "sch\u00f6n"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKVZ", "$.", "KON", "PPER", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "wie er so dastand mit den hellen Augen", "tokens": ["wie", "er", "so", "das\u00b7tand", "mit", "den", "hel\u00b7len", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.11": {"text": "und ganz bestrahlt von Lust und Glanz der Bl\u00fcten.", "tokens": ["und", "ganz", "be\u00b7strahlt", "von", "Lust", "und", "Glanz", "der", "Bl\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "APPR", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wir gingen durch die leise laue Nacht.", "tokens": ["Wir", "gin\u00b7gen", "durch", "die", "lei\u00b7se", "lau\u00b7e", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und wie der fernen Brunnen Silberton", "tokens": ["Und", "wie", "der", "fer\u00b7nen", "Brun\u00b7nen", "Sil\u00b7ber\u00b7ton"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "fast nur aufbebte wie ein dunkler Zweig", "tokens": ["fast", "nur", "auf\u00b7beb\u00b7te", "wie", "ein", "dunk\u00b7ler", "Zweig"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "vom liebetrunknen Nachtwind anger\u00fchrt", "tokens": ["vom", "lie\u00b7be\u00b7trunk\u00b7nen", "Nacht\u00b7wind", "an\u00b7ge\u00b7r\u00fchrt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "und hie und da ein schwacher Laut der Lust", "tokens": ["und", "hie", "und", "da", "ein", "schwa\u00b7cher", "Laut", "der", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KON", "ADV", "ART", "ADJA", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "die Nacht durchwehte starben unsre Worte", "tokens": ["die", "Nacht", "durch\u00b7weh\u00b7te", "star\u00b7ben", "uns\u00b7re", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "und schweigend gingen wir und lauschten nur", "tokens": ["und", "schwei\u00b7gend", "gin\u00b7gen", "wir", "und", "lauschten", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "ged\u00e4mpftem Knirschen der zerknickten Halme", "tokens": ["ge\u00b7d\u00e4mpf\u00b7tem", "Knir\u00b7schen", "der", "zer\u00b7knick\u00b7ten", "Hal\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "und wie vom buschigen Ge\u00e4st gescheucht", "tokens": ["und", "wie", "vom", "bu\u00b7schi\u00b7gen", "Ge\u00b7\u00e4st", "ge\u00b7scheucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPRART", "ADJA", "NN", "VVPP"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "ein gro\u00dfer Vogel rauschend uns umstrich", "tokens": ["ein", "gro\u00b7\u00dfer", "Vo\u00b7gel", "rau\u00b7schend", "uns", "um\u00b7strich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "und gingen hin und fanden nicht ein Wort", "tokens": ["und", "gin\u00b7gen", "hin", "und", "fan\u00b7den", "nicht", "ein", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "zu sagen was in dieser Nacht erwuchs", "tokens": ["zu", "sa\u00b7gen", "was", "in", "die\u00b7ser", "Nacht", "er\u00b7wuchs"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "PRELS", "APPR", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "und heller strahlte als der hei\u00dfe Glanz", "tokens": ["und", "hel\u00b7ler", "strahl\u00b7te", "als", "der", "hei\u00b7\u00dfe", "Glanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "der von ergl\u00fchten Rosenb\u00fcschen flie\u00dft.", "tokens": ["der", "von", "er\u00b7gl\u00fch\u00b7ten", "Ro\u00b7sen\u00b7b\u00fc\u00b7schen", "flie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Das ist nun alles lang vorbei. Und war", "tokens": ["Das", "ist", "nun", "al\u00b7les", "lang", "vor\u00b7bei", ".", "Und", "war"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "PIS", "ADJD", "PTKVZ", "$.", "KON", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "so s\u00fc\u00df doch. Wenn von dunklem Sims ich leicht", "tokens": ["so", "s\u00fc\u00df", "doch", ".", "Wenn", "von", "dunk\u00b7lem", "Sims", "ich", "leicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "$.", "KOUS", "APPR", "ADJA", "NN", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "mich niederschwang und atmend stand und dann", "tokens": ["mich", "nie\u00b7der\u00b7schwang", "und", "at\u00b7mend", "stand", "und", "dann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "ADJD", "VVFIN", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "so hinlief und die warme Nachtluft mich", "tokens": ["so", "hin\u00b7lief", "und", "die", "war\u00b7me", "Nacht\u00b7luft", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "ART", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "zitternd umsp\u00fclte an gef\u00fcllten Beeten", "tokens": ["zit\u00b7ternd", "um\u00b7sp\u00fcl\u00b7te", "an", "ge\u00b7f\u00fcll\u00b7ten", "Bee\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.20": {"text": "vorbei und goldnen Brunnen durch den Glanz", "tokens": ["vor\u00b7bei", "und", "gold\u00b7nen", "Brun\u00b7nen", "durch", "den", "Glanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKVZ", "KON", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "der hellen Wiese zum Granatbaum der", "tokens": ["der", "hel\u00b7len", "Wie\u00b7se", "zum", "Gra\u00b7nat\u00b7baum", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "ART"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "mit Purpurarmen uns umgitterte \u2013", "tokens": ["mit", "Pur\u00b7pu\u00b7rar\u00b7men", "uns", "um\u00b7git\u00b7ter\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Leuchtend wie schwere goldne Ampeln hingen", "tokens": ["Leuch\u00b7tend", "wie", "schwe\u00b7re", "gold\u00b7ne", "Am\u00b7peln", "hin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ADJA", "ADJA", "NN", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.24": {"text": "die \u00c4pfel. Und in seiner Krone sangen", "tokens": ["die", "\u00c4p\u00b7fel", ".", "Und", "in", "sei\u00b7ner", "Kro\u00b7ne", "san\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "KON", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "zwei Nachtigallen. Leise zog ihr Lied", "tokens": ["zwei", "Nach\u00b7ti\u00b7gal\u00b7len", ".", "Lei\u00b7se", "zog", "ihr", "Lied"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "NN", "$.", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "durch fernster G\u00e4rten atemloses Dunkel", "tokens": ["durch", "ferns\u00b7ter", "G\u00e4r\u00b7ten", "a\u00b7tem\u00b7lo\u00b7ses", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "und wie verzaubert. Wenn ich so allein", "tokens": ["und", "wie", "ver\u00b7zau\u00b7bert", ".", "Wenn", "ich", "so", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VVPP", "$.", "KOUS", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "unter den \u00c4sten stand dann sickerte", "tokens": ["un\u00b7ter", "den", "\u00c4s\u00b7ten", "stand", "dann", "si\u00b7cker\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADV", "VVFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.29": {"text": "wie Bl\u00fctentau der Wohllaut auf mich nieder", "tokens": ["wie", "Bl\u00fc\u00b7ten\u00b7tau", "der", "Wohl\u00b7laut", "auf", "mich", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ART", "NN", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "und k\u00fcrzte mir die langen hei\u00dfen Stunden", "tokens": ["und", "k\u00fcrz\u00b7te", "mir", "die", "lan\u00b7gen", "hei\u00b7\u00dfen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "denn manchmal kam er sp\u00e4t. Und durch die B\u00fcsche", "tokens": ["denn", "manch\u00b7mal", "kam", "er", "sp\u00e4t", ".", "Und", "durch", "die", "B\u00fc\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "$.", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "wehte ein fremder Schauer der mich schreckte.", "tokens": ["weh\u00b7te", "ein", "frem\u00b7der", "Schau\u00b7er", "der", "mich", "schreck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Und einmal als die Sommernacht wie Gold", "tokens": ["Und", "ein\u00b7mal", "als", "die", "Som\u00b7mer\u00b7nacht", "wie", "Gold"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOUS", "ART", "NN", "KOKOM", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "zwischen den Zweigen hing und alle Blumen", "tokens": ["zwi\u00b7schen", "den", "Zwei\u00b7gen", "hing", "und", "al\u00b7le", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "wie Flammen in den roten Vollmond gl\u00fchten", "tokens": ["wie", "Flam\u00b7men", "in", "den", "ro\u00b7ten", "Voll\u00b7mond", "gl\u00fch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "hob er mich auf und trug mich hin ich schlang", "tokens": ["hob", "er", "mich", "auf", "und", "trug", "mich", "hin", "ich", "schlang"], "token_info": ["word", "word"