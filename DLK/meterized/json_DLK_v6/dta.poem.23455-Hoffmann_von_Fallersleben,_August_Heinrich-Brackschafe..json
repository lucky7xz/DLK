{"dta.poem.23455": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Brackschafe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O zeig's nicht erst durch's Band im Knopfloch,", "tokens": ["O", "zeig's", "nicht", "erst", "durch's", "Band", "im", "Kno\u00b7pfloch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "ADV", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ganze Welt wei\u00df was du bist:", "tokens": ["Die", "gan\u00b7ze", "Welt", "wei\u00df", "was", "du", "bist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Warum denn zeigst du armer Tropf noch,", "tokens": ["Wa\u00b7rum", "denn", "zeigst", "du", "ar\u00b7mer", "Tropf", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie billig deine Seele ist?", "tokens": ["Wie", "bil\u00b7lig", "dei\u00b7ne", "See\u00b7le", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch gut! so zeichnet man was Brack ist", "tokens": ["Doch", "gut", "!", "so", "zeich\u00b7net", "man", "was", "Brack", "ist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$.", "ADV", "VVFIN", "PIS", "PWS", "NN", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In allen gro\u00dfen Heerden aus;", "tokens": ["In", "al\u00b7len", "gro\u00b7\u00dfen", "Heer\u00b7den", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So lernen wir was Schranzenpack ist", "tokens": ["So", "ler\u00b7nen", "wir", "was", "Schran\u00b7zen\u00b7pack", "ist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "NN", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und reif zum gro\u00dfen V\u00f6lkerschmaus.", "tokens": ["Und", "reif", "zum", "gro\u00b7\u00dfen", "V\u00f6l\u00b7ker\u00b7schmaus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}