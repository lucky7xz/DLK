{"textgrid.poem.24153": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich f\u00fchle keinen Schmerz und bin doch krank;", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich f\u00fchle keinen Schmerz und bin doch krank;", "tokens": ["Ich", "f\u00fch\u00b7le", "kei\u00b7nen", "Schmerz", "und", "bin", "doch", "krank", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "KON", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mir ist die Kraft genommen, ich bin leer.", "tokens": ["Mir", "ist", "die", "Kraft", "ge\u00b7nom\u00b7men", ",", "ich", "bin", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich lebe ab, so wie ein Rad abl\u00e4uft,", "tokens": ["Ich", "le\u00b7be", "ab", ",", "so", "wie", "ein", "Rad", "ab\u00b7l\u00e4uft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ADV", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das von der Feder, die es trieb und hielt,", "tokens": ["Das", "von", "der", "Fe\u00b7der", ",", "die", "es", "trieb", "und", "hielt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gel\u00f6st ward. \u2013 Ach, sie pflegen mich so lieb,", "tokens": ["Ge\u00b7l\u00f6st", "ward", ".", "\u2013", "Ach", ",", "sie", "pfle\u00b7gen", "mich", "so", "lieb", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$.", "$(", "ITJ", "$,", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und dennoch wei\u00df ichs, balde ists vorbei.", "tokens": ["Und", "den\u00b7noch", "wei\u00df", "ichs", ",", "bal\u00b7de", "ists", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "$,", "ADV", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und bin nicht traurig. Ruhe wird mein Teil,", "tokens": ["Und", "bin", "nicht", "trau\u00b7rig", ".", "Ru\u00b7he", "wird", "mein", "Teil", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "ADJD", "$.", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ich werde ruhig bl\u00fchn in leichtem Wind,", "tokens": ["Ich", "wer\u00b7de", "ru\u00b7hig", "bl\u00fchn", "in", "leich\u00b7tem", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wie meine Blumen, die im Garten sind.", "tokens": ["Wie", "mei\u00b7ne", "Blu\u00b7men", ",", "die", "im", "Gar\u00b7ten", "sind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "PRELS", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}