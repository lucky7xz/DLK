{"textgrid.poem.43968": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach, liebstes Lenchen, s\u00e4hstu hier", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, liebstes Lenchen, s\u00e4hstu hier", "tokens": ["Ach", ",", "liebs\u00b7tes", "Len\u00b7chen", ",", "s\u00e4h\u00b7stu", "hier"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "ADJA", "NN", "$,", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Herz im Blute wallen,", "tokens": ["Mein", "Herz", "im", "Blu\u00b7te", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich weis vorwahr, es w\u00fcrde dir", "tokens": ["Ich", "weis", "vor\u00b7wahr", ",", "es", "w\u00fcr\u00b7de", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "VVFIN", "$,", "PPER", "VAFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sogar sein Schmerz gefallen;", "tokens": ["So\u00b7gar", "sein", "Schmerz", "ge\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Denn dieser kommt aus Z\u00e4rtligkeit", "tokens": ["Denn", "die\u00b7ser", "kommt", "aus", "Z\u00e4rt\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und aus der . . . Liebe,", "tokens": ["Und", "aus", "der", ".", ".", ".", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "punct"], "pos": ["KON", "APPR", "ART", "$.", "$.", "$.", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Womit ich mich bey . . . Zeit", "tokens": ["Wo\u00b7mit", "ich", "mich", "bey", ".", ".", ".", "Zeit"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word"], "pos": ["PWAV", "PPER", "PRF", "PTKVZ", "$.", "$.", "$.", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Um deine Qual betr\u00fcbe.", "tokens": ["Um", "dei\u00b7ne", "Qual", "be\u00b7tr\u00fc\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich soll dich in der . . . . .", "tokens": ["Ich", "soll", "dich", "in", "der", ".", ".", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "punct", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "ART", "$.", "$.", "$.", "$.", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Bey fremden Leuten la\u00dfen;", "tokens": ["Bey", "frem\u00b7den", "Leu\u00b7ten", "la\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Himmel sende doch die Zeit,", "tokens": ["Der", "Him\u00b7mel", "sen\u00b7de", "doch", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich gl\u00fccklich zu umfa\u00dfen.", "tokens": ["Dich", "gl\u00fcck\u00b7lich", "zu", "um\u00b7fa\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die andern, so mit mir studirt,", "tokens": ["Die", "an\u00b7dern", ",", "so", "mit", "mir", "stu\u00b7dirt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erlangen Ruhm und Gl\u00fccke,", "tokens": ["Er\u00b7lan\u00b7gen", "Ruhm", "und", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und ob mir eben das gebierth,", "tokens": ["Und", "ob", "mir", "e\u00b7ben", "das", "ge\u00b7bier\u00b7th", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So bleib ich weit zur\u00fccke.", "tokens": ["So", "bleib", "ich", "weit", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es thut mir deinetwegen weh", "tokens": ["Es", "thut", "mir", "dei\u00b7net\u00b7we\u00b7gen", "weh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und macht mich ganz zu Schanden,", "tokens": ["Und", "macht", "mich", "ganz", "zu", "Schan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Soda\u00df ich wie im Traume geh,", "tokens": ["So\u00b7da\u00df", "ich", "wie", "im", "Trau\u00b7me", "geh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOKOM", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Scherz ist mehr vorhanden.", "tokens": ["Kein", "Scherz", "ist", "mehr", "vor\u00b7han\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Feuer l\u00e4st in Adern nach,", "tokens": ["Das", "Feu\u00b7er", "l\u00e4st", "in", "A\u00b7dern", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": ". . . . Muth und Kr\u00e4fte schwinden,", "tokens": [".", ".", ".", ".", "Muth", "und", "Kr\u00e4f\u00b7te", "schwin\u00b7den", ","], "token_info": ["punct", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$.", "$.", "$.", "$.", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Da Laster, . . . . . Schmach", "tokens": ["Da", "Las\u00b7ter", ",", ".", ".", ".", ".", ".", "Schmach"], "token_info": ["word", "word", "punct", "punct", "punct", "punct", "punct", "punct", "word"], "pos": ["KOUS", "NN", "$,", "$.", "$.", "$.", "$.", "$.", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Mich allenthalben binden.", "tokens": ["Mich", "al\u00b7len\u00b7thal\u00b7ben", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Was that dein liebstes Herze nicht", "tokens": ["Was", "that", "dein", "liebs\u00b7tes", "Her\u00b7ze", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPOSAT", "ADJA", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um meinetwillen tragen,", "tokens": ["Um", "mei\u00b7net\u00b7wil\u00b7len", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": ". . . . . . . . . . . . . . mir geschicht,", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "mir", "ge\u00b7schicht", ","], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "word", "word", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "PPER", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": ". . . . . . . . . . . . Plagen.", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "Pla\u00b7gen", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "word", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Was hab ich dir nicht vor Gefahr", "tokens": ["Was", "hab", "ich", "dir", "nicht", "vor", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "PPER", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Zedliz aufgeladen,", "tokens": ["In", "Zed\u00b7liz", "auf\u00b7ge\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Allwo dein . . . . Labsahl war,", "tokens": ["All\u00b7wo", "dein", ".", ".", ".", ".", "Lab\u00b7sahl", "war", ","], "token_info": ["word", "word", "punct", "punct", "punct", "punct", "word", "word", "punct"], "pos"