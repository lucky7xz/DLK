{"dta.poem.8871": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Iv.  \n  Shakespear in seinen Sonetten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.71", "af:0.28"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Denn wer, wie du, vermag so tief zu dringen", "tokens": ["Denn", "wer", ",", "wie", "du", ",", "ver\u00b7mag", "so", "tief", "zu", "drin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "$,", "PWAV", "PPER", "$,", "VVFIN", "ADV", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In's tiefste Herz? Wenn du beginnst zu singen,", "tokens": ["In's", "tiefs\u00b7te", "Herz", "?", "Wenn", "du", "be\u00b7ginnst", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "KOUS", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verstummen wir als kl\u00e4gliche Verstummer.", "tokens": ["Ver\u00b7stum\u00b7men", "wir", "als", "kl\u00e4g\u00b7li\u00b7che", "Ver\u00b7stum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nicht M\u00e4dchenlaunen st\u00f6rten deinen Schlummer,", "tokens": ["Nicht", "M\u00e4d\u00b7chen\u00b7lau\u00b7nen", "st\u00f6r\u00b7ten", "dei\u00b7nen", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch stets um Freundschaft sehn wir warm dich ringen:", "tokens": ["Doch", "stets", "um", "Freund\u00b7schaft", "sehn", "wir", "warm", "dich", "rin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVFIN", "PPER", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein Freund errettet dich aus Weiberschlingen,", "tokens": ["Dein", "Freund", "er\u00b7ret\u00b7tet", "dich", "aus", "Wei\u00b7ber\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und seine Sch\u00f6nheit ist dein Ruhm und Kummer.", "tokens": ["Und", "sei\u00b7ne", "Sch\u00f6n\u00b7heit", "ist", "dein", "Ruhm", "und", "Kum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Bis auf die Sorgen, die f\u00fcr ihn dich nagen,", "tokens": ["Bis", "auf", "die", "Sor\u00b7gen", ",", "die", "f\u00fcr", "ihn", "dich", "na\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erhebst du Alles zur Apotheose,", "tokens": ["Er\u00b7hebst", "du", "Al\u00b7les", "zur", "A\u00b7pot\u00b7he\u00b7o\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis auf den Schmerz, den er dich l\u00e4\u00dft ertragen!", "tokens": ["Bis", "auf", "den", "Schmerz", ",", "den", "er", "dich", "l\u00e4\u00dft", "er\u00b7tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "PRF", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie sehr dich kr\u00e4nken mag der Seelenlose,", "tokens": ["Wie", "sehr", "dich", "kr\u00e4n\u00b7ken", "mag", "der", "See\u00b7len\u00b7lo\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "VVINF", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du l\u00e4ssest nie von ihm, und siehst mit Klagen", "tokens": ["Du", "l\u00e4s\u00b7sest", "nie", "von", "ihm", ",", "und", "siehst", "mit", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "$,", "KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Wurm des Lasters in der sch\u00f6nsten Rose.", "tokens": ["Den", "Wurm", "des", "Las\u00b7ters", "in", "der", "sch\u00f6ns\u00b7ten", "Ro\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}