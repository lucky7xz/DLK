{"textgrid.poem.42496": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: M\u00fcrrisch zeigt ein grau Gewitter", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00fcrrisch zeigt ein grau Gewitter", "tokens": ["M\u00fcr\u00b7risch", "zeigt", "ein", "grau", "Ge\u00b7wit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine finstre Stirn im S\u00fcden.", "tokens": ["Sei\u00b7ne", "finst\u00b7re", "Stirn", "im", "S\u00fc\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "An der Himmelsmaske lauert", "tokens": ["An", "der", "Him\u00b7mels\u00b7mas\u00b7ke", "lau\u00b7ert"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00fcstern l\u00e4ngst zum Sprung der Blitz.", "tokens": ["L\u00fcs\u00b7tern", "l\u00e4ngst", "zum", "Sprung", "der", "Blitz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie die Schlacht, die meilenferne,", "tokens": ["Wie", "die", "Schlacht", ",", "die", "mei\u00b7len\u00b7fer\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dumpf ununterbrochen donnert,", "tokens": ["Dumpf", "un\u00b7un\u00b7ter\u00b7bro\u00b7chen", "don\u00b7nert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich dann drohend langsam n\u00e4hert,", "tokens": ["Sich", "dann", "dro\u00b7hend", "lang\u00b7sam", "n\u00e4\u00b7hert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Rollt das schwere Wetter an.", "tokens": ["Rollt", "das", "schwe\u00b7re", "Wet\u00b7ter", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Eine kleine liebe Schwalbe,", "tokens": ["Ei\u00b7ne", "klei\u00b7ne", "lie\u00b7be", "Schwal\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die sich schon ins Nest gefl\u00fcchtet,", "tokens": ["Die", "sich", "schon", "ins", "Nest", "ge\u00b7fl\u00fcch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Steckt noch einmal sehr f\u00fcrwitzig", "tokens": ["Steckt", "noch", "ein\u00b7mal", "sehr", "f\u00fcr\u00b7wit\u00b7zig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "CARD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus dem Schlupf das K\u00f6pfchen vor.", "tokens": ["Aus", "dem", "Schlupf", "das", "K\u00f6pf\u00b7chen", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbund ich wag es: In die L\u00fcfte", "tokens": ["\u00bb", "und", "ich", "wag", "es", ":", "In", "die", "L\u00fcf\u00b7te"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "KON", "PPER", "VVFIN", "PPER", "$.", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwing ich mich, was kann das geben,", "tokens": ["Schwing", "ich", "mich", ",", "was", "kann", "das", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "PWS", "VMFIN", "PDS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schneller flieg ich als der Sturmwind,", "tokens": ["Schnel\u00b7ler", "flieg", "ich", "als", "der", "Sturm\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KOUS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schneller als der schnellste Blitz!\u00ab", "tokens": ["Schnel\u00b7ler", "als", "der", "schnells\u00b7te", "Blitz", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "KOKOM", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Kleine Schwalbe, la\u00df dich warnen,", "tokens": ["Klei\u00b7ne", "Schwal\u00b7be", ",", "la\u00df", "dich", "war\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hagel st\u00f6\u00dft dir das Gefieder;", "tokens": ["Ha\u00b7gel", "st\u00f6\u00dft", "dir", "das", "Ge\u00b7fie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bleibe unter deinem Giebel,", "tokens": ["Blei\u00b7be", "un\u00b7ter", "dei\u00b7nem", "Gie\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcbermut tut selten gut.", "tokens": ["\u00dc\u00b7ber\u00b7mut", "tut", "sel\u00b7ten", "gut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch mit lautem Zwitschern schie\u00dft sie", "tokens": ["Doch", "mit", "lau\u00b7tem", "Zwit\u00b7schern", "schie\u00dft", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die H\u00f6he, immer h\u00f6her,", "tokens": ["In", "die", "H\u00f6\u00b7he", ",", "im\u00b7mer", "h\u00f6\u00b7her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kreist und steigt und schwenkt und hebt sich,", "tokens": ["Kreist", "und", "steigt", "und", "schwenkt", "und", "hebt", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "PRF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tummelt sich nach Herzenslust.", "tokens": ["Tum\u00b7melt", "sich", "nach", "Her\u00b7zens\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und sie schl\u00e4gt den flinken Fl\u00fcgel", "tokens": ["Und", "sie", "schl\u00e4gt", "den", "flin\u00b7ken", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spottend an die schwarze Wolke.", "tokens": ["Spot\u00b7tend", "an", "die", "schwar\u00b7ze", "Wol\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbwollen um die Wette fliegen,", "tokens": ["\u00bb", "wol\u00b7len", "um", "die", "Wet\u00b7te", "flie\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Komm heraus, du Blendeblitz!\u00ab", "tokens": ["Komm", "he\u00b7raus", ",", "du", "Blen\u00b7de\u00b7blitz", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Kleine Schwalbe, la\u00df dich warnen,", "tokens": ["Klei\u00b7ne", "Schwal\u00b7be", ",", "la\u00df", "dich", "war\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df zum letztenmal dich warnen;", "tokens": ["La\u00df", "zum", "letz\u00b7ten\u00b7mal", "dich", "war\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPRART", "ADV", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Siehst du nicht das blaue Feuer,", "tokens": ["Siehst", "du", "nicht", "das", "blau\u00b7e", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4misch \u00e4ugt es hinterm Spalt.", "tokens": ["H\u00e4\u00b7misch", "\u00e4ugt", "es", "hin\u00b7term", "Spalt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "\u00bbkomm heraus, du H\u00e4userz\u00fcnder,", "tokens": ["\u00bb", "komm", "he\u00b7raus", ",", "du", "H\u00e4u\u00b7ser\u00b7z\u00fcn\u00b7der", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur hervor, du Wolkenf\u00e4rber,", "tokens": ["Nur", "her\u00b7vor", ",", "du", "Wol\u00b7ken\u00b7f\u00e4r\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Immerzu, du rasche Kerze!", "tokens": ["Im\u00b7mer\u00b7zu", ",", "du", "ra\u00b7sche", "Ker\u00b7ze", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gilt die Wette, schlag ich dich.", "tokens": ["Gilt", "die", "Wet\u00b7te", ",", "schlag", "ich", "dich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Lassen wir uns niederfallen,", "tokens": ["Las\u00b7sen", "wir", "uns", "nie\u00b7der\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eins, zwei, drei, wie Steine sinken;", "tokens": ["Eins", ",", "zwei", ",", "drei", ",", "wie", "Stei\u00b7ne", "sin\u00b7ken", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "CARD", "$,", "CARD", "$,", "PWAV", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit Jubel hat gewonnen,", "tokens": ["Und", "mit", "Ju\u00b7bel", "hat", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer zuerst die Erde k\u00fc\u00dft.", "tokens": ["Wer", "zu\u00b7erst", "die", "Er\u00b7de", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Nun, ich merke, Regenpf\u00f6rtner,", "tokens": ["Nun", ",", "ich", "mer\u00b7ke", ",", "Re\u00b7gen\u00b7pf\u00f6rt\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Menschenschrecker, Eichenspeller,", "tokens": ["Men\u00b7schen\u00b7schre\u00b7cker", ",", "Ei\u00b7chen\u00b7spel\u00b7ler", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6llenspro\u00df und Sonnenvetter,", "tokens": ["H\u00f6l\u00b7len\u00b7spro\u00df", "und", "Son\u00b7nen\u00b7vet\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ei, du wagst es nicht mit mir!\u00ab", "tokens": ["Ei", ",", "du", "wagst", "es", "nicht", "mit", "mir", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Pl\u00f6tzlich, ach, die Strahlengarbe", "tokens": ["Pl\u00f6tz\u00b7lich", ",", "ach", ",", "die", "Strah\u00b7len\u00b7gar\u00b7be"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["ADJD", "$,", "ITJ", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlug auf ihrem Weg nach unten", "tokens": ["Schlug", "auf", "ih\u00b7rem", "Weg", "nach", "un\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "APPR", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u2013 Platz da, Bahn frei, Dampf und Donner \u2013", "tokens": ["\u2013", "Platz", "da", ",", "Bahn", "frei", ",", "Dampf", "und", "Don\u00b7ner", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NN", "PTKVZ", "$,", "NN", "PTKVZ", "$,", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine kleine Schwalbe tot.", "tokens": ["Mei\u00b7ne", "klei\u00b7ne", "Schwal\u00b7be", "tot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}