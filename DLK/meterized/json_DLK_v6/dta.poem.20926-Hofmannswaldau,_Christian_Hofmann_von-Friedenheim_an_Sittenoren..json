{"dta.poem.20926": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Friedenheim an Sittenoren.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Dein Friedenheim schreibt hier/ geliebte Sit-", "tokens": ["Dein", "Frie\u00b7den\u00b7heim", "schreibt", "hier", "/", "ge\u00b7lieb\u00b7te", "Sit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$(", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "tenore/", "tokens": ["te\u00b7no\u00b7re", "/"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Der mehr itzund in dir als in ihm selber lebt/", "tokens": ["Der", "mehr", "it\u00b7zund", "in", "dir", "als", "in", "ihm", "sel\u00b7ber", "lebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPR", "PPER", "KOKOM", "APPR", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Komt gleich mein Seuffzer dir nicht st\u00fcndlich", "tokens": ["Komt", "gleich", "mein", "Seuff\u00b7zer", "dir", "nicht", "st\u00fcnd\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "PPOSAT", "NN", "PPER", "PTKNEG", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "vor das Ohre/", "tokens": ["vor", "das", "Oh\u00b7re", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "So schwer\u2019 ich da\u00df mein Hertz an deinem Hertzen klebt.", "tokens": ["So", "schwer'", "ich", "da\u00df", "mein", "Hertz", "an", "dei\u00b7nem", "Hert\u00b7zen", "klebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich lebe nur in dir und bin mir abgestorben/", "tokens": ["Ich", "le\u00b7be", "nur", "in", "dir", "und", "bin", "mir", "ab\u00b7ge\u00b7stor\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "KON", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich bin dem Monden gleich der ohne Sonn\u2019 erblast/", "tokens": ["Ich", "bin", "dem", "Mon\u00b7den", "gleich", "der", "oh\u00b7ne", "Sonn'", "er\u00b7blast", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ART", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Bist du zu weit von mir/ so bin ich auch verdorben/", "tokens": ["Bist", "du", "zu", "weit", "von", "mir", "/", "so", "bin", "ich", "auch", "ver\u00b7dor\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKA", "ADJD", "APPR", "PPER", "$(", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie leb\u2019 ich ohne dich/ die du mein Hertze hast?", "tokens": ["Wie", "leb'", "ich", "oh\u00b7ne", "dich", "/", "die", "du", "mein", "Hert\u00b7ze", "hast", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "PPER", "$(", "PRELS", "PPER", "PPOSAT", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Doch mag ich nicht zuviel von meiner Liebe sagen/", "tokens": ["Doch", "mag", "ich", "nicht", "zu\u00b7viel", "von", "mei\u00b7ner", "Lie\u00b7be", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mein Auge das verrieth dir erstlich meine Pein/", "tokens": ["Mein", "Au\u00b7ge", "das", "ver\u00b7rieth", "dir", "erst\u00b7lich", "mei\u00b7ne", "Pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDS", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und was ich dir hernach verst\u00e4ndlich f\u00fcrgetragen/", "tokens": ["Und", "was", "ich", "dir", "her\u00b7nach", "ver\u00b7st\u00e4nd\u00b7lich", "f\u00fcr\u00b7ge\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PPER", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das wird dir ja nicht mehr verborgen k\u00f6nnen seyn.", "tokens": ["Das", "wird", "dir", "ja", "nicht", "mehr", "ver\u00b7bor\u00b7gen", "k\u00f6n\u00b7nen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "VVINF", "VMINF", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Du weist wie offte mir ein heisser Blick entgangen/", "tokens": ["Du", "weist", "wie", "off\u00b7te", "mir", "ein", "heis\u00b7ser", "Blick", "ent\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "VVFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wann ich die Lantze dir zuehren eingelegt/", "tokens": ["Wann", "ich", "die", "Lant\u00b7ze", "dir", "zu\u00b7eh\u00b7ren", "ein\u00b7ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PPER", "VVINF", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich hoffte dazumahl ein Kleinod zuerlangen/", "tokens": ["Ich", "hoff\u00b7te", "da\u00b7zu\u00b7mahl", "ein", "Klei\u00b7nod", "zu\u00b7er\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "In das der Himmel selbst sein Bild hat eingepregt.", "tokens": ["In", "das", "der", "Him\u00b7mel", "selbst", "sein", "Bild", "hat", "ein\u00b7ge\u00b7pregt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "ADV", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Dein sch\u00f6nes Auge gab dem Pferde Muth und Fl\u00fcgel/", "tokens": ["Dein", "sch\u00f6\u00b7nes", "Au\u00b7ge", "gab", "dem", "Pfer\u00b7de", "Muth", "und", "Fl\u00fc\u00b7gel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Es machte mich behertzt/ und meine Lantze scharf/", "tokens": ["Es", "mach\u00b7te", "mich", "be\u00b7hertzt", "/", "und", "mei\u00b7ne", "Lant\u00b7ze", "scharf", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "$(", "KON", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Es f\u00fchrte mehr als ich die wunderleichten Z\u00fcgel/", "tokens": ["Es", "f\u00fchr\u00b7te", "mehr", "als", "ich", "die", "wun\u00b7der\u00b7leich\u00b7ten", "Z\u00fc\u00b7gel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KOUS", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "In dem ich mein Gesicht auf deine Br\u00fcste warf:", "tokens": ["In", "dem", "ich", "mein", "Ge\u00b7sicht", "auf", "dei\u00b7ne", "Br\u00fcs\u00b7te", "warf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ich wei\u00df wie offte mich dein Namen hat verrathen/", "tokens": ["Ich", "wei\u00df", "wie", "off\u00b7te", "mich", "dein", "Na\u00b7men", "hat", "ver\u00b7ra\u00b7then", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "VVFIN", "PPER", "PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und nur das leichte Blut zum Schimpf herauf ge-", "tokens": ["Und", "nur", "das", "leich\u00b7te", "Blut", "zum", "Schimpf", "her\u00b7auf", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "APPRART", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "bracht/", "tokens": ["bracht", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.26": {"text": "Du wahrest F\u00fchrerin von allen meinen Thaten/", "tokens": ["Du", "wah\u00b7rest", "F\u00fch\u00b7re\u00b7rin", "von", "al\u00b7len", "mei\u00b7nen", "Tha\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PIAT", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Mein Sinnen war auf nichts/ als nur auf dich bedacht.", "tokens": ["Mein", "Sin\u00b7nen", "war", "auf", "nichts", "/", "als", "nur", "auf", "dich", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PIS", "$(", "KOKOM", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ich schwur auf deine Treu/ ich hoffte hier zusterben/", "tokens": ["Ich", "schwur", "auf", "dei\u00b7ne", "Treu", "/", "ich", "hoff\u00b7te", "hier", "zus\u00b7ter\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$(", "PPER", "VVFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Ich wolte Jason seyn/ bey dir/ mein golden Fl\u00fc\u00df/", "tokens": ["Ich", "wol\u00b7te", "Ja\u00b7son", "seyn", "/", "bey", "dir", "/", "mein", "gol\u00b7den", "Fl\u00fc\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "VAINF", "$(", "APPR", "PPER", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Gedancken ohne Frucht! Ich f\u00fchle mein Verderben/", "tokens": ["Ge\u00b7dan\u00b7cken", "oh\u00b7ne", "Frucht", "!", "Ich", "f\u00fch\u00b7le", "mein", "Ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und meine Hoffnung kriegt hier einen Todes Ri\u00df.", "tokens": ["Und", "mei\u00b7ne", "Hoff\u00b7nung", "kriegt", "hier", "ei\u00b7nen", "To\u00b7des", "Ri\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ein unverhoffter Schlu\u00df/ der \u00fcberall erschollen/", "tokens": ["Ein", "un\u00b7ver\u00b7hoff\u00b7ter", "Schlu\u00df", "/", "der", "\u00fc\u00b7be\u00b7rall", "er\u00b7schol\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Jagt meinen Hertzen itzt den Frost des Schreckens ein/", "tokens": ["Jagt", "mei\u00b7nen", "Hert\u00b7zen", "itzt", "den", "Frost", "des", "Schre\u00b7ckens", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ART", "NN", "ART", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Es scheinet/ da\u00df die Lust so vor aus mir gequollen/", "tokens": ["Es", "schei\u00b7net", "/", "da\u00df", "die", "Lust", "so", "vor", "aus", "mir", "ge\u00b7quol\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "ART", "NN", "ADV", "APPR", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Mir frembde werden will/ und nicht kan Nachbar seyn.", "tokens": ["Mir", "fremb\u00b7de", "wer\u00b7den", "will", "/", "und", "nicht", "kan", "Nach\u00b7bar", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VAINF", "VMFIN", "$(", "KON", "PTKNEG", "VMFIN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wir sollen auf die See durch tausend wilde Wellen/", "tokens": ["Wir", "sol\u00b7len", "auf", "die", "See", "durch", "tau\u00b7send", "wil\u00b7de", "Wel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "APPR", "CARD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Doch dieses Wasser lescht der Liebe Feuer nicht/", "tokens": ["Doch", "die\u00b7ses", "Was\u00b7ser", "lescht", "der", "Lie\u00b7be", "Feu\u00b7er", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "ART", "NN", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ich darf mich zwar allhier nicht wohl zu dir gesellen/", "tokens": ["Ich", "darf", "mich", "zwar", "all\u00b7hier", "nicht", "wohl", "zu", "dir", "ge\u00b7sel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "PTKNEG", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Doch schau\u2019 ich warlich kaum/ was unsern F\u00fcrsatz", "tokens": ["Doch", "schau'", "ich", "war\u00b7lich", "kaum", "/", "was", "un\u00b7sern", "F\u00fcr\u00b7satz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "$(", "PWS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "bricht.", "tokens": ["bricht", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.41": {"text": "Ich soll mich in ein Schiff weit weit von dir begeben/", "tokens": ["Ich", "soll", "mich", "in", "ein", "Schiff", "weit", "weit", "von", "dir", "be\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "ART", "NN", "ADJD", "ADJD", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Das gehet nur den Leib und nicht die Geister an/", "tokens": ["Das", "ge\u00b7het", "nur", "den", "Leib", "und", "nicht", "die", "Geis\u00b7ter", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "KON", "PTKNEG", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Mein Wunsch und Seuffzer soll in deinem Seegel", "tokens": ["Mein", "Wunsch", "und", "Seuff\u00b7zer", "soll", "in", "dei\u00b7nem", "See\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "VMFIN", "APPR", "PPOSAT