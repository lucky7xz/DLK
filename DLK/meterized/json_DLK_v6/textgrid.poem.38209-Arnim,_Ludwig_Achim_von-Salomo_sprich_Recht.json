{"textgrid.poem.38209": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Salomo sprich Recht", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es waren einmal zwei Gespielen,", "tokens": ["Es", "wa\u00b7ren", "ein\u00b7mal", "zwei", "Ge\u00b7spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie giengen ins Feld spazieren.", "tokens": ["Sie", "gien\u00b7gen", "ins", "Feld", "spa\u00b7zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die eine die war so munter und wohl auf,", "tokens": ["Die", "ei\u00b7ne", "die", "war", "so", "mun\u00b7ter", "und", "wohl", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ART", "VAFIN", "ADV", "ADJD", "KON", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die andre trauret sehre, ja sehre.", "tokens": ["Die", "and\u00b7re", "trau\u00b7ret", "seh\u00b7re", ",", "ja", "seh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "VVFIN", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wir beide haben einen Knaben so lieb,", "tokens": ["Wir", "bei\u00b7de", "ha\u00b7ben", "ei\u00b7nen", "Kna\u00b7ben", "so", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Den k\u00f6nnen wir nicht theilen, ja theilen.", "tokens": ["Den", "k\u00f6n\u00b7nen", "wir", "nicht", "thei\u00b7len", ",", "ja", "thei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ach Gespielin liebe meine,", "tokens": ["Ach", "Ge\u00b7spie\u00b7lin", "lie\u00b7be", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VVFIN", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "La\u00df mir den Knaben alleine.", "tokens": ["La\u00df", "mir", "den", "Kna\u00b7ben", "al\u00b7lei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADV", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.9": {"text": "Ich will dir meinen Bruder geben,", "tokens": ["Ich", "will", "dir", "mei\u00b7nen", "Bru\u00b7der", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Meines Vaters Gut zum Theile, ja Theile.", "tokens": ["Mei\u00b7nes", "Va\u00b7ters", "Gut", "zum", "Thei\u00b7le", ",", "ja", "Thei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPRART", "NN", "$,", "ADV", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Ei deinen Bruder mag ich nicht,", "tokens": ["Ei", "dei\u00b7nen", "Bru\u00b7der", "mag", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VMFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Deines Vaters Gut veracht ich nicht.", "tokens": ["Dei\u00b7nes", "Va\u00b7ters", "Gut", "ver\u00b7acht", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Ich wollt nicht nehmen Silber und Gold,", "tokens": ["Ich", "wollt", "nicht", "neh\u00b7men", "Sil\u00b7ber", "und", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.14": {"text": "Da\u00df ich den Knaben lassen sollt, ja lassen sollt.", "tokens": ["Da\u00df", "ich", "den", "Kna\u00b7ben", "las\u00b7sen", "sollt", ",", "ja", "las\u00b7sen", "sollt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "VMFIN", "$,", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}