{"textgrid.poem.30265": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "79.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Augen sind Kohl-schwartz, drau\u00df dennoch Feuer blitzt,", "tokens": ["Dein", "Au\u00b7gen", "sind", "Kohl\u00b7schwartz", ",", "drau\u00df", "den\u00b7noch", "Feu\u00b7er", "blitzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "PAV", "ADV", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Quintilla; deine Haut ist Schnee, der dennoch hitzt.", "tokens": ["Quin\u00b7til\u00b7la", ";", "dei\u00b7ne", "Haut", "ist", "Schnee", ",", "der", "den\u00b7noch", "hitzt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPOSAT", "NN", "VAFIN", "NN", "$,", "PRELS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du Jungfern-Wunder du! was macht die zarte Brust?", "tokens": ["Du", "Jung\u00b7fern\u00b7Wun\u00b7der", "du", "!", "was", "macht", "die", "zar\u00b7te", "Brust", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPER", "$.", "PWS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie gibt den Grossen Mut und einem Kleinen Kost.", "tokens": ["Sie", "gibt", "den", "Gros\u00b7sen", "Mut", "und", "ei\u00b7nem", "Klei\u00b7nen", "Kost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "