{"dta.poem.3887": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "12) Schreiben an Herr Cramer und  \n seiner verlobten Jgfr. S\u00f6llnerin.  \n Oder Verh\u00f6rungs- Protocoll  in Heyraths-  \n Sachen.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Nachdem man ohngefehr, doch gantz gewi\u00df, ver-", "tokens": ["Nach\u00b7dem", "man", "ohn\u00b7ge\u00b7fehr", ",", "doch", "gantz", "ge\u00b7wi\u00df", ",", "ver"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PIS", "ADJD", "$,", "ADV", "ADV", "ADV", "$,", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "nommen, da\u00df Cramer in der Still in Leipzig", "tokens": ["nom\u00b7men", ",", "da\u00df", "Cra\u00b7mer", "in", "der", "Still", "in", "Leip\u00b7zig"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "NE", "APPR", "ART", "NN", "APPR", "NE"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "angekommen, und sich ", "tokens": ["an\u00b7ge\u00b7kom\u00b7men", ",", "und", "sich"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVPP", "$,", "KON", "PRF"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "gen ausersehn, auch \u00f6ffentlich geholt. So ist es", "tokens": ["gen", "au\u00b7ser\u00b7sehn", ",", "auch", "\u00f6f\u00b7fent\u00b7lich", "ge\u00b7holt", ".", "So", "ist", "es"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "VVINF", "$,", "ADV", "ADJD", "VVPP", "$.", "ADV", "VAFIN", "PPER"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "gleich geschehn, da\u00df ", "tokens": ["gleich", "ge\u00b7schehn", ",", "da\u00df"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADV", "VVPP", "$,", "KOUS"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "geb\u00fchret, den neuen Ehemann vors ", "tokens": ["ge\u00b7b\u00fch\u00b7ret", ",", "den", "neu\u00b7en", "E\u00b7he\u00b7mann", "vors"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ART", "ADJA", "NN", "APPRART"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "tiret und f\u00f6rmlich abgeh\u00f6rt. Worauf in Curia bey", "tokens": ["ti\u00b7ret", "und", "f\u00f6rm\u00b7lich", "ab\u00b7ge\u00b7h\u00f6rt", ".", "Wo\u00b7rauf", "in", "Cu\u00b7ria", "bey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "ADJD", "VVPP", "$.", "PAV", "APPR", "NE", "APPR"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "fr\u00fcher Tages-Zeit", "tokens": ["fr\u00fc\u00b7her", "Ta\u00b7ges\u00b7Zeit"], "token_info": ["word", "word"], "pos": ["ADJD", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}