{"textgrid.poem.57300": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr rechtet mit dem,", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr rechtet mit dem,", "tokens": ["Ihr", "rech\u00b7tet", "mit", "dem", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Dess grossen Namen", "tokens": ["Dess", "gros\u00b7sen", "Na\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Der sterbliche Weise", "tokens": ["Der", "sterb\u00b7li\u00b7che", "Wei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Kaum waget auszusprechen,", "tokens": ["Kaum", "wa\u00b7get", "aus\u00b7zu\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mit dem, dess grossen schrecklichen Namen", "tokens": ["Mit", "dem", ",", "dess", "gros\u00b7sen", "schreck\u00b7li\u00b7chen", "Na\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "$,", "ART", "ADJA", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Der hohe Engel", "tokens": ["Der", "ho\u00b7he", "En\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Staunend nennet,", "tokens": ["Stau\u00b7nend", "nen\u00b7net", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Mit Gott, mit Gott!", "tokens": ["Mit", "Gott", ",", "mit", "Gott", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ihr setzet euch, Gericht zu halten,", "tokens": ["Ihr", "set\u00b7zet", "euch", ",", "Ge\u00b7richt", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wegen des Lebens, und wegen des Todes,", "tokens": ["We\u00b7gen", "des", "Le\u00b7bens", ",", "und", "we\u00b7gen", "des", "To\u00b7des", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KON", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Wegen des Schicksals der Menschen,", "tokens": ["We\u00b7gen", "des", "Schick\u00b7sals", "der", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "\u00dcber Gott, Gericht \u00fcber Gott!", "tokens": ["\u00dc\u00b7ber", "Gott", ",", "Ge\u00b7richt", "\u00fc\u00b7ber", "Gott", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "APPR", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Emp\u00f6rer!", "tokens": ["Em\u00b7p\u00f6\u00b7rer", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Ihr verdammet Gott,", "tokens": ["Ihr", "ver\u00b7dam\u00b7met", "Gott", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Dass ihr geboren seyd, und sterben m\u00fcsset,", "tokens": ["Dass", "ihr", "ge\u00b7bo\u00b7ren", "seyd", ",", "und", "ster\u00b7ben", "m\u00fcs\u00b7set", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,", "KON", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Gott, Gott, Gott!", "tokens": ["Gott", ",", "Gott", ",", "Gott", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Ist euch nicht der Geist verirrt,", "tokens": ["Ist", "euch", "nicht", "der", "Geist", "ver\u00b7irrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht schwach die Seele, wie dem Knaben,", "tokens": ["Nicht", "schwach", "die", "See\u00b7le", ",", "wie", "dem", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ART", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der an der Mutter Arme noch wankt;", "tokens": ["Der", "an", "der", "Mut\u00b7ter", "Ar\u00b7me", "noch", "wankt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "So bebet!", "tokens": ["So", "be\u00b7bet", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+--", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Euer Einer war gestorben,", "tokens": ["Eu\u00b7er", "Ei\u00b7ner", "war", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PIS", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hatte gerechtet!", "tokens": ["Hat\u00b7te", "ge\u00b7rech\u00b7tet", "!"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Gericht gehalten! verdammet!", "tokens": ["Ge\u00b7richt", "ge\u00b7hal\u00b7ten", "!", "ver\u00b7dam\u00b7met", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "VVPP", "$.", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und stand vor Gott.", "tokens": ["Und", "stand", "vor", "Gott", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Die Wage klang;", "tokens": ["Die", "Wa\u00b7ge", "klang", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Sein todter Vater trauerte,", "tokens": ["Sein", "tod\u00b7ter", "Va\u00b7ter", "trau\u00b7er\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seine todte Mutter", "tokens": ["Sei\u00b7ne", "tod\u00b7te", "Mut\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Verbarg ihr Angesicht.", "tokens": ["Ver\u00b7barg", "ihr", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Wage klang, klang;", "tokens": ["Die", "Wa\u00b7ge", "klang", ",", "klang", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Es verstumte sein todter Freund,", "tokens": ["Es", "ver\u00b7stum\u00b7te", "sein", "tod\u00b7ter", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Vor Jammer versank", "tokens": ["Vor", "Jam\u00b7mer", "ver\u00b7sank"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Seine todte Braut.", "tokens": ["Sei\u00b7ne", "tod\u00b7te", "Braut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Die Wage, die Wage,", "tokens": ["Die", "Wa\u00b7ge", ",", "die", "Wa\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die furchtbare Wage klang;", "tokens": ["Die", "furcht\u00b7ba\u00b7re", "Wa\u00b7ge", "klang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und hoch empor", "tokens": ["Und", "hoch", "em\u00b7por"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Stieg die eine Schale.", "tokens": ["Stieg", "die", "ei\u00b7ne", "Scha\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}