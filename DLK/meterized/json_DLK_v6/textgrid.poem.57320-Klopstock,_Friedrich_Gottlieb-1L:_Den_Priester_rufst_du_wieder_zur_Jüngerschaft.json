{"textgrid.poem.57320": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Den Priester rufst du wieder zur J\u00fcngerschaft", "genre": "verse", "period": "N.A.", "pub_year": 1781, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Priester rufst du wieder zur J\u00fcngerschaft", "tokens": ["Den", "Pries\u00b7ter", "rufst", "du", "wie\u00b7der", "zur", "J\u00fcn\u00b7ger\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des grossen Stifters; machest zum Unterthan", "tokens": ["Des", "gros\u00b7sen", "Stif\u00b7ters", ";", "ma\u00b7chest", "zum", "Un\u00b7ter\u00b7than"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den j\u00f6chbeladnen Landmann; machst den", "tokens": ["Den", "j\u00f6ch\u00b7be\u00b7lad\u00b7nen", "Land\u00b7mann", ";", "machst", "den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "VVFIN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Juden zum Menschen. Wer hat geendet,", "tokens": ["Ju\u00b7den", "zum", "Men\u00b7schen", ".", "Wer", "hat", "ge\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$.", "PWS", "VAFIN", "VVPP", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Wie du beginnest? Wenn von des Ackerban's", "tokens": ["Wie", "du", "be\u00b7gin\u00b7nest", "?", "Wenn", "von", "des", "A\u00b7cke\u00b7rban's"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$.", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Schweiss nicht f\u00fcr ihn auch triefet des Bauren Stirn,", "tokens": ["Schweiss", "nicht", "f\u00fcr", "ihn", "auch", "trie\u00b7fet", "des", "Bau\u00b7ren", "Stirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "PPER", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Pfl\u00fcgt er nicht Eigenthum dem S\u00e4ugling,", "tokens": ["Pfl\u00fcgt", "er", "nicht", "Ei\u00b7gen\u00b7thum", "dem", "S\u00e4ug\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Seufzet er mit, wenn von Erndtelasten", "tokens": ["Seuf\u00b7zet", "er", "mit", ",", "wenn", "von", "Ernd\u00b7tel\u00b7as\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "APPR", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Der Wagen seufzt: so b\u00fcrdet Tirannenrecht", "tokens": ["Der", "Wa\u00b7gen", "seufzt", ":", "so", "b\u00fcr\u00b7det", "Ti\u00b7ran\u00b7nen\u00b7recht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "VVFIN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem unterdr\u00fcckten Landeserhaltung auf,", "tokens": ["Dem", "un\u00b7ter\u00b7dr\u00fcck\u00b7ten", "Lan\u00b7de\u00b7ser\u00b7hal\u00b7tung", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dienst, den die blutge Faust des st\u00e4rkern", "tokens": ["Dienst", ",", "den", "die", "blut\u00b7ge", "Faust", "des", "st\u00e4r\u00b7kern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Grub in die Tafel. Und die zerschl\u00e4gst du!", "tokens": ["Grub", "in", "die", "Ta\u00b7fel", ".", "Und", "die", "zer\u00b7schl\u00e4gst", "du", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$.", "KON", "ART", "VVFIN", "PPER", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Wen fasst des Mitleids Schauer nicht, wenn er sieht,", "tokens": ["Wen", "fasst", "des", "Mit\u00b7leids", "Schau\u00b7er", "nicht", ",", "wenn", "er", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "NE", "PTKNEG", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie unser P\u00f6bel Kanaans Volk entmenscht!", "tokens": ["Wie", "un\u00b7ser", "P\u00f6\u00b7bel", "Ka\u00b7na\u00b7ans", "Volk", "ent\u00b7menscht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NE", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und thut der's nicht, weil unsre F\u00fcrsten", "tokens": ["Und", "thut", "der's", "nicht", ",", "weil", "uns\u00b7re", "F\u00fcrs\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "PTKNEG", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie in zu eiserne Fessel schmieden?", "tokens": ["Sie", "in", "zu", "ei\u00b7ser\u00b7ne", "Fes\u00b7sel", "schmie\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Du l\u00f6sest ihnen, Retter! die rostige,", "tokens": ["Du", "l\u00f6\u00b7sest", "ih\u00b7nen", ",", "Ret\u00b7ter", "!", "die", "ros\u00b7ti\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$.", "ART", "ADJA", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Engangelegte Fessel vom wunden Arm;", "tokens": ["En\u00b7gan\u00b7ge\u00b7leg\u00b7te", "Fes\u00b7sel", "vom", "wun\u00b7den", "Arm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie f\u00fchlen's, glauben's kaum. So lange", "tokens": ["Sie", "f\u00fch\u00b7len's", ",", "glau\u00b7ben's", "kaum", ".", "So", "lan\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "ADV", "$.", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hat's um die elenden hergeklirret!", "tokens": ["Hat's", "um", "die", "e\u00b7len\u00b7den", "her\u00b7ge\u00b7klir\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "---+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Wir weinten Unmuth, dass uns der R\u00f6mer Rom", "tokens": ["Wir", "wein\u00b7ten", "Un\u00b7muth", ",", "dass", "uns", "der", "R\u00f6\u00b7mer", "Rom"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "KOUS", "PPER", "ART", "NN", "NE"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zwar nicht beherschte, aber doch peinigte:", "tokens": ["Zwar", "nicht", "be\u00b7herschte", ",", "a\u00b7ber", "doch", "pei\u00b7nig\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVFIN", "$,", "ADV", "ADV", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und blutig ist die andre Thr\u00e4ne,", "tokens": ["Und", "blu\u00b7tig", "ist", "die", "and\u00b7re", "Thr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dass uns der R\u00f6mlinge Rom beherschet!", "tokens": ["Dass", "uns", "der", "R\u00f6m\u00b7lin\u00b7ge", "Rom", "be\u00b7her\u00b7schet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NE", "VVFIN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Dass Deutschlands Kaiser Biegel des Zelters hielt!", "tokens": ["Dass", "Deutschlands", "Kai\u00b7ser", "Bie\u00b7gel", "des", "Zel\u00b7ters", "hielt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Dass Deutschlands Kaiser nackt um des Buhlen Schloss", "tokens": ["Dass", "Deutschlands", "Kai\u00b7ser", "nackt", "um", "des", "Buh\u00b7len", "Schloss"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "NN", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Herging, erfror; wenn nicht Matildis ...", "tokens": ["Her\u00b7ging", ",", "er\u00b7fror", ";", "wenn", "nicht", "Ma\u00b7til\u00b7dis", "..."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$.", "KOUS", "PTKNEG", "NE", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Aber du komst kaum, und siehst; so siegst du!", "tokens": ["A\u00b7ber", "du", "komst", "kaum", ",", "und", "siehst", ";", "so", "siegst", "du", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "KON", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Nun mag der kronentragende Oberm\u00f6nch,", "tokens": ["Nun", "mag", "der", "kro\u00b7nen\u00b7tra\u00b7gen\u00b7de", "O\u00b7ber\u00b7m\u00f6nch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit allen seinen purperbem\u00e4ntelten", "tokens": ["Mit", "al\u00b7len", "sei\u00b7nen", "pur\u00b7per\u00b7be\u00b7m\u00e4n\u00b7tel\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PPOSAT", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "M\u00f6nchlein, das Kanonsrecht, wie weit es", "tokens": ["M\u00f6nch\u00b7lein", ",", "das", "Ka\u00b7nons\u00b7recht", ",", "wie", "weit", "es"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ART", "NN", "$,", "PWAV", "ADJD", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Walte, beschielen. Du hast gesehen!", "tokens": ["Wal\u00b7te", ",", "be\u00b7schie\u00b7len", ".", "Du", "hast", "ge\u00b7se\u00b7hen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVINF", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}