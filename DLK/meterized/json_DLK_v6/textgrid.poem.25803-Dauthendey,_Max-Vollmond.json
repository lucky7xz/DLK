{"textgrid.poem.25803": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Vollmond", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Grellgest\u00fcrzt schrille Schluchten.", "tokens": ["Grell\u00b7ge\u00b7st\u00fcrzt", "schril\u00b7le", "Schluch\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Tief in phosphorgr\u00fcnen Schachten", "tokens": ["Tief", "in", "phos\u00b7phor\u00b7gr\u00fc\u00b7nen", "Schach\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gl\u00fchen stumm metallne Spiegel,", "tokens": ["Gl\u00fc\u00b7hen", "stumm", "me\u00b7tall\u00b7ne", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wei\u00df und lautlos festerstarrt.", "tokens": ["Wei\u00df", "und", "laut\u00b7los", "fes\u00b7ter\u00b7starrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Du liegst eingegossen blau", "tokens": ["Du", "liegst", "ein\u00b7ge\u00b7gos\u00b7sen", "blau"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor mir in dem klaren Erz.", "tokens": ["Vor", "mir", "in", "dem", "kla\u00b7ren", "Erz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ich knie nieder,", "tokens": ["Und", "ich", "knie", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Meine Augen beten:", "tokens": ["Mei\u00b7ne", "Au\u00b7gen", "be\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Strahle deinen blauen Atem in mein Blut.", "tokens": ["Strah\u00b7le", "dei\u00b7nen", "blau\u00b7en", "A\u00b7tem", "in", "mein", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Blaue Schatten knien an den Ufern.", "tokens": ["Blau\u00b7e", "Schat\u00b7ten", "kni\u00b7en", "an", "den", "U\u00b7fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "L\u00e4cheln in die Silberspiegel,", "tokens": ["L\u00e4\u00b7cheln", "in", "die", "Sil\u00b7ber\u00b7spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre gelben Augen singen hell und dunkel.", "tokens": ["Ih\u00b7re", "gel\u00b7ben", "Au\u00b7gen", "sin\u00b7gen", "hell", "und", "dun\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.5": {"line.1": {"text": "Alle, Kinder dieser bleichen Insel.", "tokens": ["Al\u00b7le", ",", "Kin\u00b7der", "die\u00b7ser", "blei\u00b7chen", "In\u00b7sel", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "NN", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Blaue Wesen, die der Mond geboren.", "tokens": ["Blau\u00b7e", "We\u00b7sen", ",", "die", "der", "Mond", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und die Feuer ihrer Augen", "tokens": ["Und", "die", "Feu\u00b7er", "ih\u00b7rer", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gl\u00fchen hell die Sprache ihres Schweigens.", "tokens": ["Gl\u00fc\u00b7hen", "hell", "die", "Spra\u00b7che", "ih\u00b7res", "Schwei\u00b7gens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Aus den wei\u00dfen Spiegeln bl\u00fchen", "tokens": ["Aus", "den", "wei\u00b7\u00dfen", "Spie\u00b7geln", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blaue Echo ihrer Schatten.", "tokens": ["Blau\u00b7e", "E\u00b7cho", "ih\u00b7rer", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Jeder betet zu dem eignen Bilde.", "tokens": ["Je\u00b7der", "be\u00b7tet", "zu", "dem", "eig\u00b7nen", "Bil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ihre goldnen Phosphoraugen", "tokens": ["Ih\u00b7re", "gold\u00b7nen", "Phos\u00b7phor\u00b7au\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00fcssen hei\u00df sich selber im Metall,", "tokens": ["K\u00fcs\u00b7sen", "hei\u00df", "sich", "sel\u00b7ber", "im", "Me\u00b7tall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PRF", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und die blauen Wesen schmelzen bleichend,", "tokens": ["Und", "die", "blau\u00b7en", "We\u00b7sen", "schmel\u00b7zen", "blei\u00b7chend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "In das eigne blaue Spiegelbild.", "tokens": ["In", "das", "eig\u00b7ne", "blau\u00b7e", "Spie\u00b7gel\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Durch die gr\u00fcnen Einsamkeiten", "tokens": ["Durch", "die", "gr\u00fc\u00b7nen", "Ein\u00b7sam\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wallt der Klagelaut der Blidatulpen,", "tokens": ["Wallt", "der", "Kla\u00b7ge\u00b7laut", "der", "Bli\u00b7da\u00b7tul\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und die elfenbeinbleichen Kelche", "tokens": ["Und", "die", "el\u00b7fen\u00b7bein\u00b7blei\u00b7chen", "Kel\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gie\u00dfen Schnee.", "tokens": ["Gie\u00b7\u00dfen", "Schnee", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.9": {"line.1": {"text": "Hoch am schneeigen Schachtrand", "tokens": ["Hoch", "am", "schne\u00b7e\u00b7i\u00b7gen", "Schach\u00b7trand"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Rauschen wei\u00df die Schwanenb\u00e4ume,", "tokens": ["Rau\u00b7schen", "wei\u00df", "die", "Schwa\u00b7nen\u00b7b\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und aus gr\u00fcnem Eis die Bl\u00fcten", "tokens": ["Und", "aus", "gr\u00fc\u00b7nem", "Eis", "die", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwingen mit kristallenen Fl\u00fcgeln", "tokens": ["Schwin\u00b7gen", "mit", "kris\u00b7tal\u00b7le\u00b7nen", "Fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+--++--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Auf und nieder.", "tokens": ["Auf", "und", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "KON", "PTKVZ", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Silbermatt ihre Wellens\u00e4nge", "tokens": ["Sil\u00b7ber\u00b7matt", "ih\u00b7re", "Wel\u00b7len\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Gleiten durch die erznen Spiegel,", "tokens": ["Glei\u00b7ten", "durch", "die", "erz\u00b7nen", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Das Metall schwingt mit den D\u00fcften,", "tokens": ["Das", "Me\u00b7tall", "schwingt", "mit", "den", "D\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Und sie wiegen dich im L\u00e4cheln", "tokens": ["Und", "sie", "wie\u00b7gen", "dich", "im", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "\u00c4therblau auf ihrem Silber.", "tokens": ["\u00c4\u00b7ther\u00b7blau", "auf", "ih\u00b7rem", "Sil\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "O, ich liebe dich mein Knabe,", "tokens": ["O", ",", "ich", "lie\u00b7be", "dich", "mein", "Kna\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mein Blut will mit dir bleichen,", "tokens": ["Und", "mein", "Blut", "will", "mit", "dir", "blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in ", "tokens": ["Und", "in"], "token_info": ["word", "word"], "pos": ["KON", "APPR"], "meter": "-+", "measure": "iambic.single"}}, "stanza.11": {"line.1": {"text": "Gr\u00fcner gl\u00fchn die Phosphorklippen,", "tokens": ["Gr\u00fc\u00b7ner", "gl\u00fchn", "die", "Phos\u00b7phor\u00b7klip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die erznen Seen spannen", "tokens": ["Und", "die", "erz\u00b7nen", "Seen", "span\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "--+-++-", "measure": "anapaest.init"}, "line.3": {"text": "Hei\u00dfer, blanker ihre Spiegel.", "tokens": ["Hei\u00b7\u00dfer", ",", "blan\u00b7ker", "ih\u00b7re", "Spie\u00b7gel", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Tief bin ich in dich geschmolzen,", "tokens": ["Tief", "bin", "ich", "in", "dich", "ge\u00b7schmol\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weich in ", "tokens": ["Weich", "in"], "token_info": ["word", "word"], "pos": ["ADJD", "APPR"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "T\u00f6nen wir im bleichen Silber.", "tokens": ["T\u00f6\u00b7nen", "wir", "im", "blei\u00b7chen", "Sil\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Ringsum zucken aus dem Spiegel", "tokens": ["Ring\u00b7sum", "zu\u00b7cken", "aus", "dem", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kalt die wei\u00dfen Seedakelche.", "tokens": ["Kalt", "die", "wei\u00b7\u00dfen", "See\u00b7da\u00b7kel\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Blendend bleichen ihre D\u00fcfte", "tokens": ["Blen\u00b7dend", "blei\u00b7chen", "ih\u00b7re", "D\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unseres Atems tiefste letzte rote Welle.", "tokens": ["Un\u00b7se\u00b7res", "A\u00b7tems", "tiefs\u00b7te", "letz\u00b7te", "ro\u00b7te", "Wel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "ADJA", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.14": {"line.1": {"text": "Wir erstarren schweigend gl\u00fchend,", "tokens": ["Wir", "er\u00b7star\u00b7ren", "schwei\u00b7gend", "gl\u00fc\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wei\u00df im wei\u00dfen erznen Spiegel.", "tokens": ["Wei\u00df", "im", "wei\u00b7\u00dfen", "erz\u00b7nen", "Spie\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}