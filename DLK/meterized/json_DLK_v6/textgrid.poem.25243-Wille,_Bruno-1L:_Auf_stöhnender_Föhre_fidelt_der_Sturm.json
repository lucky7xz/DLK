{"textgrid.poem.25243": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf st\u00f6hnender F\u00f6hre fidelt der Sturm", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf st\u00f6hnender F\u00f6hre fidelt der Sturm", "tokens": ["Auf", "st\u00f6h\u00b7nen\u00b7der", "F\u00f6h\u00b7re", "fi\u00b7delt", "der", "Sturm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Heulende d\u00fcstre Balladen;", "tokens": ["Heu\u00b7len\u00b7de", "d\u00fcst\u00b7re", "Bal\u00b7la\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Es schnaubt sein Odem, nebelfeucht", "tokens": ["Es", "schnaubt", "sein", "O\u00b7dem", ",", "ne\u00b7bel\u00b7feucht"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Von nordischen Seegestaden.", "tokens": ["Von", "nor\u00b7di\u00b7schen", "See\u00b7ge\u00b7sta\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "So tr\u00fcbe der Himmel, als w\u00e4r's schon sp\u00e4t.", "tokens": ["So", "tr\u00fc\u00b7be", "der", "Him\u00b7mel", ",", "als", "w\u00e4r's", "schon", "sp\u00e4t", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KOKOM", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Wolken pilgern traurig.", "tokens": ["Die", "Wol\u00b7ken", "pil\u00b7gern", "trau\u00b7rig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Strudel taumelt verkommenes Laub", "tokens": ["Im", "Stru\u00b7del", "tau\u00b7melt", "ver\u00b7kom\u00b7me\u00b7nes", "Laub"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Um Baumgerippe so schaurig.", "tokens": ["Um", "Baum\u00b7ge\u00b7rip\u00b7pe", "so", "schau\u00b7rig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "ADV", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ein letztes Bl\u00e4ttchen am Dornenstrauch", "tokens": ["Ein", "letz\u00b7tes", "Bl\u00e4tt\u00b7chen", "am", "Dor\u00b7nen\u00b7strauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Fr\u00f6stelt in starrem Weh ...", "tokens": ["Fr\u00f6s\u00b7telt", "in", "star\u00b7rem", "Weh", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "O mach ein Ende, Novembersturm!", "tokens": ["O", "mach", "ein", "En\u00b7de", ",", "No\u00b7vem\u00b7ber\u00b7sturm", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Deck zu, du wogender Schnee!", "tokens": ["Deck", "zu", ",", "du", "wo\u00b7gen\u00b7der", "Schnee", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}