{"textgrid.poem.49933": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "7. Stimme aus dem Glockenturm", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich von den Schwestern allein bin gut katholisch geblieben;", "tokens": ["Ich", "von", "den", "Schwes\u00b7tern", "al\u00b7lein", "bin", "gut", "ka\u00b7tho\u00b7lisch", "ge\u00b7blie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADV", "VAFIN", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Dies bezeugt euch mein Ton, hoff ich, mein goldener, noch.", "tokens": ["Dies", "be\u00b7zeugt", "euch", "mein", "Ton", ",", "hoff", "ich", ",", "mein", "gol\u00b7de\u00b7ner", ",", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "$,", "ADV", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Zwar ich klinge so mit, weil ich mu\u00df, sooft man uns l\u00e4utet,", "tokens": ["Zwar", "ich", "klin\u00b7ge", "so", "mit", ",", "weil", "ich", "mu\u00df", ",", "sooft", "man", "uns", "l\u00e4u\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "VMFIN", "$,", "ADV", "PIS", "PPER", "VVFIN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Aber ich denke mein Teil, wi\u00dft es, im stillen dabei.", "tokens": ["A\u00b7ber", "ich", "den\u00b7ke", "mein", "Teil", ",", "wi\u00dft", "es", ",", "im", "stil\u00b7len", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "$,", "APPRART", "ADJA", "PAV", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}}}}}