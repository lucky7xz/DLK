{"textgrid.poem.59421": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Dank", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Gott, dir sag ich Dank,", "tokens": ["Mein", "Gott", ",", "dir", "sag", "ich", "Dank", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df du die Jugend mir bis \u00fcber alle Wipfel", "tokens": ["Da\u00df", "du", "die", "Ju\u00b7gend", "mir", "bis", "\u00fc\u00b7ber", "al\u00b7le", "Wip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In Morgenrot getaucht und Klang,", "tokens": ["In", "Mor\u00b7gen\u00b7rot", "ge\u00b7taucht", "und", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und auf des Lebens Gipfel,", "tokens": ["Und", "auf", "des", "Le\u00b7bens", "Gip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bevor der Tag geendet,", "tokens": ["Be\u00b7vor", "der", "Tag", "ge\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vom Herzen unbewacht", "tokens": ["Vom", "Her\u00b7zen", "un\u00b7be\u00b7wacht"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den falschen Glanz gewendet,", "tokens": ["Den", "fal\u00b7schen", "Glanz", "ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df ich nicht taumle ruhmgeblendet,", "tokens": ["Da\u00df", "ich", "nicht", "taum\u00b7le", "ruhm\u00b7ge\u00b7blen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da nun herein die Nacht", "tokens": ["Da", "nun", "her\u00b7ein", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PTKVZ", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Dunkelt in ernster Pracht.", "tokens": ["Dun\u00b7kelt", "in", "erns\u00b7ter", "Pracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}