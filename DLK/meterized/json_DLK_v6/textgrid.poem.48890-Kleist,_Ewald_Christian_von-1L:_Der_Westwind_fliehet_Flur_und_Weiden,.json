{"textgrid.poem.48890": {"metadata": {"author": {"name": "Kleist, Ewald Christian von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Westwind fliehet Flur und Weiden,", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Westwind fliehet Flur und Weiden,", "tokens": ["Der", "West\u00b7wind", "flie\u00b7het", "Flur", "und", "Wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die jezt verbl\u00fchn,", "tokens": ["Die", "jezt", "ver\u00b7bl\u00fchn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "O Tirsis sollen Scherz und Freuden", "tokens": ["O", "Tir\u00b7sis", "sol\u00b7len", "Scherz", "und", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ihm entfliehn?", "tokens": ["Mit", "ihm", "ent\u00b7fliehn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Nein der Orcane wildes Blasen", "tokens": ["Nein", "der", "Or\u00b7ca\u00b7ne", "wil\u00b7des", "Bla\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "ART", "NN", "ADJA", "NN"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die um mein Gut", "tokens": ["Die", "um", "mein", "Gut"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Jezt heulend, ausgeschlossen, rasen,", "tokens": ["Jezt", "heu\u00b7lend", ",", "aus\u00b7ge\u00b7schlos\u00b7sen", ",", "ra\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hemmt nicht den Muth.", "tokens": ["Hemmt", "nicht", "den", "Muth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Komm mit mir in der \u00f6den Fluren", "tokens": ["Komm", "mit", "mir", "in", "der", "\u00f6\u00b7den", "Flu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Bereiftes Gras,", "tokens": ["Be\u00b7reif\u00b7tes", "Gras", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Verfolg mit mir des Wildes Spuren", "tokens": ["Ver\u00b7folg", "mit", "mir", "des", "Wil\u00b7des", "Spu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Wald von Glas.", "tokens": ["Im", "Wald", "von", "Glas", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Und h\u00f6r des Hains Gew\u00f6lbe schallen,", "tokens": ["Und", "h\u00f6r", "des", "Hains", "Ge\u00b7w\u00f6l\u00b7be", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenns Horn erwacht.", "tokens": ["Wenns", "Horn", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und sieh von hohen Bergen fallen", "tokens": ["Und", "sieh", "von", "ho\u00b7hen", "Ber\u00b7gen", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die schnelle Jagd.", "tokens": ["Die", "schnel\u00b7le", "Jagd", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Denn eil in meine Wohnung wieder", "tokens": ["Denn", "eil", "in", "mei\u00b7ne", "Woh\u00b7nung", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "M\u00fcd' aus dem Hain,", "tokens": ["M\u00fcd'", "aus", "dem", "Hain", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und singe mit mir s\u00fcsse Lieder", "tokens": ["Und", "sin\u00b7ge", "mit", "mir", "s\u00fcs\u00b7se", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bey frohem Wein.", "tokens": ["Bey", "fro\u00b7hem", "Wein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Und Chloris die durch ihre Saiten", "tokens": ["Und", "Chlo\u00b7ris", "die", "durch", "ih\u00b7re", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "ART", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dirs Herz entwandt,", "tokens": ["Dirs", "Herz", "ent\u00b7wandt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Soll Lalagens Gesang begleiten", "tokens": ["Soll", "La\u00b7la\u00b7gens", "Ge\u00b7sang", "be\u00b7glei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "NE", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit kluger Hand.", "tokens": ["Mit", "klu\u00b7ger", "Hand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Sieh hin! Die Sterne sind erschienen", "tokens": ["Sieh", "hin", "!", "Die", "Ster\u00b7ne", "sind", "er\u00b7schie\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$.", "ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Luna winkt,", "tokens": ["Und", "Lu\u00b7na", "winkt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sie streiten gleichsam, wer von ihnen", "tokens": ["Sie", "strei\u00b7ten", "gleich\u00b7sam", ",", "wer", "von", "ih\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PWS", "APPR", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Am besten blinkt.", "tokens": ["Am", "bes\u00b7ten", "blinkt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Den Scherz mit K\u00fcssen zu verschwistern", "tokens": ["Den", "Scherz", "mit", "K\u00fcs\u00b7sen", "zu", "ver\u00b7schwis\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und fern vom Neid,", "tokens": ["Und", "fern", "vom", "Neid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Den langen Abend zu verfl\u00fcstern", "tokens": ["Den", "lan\u00b7gen", "A\u00b7bend", "zu", "ver\u00b7fl\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ists jetzo Zeit.", "tokens": ["Ists", "jet\u00b7zo", "Zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Komm! La\u00df uns unsern Geist erheitern.", "tokens": ["Komm", "!", "La\u00df", "uns", "un\u00b7sern", "Geist", "er\u00b7hei\u00b7tern", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wen Gold erg\u00f6zt,", "tokens": ["Wen", "Gold", "er\u00b7g\u00f6zt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mag in der Fluth am Felsen scheitern,", "tokens": ["Mag", "in", "der", "Fluth", "am", "Fel\u00b7sen", "schei\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der sich entsezt.", "tokens": ["Der", "sich", "ent\u00b7sezt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Ruhm, Reichthum, Pracht des Hofs Beschwerde", "tokens": ["Ruhm", ",", "Reicht\u00b7hum", ",", "Pracht", "des", "Hofs", "Be\u00b7schwer\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Volk verehrt,", "tokens": ["Vom", "Volk", "ver\u00b7ehrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ist Wahn, und nicht des Herrn der Erde", "tokens": ["Ist", "Wahn", ",", "und", "nicht", "des", "Herrn", "der", "Er\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "$,", "KON", "PTKNEG", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Weisen werth.", "tokens": ["Des", "Wei\u00b7sen", "werth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}