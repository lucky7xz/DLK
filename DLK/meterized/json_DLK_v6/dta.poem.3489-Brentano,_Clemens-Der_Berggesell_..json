{"dta.poem.3489": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Der Berggesell .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r ich ein wilder Falke.               ", "tokens": ["W\u00e4r", "ich", "ein", "wil\u00b7der", "Fal\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So wollt ich mich schwingen auf,", "tokens": ["So", "wollt", "ich", "mich", "schwin\u00b7gen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich wollt mich nieder lassen,", "tokens": ["Ich", "wollt", "mich", "nie\u00b7der", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKVZ", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr eins Reichen B\u00fcrgers Haus.", "tokens": ["F\u00fcr", "eins", "Rei\u00b7chen", "B\u00fcr\u00b7gers", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Darinn ist ein M\u00e4gdelein,", "tokens": ["Da\u00b7rinn", "ist", "ein", "M\u00e4g\u00b7del\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Madlena ist sie genannt,", "tokens": ["Mad\u00b7le\u00b7na", "ist", "sie", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So hab ich alle meine Tag", "tokens": ["So", "hab", "ich", "al\u00b7le", "mei\u00b7ne", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein sch\u00f6ners brauns M\u00e4gdlein erkannt.", "tokens": ["Kein", "sch\u00f6\u00b7ners", "brauns", "M\u00e4gd\u00b7lein", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "An einem Montag es geschah,", "tokens": ["An", "ei\u00b7nem", "Mon\u00b7tag", "es", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An einem Montag fr\u00fch,", "tokens": ["An", "ei\u00b7nem", "Mon\u00b7tag", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da sah man die sch\u00f6ne Madlena,", "tokens": ["Da", "sah", "man", "die", "sch\u00f6\u00b7ne", "Mad\u00b7le\u00b7na", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zu dem Obern Thor ausgehn.", "tokens": ["Zu", "dem", "O\u00b7bern", "Thor", "aus\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Da fragten sie die Zarten:", "tokens": ["Da", "frag\u00b7ten", "sie", "die", "Zar\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Madlena, wo willt ", "tokens": ["Mad\u00b7le\u00b7na", ",", "wo", "willt"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "PWAV", "VMFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "In meines Vaters Garten,", "tokens": ["In", "mei\u00b7nes", "Va\u00b7ters", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da ich n\u00e4chten gewesen bin.", "tokens": ["Da", "ich", "n\u00e4ch\u00b7ten", "ge\u00b7we\u00b7sen", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "VAPP", "VAFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Und da sie in den Garten kam,", "tokens": ["Und", "da", "sie", "in", "den", "Gar\u00b7ten", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl in den Garten einlief,", "tokens": ["Wohl", "in", "den", "Gar\u00b7ten", "ein\u00b7lief", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Da lag ein sch\u00f6ner junger G'sell,", "tokens": ["Da", "lag", "ein", "sch\u00f6\u00b7ner", "jun\u00b7ger", "G'\u00b7sell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Unter einer Linden und schlief.", "tokens": ["Un\u00b7ter", "ei\u00b7ner", "Lin\u00b7den", "und", "schlief", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "KON", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Steh auf junger Geselle,", "tokens": ["Steh", "auf", "jun\u00b7ger", "Ge\u00b7sel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Steh auf, denn es ist Zeit,", "tokens": ["Steh", "auf", ",", "denn", "es", "ist", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KON", "PPER", "VAFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich h\u00f6r die Schl\u00fcssel klingen,", "tokens": ["Ich", "h\u00f6r", "die", "Schl\u00fcs\u00b7sel", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein M\u00fctterlein ist nicht weit.", "tokens": ["Mein", "M\u00fct\u00b7ter\u00b7lein", "ist", "nicht", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "H\u00f6rst du die Schl\u00fcssel klingen,", "tokens": ["H\u00f6rst", "du", "die", "Schl\u00fcs\u00b7sel", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und ist dein M\u00fctterlein nicht weit,", "tokens": ["Und", "ist", "dein", "M\u00fct\u00b7ter\u00b7lein", "nicht", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So zeuch mit mir von hinnen,", "tokens": ["So", "zeuch", "mit", "mir", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "APPR", "PPER", "APPR", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohl \u00fcber die breite Heid.", "tokens": ["Wohl", "\u00fc\u00b7ber", "die", "brei\u00b7te", "Heid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Und da sie \u00fcber die Heide kamen,", "tokens": ["Und", "da", "sie", "\u00fc\u00b7ber", "die", "Hei\u00b7de", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wohl unter ein Linde was breit,", "tokens": ["Wohl", "un\u00b7ter", "ein", "Lin\u00b7de", "was", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PWS", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da ward denselben zweien,", "tokens": ["Da", "ward", "den\u00b7sel\u00b7ben", "zwei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Seiden ein Bett bereit.", "tokens": ["Von", "Sei\u00b7den", "ein", "Bett", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Sie lagen beieinander,", "tokens": ["Sie", "la\u00b7gen", "bei\u00b7ein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bis auf drithalbe Stund,", "tokens": ["Bis", "auf", "drit\u00b7hal\u00b7be", "Stund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kehr dich brauns M\u00e4gdlein herum,", "tokens": ["Kehr", "dich", "brauns", "M\u00e4gd\u00b7lein", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Beut mir dein'n roten Mund.", "tokens": ["Beut", "mir", "dein'n", "ro\u00b7ten", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Du sagst mir viel von kehren,", "tokens": ["Du", "sagst", "mir", "viel", "von", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sagst mir von keiner Eh,", "tokens": ["Sagst", "mir", "von", "kei\u00b7ner", "Eh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich f\u00fcrcht ich hab verschlafen,", "tokens": ["Ich", "f\u00fcrcht", "ich", "hab", "ver\u00b7schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Treu und auch mein Ehr.", "tokens": ["Mein", "Treu", "und", "auch", "mein", "Ehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "F\u00fcrchst du, du habst verschlafen", "tokens": ["F\u00fcrchst", "du", ",", "du", "habst", "ver\u00b7schla\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein Treu und auch dein Ehr.", "tokens": ["Dein", "Treu", "und", "auch", "dein", "Ehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df dichs Feinslieb nicht k\u00fcmmern,", "tokens": ["La\u00df", "dichs", "Feins\u00b7lieb", "nicht", "k\u00fcm\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich nehm dich zu der Eh.", "tokens": ["Ich", "nehm", "dich", "zu", "der", "Eh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Wer ist der uns dies Liedlein sang,", "tokens": ["Wer", "ist", "der", "uns", "dies", "Lied\u00b7lein", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "PPER", "PDS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von neuen gesungen hat,", "tokens": ["Von", "neu\u00b7en", "ge\u00b7sun\u00b7gen", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVPP", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das hat gethan ein Berggesell,", "tokens": ["Das", "hat", "ge\u00b7than", "ein", "Berg\u00b7ge\u00b7sell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Sanct Annenberg in der Stadt.", "tokens": ["Auf", "Sanct", "An\u00b7nen\u00b7berg", "in", "der", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "NE", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.13": {"line.1": {"text": "Er hats gar frei gesungen,", "tokens": ["Er", "hats", "gar", "frei", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bei Meth, bei k\u00fchlem Wein,", "tokens": ["Bei", "Me\u00b7th", ",", "bei", "k\u00fch\u00b7lem", "Wein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Darbei da seyn gesessen,", "tokens": ["Dar\u00b7bei", "da", "seyn", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Drei zarte Jungfr\u00e4ulein.", "tokens": ["Drei", "zar\u00b7te", "Jung\u00b7fr\u00e4u\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}