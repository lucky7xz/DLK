{"dta.poem.19159": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus Gelb und Blau entspringt nach unten Gr\u00fcn durch Mischen;", "tokens": ["Aus", "Gelb", "und", "Blau", "ent\u00b7springt", "nach", "un\u00b7ten", "Gr\u00fcn", "durch", "Mi\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "APPR", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach oben mischt sichs nicht, dort bl\u00fcht das Roth dazwischen.", "tokens": ["Nach", "o\u00b7ben", "mischt", "sichs", "nicht", ",", "dort", "bl\u00fcht", "das", "Roth", "da\u00b7zwi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PIS", "PTKNEG", "$,", "ADV", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Besonderstes ist Roth und Allgemeinstes Gr\u00fcn,", "tokens": ["Be\u00b7son\u00b7ders\u00b7tes", "ist", "Roth", "und", "All\u00b7ge\u00b7meins\u00b7tes", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und beide fordern sich, wo Sch\u00f6nstes soll erbl\u00fchn.", "tokens": ["Und", "bei\u00b7de", "for\u00b7dern", "sich", ",", "wo", "Sch\u00f6ns\u00b7tes", "soll", "er\u00b7bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PRF", "$,", "PWAV", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Drum ist der h\u00f6chste Schmuck, vom Lenz der Welt verliehn,", "tokens": ["Drum", "ist", "der", "h\u00f6chs\u00b7te", "Schmuck", ",", "vom", "Lenz", "der", "Welt", "ver\u00b7liehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "ADJA", "NN", "$,", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf Thronen von Smaragd die Rose von Rubin.", "tokens": ["Auf", "Thro\u00b7nen", "von", "Sma\u00b7ragd", "die", "Ro\u00b7se", "von", "Ru\u00b7bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "ART", "NN", "APPR", "NE", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}