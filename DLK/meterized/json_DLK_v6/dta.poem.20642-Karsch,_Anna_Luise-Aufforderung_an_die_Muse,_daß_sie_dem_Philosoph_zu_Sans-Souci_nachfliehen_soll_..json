{"dta.poem.20642": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Aufforderung an die Muse,  \n da\u00df sie  \n  dem Philosoph zu Sans-Souci  \n nachfliehen soll .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du S\u00e4ngerin, tonvolle Muse flieh;                 ", "tokens": ["Du", "S\u00e4n\u00b7ge\u00b7rin", ",", "ton\u00b7vol\u00b7le", "Mu\u00b7se", "flieh", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erato, schwinge dich jezt leichter jezt geschwinder!", "tokens": ["E\u00b7ra\u00b7to", ",", "schwin\u00b7ge", "dich", "jezt", "leich\u00b7ter", "jezt", "ge\u00b7schwin\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADV", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sey wie des Helden Blick, und sieh:", "tokens": ["Sey", "wie", "des", "Hel\u00b7den", "Blick", ",", "und", "sieh", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "NN", "$,", "KON", "VVIMP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Von Seinem Thron nach ", "tokens": ["Von", "Sei\u00b7nem", "Thron", "nach"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ging ", "tokens": ["Ging"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Nahm mit sich die Philosophie,", "tokens": ["Nahm", "mit", "sich", "die", "Phi\u00b7lo\u00b7so\u00b7phie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Ihn begleiteten Apollo\u2019s sch\u00f6nste Kinder,", "tokens": ["Und", "Ihn", "be\u00b7glei\u00b7te\u00b7ten", "A\u00b7pol\u00b7lo's", "sch\u00f6ns\u00b7te", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NE", "ADJA", "NN", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "Calliope, Melpomene,", "tokens": ["Cal\u00b7li\u00b7o\u00b7pe", ",", "Mel\u00b7po\u00b7me\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Thalia warfen ihre Kr\u00e4nze", "tokens": ["Tha\u00b7lia", "war\u00b7fen", "ih\u00b7re", "Kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ihm an die Brust; und bey der Spree", "tokens": ["Ihm", "an", "die", "Brust", ";", "und", "bey", "der", "Spree"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "$.", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Stritt die Najade mit dem Lenze,", "tokens": ["Stritt", "die", "Na\u00b7ja\u00b7de", "mit", "dem", "Len\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df Er mit keiner gr\u00fcnen Pracht", "tokens": ["Da\u00df", "Er", "mit", "kei\u00b7ner", "gr\u00fc\u00b7nen", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Dem gr\u00f6\u00dften K\u00f6nige das Ufer sch\u00f6n gemacht.", "tokens": ["Dem", "gr\u00f6\u00df\u00b7ten", "K\u00f6\u00b7ni\u00b7ge", "das", "U\u00b7fer", "sch\u00f6n", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Fr\u00fchling wandte sich, und lie\u00df die Nymphe klagen,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "wand\u00b7te", "sich", ",", "und", "lie\u00df", "die", "Nym\u00b7phe", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "KON", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und l\u00e4chelte dem Helden nach,", "tokens": ["Und", "l\u00e4\u00b7chel\u00b7te", "dem", "Hel\u00b7den", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der von dem Streitro\u00df ward getragen,", "tokens": ["Der", "von", "dem", "Strei\u00b7tro\u00df", "ward", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das mit Ihm durch die Feldschlacht brach", "tokens": ["Das", "mit", "Ihm", "durch", "die", "Feld\u00b7schlacht", "brach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bey Liegnitz und bey Torgau, schnaubend", "tokens": ["Bey", "Lie\u00b7gnitz", "und", "bey", "Tor\u00b7gau", ",", "schnau\u00b7bend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NE", "$,", "ADJD"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Aus seiner Nase Dampf und Glut,", "tokens": ["Aus", "sei\u00b7ner", "Na\u00b7se", "Dampf", "und", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und donnernd mit dem Huf, wenn hei\u00dfe Feindeswut", "tokens": ["Und", "don\u00b7nernd", "mit", "dem", "Huf", ",", "wenn", "hei\u00b7\u00dfe", "Fein\u00b7des\u00b7wut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nach ", "tokens": ["Nach"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "Dies edle Ro\u00df, von Menschenblut", "tokens": ["Dies", "ed\u00b7le", "Ro\u00df", ",", "von", "Men\u00b7schen\u00b7blut"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "ADJA", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Oft roth gef\u00e4rbt, bis an die M\u00e4hne,", "tokens": ["Oft", "roth", "ge\u00b7f\u00e4rbt", ",", "bis", "an", "die", "M\u00e4h\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,", "KOUS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Trug den Monarchen jezt durch Fluren, wo die Thr\u00e4ne", "tokens": ["Trug", "den", "Mon\u00b7ar\u00b7chen", "jezt", "durch", "Flu\u00b7ren", ",", "wo", "die", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADV", "APPR", "NN", "$,", "PWAV", "ART", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Des Wolkenhimmels, in der Nacht", "tokens": ["Des", "Wol\u00b7ken\u00b7him\u00b7mels", ",", "in", "der", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Staub gel\u00f6schet und moo\u00dfweiche Bahn gemacht,", "tokens": ["Den", "Staub", "ge\u00b7l\u00f6\u00b7schet", "und", "moo\u00df\u00b7wei\u00b7che", "Bahn", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+++-+-+", "measure": "unknown.measure.septa"}, "line.6": {"text": "Und Veilchen schnell hervorgebracht,", "tokens": ["Und", "Veil\u00b7chen", "schnell", "her\u00b7vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf beyden Seiten Ihm zu bl\u00fchen.", "tokens": ["Auf", "bey\u00b7den", "Sei\u00b7ten", "Ihm", "zu", "bl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Er kam, und Ph\u00f6bus fuhr in aller seiner Pracht", "tokens": ["Er", "kam", ",", "und", "Ph\u00f6\u00b7bus", "fuhr", "in", "al\u00b7ler", "sei\u00b7ner", "Pracht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "NE", "VVFIN", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dicht \u00fcber ", "tokens": ["Dicht", "\u00fc\u00b7ber"], "token_info": ["word", "word"], "pos": ["ADJD", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Ihr steinern Haupt, verwundrungsvoll,", "tokens": ["Ihr", "stei\u00b7nern", "Haupt", ",", "ver\u00b7wund\u00b7rungs\u00b7voll", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Da, wo in goldner Zeit sein Saitenspiel erscholl.", "tokens": ["Da", ",", "wo", "in", "gold\u00b7ner", "Zeit", "sein", "Sai\u00b7ten\u00b7spiel", "er\u00b7scholl", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "APPR", "ADJA", "NN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Pompejus neigte den C\u00e4saren", "tokens": ["Pom\u00b7pe\u00b7jus", "neig\u00b7te", "den", "C\u00e4\u00b7sa\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Die Stirne zu, verga\u00df in diesem Augenblick,", "tokens": ["Die", "Stir\u00b7ne", "zu", ",", "ver\u00b7ga\u00df", "in", "die\u00b7sem", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df beyde seines Ruhms und Hauses Feinde waren;", "tokens": ["Da\u00df", "bey\u00b7de", "sei\u00b7nes", "Ruhms", "und", "Hau\u00b7ses", "Fein\u00b7de", "wa\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "KON", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und frug: \u201eWie? Kommt der Held zur\u00fcck?", "tokens": ["Und", "frug", ":", "\u201e", "Wie", "?", "Kommt", "der", "Held", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PWAV", "$.", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201everberget euch mit mir, o R\u00f6mer! alle Celten,", "tokens": ["\u201e", "ver\u00b7ber\u00b7get", "euch", "mit", "mir", ",", "o", "R\u00f6\u00b7mer", "!", "al\u00b7le", "Cel\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "APPR", "PPER", "$,", "FM", "NN", "$.", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eambronen, Gallier, und was aus beyden Welten", "tokens": ["\u201e", "am\u00b7bro\u00b7nen", ",", "Gal\u00b7lier", ",", "und", "was", "aus", "bey\u00b7den", "Wel\u00b7ten"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVINF", "$,", "NN", "$,", "KON", "PWS", "APPR", "PIAT", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "\u201esich jemals wider Rom erk\u00fchnt!", "tokens": ["\u201e", "sich", "je\u00b7mals", "wi\u00b7der", "Rom", "er\u00b7k\u00fchnt", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRF", "ADV", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u201ebezwungne Parther, Thracier und Scythen,", "tokens": ["\u201e", "be\u00b7zwung\u00b7ne", "Par\u00b7ther", ",", "Thra\u00b7cier", "und", "Scyt\u00b7hen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "\u201edie grimmig noch zu seyn gefesselt sich bem\u00fchten;", "tokens": ["\u201e", "die", "grim\u00b7mig", "noch", "zu", "seyn", "ge\u00b7fes\u00b7selt", "sich", "be\u00b7m\u00fch\u00b7ten", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJD", "ADV", "PTKZU", "VAINF", "VVFIN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201edenn jeglicher Triumph verdient", "tokens": ["\u201e", "denn", "jeg\u00b7li\u00b7cher", "Tri\u00b7umph", "ver\u00b7dient"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "KON", "PIAT", "NN", "VVPP"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "\u201enicht mehr den Namen; seht die gr\u00f6\u00dfern Lorbeerreiser", "tokens": ["\u201e", "nicht", "mehr", "den", "Na\u00b7men", ";", "seht", "die", "gr\u00f6\u00b7\u00dfern", "Lor\u00b7beer\u00b7rei\u00b7ser"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PTKNEG", "ADV", "ART", "NN", "$.", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eum unsers ", "tokens": ["\u201e", "um", "un\u00b7sers"], "token_info": ["punct", "word", "word"], "pos": ["$(", "APPR", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Er sprachs: da b\u00fcckten sich Roms hochber\u00fchmte Kayser;", "tokens": ["Er", "sprachs", ":", "da", "b\u00fcck\u00b7ten", "sich", "Roms", "hoch\u00b7be\u00b7r\u00fchm\u00b7te", "Kay\u00b7ser", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "VVFIN", "PRF", "NE", "ADJA", "NN", "$."], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Pompejens Lobspruch ward geglaubt.", "tokens": ["Pom\u00b7pe\u00b7jens", "Lob\u00b7spruch", "ward", "ge\u00b7glaubt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Die C\u00e4sars schwiegen, und die Griechen", "tokens": ["Die", "C\u00e4\u00b7sars", "schwie\u00b7gen", ",", "und", "die", "Grie\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Vom Agamemnon an bis zum Leonidas,", "tokens": ["Vom", "A\u00b7ga\u00b7mem\u00b7non", "an", "bis", "zum", "Leo\u00b7ni\u00b7das", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPR", "APPRART", "NE", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.14": {"text": "Belegten seiner nun vers\u00f6hnten Feindin Ha\u00df,", "tokens": ["Be\u00b7leg\u00b7ten", "sei\u00b7ner", "nun", "ver\u00b7s\u00f6hn\u00b7ten", "Fein\u00b7din", "Ha\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADV", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Laconisch mit schuldlosen Fl\u00fcchen,", "tokens": ["La\u00b7co\u00b7nisch", "mit", "schuld\u00b7lo\u00b7sen", "Fl\u00fc\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Weil Sie, durch ihren dritten Krieg,", "tokens": ["Weil", "Sie", ",", "durch", "ih\u00b7ren", "drit\u00b7ten", "Krieg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Den Ruhm des Siegers, der schon alles \u00fcberstieg,", "tokens": ["Den", "Ruhm", "des", "Sie\u00b7gers", ",", "der", "schon", "al\u00b7les", "\u00fc\u00b7bers\u00b7tieg", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Bis zur Unsterblichkeit erhoben \u2014", "tokens": ["Bis", "zur", "U\u00b7nsterb\u00b7lich\u00b7keit", "er\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "VVPP", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.19": {"text": "O Muse! sangst du Seinen Sieg,", "tokens": ["O", "Mu\u00b7se", "!", "sangst", "du", "Sei\u00b7nen", "Sieg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "So kenn auch deine Pflicht, ", "tokens": ["So", "kenn", "auch", "dei\u00b7ne", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}