{"textgrid.poem.55953": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf einmal ist aus allem Gr\u00fcn im Park", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf einmal ist aus allem Gr\u00fcn im Park", "tokens": ["Auf", "ein\u00b7mal", "ist", "aus", "al\u00b7lem", "Gr\u00fcn", "im", "Park"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VAFIN", "APPR", "PIS", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "man wei\u00df nicht was, ein Etwas, fortgenommen;", "tokens": ["man", "wei\u00df", "nicht", "was", ",", "ein", "Et\u00b7was", ",", "fort\u00b7ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "PIS", "$,", "ART", "ADV", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "man f\u00fchlt ihn n\u00e4her an die Fenster kommen", "tokens": ["man", "f\u00fchlt", "ihn", "n\u00e4\u00b7her", "an", "die", "Fens\u00b7ter", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und schweigsam sein. Inst\u00e4ndig nur und stark", "tokens": ["und", "schweig\u00b7sam", "sein", ".", "In\u00b7st\u00e4n\u00b7dig", "nur", "und", "stark"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAINF", "$.", "ADJD", "ADV", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "ert\u00f6nt aus dem Geh\u00f6lz der Regenpfeifer,", "tokens": ["er\u00b7t\u00f6nt", "aus", "dem", "Ge\u00b7h\u00f6lz", "der", "Re\u00b7gen\u00b7pfei\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "man denkt an einen Hieronymus:", "tokens": ["man", "denkt", "an", "ei\u00b7nen", "Hie\u00b7ro\u00b7ny\u00b7mus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NE", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "so sehr steigt irgend Einsamkeit und Eifer", "tokens": ["so", "sehr", "steigt", "ir\u00b7gend", "Ein\u00b7sam\u00b7keit", "und", "Ei\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "aus dieser einen Stimme, die der Gu\u00df", "tokens": ["aus", "die\u00b7ser", "ei\u00b7nen", "Stim\u00b7me", ",", "die", "der", "Gu\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "erh\u00f6ren wird. Des Saales W\u00e4nde sind", "tokens": ["er\u00b7h\u00f6\u00b7ren", "wird", ".", "Des", "Saa\u00b7les", "W\u00e4n\u00b7de", "sind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVINF", "VAFIN", "$.", "ART", "NN", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "mit ihren Bildern von uns fortgetreten,", "tokens": ["mit", "ih\u00b7ren", "Bil\u00b7dern", "von", "uns", "fort\u00b7ge\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "als d\u00fcrften sie nicht h\u00f6ren was wir sagen.", "tokens": ["als", "d\u00fcrf\u00b7ten", "sie", "nicht", "h\u00f6\u00b7ren", "was", "wir", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "PTKNEG", "VVINF", "PWS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es spiegeln die verblichenen Tapeten", "tokens": ["Es", "spie\u00b7geln", "die", "ver\u00b7bli\u00b7che\u00b7nen", "Ta\u00b7pe\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "das ungewisse Licht von Nachmittagen,", "tokens": ["das", "un\u00b7ge\u00b7wis\u00b7se", "Licht", "von", "Nach\u00b7mit\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "in denen man sich f\u00fcrchtete als Kind.", "tokens": ["in", "de\u00b7nen", "man", "sich", "f\u00fcrch\u00b7te\u00b7te", "als", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "PRF", "VVFIN", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}