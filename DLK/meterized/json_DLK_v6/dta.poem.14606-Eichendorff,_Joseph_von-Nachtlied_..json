{"dta.poem.14606": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Nachtlied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Frisch auf denn, liebe Nachtigall,", "tokens": ["Frisch", "auf", "denn", ",", "lie\u00b7be", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADV", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du Wasserfall mit hellem Schall!", "tokens": ["Du", "Was\u00b7ser\u00b7fall", "mit", "hel\u00b7lem", "Schall", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gott loben wollen wir vereint,", "tokens": ["Gott", "lo\u00b7ben", "wol\u00b7len", "wir", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VMFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis da\u00df der lichte Morgen scheint!", "tokens": ["Bis", "da\u00df", "der", "lich\u00b7te", "Mor\u00b7gen", "scheint", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}