{"textgrid.poem.61883": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Sp\u00e4therbstlaube steht mein Leben,", "tokens": ["Im", "Sp\u00e4t\u00b7herbst\u00b7lau\u00b7be", "steht", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Ende ging das frohe Spiel,", "tokens": ["Zu", "En\u00b7de", "ging", "das", "fro\u00b7he", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Sonn' erbla\u00dft, die Nebel weben,", "tokens": ["Die", "Sonn'", "er\u00b7bla\u00dft", ",", "die", "Ne\u00b7bel", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und bald, ich f\u00fchl's, bin ich am Ziel.", "tokens": ["Und", "bald", ",", "ich", "f\u00fchl's", ",", "bin", "ich", "am", "Ziel", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch nicht in klagenden Akkorden", "tokens": ["Doch", "nicht", "in", "kla\u00b7gen\u00b7den", "Ak\u00b7kor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hinsterben soll mein Harfenschlag,", "tokens": ["Hins\u00b7ter\u00b7ben", "soll", "mein", "Har\u00b7fen\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Zwei Freuden sind mir noch geworden,", "tokens": ["Zwei", "Freu\u00b7den", "sind", "mir", "noch", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "ADV", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Drum ich begl\u00fcckt mich preisen mag.", "tokens": ["Drum", "ich", "be\u00b7gl\u00fcckt", "mich", "prei\u00b7sen", "mag."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PAV", "PPER", "VVFIN", "PPER", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich sah mit Augen noch die Siege", "tokens": ["Ich", "sah", "mit", "Au\u00b7gen", "noch", "die", "Sie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des deutschen Volks und sah das Reich", "tokens": ["Des", "deut\u00b7schen", "Volks", "und", "sah", "das", "Reich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und legt' auf eines Enkels Wiege", "tokens": ["Und", "legt'", "auf", "ei\u00b7nes", "En\u00b7kels", "Wie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den frisch erk\u00e4mpften Eichenzweig.", "tokens": ["Den", "frisch", "er\u00b7k\u00e4mpf\u00b7ten", "Ei\u00b7chen\u00b7zweig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}