{"textgrid.poem.55870": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einst als am Saum der W\u00fcsten sich", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst als am Saum der W\u00fcsten sich", "tokens": ["Einst", "als", "am", "Saum", "der", "W\u00fcs\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "APPRART", "NN", "ART", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "auftat die Hand des Herrn", "tokens": ["auf\u00b7tat", "die", "Hand", "des", "Herrn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "wie eine Frucht, die sommerlich", "tokens": ["wie", "ei\u00b7ne", "Frucht", ",", "die", "som\u00b7mer\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "verk\u00fcndet ihren Kern,", "tokens": ["ver\u00b7k\u00fcn\u00b7det", "ih\u00b7ren", "Kern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "da war ein Wunder: Fern", "tokens": ["da", "war", "ein", "Wun\u00b7der", ":", "Fern"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$.", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "erkannten und begr\u00fc\u00dften sich", "tokens": ["er\u00b7kann\u00b7ten", "und", "be\u00b7gr\u00fc\u00df\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "drei K\u00f6nige und ein Stern.", "tokens": ["drei", "K\u00f6\u00b7ni\u00b7ge", "und", "ein", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "KON", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Drei K\u00f6nige von Unterwegs", "tokens": ["Drei", "K\u00f6\u00b7ni\u00b7ge", "von", "Un\u00b7ter\u00b7wegs"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und der Stern \u00dcberall,", "tokens": ["und", "der", "Stern", "\u00dc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,"], "meter": "--++-+", "measure": "anapaest.init"}, "line.3": {"text": "die zogen alle (\u00fcberlegs!)", "tokens": ["die", "zo\u00b7gen", "al\u00b7le", "(", "\u00fc\u00b7ber\u00b7legs", "!", ")"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ART", "VVFIN", "PIAT", "$(", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "so rechts ein Rex und links ein Rex", "tokens": ["so", "rechts", "ein", "Rex", "und", "links", "ein", "Rex"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "KON", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "zu einem stillen Stall.", "tokens": ["zu", "ei\u00b7nem", "stil\u00b7len", "Stall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was brachten die nicht alles mit", "tokens": ["Was", "brach\u00b7ten", "die", "nicht", "al\u00b7les", "mit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "PTKNEG", "PIS", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "zum Stall von Bethlehem!", "tokens": ["zum", "Stall", "von", "Beth\u00b7le\u00b7hem", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weithin erklirrte jeder Schritt,", "tokens": ["Weit\u00b7hin", "er\u00b7klirr\u00b7te", "je\u00b7der", "Schritt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und der auf einem Rappen ritt,", "tokens": ["und", "der", "auf", "ei\u00b7nem", "Rap\u00b7pen", "ritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "sa\u00df samten und bequem.", "tokens": ["sa\u00df", "sam\u00b7ten", "und", "be\u00b7quem", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und der zu seiner Rechten ging,", "tokens": ["Und", "der", "zu", "sei\u00b7ner", "Rech\u00b7ten", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "der war ein goldner Mann,", "tokens": ["der", "war", "ein", "gold\u00b7ner", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "und der zu seiner Linken fing", "tokens": ["und", "der", "zu", "sei\u00b7ner", "Lin\u00b7ken", "fing"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "mit Schwung und Schwing", "tokens": ["mit", "Schwung", "und", "Schwing"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "und Klang und Kling", "tokens": ["und", "Klang", "und", "Kling"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "aus einem runden Silberding,", "tokens": ["aus", "ei\u00b7nem", "run\u00b7den", "Sil\u00b7ber\u00b7ding", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "das wiegend und in Ringen hing,", "tokens": ["das", "wie\u00b7gend", "und", "in", "Rin\u00b7gen", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVPP", "KON", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "ganz blau zu rauchen an.", "tokens": ["ganz", "blau", "zu", "rau\u00b7chen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Da lachte der Stern \u00dcberall", "tokens": ["Da", "lach\u00b7te", "der", "Stern", "\u00dc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "so seltsam \u00fcber sie,", "tokens": ["so", "selt\u00b7sam", "\u00fc\u00b7ber", "sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "und lief voraus und stand am Stall", "tokens": ["und", "lief", "vo\u00b7raus", "und", "stand", "am", "Stall"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "und sagte zu Marie:", "tokens": ["und", "sag\u00b7te", "zu", "Ma\u00b7rie", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da bring ich eine Wanderschaft", "tokens": ["Da", "bring", "ich", "ei\u00b7ne", "Wan\u00b7der\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "aus vieler Fremde her.", "tokens": ["aus", "vie\u00b7ler", "Frem\u00b7de", "her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drei K\u00f6nige mit ", "tokens": ["Drei", "K\u00f6\u00b7ni\u00b7ge", "mit"], "token_info": ["word", "word", "word"], "pos": ["CARD", "NN", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "von Gold und Topas schwer", "tokens": ["von", "Gold", "und", "To\u00b7pas", "schwer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "und dunkel, tumb und heidenhaft, \u2013", "tokens": ["und", "dun\u00b7kel", ",", "tumb", "und", "hei\u00b7den\u00b7haft", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "$,", "VVFIN", "KON", "ADJD", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "erschrick mir nicht zu sehr.", "tokens": ["er\u00b7schrick", "mir", "nicht", "zu", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "PTKA", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie haben alle drei zuhaus", "tokens": ["Sie", "ha\u00b7ben", "al\u00b7le", "drei", "zu\u00b7haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "CARD", "XY"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "zw\u00f6lf T\u00f6chter, keinen Sohn,", "tokens": ["zw\u00f6lf", "T\u00f6ch\u00b7ter", ",", "kei\u00b7nen", "Sohn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "so bitten sie sich deinen aus", "tokens": ["so", "bit\u00b7ten", "sie", "sich", "dei\u00b7nen", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PPOSAT", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "als Sonne ihres Himmelblaus", "tokens": ["als", "Son\u00b7ne", "ih\u00b7res", "Him\u00b7mel\u00b7blaus"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "und Trost f\u00fcr ihren Thron.", "tokens": ["und", "Trost", "f\u00fcr", "ih\u00b7ren", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Doch mu\u00dft du nicht gleich glauben: blo\u00df", "tokens": ["Doch", "mu\u00dft", "du", "nicht", "gleich", "glau\u00b7ben", ":", "blo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$.", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "ein Funkelf\u00fcrst und Heidenscheich", "tokens": ["ein", "Fun\u00b7kel\u00b7f\u00fcrst", "und", "Hei\u00b7den\u00b7scheich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "sei deines Sohnes Los.", "tokens": ["sei", "dei\u00b7nes", "Soh\u00b7nes", "Los", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Bedenk, der Weg ist gro\u00df.", "tokens": ["Be\u00b7denk", ",", "der", "Weg", "ist", "gro\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Sie wandern lange, Hirten gleich,", "tokens": ["Sie", "wan\u00b7dern", "lan\u00b7ge", ",", "Hir\u00b7ten", "gleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "inzwischen f\u00e4llt ihr reifes Reich", "tokens": ["in\u00b7zwi\u00b7schen", "f\u00e4llt", "ihr", "rei\u00b7fes", "Reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "wei\u00df Gott wem in den Schoo\u00df.", "tokens": ["wei\u00df", "Gott", "wem", "in", "den", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PWS", "APPR", "ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.19": {"text": "Und w\u00e4hrend hier, wie Westwind warm,", "tokens": ["Und", "w\u00e4h\u00b7rend", "hier", ",", "wie", "West\u00b7wind", "warm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "$,", "PWAV", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "der Ochs ihr Ohr umschnaubt,", "tokens": ["der", "Ochs", "ihr", "Ohr", "um\u00b7schnaubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "sind sie vielleicht schon alle arm", "tokens": ["sind", "sie", "viel\u00b7leicht", "schon", "al\u00b7le", "arm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PIS", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "und so wie ohne Haupt.", "tokens": ["und", "so", "wie", "oh\u00b7ne", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Drum mach mit deinem L\u00e4cheln licht", "tokens": ["Drum", "mach", "mit", "dei\u00b7nem", "L\u00e4\u00b7cheln", "licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "die Wirrnis, die sie sind,", "tokens": ["die", "Wirr\u00b7nis", ",", "die", "sie", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "und wende du dein Angesicht", "tokens": ["und", "wen\u00b7de", "du", "dein", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "nach Aufgang und dein Kind;", "tokens": ["nach", "Auf\u00b7gang", "und", "dein", "Kind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "dort liegt in blauen Linien,", "tokens": ["dort", "liegt", "in", "blau\u00b7en", "Li\u00b7ni\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.28": {"text": "was jeder dir verlie\u00df:", "tokens": ["was", "je\u00b7der", "dir", "ver\u00b7lie\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Smaragda und Rubinien", "tokens": ["Sma\u00b7rag\u00b7da", "und", "Ru\u00b7bi\u00b7ni\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "und die Tale von T\u00fcrkis.", "tokens": ["und", "die", "Ta\u00b7le", "von", "T\u00fcr\u00b7kis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NE", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}}}}