{"textgrid.poem.60050": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das arme Veilchen! Sieh, o sieh!", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das arme Veilchen! Sieh, o sieh!", "tokens": ["Das", "ar\u00b7me", "Veil\u00b7chen", "!", "Sieh", ",", "o", "sieh", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NE", "$,", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da lebt's in todtem Moos!", "tokens": ["Da", "lebt's", "in", "tod\u00b7tem", "Moos", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kommst, armes Veilchen, kommst zu fr\u00fch", "tokens": ["Kommst", ",", "ar\u00b7mes", "Veil\u00b7chen", ",", "kommst", "zu", "fr\u00fch"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "VVFIN", "PTKA", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus deiner Mutter Schoo\u00df!", "tokens": ["Aus", "dei\u00b7ner", "Mut\u00b7ter", "Schoo\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Lebst Einen Morgen, jammerst mich;", "tokens": ["Lebst", "Ei\u00b7nen", "Mor\u00b7gen", ",", "jam\u00b7merst", "mich", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Siehst weder Laub noch Gras;", "tokens": ["Siehst", "we\u00b7der", "Laub", "noch", "Gras", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "NN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit seinem Fittig mordet dich", "tokens": ["Mit", "sei\u00b7nem", "Fit\u00b7tig", "mor\u00b7det", "dich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der M\u00f6rder Boreas.", "tokens": ["Der", "M\u00f6r\u00b7der", "Bo\u00b7re\u00b7as", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mu\u00dft sterben, Veilchen! weil du mu\u00dft,", "tokens": ["Mu\u00dft", "ster\u00b7ben", ",", "Veil\u00b7chen", "!", "weil", "du", "mu\u00dft", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "NN", "$.", "KOUS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Alt einen Tag; o weh!", "tokens": ["Alt", "ei\u00b7nen", "Tag", ";", "o", "weh", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$.", "FM", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So stirb an meines M\u00e4dchens Brust,", "tokens": ["So", "stirb", "an", "mei\u00b7nes", "M\u00e4d\u00b7chens", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich dich sterben seh.", "tokens": ["Da\u00df", "ich", "dich", "ster\u00b7ben", "seh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da b\u00fcckt sichs, und mit nassem Blick", "tokens": ["Da", "b\u00fcckt", "sichs", ",", "und", "mit", "nas\u00b7sem", "Blick"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Siehts nieder, bricht dich ab;", "tokens": ["Siehts", "nie\u00b7der", ",", "bricht", "dich", "ab", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "PTKVZ", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Stirbst, Veilchen! gehst zu dem zur\u00fcck,", "tokens": ["Stirbst", ",", "Veil\u00b7chen", "!", "gehst", "zu", "dem", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "VVFIN", "APPR", "ART", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der dir das Leben gab.", "tokens": ["Der", "dir", "das", "Le\u00b7ben", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Stirbst, Veilchen! liegst, ein wenig Staub;", "tokens": ["Stirbst", ",", "Veil\u00b7chen", "!", "liegst", ",", "ein", "we\u00b7nig", "Staub", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "VVFIN", "$,", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein wenig Staub auch wir,", "tokens": ["Ein", "we\u00b7nig", "Staub", "auch", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "So gut wie du, des Todes Raub,", "tokens": ["So", "gut", "wie", "du", ",", "des", "To\u00b7des", "Raub", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Einst liegen, nahe dir.", "tokens": ["Einst", "lie\u00b7gen", ",", "na\u00b7he", "dir", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Stirbst, Veilchen! duftest deinen Geist", "tokens": ["Stirbst", ",", "Veil\u00b7chen", "!", "duf\u00b7test", "dei\u00b7nen", "Geist"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$.", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In kalte Winterluft;", "tokens": ["In", "kal\u00b7te", "Win\u00b7ter\u00b7luft", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bleibst Wesen, Veilchen! Wie es hei\u00dft?", "tokens": ["Bleibst", "We\u00b7sen", ",", "Veil\u00b7chen", "!", "Wie", "es", "hei\u00dft", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "$.", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ob Monas, oder Duft?", "tokens": ["Ob", "Mo\u00b7nas", ",", "o\u00b7der", "Duft", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Obs h\u00f6her aufgestiegen ist", "tokens": ["Obs", "h\u00f6\u00b7her", "auf\u00b7ge\u00b7stie\u00b7gen", "ist"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Sch\u00f6pfers Angesicht?", "tokens": ["In", "Sch\u00f6p\u00b7fers", "An\u00b7ge\u00b7sicht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob Engel oder Milbe bist?", "tokens": ["Ob", "En\u00b7gel", "o\u00b7der", "Mil\u00b7be", "bist", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das, Veilchen! wei\u00df ich nicht.", "tokens": ["Das", ",", "Veil\u00b7chen", "!", "wei\u00df", "ich", "nicht", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wei\u00df aber, da\u00df in Sch\u00f6pfers Hand", "tokens": ["Wei\u00df", "a\u00b7ber", ",", "da\u00df", "in", "Sch\u00f6p\u00b7fers", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl aufgehoben Laub", "tokens": ["Wohl", "auf\u00b7ge\u00b7ho\u00b7ben", "Laub"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVPP", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Ceder ist, und Meer und Land,", "tokens": ["Und", "Ce\u00b7der", "ist", ",", "und", "Meer", "und", "Land", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "$,", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Sonn' und Sonnenstaub.", "tokens": ["Und", "Sonn'", "und", "Son\u00b7nen\u00b7staub", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Deswegen wir mit nassem Blick", "tokens": ["Des\u00b7we\u00b7gen", "wir", "mit", "nas\u00b7sem", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht sehn in unser Grab:", "tokens": ["Nicht", "sehn", "in", "un\u00b7ser", "Grab", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Genug! wir gehn zu dem zur\u00fcck,", "tokens": ["Ge\u00b7nug", "!", "wir", "gehn", "zu", "dem", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VVFIN", "APPR", "ART", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der uns das Leben gab.", "tokens": ["Der", "uns", "das", "Le\u00b7ben", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}