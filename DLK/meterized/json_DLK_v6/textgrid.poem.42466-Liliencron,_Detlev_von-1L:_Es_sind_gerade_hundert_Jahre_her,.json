{"textgrid.poem.42466": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es sind gerade hundert Jahre her,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sind gerade hundert Jahre her,", "tokens": ["Es", "sind", "ge\u00b7ra\u00b7de", "hun\u00b7dert", "Jah\u00b7re", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als Deutschland in der tiefsten Schande lag.", "tokens": ["Als", "Deutschland", "in", "der", "tiefs\u00b7ten", "Schan\u00b7de", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es sind gerade hundert Jahre her,", "tokens": ["Es", "sind", "ge\u00b7ra\u00b7de", "hun\u00b7dert", "Jah\u00b7re", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als Kleist sein Schauspiel schrieb: Die Herrmannsschlacht.", "tokens": ["Als", "Kleist", "sein", "Schau\u00b7spiel", "schrieb", ":", "Die", "Herr\u00b7manns\u00b7schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "VVFIN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er schrieb es voller Ha\u00df und Wutgest\u00f6hn,", "tokens": ["Er", "schrieb", "es", "vol\u00b7ler", "Ha\u00df", "und", "Wut\u00b7ge\u00b7st\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df mancher Vers den rechten Takt verlor,", "tokens": ["Da\u00df", "man\u00b7cher", "Vers", "den", "rech\u00b7ten", "Takt", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So wild und au\u00dfer sich schrieb er sein Drama.", "tokens": ["So", "wild", "und", "au\u00b7\u00dfer", "sich", "schrieb", "er", "sein", "Dra\u00b7ma", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "APPR", "PRF", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Und jeder, der die Handschrift las, fand drin", "tokens": ["Und", "je\u00b7der", ",", "der", "die", "Hand\u00b7schrift", "las", ",", "fand", "drin"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PIS", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die \u00c4hnlichkeit, die zwischen Rom von ehmals", "tokens": ["Die", "\u00c4hn\u00b7lich\u00b7keit", ",", "die", "zwi\u00b7schen", "Rom", "von", "eh\u00b7mals"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NE", "APPR", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und jenem unerh\u00f6rten Zwingherrn war,", "tokens": ["Und", "je\u00b7nem", "un\u00b7er\u00b7h\u00f6r\u00b7ten", "Zwing\u00b7herrn", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Der unser Vaterland in Ketten warf:", "tokens": ["Der", "un\u00b7ser", "Va\u00b7ter\u00b7land", "in", "Ket\u00b7ten", "warf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Napoleon. Der Dichter starb. Sein St\u00fcck", "tokens": ["Na\u00b7po\u00b7le\u00b7on", ".", "Der", "Dich\u00b7ter", "starb", ".", "Sein", "St\u00fcck"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$.", "ART", "NN", "VVFIN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ward jahrelang nach seinem Tode erst", "tokens": ["Ward", "jah\u00b7re\u00b7lang", "nach", "sei\u00b7nem", "To\u00b7de", "erst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Gedruckt. Und sp\u00e4rlich war die Auff\u00fchrung", "tokens": ["Ge\u00b7druckt", ".", "Und", "sp\u00e4r\u00b7lich", "war", "die", "Auf\u00b7f\u00fch\u00b7rung"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$.", "KON", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.15": {"text": "Bis jetzt. Der gro\u00dfe, ungl\u00fcckliche Dichter", "tokens": ["Bis", "jetzt", ".", "Der", "gro\u00b7\u00dfe", ",", "un\u00b7gl\u00fcck\u00b7li\u00b7che", "Dich\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADV", "$.", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Hats niemals auf der B\u00fchne wirken sehn.", "tokens": ["Hats", "nie\u00b7mals", "auf", "der", "B\u00fch\u00b7ne", "wir\u00b7ken", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Nichts ist darin von Ebenma\u00df und Wohlklang;", "tokens": ["Nichts", "ist", "da\u00b7rin", "von", "E\u00b7ben\u00b7ma\u00df", "und", "Wohl\u00b7klang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PAV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Nur das Genie spricht hart aus jedem Wort,", "tokens": ["Nur", "das", "Ge\u00b7nie", "spricht", "hart", "aus", "je\u00b7dem", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Aus jedem Vers schreit sein emp\u00f6rtes Herz.", "tokens": ["Aus", "je\u00b7dem", "Vers", "schreit", "sein", "em\u00b7p\u00f6r\u00b7tes", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zum Andenken an Bismarcks Todestag,", "tokens": ["Zum", "An\u00b7den\u00b7ken", "an", "Bis\u00b7marcks", "To\u00b7des\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Der vor zehn Jahren alle Welt durchbebte,", "tokens": ["Der", "vor", "zehn", "Jah\u00b7ren", "al\u00b7le", "Welt", "durch\u00b7beb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "CARD", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Soll heute hier die Herrmannsschlacht erscheinen.", "tokens": ["Soll", "heu\u00b7te", "hier", "die", "Herr\u00b7manns\u00b7schlacht", "er\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kein besserer Name kann Kleists R\u00e4cher sein.", "tokens": ["Kein", "bes\u00b7se\u00b7rer", "Na\u00b7me", "kann", "Kleists", "R\u00e4\u00b7cher", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "NE", "NN", "VAINF", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Was er gewollt: das gro\u00dfe Vaterland,", "tokens": ["Was", "er", "ge\u00b7wollt", ":", "das", "gro\u00b7\u00dfe", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMPP", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Bismarck hats durchgesetzt mit seiner Kraft,", "tokens": ["Bis\u00b7marck", "hats", "durch\u00b7ge\u00b7setzt", "mit", "sei\u00b7ner", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Auf erznem Felsgrund steht das Deutsche Reich.", "tokens": ["Auf", "erz\u00b7nem", "Fels\u00b7grund", "steht", "das", "Deut\u00b7sche", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}