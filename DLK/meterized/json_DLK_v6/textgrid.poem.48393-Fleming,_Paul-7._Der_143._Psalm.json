{"textgrid.poem.48393": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "7. Der 143. Psalm", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr, Herr, erh\u00f6re mich und nimb mein Flehen an.", "tokens": ["Herr", ",", "Herr", ",", "er\u00b7h\u00f6\u00b7re", "mich", "und", "nimb", "mein", "Fle\u00b7hen", "an", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "PPER", "KON", "VVIMP", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So was vor dir dein Recht und Wahrheit gelten kan,", "tokens": ["So", "was", "vor", "dir", "dein", "Recht", "und", "Wahr\u00b7heit", "gel\u00b7ten", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "APPR", "PPER", "PPOSAT", "NN", "KON", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so schaffe mir auch Recht. Doch f\u00fchr nicht ins Gerichte", "tokens": ["so", "schaf\u00b7fe", "mir", "auch", "Recht", ".", "Doch", "f\u00fchr", "nicht", "ins", "Ge\u00b7rich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN", "$.", "KON", "VVFIN", "PTKNEG", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "mich, deinen S\u00fcndiger. Was h\u00e4lt wol das Gewichte", "tokens": ["mich", ",", "dei\u00b7nen", "S\u00fcn\u00b7di\u00b7ger", ".", "Was", "h\u00e4lt", "wol", "das", "Ge\u00b7wich\u00b7te"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$.", "PWS", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "f\u00fcr Unschuld deinem Satz? Und wenn die gro\u00dfe Welt", "tokens": ["f\u00fcr", "Un\u00b7schuld", "dei\u00b7nem", "Satz", "?", "Und", "wenn", "die", "gro\u00b7\u00dfe", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$.", "KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "auf einen blachen Platz dir vor die Augen stellt'", "tokens": ["auf", "ei\u00b7nen", "bla\u00b7chen", "Platz", "dir", "vor", "die", "Au\u00b7gen", "stellt'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "ihr ungez\u00e4hltes Volk, so w\u00fcrd' in solchen allen", "tokens": ["ihr", "un\u00b7ge\u00b7z\u00e4hl\u00b7tes", "Volk", ",", "so", "w\u00fcrd'", "in", "sol\u00b7chen", "al\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADV", "VAFIN", "APPR", "PIAT", "PIS"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "ja nicht auf einen nur dein rechtes Urteil fallen,", "tokens": ["ja", "nicht", "auf", "ei\u00b7nen", "nur", "dein", "rech\u00b7tes", "Ur\u00b7teil", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "ART", "ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "da\u00df er sei ohne Schuld. Di\u00df bitt' ich nur allein,", "tokens": ["da\u00df", "er", "sei", "oh\u00b7ne", "Schuld", ".", "Di\u00df", "bitt'", "ich", "nur", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "APPR", "NN", "$.", "PDS", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "da\u00df ich des Feindes Spiel so gar nicht m\u00f6ge sein.", "tokens": ["da\u00df", "ich", "des", "Fein\u00b7des", "Spiel", "so", "gar", "nicht", "m\u00f6\u00b7ge", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "ADV", "ADV", "PTKNEG", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Denn er verfolget mir aufs \u00e4u\u00dferste mein Leben,", "tokens": ["Denn", "er", "ver\u00b7fol\u00b7get", "mir", "aufs", "\u00e4u\u00b7\u00dfers\u00b7te", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPRART", "ADJA", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "ich mu\u00df in steter Furcht f\u00fcr seinem Trutzen schweben.", "tokens": ["ich", "mu\u00df", "in", "ste\u00b7ter", "Furcht", "f\u00fcr", "sei\u00b7nem", "Trut\u00b7zen", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "F\u00fcr ihm verkriech' ich mich, ich bin sein ewger Raub.", "tokens": ["F\u00fcr", "ihm", "ver\u00b7kriech'", "ich", "mich", ",", "ich", "bin", "sein", "ew\u00b7ger", "Raub", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PRF", "$,", "PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ereilet er mich denn, so wirft er mich in Staub", "tokens": ["Er\u00b7ei\u00b7let", "er", "mich", "denn", ",", "so", "wirft", "er", "mich", "in", "Staub"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$,", "ADV", "VVFIN", "PPER", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "und in ein finster Loch, da mich kein Liecht bestralet,", "tokens": ["und", "in", "ein", "fins\u00b7ter", "Loch", ",", "da", "mich", "kein", "Liecht", "be\u00b7stra\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "ich bin den Todten gleich. Wenn er so hoch herpralet,", "tokens": ["ich", "bin", "den", "Tod\u00b7ten", "gleich", ".", "Wenn", "er", "so", "hoch", "her\u00b7pra\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$.", "KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "so \u00e4ngstet sich mein Geist. Mein Herze wird verzehrt,", "tokens": ["so", "\u00e4ngs\u00b7tet", "sich", "mein", "Geist", ".", "Mein", "Her\u00b7ze", "wird", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "da\u00df er ohn' Unterla\u00df so grimmig an mich f\u00e4hrt,", "tokens": ["da\u00df", "er", "ohn'", "Un\u00b7ter\u00b7la\u00df", "so", "grim\u00b7mig", "an", "mich", "f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADV", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "da\u00df ich so h\u00fclflos bin. Denk ich denn an die Werke", "tokens": ["da\u00df", "ich", "so", "h\u00fcl\u00b7flos", "bin", ".", "Denk", "ich", "denn", "an", "die", "Wer\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$.", "NN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "die du vorhin getan durch deiner H\u00e4nde St\u00e4rke,", "tokens": ["die", "du", "vor\u00b7hin", "ge\u00b7tan", "durch", "dei\u00b7ner", "H\u00e4n\u00b7de", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "so tret' ich auch vor dich und bringe sie dir f\u00fcr,", "tokens": ["so", "tret'", "ich", "auch", "vor", "dich", "und", "brin\u00b7ge", "sie", "dir", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "KON", "VVFIN", "PPER", "PPER", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "und bitte, da\u00df du auch so wollest helfen mir.", "tokens": ["und", "bit\u00b7te", ",", "da\u00df", "du", "auch", "so", "wol\u00b7lest", "hel\u00b7fen", "mir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "KOUS", "PPER", "ADV", "ADV", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ich strecke Nacht und Tag zu dir die lassen Arme,", "tokens": ["Ich", "stre\u00b7cke", "Nacht", "und", "Tag", "zu", "dir", "die", "las\u00b7sen", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "nach dir, Herr, durstet mich in diesem d\u00fcrren Harme,", "tokens": ["nach", "dir", ",", "Herr", ",", "durs\u00b7tet", "mich", "in", "die\u00b7sem", "d\u00fcr\u00b7ren", "Har\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "NN", "$,", "VVFIN", "PRF", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "wie ein entsaftet Land, das sich zum Himmel neigt,", "tokens": ["wie", "ein", "ent\u00b7saf\u00b7tet", "Land", ",", "das", "sich", "zum", "Him\u00b7mel", "neigt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "VVPP", "NN", "$,", "PRELS", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "und der erz\u00fcrnten Burg die tiefen Risse zeigt,", "tokens": ["und", "der", "er\u00b7z\u00fcrn\u00b7ten", "Burg", "die", "tie\u00b7fen", "Ris\u00b7se", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "gleich einem Seufzenden. Merk auf, Herr, Herr, erh\u00f6re!", "tokens": ["gleich", "ei\u00b7nem", "Seuf\u00b7zen\u00b7den", ".", "Merk", "auf", ",", "Herr", ",", "Herr", ",", "er\u00b7h\u00f6\u00b7re", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "$.", "NN", "PTKVZ", "$,", "NN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "erschein', erscheine bald in deiner gro\u00dfen Ehre,", "tokens": ["er\u00b7schein'", ",", "er\u00b7schei\u00b7ne", "bald", "in", "dei\u00b7ner", "gro\u00b7\u00dfen", "Eh\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "eh mir der Geist entwischt, der nicht herwiederzeucht,", "tokens": ["eh", "mir", "der", "Geist", "ent\u00b7wischt", ",", "der", "nicht", "her\u00b7wie\u00b7der\u00b7zeucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,", "PRELS", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "wenn er uns einmal nur durch unsre Lippen fleucht!", "tokens": ["wenn", "er", "uns", "ein\u00b7mal", "nur", "durch", "uns\u00b7re", "Lip\u00b7pen", "fleucht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Verbirg dein Antlitz nicht, du Sonne meiner Seelen!", "tokens": ["Ver\u00b7birg", "dein", "Ant\u00b7litz", "nicht", ",", "du", "Son\u00b7ne", "mei\u00b7ner", "See\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PTKNEG", "$,", "PPER", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "sonst werd' ich denen gleich, so in die schwarze H\u00f6len", "tokens": ["sonst", "werd'", "ich", "de\u00b7nen", "gleich", ",", "so", "in", "die", "schwar\u00b7ze", "H\u00f6\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PDS", "ADV", "$,", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "des Todes fahren ab, als wie in einen Schacht,", "tokens": ["des", "To\u00b7des", "fah\u00b7ren", "ab", ",", "als", "wie", "in", "ei\u00b7nen", "Schacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "KOUS", "KOKOM", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "ohn' alle Wiederkunft, und sind in langer Nacht.", "tokens": ["ohn'", "al\u00b7le", "Wie\u00b7der\u00b7kunft", ",", "und", "sind", "in", "lan\u00b7ger", "Nacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "KON", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Herr, s\u00e4ume dich doch nicht! La\u00df deine fr\u00fche Gnade", "tokens": ["Herr", ",", "s\u00e4u\u00b7me", "dich", "doch", "nicht", "!", "La\u00df", "dei\u00b7ne", "fr\u00fc\u00b7he", "Gna\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "PTKNEG", "$.", "VVIMP", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "mir bald zu wissen tun, und da\u00df ich auf dem Pfade,", "tokens": ["mir", "bald", "zu", "wis\u00b7sen", "tun", ",", "und", "da\u00df", "ich", "auf", "dem", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "VVINF", "$,", "KON", "KOUS", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "den du wilst, geh' herein, so mache mir ihn kund!", "tokens": ["den", "du", "wilst", ",", "geh'", "her\u00b7ein", ",", "so", "ma\u00b7che", "mir", "ihn", "kund", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VMFIN", "$,", "VVFIN", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Nach dir Herr, Herr, nach dir seufz' ich mit Seel und Mund'.", "tokens": ["Nach", "dir", "Herr", ",", "Herr", ",", "nach", "dir", "seuf\u00b7z'", "ich", "mit", "Seel", "und", "Mund'", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "$,", "NN", "$,", "APPR", "PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.39": {"text": "Ich hoffe blo\u00df auf dich. Gott, du bist mein Erretter,", "tokens": ["Ich", "hof\u00b7fe", "blo\u00df", "auf", "dich", ".", "Gott", ",", "du", "bist", "mein", "Er\u00b7ret\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "$.", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.40": {"text": "und meiner Zuflucht Schutz entgegen alle Sp\u00f6tter,", "tokens": ["und", "mei\u00b7ner", "Zu\u00b7flucht", "Schutz", "ent\u00b7ge\u00b7gen", "al\u00b7le", "Sp\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "die mir den Tod gedr\u00e4ut. Ich steife mich auf dich,", "tokens": ["die", "mir", "den", "Tod", "ge\u00b7dr\u00e4ut", ".", "Ich", "stei\u00b7fe", "mich", "auf", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$.", "PPER", "VVFIN", "PRF", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "und achte sie f\u00fcr nichts. Hinwieder, lehre mich", "tokens": ["und", "ach\u00b7te", "sie", "f\u00fcr", "nichts", ".", "Hin\u00b7wie\u00b7der", ",", "leh\u00b7re", "mich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "ADJA", "PPER", "APPR", "PIS", "$.", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "nach deinem Willen tun, denn du bist mein Regierer!", "tokens": ["nach", "dei\u00b7nem", "Wil\u00b7len", "tun", ",", "denn", "du", "bist", "mein", "Re\u00b7gie\u00b7rer", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,", "KON", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Dein guter werther Geist sei allezeit mein F\u00fchrer", "tokens": ["Dein", "gu\u00b7ter", "wert\u00b7her", "Geist", "sei", "al\u00b7le\u00b7zeit", "mein", "F\u00fch\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "auf wolgebahnter Bahn! Verh\u00e4nge, Herr, doch nicht,", "tokens": ["auf", "wol\u00b7ge\u00b7bahn\u00b7ter", "Bahn", "!", "Ver\u00b7h\u00e4n\u00b7ge", ",", "Herr", ",", "doch", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "NN", "$,", "NN", "$,", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "da\u00df der, so dir dein Lob des hohen Namens spricht,", "tokens": ["da\u00df", "der", ",", "so", "dir", "dein", "Lob", "des", "ho\u00b7hen", "Na\u00b7mens", "spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "ADV", "PPER", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "fahr' unerquicket hin! F\u00fchr' aus den gro\u00dfen N\u00f6ten", "tokens": ["fahr'", "un\u00b7er\u00b7quic\u00b7ket", "hin", "!", "F\u00fchr'", "aus", "den", "gro\u00b7\u00dfen", "N\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$.", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.48": {"text": "mein' halberlegne Seel' und la\u00df sie nicht gar t\u00f6ten!", "tokens": ["mein'", "hal\u00b7ber\u00b7leg\u00b7ne", "Seel'", "und", "la\u00df", "sie", "nicht", "gar", "t\u00f6\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "VVIMP", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Gott, weil du bist gerecht, so sihe doch darein", "tokens": ["Gott", ",", "weil", "du", "bist", "ge\u00b7recht", ",", "so", "si\u00b7he", "doch", "da\u00b7rein"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "VAFIN", "ADJD", "$,", "ADV", "VVFIN", "ADV", "PAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "und la\u00df mich Armen nicht in steter Unruh sein!", "tokens": ["und", "la\u00df", "mich", "Ar\u00b7men", "nicht", "in", "ste\u00b7ter", "Un\u00b7ruh", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "NN", "PTKNEG", "APPR", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Verst\u00f6re meinen Feind von deiner G\u00fcte wegen!", "tokens": ["Ver\u00b7st\u00f6\u00b7re", "mei\u00b7nen", "Feind", "von", "dei\u00b7ner", "G\u00fc\u00b7te", "we\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Setz' ihnen dich f\u00fcr mich zur rechten Rach' entgegen!", "tokens": ["Setz'", "ih\u00b7nen", "dich", "f\u00fcr", "mich", "zur", "rech\u00b7ten", "Rach'", "ent\u00b7ge\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPER", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Du wirst, Herr, richten wol die Seelen\u00e4ngster hin,", "tokens": ["Du", "wirst", ",", "Herr", ",", "rich\u00b7ten", "wol", "die", "See\u00b7len\u00b7\u00e4ngs\u00b7ter", "hin", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "NN", "$,", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "wenn du nur denken wilst, da\u00df ich dein Knecht noch bin.", "tokens": ["wenn", "du", "nur", "den\u00b7ken", "wilst", ",", "da\u00df", "ich", "dein", "Knecht", "noch", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}