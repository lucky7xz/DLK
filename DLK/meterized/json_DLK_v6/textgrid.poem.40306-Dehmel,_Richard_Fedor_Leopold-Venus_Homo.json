{"textgrid.poem.40306": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Venus Homo", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bettle nicht vor mir mit deinen Br\u00fcsten,", "tokens": ["Bett\u00b7le", "nicht", "vor", "mir", "mit", "dei\u00b7nen", "Br\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "deinen Br\u00fcsten bin ich kalt;", "tokens": ["dei\u00b7nen", "Br\u00fcs\u00b7ten", "bin", "ich", "kalt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "tausend Jahre alt", "tokens": ["tau\u00b7send", "Jah\u00b7re", "alt"], "token_info": ["word", "word", "word"], "pos": ["CARD", "NN", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "ist dein Blick mit seinen L\u00fcsten.", "tokens": ["ist", "dein", "Blick", "mit", "sei\u00b7nen", "L\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sieh mich an, wie Du als Braut gethan:", "tokens": ["Sieh", "mich", "an", ",", "wie", "Du", "als", "Braut", "ge\u00b7than", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "PWAV", "PPER", "KOUS", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "mit dem Blick des Grauens vor der Schlange!", "tokens": ["mit", "dem", "Blick", "des", "Grau\u00b7ens", "vor", "der", "Schlan\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Viel zu lange", "tokens": ["Viel", "zu", "lan\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "war ich, Weib, dein Mann.", "tokens": ["war", "ich", ",", "Weib", ",", "dein", "Mann", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Willst du Gift aus meiner Wurzel saugen?", "tokens": ["Willst", "du", "Gift", "aus", "mei\u00b7ner", "Wur\u00b7zel", "sau\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "unverwundbar bin ich deinem Bi\u00df!", "tokens": ["un\u00b7ver\u00b7wund\u00b7bar", "bin", "ich", "dei\u00b7nem", "Bi\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Folge mir ins Paradies:", "tokens": ["Fol\u00b7ge", "mir", "ins", "Pa\u00b7ra\u00b7dies", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "sieh mich an mit deinen Menschenaugen ...", "tokens": ["sieh", "mich", "an", "mit", "dei\u00b7nen", "Men\u00b7schen\u00b7au\u00b7gen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}