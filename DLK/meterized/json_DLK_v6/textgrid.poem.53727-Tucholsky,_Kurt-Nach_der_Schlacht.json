{"textgrid.poem.53727": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Nach der Schlacht", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenns mir mal schlecht geht, wird mich keiner kennen.", "tokens": ["Wenns", "mir", "mal", "schlecht", "geht", ",", "wird", "mich", "kei\u00b7ner", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$,", "VAFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein fremder Hunger langweilt f\u00fcrchterlich.", "tokens": ["Ein", "frem\u00b7der", "Hun\u00b7ger", "lang\u00b7weilt", "f\u00fcrch\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und mancher sagt, h\u00f6rt er den Namen nennen:", "tokens": ["Und", "man\u00b7cher", "sagt", ",", "h\u00f6rt", "er", "den", "Na\u00b7men", "nen\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00bbja, ich erinnre mich . . . \u00ab", "tokens": ["\u00bb", "ja", ",", "ich", "e\u00b7rinn\u00b7re", "mich", ".", ".", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["$(", "PTKANT", "$,", "PPER", "VVFIN", "PPER", "$.", "$.", "$.", "$("], "meter": "-+---+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "An allen T\u00fcren klingle ich vergebens.", "tokens": ["An", "al\u00b7len", "T\u00fc\u00b7ren", "kling\u00b7le", "ich", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich schlucke so, wenn ich da drau\u00dfen steh.", "tokens": ["Ich", "schlu\u00b7cke", "so", ",", "wenn", "ich", "da", "drau\u00b7\u00dfen", "steh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es bleibt als Fazit eines ganzen Lebens:", "tokens": ["Es", "bleibt", "als", "Fa\u00b7zit", "ei\u00b7nes", "gan\u00b7zen", "Le\u00b7bens", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbmein Gott, das ist pass\u00e9 \u2013!\u00ab", "tokens": ["\u00bb", "mein", "Gott", ",", "das", "ist", "pass\u00e9", "\u2013", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PDS", "VAFIN", "NE", "$(", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Es kommt ein Freund aus fr\u00fchem bessern Tagen,", "tokens": ["Es", "kommt", "ein", "Freund", "aus", "fr\u00fc\u00b7hem", "bes\u00b7sern", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "der spricht mit mir ein gutes M\u00e4nnerwort", "tokens": ["der", "spricht", "mit", "mir", "ein", "gu\u00b7tes", "M\u00e4n\u00b7ner\u00b7wort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und spricht und schenkt mir einen alten Kragen", "tokens": ["und", "spricht", "und", "schenkt", "mir", "ei\u00b7nen", "al\u00b7ten", "Kra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und macht rasch wieder fort.", "tokens": ["und", "macht", "rasch", "wie\u00b7der", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wenns mir mal schlecht geht, will ich mich verstecken.", "tokens": ["Wenns", "mir", "mal", "schlecht", "geht", ",", "will", "ich", "mich", "ver\u00b7ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$,", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da sind ja andre noch viel schlimmer dran:", "tokens": ["Da", "sind", "ja", "and\u00b7re", "noch", "viel", "schlim\u00b7mer", "dran", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIS", "ADV", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da gibt es welche bettelnd an den Ecken.", "tokens": ["Da", "gibt", "es", "wel\u00b7che", "bet\u00b7telnd", "an", "den", "E\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die stehen Mann f\u00fcr Mann.", "tokens": ["Die", "ste\u00b7hen", "Mann", "f\u00fcr", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was klag denn ich, wenn ich einst nicht mehr tauge?", "tokens": ["Was", "klag", "denn", "ich", ",", "wenn", "ich", "einst", "nicht", "mehr", "tau\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "PPER", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den andern ward, nach blutigem H\u00f6llentanz,", "tokens": ["Den", "an\u00b7dern", "ward", ",", "nach", "blu\u00b7ti\u00b7gem", "H\u00f6l\u00b7len\u00b7tanz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "mit Holzbein und mit ausgelaufnem Auge", "tokens": ["mit", "Holz\u00b7bein", "und", "mit", "aus\u00b7ge\u00b7lauf\u00b7nem", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "der Dank des Vaterlands.", "tokens": ["der", "Dank", "des", "Va\u00b7ter\u00b7lands", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}