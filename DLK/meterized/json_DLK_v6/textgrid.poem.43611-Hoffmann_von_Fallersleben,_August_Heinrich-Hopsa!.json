{"textgrid.poem.43611": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Hopsa!", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hopsa! Geiger, Pfeifer,", "tokens": ["Hop\u00b7sa", "!", "Gei\u00b7ger", ",", "Pfei\u00b7fer", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "NN", "$,", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Jetzo spielt uns einen Schleifer!", "tokens": ["Jet\u00b7zo", "spielt", "uns", "ei\u00b7nen", "Schlei\u00b7fer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hopsa! Geiger, Pfeifer,", "tokens": ["Hop\u00b7sa", "!", "Gei\u00b7ger", ",", "Pfei\u00b7fer", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "NN", "$,", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Spielt uns auf zum Tanz!", "tokens": ["Spielt", "uns", "auf", "zum", "Tanz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Hopsa! Max und K\u00e4thchen,", "tokens": ["Hop\u00b7sa", "!", "Max", "und", "K\u00e4th\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "KON", "NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Dreht euch rum und schlagt ein R\u00e4dchen!", "tokens": ["Dreht", "euch", "rum", "und", "schlagt", "ein", "R\u00e4d\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hopsa! Max und K\u00e4thchen,", "tokens": ["Hop\u00b7sa", "!", "Max", "und", "K\u00e4th\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "KON", "NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Dreht euch rum und tanzt!", "tokens": ["Dreht", "euch", "rum", "und", "tanzt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "KON", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Hopsa! Fritz und Fr\u00e4nzel,", "tokens": ["Hop\u00b7sa", "!", "Fritz", "und", "Fr\u00e4n\u00b7zel", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Dreht euch rum und macht ein T\u00e4nzel!", "tokens": ["Dreht", "euch", "rum", "und", "macht", "ein", "T\u00e4n\u00b7zel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hopsa! Fritz und Fr\u00e4nzel,", "tokens": ["Hop\u00b7sa", "!", "Fritz", "und", "Fr\u00e4n\u00b7zel", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Dreht euch rum und tanzt!", "tokens": ["Dreht", "euch", "rum", "und", "tanzt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "KON", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Hopsa! Annemiedel,", "tokens": ["Hop\u00b7sa", "!", "An\u00b7ne\u00b7mie\u00b7del", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Dreh' dich rum, tanz' nach der Fiedel!", "tokens": ["Dreh'", "dich", "rum", ",", "tanz'", "nach", "der", "Fie\u00b7del", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Hopsa! Annemiedel,", "tokens": ["Hop\u00b7sa", "!", "An\u00b7ne\u00b7mie\u00b7del", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Dreh' dich rum und tanz'!", "tokens": ["Dreh'", "dich", "rum", "und", "tanz'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Hopsa! Alexander,", "tokens": ["Hop\u00b7sa", "!", "A\u00b7lex\u00b7an\u00b7der", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Komm, wir tanzen mit einander!", "tokens": ["Komm", ",", "wir", "tan\u00b7zen", "mit", "ein\u00b7an\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "APPR", "PRF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hopsa! Alexander,", "tokens": ["Hop\u00b7sa", "!", "A\u00b7lex\u00b7an\u00b7der", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Tanz' mit mir herum!", "tokens": ["Tanz'", "mit", "mir", "he\u00b7rum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Hopsa! kleiner Wenzel!", "tokens": ["Hop\u00b7sa", "!", "klei\u00b7ner", "Wen\u00b7zel", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Du auch mu\u00dft heran zum T\u00e4nzel!", "tokens": ["Du", "auch", "mu\u00dft", "he\u00b7ran", "zum", "T\u00e4n\u00b7zel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hopsa! Alle mit einander", "tokens": ["Hop\u00b7sa", "!", "Al\u00b7le", "mit", "ein\u00b7an\u00b7der"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "PIS", "APPR", "PRF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tanzen wir herum!", "tokens": ["Tan\u00b7zen", "wir", "he\u00b7rum", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}