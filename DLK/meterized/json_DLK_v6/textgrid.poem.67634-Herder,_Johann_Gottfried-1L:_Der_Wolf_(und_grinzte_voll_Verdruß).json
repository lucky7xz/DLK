{"textgrid.poem.67634": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Wolf (und grinzte voll Verdru\u00df)", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Wolf (und grinzte voll Verdru\u00df)", "tokens": ["Der", "Wolf", "(", "und", "grinz\u00b7te", "voll", "Ver\u00b7dru\u00df", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$(", "KON", "VVFIN", "ADJD", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach: \u00bbLamm, was tr\u00fcbst Du mir den Flu\u00df?\u00ab", "tokens": ["Sprach", ":", "\u00bb", "Lamm", ",", "was", "tr\u00fcbst", "Du", "mir", "den", "Flu\u00df", "?", "\u00ab"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "$(", "NN", "$,", "PWS", "VVFIN", "PPER", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbach, gn\u00e4d'ger Herr, es kommt ja mir", "tokens": ["\u00bb", "ach", ",", "gn\u00e4d'\u00b7ger", "Herr", ",", "es", "kommt", "ja", "mir"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ITJ", "$,", "ADJA", "NN", "$,", "PPER", "VVFIN", "ADV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Wasser nur von Dir.\u00ab", "tokens": ["Das", "Was\u00b7ser", "nur", "von", "Dir", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbso hast Du mich vor Tag und Jahr", "tokens": ["\u00bb", "so", "hast", "Du", "mich", "vor", "Tag", "und", "Jahr"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VAFIN", "PPER", "PRF", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Grausam belogen.\u00ab \u00bbNein, f\u00fcrwahr,", "tokens": ["Grau\u00b7sam", "be\u00b7lo\u00b7gen", ".", "\u00ab", "\u00bb", "Nein", ",", "f\u00fcr\u00b7wahr", ","], "token_info": ["word", "word", "punct", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVPP", "$.", "$(", "$(", "PTKANT", "$,", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Gestrenger Herr! denn damals war", "tokens": ["Ge\u00b7stren\u00b7ger", "Herr", "!", "denn", "da\u00b7mals", "war"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "KON", "ADV", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich noch nicht auf der Welt.\u00ab", "tokens": ["Ich", "noch", "nicht", "auf", "der", "Welt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "\u00bbso siehst Du doch das abgefretzte Feld;", "tokens": ["\u00bb", "so", "siehst", "Du", "doch", "das", "ab\u00b7ge\u00b7fretz\u00b7te", "Feld", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wer hat denn das als Du gethan?\u00ab", "tokens": ["Wer", "hat", "denn", "das", "als", "Du", "ge\u00b7than", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ART", "KOUS", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "\u00bbund bin ja ohne Zahn!\u00ab", "tokens": ["\u00bb", "und", "bin", "ja", "oh\u00b7ne", "Zahn", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "VAFIN", "ADV", "APPR", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "\u00bbrechthabe!\u00ab sprach der Wolf und traf", "tokens": ["\u00bb", "recht\u00b7ha\u00b7be", "!", "\u00ab", "sprach", "der", "Wolf", "und", "traf"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "$.", "$(", "VVFIN", "ART", "NE", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Mit Klauen auf das Schaf.", "tokens": ["Mit", "Klau\u00b7en", "auf", "das", "Schaf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Der Wolf lief fort und lief", "tokens": ["Der", "Wolf", "lief", "fort", "und", "lief"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Und brach ins \u2013 \u2013 \u2013 iv", "tokens": ["Und", "brach", "ins", "\u2013", "\u2013", "\u2013", "iv"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word"], "pos": ["KON", "VVFIN", "APPRART", "$(", "$(", "$(", "XY"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Und fand viel Unrecht sich gethan", "tokens": ["Und", "fand", "viel", "Un\u00b7recht", "sich", "ge\u00b7than"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN", "PRF", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und wetzte seinen Zahn", "tokens": ["Und", "wetz\u00b7te", "sei\u00b7nen", "Zahn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Und fand sich \u2013 \u2013 \u2013", "tokens": ["Und", "fand", "sich", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "PRF", "$(", "$(", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "So lang' gestohlen", "tokens": ["So", "lang'", "ge\u00b7stoh\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.20": {"text": "Und fand, da\u00df \u2013 \u2013 \u2013 armes Schaf", "tokens": ["Und", "fand", ",", "da\u00df", "\u2013", "\u2013", "\u2013", "ar\u00b7mes", "Schaf"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "$(", "$(", "$(", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Ihm sein Fahrwasser tr\u00fcbt,", "tokens": ["Ihm", "sein", "Fahr\u00b7was\u00b7ser", "tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Und ward ins Schaf verliebt,", "tokens": ["Und", "ward", "ins", "Schaf", "ver\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Und \u2013 gn\u00e4d'ge Straf'! \u2013", "tokens": ["Und", "\u2013", "gn\u00e4d'\u00b7ge", "Straf'", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "$(", "ADJA", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.24": {"text": "Er fra\u00df das Schaf.", "tokens": ["Er", "fra\u00df", "das", "Schaf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}