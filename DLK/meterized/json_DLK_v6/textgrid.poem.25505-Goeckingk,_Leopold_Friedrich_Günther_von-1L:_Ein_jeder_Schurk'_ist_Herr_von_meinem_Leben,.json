{"textgrid.poem.25505": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein jeder Schurk' ist Herr von meinem Leben,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein jeder Schurk' ist Herr von meinem Leben,", "tokens": ["Ein", "je\u00b7der", "Schurk'", "ist", "Herr", "von", "mei\u00b7nem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie ", "tokens": ["Wie"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Was sollt' ich denn vor dir, o Tod! noch beben?", "tokens": ["Was", "sollt'", "ich", "denn", "vor", "dir", ",", "o", "Tod", "!", "noch", "be\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "APPR", "PPER", "$,", "FM", "NN", "$.", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da ist mein Leib! \u2013 mein Geist \u2013 lacht der Gefahr!", "tokens": ["Da", "ist", "mein", "Leib", "!", "\u2013", "mein", "Geist", "\u2013", "lacht", "der", "Ge\u00b7fahr", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$.", "$(", "PPOSAT", "NN", "$(", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein jeder Brand ist Herr von meinem Gute:", "tokens": ["Ein", "je\u00b7der", "Brand", "ist", "Herr", "von", "mei\u00b7nem", "Gu\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was hinge sich mein Herz an diesen Tand?", "tokens": ["Was", "hin\u00b7ge", "sich", "mein", "Herz", "an", "die\u00b7sen", "Tand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "PPOSAT", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nur wenig Gl\u00fcck brauch' ich bei meinem Muthe,", "tokens": ["Nur", "we\u00b7nig", "Gl\u00fcck", "brauch'", "ich", "bei", "mei\u00b7nem", "Mu\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und diesen Muth setzt keine Flamm' in Brand.", "tokens": ["Und", "die\u00b7sen", "Muth", "setzt", "kei\u00b7ne", "Flamm'", "in", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der K\u00f6nig ist zwar Herr von meinem Range,", "tokens": ["Der", "K\u00f6\u00b7nig", "ist", "zwar", "Herr", "von", "mei\u00b7nem", "Ran\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Allein zum Gl\u00fcck' nur in der K\u00f6rperwelt:", "tokens": ["Al\u00b7lein", "zum", "Gl\u00fcck", "nur", "in", "der", "K\u00f6r\u00b7per\u00b7welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Was w\u00e4r' ich viel f\u00fcr seinem Titel bange?", "tokens": ["Was", "w\u00e4r'", "ich", "viel", "f\u00fcr", "sei\u00b7nem", "Ti\u00b7tel", "ban\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Wenn Weisen nur mein Name noch gef\u00e4llt.", "tokens": ["Wenn", "Wei\u00b7sen", "nur", "mein", "Na\u00b7me", "noch", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Drum, was du thun willst, Schicksal! nun das thue!", "tokens": ["Drum", ",", "was", "du", "thun", "willst", ",", "Schick\u00b7sal", "!", "nun", "das", "thue", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$,", "NN", "$.", "ADV", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Verfolgst du mich: ich bleibe willig stehn;", "tokens": ["Ver\u00b7folgst", "du", "mich", ":", "ich", "blei\u00b7be", "wil\u00b7lig", "stehn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$.", "PPER", "VVFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Du, Nantchen, nur bist Herr von meiner Ruhe,", "tokens": ["Du", ",", "Nant\u00b7chen", ",", "nur", "bist", "Herr", "von", "mei\u00b7ner", "Ru\u00b7he", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$,", "ADV", "VAFIN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Nimmst du mir die, dann ist's um mich geschehn!", "tokens": ["Nimmst", "du", "mir", "die", ",", "dann", "ist's", "um", "mich", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "$,", "ADV", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}