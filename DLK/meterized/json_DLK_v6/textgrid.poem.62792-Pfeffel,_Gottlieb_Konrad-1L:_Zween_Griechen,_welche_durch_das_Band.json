{"textgrid.poem.62792": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zween Griechen, welche durch das Band", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zween Griechen, welche durch das Band", "tokens": ["Zween", "Grie\u00b7chen", ",", "wel\u00b7che", "durch", "das", "Band"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sympathie verbr\u00fcdert waren,", "tokens": ["Der", "Sym\u00b7pa\u00b7thie", "ver\u00b7br\u00fc\u00b7dert", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verlie\u00dfen jung ihr Vaterland,", "tokens": ["Ver\u00b7lie\u00b7\u00dfen", "jung", "ihr", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und suchten Gl\u00fcck bey den Barbaren.", "tokens": ["Und", "such\u00b7ten", "Gl\u00fcck", "bey", "den", "Bar\u00b7ba\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Das Schicksal trennte sie: Porphyr", "tokens": ["Das", "Schick\u00b7sal", "trenn\u00b7te", "sie", ":", "Por\u00b7phyr"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kam nach Illyrien, ward Kriegsknecht, Offizier,", "tokens": ["Kam", "nach", "Il\u00b7ly\u00b7ri\u00b7en", ",", "ward", "Kriegs\u00b7knecht", ",", "Of\u00b7fi\u00b7zier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "APPR", "NE", "$,", "VAFIN", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Spion, Feldmarschall, Gro\u00dfvezier", "tokens": ["Spi\u00b7on", ",", "Feld\u00b7mar\u00b7schall", ",", "Gro\u00df\u00b7ve\u00b7zier"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Und kurz, in Zeit von zwanzig Jahren", "tokens": ["Und", "kurz", ",", "in", "Zeit", "von", "zwan\u00b7zig", "Jah\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "APPR", "NN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Bestieg er als der Schwiegersohn", "tokens": ["Be\u00b7stieg", "er", "als", "der", "Schwie\u00b7ger\u00b7sohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Des K\u00f6nigs den ererbten Thron.", "tokens": ["Des", "K\u00f6\u00b7nigs", "den", "er\u00b7erb\u00b7ten", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Aret, der nichts von ihm erfahren,", "tokens": ["A\u00b7ret", ",", "der", "nichts", "von", "ihm", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PIS", "APPR", "PPER", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Kam als ein armer Philosoph", "tokens": ["Kam", "als", "ein", "ar\u00b7mer", "Phi\u00b7lo\u00b7soph"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Vom Ungl\u00fcck stets verfolgt, an seines Freundes Hof,", "tokens": ["Vom", "Un\u00b7gl\u00fcck", "stets", "ver\u00b7folgt", ",", "an", "sei\u00b7nes", "Freun\u00b7des", "Hof", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der eben Audienz ertheilte.", "tokens": ["Der", "e\u00b7ben", "Au\u00b7di\u00b7enz", "er\u00b7theil\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Was seh ich, Himmel! rief Aret,", "tokens": ["Was", "seh", "ich", ",", "Him\u00b7mel", "!", "rief", "A\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$.", "VVFIN", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Der weinend ihm entgegen eilte,", "tokens": ["Der", "wei\u00b7nend", "ihm", "ent\u00b7ge\u00b7gen", "eil\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVPP", "PPER", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Porphyr, mein Bruder! Was? fiel seine Majest\u00e4t", "tokens": ["Por\u00b7phyr", ",", "mein", "Bru\u00b7der", "!", "Was", "?", "fiel", "sei\u00b7ne", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "NN", "$.", "PWS", "$.", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Err\u00f6thend ihm ins Wort; hinweg mit diesem Tollen,", "tokens": ["Er\u00b7r\u00f6\u00b7thend", "ihm", "ins", "Wort", ";", "hin\u00b7weg", "mit", "die\u00b7sem", "Tol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "APPRART", "NN", "$.", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der unsern Stand vergi\u00dft! Vielleicht hat gar ein Feind", "tokens": ["Der", "un\u00b7sern", "Stand", "ver\u00b7gi\u00dft", "!", "Viel\u00b7leicht", "hat", "gar", "ein", "Feind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$.", "ADV", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Sich hinter ihm verbergen wollen.", "tokens": ["Sich", "hin\u00b7ter", "ihm", "ver\u00b7ber\u00b7gen", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Vergieb mir, sprach Aret, ich h\u00e4tte keinen Freund", "tokens": ["Ver\u00b7gieb", "mir", ",", "sprach", "A\u00b7ret", ",", "ich", "h\u00e4t\u00b7te", "kei\u00b7nen", "Freund"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "VVFIN", "NE", "$,", "PPER", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Auf einem Throne suchen sollen.", "tokens": ["Auf", "ei\u00b7nem", "Thro\u00b7ne", "su\u00b7chen", "sol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}