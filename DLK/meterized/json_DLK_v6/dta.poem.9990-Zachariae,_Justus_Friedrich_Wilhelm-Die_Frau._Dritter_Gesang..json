{"dta.poem.9990": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Frau.  \n   \n Dritter Gesang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20674-3", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl dem Manne, dem Gott zum Geschenk ein tu-\ngendhaft Weib gab!", "tokens": ["Wohl", "dem", "Man\u00b7ne", ",", "dem", "Gott", "zum", "Ge\u00b7schenk", "ein", "tu", "gend\u00b7haft", "Weib", "gab", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "APPRART", "NN", "ART", "TRUNC", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Freude beseeligt sein Herz; und Reichthum f\u00fcllet sein", "tokens": ["Freu\u00b7de", "be\u00b7see\u00b7ligt", "sein", "Herz", ";", "und", "Reicht\u00b7hum", "f\u00fcl\u00b7let", "sein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$.", "KON", "NN", "VVFIN", "PPOSAT"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Sieh! wie reizend tritt sie einher in heiterer Anmuth,", "tokens": ["Sieh", "!", "wie", "rei\u00b7zend", "tritt", "sie", "ein\u00b7her", "in", "hei\u00b7te\u00b7rer", "An\u00b7muth", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "ADJD", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Gleich der Unsterblichen einer. Vor ihrem zaubern-", "tokens": ["Gleich", "der", "U\u00b7nsterb\u00b7li\u00b7chen", "ei\u00b7ner", ".", "Vor", "ih\u00b7rem", "zau\u00b7bern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "$.", "APPR", "PPOSAT", "TRUNC"], "meter": "+-+---+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Weichen die Sorgen, wie Nebel entfliehn vorm Stra-", "tokens": ["Wei\u00b7chen", "die", "Sor\u00b7gen", ",", "wie", "Ne\u00b7bel", "ent\u00b7fliehn", "vorm", "Stra"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PWAV", "NN", "VVINF", "APPRART", "NN"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.6": {"text": "Um sie h\u00e4ngen sich liebliche Kinder, wie Liebesg\u00f6tter", "tokens": ["Um", "sie", "h\u00e4n\u00b7gen", "sich", "lieb\u00b7li\u00b7che", "Kin\u00b7der", ",", "wie", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUI", "PPER", "VVFIN", "PRF", "ADJA", "NN", "$,", "PWAV", "NN"], "meter": "--+--+--+--+-+-", "measure": "anapaest.tetra.plus"}, "line.7": {"text": "An dem G\u00fcrtel Cytherens. Die s\u00fcsse harmonische Rede", "tokens": ["An", "dem", "G\u00fcr\u00b7tel", "Cy\u00b7the\u00b7rens", ".", "Die", "s\u00fcs\u00b7se", "har\u00b7mo\u00b7ni\u00b7sche", "Re\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE", "$.", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Dringt mit Schmeicheln ins Herz des Mannes; er he-", "tokens": ["Dringt", "mit", "Schmei\u00b7cheln", "ins", "Herz", "des", "Man\u00b7nes", ";", "er", "he"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "APPRART", "NN", "ART", "NN", "$.", "PPER", "TRUNC"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Preist sich begl\u00fcckt, und danket der Vorsicht sein irdi-", "tokens": ["Preist", "sich", "be\u00b7gl\u00fcckt", ",", "und", "dan\u00b7ket", "der", "Vor\u00b7sicht", "sein", "ir\u00b7di"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "VVPP", "$,", "KON", "VVFIN", "ART", "NN", "PPOSAT", "TRUNC"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.10": {"text": "Sch\u00f6n ists, wer an m\u00e4chtigen Fl\u00fcssen die eige-", "tokens": ["Sch\u00f6n", "ists", ",", "wer", "an", "m\u00e4ch\u00b7ti\u00b7gen", "Fl\u00fcs\u00b7sen", "die", "ei\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "$,", "PWS", "APPR", "ADJA", "NN", "ART", "TRUNC"], "meter": "+---+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Ueber den Ocean sendet, und an den fetten Gestaden", "tokens": ["Ue\u00b7ber", "den", "O\u00b7cean", "sen\u00b7det", ",", "und", "an", "den", "fet\u00b7ten", "Ge\u00b7sta\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Mengen von Heerden ern\u00e4hrt; sch\u00f6n ists, die Schaaren", "tokens": ["Men\u00b7gen", "von", "Heer\u00b7den", "er\u00b7n\u00e4hrt", ";", "sch\u00f6n", "ists", ",", "die", "Schaa\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "NN", "VVPP", "$.", "ADJD", "VAFIN", "$,", "ART", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.13": {"text": "M\u00e4hen zu sehn, auf eigenem Land, von Seegen bedecket;", "tokens": ["M\u00e4\u00b7hen", "zu", "sehn", ",", "auf", "ei\u00b7ge\u00b7nem", "Land", ",", "von", "See\u00b7gen", "be\u00b7de\u00b7cket", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Oder die eignen ergiebigen Berge zu Sch\u00e4tzen zu", "tokens": ["O\u00b7der", "die", "eig\u00b7nen", "er\u00b7gie\u00b7bi\u00b7gen", "Ber\u00b7ge", "zu", "Sch\u00e4t\u00b7zen", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN", "APPR", "NN", "PTKZU"], "meter": "+--+--+--+--+-+", "measure": "diphilius"}, "line.15": {"text": "Sch\u00f6n ists, in dem Schoo\u00dfe des Ruhms, im Zirkel", "tokens": ["Sch\u00f6n", "ists", ",", "in", "dem", "Schoo\u00b7\u00dfe", "des", "Ruhms", ",", "im", "Zir\u00b7kel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VAFIN", "$,", "APPR", "ART", "NN", "ART", "NN", "$,", "APPRART", "NN"], "meter": "+---+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.16": {"text": "Aus Krystallen zu trinken; befreyt von der Sorge des", "tokens": ["Aus", "Krys\u00b7tal\u00b7len", "zu", "trin\u00b7ken", ";", "be\u00b7freyt", "von", "der", "Sor\u00b7ge", "des"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$.", "VVPP", "APPR", "ART", "NN", "ART"], "meter": "--+--+--+--+--", "measure": "anapaest.tetra.plus"}, "line.17": {"text": "K\u00f6nigsgnaden erzeigen zu k\u00f6nnen, \u2014 und doch ist es", "tokens": ["K\u00f6\u00b7nigs\u00b7gna\u00b7den", "er\u00b7zei\u00b7gen", "zu", "k\u00f6n\u00b7nen", ",", "und", "doch", "ist", "es"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "PTKZU", "VMINF", "$,", "$(", "KON", "ADV", "VAFIN", "PPER"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.18": {"text": "In den Armen der weiblichen Tugend dem Himmel zu", "tokens": ["In", "den", "Ar\u00b7men", "der", "weib\u00b7li\u00b7chen", "Tu\u00b7gend", "dem", "Him\u00b7mel", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "ART", "NN", "PTKZU"], "meter": "--+--+--+--+-+", "measure": "anapaest.tetra.plus"}, "line.19": {"text": "So wie Aurora die Wellen verl\u00e4\u00dft, verl\u00e4\u00dft sie", "tokens": ["So", "wie", "Au\u00b7ro\u00b7ra", "die", "Wel\u00b7len", "ver\u00b7l\u00e4\u00dft", ",", "ver\u00b7l\u00e4\u00dft", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "KOKOM", "NE", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER"], "meter": "--+---+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Jhres Gemahls, und geht, wie die Sonne, dem fro-", "tokens": ["Ih\u00b7res", "Ge\u00b7mahls", ",", "und", "geht", ",", "wie", "die", "Son\u00b7ne", ",", "dem", "fro"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KON", "VVFIN", "$,", "PWAV", "ART", "NN", "$,", "ART", "TRUNC"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.21": {"text": "Keine gek\u00fcnstelten Wasser benetzen die bl\u00fchenden", "tokens": ["Kei\u00b7ne", "ge\u00b7k\u00fcns\u00b7tel\u00b7ten", "Was\u00b7ser", "be\u00b7net\u00b7zen", "die", "bl\u00fc\u00b7hen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "ART", "ADJA"], "meter": "+--+--+--+--+--", "measure": "diphilius"}, "line.22": {"text": "Sondern sie taucht ihr holdes Gesicht in den lauteren", "tokens": ["Son\u00b7dern", "sie", "taucht", "ihr", "hol\u00b7des", "Ge\u00b7sicht", "in", "den", "lau\u00b7te\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "ART", "VVINF"], "meter": "+--+-+--+--+-+", "measure": "iambic.hexa.invert"}, "line.23": {"text": "Und sie ist sch\u00f6n, wie Venus im Bade. Nicht Stun-", "tokens": ["Und", "sie", "ist", "sch\u00f6n", ",", "wie", "Ve\u00b7nus", "im", "Ba\u00b7de", ".", "Nicht", "Stun"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$,", "PWAV", "NN", "APPRART", "NN", "$.", "PTKNEG", "TRUNC"], "meter": "---+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.24": {"text": "Ueber dem Putze des fliegenden Haars. Sie stralet", "tokens": ["Ue\u00b7ber", "dem", "Put\u00b7ze", "des", "flie\u00b7gen\u00b7den", "Haars", ".", "Sie", "stra\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.25": {"text": "Jm Japanischen Stoff; die reine weisseste Leinwand", "tokens": ["Jm", "Ja\u00b7pa\u00b7ni\u00b7schen", "Stoff", ";", "die", "rei\u00b7ne", "weis\u00b7ses\u00b7te", "Lein\u00b7wand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$.", "ART", "ADJA", "ADJA", "NN"], "meter": "-+---+-+-+--++", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Flie\u00dft um die marmornen Glieder, und eine thauigte", "tokens": ["Flie\u00dft", "um", "die", "mar\u00b7mor\u00b7nen", "Glie\u00b7der", ",", "und", "ei\u00b7ne", "thau\u00b7ig\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "KON", "ART", "VVFIN"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.27": {"text": "Nur halbaufgebl\u00fcht, schm\u00fccket die Stirn. So weckt", "tokens": ["Nur", "halb\u00b7auf\u00b7ge\u00b7bl\u00fcht", ",", "schm\u00fc\u00b7cket", "die", "Stirn", ".", "So", "weckt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN"], "meter": "-++-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Mit dem frischesten Morgenku\u00df auf. Am reinlichen", "tokens": ["Mit", "dem", "fri\u00b7sches\u00b7ten", "Mor\u00b7gen\u00b7ku\u00df", "auf", ".", "Am", "rein\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$.", "APPRART", "ADJA"], "meter": "--+--+--+-+--", "measure": "anapaest.tri.plus"}, "line.29": {"text": "Sitzt sie mit ihm, und versammelt um sich die liebli-", "tokens": ["Sitzt", "sie", "mit", "ihm", ",", "und", "ver\u00b7sam\u00b7melt", "um", "sich", "die", "lieb\u00b7li"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,", "KON", "VVFIN", "APPR", "PRF", "ART", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.30": {"text": "Ruft die Sorge des Staats den Mann zu fr\u00fchen Ge-", "tokens": ["Ruft", "die", "Sor\u00b7ge", "des", "Staats", "den", "Mann", "zu", "fr\u00fc\u00b7hen", "Ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "ART", "NN", "APPR", "ADJA", "TRUNC"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.31": {"text": "So entweicht sie unter die Schatten des l\u00e4ndlichen", "tokens": ["So", "ent\u00b7weicht", "sie", "un\u00b7ter", "die", "Schat\u00b7ten", "des", "l\u00e4nd\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJA"], "meter": "--+-+--+--+--", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "N\u00e4ht in der schattichten Laube von Linden; indes da\u00df", "tokens": ["N\u00e4ht", "in", "der", "schat\u00b7tich\u00b7ten", "Lau\u00b7be", "von", "Lin\u00b7den", ";", "in\u00b7des", "da\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "APPR", "NE", "$.", "ADV", "KOUS"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.33": {"text": "Blumen sammelt, die Schwester zu kr\u00e4nzen, im thauig-", "tokens": ["Blu\u00b7men", "sam\u00b7melt", ",", "die", "Schwes\u00b7ter", "zu", "kr\u00e4n\u00b7zen", ",", "im", "thau\u00b7ig"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,", "APPRART", "TRUNC"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.34": {"text": "Hinter dem Frosch her setzt, und nach dem Schmetter-", "tokens": ["Hin\u00b7ter", "dem", "Frosch", "her", "setzt", ",", "und", "nach", "dem", "Schmet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APZR", "VVFIN", "$,", "KON", "APPR", "ART", "TRUNC"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.35": {"text": "Oder sie wandelt auch \u00fcber den Hof; betrachtet die", "tokens": ["O\u00b7der", "sie", "wan\u00b7delt", "auch", "\u00fc\u00b7ber", "den", "Hof", ";", "be\u00b7trach\u00b7tet", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$.", "VVFIN", "ART"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.36": {"text": "Jhrer weissen gekr\u00f6nten H\u00fcner; indes da\u00df die Tauben", "tokens": ["Ih\u00b7rer", "weis\u00b7sen", "ge\u00b7kr\u00f6n\u00b7ten", "H\u00fc\u00b7ner", ";", "in\u00b7des", "da\u00df", "die", "Tau\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$.", "ADV", "KOUS", "ART", "NN"], "meter": "+-+--+-+-+++-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.37": {"text": "Rauschend vom Dache sich st\u00fcrzen, und ihre Gebiethrin", "tokens": ["Rau\u00b7schend", "vom", "Da\u00b7che", "sich", "st\u00fcr\u00b7zen", ",", "und", "ih\u00b7re", "Ge\u00b7bieth\u00b7rin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "PRF", "VVINF", "$,", "KON", "PPOSAT", "NN"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.38": {"text": "Dann ertheilt sie der K\u00fcche Befehl, und steigt auch", "tokens": ["Dann", "er\u00b7theilt", "sie", "der", "K\u00fc\u00b7che", "Be\u00b7fehl", ",", "und", "steigt", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$,", "KON", "VVFIN", "ADV"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.39": {"text": "Zu den Gew\u00f6lben des Weingotts hinab, und sorgt f\u00fcr", "tokens": ["Zu", "den", "Ge\u00b7w\u00f6l\u00b7ben", "des", "Wein\u00b7gotts", "hin\u00b7ab", ",", "und", "sorgt", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "APPR"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.40": {"text": "Jhrer Sch\u00e4tze vom Rhein, und f\u00fcr die Tokayische Traube.", "tokens": ["Ih\u00b7rer", "Sch\u00e4t\u00b7ze", "vom", "Rhein", ",", "und", "f\u00fcr", "die", "To\u00b7ka\u00b7yisc\u00b7he", "Trau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NE", "$,", "KON", "APPR", "A