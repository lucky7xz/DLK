", "ART", "NN", "APPR", "ADJA", "NN", "$.", "ART", "TRUNC"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.50": {"text": "Hat sein Herz nie entflammt, nie hat ein qu\u00e4lender", "tokens": ["Hat", "sein", "Herz", "nie", "ent\u00b7flammt", ",", "nie", "hat", "ein", "qu\u00e4\u00b7len\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$,", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Jhrer Keuschheit und Treu sein sanftes Lager umflattert.", "tokens": ["Ih\u00b7rer", "Keuschheit", "und", "Treu", "sein", "sanf\u00b7tes", "La\u00b7ger", "um\u00b7flat\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.52": {"text": "Goldbedeckte Verf\u00fchrer der Unschuld, und witzige Narren,", "tokens": ["Gold\u00b7be\u00b7deck\u00b7te", "Ver\u00b7f\u00fch\u00b7rer", "der", "Un\u00b7schuld", ",", "und", "wit\u00b7zi\u00b7ge", "Nar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,", "KON", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.53": {"text": "Plaudrer ohne Gehirn, umgeben nie ihren Caffeetisch.", "tokens": ["Plaud\u00b7rer", "oh\u00b7ne", "Ge\u00b7hirn", ",", "um\u00b7ge\u00b7ben", "nie", "ih\u00b7ren", "Caf\u00b7fee\u00b7tisch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "VVPP", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+--++-", "measure": "trochaic.septa.relaxed"}, "line.54": {"text": "Sie auch bl\u00e4ht sich im Canapee nicht bey heiligen", "tokens": ["Sie", "auch", "bl\u00e