{"textgrid.poem.32250": {"metadata": {"author": {"name": "Fischart, Johann", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1568, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie jhr dann solchs in Franckreich secht,", "tokens": ["Wie", "jhr", "dann", "solchs", "in", "Fran\u00b7ck\u00b7reich", "secht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PIS", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Da nur ein Florentinisch Henn,", "tokens": ["Da", "nur", "ein", "Flo\u00b7ren\u00b7ti\u00b7nisch", "Henn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein alt seyt vnd faule senn,", "tokens": ["Ein", "alt", "seyt", "vnd", "fau\u00b7le", "senn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Gallos vnd das Hanengschlecht", "tokens": ["Die", "Gal\u00b7los", "vnd", "das", "Ha\u00b7neng\u00b7schlecht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wil zu Capaunen machen schlecht", "tokens": ["Wil", "zu", "Ca\u00b7pau\u00b7nen", "ma\u00b7chen", "schlecht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "NN", "VVINF", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Vnd au\u00df den Galliern Galliner,", "tokens": ["Vnd", "au\u00df", "den", "Gal\u00b7li\u00b7ern", "Gal\u00b7li\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NE", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Au\u00df freien Francken Frauendiner,", "tokens": ["Au\u00df", "frei\u00b7en", "Fran\u00b7cken", "Frau\u00b7en\u00b7di\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Au\u00df Musicseyten sennengflecht,", "tokens": ["Au\u00df", "Mu\u00b7si\u00b7csey\u00b7ten", "sen\u00b7neng\u00b7flecht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Darumb weil sich die rein Quintseyten", "tokens": ["Da\u00b7rumb", "weil", "sich", "die", "rein", "Quint\u00b7sey\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "KOUS", "PRF", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Nicht nach dem alten Trummscheit leyten,", "tokens": ["Nicht", "nach", "dem", "al\u00b7ten", "Trumm\u00b7scheit", "ley\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Vnd der Han sich seins Kam\u00df ermant,", "tokens": ["Vnd", "der", "Han", "sich", "seins", "Kam\u00df", "er\u00b7mant", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.12": {"text": "Vnd nicht die Henn zum Meyster leyd,", "tokens": ["Vnd", "nicht", "die", "Henn", "zum", "Meys\u00b7ter", "leyd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "So sicht man heut ein solchen streit,", "tokens": ["So", "sicht", "man", "heut", "ein", "sol\u00b7chen", "streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Die Henn zu treiben in jrn standt.", "tokens": ["Die", "Henn", "zu", "trei\u00b7ben", "in", "jrn", "standt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}}}}}