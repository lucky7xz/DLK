{"textgrid.poem.59292": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Spaziergang", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ochse, wie bist du so stattlich, bedachtsam, flei\u00dfig und n\u00fctzlich!", "tokens": ["O\u00b7chse", ",", "wie", "bist", "du", "so", "statt\u00b7lich", ",", "be\u00b7dacht\u00b7sam", ",", "flei\u00b7\u00dfig", "und", "n\u00fctz\u00b7lich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VAFIN", "PPER", "ADV", "ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wahrlich, ich brauche dich sehr \u2013 aber du bist doch ein Ochs!", "tokens": ["Wahr\u00b7lich", ",", "ich", "brau\u00b7che", "dich", "sehr", "\u2013", "a\u00b7ber", "du", "bist", "doch", "ein", "Ochs", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "PPER", "ADV", "$(", "KON", "PPER", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}}, "stanza.2": {"line.1": {"text": "Ho da! Kartoffeln und ihr, \u00f6konomische Knollengew\u00e4chse,", "tokens": ["Ho", "da", "!", "Kar\u00b7tof\u00b7feln", "und", "ihr", ",", "\u00f6\u00b7kon\u00b7o\u00b7mi\u00b7sche", "Knol\u00b7len\u00b7ge\u00b7w\u00e4ch\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "$.", "NN", "KON", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Schreiten kaum kann man; gemach! macht euch nicht gar zu sehr breit!", "tokens": ["Schrei\u00b7ten", "kaum", "kann", "man", ";", "ge\u00b7mach", "!", "macht", "euch", "nicht", "gar", "zu", "sehr", "breit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VMFIN", "PIS", "$.", "ADV", "$.", "VVFIN", "PPER", "PTKNEG", "ADV", "PTKA", "ADV", "ADJD", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}}, "stanza.3": {"line.1": {"text": "Gr\u00fc\u00df dich, Klatschrose und G\u00e4nseblum, Butterblum, l\u00e4ndliches V\u00f6lkchen,", "tokens": ["Gr\u00fc\u00df", "dich", ",", "Klatsc\u00b7hro\u00b7se", "und", "G\u00e4n\u00b7se\u00b7blum", ",", "But\u00b7ter\u00b7blum", ",", "l\u00e4nd\u00b7li\u00b7ches", "V\u00f6lk\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "KON", "NE", "$,", "NE", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Schmucklos und ohne Geruch, unschuldig \u2013 weiter sonst nichts? \u2013", "tokens": ["Schmuck\u00b7los", "und", "oh\u00b7ne", "Ge\u00b7ruch", ",", "un\u00b7schul\u00b7dig", "\u2013", "wei\u00b7ter", "sonst", "nichts", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "KON", "APPR", "NN", "$,", "ADJD", "$(", "ADV", "ADV", "PIS", "$.", "$("], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Nelke, du reizendes Kind, wie hast du so gar nichts Bescheidnes!", "tokens": ["Nel\u00b7ke", ",", "du", "rei\u00b7zen\u00b7des", "Kind", ",", "wie", "hast", "du", "so", "gar", "nichts", "Be\u00b7scheid\u00b7nes", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJA", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADV", "ADV", "PIS", "NN", "$."], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Jauchzende Farben voll Lust flammst du ins traurige Gr\u00fcn,", "tokens": ["Jauch\u00b7zen\u00b7de", "Far\u00b7ben", "voll", "Lust", "flammst", "du", "ins", "trau\u00b7ri\u00b7ge", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "NN", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Tief von den eigenen D\u00fcften du selber lustig berauschet,", "tokens": ["Tief", "von", "den", "ei\u00b7ge\u00b7nen", "D\u00fcf\u00b7ten", "du", "sel\u00b7ber", "lus\u00b7tig", "be\u00b7rau\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Spiele denn, brenne, von dir la\u00df ich berauschen mich gern!", "tokens": ["Spie\u00b7le", "denn", ",", "bren\u00b7ne", ",", "von", "dir", "la\u00df", "ich", "be\u00b7rau\u00b7schen", "mich", "gern", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "VVFIN", "$,", "APPR", "PPER", "VVFIN", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}}}}}