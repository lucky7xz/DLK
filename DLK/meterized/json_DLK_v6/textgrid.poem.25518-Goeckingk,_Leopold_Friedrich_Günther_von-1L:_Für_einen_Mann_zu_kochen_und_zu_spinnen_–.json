{"textgrid.poem.25518": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: F\u00fcr einen Mann zu kochen und zu spinnen \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcr einen Mann zu kochen und zu spinnen \u2013", "tokens": ["F\u00fcr", "ei\u00b7nen", "Mann", "zu", "ko\u00b7chen", "und", "zu", "spin\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Unw\u00fcrdiger Beruf!", "tokens": ["Un\u00b7w\u00fcr\u00b7di\u00b7ger", "Be\u00b7ruf", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn's der nur ist, wozu mit diesen Sinnen", "tokens": ["Wenn's", "der", "nur", "ist", ",", "wo\u00b7zu", "mit", "die\u00b7sen", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADV", "VAFIN", "$,", "PWAV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und diesem Geist' mich die Natur erschuf.", "tokens": ["Und", "die\u00b7sem", "Geist'", "mich", "die", "Na\u00b7tur", "er\u00b7schuf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hat sie nur blo\u00df die wundervollen Zonen", "tokens": ["Hat", "sie", "nur", "blo\u00df", "die", "wun\u00b7der\u00b7vol\u00b7len", "Zo\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "F\u00fcr M\u00e4nner ausgespannt?", "tokens": ["F\u00fcr", "M\u00e4n\u00b7ner", "aus\u00b7ge\u00b7spannt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und darf ich, gleich dem Stier', sie nur bewohnen?", "tokens": ["Und", "darf", "ich", ",", "gleich", "dem", "Stier'", ",", "sie", "nur", "be\u00b7woh\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "$,", "ADV", "ART", "NN", "$,", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der, wenn er stirbt, sein Futter nur gekannt?", "tokens": ["Der", ",", "wenn", "er", "stirbt", ",", "sein", "Fut\u00b7ter", "nur", "ge\u00b7kannt", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Soll ich nicht auch in jenem Leben leben?", "tokens": ["Soll", "ich", "nicht", "auch", "in", "je\u00b7nem", "Le\u00b7ben", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wer wird ein Wunder thun,", "tokens": ["Wer", "wird", "ein", "Wun\u00b7der", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Und meinem Geist' dort Saphos Denkkraft geben,", "tokens": ["Und", "mei\u00b7nem", "Geist'", "dort", "Sa\u00b7phos", "Denk\u00b7kraft", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "NE", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Lie\u00df ich ihn hier bei T\u00f6pf' und Spindeln ruhn?", "tokens": ["Lie\u00df", "ich", "ihn", "hier", "bei", "T\u00f6pf'", "und", "Spin\u00b7deln", "ruhn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "APPR", "NE", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ist's nicht genug, die H\u00e4lfte meines Lebens", "tokens": ["Ist's", "nicht", "ge\u00b7nug", ",", "die", "H\u00e4lf\u00b7te", "mei\u00b7nes", "Le\u00b7bens"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PTKNEG", "ADV", "$,", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Gesch\u00e4ft'ge Martha seyn?", "tokens": ["Ge\u00b7sch\u00e4ft'\u00b7ge", "Mar\u00b7tha", "seyn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Ist's Hochverrath, ist's Thorheit, ist's vergebens,", "tokens": ["Ist's", "Hoch\u00b7ver\u00b7rath", ",", "ist's", "Thor\u00b7heit", ",", "ist's", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "NN", "$,", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Der Weisheit kaum die andre H\u00e4lfte weihn?", "tokens": ["Der", "Weis\u00b7heit", "kaum", "die", "and\u00b7re", "H\u00e4lf\u00b7te", "weihn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "O M\u00e4nner! M\u00e4nner! so uns zu erziehen! \u2013", "tokens": ["O", "M\u00e4n\u00b7ner", "!", "M\u00e4n\u00b7ner", "!", "so", "uns", "zu", "er\u00b7zie\u00b7hen", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "$.", "NN", "$.", "ADV", "PPER", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Wenn Nesseln an dem Bach'", "tokens": ["Wenn", "Nes\u00b7seln", "an", "dem", "Bach'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Des Lebens unter Euren Veilchen bl\u00fchen:", "tokens": ["Des", "Le\u00b7bens", "un\u00b7ter", "Eu\u00b7ren", "Veil\u00b7chen", "bl\u00fc\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Wer soll sie j\u00e4ten? Eure Gattin? ach!", "tokens": ["Wer", "soll", "sie", "j\u00e4\u00b7ten", "?", "Eu\u00b7re", "Gat\u00b7tin", "?", "ach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVFIN", "$.", "PPOSAT", "NN", "$.", "XY", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Was sind schon mir die M\u00e4dchen nicht f\u00fcr Dinger,", "tokens": ["Was", "sind", "schon", "mir", "die", "M\u00e4d\u00b7chen", "nicht", "f\u00fcr", "Din\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "PPER", "ART", "NN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Gilt's f\u00fcr mein Herz und Geist!", "tokens": ["Gilt's", "f\u00fcr", "mein", "Herz", "und", "Geist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Was sind sie? Ha! Gesellschaft f\u00fcr die Finger,", "tokens": ["Was", "sind", "sie", "?", "Ha", "!", "Ge\u00b7sell\u00b7schaft", "f\u00fcr", "die", "Fin\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$.", "ITJ", "$.", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Wenn mich der Zwang Quadrille spielen hei\u00dft.", "tokens": ["Wenn", "mich", "der", "Zwang", "Quad\u00b7ril\u00b7le", "spie\u00b7len", "hei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "O da\u00df ich doch nur eine Babet h\u00e4tte!", "tokens": ["O", "da\u00df", "ich", "doch", "nur", "ei\u00b7ne", "Ba\u00b7bet", "h\u00e4t\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "ADV", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Wie wollt' ich mit ihr thun!", "tokens": ["Wie", "wollt'", "ich", "mit", "ihr", "thun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Wir liebten uns einander um die Wette,", "tokens": ["Wir", "lieb\u00b7ten", "uns", "ein\u00b7an\u00b7der", "um", "die", "Wet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Wir wollten Nachts auf Einem Kissen ruhn;", "tokens": ["Wir", "woll\u00b7ten", "Nachts", "auf", "Ei\u00b7nem", "Kis\u00b7sen", "ruhn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Umfa\u00dft, zusammen durch das Leben eilen,", "tokens": ["Um\u00b7fa\u00dft", ",", "zu\u00b7sam\u00b7men", "durch", "das", "Le\u00b7ben", "ei\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Die B\u00fcrden leicht und schwer,", "tokens": ["Die", "B\u00fcr\u00b7den", "leicht", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Die Freuden gro\u00df und klein zusammen theilen \u2013", "tokens": ["Die", "Freu\u00b7den", "gro\u00df", "und", "klein", "zu\u00b7sam\u00b7men", "thei\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "O wei\u00dft du, Freund! denn keine Babet mehr?", "tokens": ["O", "wei\u00dft", "du", ",", "Freund", "!", "denn", "kei\u00b7ne", "Ba\u00b7bet", "mehr", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "NN", "$.", "ADV", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}