{"textgrid.poem.43442": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Esel wollten den Fasching begehn,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Esel wollten den Fasching begehn,", "tokens": ["Die", "E\u00b7sel", "woll\u00b7ten", "den", "Fa\u00b7sching", "be\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das konnt' ohne Masken und Schellen geschehn.", "tokens": ["Das", "konnt'", "oh\u00b7ne", "Mas\u00b7ken", "und", "Schel\u00b7len", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Tschahi, tschaha, tschaho!", "tokens": ["Tscha\u00b7hi", ",", "tscha\u00b7ha", ",", "tscha\u00b7ho", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ITJ", "$,", "ITJ", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "Wie war'n die Esel froh!", "tokens": ["Wie", "wa\u00b7r'n", "die", "E\u00b7sel", "froh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wie jauchzte laut ein Jeder, Jeder, Jeder", "tokens": ["Wie", "jauchz\u00b7te", "laut", "ein", "Je\u00b7der", ",", "Je\u00b7der", ",", "Je\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PWAV", "VVFIN", "APPR", "ART", "PIS", "$,", "PIS", "$,", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und machte gro\u00df Halloh!", "tokens": ["Und", "mach\u00b7te", "gro\u00df", "Hal\u00b7loh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie hatten einen Narrenk\u00f6nig sich erkor'n,", "tokens": ["Sie", "hat\u00b7ten", "ei\u00b7nen", "Nar\u00b7ren\u00b7k\u00f6\u00b7nig", "sich", "er\u00b7kor'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das war geworden ", "tokens": ["Das", "war", "ge\u00b7wor\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VAFIN", "VAPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Tschahi, tschaha, tschaho!", "tokens": ["Tscha\u00b7hi", ",", "tscha\u00b7ha", ",", "tscha\u00b7ho", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ITJ", "$,", "ITJ", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "Wie war'n die Esel froh!", "tokens": ["Wie", "wa\u00b7r'n", "die", "E\u00b7sel", "froh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wie jauchzte laut ein Jeder, Jeder, Jeder", "tokens": ["Wie", "jauchz\u00b7te", "laut", "ein", "Je\u00b7der", ",", "Je\u00b7der", ",", "Je\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PWAV", "VVFIN", "APPR", "ART", "PIS", "$,", "PIS", "$,", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und machte gro\u00df Halloh!", "tokens": ["Und", "mach\u00b7te", "gro\u00df", "Hal\u00b7loh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie hatten einen Hofstaat ihm beigesellt,", "tokens": ["Sie", "hat\u00b7ten", "ei\u00b7nen", "Hof\u00b7staat", "ihm", "bei\u00b7ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So ganz wie es geht in der Mannthierwelt.", "tokens": ["So", "ganz", "wie", "es", "geht", "in", "der", "Mannt\u00b7hier\u00b7welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOKOM", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Tschahi, tschaha tschaho!", "tokens": ["Tscha\u00b7hi", ",", "tscha\u00b7ha", "tscha\u00b7ho", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "FM", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "Wie war'n die Esel froh!", "tokens": ["Wie", "wa\u00b7r'n", "die", "E\u00b7sel", "froh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wie jauchzte laut ein Jeder, Jeder, Jeder", "tokens": ["Wie", "jauchz\u00b7te", "laut", "ein", "Je\u00b7der", ",", "Je\u00b7der", ",", "Je\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PWAV", "VVFIN", "APPR", "ART", "PIS", "$,", "PIS", "$,", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und machte gro\u00df Halloh!", "tokens": ["Und", "mach\u00b7te", "gro\u00df", "Hal\u00b7loh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und als nun die Esel so gingen im Zug,", "tokens": ["Und", "als", "nun", "die", "E\u00b7sel", "so", "gin\u00b7gen", "im", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Da kam ein Kettenhund herbei und frug:", "tokens": ["Da", "kam", "ein", "Ket\u00b7ten\u00b7hund", "her\u00b7bei", "und", "frug", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbtschahi, tschaha, tschaho!", "tokens": ["\u00bb", "tscha\u00b7hi", ",", "tscha\u00b7ha", ",", "tscha\u00b7ho", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "FM.la", "$,", "ITJ", "$,", "ITJ", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "Warum seid ihr so froh?", "tokens": ["Wa\u00b7rum", "seid", "ihr", "so", "froh", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+---+", "measure": "dactylic.init"}, "line.5": {"text": "Warum doch jauchzt ein Jeder, Jeder, Jeder", "tokens": ["Wa\u00b7rum", "doch", "jauchzt", "ein", "Je\u00b7der", ",", "Je\u00b7der", ",", "Je\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "PIS", "$,", "PIS", "$,", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und macht ein solch Halloh?\u00ab", "tokens": ["Und", "macht", "ein", "solch", "Hal\u00b7loh", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Maskenfreiheit die haben auch wir,", "tokens": ["Die", "Mas\u00b7ken\u00b7frei\u00b7heit", "die", "ha\u00b7ben", "auch", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "ADV", "PPER", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die brauchen wir jetzo nach unsrer Manier.", "tokens": ["Die", "brau\u00b7chen", "wir", "jet\u00b7zo", "nach", "uns\u00b7rer", "Ma\u00b7nier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--++-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Tschahi, tschaha, tschaho!", "tokens": ["Tscha\u00b7hi", ",", "tscha\u00b7ha", ",", "tscha\u00b7ho", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ITJ", "$,", "ITJ", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "Drum sind wir Esel froh,", "tokens": ["Drum", "sind", "wir", "E\u00b7sel", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drum jauchzet auch ein Jeder, Jeder, Jeder", "tokens": ["Drum", "jauch\u00b7zet", "auch", "ein", "Je\u00b7der", ",", "Je\u00b7der", ",", "Je\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PAV", "VVFIN", "ADV", "ART", "PIS", "$,", "PIS", "$,", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und macht ein solch Halloh.", "tokens": ["Und", "macht", "ein", "solch", "Hal\u00b7loh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbihr Esel bleibt doch Esel und werdet nie gescheidt,", "tokens": ["\u00bb", "ihr", "E\u00b7sel", "bleibt", "doch", "E\u00b7sel", "und", "wer\u00b7det", "nie", "ge\u00b7scheidt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "ADV", "NN", "KON", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Was kann euch doch n\u00fctzen die Maskenfreiheit?", "tokens": ["Was", "kann", "euch", "doch", "n\u00fct\u00b7zen", "die", "Mas\u00b7ken\u00b7frei\u00b7heit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Tschahi, tschaha, tschaheit!", "tokens": ["Tscha\u00b7hi", ",", "tscha\u00b7ha", ",", "tscha\u00b7heit", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ITJ", "$,", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ihr seid doch nie gescheidt,", "tokens": ["Ihr", "seid", "doch", "nie", "ge\u00b7scheidt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es bleibet doch ein Jeder, Jeder, Jeder", "tokens": ["Es", "blei\u00b7bet", "doch", "ein", "Je\u00b7der", ",", "Je\u00b7der", ",", "Je\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "PIS", "$,", "PIS", "$,", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein Esel allezeit.\u00ab", "tokens": ["Ein", "E\u00b7sel", "al\u00b7le\u00b7zeit", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbso eine Freiheit, n\u00e4rrisch zu sein,", "tokens": ["\u00bb", "so", "ei\u00b7ne", "Frei\u00b7heit", ",", "n\u00e4r\u00b7risch", "zu", "sein", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ART", "NN", "$,", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Tschahi, tschaha, tschahei!", "tokens": ["Tscha\u00b7hi", ",", "tscha\u00b7ha", ",", "tscha\u00b7hei", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ITJ", "$,", "FM", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ja, zeigtet ihr euch frei,", "tokens": ["Ja", ",", "zeig\u00b7tet", "ihr", "euch", "frei", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So m\u00fc\u00dfte halt ein Jeder, Jeder, Jeder", "tokens": ["So", "m\u00fc\u00df\u00b7te", "halt", "ein", "Je\u00b7der", ",", "Je\u00b7der", ",", "Je\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "VMFIN", "VVFIN", "ART", "PIS", "$,", "PIS", "$,", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gleich vor die Polizei.\u00ab", "tokens": ["Gleich", "vor", "die", "Po\u00b7li\u00b7zei", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}