{"textgrid.poem.43122": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: In bangen N\u00e4chten, wenn der graue Wahnsinn", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In bangen N\u00e4chten, wenn der graue Wahnsinn", "tokens": ["In", "ban\u00b7gen", "N\u00e4ch\u00b7ten", ",", "wenn", "der", "grau\u00b7e", "Wahn\u00b7sinn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit d\u00fcrren Fingern an das Hirn mir pochte,", "tokens": ["Mit", "d\u00fcr\u00b7ren", "Fin\u00b7gern", "an", "das", "Hirn", "mir", "poch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn gl\u00fch'nde Thr\u00e4nen meine Kissen netzten,", "tokens": ["Wenn", "gl\u00fch'n\u00b7de", "Thr\u00e4\u00b7nen", "mei\u00b7ne", "Kis\u00b7sen", "netz\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein wildes Herz vor Zorn und Sehnsucht kochte \u2013", "tokens": ["Mein", "wil\u00b7des", "Herz", "vor", "Zorn", "und", "Sehn\u00b7sucht", "koch\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In solchen N\u00e4chten war mir der Gedanke,", "tokens": ["In", "sol\u00b7chen", "N\u00e4ch\u00b7ten", "war", "mir", "der", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df Du noch lebst, da\u00df ich Dich wiedersehe,", "tokens": ["Da\u00df", "Du", "noch", "lebst", ",", "da\u00df", "ich", "Dich", "wie\u00b7der\u00b7se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein Stern, nach dem ich zitternd hob die H\u00e4nde \u2013", "tokens": ["Ein", "Stern", ",", "nach", "dem", "ich", "zit\u00b7ternd", "hob", "die", "H\u00e4n\u00b7de", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVPP", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und trotzig weiter schleppt' ich dann mein Wehe.", "tokens": ["Und", "trot\u00b7zig", "wei\u00b7ter", "schleppt'", "ich", "dann", "mein", "We\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich sah Dich wieder \u2013 wieder pl\u00f6tzlich flammten", "tokens": ["Ich", "sah", "Dich", "wie\u00b7der", "\u2013", "wie\u00b7der", "pl\u00f6tz\u00b7lich", "flamm\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$(", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie alle auf, die alten Wahnsinnsgluthen,", "tokens": ["Sie", "al\u00b7le", "auf", ",", "die", "al\u00b7ten", "Wahn\u00b7sinns\u00b7glu\u00b7then", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "PTKVZ", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der wilde Zorn, der Schmerz, die herbe Liebe \u2013", "tokens": ["Der", "wil\u00b7de", "Zorn", ",", "der", "Schmerz", ",", "die", "her\u00b7be", "Lie\u00b7be", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es war, als m\u00fc\u00dfte ich vor Dir verbluten.", "tokens": ["Es", "war", ",", "als", "m\u00fc\u00df\u00b7te", "ich", "vor", "Dir", "ver\u00b7blu\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du aber standest mit dem argen L\u00e4cheln,", "tokens": ["Du", "a\u00b7ber", "stan\u00b7dest", "mit", "dem", "ar\u00b7gen", "L\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das mir bekannt aus gottverfluchten Tagen;", "tokens": ["Das", "mir", "be\u00b7kannt", "aus", "gott\u00b7ver\u00b7fluch\u00b7ten", "Ta\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der fahle Blick macht mir das Herz erstarren:", "tokens": ["Der", "fah\u00b7le", "Blick", "macht", "mir", "das", "Herz", "er\u00b7star\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Es war ein freches, antwortsich'res Fragen!", "tokens": ["Es", "war", "ein", "fre\u00b7ches", ",", "ant\u00b7wort\u00b7sich'\u00b7res", "Fra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und Deine H\u00e4nde streckten fiebergl\u00fchend", "tokens": ["Und", "Dei\u00b7ne", "H\u00e4n\u00b7de", "streck\u00b7ten", "fie\u00b7berg\u00b7l\u00fc\u00b7hend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich pl\u00f6tzlich so begehrend mir entgegen,", "tokens": ["Sich", "pl\u00f6tz\u00b7lich", "so", "be\u00b7geh\u00b7rend", "mir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADV", "VVPP", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und mehr und mehr sah ich Dein Bild erblassen,", "tokens": ["Und", "mehr", "und", "mehr", "sah", "ich", "Dein", "Bild", "er\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Das mich begleitet einst auf allen Wegen:", "tokens": ["Das", "mich", "be\u00b7glei\u00b7tet", "einst", "auf", "al\u00b7len", "We\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "\u00bbdas ist er nicht!\u00ab schrie es in meiner Seele,", "tokens": ["\u00bb", "das", "ist", "er", "nicht", "!", "\u00ab", "schrie", "es", "in", "mei\u00b7ner", "See\u00b7le", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "PPER", "PTKNEG", "$.", "$(", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "\u00bbso war er nie, so kann er nimmer werden.\u00ab", "tokens": ["\u00bb", "so", "war", "er", "nie", ",", "so", "kann", "er", "nim\u00b7mer", "wer\u00b7den", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "ADV", "$,", "ADV", "VMFIN", "PPER", "ADV", "VAINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wof\u00fcr w\u00e4r' meine Seligkeit verspielet,", "tokens": ["Wo\u00b7f\u00fcr", "w\u00e4r'", "mei\u00b7ne", "Se\u00b7lig\u00b7keit", "ver\u00b7spie\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wof\u00fcr w\u00e4r' ich verflucht \u2013 verflucht auf Erden! \u2013 \u2013", "tokens": ["Wo\u00b7f\u00fcr", "w\u00e4r'", "ich", "ver\u00b7flucht", "\u2013", "ver\u00b7flucht", "auf", "Er\u00b7den", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVPP", "$(", "VVPP", "APPR", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}