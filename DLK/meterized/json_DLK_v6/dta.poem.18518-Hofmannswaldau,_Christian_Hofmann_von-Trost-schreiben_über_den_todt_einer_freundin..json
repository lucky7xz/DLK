{"dta.poem.18518": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Trost-schreiben \u00fcber den todt  \n einer freundin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geliebte Freundin/ wann mein brieff nach wermuth", "tokens": ["Ge\u00b7lieb\u00b7te", "Freun\u00b7din", "/", "wann", "mein", "brieff", "nach", "wer\u00b7muth"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "$(", "PWAV", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "schmecket/", "tokens": ["schme\u00b7cket", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Wann Coloquinten er in seinen zeilen tr\u00e4gt/", "tokens": ["Wann", "Co\u00b7loq\u00b7uin\u00b7ten", "er", "in", "sei\u00b7nen", "zei\u00b7len", "tr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wann ihn statt rothem wachs ein schwartzes siegel decket/", "tokens": ["Wann", "ihn", "statt", "ro\u00b7them", "wachs", "ein", "schwart\u00b7zes", "sie\u00b7gel", "de\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und sich ein zittrend ach! in allen syllben regt;", "tokens": ["Und", "sich", "ein", "zitt\u00b7rend", "ach", "!", "in", "al\u00b7len", "syll\u00b7ben", "regt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "ADJD", "ITJ", "$.", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So wisse/ da\u00df ich selbst mit schmertzen bin umfangen/", "tokens": ["So", "wis\u00b7se", "/", "da\u00df", "ich", "selbst", "mit", "schmert\u00b7zen", "bin", "um\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "PPER", "ADV", "APPR", "VVINF", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Di\u00df blat wil meiner noth ein treuer zeuge seyn/", "tokens": ["Di\u00df", "blat", "wil", "mei\u00b7ner", "noth", "ein", "treu\u00b7er", "zeu\u00b7ge", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VMFIN", "PPOSAT", "NN", "ART", "ADJA", "VVFIN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denn da dein theurstes guth aus dieser welt gegangen/", "tokens": ["Denn", "da", "dein", "theurs\u00b7tes", "guth", "aus", "die\u00b7ser", "welt", "ge\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "ADJA", "NN", "APPR", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mischt sich nicht ungereimt mein ach! zu deinem ein.", "tokens": ["Mischt", "sich", "nicht", "un\u00b7ge\u00b7reimt", "mein", "ach", "!", "zu", "dei\u00b7nem", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKNEG", "ADJD", "PPOSAT", "ITJ", "$.", "APPR", "PPOSAT", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Es hat die trauer-post mich allzusehr erschrecket/", "tokens": ["Es", "hat", "die", "trau\u00b7e\u00b7rpost", "mich", "all\u00b7zu\u00b7sehr", "er\u00b7schre\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die dir ein treues hertz/ mir eine Freundin nimmt/", "tokens": ["Die", "dir", "ein", "treu\u00b7es", "hertz", "/", "mir", "ei\u00b7ne", "Freun\u00b7din", "nimmt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "$(", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und nichts als angst und weh in meiner seel erwecket/", "tokens": ["Und", "nichts", "als", "angst", "und", "weh", "in", "mei\u00b7ner", "seel", "er\u00b7we\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "VVPP", "KON", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So da\u00df mein hertze selbst in heissen thr\u00e4nen schwimmt.", "tokens": ["So", "da\u00df", "mein", "hert\u00b7ze", "selbst", "in", "heis\u00b7sen", "thr\u00e4\u00b7nen", "schwimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPOSAT", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Drumb wei\u00df ich fast auch nicht/ ob ich soll thr\u00e4nen schicken/", "tokens": ["Drumb", "wei\u00df", "ich", "fast", "auch", "nicht", "/", "ob", "ich", "soll", "thr\u00e4\u00b7nen", "schi\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADV", "PTKNEG", "$(", "KOUS", "PPER", "VMFIN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ob aber h\u00fclff und rath vor deine todes-pein/", "tokens": ["Ob", "a\u00b7ber", "h\u00fclff", "und", "rath", "vor", "dei\u00b7ne", "to\u00b7des\u00b7pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "KON", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die dich mit k\u00fcmmern\u00fc\u00df und tr\u00fcbsal will umstricken/", "tokens": ["Die", "dich", "mit", "k\u00fcm\u00b7mer\u00b7n\u00fc\u00df", "und", "tr\u00fcb\u00b7sal", "will", "um\u00b7stri\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ADJD", "KON", "ADJD", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und dich heist ohne trost/ mich ohne freuden seyn.", "tokens": ["Und", "dich", "heist", "oh\u00b7ne", "trost", "/", "mich", "oh\u00b7ne", "freu\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "$(", "PPER", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Zu thr\u00e4nen hat mich l\u00e4ngst die freundschafft dir verbunden/", "tokens": ["Zu", "thr\u00e4\u00b7nen", "hat", "mich", "l\u00e4ngst", "die", "freund\u00b7schafft", "dir", "ver\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "PPER", "ADV", "ART", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Sie ist ein festes band das uns verkn\u00fcpffet h\u00e4lt.", "tokens": ["Sie", "ist", "ein", "fes\u00b7tes", "band", "das", "uns", "ver\u00b7kn\u00fcpf\u00b7fet", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "VVFIN", "ART", "PPER", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Kein wunder/ wann ich nun auch deinen schmertz empfunden/", "tokens": ["Kein", "wun\u00b7der", "/", "wann", "ich", "nun", "auch", "dei\u00b7nen", "schmertz", "emp\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$(", "PWAV", "PPER", "ADV", "ADV", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und wann dein jammernd ach! auf meine geister f\u00e4llt.", "tokens": ["Und", "wann", "dein", "jam\u00b7mernd", "ach", "!", "auf", "mei\u00b7ne", "geis\u00b7ter", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "ADJD", "ITJ", "$.", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Will gleiche freundschafft sich in gleichen seelen finden/", "tokens": ["Will", "glei\u00b7che", "freund\u00b7schafft", "sich", "in", "glei\u00b7chen", "see\u00b7len", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "VVFIN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wie kan es anders seyn/ sie f\u00fchlen gleiche noth/", "tokens": ["Wie", "kan", "es", "an\u00b7ders", "seyn", "/", "sie", "f\u00fch\u00b7len", "glei\u00b7che", "noth", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "VAINF", "$(", "PPER", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Dein wohlseyn konte mich zuvor mit lust umbwinden/", "tokens": ["Dein", "wohl\u00b7seyn", "kon\u00b7te", "mich", "zu\u00b7vor", "mit", "lust", "um\u00b7bwin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Jtzt ist dein trauer-stand mir \u00e4rger als der todt.", "tokens": ["Jtzt", "ist", "dein", "trau\u00b7e\u00b7rstand", "mir", "\u00e4r\u00b7ger", "als", "der", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PPER", "ADJD", "KOKOM", "ART", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wie solt ich mich demnach zum troste wohl bequehmen/", "tokens": ["Wie", "solt", "ich", "mich", "dem\u00b7nach", "zum", "tros\u00b7te", "wohl", "be\u00b7queh\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "PAV", "APPRART", "ADJA", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ich der ich selber fast ohn trost und h\u00fclffe bin?", "tokens": ["Ich", "der", "ich", "sel\u00b7ber", "fast", "ohn", "trost", "und", "h\u00fclf\u00b7fe", "bin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "PPER", "ADV", "ADV", "APPR", "NN", "KON", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Kan man aus fremder noth offt rath und trost hernehmen/", "tokens": ["Kan", "man", "aus", "frem\u00b7der", "noth", "offt", "rath", "und", "trost", "her\u00b7neh\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "ADJA", "NN", "ADV", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "So tr\u00f6ste dich denn auch mit meinem krancken sinn.", "tokens": ["So", "tr\u00f6s\u00b7te", "dich", "denn", "auch", "mit", "mei\u00b7nem", "kran\u00b7cken", "sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Kein ander mittel wei\u00df ich dir itzt beyzubringen/", "tokens": ["Kein", "an\u00b7der", "mit\u00b7tel", "wei\u00df", "ich", "dir", "itzt", "bey\u00b7zu\u00b7brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPER", "PPER", "ADV", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Schau meine schmertzen nur und meine wehmuth an", "tokens": ["Schau", "mei\u00b7ne", "schmert\u00b7zen", "nur", "und", "mei\u00b7ne", "weh\u00b7muth", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "VVFIN", "ADV", "KON", "PPOSAT", "NN", "PTKVZ"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.32": {"text": "Kan mich dein ungl\u00fcck nun zu solchem trauren zwingen/", "tokens": ["Kan", "mich", "dein", "un\u00b7gl\u00fcck", "nun", "zu", "sol\u00b7chem", "trau\u00b7ren", "zwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "ADJD", "ADV", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "So wei\u00df ich/ da\u00df dich auch mein ungl\u00fcck tr\u00f6sten kan.", "tokens": ["So", "wei\u00df", "ich", "/", "da\u00df", "dich", "auch", "mein", "un\u00b7gl\u00fcck", "tr\u00f6s\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Zwar ist der ri\u00df zu streng/ dem aller trost mu\u00df weichen/", "tokens": ["Zwar", "ist", "der", "ri\u00df", "zu", "streng", "/", "dem", "al\u00b7ler", "trost", "mu\u00df", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "VVFIN", "PTKA", "ADJD", "$(", "ART", "PIAT", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ein tieff-verwundtes hertz nimmt keine pflaster an.", "tokens": ["Ein", "tief\u00b7fver\u00b7wund\u00b7tes", "hertz", "nimmt", "kei\u00b7ne", "pflas\u00b7ter", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wer unter todten steht/ wird selber offt zur leichen", "tokens": ["Wer", "un\u00b7ter", "tod\u00b7ten", "steht", "/", "wird", "sel\u00b7ber", "offt", "zur", "lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ADJA", "VVFIN", "$(", "VAFIN", "ADV", "ADV", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Und lehrt/ da\u00df nur ein grab die wunden heilen kan.", "tokens": ["Und", "lehrt", "/", "da\u00df", "nur", "ein", "grab", "die", "wun\u00b7den", "hei\u00b7len", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "ADV", "ART", "NN", "ART", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ich zweiffle nicht/ du wirst auch todten \u00e4hnlich scheinen/", "tokens": ["Ich", "zweiff\u00b7le", "nicht", "/", "du", "wirst", "auch", "tod\u00b7ten", "\u00e4hn\u00b7lich", "schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$(", "PPER", "VAFIN", "ADV", "ADJA", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die du nur leich auf leich bi\u00dfher erlebet hast;", "tokens": ["Die", "du", "nur", "leich", "auf", "leich", "bi\u00df\u00b7her", "er\u00b7le\u00b7bet", "hast", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "APPR", "ADJD", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.40": {"text": "Vor kurtzem n\u00f6thigten zwo freundin dich zum weinen/", "tokens": ["Vor", "kurt\u00b7zem", "n\u00f6\u00b7thig\u00b7ten", "zwo", "freun\u00b7din", "dich", "zum", "wei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "CARD", "NN", "PPER", "APPRART", "VVINF", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.41": {"text": "Jetzt aber f\u00fchlstu erst die rechte kummerlast.", "tokens": ["Jetzt", "a\u00b7ber", "f\u00fchl\u00b7stu", "erst", "die", "rech\u00b7te", "kum\u00b7mer\u00b7last", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Nachdem dein ancker selbst in tausend st\u00fcck zerbrochen/", "tokens": ["Nach\u00b7dem", "dein", "an\u00b7cker", "selbst", "in", "tau\u00b7send", "st\u00fcck", "zer\u00b7bro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "APPR", "CARD", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Auff welchen du bi\u00dfher dein wohlergehn gesetzt;", "tokens": ["Auff", "wel\u00b7chen", "du", "bi\u00df\u00b7her", "dein", "woh\u00b7ler\u00b7gehn", "ge\u00b7setzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PPER", "ADV", "PPOSAT", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Und sich der grimme todt so starck an dir gerochen/", "tokens": ["Und", "sich", "der", "grim\u00b7me", "todt", "so", "starck", "an", "dir", "ge\u00b7ro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "ADJA", "ADJD", "ADV", "ADJD", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Da\u00df auch dein hertze selbst bi\u00df auf das blut verletzt.", "tokens": ["Da\u00df", "auch", "dein", "hert\u00b7ze", "selbst", "bi\u00df", "auf", "das", "blut", "ver\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ADV", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Kein wunder/ wann dir nun will alles licht gebrechen/", "tokens": ["Kein", "wun\u00b7der", "/", "wann", "dir", "nun", "will", "al\u00b7les", "licht", "ge\u00b7bre\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$(", "PWAV", "PPER", "ADV", "VMFIN", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Wer schaut nicht finstern\u00fc\u00df wann unsre sonn entweicht", "tokens": ["Wer", "schaut", "nicht", "fins\u00b7ter\u00b7n\u00fc\u00df", "wann", "uns\u00b7re", "sonn", "ent\u00b7weicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "VVFIN", "PWAV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Wer kan von fr\u00fchlings-lust und von vergn\u00fcgen sprechen/", "tokens": ["Wer", "kan", "von", "fr\u00fch\u00b7lings\u00b7lust", "und", "von", "ver\u00b7gn\u00fc\u00b7gen", "spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPR", "ADJD", "KON", "APPR", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Wann der ergrimmte nord durch unsre felder streicht.", "tokens": ["Wann", "der", "er\u00b7grimm\u00b7te", "nord", "durch", "uns\u00b7re", "fel\u00b7der", "streicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Wer kan gro\u00dfm\u00fcthig stehn/ wann wolck und donner krachen?", "tokens": ["Wer", "kan", "gro\u00df\u00b7m\u00fct\u00b7hig", "stehn", "/", "wann", "wolck", "und", "don\u00b7ner", "kra\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADJD", "VVINF", "$(", "PWAV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Welch mensch schaut sonn und todt mit steiffen augen an?", "tokens": ["Welch", "mensch", "schaut", "sonn", "und", "todt", "mit", "steif\u00b7fen", "au\u00b7gen", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "KON", "ADJD", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Wer kan wind/ hagel/ sturm und aller wetter lachen/", "tokens": ["Wer", "kan", "wind", "/", "ha\u00b7gel", "/", "sturm", "und", "al\u00b7ler", "wet\u00b7ter", "la\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "NE", "$(", "NE", "$(", "ADJD", "KON", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Wenn unser schiff schon selbst mit wellen angethan?", "tokens": ["Wenn", "un\u00b7ser", "schiff", "schon", "selbst", "mit", "wel\u00b7len", "an\u00b7ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "ADV", "APPR", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Es ist nur allzuviel das sehn zu grabe schicken/", "tokens": ["Es", "ist", "nur", "all\u00b7zu\u00b7viel", "das", "sehn", "zu", "gra\u00b7be", "schi\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "PDS", "VVINF", "PTKZU", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Was wir mehr als uns selbst auf dieser welt geliebt.", "tokens": ["Was", "wir", "mehr", "als", "uns", "selbst", "auf", "die\u00b7ser", "welt", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "KOUS", "PPER", "ADV", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Auch palmen kan die last offt zu der erden dr\u00fccken/", "tokens": ["Auch", "pal\u00b7men", "kan", "die", "last", "offt", "zu", "der", "er\u00b7den", "dr\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "ART", "VVFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.57": {"text": "Wie solt ein mensch nicht seyn ob solchen fall betr\u00fcbt?", "tokens": ["Wie", "solt", "ein", "mensch", "nicht", "seyn", "ob", "sol\u00b7chen", "fall", "be\u00b7tr\u00fcbt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "PTKNEG", "VAINF", "KOUS", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Drum glaub ich leichtlich auch dein jammer-volles klagen/", "tokens": ["Drum", "glaub", "ich", "leicht\u00b7lich", "auch", "dein", "jam\u00b7mer\u00b7vol\u00b7les", "kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "ADV", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Allein wo denckstu hin bey dieser traurigkeit?", "tokens": ["Al\u00b7lein", "wo", "dencks\u00b7tu", "hin", "bey", "die\u00b7ser", "trau\u00b7rig\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PDS", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Wie lange wiltu noch die kummer-fessel tragen?", "tokens": ["Wie", "lan\u00b7ge", "wil\u00b7tu", "noch", "die", "kum\u00b7mer\u00b7fes\u00b7sel", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Soll dann dein ange nie von thr\u00e4nen seyn befreyt?", "tokens": ["Soll", "dann", "dein", "an\u00b7ge", "nie", "von", "thr\u00e4\u00b7nen", "seyn", "be\u00b7freyt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "ADV", "APPR", "NN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Ach! Freundin weine nicht/ da\u00df menschen sind gestorben.", "tokens": ["Ach", "!", "Freun\u00b7din", "wei\u00b7ne", "nicht", "/", "da\u00df", "men\u00b7schen", "sind", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "VVFIN", "PTKNEG", "$(", "KOUS", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Dir ist nicht unbekandt des lebens strenge noth.", "tokens": ["Dir", "ist", "nicht", "un\u00b7be\u00b7kandt", "des", "le\u00b7bens", "stren\u00b7ge", "noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Wer hat nicht angst und pein vor labsal hier erworben/", "tokens": ["Wer", "hat", "nicht", "angst", "und", "pein", "vor", "lab\u00b7sal", "hier", "er\u00b7wor\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PTKNEG", "VVPP", "KON", "NN", "APPR", "NE", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Was tr\u00f6stet endlich uns wohl bessers als der todt?", "tokens": ["Was", "tr\u00f6s\u00b7tet", "end\u00b7lich", "uns", "wohl", "bes\u00b7sers", "als", "der", "todt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "PPER", "ADV", "ADV", "KOUS", "ART", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Es ist die\u00df leben ja voll m\u00fch und voller sorgen/", "tokens": ["Es", "ist", "die\u00df", "le\u00b7ben", "ja", "voll", "m\u00fch", "und", "vol\u00b7ler", "sor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDS", "VVFIN", "ADV", "ADJD", "ADJD", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Voll \u00fcberdru\u00df und pein/ so uns gefangen h\u00e4lt/", "tokens": ["Voll", "\u00fc\u00b7berd\u00b7ru\u00df", "und", "pein", "/", "so", "uns", "ge\u00b7fan\u00b7gen", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD