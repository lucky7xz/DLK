{"textgrid.poem.67545": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Als ich von Livland aus zu Schiffe ging", "genre": "verse", "period": "N.A.", "pub_year": 1769, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh, Freund, da fliehn sie hin im Ungewitter,", "tokens": ["Sieh", ",", "Freund", ",", "da", "fliehn", "sie", "hin", "im", "Un\u00b7ge\u00b7wit\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Freunde meiner Jugend! Sie,", "tokens": ["Die", "Freun\u00b7de", "mei\u00b7ner", "Ju\u00b7gend", "!", "Sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$.", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die liebek\u00fchn uns bis zur Grenze folgten", "tokens": ["Die", "lie\u00b7be\u00b7k\u00fchn", "uns", "bis", "zur", "Gren\u00b7ze", "folg\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des alten Oceans.", "tokens": ["Des", "al\u00b7ten", "O\u00b7ceans", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Am Himmel traten vor des Vaters Antlitz", "tokens": ["Am", "Him\u00b7mel", "tra\u00b7ten", "vor", "des", "Va\u00b7ters", "Ant\u00b7litz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Sterne, Abendstern und Mond!", "tokens": ["Die", "Ster\u00b7ne", ",", "A\u00b7bends\u00b7tern", "und", "Mond", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Er segnet' sie hinweg. Da rief zum Meer uns", "tokens": ["Er", "seg\u00b7net'", "sie", "hin\u00b7weg", ".", "Da", "rief", "zum", "Meer", "uns"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "ADV", "VVFIN", "APPRART", "NN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der wehnde Himmelssohn.", "tokens": ["Der", "wehn\u00b7de", "Him\u00b7mels\u00b7sohn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da schied der letzte Ku\u00df. An's Oceanes", "tokens": ["Da", "schied", "der", "letz\u00b7te", "Ku\u00df", ".", "An's", "O\u00b7cea\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "NE", "NE"], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Wildwehndem, unabsehbarn Reich,", "tokens": ["Wild\u00b7wehn\u00b7dem", ",", "un\u00b7ab\u00b7seh\u00b7barn", "Reich", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "In reger Lust, im Angesicht des Himmels", "tokens": ["In", "re\u00b7ger", "Lust", ",", "im", "An\u00b7ge\u00b7sicht", "des", "Him\u00b7mels"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der treuste letzte Ku\u00df,", "tokens": ["Der", "treus\u00b7te", "letz\u00b7te", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der je geschieden ward! Und ach! nie rascher,", "tokens": ["Der", "je", "ge\u00b7schie\u00b7den", "ward", "!", "Und", "ach", "!", "nie", "ra\u00b7scher", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "VAFIN", "$.", "KON", "XY", "$.", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So bebend furchtbar schwankender", "tokens": ["So", "be\u00b7bend", "furcht\u00b7bar", "schwan\u00b7ken\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJD", "ADJA"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Hinweggeschieden! Hier an zweer Schiffe", "tokens": ["Hin\u00b7weg\u00b7ge\u00b7schie\u00b7den", "!", "Hier", "an", "zweer", "Schif\u00b7fe"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Aufbebend fliehndem Rand \u2013", "tokens": ["Auf\u00b7be\u00b7bend", "fliehn\u00b7dem", "Rand", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Ein Ungewitter ri\u00df ihn! Ahnungsdonner \u2013", "tokens": ["Ein", "Un\u00b7ge\u00b7wit\u00b7ter", "ri\u00df", "ihn", "!", "Ah\u00b7nungs\u00b7don\u00b7ner", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie? ahnetest Du ewig ihn,", "tokens": ["Wie", "?", "ah\u00b7ne\u00b7test", "Du", "e\u00b7wig", "ihn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "ADJD", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Ku\u00df der Trennung? soll er nimmer werden", "tokens": ["Den", "Ku\u00df", "der", "Tren\u00b7nung", "?", "soll", "er", "nim\u00b7mer", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "VMFIN", "PPER", "ADV", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Ku\u00df des Kommenden?", "tokens": ["Der", "Ku\u00df", "des", "Kom\u00b7men\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}}, "stanza.6": {"line.1": {"text": "Zu ihren H\u00fctten kommend! \u2013 Zu den H\u00fctten", "tokens": ["Zu", "ih\u00b7ren", "H\u00fct\u00b7ten", "kom\u00b7mend", "!", "\u2013", "Zu", "den", "H\u00fct\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$.", "$(", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Noth und Freude, wo sie jetzt", "tokens": ["Der", "Noth", "und", "Freu\u00b7de", ",", "wo", "sie", "jetzt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "$,", "PWAV", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hinkehren \u2013 blicken noch vom schwanken Boote", "tokens": ["Hin\u00b7keh\u00b7ren", "\u2013", "bli\u00b7cken", "noch", "vom", "schwan\u00b7ken", "Boo\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "VVFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Thr\u00e4nen nach uns her,", "tokens": ["Mit", "Thr\u00e4\u00b7nen", "nach", "uns", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Mit Thr\u00e4nen nach uns her! und senden Boten,", "tokens": ["Mit", "Thr\u00e4\u00b7nen", "nach", "uns", "her", "!", "und", "sen\u00b7den", "Bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "PTKVZ", "$.", "KON", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Freundschaft Seufzer, uns nur weg,", "tokens": ["Der", "Freund\u00b7schaft", "Seuf\u00b7zer", ",", "uns", "nur", "weg", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur weg zu fl\u00fcgeln! Sieh! da ist ihr Boot nur", "tokens": ["Nur", "weg", "zu", "fl\u00fc\u00b7geln", "!", "Sieh", "!", "da", "ist", "ihr", "Boot", "nur"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PTKZU", "VVINF", "$.", "NE", "$.", "ADV", "VAFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schon Wolke \u2013 nur ein Punkt,", "tokens": ["Schon", "Wol\u00b7ke", "\u2013", "nur", "ein", "Punkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$(", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ein schwarzer Punkt im Meer. O Freund, wie Alles,", "tokens": ["Ein", "schwar\u00b7zer", "Punkt", "im", "Meer", ".", "O", "Freund", ",", "wie", "Al\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$.", "NE", "NN", "$,", "PWAV", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Einst Alles, Alles uns wird sein", "tokens": ["Einst", "Al\u00b7les", ",", "Al\u00b7les", "uns", "wird", "sein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "$,", "PIS", "PPER", "VAFIN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein schwarzer Punkt im Meer! Verlorne Freunde", "tokens": ["Ein", "schwar\u00b7zer", "Punkt", "im", "Meer", "!", "Ver\u00b7lor\u00b7ne", "Freun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$.", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Freud' und Lebenszeit", "tokens": ["Und", "Freud'", "und", "Le\u00b7bens\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Im Ungewitter abgerissen! Schone,", "tokens": ["Im", "Un\u00b7ge\u00b7wit\u00b7ter", "ab\u00b7ge\u00b7ris\u00b7sen", "!", "Scho\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$.", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du wilder Sohn des Himmels, Du!", "tokens": ["Du", "wil\u00b7der", "Sohn", "des", "Him\u00b7mels", ",", "Du", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "NN", "$,", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du kehrst mit ihnen eine Welt voll edler", "tokens": ["Du", "kehrst", "mit", "ih\u00b7nen", "ei\u00b7ne", "Welt", "voll", "ed\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verlorner Freund' hinab!", "tokens": ["Ver\u00b7lor\u00b7ner", "Freund'", "hin\u00b7ab", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Sie waren edel, waren meiner Jugend", "tokens": ["Sie", "wa\u00b7ren", "e\u00b7del", ",", "wa\u00b7ren", "mei\u00b7ner", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der sch\u00f6nste Theil! die Lebenszeit", "tokens": ["Der", "sch\u00f6ns\u00b7te", "Theil", "!", "die", "Le\u00b7bens\u00b7zeit"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Freude! waren mir wie jene Fluren,", "tokens": ["Der", "Freu\u00b7de", "!", "wa\u00b7ren", "mir", "wie", "je\u00b7ne", "Flu\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "VAFIN", "PPER", "KOKOM", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die ich geno\u00df und sang!", "tokens": ["Die", "ich", "ge\u00b7no\u00df", "und", "sang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Geno\u00df und sang! Dort fliehn sie, Freund' und Fluren,", "tokens": ["Ge\u00b7no\u00df", "und", "sang", "!", "Dort", "fliehn", "sie", ",", "Freund'", "und", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Genossen und verloren mir,", "tokens": ["Ge\u00b7nos\u00b7sen", "und", "ver\u00b7lo\u00b7ren", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Lenz', ach! nimmer, nimmer wiederkehrend,", "tokens": ["Wie", "Lenz'", ",", "ach", "!", "nim\u00b7mer", ",", "nim\u00b7mer", "wie\u00b7der\u00b7keh\u00b7rend", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "ITJ", "$.", "ADV", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Freund, wie uns einst die Welt! \u2013", "tokens": ["Freund", ",", "wie", "uns", "einst", "die", "Welt", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "ADV", "ART", "NN", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.12": {"line.1": {"text": "Denn sieh, dort sinkt der Himmel, dessen Kindern,", "tokens": ["Denn", "sieh", ",", "dort", "sinkt", "der", "Him\u00b7mel", ",", "des\u00b7sen", "Kin\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "ADV", "VVFIN", "ART", "NN", "$,", "PRELAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem holden West- und Abendroth", "tokens": ["Dem", "hol\u00b7den", "West", "und", "A\u00b7ben\u00b7droth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ihrer holdren Schwester Morgenr\u00f6the,", "tokens": ["Und", "ih\u00b7rer", "hold\u00b7ren", "Schwes\u00b7ter", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wir dort um Gunst gebuhlt.", "tokens": ["Wir", "dort", "um", "Gunst", "ge\u00b7buhlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Sieh noch den Himmel, Mann! er wird schon Wolke,", "tokens": ["Sieh", "noch", "den", "Him\u00b7mel", ",", "Mann", "!", "er", "wird", "schon", "Wol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$,", "NN", "$.", "PPER", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Senkt scheidend schon sein Angesicht", "tokens": ["Senkt", "schei\u00b7dend", "schon", "sein", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In tr\u00fcben Meeressaum. Seh' ich Dich wieder,", "tokens": ["In", "tr\u00fc\u00b7ben", "Mee\u00b7res\u00b7saum", ".", "Seh'", "ich", "Dich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Du 