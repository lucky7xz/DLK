{"textgrid.poem.63195": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es halten deine blumenhaften H\u00e4nde", "tokens": ["Es", "hal\u00b7ten", "dei\u00b7ne", "blu\u00b7men\u00b7haf\u00b7ten", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Erde Achse, die sich leise dreht.", "tokens": ["Der", "Er\u00b7de", "A\u00b7chse", ",", "die", "sich", "lei\u00b7se", "dreht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und selbst des Krieges blutendes Ger\u00e4t", "tokens": ["Und", "selbst", "des", "Krie\u00b7ges", "blu\u00b7ten\u00b7des", "Ge\u00b7r\u00e4t"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wird Erntesichel \u00fcberm Herbstgel\u00e4nde.", "tokens": ["Wird", "Ern\u00b7te\u00b7si\u00b7chel", "\u00fc\u00b7berm", "Herbst\u00b7ge\u00b7l\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es rauschen hinter deinem Felsenhaupt", "tokens": ["Es", "rau\u00b7schen", "hin\u00b7ter", "dei\u00b7nem", "Fel\u00b7sen\u00b7haupt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die violetten Str\u00f6me in den Adern.", "tokens": ["Die", "vi\u00b7o\u00b7let\u00b7ten", "Str\u00f6\u00b7me", "in", "den", "A\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und deine blauen Blicke blondbelaubt", "tokens": ["Und", "dei\u00b7ne", "blau\u00b7en", "Bli\u00b7cke", "blond\u00b7be\u00b7laubt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Entketten sich zu seligen Fluggeschwadern.", "tokens": ["Ent\u00b7ket\u00b7ten", "sich", "zu", "se\u00b7li\u00b7gen", "Flug\u00b7ge\u00b7schwa\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich sehe wohl die leuchtenden Maschinen,", "tokens": ["Ich", "se\u00b7he", "wohl", "die", "leuch\u00b7ten\u00b7den", "Ma\u00b7schi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Allein ich bin im Fernen irgendwo,", "tokens": ["Al\u00b7lein", "ich", "bin", "im", "Fer\u00b7nen", "ir\u00b7gend\u00b7wo", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "APPRART", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In Gr\u00f6nland und als Eskimo,", "tokens": ["In", "Gr\u00f6n\u00b7land", "und", "als", "Es\u00b7ki\u00b7mo", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "KOUS", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um dort dem Walfisch und dem Tran zu dienen.", "tokens": ["Um", "dort", "dem", "Wal\u00b7fisch", "und", "dem", "Tran", "zu", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ART", "NN", "KON", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}