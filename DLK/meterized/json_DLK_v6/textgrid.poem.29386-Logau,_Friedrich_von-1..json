{"textgrid.poem.29386": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1651, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So mirs gehet, wie ich wil,", "tokens": ["So", "mirs", "ge\u00b7het", ",", "wie", "ich", "wil", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00fcntsch ich Leser nicht zu viel;", "tokens": ["W\u00fcnt\u00b7sch", "ich", "Le\u00b7ser", "nicht", "zu", "viel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "PTKNEG", "PTKA", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn viel Leser sind viel Richter;", "tokens": ["Denn", "viel", "Le\u00b7ser", "sind", "viel", "Rich\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vielen aber taug kein Tichter.", "tokens": ["Vie\u00b7len", "a\u00b7ber", "taug", "kein", "Tich\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}