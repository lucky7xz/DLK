{"dta.poem.15597": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den durst'gen Gaumen labt ein Trunk, und nicht den satten;", "tokens": ["Den", "dur\u00b7st'\u00b7gen", "Gau\u00b7men", "labt", "ein", "Trunk", ",", "und", "nicht", "den", "sat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,", "KON", "PTKNEG", "ART", "VVFIN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Doch gr\u00fcnem kommt der Thau, nicht d\u00fcrrem Holz zu Statten.", "tokens": ["Doch", "gr\u00fc\u00b7nem", "kommt", "der", "Thau", ",", "nicht", "d\u00fcr\u00b7rem", "Holz", "zu", "Stat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVFIN", "ART", "NN", "$,", "PTKNEG", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ohn' Unzul\u00e4nglichkeit wirst du kein Heil verlangen,", "tokens": ["Ohn'", "Un\u00b7zu\u00b7l\u00e4ng\u00b7lich\u00b7keit", "wirst", "du", "kein", "Heil", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ohn' Empf\u00e4nglichkeit kannst du's auch nicht empfangen.", "tokens": ["Doch", "ohn'", "Emp\u00b7f\u00e4ng\u00b7lich\u00b7keit", "kannst", "du's", "auch", "nicht", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "wor