{"textgrid.poem.42958": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich kenne den Herrn Junkers nicht.", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kenne den Herrn Junkers nicht.", "tokens": ["Ich", "ken\u00b7ne", "den", "Herrn", "Jun\u00b7kers", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mag es auch schmeichlerisch klingen,", "tokens": ["Mag", "es", "auch", "schmeich\u00b7le\u00b7risch", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Ich widme ihm dennoch dies Gedicht,", "tokens": ["Ich", "wid\u00b7me", "ihm", "den\u00b7noch", "dies", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PDS", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Beschwingt von seinen Schwingen.", "tokens": ["Be\u00b7schwingt", "von", "sei\u00b7nen", "Schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Aus meiner Laune steigt es frei,", "tokens": ["Aus", "mei\u00b7ner", "Lau\u00b7ne", "steigt", "es", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entflogen, nicht entwachsen.", "tokens": ["Ent\u00b7flo\u00b7gen", ",", "nicht", "ent\u00b7wach\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es reimt sich Kriecher- und Fliegerei", "tokens": ["Es", "reimt", "sich", "Krie\u00b7cher", "und", "Flie\u00b7ge\u00b7rei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "TRUNC", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nicht einmal gut in Sachsen.", "tokens": ["Nicht", "ein\u00b7mal", "gut", "in", "Sach\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "APPR", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich bin mit Junkers' Maschinen schon", "tokens": ["Ich", "bin", "mit", "Jun\u00b7ker\u00b7s'", "Ma\u00b7schi\u00b7nen", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NE", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Oft \u00fcber die Lande geflogen,", "tokens": ["Oft", "\u00fc\u00b7ber", "die", "Lan\u00b7de", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Hab meinen Tages- und Wochenlohn", "tokens": ["Hab", "mei\u00b7nen", "Ta\u00b7ges", "und", "Wo\u00b7chen\u00b7lohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "TRUNC", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dar\u00fcber oft weit \u00fcberzogen,", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "oft", "weit", "\u00fc\u00b7berz\u00b7o\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sprach immer zu mir zuvor: \u00bb\u00dcberleg's", "tokens": ["Sprach", "im\u00b7mer", "zu", "mir", "zu\u00b7vor", ":", "\u00bb", "\u00dc\u00b7ber\u00b7leg's"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["NN", "ADV", "APPR", "PPER", "ADV", "$.", "$(", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dir!\u00ab \u2013 Aber flog doch auf gehimmelt", "tokens": ["Dir", "!", "\u00ab", "\u2013", "A\u00b7ber", "flog", "doch", "auf", "ge\u00b7him\u00b7melt"], "token_info": ["word", "punct", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$.", "$(", "$(", "KON", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durch Wetter und Wolken. Und fand unterwegs", "tokens": ["Durch", "Wet\u00b7ter", "und", "Wol\u00b7ken", ".", "Und", "fand", "un\u00b7ter\u00b7wegs"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$.", "KON", "VVFIN", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ein Gl\u00fcck, das unten verschimmelt.", "tokens": ["Ein", "Gl\u00fcck", ",", "das", "un\u00b7ten", "ver\u00b7schim\u00b7melt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Ich stehe nun \u2013 scheint's mir \u2013 auf gleichem Fu\u00df", "tokens": ["Ich", "ste\u00b7he", "nun", "\u2013", "scheint's", "mir", "\u2013", "auf", "glei\u00b7chem", "Fu\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$(", "VVFIN", "PPER", "$(", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit den M\u00f6wen, Adlern und Schwalben.", "tokens": ["Mit", "den", "M\u00f6\u00b7wen", ",", "Ad\u00b7lern", "und", "Schwal\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich sende Herrn Junkers meinen Gru\u00df", "tokens": ["Ich", "sen\u00b7de", "Herrn", "Jun\u00b7kers", "mei\u00b7nen", "Gru\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "NE", "PPOSAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und komme ihm zweimal einen Halben.", "tokens": ["Und", "kom\u00b7me", "ihm", "zwei\u00b7mal", "ei\u00b7nen", "Hal\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}