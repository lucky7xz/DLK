{"textgrid.poem.28207": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die schwartze Reuterey war alter Zeit die beste;", "tokens": ["Die", "schwart\u00b7ze", "Reu\u00b7te\u00b7rey", "war", "al\u00b7ter", "Zeit", "die", "bes\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJA", "NN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die beste Reuterey ist dieser Zeit die feste.", "tokens": ["Die", "bes\u00b7te", "Reu\u00b7te\u00b7rey", "ist", "die\u00b7ser", "Zeit", "die", "fes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PDAT", "NN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zwar ware sie dort schwartz von Farben, nicht von Sinnen;", "tokens": ["Zwar", "wa\u00b7re", "sie", "dort", "schwartz", "von", "Far\u00b7ben", ",", "nicht", "von", "Sin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "APPR", "NN", "$,", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zwar ist sie hier nicht schwartz von aussen, doch von innen.", "tokens": ["Zwar", "ist", "sie", "hier", "nicht", "schwartz", "von", "aus\u00b7sen", ",", "doch", "von", "in\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "APPR", "VVINF", "$,", "ADV", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}