{"textgrid.poem.24062": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Innocentia", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der klare Blick gradaus, weit in Welt,", "tokens": ["Der", "kla\u00b7re", "Blick", "gra\u00b7daus", ",", "weit", "in", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und eine Welt in diesem klaren Blicke:", "tokens": ["Und", "ei\u00b7ne", "Welt", "in", "die\u00b7sem", "kla\u00b7ren", "Bli\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da ruht die Liebe und der Schmerz im Traum,", "tokens": ["Da", "ruht", "die", "Lie\u00b7be", "und", "der", "Schmerz", "im", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Sch\u00f6nheit schl\u00e4gt die Wogen dr\u00fcber her", "tokens": ["Und", "Sch\u00f6n\u00b7heit", "schl\u00e4gt", "die", "Wo\u00b7gen", "dr\u00fc\u00b7ber", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "PAV", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie Fr\u00fchlingswind. Der schlanke Lilienstengel", "tokens": ["Wie", "Fr\u00fch\u00b7lings\u00b7wind", ".", "Der", "schlan\u00b7ke", "Li\u00b7li\u00b7ens\u00b7ten\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In wei\u00dfer Hand ragt unbewegt und heilig.", "tokens": ["In", "wei\u00b7\u00dfer", "Hand", "ragt", "un\u00b7be\u00b7wegt", "und", "hei\u00b7lig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Augen schlo\u00df ich, und dasselbe Bild", "tokens": ["Die", "Au\u00b7gen", "schlo\u00df", "ich", ",", "und", "das\u00b7sel\u00b7be", "Bild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KON", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sah meine Seele, ganz denselben Blick,", "tokens": ["Sah", "mei\u00b7ne", "See\u00b7le", ",", "ganz", "den\u00b7sel\u00b7ben", "Blick", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So voller Reinheit, Sch\u00f6ne und voll Liebe,", "tokens": ["So", "vol\u00b7ler", "Rein\u00b7heit", ",", "Sch\u00f6\u00b7ne", "und", "voll", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "NN", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch statt des Lilienstengels ruht im Arm", "tokens": ["Doch", "statt", "des", "Li\u00b7li\u00b7ens\u00b7ten\u00b7gels", "ruht", "im", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ein schlafendes Bambino. Mutterunschuld!", "tokens": ["Ein", "schla\u00b7fen\u00b7des", "Bam\u00b7bi\u00b7no", ".", "Mut\u00b7ter\u00b7un\u00b7schuld", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Die Welt schien mir an diesem Tage sch\u00f6n.", "tokens": ["Die", "Welt", "schien", "mir", "an", "die\u00b7sem", "Ta\u00b7ge", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PDAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}