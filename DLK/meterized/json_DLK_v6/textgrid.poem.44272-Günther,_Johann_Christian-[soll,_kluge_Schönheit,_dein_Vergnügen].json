{"textgrid.poem.44272": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[soll, kluge Sch\u00f6nheit, dein Vergn\u00fcgen]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Soll, kluge Sch\u00f6nheit, dein Vergn\u00fcgen", "tokens": ["Soll", ",", "klu\u00b7ge", "Sch\u00f6n\u00b7heit", ",", "dein", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "$,", "ADJA", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit deiner Brust ins Kloster gehn,", "tokens": ["Mit", "dei\u00b7ner", "Brust", "ins", "Klos\u00b7ter", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie, soll der Garthen brache liegen,", "tokens": ["Wie", ",", "soll", "der", "Gar\u00b7then", "bra\u00b7che", "lie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "VMFIN", "ART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf welchem Zuckerrosen stehn?", "tokens": ["Auf", "wel\u00b7chem", "Zu\u00b7cker\u00b7ro\u00b7sen", "stehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was wiltu, da sich andre freuen,", "tokens": ["Was", "wil\u00b7tu", ",", "da", "sich", "and\u00b7re", "freu\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$,", "KOUS", "PRF", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Fasten deinen Leib casteien?", "tokens": ["Mit", "Fas\u00b7ten", "dei\u00b7nen", "Leib", "cas\u00b7tei\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, sch\u00f6nes Kind, die enge Zelle", "tokens": ["Ach", ",", "sch\u00f6\u00b7nes", "Kind", ",", "die", "en\u00b7ge", "Zel\u00b7le"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist deiner Hofnung weites Grab,", "tokens": ["Ist", "dei\u00b7ner", "Hof\u00b7nung", "wei\u00b7tes", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier w\u00e4chst und ist die Qual der H\u00f6lle,", "tokens": ["Hier", "w\u00e4chst", "und", "ist", "die", "Qual", "der", "H\u00f6l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hier nimmt der Kern des Lebens ab,", "tokens": ["Hier", "nimmt", "der", "Kern", "des", "Le\u00b7bens", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und in den bangen Kirchenmauren", "tokens": ["Und", "in", "den", "ban\u00b7gen", "Kir\u00b7chen\u00b7mau\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mu\u00df auch Canarisect versauren.", "tokens": ["Mu\u00df", "auch", "Ca\u00b7na\u00b7ri\u00b7sect", "ver\u00b7sau\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das Jungfernhonig n\u00e4hrt die Galle,", "tokens": ["Das", "Jung\u00b7fern\u00b7ho\u00b7nig", "n\u00e4hrt", "die", "Gal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Einsamkeit gebiehrt den Tod,", "tokens": ["Die", "Ein\u00b7sam\u00b7keit", "ge\u00b7biehrt", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Jungfrau schwindet vor dem Falle", "tokens": ["Die", "Jung\u00b7frau", "schwin\u00b7det", "vor", "dem", "Fal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und leidet ohne Leiden Noth,", "tokens": ["Und", "lei\u00b7det", "oh\u00b7ne", "Lei\u00b7den", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Rosenkranz, der Freyheit Ende,", "tokens": ["Der", "Ro\u00b7sen\u00b7kranz", ",", "der", "Frey\u00b7heit", "En\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Beschwert der Nonnen Herz und H\u00e4nde.", "tokens": ["Be\u00b7schwert", "der", "Non\u00b7nen", "Herz", "und", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Komm, las dich in ein Kloster f\u00fchren,", "tokens": ["Komm", ",", "las", "dich", "in", "ein", "Klos\u00b7ter", "f\u00fch\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wozu der Abt den Schl\u00fc\u00dfel tr\u00e4gt", "tokens": ["Wo\u00b7zu", "der", "Abt", "den", "Schl\u00fc\u00b7\u00dfel", "tr\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Amor \u00fcber alle Th\u00fcren", "tokens": ["Und", "A\u00b7mor", "\u00fc\u00b7ber", "al\u00b7le", "Th\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NE", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dies in erhabner Schrift gepr\u00e4gt:", "tokens": ["Dies", "in", "er\u00b7hab\u00b7ner", "Schrift", "ge\u00b7pr\u00e4gt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu unsrer lieben Frauen Orden", "tokens": ["Zu", "uns\u00b7rer", "lie\u00b7ben", "Frau\u00b7en", "Or\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist dieser Ort gewidmet worden.", "tokens": ["Ist", "die\u00b7ser", "Ort", "ge\u00b7wid\u00b7met", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "VVPP", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Den Altar geben deine Br\u00fcste,", "tokens": ["Den", "Al\u00b7tar", "ge\u00b7ben", "dei\u00b7ne", "Br\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Rauchwerck gl\u00fcht in deiner Schoos;", "tokens": ["Das", "Rauch\u00b7werck", "gl\u00fcht", "in", "dei\u00b7ner", "Schoos", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier stillen wir des Fleisches L\u00fcste", "tokens": ["Hier", "stil\u00b7len", "wir", "des", "Flei\u00b7sches", "L\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und d\u00e4mpfen sie auf einen Sto\u00df,", "tokens": ["Und", "d\u00e4mp\u00b7fen", "sie", "auf", "ei\u00b7nen", "Sto\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bis wir durch ein geschw\u00e4chtes K\u00fc\u00dfen", "tokens": ["Bis", "wir", "durch", "ein", "ge\u00b7schw\u00e4ch\u00b7tes", "K\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auch in das Complet treten m\u00fc\u00dfen.", "tokens": ["Auch", "in", "das", "Com\u00b7plet", "tre\u00b7ten", "m\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}