{"textgrid.poem.45995": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[der Herr ritt nach Jerusalem]", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Herr ritt nach Jerusalem", "tokens": ["Der", "Herr", "ritt", "nach", "Je\u00b7ru\u00b7sa\u00b7lem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vor achtzehnhundertsiebzehn Jahr,", "tokens": ["Vor", "acht\u00b7zehn\u00b7hun\u00b7dert\u00b7sieb\u00b7zehn", "Jahr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Frommen war er angenehm", "tokens": ["Den", "From\u00b7men", "war", "er", "an\u00b7ge\u00b7nehm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie warfen ihre Kleider dar", "tokens": ["Sie", "war\u00b7fen", "ih\u00b7re", "Klei\u00b7der", "dar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und streuten Palmen auf den Weg", "tokens": ["Und", "streu\u00b7ten", "Pal\u00b7men", "auf", "den", "Weg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sangen Hosianna laut", "tokens": ["Und", "san\u00b7gen", "Ho\u00b7si\u00b7an\u00b7na", "laut"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NE", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O selig, wer den gr\u00fcnen Steg", "tokens": ["O", "se\u00b7lig", ",", "wer", "den", "gr\u00fc\u00b7nen", "Steg"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "$,", "PWS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und den, der auf ihm zog, erschaut!", "tokens": ["Und", "den", ",", "der", "auf", "ihm", "zog", ",", "er\u00b7schaut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mir aber ist am Palmentag", "tokens": ["Mir", "a\u00b7ber", "ist", "am", "Pal\u00b7men\u00b7tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ein zweites Einzugsfest bestellt,", "tokens": ["Ein", "zwei\u00b7tes", "Ein\u00b7zugs\u00b7fest", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Hosianna ich auch singen mag,", "tokens": ["Ho\u00b7si\u00b7an\u00b7na", "ich", "auch", "sin\u00b7gen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Ein fromm Kind zog heut ein zur Welt,", "tokens": ["Ein", "fromm", "Kind", "zog", "heut", "ein", "zur", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "ADV", "ART", "APPRART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.13": {"text": "Der Fr\u00fchling zog sein R\u00f6cklein aus", "tokens": ["Der", "Fr\u00fch\u00b7ling", "zog", "sein", "R\u00f6ck\u00b7lein", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und breitet es auf seiner Bahn,", "tokens": ["Und", "brei\u00b7tet", "es", "auf", "sei\u00b7ner", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und streute manchen Veilchenstrau\u00df,", "tokens": ["Und", "streu\u00b7te", "man\u00b7chen", "Veil\u00b7chen\u00b7strau\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Hosianna stimmten die V\u00f6glein an.", "tokens": ["Ho\u00b7si\u00b7an\u00b7na", "stimm\u00b7ten", "die", "V\u00f6\u00b7glein", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Vor nit gar lang, vor neunzehn Jahr", "tokens": ["Vor", "nit", "gar", "lang", ",", "vor", "neun\u00b7zehn", "Jahr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PTKNEG", "ADV", "ADJD", "$,", "APPR", "CARD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Fr\u00fch morgens um die sechste Stund'", "tokens": ["Fr\u00fch", "mor\u00b7gens", "um", "die", "sechs\u00b7te", "Stund'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.19": {"text": "Ward wohl mein Himmel sonnenklar,", "tokens": ["Ward", "wohl", "mein", "Him\u00b7mel", "son\u00b7nen\u00b7klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Ward wohl mein ganzes Gl\u00fcck gesund.", "tokens": ["Ward", "wohl", "mein", "gan\u00b7zes", "Gl\u00fcck", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Im Priesterhaus zu Linum ward", "tokens": ["Im", "Pries\u00b7ter\u00b7haus", "zu", "Li\u00b7num", "ward"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NE", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Geboren mir zu frommer Lust", "tokens": ["Ge\u00b7bo\u00b7ren", "mir", "zu", "from\u00b7mer", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Ein M\u00e4gdlein recht nach Christenart,", "tokens": ["Ein", "M\u00e4gd\u00b7lein", "recht", "nach", "Chris\u00b7ten\u00b7art", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Ach h\u00e4tt' ich's damals schon gewu\u00dft!", "tokens": ["Ach", "h\u00e4tt'", "ich's", "da\u00b7mals", "schon", "ge\u00b7wu\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PIS", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Da zog mein Himmelschl\u00fcsselbein,", "tokens": ["Da", "zog", "mein", "Him\u00b7mel\u00b7schl\u00fcs\u00b7sel\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Mein Herz, mein Seel', mein Du, mein Ich,", "tokens": ["Mein", "Herz", ",", "mein", "Seel'", ",", "mein", "Du", ",", "mein", "Ich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "PPER", "$,", "PPOSAT", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Mein lieb Linum zur Welt herein.", "tokens": ["Mein", "lieb", "Li\u00b7num", "zur", "Welt", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NE", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Wie liebte da mein Heiland mich!", "tokens": ["Wie", "lieb\u00b7te", "da", "mein", "Hei\u00b7land", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PPOSAT", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Und streuen will ich nun fortan", "tokens": ["Und", "streu\u00b7en", "will", "ich", "nun", "for\u00b7tan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Was ich vermag an frommer Zier", "tokens": ["Was", "ich", "ver\u00b7mag", "an", "from\u00b7mer", "Zier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Wohl meinem Linum auf die Bahn,", "tokens": ["Wohl", "mei\u00b7nem", "Li\u00b7num", "auf", "die", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Sie wei\u00df wohl, Herr, ich streu' es dir.", "tokens": ["Sie", "wei\u00df", "wohl", ",", "Herr", ",", "ich", "streu'", "es", "dir", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "NN", "$,", "PPER", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Ich werf' zur Erd' mein altes Kleid,", "tokens": ["Ich", "wer\u00b7f'", "zur", "Erd'", "mein", "al\u00b7tes", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.34": {"text": "Brech' ab die \u00fcpp'gen Zweige mein,", "tokens": ["Brech'", "ab", "die", "\u00fcpp'\u00b7gen", "Zwei\u00b7ge", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Beginne eine neue Zeit", "tokens": ["Be\u00b7gin\u00b7ne", "ei\u00b7ne", "neu\u00b7e", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Und werde wieder klar und rein.", "tokens": ["Und", "wer\u00b7de", "wie\u00b7der", "klar", "und", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Ich darf nicht l\u00e4nger dumm und blind", "tokens": ["Ich", "darf", "nicht", "l\u00e4n\u00b7ger", "dumm", "und", "blind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "In deine lieben Augen sehn,", "tokens": ["In", "dei\u00b7ne", "lie\u00b7ben", "Au\u00b7gen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Ich mu\u00df ganz rein, du liebes Kind", "tokens": ["Ich", "mu\u00df", "ganz", "rein", ",", "du", "lie\u00b7bes", "Kind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "$,", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Vor deinem ird'schen Zuge gehn.", "tokens": ["Vor", "dei\u00b7nem", "ird'\u00b7schen", "Zu\u00b7ge", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Lieb Linum hab' nur guten Mut", "tokens": ["Lieb", "Li\u00b7num", "hab'", "nur", "gu\u00b7ten", "Mut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "VAFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Bleib mir nur treu, ich werd' bald fromm", "tokens": ["Bleib", "mir", "nur", "treu", ",", "ich", "werd'", "bald", "fromm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "So fromm wie du und still und gut,", "tokens": ["So", "fromm", "wie", "du", "und", "still", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Da\u00df ich mit dir zum Himmel komm'!", "tokens": ["Da\u00df", "ich", "mit", "dir", "zum", "Him\u00b7mel", "komm'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Da reit' ich auf der Eselin,", "tokens": ["Da", "reit'", "ich", "auf", "der", "E\u00b7se\u00b7lin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Du setzt dich auf das F\u00fcllen klein,", "tokens": ["Du", "setzt", "dich", "auf", "das", "F\u00fcl\u00b7len", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "So ziehn wir mit einf\u00e4lt'gem Sinn", "tokens": ["So", "ziehn", "wir", "mit", "ein\u00b7f\u00e4lt'\u00b7gem", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Nach neu Jerusalem hinein.", "tokens": ["Nach", "neu", "Je\u00b7ru\u00b7sa\u00b7lem", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "NE", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Sei nur nicht b\u00f6s, es ist nun so,", "tokens": ["Sei", "nur", "nicht", "b\u00f6s", ",", "es", "ist", "nun", "so", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der liebe Gott aufs Herz nur sieht,", "tokens": ["Der", "lie\u00b7be", "Gott", "aufs", "Herz", "nur", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So tust du auch, drum bin ich froh,", "tokens": ["So", "tust", "du", "auch", ",", "drum", "bin", "ich", "froh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PAV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und geb' dir dies einf\u00e4lt'ge Lied.", "tokens": ["Und", "geb'", "dir", "dies", "ein\u00b7f\u00e4lt'\u00b7ge", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDS", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}