{"textgrid.poem.44313": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: So feurig, unverf\u00e4lscht und ", "genre": "verse", "period": "N.A.", "pub_year": 1808, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So feurig, unverf\u00e4lscht und ", "tokens": ["So", "feu\u00b7rig", ",", "un\u00b7ver\u00b7f\u00e4lscht", "und"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADJD", "KON"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "wie unsers Vaterlandes ", "tokens": ["wie", "un\u00b7sers", "Va\u00b7ter\u00b7lan\u00b7des"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "mu\u00df Freundschaft sein; fest mu\u00df sie ", "tokens": ["mu\u00df", "Freund\u00b7schaft", "sein", ";", "fest", "mu\u00df", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "NN", "VAINF", "$.", "ADJD", "VMFIN", "PPER"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "wenn auch des Schicksals M\u00e4chte ", "tokens": ["wenn", "auch", "des", "Schick\u00b7sals", "M\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie kann uns Seligkeit ", "tokens": ["Sie", "kann", "uns", "Se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "selbst wenn wir mit dem Ungl\u00fcck ", "tokens": ["selbst", "wenn", "wir", "mit", "dem", "Un\u00b7gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "und nimmer reizt selbst Kr\u00f6sus ", "tokens": ["und", "nim\u00b7mer", "reizt", "selbst", "Kr\u00f6\u00b7sus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "den Gl\u00fccklichen, dem sie ist ", "tokens": ["den", "Gl\u00fcck\u00b7li\u00b7chen", ",", "dem", "sie", "ist"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VAFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "er wird nicht nach dem Gl\u00fccke ", "tokens": ["er", "wird", "nicht", "nach", "dem", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "um das sonst Menschenkinder ", "tokens": ["um", "das", "sonst", "Men\u00b7schen\u00b7kin\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "und wenn die Freunde Freund ihn ", "tokens": ["und", "wenn", "die", "Freun\u00b7de", "Freund", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "kann keine Unbild ihn ", "tokens": ["kann", "kei\u00b7ne", "Un\u00b7bild", "ihn"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PIAT", "NN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}