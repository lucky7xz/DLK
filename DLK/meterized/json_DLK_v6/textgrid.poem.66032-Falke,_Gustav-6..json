{"textgrid.poem.66032": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwie meldest du's ihr, wie nimmt sie es auf?\u00ab", "tokens": ["\u00bb", "wie", "mel\u00b7dest", "du's", "ihr", ",", "wie", "nimmt", "sie", "es", "auf", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PIS", "PPER", "$,", "PWAV", "VVFIN", "PPER", "PPER", "PTKVZ", "$.", "$("], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So fragte ich mich, und stockend dann quollen", "tokens": ["So", "frag\u00b7te", "ich", "mich", ",", "und", "sto\u00b7ckend", "dann", "quol\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "KON", "ADJD", "ADV", "VVINF"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Worte hervor nur. So hindert den Lauf", "tokens": ["Die", "Wor\u00b7te", "her\u00b7vor", "nur", ".", "So", "hin\u00b7dert", "den", "Lauf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "ADV", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Des klaren Baches der plumpe Stein,", "tokens": ["Des", "kla\u00b7ren", "Ba\u00b7ches", "der", "plum\u00b7pe", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der, Schlamm aufw\u00fchlend, die Flut verdickt.", "tokens": ["Der", ",", "Schlamm", "auf\u00b7w\u00fch\u00b7lend", ",", "die", "Flut", "ver\u00b7dickt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "NN", "VVPP", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Doch blieb sie still bei dem unheilvollen", "tokens": ["Doch", "blieb", "sie", "still", "bei", "dem", "un\u00b7heil\u00b7vol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "ADJA"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Bericht, und als ich beschwor sie, erstickt", "tokens": ["Be\u00b7richt", ",", "und", "als", "ich", "be\u00b7schwor", "sie", ",", "er\u00b7stickt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "KON", "KOUS", "PPER", "VVFIN", "PPER", "$,", "VVPP"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Jedes Wort halb im Schlund, die Schuld w\u00e4r' nicht mein,", "tokens": ["Je\u00b7des", "Wort", "halb", "im", "Schlund", ",", "die", "Schuld", "w\u00e4r'", "nicht", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "APPRART", "NN", "$,", "ART", "NN", "VAFIN", "PTKNEG", "PPOSAT", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Ich w\u00e4re kein M\u00f6rder, da sah sie mich an", "tokens": ["Ich", "w\u00e4\u00b7re", "kein", "M\u00f6r\u00b7der", ",", "da", "sah", "sie", "mich", "an"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "ADV", "VVFIN", "PPER", "PRF", "APPR"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Mit gro\u00dfen Augen und gab mir die Hand.", "tokens": ["Mit", "gro\u00b7\u00dfen", "Au\u00b7gen", "und", "gab", "mir", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "\u00bbihr seid ohne Schuld\u00ab sprach leise sie drauf,", "tokens": ["\u00bb", "ihr", "seid", "oh\u00b7ne", "Schuld", "\u00ab", "sprach", "lei\u00b7se", "sie", "drauf", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "APPR", "NN", "$(", "VVFIN", "ADJD", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.12": {"text": "\u00bbgott sei ihm gn\u00e4dig und uns.\u00ab Doch dann,", "tokens": ["\u00bb", "gott", "sei", "ihm", "gn\u00e4\u00b7dig", "und", "uns", ".", "\u00ab", "Doch", "dann", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PPER", "ADJD", "KON", "PPER", "$.", "$(", "KON", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Sie hatte schnell sich abgewandt,", "tokens": ["Sie", "hat\u00b7te", "schnell", "sich", "ab\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Kam's wie aus tiefstem Innern herauf,", "tokens": ["Kam's", "wie", "aus", "tiefs\u00b7tem", "In\u00b7nern", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Ein Schluchzen, ein Beben, und vor das Gesicht", "tokens": ["Ein", "Schluch\u00b7zen", ",", "ein", "Be\u00b7ben", ",", "und", "vor", "das", "Ge\u00b7sicht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.16": {"text": "Die H\u00e4nde schlagend, sie weinte nicht,", "tokens": ["Die", "H\u00e4n\u00b7de", "schla\u00b7gend", ",", "sie", "wein\u00b7te", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Nein, schien in Thr\u00e4nen zerflie\u00dfen zu wollen,", "tokens": ["Nein", ",", "schien", "in", "Thr\u00e4\u00b7nen", "zer\u00b7flie\u00b7\u00dfen", "zu", "wol\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "APPR", "NN", "VVINF", "PTKZU", "VMINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Die tropfenweis durch die Finger ihr quollen.", "tokens": ["Die", "trop\u00b7fen\u00b7weis", "durch", "die", "Fin\u00b7ger", "ihr", "quol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Da kehrte ich ab mich und lie\u00df sie allein,", "tokens": ["Da", "kehr\u00b7te", "ich", "ab", "mich", "und", "lie\u00df", "sie", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.20": {"text": "Und dachte nachher: Es wird so sein,", "tokens": ["Und", "dach\u00b7te", "nach\u00b7her", ":", "Es", "wird", "so", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$.", "PPER", "VAFIN", "ADV", "VAINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Sie hat mehr als ich verloren ihn;", "tokens": ["Sie", "hat", "mehr", "als", "ich", "ver\u00b7lo\u00b7ren", "ihn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KOUS", "PPER", "VVPP", "PPER", "$."], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.22": {"text": "Es ist alles so, wie es lange mir schien,", "tokens": ["Es", "ist", "al\u00b7les", "so", ",", "wie", "es", "lan\u00b7ge", "mir", "schien", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADV", "$,", "PWAV", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.23": {"text": "Und, ich leugne es nicht, ich g\u00f6nnte es ihr,", "tokens": ["Und", ",", "ich", "leug\u00b7ne", "es", "nicht", ",", "ich", "g\u00f6nn\u00b7te", "es", "ihr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "PPER", "PPER", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.24": {"text": "Und der Teufel hatte seine Lust an mir.", "tokens": ["Und", "der", "Teu\u00b7fel", "hat\u00b7te", "sei\u00b7ne", "Lust", "an", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.25": {"text": "\u00bbsie ist dein! sie ist dein! Was z\u00f6gerst du noch?\u00ab", "tokens": ["\u00bb", "sie", "ist", "dein", "!", "sie", "ist", "dein", "!", "Was", "z\u00f6\u00b7gerst", "du", "noch", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPOSAT", "$.", "PPER", "VAFIN", "PPOSAT", "$.", "PWS", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.26": {"text": "So h\u00f6rte ich's immer. Doch anfangs verkroch", "tokens": ["So", "h\u00f6r\u00b7te", "ich's", "im\u00b7mer", ".", "Doch", "an\u00b7fangs", "ver\u00b7kroch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$.", "KON", "ADV", "VVFIN"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.27": {"text": "Ich mich feige davor, verstopfte die Ohren,", "tokens": ["Ich", "mich", "fei\u00b7ge", "da\u00b7vor", ",", "ver\u00b7stopf\u00b7te", "die", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADJD", "PAV", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Doch waren der Tugend M\u00fchen verloren.", "tokens": ["Doch", "wa\u00b7ren", "der", "Tu\u00b7gend", "M\u00fc\u00b7hen", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Nach Tagen schon, und ich atmete frei:", "tokens": ["Nach", "Ta\u00b7gen", "schon", ",", "und", "ich", "at\u00b7me\u00b7te", "frei", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "$,", "KON", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.30": {"text": "Was qu\u00e4lst du dich, Narr! Ist's nicht einerlei?", "tokens": ["Was", "qu\u00e4lst", "du", "dich", ",", "Narr", "!", "Ist's", "nicht", "ei\u00b7ner\u00b7lei", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "$,", "NE", "$.", "NE", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Ob du oder er? Und was einem sie gab,", "tokens": ["Ob", "du", "o\u00b7der", "er", "?", "Und", "was", "ei\u00b7nem", "sie", "gab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KON", "PPER", "$.", "KON", "PWS", "ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.32": {"text": "Das schl\u00e4gt sie dem andern wohl auch nicht ab,", "tokens": ["Das", "schl\u00e4gt", "sie", "dem", "an\u00b7dern", "wohl", "auch", "nicht", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "ADJA", "ADV", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.33": {"text": "Und brauchst du Gewalt, wer will dich halten?", "tokens": ["Und", "brauchst", "du", "Ge\u00b7walt", ",", "wer", "will", "dich", "hal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$,", "PWS", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.34": {"text": "Du bist nun Herr und kannst frei hier schalten.", "tokens": ["Du", "bist", "nun", "Herr", "und", "kannst", "frei", "hier", "schal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "KON", "VMFIN", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und trat ich dann vor sie mit solchen Gedanken,", "tokens": ["Und", "trat", "ich", "dann", "vor", "sie", "mit", "sol\u00b7chen", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Dann f\u00fchlte den Stolz ich der St\u00e4rke schwanken,", "tokens": ["Dann", "f\u00fchl\u00b7te", "den", "Stolz", "ich", "der", "St\u00e4r\u00b7ke", "schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und f\u00fchlte mich klein und besch\u00e4mt, und schlich", "tokens": ["Und", "f\u00fchl\u00b7te", "mich", "klein", "und", "be\u00b7sch\u00e4mt", ",", "und", "schlich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,", "KON", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Vor einem Blick oft bei Seite mich.", "tokens": ["Vor", "ei\u00b7nem", "Blick", "oft", "bei", "Sei\u00b7te", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "APPR", "NN", "PPER", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ach, sie war sch\u00f6n, bei Gott, wie ein Weib", "tokens": ["Ach", ",", "sie", "war", "sch\u00f6n", ",", "bei", "Gott", ",", "wie", "ein", "Weib"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADJD", "$,", "APPR", "NN", "$,", "PWAV", "ART", "NN"], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ich selten sah, und so stolz und rein,", "tokens": ["Ich", "sel\u00b7ten", "sah", ",", "und", "so", "stolz", "und", "rein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "$,", "KON", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dass immer ich wieder beschwor, diesen Leib", "tokens": ["Dass", "im\u00b7mer", "ich", "wie\u00b7der", "be\u00b7schwor", ",", "die\u00b7sen", "Leib"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "ADV", "VVFIN", "$,", "PDAT", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Hat Jens nicht besessen, es kann nicht sein!", "tokens": ["Hat", "Jens", "nicht", "be\u00b7ses\u00b7sen", ",", "es", "kann", "nicht", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "PTKNEG", "VVINF", "$,", "PPER", "VMFIN", "PTKNEG", "VAINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Der Blick kann nicht l\u00fcgen, so still und klar", "tokens": ["Der", "Blick", "kann", "nicht", "l\u00fc\u00b7gen", ",", "so", "still", "und", "klar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVFIN", "$,", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Sieht kein Weib, das schon einmal erniedrigt war,", "tokens": ["Sieht", "kein", "Weib", ",", "das", "schon", "ein\u00b7mal", "er\u00b7nied\u00b7rigt", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,", "PRELS", "ADV", "ADV", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Einem Mann in die Augen, der ihrer begehrt.", "tokens": ["Ei\u00b7nem", "Mann", "in", "die", "Au\u00b7gen", ",", "der", "ih\u00b7rer", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "VVPP", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Und so hielt sie mich fern wie mit flammendem Schwert.", "tokens": ["Und", "so", "hielt", "sie", "mich", "fern", "wie", "mit", "flam\u00b7men\u00b7dem", "Schwert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "ADJD", "KOKOM", "APPR", "ADJA", "NN", "$."], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "Wie lange doch soll wohl solch Zustand bestehn?", "tokens": ["Wie", "lan\u00b7ge", "doch", "soll", "wohl", "solch", "Zu\u00b7stand", "be\u00b7stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "VMFIN", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Unter Menschen von Fleisch und Bein und Blut,", "tokens": ["Un\u00b7ter", "Men\u00b7schen", "von", "Fleisch", "und", "Bein", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Und jungem Blut und gekocht von der Glut", "tokens": ["Und", "jun\u00b7gem", "Blut", "und", "ge\u00b7kocht", "von", "der", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "KON", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Der Leidenschaft und der Tropenglut,", "tokens": ["Der", "Lei\u00b7den\u00b7schaft", "und", "der", "Tro\u00b7pen\u00b7glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "So im t\u00e4glichen Nebeneinandergehn,", "tokens": ["So", "im", "t\u00e4g\u00b7li\u00b7chen", "Ne\u00b7ben\u00b7ein\u00b7an\u00b7der\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.14": {"text": "Wie lange wohl? \u2013 Und so kam er, der Tag,", "tokens": ["Wie", "lan\u00b7ge", "wohl", "?", "\u2013", "Und", "so", "kam", "er", ",", "der", "Tag", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "$.", "$(", "KON", "ADV", "VVFIN", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Kam sicher, wo sie in den Armen mir lag.", "tokens": ["Kam", "si\u00b7cher", ",", "wo", "sie", "in", "den", "Ar\u00b7men", "mir", "lag", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "PWAV", "PPER", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.16": {"text": "Und nicht S\u00fcnde war es, nicht niedere Lust,", "tokens": ["Und", "nicht", "S\u00fcn\u00b7de", "war", "es", ",", "nicht", "nie\u00b7de\u00b7re", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "NN", "VAFIN", "PPER", "$,", "PTKNEG", "ADJA", "NN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Die sie endlich zwang an meine Brust.", "tokens": ["Die", "sie", "end\u00b7lich", "zwang", "an", "mei\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.18": {"text": "Ich liebte sie, wie man nur lieben kann,", "tokens": ["Ich", "lieb\u00b7te", "sie", ",", "wie", "man", "nur", "lie\u00b7ben", "kann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PIS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Und je schwerer den langen Kampf ich gewann,", "tokens": ["Und", "je", "schwe\u00b7rer", "den", "lan\u00b7gen", "Kampf", "ich", "ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-++--+-+--+", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Je herrlicher labte der Sieg zuletzt.", "tokens": ["Je", "herr\u00b7li\u00b7cher", "lab\u00b7te", "der", "Sieg", "zu\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Und sie gestand mir, was kaum ich gehofft,", "tokens": ["Und", "sie", "ge\u00b7stand", "mir", ",", "was", "kaum", "ich", "ge\u00b7hofft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "PRELS", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie auch sie sich umsonst zur Wehre gesetzt,", "tokens": ["Wie", "auch", "sie", "sich", "um\u00b7sonst", "zur", "Weh\u00b7re", "ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PRF", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Wie auch sie in Qualen gerungen oft,", "tokens": ["Wie", "auch", "sie", "in", "Qua\u00b7len", "ge\u00b7run\u00b7gen", "oft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "APPR", "NN", "VVPP", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Von gleicher Leidenschaft, gleicher Glut", "tokens": ["Von", "glei\u00b7cher", "Lei\u00b7den\u00b7schaft", ",", "glei\u00b7cher", "Glut"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Durchfiebert, wie ich, und schon lange mir gut,", "tokens": ["Durch\u00b7fie\u00b7bert", ",", "wie", "ich", ",", "und", "schon", "lan\u00b7ge", "mir", "gut", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "PPER", "$,", "KON", "ADV", "ADV", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Schon damals, als Jens \u2013 \u2013 doch mit Purpurscham", "tokens": ["Schon", "da\u00b7mals", ",", "als", "Jens", "\u2013", "\u2013", "doch", "mit", "Pur\u00b7pur\u00b7scham"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "NE", "$(", "$(", "ADV", "APPR", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Gestand sie mir leis, dass ans Ziel er nicht kam.", "tokens": ["Ge\u00b7stand", "sie", "mir", "leis", ",", "dass", "ans", "Ziel", "er", "nicht", "kam", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ADJD", "$,", "KOUS", "APPRART", "NN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Und dann rauschten die Wipfel der Palmen sacht", "tokens": ["Und", "dann", "rauschten", "die", "Wip\u00b7fel", "der", "Pal\u00b7men", "sacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Uns das Hochzeitslied in der ersten Nacht.", "tokens": ["Uns", "das", "Hoch\u00b7zeits\u00b7lied", "in", "der", "ers\u00b7ten", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Und war ich je gl\u00fccklich, so war es die Zeit", "tokens": ["Und", "war", "ich", "je", "gl\u00fcck\u00b7lich", ",", "so", "war", "es", "die", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "VAFIN", "PPER", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "In der weltverlassenen Einsamkeit.", "tokens": ["In", "der", "welt\u00b7ver\u00b7las\u00b7se\u00b7nen", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "So dachte ich mir das Paradies,", "tokens": ["So", "dach\u00b7te", "ich", "mir", "das", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und war kein Engel, der aus uns wies", "tokens": ["Und", "war", "kein", "En\u00b7gel", ",", "der", "aus", "uns", "wies"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PIAT", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Mit feurigem Schwert. Und so rann die Zeit,", "tokens": ["Mit", "feu\u00b7ri\u00b7gem", "Schwert", ".", "Und", "so", "rann", "die", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und wir w\u00fcnschten nichts mehr, und der Tod schien weit.", "tokens": ["Und", "wir", "w\u00fcnschten", "nichts", "mehr", ",", "und", "der", "Tod", "schien", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIS", "ADV", "$,", "KON", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Drei Jahre, da hat man sich eingew\u00f6hnt,", "tokens": ["Drei", "Jah\u00b7re", ",", "da", "hat", "man", "sich", "ein\u00b7ge\u00b7w\u00f6hnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "ADV", "VAFIN", "PIS", "PRF", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Hat abgeschlossen, sich ausges\u00f6hnt.", "tokens": ["Hat", "ab\u00b7ge\u00b7schlos\u00b7sen", ",", "sich", "aus\u00b7ge\u00b7s\u00f6hnt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "PRF", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wohl h\u00e4tten gejauchzt wir, gejubelt, gewiss!", "tokens": ["Wohl", "h\u00e4t\u00b7ten", "ge\u00b7jauchzt", "wir", ",", "ge\u00b7ju\u00b7belt", ",", "ge\u00b7wiss", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "PPER", "$,", "VVPP", "$,", "ADJA", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wenn ein Schiff uns dem Paradies entriss,", "tokens": ["Wenn", "ein", "Schiff", "uns", "dem", "Pa\u00b7ra\u00b7dies", "ent\u00b7riss", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch klagten wir nicht, da fern es blieb,", "tokens": ["Doch", "klag\u00b7ten", "wir", "nicht", ",", "da", "fern", "es", "blieb", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und lebten zusammen und hatten uns lieb.", "tokens": ["Und", "leb\u00b7ten", "zu\u00b7sam\u00b7men", "und", "hat\u00b7ten", "uns", "lieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Doch konnt' es so bleiben? Ist Menschengl\u00fcck", "tokens": ["Doch", "konnt'", "es", "so", "blei\u00b7ben", "?", "Ist", "Men\u00b7schen\u00b7gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF", "$.", "VAFIN", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wie die Welle nicht fl\u00fcchtig, falsch, voller T\u00fcck?", "tokens": ["Wie", "die", "Wel\u00b7le", "nicht", "fl\u00fcch\u00b7tig", ",", "falsch", ",", "vol\u00b7ler", "T\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PTKNEG", "ADJD", "$,", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ich Narr! als ob ich's erprobt nicht oft,", "tokens": ["Ich", "Narr", "!", "als", "ob", "ich's", "er\u00b7probt", "nicht", "oft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$.", "KOKOM", "KOUS", "PIS", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nicht immer umsonst gestrebt, gehofft,", "tokens": ["Nicht", "im\u00b7mer", "um\u00b7sonst", "ge\u00b7strebt", ",", "ge\u00b7hofft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "VVPP", "$,", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Gesorgt und geliebt, und glaubte nun hier", "tokens": ["Ge\u00b7sorgt", "und", "ge\u00b7liebt", ",", "und", "glaub\u00b7te", "nun", "hier"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "VVPP", "$,", "KON", "VVFIN", "ADV", "ADV"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Auf dem Felseneiland w\u00fcrd' lachen mir", "tokens": ["Auf", "dem", "Fel\u00b7sen\u00b7ei\u00b7land", "w\u00fcrd'", "la\u00b7chen", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "VVFIN", "PPER"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Ein best\u00e4ndiges Gl\u00fcck. Zu bald nur, ach", "tokens": ["Ein", "be\u00b7st\u00e4n\u00b7di\u00b7ges", "Gl\u00fcck", ".", "Zu", "bald", "nur", ",", "ach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$.", "APPR", "ADV", "ADV", "$,", "XY"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Zu bald ward es anders.", "tokens": ["Zu", "bald", "ward", "es", "an\u00b7ders", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Mir ist's noch wie heute.", "tokens": ["Mir", "ist's", "noch", "wie", "heu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KOKOM", "ADV", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wir hatten wie Kinder die Insel weit", "tokens": ["Wir", "hat\u00b7ten", "wie", "Kin\u00b7der", "die", "In\u00b7sel", "weit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "NN", "ART", "NN", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Durchstreift in sorgloser Fr\u00f6hlichkeit,", "tokens": ["Durch\u00b7streift", "in", "sorg\u00b7lo\u00b7ser", "Fr\u00f6h\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "++-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und ich hatte mit Blumen das Haar ihr durchschlungen,", "tokens": ["Und", "ich", "hat\u00b7te", "mit", "Blu\u00b7men", "das", "Haar", "ihr", "durch\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "NN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Nachdem wir zuvor in dem Silberbach", "tokens": ["Nach\u00b7dem", "wir", "zu\u00b7vor", "in", "dem", "Sil\u00b7ber\u00b7bach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Die Glieder erfrischt. Dann, wie es sie freute,", "tokens": ["Die", "Glie\u00b7der", "er\u00b7frischt", ".", "Dann", ",", "wie", "es", "sie", "freu\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Hatten im Gehen ein Lied wir gesungen,", "tokens": ["Hat\u00b7ten", "im", "Ge\u00b7hen", "ein", "Lied", "wir", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Nur einen Vers, wir wussten nicht mehr;", "tokens": ["Nur", "ei\u00b7nen", "Vers", ",", "wir", "wuss\u00b7ten", "nicht", "mehr", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Es stammte noch von der Schule her,", "tokens": ["Es", "stamm\u00b7te", "noch", "von", "der", "Schu\u00b7le", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Eine einfache Kindermelodie.", "tokens": ["Ei\u00b7ne", "ein\u00b7fa\u00b7che", "Kin\u00b7der\u00b7me\u00b7lo\u00b7die", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Da zog sie mich an sich und l\u00e4chelte \u2013 nie", "tokens": ["Da", "zog", "sie", "mich", "an", "sich", "und", "l\u00e4\u00b7chel\u00b7te", "\u2013", "nie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PRF", "KON", "VVFIN", "$(", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.12": {"text": "Vergess' ich die Stunde \u2013 und hold \u00fcbergossen", "tokens": ["Ver\u00b7ge\u00b7ss'", "ich", "die", "Stun\u00b7de", "\u2013", "und", "hold", "\u00fc\u00b7ber\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "$(", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Von lieblicher Scham, gestand mir ihr Mund,", "tokens": ["Von", "lieb\u00b7li\u00b7cher", "Scham", ",", "ge\u00b7stand", "mir", "ihr", "Mund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Was seit kurzem sie hielt im Scho\u00df umschlossen.", "tokens": ["Was", "seit", "kur\u00b7zem", "sie", "hielt", "im", "Scho\u00df", "um\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "PPER", "VVFIN", "APPRART", "NN", "VVINF", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "Das sicherste Siegel unserm Bund.", "tokens": ["Das", "si\u00b7chers\u00b7te", "Sie\u00b7gel", "un\u00b7serm", "Bund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "So gro\u00df war die Freude, so gro\u00df das Gl\u00fcck,", "tokens": ["So", "gro\u00df", "war", "die", "Freu\u00b7de", ",", "so", "gro\u00df", "das", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$,", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Jeder andre Gedanke trat zur\u00fcck", "tokens": ["Je\u00b7der", "and\u00b7re", "Ge\u00b7dan\u00b7ke", "trat", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PTKVZ"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "An Schmerzen und Sorgen. Doch in der Nacht,", "tokens": ["An", "Schmer\u00b7zen", "und", "Sor\u00b7gen", ".", "Doch", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$.", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da meldete sich's bei mir mit Macht,", "tokens": ["Da", "mel\u00b7de\u00b7te", "sich's", "bei", "mir", "mit", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "PPER", "APPR", "NN", "$,"], "meter": "-----+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und ich bebte und sorgte im Herzen, und schrie", "tokens": ["Und", "ich", "beb\u00b7te", "und", "sorg\u00b7te", "im", "Her\u00b7zen", ",", "und", "schrie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punc