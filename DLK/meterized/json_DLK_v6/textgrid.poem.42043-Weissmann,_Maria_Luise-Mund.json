{"textgrid.poem.42043": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Mund", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin nur noch ein Mund, der zu Dir spricht,", "tokens": ["Ich", "bin", "nur", "noch", "ein", "Mund", ",", "der", "zu", "Dir", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So schwand ich hin, verlor sich mein Gesicht", "tokens": ["So", "schwand", "ich", "hin", ",", "ver\u00b7lor", "sich", "mein", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und all der Leib, zu dem ich mich versammelt.", "tokens": ["Und", "all", "der", "Leib", ",", "zu", "dem", "ich", "mich", "ver\u00b7sam\u00b7melt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich bin nur noch ein Mund, der zu Dir stammelt,", "tokens": ["Ich", "bin", "nur", "noch", "ein", "Mund", ",", "der", "zu", "Dir", "stam\u00b7melt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der leben blieb, sein Sterben Dir zu k\u00fcnden:", "tokens": ["Der", "le\u00b7ben", "blieb", ",", "sein", "Ster\u00b7ben", "Dir", "zu", "k\u00fcn\u00b7den", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VVFIN", "$,", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Er tut sich auf, und mu\u00df schon in Dich m\u00fcnden.", "tokens": ["Er", "tut", "sich", "auf", ",", "und", "mu\u00df", "schon", "in", "Dich", "m\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$,", "KON", "VMFIN", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}