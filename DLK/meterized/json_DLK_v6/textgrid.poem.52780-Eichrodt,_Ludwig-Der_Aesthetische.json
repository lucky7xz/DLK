{"textgrid.poem.52780": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Aesthetische", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bandwurmstrophen, g\u00f6ttliche Terzinen", "tokens": ["Band\u00b7wurm\u00b7stro\u00b7phen", ",", "g\u00f6tt\u00b7li\u00b7che", "Ter\u00b7zi\u00b7nen"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Oder auch ", "tokens": ["O\u00b7der", "auch"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Lass' ich los, das eigne Versmas preisend,", "tokens": ["Lass'", "ich", "los", ",", "das", "eig\u00b7ne", "Vers\u00b7mas", "prei\u00b7send", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Aus Petrarka, Dante, G\u00f6the weisend,", "tokens": ["Aus", "Pe\u00b7trar\u00b7ka", ",", "Dan\u00b7te", ",", "G\u00f6\u00b7the", "wei\u00b7send", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NE", "$,", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Platen jedenfalls in Wundergl\u00e4tte,", "tokens": ["Pla\u00b7ten", "je\u00b7den\u00b7falls", "in", "Wun\u00b7der\u00b7gl\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Meisterbrief ausstellend Allen ihnen,", "tokens": ["Meis\u00b7ter\u00b7brief", "aus\u00b7stel\u00b7lend", "Al\u00b7len", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "NE", "PPER", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Deutsches hold mit W\u00e4lschestem verschwei\u00dfend,", "tokens": ["Deut\u00b7sches", "hold", "mit", "W\u00e4l\u00b7sches\u00b7tem", "ver\u00b7schwei\u00b7\u00dfend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Z\u00e4hlend kunstverwandte Musenst\u00e4dte,", "tokens": ["Z\u00e4h\u00b7lend", "kunst\u00b7ver\u00b7wand\u00b7te", "Mu\u00b7sen\u00b7st\u00e4d\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Gold verm\u00e4hlend dem Ultramarinen,", "tokens": ["Gold", "ver\u00b7m\u00e4h\u00b7lend", "dem", "Ult\u00b7ra\u00b7ma\u00b7ri\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Plaudernd \u00fcber Tizian's Palette,", "tokens": ["Plau\u00b7dernd", "\u00fc\u00b7ber", "Ti\u00b7zian's", "Pa\u00b7let\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NE", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Etwas \u00fcberfl\u00fcssig um mich bei\u00dfend,", "tokens": ["Et\u00b7was", "\u00fc\u00b7berf\u00b7l\u00fcs\u00b7sig", "um", "mich", "bei\u00b7\u00dfend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Als ob man mich angetastet h\u00e4tte \u2013", "tokens": ["Als", "ob", "man", "mich", "an\u00b7ge\u00b7tas\u00b7tet", "h\u00e4t\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "PRF", "VVPP", "VAFIN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Werd' ich schlie\u00dflich stumm,", "tokens": ["Werd'", "ich", "schlie\u00df\u00b7lich", "stumm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.14": {"text": "Steh' vor hohem Publikum", "tokens": ["Steh'", "vor", "ho\u00b7hem", "Pub\u00b7li\u00b7kum"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Da mit stolzen Bettlermienen.", "tokens": ["Da", "mit", "stol\u00b7zen", "Bett\u00b7ler\u00b7mie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}