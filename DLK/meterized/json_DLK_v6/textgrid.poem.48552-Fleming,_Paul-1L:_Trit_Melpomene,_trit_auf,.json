, "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PIS", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df dich kein Feind k\u00f6nnen jagen,", "tokens": ["da\u00df", "dich", "kein", "Feind", "k\u00f6n\u00b7nen", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00fcberm\u00f6gen keine Macht.", "tokens": ["\u00fc\u00b7ber\u00b7m\u00f6\u00b7gen", "kei\u00b7ne", "Macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was f\u00fcr Macht dich \u00fcbermocht,", "tokens": ["Was", "f\u00fcr", "Macht", "dich", "\u00fc\u00b7ber\u00b7mocht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "die ist die, so Alles pocht.", "tokens": ["die", "ist", "die", ",", "so", "Al\u00b7les", "pocht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "$,", "ADV", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Dein Ged\u00e4chtn\u00fc\u00df, werter Held,", "tokens": ["Dein", "Ge\u00b7d\u00e4cht\u00b7n\u00fc\u00df", ",", "wer\u00b7ter", "Held", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "soll dort neben Ph\u00f6bus stehen,", "tokens": ["soll", "dort", "ne\u00b7ben", "Ph\u00f6\u00b7bus", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "NE", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "auf und nieder mit ihm gehen", "tokens": ["auf", "und", "nie\u00b7der", "mit", "ihm", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KON", "PTKVZ", "APPR", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und der Welt sein vorgestellt.", "tokens": ["und", "der", "Welt", "sein", "vor\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Du wirst nicht vergessen sein,", "tokens": ["Du", "wirst", "nicht", "ver\u00b7ges\u00b7sen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "weil wir haben seinen Schein.", "tokens": ["weil", "wir", "ha\u00b7ben", "sei\u00b7nen", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}