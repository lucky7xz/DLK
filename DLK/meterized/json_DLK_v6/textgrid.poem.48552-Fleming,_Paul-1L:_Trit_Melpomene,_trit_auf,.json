{"textgrid.poem.48552": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Trit Melpomene, trit auf,", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trit Melpomene, trit auf,", "tokens": ["Trit", "Mel\u00b7po\u00b7me\u00b7ne", ",", "trit", "auf", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "la\u00df die Trauersaiten t\u00f6nen,", "tokens": ["la\u00df", "die", "Trau\u00b7er\u00b7sai\u00b7ten", "t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "als an die mich zu gew\u00f6nen", "tokens": ["als", "an", "die", "mich", "zu", "ge\u00b7w\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PRELS", "PPER", "PTKZU", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "zwingt der tr\u00fcben Zeiten Lauf!", "tokens": ["zwingt", "der", "tr\u00fc\u00b7ben", "Zei\u00b7ten", "Lauf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich will in den stillen Klang", "tokens": ["Ich", "will", "in", "den", "stil\u00b7len", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "stimmen diesen Leidgesang.", "tokens": ["stim\u00b7men", "die\u00b7sen", "Leid\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Zier des Stammes, Preis der Jugend,", "tokens": ["Zier", "des", "Stam\u00b7mes", ",", "Preis", "der", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "teurer Ausbund aller Tugend,", "tokens": ["teu\u00b7rer", "Aus\u00b7bund", "al\u00b7ler", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "du Ger\u00fchmter vor der Welt,", "tokens": ["du", "Ge\u00b7r\u00fchm\u00b7ter", "vor", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "o du wertester ", "tokens": ["o", "du", "wer\u00b7tes\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["FM", "PPER", "NN"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.5": {"text": "aller Menschen liebe Lust!", "tokens": ["al\u00b7ler", "Men\u00b7schen", "lie\u00b7be", "Lust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Bist denn du auch gangen hin,", "tokens": ["Bist", "denn", "du", "auch", "gan\u00b7gen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KON", "PPER", "ADV", "VVPP", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "dannenher kein R\u00fcckweg gehet,", "tokens": ["dan\u00b7nen\u00b7her", "kein", "R\u00fcck\u00b7weg", "ge\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "da das Endmal Allen stehet,", "tokens": ["da", "das", "End\u00b7mal", "Al\u00b7len", "ste\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "(allen,so denkt unser Sinn),", "tokens": ["(", "al\u00b7len", ",", "so", "denkt", "un\u00b7ser", "Sinn", ")", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PIAT", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$(", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "bist denn du auch alsobald", "tokens": ["bist", "denn", "du", "auch", "al\u00b7so\u00b7bald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "KON", "PPER", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "vor dem Alter worden kalt?", "tokens": ["vor", "dem", "Al\u00b7ter", "wor\u00b7den", "kalt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAPP", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Du, von dem man neulich nur", "tokens": ["Du", ",", "von", "dem", "man", "neu\u00b7lich", "nur"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "APPR", "PRELS", "PIS", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "r\u00fchmet' an der Helden Reie", "tokens": ["r\u00fch\u00b7met'", "an", "der", "Hel\u00b7den", "Rei\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "solche Tapferkeit und Treue,", "tokens": ["sol\u00b7che", "Tap\u00b7fer\u00b7keit", "und", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "der du deiner Liebe Spur", "tokens": ["der", "du", "dei\u00b7ner", "Lie\u00b7be", "Spur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "vor das traute Vaterland", "tokens": ["vor", "das", "trau\u00b7te", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "machtest durch die Faust bekant?", "tokens": ["mach\u00b7test", "durch", "die", "Faust", "be\u00b7kant", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ja, du namest dir auch f\u00fcr,", "tokens": ["Ja", ",", "du", "na\u00b7mest", "dir", "auch", "f\u00fcr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "ADV", "APPR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "vor die Deinen gar zu sterben", "tokens": ["vor", "die", "Dei\u00b7nen", "gar", "zu", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PPOSAT", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und den werten Dank zu erben,", "tokens": ["und", "den", "wer\u00b7ten", "Dank", "zu", "er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "der Erl\u00f6sern ziemt und dir.", "tokens": ["der", "Er\u00b7l\u00f6\u00b7sern", "ziemt", "und", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dein Wundsch war in selber Not:", "tokens": ["Dein", "Wund\u00b7sch", "war", "in", "sel\u00b7ber", "Not", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "frei sein wollen oder tot.", "tokens": ["frei", "sein", "wol\u00b7len", "o\u00b7der", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "VMFIN", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und wir brachtens auch darzu,", "tokens": ["Und", "wir", "brach\u00b7tens", "auch", "dar\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df wir uns nun Sieger melden,", "tokens": ["da\u00df", "wir", "uns", "nun", "Sie\u00b7ger", "mel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "durch so streitbeherzte Helden,", "tokens": ["durch", "so", "streit\u00b7be\u00b7herz\u00b7te", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "die sich hielten gleich wie du,", "tokens": ["die", "sich", "hiel\u00b7ten", "gleich", "wie", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "VVFIN", "ADV", "KOKOM", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "die ihr Blut um gleichen Kauf,", "tokens": ["die", "ihr", "Blut", "um", "glei\u00b7chen", "Kauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "gleichwie du auch, setzten auf.", "tokens": ["gleich\u00b7wie", "du", "auch", ",", "setz\u00b7ten", "auf", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Mars ersahe dein Gesicht',", "tokens": ["Mars", "er\u00b7sa\u00b7he", "dein", "Ge\u00b7sicht'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "als du in den Waffen rungest,", "tokens": ["als", "du", "in", "den", "Waf\u00b7fen", "run\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "zornig durch die Feinde drungest;", "tokens": ["zor\u00b7nig", "durch", "die", "Fein\u00b7de", "drun\u00b7gest", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "deiner wolt' er warten nicht.", "tokens": ["dei\u00b7ner", "wolt'", "er", "war\u00b7ten", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VMFIN", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mancher Man, der sonst war wert,", "tokens": ["Man\u00b7cher", "Man", ",", "der", "sonst", "war", "wert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIS", "$,", "PRELS", "ADV", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "ward erletzet durch dein Schwert.", "tokens": ["ward", "er\u00b7let\u00b7zet", "durch", "dein", "Schwert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Was du hast f\u00fcr uns gewagt,", "tokens": ["Was", "du", "hast", "f\u00fcr", "uns", "ge\u00b7wagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "zeigen deine tiefen Wunden,", "tokens": ["zei\u00b7gen", "dei\u00b7ne", "tie\u00b7fen", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "so du vor der Faust empfunden.", "tokens": ["so", "du", "vor", "der", "Faust", "emp\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "R\u00fcchtig ist es und besagt,", "tokens": ["R\u00fcch\u00b7tig", "ist", "es", "und", "be\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "was dein k\u00fchner Arm getan", "tokens": ["was", "dein", "k\u00fch\u00b7ner", "Arm", "ge\u00b7tan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "auf dem ", "tokens": ["auf", "dem"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.9": {"line.1": {"text": "Ietzund, da du hast gesiegt", "tokens": ["Ie\u00b7tzund", ",", "da", "du", "hast", "ge\u00b7siegt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "VAFIN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "und mit Ehren k\u00f6mmest wieder,", "tokens": ["und", "mit", "Eh\u00b7ren", "k\u00f6m\u00b7mest", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "legest du dich, Werter, nieder", "tokens": ["le\u00b7gest", "du", "dich", ",", "Wer\u00b7ter", ",", "nie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "PRF", "$,", "NN", "$,", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und wirst durch den Tod bekriegt.", "tokens": ["und", "wirst", "durch", "den", "Tod", "be\u00b7kriegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Da nun Alles Friede war,", "tokens": ["Da", "nun", "Al\u00b7les", "Frie\u00b7de", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "kamst du erstlich in Gefahr.", "tokens": ["kamst", "du", "erst\u00b7lich", "in", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Er, der Tod, floh selbst vor dir,", "tokens": ["Er", ",", "der", "Tod", ",", "floh", "selbst", "vor", "dir", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$,", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "als du ihm nicht woltest weichen,", "tokens": ["als", "du", "ihm", "nicht", "wol\u00b7test", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "da er pl\u00f6tzlich sah' erbleichen", "tokens": ["da", "er", "pl\u00f6tz\u00b7lich", "sah'", "er\u00b7blei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "manchen Man durch dein Rappier,", "tokens": ["man\u00b7chen", "Man", "durch", "dein", "Rap\u00b7pier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIS", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "wie von deiner k\u00fchnen Faust", "tokens": ["wie", "von", "dei\u00b7ner", "k\u00fch\u00b7nen", "Faust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "mancher Feind ward angestrau\u00dft.", "tokens": ["man\u00b7cher", "Feind", "ward", "an\u00b7ge\u00b7strau\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Nun erschleicht er h\u00e4misch dich", "tokens": ["Nun", "er\u00b7schleicht", "er", "h\u00e4\u00b7misch", "dich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "und verletzet dir dein Leben,", "tokens": ["und", "ver\u00b7let\u00b7zet", "dir", "dein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "das zuvor sich wolte geben", "tokens": ["das", "zu\u00b7vor", "sich", "wol\u00b7te", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "PRF", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "keinem Schosse, keinem Stich.", "tokens": ["kei\u00b7nem", "Schos\u00b7se", ",", "kei\u00b7nem", "Stich", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ohne vorbesagte Zeit", "tokens": ["Oh\u00b7ne", "vor\u00b7be\u00b7sag\u00b7te", "Zeit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "k\u00f6mmst du um und au\u00dfer Streit.", "tokens": ["k\u00f6mmst", "du", "um", "und", "au\u00b7\u00dfer", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "H\u00e4ttestu doch deine Kraft", "tokens": ["H\u00e4t\u00b7tes\u00b7tu", "doch", "dei\u00b7ne", "Kraft"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Andern auch bezeigen sollen!", "tokens": ["An\u00