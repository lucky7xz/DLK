{"dta.poem.7643": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was deinem innern Trieb ist angemessen, treibe,", "tokens": ["Was", "dei\u00b7nem", "in\u00b7nern", "Trieb", "ist", "an\u00b7ge\u00b7mes\u00b7sen", ",", "trei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VAFIN", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur da\u00df fein auch der Trieb ein angeme\u00dfner bleibe!", "tokens": ["Nur", "da\u00df", "fein", "auch", "der", "Trieb", "ein", "an\u00b7ge\u00b7me\u00df\u00b7ner", "blei\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ADJD", "ADV", "ART", "NN", "ART", "ADJA", "VVFIN", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Und was du liebend treibst, la\u00df dir das H\u00f6chste gelten,", "tokens": ["Und", "was", "du", "lie\u00b7bend", "treibst", ",", "la\u00df", "dir", "das", "H\u00f6chs\u00b7te", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "VVFIN", "$,", "VVIMP", "PPER", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ohn' anderstreibende misliebig drum zu schelten.", "tokens": ["Ohn'", "an\u00b7der\u00b7strei\u00b7ben\u00b7de", "mis\u00b7lie\u00b7big", "drum", "zu", "schel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJD", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sei doch in jeder Art ein H\u00f6chstes offenbart;", "tokens": ["Sei", "doch", "in", "je\u00b7der", "Art", "ein", "H\u00f6chs\u00b7tes", "of\u00b7fen\u00b7bart", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PIAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du offenbare dein H\u00f6chstes in deiner Art!", "tokens": ["Du", "of\u00b7fen\u00b7ba\u00b7re", "dein", "H\u00f6chs\u00b7tes", "in", "dei\u00b7ner", "Art", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}