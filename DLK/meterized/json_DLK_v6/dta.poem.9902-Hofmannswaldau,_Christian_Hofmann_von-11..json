{"dta.poem.9902": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bald mit zucker/ bald mit gift/ wie es kommt/ bin ich gef\u00fcllet/", "tokens": ["Bald", "mit", "zu\u00b7cker", "/", "bald", "mit", "gift", "/", "wie", "es", "kommt", "/", "bin", "ich", "ge\u00b7f\u00fcl\u00b7let", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$(", "ADV", "APPR", "NN", "$(", "PWAV", "PPER", "VVFIN", "$(", "VAFIN", "PPER", "VVPP", "$("], "meter": "------+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Durch mich mancher fchwerer streit ohne schwerdt-streich wird", "tokens": ["Durch", "mich", "man\u00b7cher", "fchwe\u00b7rer", "streit", "oh\u00b7ne", "schwerdt\u00b7streich", "wird"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PIAT", "ADJA", "NN", "APPR", "ADJD", "VAFIN"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "gestillet.", "tokens": ["ge\u00b7stil\u00b7let", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Ungesehn entdeck ich di\u00df/ was ein mensch nur mag gedencken/", "tokens": ["Un\u00b7ge\u00b7sehn", "ent\u00b7deck", "ich", "di\u00df", "/", "was", "ein", "mensch", "nur", "mag", "ge\u00b7den\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPER", "PDS", "$(", "PWS", "ART", "NN", "ADV", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Und die hertzen kan ich so/ wie es mir gef\u00e4llet/ lencken.", "tokens": ["Und", "die", "hert\u00b7zen", "kan", "ich", "so", "/", "wie", "es", "mir", "ge\u00b7f\u00e4l\u00b7let", "/", "len\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "PPER", "ADV", "$(", "PWAV", "PPER", "PPER", "VVPP", "$(", "VVINF", "$."], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}}}}}