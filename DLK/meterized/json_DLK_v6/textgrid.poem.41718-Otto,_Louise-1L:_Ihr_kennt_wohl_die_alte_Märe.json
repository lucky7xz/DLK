{"textgrid.poem.41718": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr kennt wohl die alte M\u00e4re", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr kennt wohl die alte M\u00e4re", "tokens": ["Ihr", "kennt", "wohl", "die", "al\u00b7te", "M\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und kennt wohl das sch\u00f6ne Gedicht", "tokens": ["Und", "kennt", "wohl", "das", "sch\u00f6\u00b7ne", "Ge\u00b7dicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und wi\u00dft was vom K\u00e4tchen von Heilbronn,", "tokens": ["Und", "wi\u00dft", "was", "vom", "K\u00e4t\u00b7chen", "von", "Heil\u00b7bronn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "APPRART", "NN", "APPR", "NE", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Man immer noch kundet und spricht?", "tokens": ["Man", "im\u00b7mer", "noch", "kun\u00b7det", "und", "spricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADV", "ADJD", "KON", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Wie einst sie gelaufen dem Ritter,", "tokens": ["Wie", "einst", "sie", "ge\u00b7lau\u00b7fen", "dem", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "VVPP", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Dem Wetter von Strahl lange nach,", "tokens": ["Dem", "Wet\u00b7ter", "von", "Strahl", "lan\u00b7ge", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Bis Lieb seinen Stolz \u00fcberwunden,", "tokens": ["Bis", "Lieb", "sei\u00b7nen", "Stolz", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Er pl\u00f6tzlich zu F\u00fc\u00dfen ihr lag!", "tokens": ["Er", "pl\u00f6tz\u00b7lich", "zu", "F\u00fc\u00b7\u00dfen", "ihr", "lag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Und habt Ihr das M\u00e4dchen gescholten,", "tokens": ["Und", "habt", "Ihr", "das", "M\u00e4d\u00b7chen", "ge\u00b7schol\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Das der inneren Stimme vertraut", "tokens": ["Das", "der", "in\u00b7ne\u00b7ren", "Stim\u00b7me", "ver\u00b7traut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "ADJA", "NN", "ADJD"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Habt ihr sie doch gern als Prinzessin", "tokens": ["Habt", "ihr", "sie", "doch", "gern", "als", "Prin\u00b7zes\u00b7sin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "ADV", "KOUS", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Am Ziel ihrer Sehnsucht geschaut.", "tokens": ["Am", "Ziel", "ih\u00b7rer", "Sehn\u00b7sucht", "ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Ich mache die M\u00e4r mir zu Nutze", "tokens": ["Ich", "ma\u00b7che", "die", "M\u00e4r", "mir", "zu", "Nut\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "PTKZU", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Erneu sie in jetziger Zeit;", "tokens": ["Er\u00b7neu", "sie", "in", "jet\u00b7zi\u00b7ger", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch ich folge treu einem Ritter", "tokens": ["Auch", "ich", "fol\u00b7ge", "treu", "ei\u00b7nem", "Rit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "ADJD", "ART", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Aus meiner Verborgenheit.", "tokens": ["Aus", "mei\u00b7ner", "Ver\u00b7bor\u00b7gen\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Er sitzet gar stattlich zu Rosse,", "tokens": ["Er", "sit\u00b7zet", "gar", "statt\u00b7lich", "zu", "Ros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Hoch flattert der Helmbusch ihm nach,", "tokens": ["Hoch", "flat\u00b7tert", "der", "Helm\u00b7busch", "ihm", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Es blitzet die m\u00e4chtige Klinge,", "tokens": ["Es", "blit\u00b7zet", "die", "m\u00e4ch\u00b7ti\u00b7ge", "Klin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der mancher Gewaltge erlag.", "tokens": ["Der", "man\u00b7cher", "Ge\u00b7walt\u00b7ge", "er\u00b7lag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Mich treibt eine innere Ahnung,", "tokens": ["Mich", "treibt", "ei\u00b7ne", "in\u00b7ne\u00b7re", "Ah\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Durchzittert ein st\u00fcrmischer Drang", "tokens": ["Durch\u00b7zit\u00b7tert", "ein", "st\u00fcr\u00b7mi\u00b7scher", "Drang"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ihm immer und ewig zu folgen \u2013", "tokens": ["Ihm", "im\u00b7mer", "und", "e\u00b7wig", "zu", "fol\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KON", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Doch oftmals wie wird mir so bang!", "tokens": ["Doch", "oft\u00b7mals", "wie", "wird", "mir", "so", "bang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Wenn alle die Knappen und Ritter", "tokens": ["Wenn", "al\u00b7le", "die", "Knap\u00b7pen", "und", "Rit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ich sehe zur Seite ihm ziehn,", "tokens": ["Ich", "se\u00b7he", "zur", "Sei\u00b7te", "ihm", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Geschm\u00fccket auf mutigen Rossen,", "tokens": ["Ge\u00b7schm\u00fc\u00b7cket", "auf", "mu\u00b7ti\u00b7gen", "Ros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Im Glanze die Waffen ergl\u00fchn: \u2013", "tokens": ["Im", "Glan\u00b7ze", "die", "Waf\u00b7fen", "er\u00b7gl\u00fchn", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.8": {"line.1": {"text": "Und seh nun mich Arme daneben", "tokens": ["Und", "seh", "nun", "mich", "Ar\u00b7me", "da\u00b7ne\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PPER", "NN", "PAV"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von Rittern wohl nimmer bemerkt,", "tokens": ["Von", "Rit\u00b7tern", "wohl", "nim\u00b7mer", "be\u00b7merkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Von Knappen geh\u00f6hnt und gescholten \u2013", "tokens": ["Von", "Knap\u00b7pen", "ge\u00b7h\u00f6hnt", "und", "ge\u00b7schol\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "KON", "VVPP", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Dann hab ich umsonst mich gest\u00e4rkt! \u2013", "tokens": ["Dann", "hab", "ich", "um\u00b7sonst", "mich", "ge\u00b7st\u00e4rkt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "PPER", "VVPP", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.9": {"line.1": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Der Ruhm ist Graf Wetter von Strahl!", "tokens": ["Der", "Ruhm", "ist", "Graf", "Wet\u00b7ter", "von", "Strahl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NE", "NN", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Dem werd ich zu folgen getrieben", "tokens": ["Dem", "werd", "ich", "zu", "fol\u00b7gen", "ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "PTKZU", "VVINF", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Aus Ahnung und Drang \u2013 nicht aus Wahl!", "tokens": ["Aus", "Ah\u00b7nung", "und", "Drang", "\u2013", "nicht", "aus", "Wahl", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$(", "PTKNEG", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.10": {"line.1": {"text": "Doch bleib ich die niedrige K\u00e4the,", "tokens": ["Doch", "bleib", "ich", "die", "nied\u00b7ri\u00b7ge", "K\u00e4\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Zu klar nur erkennt das mein Sinn,", "tokens": ["Zu", "klar", "nur", "er\u00b7kennt", "das", "mein", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ADV", "VVFIN", "ART", "PPOSAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wenn ich nicht die Tochter des Kaisers,", "tokens": ["Wenn", "ich", "nicht", "die", "Toch\u00b7ter", "des", "Kai\u00b7sers", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Des Sch\u00f6pfers des Genius bin!", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "des", "Ge\u00b7nius", "bin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}