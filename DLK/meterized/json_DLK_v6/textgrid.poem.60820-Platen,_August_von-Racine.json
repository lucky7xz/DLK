{"textgrid.poem.60820": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Racine", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sinnreich trat in die Spuren ich ein des bewunderten Meisters;", "tokens": ["Sinn\u00b7reich", "trat", "in", "die", "Spu\u00b7ren", "ich", "ein", "des", "be\u00b7wun\u00b7der\u00b7ten", "Meis\u00b7ters", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "PPER", "ART", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Aber verweichlicht schon, \u00e4rmer an Kraft und Genie.", "tokens": ["A\u00b7ber", "ver\u00b7weich\u00b7licht", "schon", ",", "\u00e4r\u00b7mer", "an", "Kraft", "und", "Ge\u00b7nie", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Doch weil allzugalant ich der Liebe Sophistik entfaltet,", "tokens": ["Doch", "weil", "all\u00b7zu\u00b7ga\u00b7lant", "ich", "der", "Lie\u00b7be", "So\u00b7phis\u00b7tik", "ent\u00b7fal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADJD", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Huldigen mir Frankreichs Kritiker allzugalant.", "tokens": ["Hul\u00b7di\u00b7gen", "mir", "Fran\u00b7kreichs", "Kri\u00b7ti\u00b7ker", "all\u00b7zu\u00b7ga\u00b7lant", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NE", "NN", "ADJD", "$."], "meter": "----+-+--+---", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Zwar Melpomene segnete mich; doch wandte sich Klio", "tokens": ["Zwar", "Mel\u00b7po\u00b7me\u00b7ne", "seg\u00b7ne\u00b7te", "mich", ";", "doch", "wand\u00b7te", "sich", "Klio"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NE", "VVFIN", "PPER", "$.", "ADV", "VVFIN", "PRF", "NE"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Weg, sie erkannte jedoch meinen Britannicus an.", "tokens": ["Weg", ",", "sie", "er\u00b7kann\u00b7te", "je\u00b7doch", "mei\u00b7nen", "Bri\u00b7tan\u00b7ni\u00b7cus", "an", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}