{"textgrid.poem.59383": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Fromme", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sa\u00df ein Kind gebunden und gefangen,", "tokens": ["Es", "sa\u00df", "ein", "Kind", "ge\u00b7bun\u00b7den", "und", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo vor der Menschen eitlem Tun und Schallen", "tokens": ["Wo", "vor", "der", "Men\u00b7schen", "eit\u00b7lem", "Tun", "und", "Schal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Vorzeit Wunderlaute tr\u00fcb verhallen;", "tokens": ["Der", "Vor\u00b7zeit", "Wun\u00b7der\u00b7lau\u00b7te", "tr\u00fcb", "ver\u00b7hal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der alten Heimat dacht es voll Verlangen.", "tokens": ["Der", "al\u00b7ten", "Hei\u00b7mat", "dacht", "es", "voll", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da sieht es drau\u00dfen Str\u00f6me, hell ergangen,", "tokens": ["Da", "sieht", "es", "drau\u00b7\u00dfen", "Str\u00f6\u00b7me", ",", "hell", "er\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch zaubrisch Land viel Pilger, S\u00e4nger wallen,", "tokens": ["Durch", "zaub\u00b7risch", "Land", "viel", "Pil\u00b7ger", ",", "S\u00e4n\u00b7ger", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJD", "NN", "PIAT", "NN", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "K\u00fchl rauscht der Wald, die lust'gen H\u00f6rner schallen,", "tokens": ["K\u00fchl", "rauscht", "der", "Wald", ",", "die", "lust'\u00b7gen", "H\u00f6r\u00b7ner", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aurora scheint, so weit die Blicke langen. \u2013", "tokens": ["Au\u00b7ro\u00b7ra", "scheint", ",", "so", "weit", "die", "Bli\u00b7cke", "lan\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "$,", "ADV", "ADJD", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O la\u00df die Sehnsucht ganz dein Herz durchdringen!", "tokens": ["O", "la\u00df", "die", "Sehn\u00b7sucht", "ganz", "dein", "Herz", "durch\u00b7drin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So legt sich bl\u00fchend um die Welt dein Trauern", "tokens": ["So", "legt", "sich", "bl\u00fc\u00b7hend", "um", "die", "Welt", "dein", "Trau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und himmlisch wird dein Schmerz und deine Sorgen.", "tokens": ["Und", "himm\u00b7lisch", "wird", "dein", "Schmerz", "und", "dei\u00b7ne", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein frisch Gem\u00fct mag wohl die Welt bezwingen,", "tokens": ["Ein", "frisch", "Ge\u00b7m\u00fct", "mag", "wohl", "die", "Welt", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein recht Gebet bricht Banden bald und Mauern:", "tokens": ["Ein", "recht", "Ge\u00b7bet", "bricht", "Ban\u00b7den", "bald", "und", "Mau\u00b7ern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "NN", "ADV", "KON", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und frei springst du hinunter in den Morgen.", "tokens": ["Und", "frei", "springst", "du", "hin\u00b7un\u00b7ter", "in", "den", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Willkommen, Liebchen, denn am Meeresstrande!", "tokens": ["Will\u00b7kom\u00b7men", ",", "Lieb\u00b7chen", ",", "denn", "am", "Mee\u00b7res\u00b7stran\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie rauschen lockend da ans Herz die Wellen", "tokens": ["Wie", "rau\u00b7schen", "lo\u00b7ckend", "da", "ans", "Herz", "die", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADJD", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und tiefe Sehnsucht will die Seele schwellen,", "tokens": ["Und", "tie\u00b7fe", "Sehn\u00b7sucht", "will", "die", "See\u00b7le", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn andre tr\u00e4ge schlafen auf dem Lande.", "tokens": ["Wenn", "and\u00b7re", "tr\u00e4\u00b7ge", "schla\u00b7fen", "auf", "dem", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So walte Gott! \u2013 ich l\u00f6s des Schiffleins Bande,", "tokens": ["So", "wal\u00b7te", "Gott", "!", "\u2013", "ich", "l\u00f6s", "des", "Schif\u00b7fleins", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$.", "$(", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wegweiser sind die Stern, die ewig hellen,", "tokens": ["Weg\u00b7wei\u00b7ser", "sind", "die", "Stern", ",", "die", "e\u00b7wig", "hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Viel Segel fahren da und frisch' Gesellen", "tokens": ["Viel", "Se\u00b7gel", "fah\u00b7ren", "da", "und", "frisch'", "Ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Begr\u00fc\u00dfen uns von ihrer Schiffe Rande.", "tokens": ["Be\u00b7gr\u00fc\u00b7\u00dfen", "uns", "von", "ih\u00b7rer", "Schif\u00b7fe", "Ran\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wir sitzen still, gleich Schw\u00e4nen zieht das Segel,", "tokens": ["Wir", "sit\u00b7zen", "still", ",", "gleich", "Schw\u00e4\u00b7nen", "zieht", "das", "Se\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ADV", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich schau in deiner Augen lichte Sterne,", "tokens": ["Ich", "schau", "in", "dei\u00b7ner", "Au\u00b7gen", "lich\u00b7te", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du schweigst und schauerst heimlich oft zusammen.", "tokens": ["Du", "schweigst", "und", "schau\u00b7erst", "heim\u00b7lich", "oft", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Blick auf! Schon schweifen Paradiesesv\u00f6gel,", "tokens": ["Blick", "auf", "!", "Schon", "schwei\u00b7fen", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7v\u00f6\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schon wehen Wunderkl\u00e4nge aus der Ferne,", "tokens": ["Schon", "we\u00b7hen", "Wun\u00b7der\u00b7kl\u00e4n\u00b7ge", "aus", "der", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Garten Gottes steigt aus Morgenflammen.", "tokens": ["Der", "Gar\u00b7ten", "Got\u00b7tes", "steigt", "aus", "Mor\u00b7gen\u00b7flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}