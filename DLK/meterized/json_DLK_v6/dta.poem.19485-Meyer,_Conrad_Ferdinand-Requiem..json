{"dta.poem.19485": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Requiem.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Bei der Abendsonne Wandern", "tokens": ["Bei", "der", "A\u00b7bend\u00b7son\u00b7ne", "Wan\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wann ein Dorf den Strahl verlor,", "tokens": ["Wann", "ein", "Dorf", "den", "Strahl", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Klagt sein Dunkeln es den andern", "tokens": ["Klagt", "sein", "Dun\u00b7keln", "es", "den", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "PPER", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit vertrauten T\u00f6nen vor:", "tokens": ["Mit", "ver\u00b7trau\u00b7ten", "T\u00f6\u00b7nen", "vor", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201eviele Schl\u00e4ge, viele Schl\u00e4ge", "tokens": ["\u201e", "vie\u00b7le", "Schl\u00e4\u00b7ge", ",", "vie\u00b7le", "Schl\u00e4\u00b7ge"], "token_info": ["punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Thut an einem Tag das Herz,", "tokens": ["Thut", "an", "ei\u00b7nem", "Tag", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenig Schl\u00e4ge, wenig Schl\u00e4ge", "tokens": ["We\u00b7nig", "Schl\u00e4\u00b7ge", ",", "we\u00b7nig", "Schl\u00e4\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Thut im D\u00e4mmerlicht das Erz!\u201c", "tokens": ["Thut", "im", "D\u00e4m\u00b7mer\u00b7licht", "das", "Erz", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Noch ein Gl\u00f6cklein hat geschwiegen", "tokens": ["Noch", "ein", "Gl\u00f6c\u00b7klein", "hat", "ge\u00b7schwie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der H\u00f6he bis zuletzt.", "tokens": ["Auf", "der", "H\u00f6\u00b7he", "bis", "zu\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun beginnt es sich zu wiegen,", "tokens": ["Nun", "be\u00b7ginnt", "es", "sich", "zu", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Horch, mein Kilchberg l\u00e4utet jetzt!", "tokens": ["Horch", ",", "mein", "Kilch\u00b7berg", "l\u00e4u\u00b7tet", "jetzt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}