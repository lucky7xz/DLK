{"textgrid.poem.56209": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "9", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Rosenzweig dich schm\u00fccken?", "tokens": ["Ein", "Ro\u00b7sen\u00b7zweig", "dich", "schm\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du Wilder, wie will sich's schicken?", "tokens": ["Du", "Wil\u00b7der", ",", "wie", "will", "sich's", "schi\u00b7cken", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PWAV", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Was hast du mit Rosen gemein? \u2013", "tokens": ["Was", "hast", "du", "mit", "Ro\u00b7sen", "ge\u00b7mein", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "NN", "ADJD", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Es stehen drei Sterne am Himmel,", "tokens": ["Es", "ste\u00b7hen", "drei", "Ster\u00b7ne", "am", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Die geben der Lieb ihren Schein.", "tokens": ["Die", "ge\u00b7ben", "der", "Lieb", "ih\u00b7ren", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Zwei Knospen am Zweig und die Rose", "tokens": ["Zwei", "Knos\u00b7pen", "am", "Zweig", "und", "die", "Ro\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "APPRART", "NN", "KON", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Entscheiden nun meine Lose,", "tokens": ["Ent\u00b7schei\u00b7den", "nun", "mei\u00b7ne", "Lo\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Die Dreie, die mein ich allein. \u2013", "tokens": ["Die", "Drei\u00b7e", ",", "die", "mein", "ich", "al\u00b7lein", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es stehen drei Sterne am Himmel,", "tokens": ["Es", "ste\u00b7hen", "drei", "Ster\u00b7ne", "am", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Die geben der Lieb ihren Schein.", "tokens": ["Die", "ge\u00b7ben", "der", "Lieb", "ih\u00b7ren", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Die Rose, die zarte, bl\u00fchet,", "tokens": ["Die", "Ro\u00b7se", ",", "die", "zar\u00b7te", ",", "bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Liebe bl\u00fchet und gl\u00fchet,", "tokens": ["Die", "Lie\u00b7be", "bl\u00fc\u00b7het", "und", "gl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das f\u00fchl ich im Herzen mein. \u2013", "tokens": ["Das", "f\u00fchl", "ich", "im", "Her\u00b7zen", "mein", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "NN", "PPOSAT", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Es stehen drei Sterne am Himmel,", "tokens": ["Es", "ste\u00b7hen", "drei", "Ster\u00b7ne", "am", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Die geben der Lieb ihren Schein.", "tokens": ["Die", "ge\u00b7ben", "der", "Lieb", "ih\u00b7ren", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Noch Knospen im gr\u00fcnen Laube,", "tokens": ["Noch", "Knos\u00b7pen", "im", "gr\u00fc\u00b7nen", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Hoffnung und der Glaube,", "tokens": ["Die", "Hoff\u00b7nung", "und", "der", "Glau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie m\u00fcssen zur Bl\u00fcte gedeihn. \u2013", "tokens": ["Sie", "m\u00fcs\u00b7sen", "zur", "Bl\u00fc\u00b7te", "ge\u00b7deihn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "VVINF", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Es stehen drei Sterne am Himmel,", "tokens": ["Es", "ste\u00b7hen", "drei", "Ster\u00b7ne", "am", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Die geben der Lieb ihren Schein.", "tokens": ["Die", "ge\u00b7ben", "der", "Lieb", "ih\u00b7ren", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Ich pflanz ihn in meinen Garten,", "tokens": ["Ich", "pflanz", "ihn", "in", "mei\u00b7nen", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Den Zweig, und seiner zu warten,", "tokens": ["Den", "Zweig", ",", "und", "sei\u00b7ner", "zu", "war\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PPOSAT", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dem will ich ernst mich weihn. \u2013", "tokens": ["Dem", "will", "ich", "ernst", "mich", "weihn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es stehen drei Sterne am Himmel,", "tokens": ["Es", "ste\u00b7hen", "drei", "Ster\u00b7ne", "am", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Die geben der Lieb ihren Schein.", "tokens": ["Die", "ge\u00b7ben", "der", "Lieb", "ih\u00b7ren", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Ich seh ihn im freudigen Traume", "tokens": ["Ich", "seh", "ihn", "im", "freu\u00b7di\u00b7gen", "Trau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Erwachsen zum starken Baume,", "tokens": ["Er\u00b7wach\u00b7sen", "zum", "star\u00b7ken", "Bau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mein Obdach soll er sein. \u2013", "tokens": ["Mein", "Ob\u00b7dach", "soll", "er", "sein", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VAINF", "$.", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Es stehen drei Sterne am Himmel,", "tokens": ["Es", "ste\u00b7hen", "drei", "Ster\u00b7ne", "am", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Die geben der Lieb ihren Schein.", "tokens": ["Die", "ge\u00b7ben", "der", "Lieb", "ih\u00b7ren", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Und hat der Traum mich betrogen,", "tokens": ["Und", "hat", "der", "Traum", "mich", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Verdorrend der Zweig mich belogen,", "tokens": ["Ver\u00b7dor\u00b7rend", "der", "Zweig", "mich", "be\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Mag alles dann L\u00fcge sein;", "tokens": ["Mag", "al\u00b7les", "dann", "L\u00fc\u00b7ge", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "NN", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dann steht kein Stern am Himmel,", "tokens": ["Dann", "steht", "kein", "Stern", "am", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Kein Stern gibt der Liebe den Schein.", "tokens": ["Kein", "Stern", "gibt", "der", "Lie\u00b7be", "den", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}