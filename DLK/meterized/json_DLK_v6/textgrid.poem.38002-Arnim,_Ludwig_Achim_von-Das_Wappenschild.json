{"textgrid.poem.38002": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Das Wappenschild", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "St\u00fcrmt, rei\u00dft und rast ihr Ungl\u00fcckswinde,", "tokens": ["St\u00fcrmt", ",", "rei\u00dft", "und", "rast", "ihr", "Un\u00b7gl\u00fccks\u00b7win\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zeigt eure ganze Tiranney,", "tokens": ["Zeigt", "eu\u00b7re", "gan\u00b7ze", "Ti\u00b7ran\u00b7ney", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zerbrecht, zerschlagt so Zweig als Rinde,", "tokens": ["Zer\u00b7brecht", ",", "zer\u00b7schlagt", "so", "Zweig", "als", "Rin\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ADV", "NN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und werft den Hofnungsbaum entzwey;", "tokens": ["Und", "werft", "den", "Hof\u00b7nungs\u00b7baum", "ent\u00b7zwey", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dies Hagelwetter", "tokens": ["Dies", "Ha\u00b7gel\u00b7wet\u00b7ter"], "token_info": ["word", "word"], "pos": ["PDS", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Trift Stamm und Bl\u00e4tter,", "tokens": ["Trift", "Stamm", "und", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Die Wurzel bleibt,", "tokens": ["Die", "Wur\u00b7zel", "bleibt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Bis Sturm und Regen", "tokens": ["Bis", "Sturm", "und", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Ihr W\u00fcthen legen,", "tokens": ["Ihr", "W\u00fct\u00b7hen", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Da\u00df sie von neuem gr\u00fcnt und Aeste treibt.", "tokens": ["Da\u00df", "sie", "von", "neu\u00b7em", "gr\u00fcnt", "und", "A\u00b7es\u00b7te", "treibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "VVFIN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Mein Herz giebt keinem Diamanten,", "tokens": ["Mein", "Herz", "giebt", "kei\u00b7nem", "Di\u00b7a\u00b7man\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Geist der Eiche wenig nach;", "tokens": ["Mein", "Geist", "der", "Ei\u00b7che", "we\u00b7nig", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn Erd und Himmel mich verbannten,", "tokens": ["Wenn", "Erd", "und", "Him\u00b7mel", "mich", "ver\u00b7bann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So trotz ich doch dem Ungemach:", "tokens": ["So", "trotz", "ich", "doch", "dem", "Un\u00b7ge\u00b7mach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weicht falsche Freunde,", "tokens": ["Weicht", "fal\u00b7sche", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Schlagt bittre Feinde,", "tokens": ["Schlagt", "bitt\u00b7re", "Fein\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Mein Heldenmuth", "tokens": ["Mein", "Hel\u00b7den\u00b7muth"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Ist nicht zu d\u00e4mpfen,", "tokens": ["Ist", "nicht", "zu", "d\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Drum will ich k\u00e4mpfen,", "tokens": ["Drum", "will", "ich", "k\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Und sehn was die Geduld f\u00fcr Wunder thut.", "tokens": ["Und", "sehn", "was", "die", "Ge\u00b7duld", "f\u00fcr", "Wun\u00b7der", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "PRELS", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Liebe schenkt aus goldnen Schaalen", "tokens": ["Die", "Lie\u00b7be", "schenkt", "aus", "gold\u00b7nen", "Schaa\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir einen Wein zur Tapferkeit,", "tokens": ["Mir", "ei\u00b7nen", "Wein", "zur", "Tap\u00b7fer\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verspricht mir guten Sold zu zahlen", "tokens": ["Ver\u00b7spricht", "mir", "gu\u00b7ten", "Sold", "zu", "zah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fchrt mich muthig in den Streit;", "tokens": ["Und", "f\u00fchrt", "mich", "mut\u00b7hig", "in", "den", "Streit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da will ich siegen,", "tokens": ["Da", "will", "ich", "sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Hier will ich kriegen;", "tokens": ["Hier", "will", "ich", "krie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Ein gr\u00fcnes Feld", "tokens": ["Ein", "gr\u00fc\u00b7nes", "Feld"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Dient meinem Schilde", "tokens": ["Dient", "mei\u00b7nem", "Schil\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Zum Wappenbilde,", "tokens": ["Zum", "Wap\u00b7pen\u00b7bil\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Allwo ein Palmenbaum zwey Anker hebt.", "tokens": ["All\u00b7wo", "ein", "Pal\u00b7men\u00b7baum", "zwey", "An\u00b7ker", "hebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}