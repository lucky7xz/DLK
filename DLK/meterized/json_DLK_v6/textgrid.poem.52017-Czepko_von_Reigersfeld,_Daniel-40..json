{"textgrid.poem.52017": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "40.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der zwischen Harn und Koth in seinem Sode liegt,", "tokens": ["Der", "zwi\u00b7schen", "Harn", "und", "Koth", "in", "sei\u00b7nem", "So\u00b7de", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und t\u00e4glich also lebt, als wie er war gebohren,", "tokens": ["Und", "t\u00e4g\u00b7lich", "al\u00b7so", "lebt", ",", "als", "wie", "er", "war", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "$,", "KOUS", "PWAV", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hat eine schwere Zung, in dem er trinckt, gekriegt,", "tokens": ["Hat", "ei\u00b7ne", "schwe\u00b7re", "Zung", ",", "in", "dem", "er", "trinckt", ",", "ge\u00b7kriegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Drumb hat zu plappern ihm der Unterbauch erkohren.", "tokens": ["Drumb", "hat", "zu", "plap\u00b7pern", "ihm", "der", "Un\u00b7ter\u00b7bauch", "er\u00b7koh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PTKZU", "VVFIN", "PPER", "ART", "NN", 