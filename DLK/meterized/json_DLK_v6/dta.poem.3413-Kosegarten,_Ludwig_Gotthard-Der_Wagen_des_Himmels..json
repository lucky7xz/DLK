{"dta.poem.3413": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Wagen des Himmels.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie f&#228;hrst du blitzend, fey'rlich, mit Majest&#228;t,", "tokens": ["Wie", "f", "&#228;", "hrst", "du", "blit\u00b7zend", ",", "fey'\u00b7rlich", ",", "mit", "Ma\u00b7jest", "&#228;", "t", ","], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "punct", "word", "punct", "word", "word", "XML_entity", "word", "punct"], "pos": ["PWAV", "NE", "$(", "VVFIN", "PPER", "ADJD", "$,", "ADJD", "$,", "APPR", "NE", "$(", "VVFIN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "O goldner Himmelswagen, in blauer Luft!", "tokens": ["O", "gold\u00b7ner", "Him\u00b7mels\u00b7wa\u00b7gen", ",", "in", "blau\u00b7er", "Luft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ich h\u00f6re deiner R\u00e4der Rasseln,", "tokens": ["Ich", "h\u00f6\u00b7re", "dei\u00b7ner", "R\u00e4\u00b7der", "Ras\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6re das Wiehern der Fl\u00fcgelrosse.", "tokens": ["H\u00f6\u00b7re", "das", "Wie\u00b7hern", "der", "Fl\u00fc\u00b7gel\u00b7ros\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Sieh, wie sie Flammen schnauben, die Wieherer!", "tokens": ["Sieh", ",", "wie", "sie", "Flam\u00b7men", "schnau\u00b7ben", ",", "die", "Wie\u00b7he\u00b7rer", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "NN", "VVINF", "$,", "ART", "NN", "$."], "meter": "++-+-+-+---", "measure": "unknown.measure.penta"}, "line.2": {"text": "Sieh, wie sie Funken schlagen, die Schnaubenden!", "tokens": ["Sieh", ",", "wie", "sie", "Fun\u00b7ken", "schla\u00b7gen", ",", "die", "Schnau\u00b7ben\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "NN", "VVINF", "$,", "ART", "NN", "$."], "meter": "+--+-+--+--", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Horch, wie die ehrne Himmelsstrasse", "tokens": ["Horch", ",", "wie", "die", "ehr\u00b7ne", "Him\u00b7mels\u00b7stras\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dr\u00f6hnt von dem Stampfen der Demanthufe.", "tokens": ["Dr\u00f6hnt", "von", "dem", "Stamp\u00b7fen", "der", "De\u00b7mant\u00b7hu\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Ich liebe dich, ich habe dich l\u00e4ngst geliebt,", "tokens": ["Ich", "lie\u00b7be", "dich", ",", "ich", "ha\u00b7be", "dich", "l\u00e4ngst", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du k\u00f6niglicher Wagen. Des Knaben Blick", "tokens": ["Du", "k\u00f6\u00b7nig\u00b7li\u00b7cher", "Wa\u00b7gen", ".", "Des", "Kna\u00b7ben", "Blick"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$.", "ART", "NN", "NN"], "meter": "-----+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hat oft in tiefen Mittern\u00e4chten", "tokens": ["Hat", "oft", "in", "tie\u00b7fen", "Mit\u00b7ter\u00b7n\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ahnend und staunend an dir gehangen.", "tokens": ["Ah\u00b7nend", "und", "stau\u00b7nend", "an", "dir", "ge\u00b7han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Nun lieb' ich dich noch st\u00e4rker, du Herrlicher!", "tokens": ["Nun", "lieb'", "ich", "dich", "noch", "st\u00e4r\u00b7ker", ",", "du", "Herr\u00b7li\u00b7cher", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "$,", "PPER", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich seh dich oft mit inniger Wehmuth an,", "tokens": ["Ich", "seh", "dich", "oft", "mit", "in\u00b7ni\u00b7ger", "Weh\u00b7muth", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und eine leise Thr\u00e4ne bebet", "tokens": ["Und", "ei\u00b7ne", "lei\u00b7se", "Thr\u00e4\u00b7ne", "be\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In des entbrannteren J\u00fcnglings Wimper.", "tokens": ["In", "des", "ent\u00b7brann\u00b7te\u00b7ren", "J\u00fcng\u00b7lings", "Wim\u00b7per", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Ein sanftes Thr\u00e4nchen weint' ich an Idens Brust.", "tokens": ["Ein", "sanf\u00b7tes", "Thr\u00e4n\u00b7chen", "weint'", "ich", "an", "I\u00b7dens", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "NE", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein sanftes Thr\u00e4nchen weinte die Himmlische,", "tokens": ["Ein", "sanf\u00b7tes", "Thr\u00e4n\u00b7chen", "wein\u00b7te", "die", "Himm\u00b7li\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Als wir uns in vertrauter Laube,", "tokens": ["Als", "wir", "uns", "in", "ver\u00b7trau\u00b7ter", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Busen an Busen, umschlungen hielten.", "tokens": ["Bu\u00b7sen", "an", "Bu\u00b7sen", ",", "um\u00b7schlun\u00b7gen", "hiel\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "VVPP", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Tief Mitternacht war um uns. Der bl\u00fchende", "tokens": ["Tief", "Mit\u00b7ter\u00b7nacht", "war", "um", "uns", ".", "Der", "bl\u00fc\u00b7hen\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "NN", "VAFIN", "APPR", "PPER", "$.", "ART", "ADJA"], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Jasmin der Laube duftete rings um uns.", "tokens": ["Jas\u00b7min", "der", "Lau\u00b7be", "duf\u00b7te\u00b7te", "rings", "um", "uns", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Der Schwan, der Adler und die Leyer", "tokens": ["Der", "Schwan", ",", "der", "Ad\u00b7ler", "und", "die", "Le\u00b7yer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Blinkten uns an durch das Gr\u00fcn der Laube.", "tokens": ["Blink\u00b7ten", "uns", "an", "durch", "das", "Gr\u00fcn", "der", "Lau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Da zuckte durch die Seele des Staunenden", "tokens": ["Da", "zuck\u00b7te", "durch", "die", "See\u00b7le", "des", "Stau\u00b7nen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Wehgedanke, dumpf, wie ein Unkenlaut,", "tokens": ["Ein", "Weh\u00b7ge\u00b7dan\u00b7ke", ",", "dumpf", ",", "wie", "ein", "Un\u00b7ken\u00b7laut", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und schaurig, wie der Duft des Wermuths,", "tokens": ["Und", "schau\u00b7rig", ",", "wie", "der", "Duft", "des", "Wer\u00b7muths", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ach, der Gedanke des nahen Scheidens.", "tokens": ["Ach", ",", "der", "Ge\u00b7dan\u00b7ke", "des", "na\u00b7hen", "Schei\u00b7dens", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Und inniger umschlang ich die Liebliche.", "tokens": ["Und", "in\u00b7ni\u00b7ger", "um\u00b7schlang", "ich", "die", "Lieb\u00b7li\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "--+--+--+--", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Erhaben schw\u00e4rmend wand ich aus ihrem Arm", "tokens": ["Er\u00b7ha\u00b7ben", "schw\u00e4r\u00b7mend", "wand", "ich", "aus", "ih\u00b7rem", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mich los, und sah des Himmelswagens", "tokens": ["Mich", "los", ",", "und", "sah", "des", "Him\u00b7mels\u00b7wa\u00b7gens"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Goldne Deichsel das Laub durchfunkeln.", "tokens": ["Gold\u00b7ne", "Deich\u00b7sel", "das", "Laub", "durch\u00b7fun\u00b7keln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Emporgetragen \u00fcber das Irdische,", "tokens": ["Em\u00b7por\u00b7ge\u00b7tra\u00b7gen", "\u00fc\u00b7ber", "das", "Ir\u00b7di\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ergriff ich Idens Lilienhand, und sprach:", "tokens": ["Er\u00b7griff", "ich", "I\u00b7dens", "Li\u00b7li\u00b7en\u00b7hand", ",", "und", "sprach", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "NE", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "\u201esiehst du des hohen Himmelswagens", "tokens": ["\u201e", "siehst", "du", "des", "ho\u00b7hen", "Him\u00b7mels\u00b7wa\u00b7gens"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u201egoldene Deichsel? sie sey uns heilig!", "tokens": ["\u201e", "gol\u00b7de\u00b7ne", "Deich\u00b7sel", "?", "sie", "sey", "uns", "hei\u00b7lig", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$.", "PPER", "VAFIN", "PPER", "ADJD", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "\u201eeinst werd' ich um dich trauren in \u00f6der Fern'.", "tokens": ["\u201e", "einst", "werd'", "ich", "um", "dich", "trau\u00b7ren", "in", "\u00f6\u00b7der", "Fern'", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "APPR", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "\u201eeinst wirst du nach mir schmachten in \u00f6der Fern'.", "tokens": ["\u201e", "einst", "wirst", "du", "nach", "mir", "schmach\u00b7ten", "in", "\u00f6\u00b7der", "Fern'", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "APPR", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "\u201edann seyd, ihr Gl\u00e4nzenden, uns B\u00fcrgen", "tokens": ["\u201e", "dann", "seyd", ",", "ihr", "Gl\u00e4n\u00b7zen\u00b7den", ",", "uns", "B\u00fcr\u00b7gen"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADV", "VAFIN", "$,", "PPOSAT", "NN", "$,", "PPER", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eunsrer Vereinung auf sch\u00f6nern Fluren!\u201c", "tokens": ["\u201e", "uns\u00b7rer", "Ver\u00b7ei\u00b7nung", "auf", "sch\u00f6\u00b7nern", "Flu\u00b7ren", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Ich sprachs, und eine Thr\u00e4ne der Wehmuth rann", "tokens": ["Ich", "sprachs", ",", "und", "ei\u00b7ne", "Thr\u00e4\u00b7ne", "der", "Weh\u00b7muth", "rann"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von Idens blasser Wange. Sie rinnt seitdem", "tokens": ["Von", "I\u00b7dens", "blas\u00b7ser", "Wan\u00b7ge", ".", "Sie", "rinnt", "seit\u00b7dem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr oft die Wang' hinab, wenn D\u00e4mmrung", "tokens": ["Ihr", "oft", "die", "Wang'", "hin\u00b7ab", ",", "wenn", "D\u00e4mm\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "ART", "NN", "PTKVZ", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rings um sie wallt und die Sterne funkeln \u2014", "tokens": ["Rings", "um", "sie", "wallt", "und", "die", "Ster\u00b7ne", "fun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVFIN", "KON", "ART", "NN", "VVINF", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Der du des ", "tokens": ["Der", "du", "des"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPER", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Zum Sph\u00e4rentanz die ", "tokens": ["Zum", "Sph\u00e4\u00b7ren\u00b7tanz", "die"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Schliffst den kristallenen Freuden", "tokens": ["Schliffst", "den", "kris\u00b7tal\u00b7le\u00b7nen", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Vernimm der Liebe Flehen, Allliebender!", "tokens": ["Ver\u00b7nimm", "der", "Lie\u00b7be", "Fle\u00b7hen", ",", "A\u00b7lllie\u00b7ben\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "NN", "$,", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nicht reisse mich aus meiner Erw\u00e4hlten Arm!", "tokens": ["Nicht", "reis\u00b7se", "mich", "aus", "mei\u00b7ner", "Er\u00b7w\u00e4hl\u00b7ten", "Arm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nur Sie, nur Sie lass mir! Lass Iden,", "tokens": ["Nur", "Sie", ",", "nur", "Sie", "lass", "mir", "!", "Lass", "I\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "ADV", "PPER", "VVFIN", "PPER", "$.", "NE", "NE", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Iden am Arm mich durchs Leben wandeln!", "tokens": ["I\u00b7den", "am", "Arm", "mich", "durchs", "Le\u00b7ben", "wan\u00b7deln", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.14": {"line.1": {"text": "Von ihrem Engell\u00e4cheln zur That entflammt,", "tokens": ["Von", "ih\u00b7rem", "En\u00b7gel\u00b7l\u00e4\u00b7cheln", "zur", "That", "ent\u00b7flammt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von ihrem keuschen Kusse mit Kraft beseelt,", "tokens": ["Von", "ih\u00b7rem", "keu\u00b7schen", "Kus\u00b7se", "mit", "Kraft", "be\u00b7seelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVPP", "$,"], 