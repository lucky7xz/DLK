{"textgrid.poem.39282": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was des Herzens Drang geschaffen,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was des Herzens Drang geschaffen,", "tokens": ["Was", "des", "Her\u00b7zens", "Drang", "ge\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Magst du treu und fest bewahren,", "tokens": ["Magst", "du", "treu", "und", "fest", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis in freudigem Erraffen", "tokens": ["Bis", "in", "freu\u00b7di\u00b7gem", "Er\u00b7raf\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich der Klang mag offenbaren.", "tokens": ["Sich", "der", "Klang", "mag", "of\u00b7fen\u00b7ba\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df ihn dann zum Lichte schie\u00dfen,", "tokens": ["La\u00df", "ihn", "dann", "zum", "Lich\u00b7te", "schie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn er nicht erliegt der Blendung,", "tokens": ["Wenn", "er", "nicht", "er\u00b7liegt", "der", "Blen\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn die Sonne zu begr\u00fc\u00dfen", "tokens": ["Denn", "die", "Son\u00b7ne", "zu", "be\u00b7gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist allein des Adlers Sendung.", "tokens": ["Ist", "al\u00b7lein", "des", "Ad\u00b7lers", "Sen\u00b7dung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Setz' ihn dann auf Rosses R\u00fccken,", "tokens": ["Setz'", "ihn", "dann", "auf", "Ros\u00b7ses", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn er's k\u00fchn vermag zu spornen,", "tokens": ["Wenn", "er's", "k\u00fchn", "ver\u00b7mag", "zu", "spor\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lehr' ihn dann erst Rosen pfl\u00fccken,", "tokens": ["Lehr'", "ihn", "dann", "erst", "Ro\u00b7sen", "pfl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn die H\u00e4nd' er wahrt vor Dornen.", "tokens": ["Wenn", "die", "H\u00e4nd'", "er", "wahrt", "vor", "Dor\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Lehr' ihn dann das Meer befahren,", "tokens": ["Lehr'", "ihn", "dann", "das", "Meer", "be\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn sein Segel wehrt den St\u00fcrmen,", "tokens": ["Wenn", "sein", "Se\u00b7gel", "wehrt", "den", "St\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Send' ihn dann in Schlachtgefahren,", "tokens": ["Send'", "ihn", "dann", "in", "Schlacht\u00b7ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn ihn starke Panzer schirmen.", "tokens": ["Wenn", "ihn", "star\u00b7ke", "Pan\u00b7zer", "schir\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "La\u00df ihn dann durch Strudel tauchen,", "tokens": ["La\u00df", "ihn", "dann", "durch", "Stru\u00b7del", "tau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn sein Arm die Flut mag zwingen,", "tokens": ["Wenn", "sein", "Arm", "die", "Flut", "mag", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann erst in die Tuba hauchen,", "tokens": ["Dann", "erst", "in", "die", "Tu\u00b7ba", "hau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn ihm nicht die Adern springen.", "tokens": ["Wenn", "ihm", "nicht", "die", "A\u00b7dern", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "F\u00fchr' ihn dann zu blut'gen Siegen,", "tokens": ["F\u00fchr'", "ihn", "dann", "zu", "blut'\u00b7gen", "Sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn der Arm nicht macht das Schwert matt,", "tokens": ["Wenn", "der", "Arm", "nicht", "macht", "das", "Schwert", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lehr' ihn in der Rennbahn fliegen,", "tokens": ["Lehr'", "ihn", "in", "der", "Renn\u00b7bahn", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn er Schenkelkraft bew\u00e4hrt hat.", "tokens": ["Wenn", "er", "Schen\u00b7kel\u00b7kraft", "be\u00b7w\u00e4hrt", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Der steht nimmer im Gefechte,", "tokens": ["Der", "steht", "nim\u00b7mer", "im", "Ge\u00b7fech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dem vom morschen Holz die Lanze,", "tokens": ["Dem", "vom", "mor\u00b7schen", "Holz", "die", "Lan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und des Ringers lahme Rechte", "tokens": ["Und", "des", "Rin\u00b7gers", "lah\u00b7me", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimmer greift zum Siegeskranze.", "tokens": ["Nim\u00b7mer", "greift", "zum", "Sie\u00b7ges\u00b7kran\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}