{"dta.poem.18210": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1.  \n  Meine Nachbarschaft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Auf schmutzge D\u00e4cher und auf ru\u00dfge Mauern,", "tokens": ["Auf", "schmutz\u00b7ge", "D\u00e4\u00b7cher", "und", "auf", "ru\u00df\u00b7ge", "Mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Doch wer wie ich ein St\u00fcckchen Philosoph,", "tokens": ["Doch", "wer", "wie", "ich", "ein", "St\u00fcck\u00b7chen", "Phi\u00b7lo\u00b7soph", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "KOKOM", "PPER", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "L\u00e4\u00dft darum sich noch lange nicht bedauern.", "tokens": ["L\u00e4\u00dft", "da\u00b7rum", "sich", "noch", "lan\u00b7ge", "nicht", "be\u00b7dau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "PRF", "ADV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein wenig Luft, ein wenig Sonnenlicht", "tokens": ["Ein", "we\u00b7nig", "Luft", ",", "ein", "we\u00b7nig", "Son\u00b7nen\u00b7licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "$,", "ART", "PIAT", "NN"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.5": {"text": "Dringt schlie\u00dflich auch durch ", "tokens": ["Dringt", "schlie\u00df\u00b7lich", "auch", "durch"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Zu hungern und zu frieren brauch ich nicht", "tokens": ["Zu", "hun\u00b7gern", "und", "zu", "frie\u00b7ren", "brauch", "ich", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und all mein Thun ist nur ein wenig Schreiben.", "tokens": ["Und", "all", "mein", "Thun", "ist", "nur", "ein", "we\u00b7nig", "Schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "VAFIN", "ADV", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein wenig Schreiben, wenn ich stundenlang", "tokens": ["Ein", "we\u00b7nig", "Schrei\u00b7ben", ",", "wenn", "ich", "stun\u00b7den\u00b7lang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Mich einlas in die Wunderwelt der Alten,", "tokens": ["Mich", "ein\u00b7las", "in", "die", "Wun\u00b7der\u00b7welt", "der", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis endlich, endlich es auch mir gelang,", "tokens": ["Bis", "end\u00b7lich", ",", "end\u00b7lich", "es", "auch", "mir", "ge\u00b7lang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "ADV", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was ich gef\u00fchlt, zum Wohllaut zu gestalten.", "tokens": ["Was", "ich", "ge\u00b7f\u00fchlt", ",", "zum", "Wohl\u00b7laut", "zu", "ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dann flie\u00dft es um mich wie ein Heilgenschein", "tokens": ["Dann", "flie\u00dft", "es", "um", "mich", "wie", "ein", "Heil\u00b7gen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "KOKOM", "ART", "NN"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.6": {"text": "Und mir im Herzen bauen sich Alt\u00e4re;", "tokens": ["Und", "mir", "im", "Her\u00b7zen", "bau\u00b7en", "sich", "Al\u00b7t\u00e4\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VVFIN", "PRF", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So k\u00f6nnt' ich gl\u00fccklich und zufrieden sein,", "tokens": ["So", "k\u00f6nnt'", "ich", "gl\u00fcck\u00b7lich", "und", "zu\u00b7frie\u00b7den", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "KON", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wenn ach, nur meine Nachbarschaft nicht w\u00e4re!", "tokens": ["Wenn", "ach", ",", "nur", "mei\u00b7ne", "Nach\u00b7bar\u00b7schaft", "nicht", "w\u00e4\u00b7re", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ITJ", "$,", "ADV", "PPOSAT", "NN", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Kein Schw\u00e4rmer ist es, der die Fl\u00f6te liebt", "tokens": ["Kein", "Schw\u00e4r\u00b7mer", "ist", "es", ",", "der", "die", "Fl\u00f6\u00b7te", "liebt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und auf ihr nur \u201edes Sommers letzte Rose\u201c,", "tokens": ["Und", "auf", "ihr", "nur", "\u201e", "des", "Som\u00b7mers", "letz\u00b7te", "Ro\u00b7se", "\u201c", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "$(", "ART", "NN", "ADJA", "NN", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kein Tanzgenie, das ewig Stunden gibt,", "tokens": ["Kein", "Tanz\u00b7ge\u00b7nie", ",", "das", "e\u00b7wig", "Stun\u00b7den", "gibt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auch kein klavierverr\u00fcckter Virtuose:", "tokens": ["Auch", "kein", "kla\u00b7vier\u00b7ver\u00b7r\u00fcck\u00b7ter", "Vir\u00b7tu\u00b7o\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein armer Schuster nur, der n\u00e4chtens flickt,", "tokens": ["Ein", "ar\u00b7mer", "Schus\u00b7ter", "nur", ",", "der", "n\u00e4ch\u00b7tens", "flickt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wenn l\u00e4ngst aufs Dach herab die Sterne scheinen,", "tokens": ["Wenn", "l\u00e4ngst", "aufs", "Dach", "her\u00b7ab", "die", "Ster\u00b7ne", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPRART", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Inde\u00df sein Weib daneben sitzt und strickt", "tokens": ["In\u00b7de\u00df", "sein", "Weib", "da\u00b7ne\u00b7ben", "sitzt", "und", "strickt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "PAV", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und seine Kinderchen vor Hunger weinen!", "tokens": ["Und", "sei\u00b7ne", "Kin\u00b7der\u00b7chen", "vor", "Hun\u00b7ger", "wei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}}, "stanza.4": {"line.1": {"text": "O Gott, wie oft nicht schon hat dieser Laut", "tokens": ["O", "Gott", ",", "wie", "oft", "nicht", "schon", "hat", "die\u00b7ser", "Laut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PWAV", "ADV", "PTKNEG", "ADV", "VAFIN", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mich mitten aus dem tiefsten Schlaf ger\u00fcttelt!", "tokens": ["Mich", "mit\u00b7ten", "aus", "dem", "tiefs\u00b7ten", "Schlaf", "ge\u00b7r\u00fct\u00b7telt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wenn ich halbwach dann mich umgeschaut,", "tokens": ["Und", "wenn", "ich", "halb\u00b7wach", "dann", "mich", "um\u00b7ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hat wild es wie ein Fieber mich gesch\u00fcttelt.", "tokens": ["Hat", "wild", "es", "wie", "ein", "Fie\u00b7ber", "mich", "ge\u00b7sch\u00fct\u00b7telt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "KOKOM", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Des M\u00e4dchens Schluchzen und des Knaben Schrei", "tokens": ["Des", "M\u00e4d\u00b7chens", "Schluch\u00b7zen", "und", "des", "Kna\u00b7ben", "Schrei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und ganz zuletzt des S\u00e4uglings leises Wimmern \u2014", "tokens": ["Und", "ganz", "zu\u00b7letzt", "des", "S\u00e4ug\u00b7lings", "lei\u00b7ses", "Wim\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Mir war's, als h\u00f6rte ich dann nebenbei", "tokens": ["Mir", "wa\u00b7r's", ",", "als", "h\u00f6r\u00b7te", "ich", "dann", "ne\u00b7ben\u00b7bei"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "ADV", "APPR"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Drei kleine, kleine schwarze Bettlein zimmern.", "tokens": ["Drei", "klei\u00b7ne", ",", "klei\u00b7ne", "schwar\u00b7ze", "Bet\u00b7tlein", "zim\u00b7mern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "$,", "ADJA", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Mir war's, als rollte dumpf dann vor das Haus", "tokens": ["Mir", "wa\u00b7r's", ",", "als", "roll\u00b7te", "dumpf", "dann", "vor", "das", "Haus"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "ADJD", "ADV", "APPR", "ART", "NN"], "meter": "----+-+-+-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Der nur zu wohlbekannte Armenwagen", "tokens": ["Der", "nur", "zu", "wohl\u00b7be\u00b7kann\u00b7te", "Ar\u00b7men\u00b7wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und jene Bettlein trugen sie hinaus", "tokens": ["Und", "je\u00b7ne", "Bet\u00b7tlein", "tru\u00b7gen", "sie", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "VVFIN", "PPER", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und luden sie in seinen d\u00fcstern Schragen.", "tokens": ["Und", "lu\u00b7den", "sie", "in", "sei\u00b7nen", "d\u00fcs\u00b7tern", "Schra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Kutscher aber nahm noch einen Schluck", "tokens": ["Der", "Kut\u00b7scher", "a\u00b7ber", "nahm", "noch", "ei\u00b7nen", "Schluck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und peitschte fluchend seine magren Schinder", "tokens": ["Und", "peitschte", "flu\u00b7chend", "sei\u00b7ne", "mag\u00b7ren", "Schin\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und \u00fcbers Pflaster dann ging's Ruck auf Ruck,", "tokens": ["Und", "\u00fc\u00b7bers", "Pflas\u00b7ter", "dann", "ging's", "Ruck", "auf", "Ruck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADV", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Doch ach, noch immer wimmerten die Kinder!", "tokens": ["Doch", "ach", ",", "noch", "im\u00b7mer", "wim\u00b7mer\u00b7ten", "die", "Kin\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "ADV", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und immer, immer noch klang's mir im Ohr,", "tokens": ["Und", "im\u00b7mer", ",", "im\u00b7mer", "noch", "klang's", "mir", "im", "Ohr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Wenn schon der Morgen durch das Fenster blickte,", "tokens": ["Wenn", "schon", "der", "Mor\u00b7gen", "durch", "das", "Fens\u00b7ter", "blick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und mir ums Auge hing ein Thr\u00e4nenflor,", "tokens": ["Und", "mir", "ums", "Au\u00b7ge", "hing", "ein", "Thr\u00e4\u00b7nen\u00b7flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn ich dann stumm mein Tagewerk beschickte.", "tokens": ["Wenn", "ich", "dann", "stumm", "mein", "Ta\u00b7ge\u00b7werk", "be\u00b7schick\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was half mir nun mein \u201eSt\u00fcckchen Philosoph\u201c?", "tokens": ["Was", "half", "mir", "nun", "mein", "\u201e", "St\u00fcck\u00b7chen", "Phi\u00b7lo\u00b7soph", "\u201c", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PPOSAT", "$(", "NN", "NE", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In Tr\u00fcmmer fiel, was ich so luftig baute!", "tokens": ["In", "Tr\u00fcm\u00b7mer", "fiel", ",", "was", "ich", "so", "luf\u00b7tig", "bau\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "PWS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch that's das Haus nicht, nicht der d\u00fcstre Hof,", "tokens": ["Doch", "that's", "das", "Haus", "nicht", ",", "nicht", "der", "d\u00fcst\u00b7re", "Hof", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "$,", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nein, nur die abgebrochnen Kindeslaute! \u2014", "tokens": ["Nein", ",", "nur", "die", "ab\u00b7ge\u00b7broch\u00b7nen", "Kin\u00b7des\u00b7lau\u00b7te", "!"], "token_info": ["word", "punct"