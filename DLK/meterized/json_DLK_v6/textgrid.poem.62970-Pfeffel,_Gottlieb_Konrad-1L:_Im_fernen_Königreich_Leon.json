{"textgrid.poem.62970": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im fernen K\u00f6nigreich Leon", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im fernen K\u00f6nigreich Leon", "tokens": ["Im", "fer\u00b7nen", "K\u00f6\u00b7nig\u00b7reich", "Le\u00b7on"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Liegt eine Wunderh\u00f6hle,", "tokens": ["Liegt", "ei\u00b7ne", "Wun\u00b7der\u00b7h\u00f6h\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von der man tausend Jahre schon", "tokens": ["Von", "der", "man", "tau\u00b7send", "Jah\u00b7re", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PIS", "CARD", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erz\u00e4hlt was ich erz\u00e4hle.", "tokens": ["Er\u00b7z\u00e4hlt", "was", "ich", "er\u00b7z\u00e4h\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Sage geht: auf diesem Platz", "tokens": ["Die", "Sa\u00b7ge", "geht", ":", "auf", "die\u00b7sem", "Platz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Begrub ein Magus einen Schatz", "tokens": ["Be\u00b7grub", "ein", "Ma\u00b7gus", "ei\u00b7nen", "Schatz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von einer Tonne Goldes.", "tokens": ["Von", "ei\u00b7ner", "Ton\u00b7ne", "Gol\u00b7des", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihn h\u00e4tte mancher gern geraubt;", "tokens": ["Ihn", "h\u00e4t\u00b7te", "man\u00b7cher", "gern", "ge\u00b7raubt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jedoch ein schwarzer Drache,", "tokens": ["Je\u00b7doch", "ein", "schwar\u00b7zer", "Dra\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Bastart Satans, wie man glaubt,", "tokens": ["Ein", "Bas\u00b7tart", "Sa\u00b7tans", ",", "wie", "man", "glaubt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hielt vor der H\u00f6hle Wache,", "tokens": ["Hielt", "vor", "der", "H\u00f6h\u00b7le", "Wa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und wollte sich ein K\u00e4mpfer nahn,", "tokens": ["Und", "woll\u00b7te", "sich", "ein", "K\u00e4mp\u00b7fer", "nahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So ward er stracks an seinen Zahn", "tokens": ["So", "ward", "er", "stracks", "an", "sei\u00b7nen", "Zahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie ein Kapaun gespie\u00dfet.", "tokens": ["Wie", "ein", "Ka\u00b7paun", "ge\u00b7spie\u00b7\u00dfet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die\u00df h\u00f6rte Junker Theogan", "tokens": ["Die\u00df", "h\u00f6r\u00b7te", "Jun\u00b7ker", "Theo\u00b7gan"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "NE", "NE"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.2": {"text": "Aus Rh\u00e4tiens Gebirgen", "tokens": ["Aus", "Rh\u00e4\u00b7ti\u00b7ens", "Ge\u00b7bir\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und warb zweyhundert Reuter an,", "tokens": ["Und", "warb", "zwey\u00b7hun\u00b7dert", "Reu\u00b7ter", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Unthier zu erw\u00fcrgen.", "tokens": ["Das", "Un\u00b7thier", "zu", "er\u00b7w\u00fcr\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit diesem Heere trabt der Held", "tokens": ["Mit", "die\u00b7sem", "Hee\u00b7re", "trabt", "der", "Held"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drey Monden lang durch Thal und Feld", "tokens": ["Drey", "Mon\u00b7den", "lang", "durch", "Thal", "und", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wechselt manchen Gulden.", "tokens": ["Und", "wech\u00b7selt", "man\u00b7chen", "Gul\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der letzte war bereits verzehrt,", "tokens": ["Der", "letz\u00b7te", "war", "be\u00b7reits", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als er den Ort erblickte,", "tokens": ["Als", "er", "den", "Ort", "er\u00b7blick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und mit den Seinen, wohl bewehrt,", "tokens": ["Und", "mit", "den", "Sei\u00b7nen", ",", "wohl", "be\u00b7wehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "PPOSS", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich froh zum Kampfe schickte.", "tokens": ["Sich", "froh", "zum", "Kamp\u00b7fe", "schick\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie fielen mit vereintem Muth", "tokens": ["Sie", "fie\u00b7len", "mit", "ver\u00b7ein\u00b7tem", "Muth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Lindwurm an, der H\u00f6llenwuth", "tokens": ["Den", "Lind\u00b7wurm", "an", ",", "der", "H\u00f6l\u00b7len\u00b7wuth"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Aus seinen Augen spr\u00fchte.", "tokens": ["Aus", "sei\u00b7nen", "Au\u00b7gen", "spr\u00fch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Doch manches Schwerdt und mancher Spie\u00df", "tokens": ["Doch", "man\u00b7ches", "Schwerdt", "und", "man\u00b7cher", "Spie\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zerbrach auf seinem R\u00fccken,", "tokens": ["Zer\u00b7brach", "auf", "sei\u00b7nem", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und manchen tapfern Knappen ri\u00df", "tokens": ["Und", "man\u00b7chen", "tap\u00b7fern", "Knap\u00b7pen", "ri\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Cerberus in St\u00fccken.", "tokens": ["Der", "Cer\u00b7be\u00b7rus", "in", "St\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Schon war ein Schock des Todes Raub,", "tokens": ["Schon", "war", "ein", "Schock", "des", "To\u00b7des", "Raub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und vierzig \u00e4chzten lahm im Staub,", "tokens": ["Und", "vier\u00b7zig", "\u00e4chz\u00b7ten", "lahm", "im", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "ADJA", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als Theogan ihn f\u00e4llte.", "tokens": ["Als", "Theo\u00b7gan", "ihn", "f\u00e4ll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Von Blute triefend hob der Held", "tokens": ["Von", "Blu\u00b7te", "trie\u00b7fend", "hob", "der", "Held"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Schatz von seinem Posten:", "tokens": ["Den", "Schatz", "von", "sei\u00b7nem", "Pos\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er fand an sch\u00f6nem baarem Geld", "tokens": ["Er", "fand", "an", "sch\u00f6\u00b7nem", "baa\u00b7rem", "Geld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Just seine Reisekosten;", "tokens": ["Just", "sei\u00b7ne", "Rei\u00b7se\u00b7kos\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und \u00fcberdie\u00df in einem Schrein", "tokens": ["Und", "\u00fc\u00b7ber\u00b7die\u00df", "in", "ei\u00b7nem", "Schrein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein kleines Fa\u00df mit Branntewein,", "tokens": ["Ein", "klei\u00b7nes", "Fa\u00df", "mit", "Brann\u00b7te\u00b7wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Wunden zu verbinden.", "tokens": ["Die", "Wun\u00b7den", "zu", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}