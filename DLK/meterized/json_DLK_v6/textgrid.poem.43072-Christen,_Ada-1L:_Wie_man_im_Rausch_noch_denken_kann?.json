{"textgrid.poem.43072": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie man im Rausch noch denken kann?", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie man im Rausch noch denken kann?", "tokens": ["Wie", "man", "im", "Rausch", "noch", "den\u00b7ken", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPRART", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr meint wohl, da\u00df die Gedanken,", "tokens": ["Ihr", "meint", "wohl", ",", "da\u00df", "die", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "So wie die matten, schweren F\u00fc\u00dfe", "tokens": ["So", "wie", "die", "mat\u00b7ten", ",", "schwe\u00b7ren", "F\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auch immer knicken und schwanken.", "tokens": ["Auch", "im\u00b7mer", "kni\u00b7cken", "und", "schwan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "KON", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Mein Leben ist ein langer Rausch,", "tokens": ["Mein", "Le\u00b7ben", "ist", "ein", "lan\u00b7ger", "Rausch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und weil ich darin gar viel gedacht,", "tokens": ["Und", "weil", "ich", "da\u00b7rin", "gar", "viel", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PAV", "ADV", "ADV", "VVPP", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "So hat mich das viele Denken", "tokens": ["So", "hat", "mich", "das", "vie\u00b7le", "Den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "PIAT", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Zuletzt noch n\u00fcchtern gemacht.", "tokens": ["Zu\u00b7letzt", "noch", "n\u00fcch\u00b7tern", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}