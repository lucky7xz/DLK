{"dta.poem.18090": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Martin Opitzens Elegie  \n aus Dan. Heinsii  Monobiblo.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr aber wisset nichts als nur auf Gut zu sinnen,", "tokens": ["Ihr", "a\u00b7ber", "wis\u00b7set", "nichts", "als", "nur", "auf", "Gut", "zu", "sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PIS", "KOKOM", "ADV", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und zieht bald \u00fcber Feld, bald durch das wilde Meer,", "tokens": ["Und", "zieht", "bald", "\u00fc\u00b7ber", "Feld", ",", "bald", "durch", "das", "wil\u00b7de", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "$,", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ja wohin auch die Sonn hat niemals reichen k\u00f6nnen,", "tokens": ["Ja", "wo\u00b7hin", "auch", "die", "Sonn", "hat", "nie\u00b7mals", "rei\u00b7chen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PWAV", "ADV", "ART", "NN", "VAFIN", "ADV", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da bringet ihr das Gold, den sch\u00f6nen Koth, anher.", "tokens": ["Da", "brin\u00b7get", "ihr", "das", "Gold", ",", "den", "sch\u00f6\u00b7nen", "Koth", ",", "an\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "ADJA", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und ich bedarff di\u00df nicht, was ihr an allen Enden", "tokens": ["Und", "ich", "be\u00b7darff", "di\u00df", "nicht", ",", "was", "ihr", "an", "al\u00b7len", "En\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PDS", "PTKNEG", "$,", "PWS", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zu Land und Wasser sucht, das hab ich schon bey mir;", "tokens": ["Zu", "Land", "und", "Was\u00b7ser", "sucht", ",", "das", "hab", "ich", "schon", "bey", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,", "PDS", "VAFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mein Gut ist, da\u00df ich sterb in meiner Liebsten H\u00e4nden,", "tokens": ["Mein", "Gut", "ist", ",", "da\u00df", "ich", "sterb", "in", "mei\u00b7ner", "Liebs\u00b7ten", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Strasse wandel ich gar sicher f\u00fcr und f\u00fcr.", "tokens": ["Die", "Stras\u00b7se", "wan\u00b7del", "ich", "gar", "si\u00b7cher", "f\u00fcr", "und", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dann jetzund wird mein Geist von ihrem Geist empfangen,", "tokens": ["Dann", "je\u00b7tzund", "wird", "mein", "Geist", "von", "ih\u00b7rem", "Geist", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wann er das sch\u00f6ne Thor des Mundes kommt hinein,", "tokens": ["Wann", "er", "das", "sch\u00f6\u00b7ne", "Thor", "des", "Mun\u00b7des", "kommt", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Jetzund ergeh ich mich bey den liebreichen Wangen,", "tokens": ["Je\u00b7tzund", "er\u00b7geh", "ich", "mich", "bey", "den", "lieb\u00b7rei\u00b7chen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Da Venus und ihr Sohn pers\u00f6nlich wohnhaft seyn.", "tokens": ["Da", "Ve\u00b7nus", "und", "ihr", "Sohn", "per\u00b7s\u00f6n\u00b7lich", "wohn\u00b7haft", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "PPOSAT", "NN", "ADJD", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Bald hat sie mir, ich ihr, den zarten Hals umgeben,", "tokens": ["Bald", "hat", "sie", "mir", ",", "ich", "ihr", ",", "den", "zar\u00b7ten", "Hals", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,", "PPER", "PPER", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und schau, wie die Natur so trefflich sie geziert,", "tokens": ["Und", "schau", ",", "wie", "die", "Na\u00b7tur", "so", "treff\u00b7lich", "sie", "ge\u00b7ziert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "$,", "PWAV", "ART", "NN", "ADV", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Bald in den Aeugelein enthalt ich mir das Leben,", "tokens": ["Bald", "in", "den", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", "ent\u00b7halt", "ich", "mir", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.16": {"text": "Dahin werd ich zugleich mit Sinn und Muth gef\u00fchrt.", "tokens": ["Da\u00b7hin", "werd", "ich", "zu\u00b7gleich", "mit", "Sinn", "und", "Muth", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wie der so unverschuldt sein Vaterland verlassen,", "tokens": ["Wie", "der", "so", "un\u00b7ver\u00b7schuldt", "sein", "Va\u00b7ter\u00b7land", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADV", "ADJD", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Mu\u00df suchen einen Weg der ihm gantz unbekandt,", "tokens": ["Mu\u00df", "su\u00b7chen", "ei\u00b7nen", "Weg", "der", "ihm", "gantz", "un\u00b7be\u00b7kandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "ART", "NN", "ART", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Geht \u00fcber Berg und Thal durch angenehme Strassen,", "tokens": ["Geht", "\u00fc\u00b7ber", "Berg", "und", "Thal", "durch", "an\u00b7ge\u00b7neh\u00b7me", "Stras\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Nichts achtend, als allein sein liebes Vaterland:", "tokens": ["Nichts", "ach\u00b7tend", ",", "als", "al\u00b7lein", "sein", "lie\u00b7bes", "Va\u00b7ter\u00b7land", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "$,", "KOUS", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wann er dann ungefehr erblicket einen Bronnen,", "tokens": ["Wann", "er", "dann", "un\u00b7ge\u00b7fehr", "er\u00b7bli\u00b7cket", "ei\u00b7nen", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der sonst verborgen ist in mitten in dem Wald,", "tokens": ["Der", "sonst", "ver\u00b7bor\u00b7gen", "ist", "in", "mit\u00b7ten", "in", "dem", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "VAFIN", "APPR", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Befreyet vor der Hitz und Ungedult der Sonnen,", "tokens": ["Be\u00b7fre\u00b7yet", "vor", "der", "Hitz", "und", "Un\u00b7ge\u00b7dult", "der", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da nichts als nur das Wild hat seinen Aufenthalt,", "tokens": ["Da", "nichts", "als", "nur", "das", "Wild", "hat", "sei\u00b7nen", "Auf\u00b7ent\u00b7halt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "KOKOM", "ADV", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "So ist er wolgemuth, vergisset aller Dinge,", "tokens": ["So", "ist", "er", "wol\u00b7ge\u00b7muth", ",", "ver\u00b7gis\u00b7set", "al\u00b7ler", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Erforschet nur den Quell des Br\u00fcnneleins mit Flei\u00df,", "tokens": ["Er\u00b7for\u00b7schet", "nur", "den", "Quell", "des", "Br\u00fcn\u00b7nel\u00b7eins", "mit", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und w\u00fcnscht, da\u00df ihn alldar der sanffte Schlaf umringe,", "tokens": ["Und", "w\u00fcnscht", ",", "da\u00df", "ihn", "all\u00b7dar", "der", "sanff\u00b7te", "Schlaf", "um\u00b7rin\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Weil er vor grosser Lust sich selber auch nicht wei\u00df.", "tokens": ["Weil", "er", "vor", "gros\u00b7ser", "Lust", "sich", "sel\u00b7ber", "auch", "nicht", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PRF", "ADV", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Nicht weniger auch mich, weil ich so sehr geirret", "tokens": ["Nicht", "we\u00b7ni\u00b7ger", "auch", "mich", ",", "weil", "ich", "so", "sehr", "ge\u00b7ir\u00b7ret"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ADV", "PPER", "$,", "KOUS", "PPER", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Durch Freud und h\u00f6chste Lust der s\u00fcssen Liebes-Pein,", "tokens": ["Durch", "Freud", "und", "h\u00f6chs\u00b7te", "Lust", "der", "s\u00fcs\u00b7sen", "Lie\u00b7bes\u00b7Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Weil mein Gem\u00fcthe sich in Wollust gantz verwirret,", "tokens": ["Weil", "mein", "Ge\u00b7m\u00fc\u00b7the", "sich", "in", "Wol\u00b7lust", "gantz", "ver\u00b7wir\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wird nichts nicht machen lo\u00df, als nur der Tod allein.", "tokens": ["Wird", "nichts", "nicht", "ma\u00b7chen", "lo\u00df", ",", "als", "nur", "der", "Tod", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PTKNEG", "VVINF", "PTKVZ", "$,", "KOUS", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}