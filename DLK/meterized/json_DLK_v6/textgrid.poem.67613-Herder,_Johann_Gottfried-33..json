{"textgrid.poem.67613": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zusammen reisten Hund und Hahn;", "tokens": ["Zu\u00b7sam\u00b7men", "reis\u00b7ten", "Hund", "und", "Hahn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Nacht brach an.", "tokens": ["Die", "Nacht", "brach", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der Hahn flog auf den Baum,", "tokens": ["Der", "Hahn", "flog", "auf", "den", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hund nahm vorlieb", "tokens": ["Hund", "nahm", "vor\u00b7lieb"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "ADV"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "In einer H\u00f6hle Raum.", "tokens": ["In", "ei\u00b7ner", "H\u00f6h\u00b7le", "Raum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Morgen brach an;", "tokens": ["Mor\u00b7gen", "brach", "an", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Da kr\u00e4ht' der Hahn.", "tokens": ["Da", "kr\u00e4ht'", "der", "Hahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Schnell schlich der Dieb,", "tokens": ["Schnell", "schlich", "der", "Dieb", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Herr Fuchs, heran:", "tokens": ["Herr", "Fuchs", ",", "he\u00b7ran", ":"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "NE", "$,", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "\u00bbihr' Hoheit, darf ich nahn?\u00ab", "tokens": ["\u00bb", "ih\u00b7r'", "Ho\u00b7heit", ",", "darf", "ich", "nahn", "?", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "VMFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "\u00bbfrag unten,\u00ab kr\u00e4ht der Morgens\u00e4nger Hahn,", "tokens": ["\u00bb", "frag", "un\u00b7ten", ",", "\u00ab", "kr\u00e4ht", "der", "Mor\u00b7gen\u00b7s\u00e4n\u00b7ger", "Hahn", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ADV", "$,", "$(", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "\u00bbbei meinem Pf\u00f6rtner an!\u00ab", "tokens": ["\u00bb", "bei", "mei\u00b7nem", "Pf\u00f6rt\u00b7ner", "an", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Hund aus der H\u00f6hle", "tokens": ["Hund", "aus", "der", "H\u00f6h\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Ihm an die Kehle.", "tokens": ["Ihm", "an", "die", "Keh\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein treuer Hund, ein treuer Hahn,", "tokens": ["Ein", "treu\u00b7er", "Hund", ",", "ein", "treu\u00b7er", "Hahn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Compan!", "tokens": ["Com\u00b7pan", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "--", "measure": "unknown.measure.zero"}, "line.3": {"text": "Am Wipfel oder in der H\u00f6hle", "tokens": ["Am", "Wip\u00b7fel", "o\u00b7der", "in", "der", "H\u00f6h\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}}}}}