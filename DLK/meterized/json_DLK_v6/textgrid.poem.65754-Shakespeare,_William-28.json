{"textgrid.poem.65754": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "28", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie soll es dann nun besser mit mir werden,", "tokens": ["Wie", "soll", "es", "dann", "nun", "bes\u00b7ser", "mit", "mir", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ADV", "ADJD", "APPR", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn mir der Ruhe Wohltat bleibt versagt?", "tokens": ["Wenn", "mir", "der", "Ru\u00b7he", "Wohl\u00b7tat", "bleibt", "ver\u00b7sagt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn Nacht nicht heilen will des Tags Beschwerden,", "tokens": ["Wenn", "Nacht", "nicht", "hei\u00b7len", "will", "des", "Tags", "Be\u00b7schwer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKNEG", "VVINF", "VMFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Tag an Nacht, und Nacht am Tage nagt?", "tokens": ["Und", "Tag", "an", "Nacht", ",", "und", "Nacht", "am", "Ta\u00b7ge", "nagt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "$,", "KON", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn jedes zwar dem Reich des andern gram,", "tokens": ["Wenn", "je\u00b7des", "zwar", "dem", "Reich", "des", "an\u00b7dern", "gram", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nur mich zu martern sich die H\u00e4nde reicht,", "tokens": ["Nur", "mich", "zu", "mar\u00b7tern", "sich", "die", "H\u00e4n\u00b7de", "reicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKZU", "VVINF", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Tag mit M\u00fch, die Nacht mit Seelengram;", "tokens": ["Der", "Tag", "mit", "M\u00fch", ",", "die", "Nacht", "mit", "See\u00b7len\u00b7gram", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df all mein M\u00fchen nur mich weiter von dir scheucht.", "tokens": ["Da\u00df", "all", "mein", "M\u00fc\u00b7hen", "nur", "mich", "wei\u00b7ter", "von", "dir", "scheucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "PPOSAT", "NN", "ADV", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dem Tag zu kosen sag' ich, du bist klar,", "tokens": ["Dem", "Tag", "zu", "ko\u00b7sen", "sag'", "ich", ",", "du", "bist", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Du zierst ihn, wenn am Himmel Wolken dunkeln:", "tokens": ["Du", "zierst", "ihn", ",", "wenn", "am", "Him\u00b7mel", "Wol\u00b7ken", "dun\u00b7keln", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "APPRART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Dann schmeichl' ich auch der Nacht im Rabenhaar,", "tokens": ["Dann", "schmeichl'", "ich", "auch", "der", "Nacht", "im", "Ra\u00b7ben\u00b7haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Da\u00df du den Abend stirnst, wenn keine Sterne funkeln.", "tokens": ["Da\u00df", "du", "den", "A\u00b7bend", "stirnst", ",", "wenn", "kei\u00b7ne", "Ster\u00b7ne", "fun\u00b7keln", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "KOUS", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch t\u00e4glich l\u00e4nger spinnt der Tag mein Leid;", "tokens": ["Doch", "t\u00e4g\u00b7lich", "l\u00e4n\u00b7ger", "spinnt", "der", "Tag", "mein", "Leid", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Alln\u00e4chtlich enger schn\u00fcrt die Nacht mein Folterkleid.", "tokens": ["All\u00b7n\u00e4cht\u00b7lich", "en\u00b7ger", "schn\u00fcrt", "die", "Nacht", "mein", "Fol\u00b7ter\u00b7kleid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}