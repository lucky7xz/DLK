{"dta.poem.15867": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "72.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bleibt mit den H\u00f6len, die ich sehn soll, mir vom Leibe!", "tokens": ["Bleibt", "mit", "den", "H\u00f6\u00b7len", ",", "die", "ich", "sehn", "soll", ",", "mir", "vom", "Lei\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr wi\u00dft, da\u00df ich am Licht, in freier Luft gern bleibe.", "tokens": ["Ihr", "wi\u00dft", ",", "da\u00df", "ich", "am", "Licht", ",", "in", "frei\u00b7er", "Luft", "gern", "blei\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "APPRART", "NN", "$,", "APPR", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ja w\u00e4re nicht die Welt entg\u00f6ttert wie sie ist,", "tokens": ["Ja", "w\u00e4\u00b7re", "nicht", "die", "Welt", "ent\u00b7g\u00f6t\u00b7tert", "wie", "sie", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VAFIN", "PTKNEG", "ART", "NN", "VVPP", "KOKOM", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So gieng' ich H\u00f6len sehn als Heide oder Christ.", "tokens": ["So", "gieng'", "ich", "H\u00f6\u00b7len", "sehn", "als", "Hei\u00b7de", "o\u00b7der", "Christ", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "NN", "VVINF", "KOKOM", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dort zeigt' ein Priester mir die des Trofonius,", "tokens": ["Dort", "zeigt'", "ein", "Pries\u00b7ter", "mir", "die", "des", "Tro\u00b7fo\u00b7ni\u00b7us", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ART", "ART", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da die von Tropfstein hier ein Tropf mir zeigen mu\u00df.", "tokens": ["Da", "die", "von", "Tropf\u00b7stein", "hier", "ein", "Tropf", "mir", "zei\u00b7gen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "NN", "ADV", "ART", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}