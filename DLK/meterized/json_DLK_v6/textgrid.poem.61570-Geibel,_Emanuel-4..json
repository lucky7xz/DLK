{"textgrid.poem.61570": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie s\u00e4uselt \u00fcber Tal und H\u00fcgel", "tokens": ["Wie", "s\u00e4u\u00b7selt", "\u00fc\u00b7ber", "Tal", "und", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Gru\u00df des Fr\u00fchlings heut so mild!", "tokens": ["Der", "Gru\u00df", "des", "Fr\u00fch\u00b7lings", "heut", "so", "mild", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von fern erklingt's wie Schwalbenfl\u00fcgel,", "tokens": ["Von", "fern", "er\u00b7klingt's", "wie", "Schwal\u00b7ben\u00b7fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und traumhaft br\u00fctet's im Gefild'.", "tokens": ["Und", "traum\u00b7haft", "br\u00fc\u00b7tet's", "im", "Ge\u00b7fild'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Im Stamm der alten Linde steigen", "tokens": ["Im", "Stamm", "der", "al\u00b7ten", "Lin\u00b7de", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NE", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die S\u00e4fte schon geheimnisvoll;", "tokens": ["Die", "S\u00e4f\u00b7te", "schon", "ge\u00b7heim\u00b7nis\u00b7voll", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sp\u00fcrt's und schauert mit den Zweigen", "tokens": ["Sie", "sp\u00fcrt's", "und", "schau\u00b7ert", "mit", "den", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor Freuden, da\u00df sie gr\u00fcnen soll.", "tokens": ["Vor", "Freu\u00b7den", ",", "da\u00df", "sie", "gr\u00fc\u00b7nen", "soll", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zwar decken Schleier zartgewoben", "tokens": ["Zwar", "de\u00b7cken", "Schlei\u00b7er", "zart\u00b7ge\u00b7wo\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVINF", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Himmels Angesicht noch ganz,", "tokens": ["Des", "Him\u00b7mels", "An\u00b7ge\u00b7sicht", "noch", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch rinnt durch ihr Gespinst von oben", "tokens": ["Doch", "rinnt", "durch", "ihr", "Ge\u00b7spinst", "von", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verhei\u00dfungsvoll ein wei\u00dfer Glanz.", "tokens": ["Ver\u00b7hei\u00b7\u00dfungs\u00b7voll", "ein", "wei\u00b7\u00dfer", "Glanz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er gleicht dem r\u00e4tsels\u00fc\u00dfen Schimmer,", "tokens": ["Er", "gleicht", "dem", "r\u00e4t\u00b7sel\u00b7s\u00fc\u00b7\u00dfen", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der um des M\u00e4dchens Z\u00fcge schwebt,", "tokens": ["Der", "um", "des", "M\u00e4d\u00b7chens", "Z\u00fc\u00b7ge", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das sich geliebt f\u00fchlt, doch noch immer", "tokens": ["Das", "sich", "ge\u00b7liebt", "f\u00fchlt", ",", "doch", "noch", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "PRF", "VVPP", "VVFIN", "$,", "ADV", "ADV", "ADV"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Ihr Gl\u00fcck sich zu bekennen bebt.", "tokens": ["Ihr", "Gl\u00fcck", "sich", "zu", "be\u00b7ken\u00b7nen", "bebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}