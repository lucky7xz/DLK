{"textgrid.poem.52446": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Psyche seufzt, in tiefer Kerkerhalle,", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Psyche seufzt, in tiefer Kerkerhalle,", "tokens": ["Psy\u00b7che", "seufzt", ",", "in", "tie\u00b7fer", "Ker\u00b7ker\u00b7hal\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nach Erl\u00f6sung; ach! sie forscht nach Licht:", "tokens": ["Nach", "Er\u00b7l\u00f6\u00b7sung", ";", "ach", "!", "sie", "forscht", "nach", "Licht", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "XY", "$.", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Bangt und hofft, und lauscht bei jedem Schalle,", "tokens": ["Bangt", "und", "hofft", ",", "und", "lauscht", "bei", "je\u00b7dem", "Schal\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$,", "KON", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ob das Schicksal ihre Riegel bricht.", "tokens": ["Ob", "das", "Schick\u00b7sal", "ih\u00b7re", "Rie\u00b7gel", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Psyches \u00c4therfl\u00fcgel sind gebunden;", "tokens": ["Psy\u00b7ches", "\u00c4\u00b7ther\u00b7fl\u00fc\u00b7gel", "sind", "ge\u00b7bun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Doch voll Mutes, wenn sie leise st\u00f6hnt,", "tokens": ["Doch", "voll", "Mu\u00b7tes", ",", "wenn", "sie", "lei\u00b7se", "st\u00f6hnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wei\u00df sie: Nur in schw\u00fclen Pr\u00fcfungsstunden", "tokens": ["Wei\u00df", "sie", ":", "Nur", "in", "schw\u00fc\u00b7len", "Pr\u00fc\u00b7fungs\u00b7stun\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Spro\u00dft die Palme, die den Sieger kr\u00f6nt;", "tokens": ["Spro\u00dft", "die", "Pal\u00b7me", ",", "die", "den", "Sie\u00b7ger", "kr\u00f6nt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Wei\u00df, da\u00df Dorngestrippe Rosen tragen,", "tokens": ["Wei\u00df", ",", "da\u00df", "Dorn\u00b7ge\u00b7strip\u00b7pe", "Ro\u00b7sen", "tra\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "NE", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Blumengold entkeimt der \u00f6den Gruft;", "tokens": ["Blu\u00b7men\u00b7gold", "ent\u00b7keimt", "der", "\u00f6\u00b7den", "Gruft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ihren Kranz erringt sie durch Entsagen,", "tokens": ["Ih\u00b7ren", "Kranz", "er\u00b7ringt", "sie", "durch", "Ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ihre Kr\u00e4fte st\u00e4hlt die herbe Luft.", "tokens": ["Ih\u00b7re", "Kr\u00e4f\u00b7te", "st\u00e4hlt", "die", "her\u00b7be", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Ihre Freuden kauft sie durch Entbehren,", "tokens": ["Ih\u00b7re", "Freu\u00b7den", "kauft", "sie", "durch", "Ent\u00b7beh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Durch verl\u00e4ngter Sehnsucht Wehmutstraum;", "tokens": ["Durch", "ver\u00b7l\u00e4ng\u00b7ter", "Sehn\u00b7sucht", "Weh\u00b7muts\u00b7traum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df nicht Strahlen ihr den Schlummer st\u00f6ren,", "tokens": ["Da\u00df", "nicht", "Strah\u00b7len", "ihr", "den", "Schlum\u00b7mer", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "NN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "D\u00e4mmern Schatten um des Lebens Baum.", "tokens": ["D\u00e4m\u00b7mern", "Schat\u00b7ten", "um", "des", "Le\u00b7bens", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Psyches Klag' ist Lispel einer Fl\u00f6te", "tokens": ["Psy\u00b7ches", "Klag'", "ist", "Lis\u00b7pel", "ei\u00b7ner", "Fl\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "NE", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aus dem mondbegl\u00e4nzten Weidenstrauch;", "tokens": ["Aus", "dem", "mond\u00b7be\u00b7gl\u00e4nz\u00b7ten", "Wei\u00b7den\u00b7strauch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ihre Z\u00e4hren Tau der Morgenr\u00f6te;", "tokens": ["Ih\u00b7re", "Z\u00e4h\u00b7ren", "Tau", "der", "Mor\u00b7gen\u00b7r\u00f6\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ihre Seufzer Nachtviolenhauch.", "tokens": ["Ih\u00b7re", "Seuf\u00b7zer", "Nacht\u00b7vi\u00b7o\u00b7len\u00b7hauch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Bei Cypressen spro\u00dften ihre Myrten;", "tokens": ["Bei", "Cyp\u00b7res\u00b7sen", "spro\u00df\u00b7ten", "ih\u00b7re", "Myr\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Weil sie viel geduldet, liebt sie viel.", "tokens": ["Weil", "sie", "viel", "ge\u00b7dul\u00b7det", ",", "liebt", "sie", "viel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Liebe f\u00fchrt nur durch der Trennung Syrten", "tokens": ["Lie\u00b7be", "f\u00fchrt", "nur", "durch", "der", "Tren\u00b7nung", "Syr\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Zu des Wiederfindens Wonneziel.", "tokens": ["Zu", "des", "Wie\u00b7der\u00b7fin\u00b7dens", "Won\u00b7ne\u00b7ziel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Dulden kann sie; B\u00fcrden mutig tragen;", "tokens": ["Dul\u00b7den", "kann", "sie", ";", "B\u00fcr\u00b7den", "mu\u00b7tig", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$.", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Stumm sich beugen vor des Schicksals Schlu\u00df;", "tokens": ["Stumm", "sich", "beu\u00b7gen", "vor", "des", "Schick\u00b7sals", "Schlu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "VVINF", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ihre Wonn' ist in gela\u00dfnen Klagen,", "tokens": ["Ih\u00b7re", "Wonn'", "ist", "in", "ge\u00b7la\u00df\u00b7nen", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und ihr Labsal des Gef\u00fchls Ergu\u00df.", "tokens": ["Und", "ihr", "Lab\u00b7sal", "des", "Ge\u00b7f\u00fchls", "Er\u00b7gu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Ach! das Vorgef\u00fchl in Finsternissen,", "tokens": ["Ach", "!", "das", "Vor\u00b7ge\u00b7f\u00fchl", "in", "Fins\u00b7ter\u00b7nis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Das zum Aufflug ihre Schwingen str\u00e4ubt,", "tokens": ["Das", "zum", "Auf\u00b7flug", "ih\u00b7re", "Schwin\u00b7gen", "str\u00e4ubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ist nur Ahndung; St\u00fcckwerk all' ihr Wissen;", "tokens": ["Ist", "nur", "Ahn\u00b7dung", ";", "St\u00fcck\u00b7werk", "all'", "ihr", "Wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "$.", "NN", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ihre Wahrheit, was sie redlich gl\u00e4ubt.", "tokens": ["Ih\u00b7re", "Wahr\u00b7heit", ",", "was", "sie", "red\u00b7lich", "gl\u00e4ubt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Dunkel birgt das Ziel von Psyches Sendung;", "tokens": ["Dun\u00b7kel", "birgt", "das", "Ziel", "von", "Psy\u00b7ches", "Sen\u00b7dung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und ein Blick, der oft in Thr\u00e4nen blinkt,", "tokens": ["Und", "ein", "Blick", ",", "der", "oft", "in", "Thr\u00e4\u00b7nen", "blinkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Reicht nicht bis zum Gipfel der Vollendung,", "tokens": ["Reicht", "nicht", "bis", "zum", "Gip\u00b7fel", "der", "Vol\u00b7len\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wo der T\u00e4uschung Nebelschleier sinkt.", "tokens": ["Wo", "der", "T\u00e4u\u00b7schung", "Ne\u00b7bel\u00b7schlei\u00b7er", "sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}