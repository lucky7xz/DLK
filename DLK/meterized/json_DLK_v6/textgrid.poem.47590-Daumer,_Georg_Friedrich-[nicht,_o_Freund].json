{"textgrid.poem.47590": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[nicht, o Freund]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht, o Freund,", "tokens": ["Nicht", ",", "o", "Freund", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "FM", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Heiligen des Bechers einen solchen Blick!", "tokens": ["Hei\u00b7li\u00b7gen", "des", "Be\u00b7chers", "ei\u00b7nen", "sol\u00b7chen", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Einen bessern!", "tokens": ["Ei\u00b7nen", "bes\u00b7sern", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADJA", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Einen Schritt", "tokens": ["Ei\u00b7nen", "Schritt"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Und noch einen der Taberne n\u00e4her r\u00fcck',", "tokens": ["Und", "noch", "ei\u00b7nen", "der", "Ta\u00b7ber\u00b7ne", "n\u00e4\u00b7her", "r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Einen bessern!", "tokens": ["Ei\u00b7nen", "bes\u00b7sern", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADJA", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Achte sie,", "tokens": ["Ach\u00b7te", "sie", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Deine Tugend, deine selbstisch eitele,", "tokens": ["Dei\u00b7ne", "Tu\u00b7gend", ",", "dei\u00b7ne", "selbs\u00b7tisch", "ei\u00b7te\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Keinen Deut werth!", "tokens": ["Kei\u00b7nen", "Deut", "werth", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "$."], "meter": "+---", "measure": "dactylic.init"}, "line.4": {"text": "Einen Schlag", "tokens": ["Ei\u00b7nen", "Schlag"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Gieb ihr und noch einen in ihr steif Genick,", "tokens": ["Gieb", "ihr", "und", "noch", "ei\u00b7nen", "in", "ihr", "steif", "Ge\u00b7nick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KON", "ADV", "ART", "APPR", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Einen bessern! \u2013", "tokens": ["Ei\u00b7nen", "bes\u00b7sern", "!", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "$.", "$("], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "B\u00e4rtiger,", "tokens": ["B\u00e4r\u00b7ti\u00b7ger", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "Ruhmgekr\u00f6nter Forscher alles Heimlichen,", "tokens": ["Ruhm\u00b7ge\u00b7kr\u00f6n\u00b7ter", "For\u00b7scher", "al\u00b7les", "Heim\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Gro\u00dfer Denker!", "tokens": ["Gro\u00b7\u00dfer", "Den\u00b7ker", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Uns der Nacht", "tokens": ["Uns", "der", "Nacht"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Dieses Seins durch einen neuen Fund entr\u00fcck',", "tokens": ["Die\u00b7ses", "Seins", "durch", "ei\u00b7nen", "neu\u00b7en", "Fund", "ent\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Einen bessern! \u2013", "tokens": ["Ei\u00b7nen", "bes\u00b7sern", "!", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "$.", "$("], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Dieser Arm", "tokens": ["Die\u00b7ser", "Arm"], "token_info": ["word", "word"], "pos": ["PDAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Hebe den Pokal und dieser schmiege sich", "tokens": ["He\u00b7be", "den", "Po\u00b7kal", "und", "die\u00b7ser", "schmie\u00b7ge", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "KON", "PDS", "VVFIN", "PRF"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Um ein Liebchen!", "tokens": ["Um", "ein", "Lieb\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Mein Gem\u00fcth,", "tokens": ["Mein", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Wenn Du kannst, durch einen andern Rath begl\u00fcck',", "tokens": ["Wenn", "Du", "kannst", ",", "durch", "ei\u00b7nen", "an\u00b7dern", "Rath", "be\u00b7gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "$,", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Einen bessern! \u2013", "tokens": ["Ei\u00b7nen", "bes\u00b7sern", "!", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "$.", "$("], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Wenn du dich", "tokens": ["Wenn", "du", "dich"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "PRF"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "H\u00e4ngen willst, so greife nicht nach schm\u00e4hlichem,", "tokens": ["H\u00e4n\u00b7gen", "willst", ",", "so", "grei\u00b7fe", "nicht", "nach", "schm\u00e4h\u00b7li\u00b7chem", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "ADV", "VVFIN", "PTKNEG", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.3": {"text": "Rohem Stricke;", "tokens": ["Ro\u00b7hem", "Stri\u00b7cke", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Winde dir", "tokens": ["Win\u00b7de", "dir"], "token_info": ["word", "word"], "pos": ["NN", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Aus gelocktem Ambra-Haar den H\u00e4nge-Strick", "tokens": ["Aus", "ge\u00b7lock\u00b7tem", "Am\u00b7bra\u00b7Haar", "den", "H\u00e4n\u00b7ge\u00b7Strick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Einen bessern! \u2013", "tokens": ["Ei\u00b7nen", "bes\u00b7sern", "!", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "$.", "$("], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Krankt Hafis,", "tokens": ["Krankt", "Ha\u00b7fis", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NE", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Mit gelahrtem, graubehaartem Medicus", "tokens": ["Mit", "ge\u00b7lahr\u00b7tem", ",", "grau\u00b7be\u00b7haar\u00b7tem", "Me\u00b7di\u00b7cus"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "O verschon' ihn;", "tokens": ["O", "ver\u00b7schon'", "ihn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Ihm sogleich", "tokens": ["Ihm", "sog\u00b7leich"], "token_info": ["word", "word"], "pos": ["PPER", "ADV"], "meter": "+--", "measure": "dactylic.init"}, "line.5": {"text": "Einen blutjung rosenwangigen Arzt beschick',", "tokens": ["Ei\u00b7nen", "blut\u00b7jung", "ro\u00b7sen\u00b7wan\u00b7gi\u00b7gen", "Arzt", "be\u00b7schick'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Einen bessern!", "tokens": ["Ei\u00b7nen", "bes\u00b7sern", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADJA", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}