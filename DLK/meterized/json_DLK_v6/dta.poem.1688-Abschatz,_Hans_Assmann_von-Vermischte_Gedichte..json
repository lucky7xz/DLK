{"dta.poem.1688": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vermischte  \n Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.85", "nl:0.14"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Fremde Kleider/ falsche Haare/", "tokens": ["Frem\u00b7de", "Klei\u00b7der", "/", "fal\u00b7sche", "Haa\u00b7re", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$(", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Falsche Treu/ verf\u00e4lschter Wein/", "tokens": ["Fal\u00b7sche", "Treu", "/", "ver\u00b7f\u00e4lschter", "Wein", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADJA", "NN", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Glatte Worte/ falscher Schein/", "tokens": ["Glat\u00b7te", "Wor\u00b7te", "/", "fal\u00b7scher", "Schein", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind anizt die beste Waare.", "tokens": ["Sind", "a\u00b7nizt", "die", "bes\u00b7te", "Waa\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer will sich bey solchen Tagen", "tokens": ["Wer", "will", "sich", "bey", "sol\u00b7chen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PRF", "APPR", "PIAT", "NN"], "meter": "-+----+-", "measure": "dactylic.init"}, "line.6": {"text": "Mit der albern Warheit plagen?", "tokens": ["Mit", "der", "al\u00b7bern", "War\u00b7heit", "pla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Leug die gr\u00f6sten Tummel-Pl\u00e4tze/", "tokens": ["Leug", "die", "gr\u00f6s\u00b7ten", "Tum\u00b7mel\u00b7Pl\u00e4t\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gib die Wort um gutten Kauff/", "tokens": ["Gib", "die", "Wort", "um", "gut\u00b7ten", "Kauff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Schneid von Ost und Westen auff/", "tokens": ["Schneid", "von", "Ost", "und", "Wes\u00b7ten", "auff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "KON", "NN", "APPR", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Darum strafft dich kein Gesetze!", "tokens": ["Da\u00b7rum", "strafft", "dich", "kein", "Ge\u00b7set\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wiltu viel die Warheit geigen/", "tokens": ["Wil\u00b7tu", "viel", "die", "War\u00b7heit", "gei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wird man dir die Feigen zeigen.", "tokens": ["Wird", "man", "dir", "die", "Fei\u00b7gen", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}