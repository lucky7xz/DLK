{"textgrid.poem.55932": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Der Panther", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sein Blick ist vom Vor\u00fcbergehn der St\u00e4be", "tokens": ["Sein", "Blick", "ist", "vom", "Vor\u00b7\u00fc\u00b7ber\u00b7gehn", "der", "St\u00e4\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "so m\u00fcd geworden, da\u00df er nichts mehr h\u00e4lt.", "tokens": ["so", "m\u00fcd", "ge\u00b7wor\u00b7den", ",", "da\u00df", "er", "nichts", "mehr", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAPP", "$,", "KOUS", "PPER", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihm ist, als ob es tausend St\u00e4be g\u00e4be", "tokens": ["Ihm", "ist", ",", "als", "ob", "es", "tau\u00b7send", "St\u00e4\u00b7be", "g\u00e4\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "PPER", "CARD", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und hinter tausend St\u00e4ben keine Welt.", "tokens": ["und", "hin\u00b7ter", "tau\u00b7send", "St\u00e4\u00b7ben", "kei\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "CARD", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der weiche Gang geschmeidig starker Schritte,", "tokens": ["Der", "wei\u00b7che", "Gang", "ge\u00b7schmei\u00b7dig", "star\u00b7ker", "Schrit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "der sich im allerkleinsten Kreise dreht,", "tokens": ["der", "sich", "im", "al\u00b7ler\u00b7kleins\u00b7ten", "Krei\u00b7se", "dreht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "ist wie ein Tanz von Kraft um eine Mitte,", "tokens": ["ist", "wie", "ein", "Tanz", "von", "Kraft", "um", "ei\u00b7ne", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "in der bet\u00e4ubt ein gro\u00dfer Wille steht.", "tokens": ["in", "der", "be\u00b7t\u00e4ubt", "ein", "gro\u00b7\u00dfer", "Wil\u00b7le", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nur manchmal schiebt der Vorhang der Pupille", "tokens": ["Nur", "manch\u00b7mal", "schiebt", "der", "Vor\u00b7hang", "der", "Pu\u00b7pil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "sich lautlos auf \u2013. Dann geht ein Bild hinein,", "tokens": ["sich", "laut\u00b7los", "auf", "\u2013", ".", "Dann", "geht", "ein", "Bild", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "$(", "$.", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "geht durch der Glieder angespannte Stille \u2013", "tokens": ["geht", "durch", "der", "Glie\u00b7der", "an\u00b7ge\u00b7spann\u00b7te", "Stil\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und h\u00f6rt im Herzen auf zu sein.", "tokens": ["und", "h\u00f6rt", "im", "Her\u00b7zen", "auf", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPR", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}