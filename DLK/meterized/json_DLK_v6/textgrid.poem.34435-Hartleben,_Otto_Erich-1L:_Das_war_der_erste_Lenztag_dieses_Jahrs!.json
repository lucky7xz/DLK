{"textgrid.poem.34435": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das war der erste Lenztag dieses Jahrs!", "genre": "verse", "period": "N.A.", "pub_year": 1886, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das war der erste Lenztag dieses Jahrs!", "tokens": ["Das", "war", "der", "ers\u00b7te", "Lenz\u00b7tag", "die\u00b7ses", "Jahrs", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Schnee zerschmolz, die Sonne leuchtete", "tokens": ["Der", "Schnee", "zer\u00b7schmolz", ",", "die", "Son\u00b7ne", "leuch\u00b7te\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "ins Herz der Menschen wie zum ersten mal ...", "tokens": ["ins", "Herz", "der", "Men\u00b7schen", "wie", "zum", "ers\u00b7ten", "mal", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "KOKOM", "APPRART", "ADJA", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da sind auch wir zur Stadt hinausgegangen.", "tokens": ["Da", "sind", "auch", "wir", "zur", "Stadt", "hin\u00b7aus\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Uns wunderte der Erde Wandlung nicht,", "tokens": ["Uns", "wun\u00b7der\u00b7te", "der", "Er\u00b7de", "Wand\u00b7lung", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "wir wussten, dass es Fr\u00fchling werden w\u00fcrde:", "tokens": ["wir", "wuss\u00b7ten", ",", "dass", "es", "Fr\u00fch\u00b7ling", "wer\u00b7den", "w\u00fcr\u00b7de", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "NN", "VAINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Fr\u00fchling! Wars denn nicht lang schon so?", "tokens": ["Fr\u00fch\u00b7ling", "!", "Wars", "denn", "nicht", "lang", "schon", "so", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VAFIN", "ADV", "PTKNEG", "ADJD", "ADV", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Der Wald", "tokens": ["Der", "Wald"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "mit seinen schwarzen, kahlen Winter\u00e4sten,", "tokens": ["mit", "sei\u00b7nen", "schwar\u00b7zen", ",", "kah\u00b7len", "Win\u00b7ter\u00b7\u00e4s\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "die nassen Wege und das faule Laub \u2013", "tokens": ["die", "nas\u00b7sen", "We\u00b7ge", "und", "das", "fau\u00b7le", "Laub", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "traurig und h\u00e4sslich war es, doch die Sonne,", "tokens": ["trau\u00b7rig", "und", "h\u00e4ss\u00b7lich", "war", "es", ",", "doch", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "PPER", "$,", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "die Zauberin, mit ihren warmen Armen", "tokens": ["die", "Zau\u00b7be\u00b7rin", ",", "mit", "ih\u00b7ren", "war\u00b7men", "Ar\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "umfasste sie die spr\u00f6de Wintererde,", "tokens": ["um\u00b7fass\u00b7te", "sie", "die", "spr\u00f6\u00b7de", "Win\u00b7ter\u00b7er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "bis dass vor Gl\u00fcck sie lachte!", "tokens": ["bis", "dass", "vor", "Gl\u00fcck", "sie", "lach\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Weisst du, Ellen:", "tokens": ["Weisst", "du", ",", "El\u00b7len", ":"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "den Tag kann ich nun niemals mehr vergessen!", "tokens": ["den", "Tag", "kann", "ich", "nun", "nie\u00b7mals", "mehr", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich will nicht sagen, was geschehen, was", "tokens": ["Ich", "will", "nicht", "sa\u00b7gen", ",", "was", "ge\u00b7sche\u00b7hen", ",", "was"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "PWS", "VVPP", "$,", "PWS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "dein Mund mir gab und sagte. Wenn du einst", "tokens": ["dein", "Mund", "mir", "gab", "und", "sag\u00b7te", ".", "Wenn", "du", "einst"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "KON", "VVFIN", "$.", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "dies Blatt, schon gelb, in deiner Mappe findest,", "tokens": ["dies", "Blatt", ",", "schon", "gelb", ",", "in", "dei\u00b7ner", "Map\u00b7pe", "fin\u00b7dest", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "ADV", "ADJD", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "auch du wirst jenes Tages dich erinnern.", "tokens": ["auch", "du", "wirst", "je\u00b7nes", "Ta\u00b7ges", "dich", "e\u00b7rin\u00b7nern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PDAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Du wirst gedenken, wie die Sonne sank,", "tokens": ["Du", "wirst", "ge\u00b7den\u00b7ken", ",", "wie", "die", "Son\u00b7ne", "sank", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVINF", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und es uns doch immer lichter wurde \u2013", "tokens": ["und", "es", "uns", "doch", "im\u00b7mer", "lich\u00b7ter", "wur\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "du wirst gedenken, wie die Lippe schwieg,", "tokens": ["du", "wirst", "ge\u00b7den\u00b7ken", ",", "wie", "die", "Lip\u00b7pe", "schwieg", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVINF", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und was das Herz, das Herz doch jubelnd sagte", "tokens": ["und", "was", "das", "Herz", ",", "das", "Herz", "doch", "ju\u00b7belnd", "sag\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "NN", "$,", "ART", "NN", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du wirst gedenken!", "tokens": ["Du", "wirst", "ge\u00b7den\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Wenn ich einst mit dir", "tokens": ["Wenn", "ich", "einst", "mit", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "den Pfad durch diese Welt gefunden habe,", "tokens": ["den", "Pfad", "durch", "die\u00b7se", "Welt", "ge\u00b7fun\u00b7den", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "dann wollen wir vereint dem Wandrer gleich,", "tokens": ["dann", "wol\u00b7len", "wir", "ver\u00b7eint", "dem", "Wand\u00b7rer", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "der einen Berg hinangeht, oft sich wendet", "tokens": ["der", "ei\u00b7nen", "Berg", "hin\u00b7an\u00b7geht", ",", "oft", "sich", "wen\u00b7det"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ART", "NN", "VVFIN", "$,", "ADV", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "und einen Blick ins sch\u00f6ne Thal hinabwirft,", "tokens": ["und", "ei\u00b7nen", "Blick", "ins", "sch\u00f6\u00b7ne", "Thal", "hin\u00b7ab\u00b7wirft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "dann wollen oft wir, Arm in Arm verschr\u00e4nkt,", "tokens": ["dann", "wol\u00b7len", "oft", "wir", ",", "Arm", "in", "Arm", "ver\u00b7schr\u00e4nkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPER", "$,", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "zur\u00fcckschaun auf die Sch\u00f6nheit jener Stunde.", "tokens": ["zu\u00b7r\u00fcck\u00b7schaun", "auf", "die", "Sch\u00f6n\u00b7heit", "je\u00b7ner", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}