{"textgrid.poem.31952": {"metadata": {"author": {"name": "Angelus Silesius", "birth": "N.A.", "death": "N.A."}, "title": "42. An den S\u00fcnder", "genre": "verse", "period": "N.A.", "pub_year": 1650, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wach auf, du toter Christ, schau, unser Pelikan", "tokens": ["Wach", "auf", ",", "du", "to\u00b7ter", "Christ", ",", "schau", ",", "un\u00b7ser", "Pe\u00b7li\u00b7kan"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,", "PTKVZ", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sprengt dich mit seinem Blut und Herzenswasser an.", "tokens": ["Sprengt", "dich", "mit", "sei\u00b7nem", "Blut", "und", "Her\u00b7zens\u00b7was\u00b7ser", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Empf\u00e4ngst du dieses recht mit aufgetanem Mund,", "tokens": ["Emp\u00b7f\u00e4ngst", "du", "die\u00b7ses", "recht", "mit", "auf\u00b7ge\u00b7ta\u00b7nem", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So bist du augenblicks lebendig und gesund. ", "tokens": ["So", "bist", "du", "au\u00b7gen\u00b7blicks", "le\u00b7ben\u00b7dig", "und", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}