{"dta.poem.2974": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr F\u00fcrsten, die ihr euch der Erde G\u00f6tter nennt,", "tokens": ["Ihr", "F\u00fcrs\u00b7ten", ",", "die", "ihr", "euch", "der", "Er\u00b7de", "G\u00f6t\u00b7ter", "nennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "PRF", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was seid ihr, wenn ihr nicht der Menschheit W\u00fcrd' erkennt?", "tokens": ["Was", "seid", "ihr", ",", "wenn", "ihr", "nicht", "der", "Menschheit", "W\u00fcrd'", "er\u00b7kennt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein blindes Ohngef\u00e4hr, gleich rauher St\u00fcrme W\u00fcthen.", "tokens": ["Ein", "blin\u00b7des", "Ohn\u00b7ge\u00b7f\u00e4hr", ",", "gleich", "rau\u00b7her", "St\u00fcr\u00b7me", "W\u00fct\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weh den in eure Hand gegebnen zarten Bl\u00fcten!", "tokens": ["Weh", "den", "in", "eu\u00b7re", "Hand", "ge\u00b7geb\u00b7nen", "zar\u00b7ten", "Bl\u00fc\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["