{"textgrid.poem.28733": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00e4u sind S\u00e4u, so weit sie leben,", "tokens": ["S\u00e4u", "sind", "S\u00e4u", ",", "so", "weit", "sie", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NE", "$,", "ADV", "ADJD", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Machen Koth und fressen Koth,", "tokens": ["Ma\u00b7chen", "Koth", "und", "fres\u00b7sen", "Koth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollen erst, nach dem sie todt,", "tokens": ["Wol\u00b7len", "erst", ",", "nach", "dem", "sie", "todt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "APPR", "PRELS", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gute W\u00fcrst und Braten geben.", "tokens": ["Gu\u00b7te", "W\u00fcrst", "und", "Bra\u00b7ten", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}