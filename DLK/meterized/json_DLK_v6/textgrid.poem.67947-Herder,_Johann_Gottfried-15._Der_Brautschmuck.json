{"textgrid.poem.67947": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "15. Der Brautschmuck", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wollt meine Liebe lieben mich,", "tokens": ["Wollt", "mei\u00b7ne", "Lie\u00b7be", "lie\u00b7ben", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und treu und hold mir seyn;", "tokens": ["Und", "treu", "und", "hold", "mir", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "PPER", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein sch\u00f6ner Brautschmuck sollte sie", "tokens": ["Ein", "sch\u00f6\u00b7ner", "Brautsc\u00b7hmuck", "soll\u00b7te", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durchs ganze Leben freun.", "tokens": ["Durchs", "gan\u00b7ze", "Le\u00b7ben", "freun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Ehre sollt' ihr H\u00fctchen seyn,", "tokens": ["Die", "Eh\u00b7re", "sollt'", "ihr", "H\u00fct\u00b7chen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das rings ihr Haupt bedeckt,", "tokens": ["Das", "rings", "ihr", "Haupt", "be\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Umfasset mit der Vorsicht Band,", "tokens": ["Um\u00b7fas\u00b7set", "mit", "der", "Vor\u00b7sicht", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Freiheit sch\u00f6n besteckt.", "tokens": ["Mit", "Frei\u00b7heit", "sch\u00f6n", "be\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Leinwand, die den zarten Bau", "tokens": ["Die", "Lein\u00b7wand", ",", "die", "den", "zar\u00b7ten", "Bau"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Glieder rings umschlie\u00dft,", "tokens": ["Der", "Glie\u00b7der", "rings", "um\u00b7schlie\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sey Unschuld, wie sie um die Brust", "tokens": ["Sey", "Un\u00b7schuld", ",", "wie", "sie", "um", "die", "Brust"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "$,", "PWAV", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der keuschen Taube flie\u00dft.", "tokens": ["Der", "keu\u00b7schen", "Tau\u00b7be", "flie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ihr W\u00e4mschen schlanke M\u00e4\u00dfigkeit", "tokens": ["Ihr", "W\u00e4m\u00b7schen", "schlan\u00b7ke", "M\u00e4\u00b7\u00dfig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Zucht und feste Treu,", "tokens": ["Und", "Zucht", "und", "fes\u00b7te", "Treu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In dem der frischen Glieder Wuchs", "tokens": ["In", "dem", "der", "fri\u00b7schen", "Glie\u00b7der", "Wuchs"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein sanfter Palmbaum sey.", "tokens": ["Ein", "sanf\u00b7ter", "Palm\u00b7baum", "sey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ihr R\u00f6ckchen sey von Artigkeit", "tokens": ["Ihr", "R\u00f6ck\u00b7chen", "sey", "von", "Ar\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und W\u00fcrde sch\u00f6n gewebt:", "tokens": ["Und", "W\u00fcr\u00b7de", "sch\u00f6n", "ge\u00b7webt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo Anstand und Bescheidenheit", "tokens": ["Wo", "An\u00b7stand", "und", "Be\u00b7schei\u00b7den\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In jeder Welle schwebt.", "tokens": ["In", "je\u00b7der", "Wel\u00b7le", "schwebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Best\u00e4ndigkeit ihr G\u00fcrtel sey,", "tokens": ["Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "ihr", "G\u00fcr\u00b7tel", "sey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tagt\u00e4glich neu und sch\u00f6n:", "tokens": ["Tag\u00b7t\u00e4g\u00b7lich", "neu", "und", "sch\u00f6n", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr M\u00e4ntelchen Dem\u00fcthigkeit,", "tokens": ["Ihr", "M\u00e4n\u00b7tel\u00b7chen", "De\u00b7m\u00fct\u00b7hig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Luft zu widerstehn.", "tokens": ["Der", "Luft", "zu", "wi\u00b7der\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ihr Halsband sey ein Perlenschmuck,", "tokens": ["Ihr", "Hals\u00b7band", "sey", "ein", "Per\u00b7len\u00b7schmuck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Herzen selbst bewu\u00dft;", "tokens": ["Dem", "Her\u00b7zen", "selbst", "be\u00b7wu\u00dft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Liebe sch\u00f6nste Rose bl\u00fch", "tokens": ["Der", "Lie\u00b7be", "sch\u00f6ns\u00b7te", "Ro\u00b7se", "bl\u00fch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf ihrer Mutterbrust.", "tokens": ["Auf", "ih\u00b7rer", "Mut\u00b7ter\u00b7brust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Umgeben mit der Hoffnung Gr\u00fcn,", "tokens": ["Um\u00b7ge\u00b7ben", "mit", "der", "Hoff\u00b7nung", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und stiller Veilchen Pracht,", "tokens": ["Und", "stil\u00b7ler", "Veil\u00b7chen", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo mir ein klein Vergi\u00df mein nicht", "tokens": ["Wo", "mir", "ein", "klein", "Ver\u00b7gi\u00df", "mein", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJD", "NN", "PPOSAT", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Mayenbl\u00fcmchen lacht.", "tokens": ["Aus", "Ma\u00b7yen\u00b7bl\u00fcm\u00b7chen", "lacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und unter ihnen ziehe sanft", "tokens": ["Und", "un\u00b7ter", "ih\u00b7nen", "zie\u00b7he", "sanft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Schleife Band sich zu;", "tokens": ["Der", "Schlei\u00b7fe", "Band", "sich", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und berg' in ihren Busen zart", "tokens": ["Und", "ber\u00b7g'", "in", "ih\u00b7ren", "Bu\u00b7sen", "zart"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Gelassenheit und Ruh.", "tokens": ["Ge\u00b7las\u00b7sen\u00b7heit", "und", "Ruh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Des Fleisses und der G\u00fcte Netz", "tokens": ["Des", "Fleis\u00b7ses", "und", "der", "G\u00fc\u00b7te", "Netz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Umwebe ihre Hand;", "tokens": ["Um\u00b7we\u00b7be", "ih\u00b7re", "Hand", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der falschen Nadel sey ein Helm", "tokens": ["Der", "fal\u00b7schen", "Na\u00b7del", "sey", "ein", "Helm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Golde Widerstand.", "tokens": ["Von", "Gol\u00b7de", "Wi\u00b7der\u00b7stand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "So binde sie mit Huld und Schaam", "tokens": ["So", "bin\u00b7de", "sie", "mit", "Huld", "und", "Schaam"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Kniee Brautband sich,", "tokens": ["Der", "Kni\u00b7ee", "Braut\u00b7band", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wandle, wie ein Engel sch\u00f6n,", "tokens": ["Und", "wand\u00b7le", ",", "wie", "ein", "En\u00b7gel", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Begl\u00fcckend sich und mich.", "tokens": ["Be\u00b7gl\u00fc\u00b7ckend", "sich", "und", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PRF", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}