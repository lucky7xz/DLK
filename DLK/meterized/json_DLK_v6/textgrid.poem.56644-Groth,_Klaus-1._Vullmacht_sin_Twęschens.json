{"textgrid.poem.56644": {"metadata": {"author": {"name": "Groth, Klaus", "birth": "N.A.", "death": "N.A."}, "title": "1. Vullmacht sin Tw\u0119schens", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wat gluddert in Blomhof un lacht achtern Tun?", "tokens": ["Wat", "glud\u00b7dert", "in", "Blom\u00b7hof", "un", "lacht", "ach\u00b7tern", "Tun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "FM", "VVFIN", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "De Vullmacht sin Tw\u0119schens, de Witt un de Brun.", "tokens": ["De", "Vull\u00b7macht", "sin", "Tw\u0119\u00b7schens", ",", "de", "Witt", "un", "de", "Brun", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "$,", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "De Vagt un de Schriwer gungn \u0119ben v\u0153rbi,", "tokens": ["De", "Vagt", "un", "de", "Schri\u00b7wer", "gungn", "\u0119ben", "v\u0153r\u00b7bi", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Weer j\u00fcs as en Beert\u00fcnn mit H\u0153nken derbi.", "tokens": ["Weer", "j\u00fcs", "as", "en", "Beer\u00b7t\u00fcnn", "mit", "H\u0153n\u00b7ken", "der\u00b7bi", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}, "stanza.3": {"line.1": {"text": "Wa lach do de Brune un sch\u00fcttel de Haar:", "tokens": ["Wa", "lach", "do", "de", "Bru\u00b7ne", "un", "sch\u00fct\u00b7tel", "de", "Haar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Du kriggst mal de Krumme, schast sehn, noch vuntjahr!", "tokens": ["Du", "kriggst", "mal", "de", "Krum\u00b7me", ",", "schast", "sehn", ",", "noch", "vunt\u00b7jahr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NE", "NN", "$,", "ADV", "VVINF", "$,", "ADV", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Wa lach do de Witte un klapp inne Hann':", "tokens": ["Wa", "lach", "do", "de", "Wit\u00b7te", "un", "klapp", "in\u00b7ne", "Hann'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Du kriggst mal de Dicke, de Dicke ton Mann! \u2013", "tokens": ["Du", "kriggst", "mal", "de", "Di\u00b7cke", ",", "de", "Di\u00b7cke", "ton", "Mann", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NE", "NN", "$,", "FM", "FM", "FM", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Ik kik d\u0153r de Paten un heff mi bedacht:", "tokens": ["Ik", "kik", "d\u0153r", "de", "Pa\u00b7ten", "un", "heff", "mi", "be\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Wat much ik denn, Schriwer w\u0119n \u2013 oder de Vagt?", "tokens": ["Wat", "much", "ik", "denn", ",", "Schri\u00b7wer", "w\u0119n", "\u2013", "o\u00b7der", "de", "Vagt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "FM.la", "FM.la", "$(", "KON", "NE", "NE", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}}}}