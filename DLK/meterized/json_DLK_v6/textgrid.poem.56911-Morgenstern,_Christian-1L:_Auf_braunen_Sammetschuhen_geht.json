{"textgrid.poem.56911": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf braunen Sammetschuhen geht", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf braunen Sammetschuhen geht", "tokens": ["Auf", "brau\u00b7nen", "Sam\u00b7met\u00b7schu\u00b7hen", "geht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "der Abend durch das m\u00fcde Land,", "tokens": ["der", "A\u00b7bend", "durch", "das", "m\u00fc\u00b7de", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "sein weiter Mantel wallt und weht,", "tokens": ["sein", "wei\u00b7ter", "Man\u00b7tel", "wallt", "und", "weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und Schlummer f\u00e4llt von seiner Hand.", "tokens": ["und", "Schlum\u00b7mer", "f\u00e4llt", "von", "sei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mit stiller Fackel steckt er nun", "tokens": ["Mit", "stil\u00b7ler", "Fa\u00b7ckel", "steckt", "er", "nun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "der Sterne treue Kerzen an.", "tokens": ["der", "Ster\u00b7ne", "treu\u00b7e", "Ker\u00b7zen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sei ruhig, Herz! Das Dunkel kann", "tokens": ["Sei", "ru\u00b7hig", ",", "Herz", "!", "Das", "Dun\u00b7kel", "kann"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "$,", "NN", "$.", "ART", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "dir nun kein Leid mehr tun.", "tokens": ["dir", "nun", "kein", "Leid", "mehr", "tun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}