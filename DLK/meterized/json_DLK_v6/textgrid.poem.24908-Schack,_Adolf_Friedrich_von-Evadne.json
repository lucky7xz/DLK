tanza.16": {"line.1": {"text": "\u00bbihr wolltet mich h\u00fcten \u2013 nun bin ich frei!", "tokens": ["\u00bb", "ihr", "woll\u00b7tet", "mich", "h\u00fc\u00b7ten", "\u2013", "nun", "bin", "ich", "frei", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PRF", "VVINF", "$(", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Zu scheiden, die sich geliebt, die zwei,", "tokens": ["Zu", "schei\u00b7den", ",", "die", "sich", "ge\u00b7liebt", ",", "die", "zwei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "PRF", "VVPP", "$,", "PRELS", "CARD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "W\u00e4hnt nicht, es werd' euch gelingen!", "tokens": ["W\u00e4hnt", "nicht", ",", "es", "werd'", "euch", "ge\u00b7lin\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Du, dessen Blitz mir den Teuern geraubt,", "tokens": ["Du", ",", "des\u00b7sen", "Blitz", "mir", "den", "Teu\u00b7ern", "ge\u00b7raubt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ohnm\u00e4chtiger Donnerer, hast du geglaubt,", "tokens": ["Ohn\u00b7m\u00e4ch\u00b7ti\u00b7ger", "Don\u00b7ne\u00b7rer", ",", "hast", "du", "ge\u00b7glaubt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.