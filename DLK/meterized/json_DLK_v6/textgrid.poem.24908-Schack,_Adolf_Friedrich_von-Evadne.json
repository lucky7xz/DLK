{"textgrid.poem.24908": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Evadne", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Evadne trauert im \u00f6den Haus,", "tokens": ["E\u00b7vad\u00b7ne", "trau\u00b7ert", "im", "\u00f6\u00b7den", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Seit Kampflust ihren Verlobten hinaus", "tokens": ["Seit", "Kampf\u00b7lust", "ih\u00b7ren", "Ver\u00b7lob\u00b7ten", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PPOSAT", "NN", "APZR"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ins Feld vor Theben getrieben;", "tokens": ["Ins", "Feld", "vor", "The\u00b7ben", "ge\u00b7trie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da naht ihr ein Bote: \u00bbO Herrin, vernimm,", "tokens": ["Da", "naht", "ihr", "ein", "Bo\u00b7te", ":", "\u00bb", "O", "Her\u00b7rin", ",", "ver\u00b7nimm", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$.", "$(", "NE", "NN", "$,", "ADJD", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und z\u00fcrne mir nicht, wenn die Botschaft schlimm!", "tokens": ["Und", "z\u00fcr\u00b7ne", "mir", "nicht", ",", "wenn", "die", "Bot\u00b7schaft", "schlimm", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der G\u00f6tter Grimm", "tokens": ["Der", "G\u00f6t\u00b7ter", "Grimm"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Ruht schwer auf dem Heere der Sieben!", "tokens": ["Ruht", "schwer", "auf", "dem", "Hee\u00b7re", "der", "Sie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Herab von den Thoren von Teben flog", "tokens": ["Her\u00b7ab", "von", "den", "Tho\u00b7ren", "von", "Te\u00b7ben", "flog"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "NE", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Gescho\u00df auf Gescho\u00df auf das Kriegergewog;", "tokens": ["Ge\u00b7scho\u00df", "auf", "Ge\u00b7scho\u00df", "auf", "das", "Krie\u00b7ger\u00b7ge\u00b7wog", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Rings t\u00fcrmten sich Haufen Toter;", "tokens": ["Rings", "t\u00fcrm\u00b7ten", "sich", "Hau\u00b7fen", "To\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da klomm dein Kapaneus, allen zuvor,", "tokens": ["Da", "klomm", "dein", "Ka\u00b7pa\u00b7neus", ",", "al\u00b7len", "zu\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "PIS", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Inmitten des Kampfs am Elektrathor", "tokens": ["In\u00b7mit\u00b7ten", "des", "Kampfs", "am", "E\u00b7lekt\u00b7ra\u00b7thor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPRART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Zur Mauer empor,", "tokens": ["Zur", "Mau\u00b7er", "em\u00b7por", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Nicht achtend die Wut der B\u00f6oter.", "tokens": ["Nicht", "ach\u00b7tend", "die", "Wut", "der", "B\u00f6o\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und hoch auf der Zinne, von Speeren umsaust,", "tokens": ["Und", "hoch", "auf", "der", "Zin\u00b7ne", ",", "von", "Spee\u00b7ren", "um\u00b7saust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Rief er und ballte nach oben die Faust:", "tokens": ["Rief", "er", "und", "ball\u00b7te", "nach", "o\u00b7ben", "die", "Faust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "APPR", "ADV", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "\u203aall deine Gew\u00f6lke t\u00fcrme,", "tokens": ["\u203a", "all", "dei\u00b7ne", "Ge\u00b7w\u00f6l\u00b7ke", "t\u00fcr\u00b7me", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ja all deine Flammen herniedergeu\u00df,", "tokens": ["Ja", "all", "dei\u00b7ne", "Flam\u00b7men", "her\u00b7nie\u00b7der\u00b7geu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PIAT", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Doch wirst du nicht hindern den Kapaneus,", "tokens": ["Doch", "wirst", "du", "nicht", "hin\u00b7dern", "den", "Ka\u00b7pa\u00b7neus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "VVINF", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Ohnm\u00e4chtiger Zeus,", "tokens": ["Ohn\u00b7m\u00e4ch\u00b7ti\u00b7ger", "Zeus", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Da\u00df er dies Theben erst\u00fcrme!\u2039", "tokens": ["Da\u00df", "er", "dies", "The\u00b7ben", "er\u00b7st\u00fcr\u00b7me", "!", "\u2039"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PDS", "NN", "VVFIN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Er rief es, und schon aus den Wolken scholl,", "tokens": ["Er", "rief", "es", ",", "und", "schon", "aus", "den", "Wol\u00b7ken", "scholl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "ADV", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Den Himmel durchhallend, Donnergeroll;", "tokens": ["Den", "Him\u00b7mel", "durch\u00b7hal\u00b7lend", ",", "Don\u00b7ner\u00b7ge\u00b7roll", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Herab auf das Haupt des Stolzen", "tokens": ["Her\u00b7ab", "auf", "das", "Haupt", "des", "Stol\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Fuhr lohend Kronions Wetterstrahl;", "tokens": ["Fuhr", "lo\u00b7hend", "Kro\u00b7nions", "Wet\u00b7ter\u00b7strahl", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "NE", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Er taumelte r\u00fcckw\u00e4rts leichenfahl,", "tokens": ["Er", "tau\u00b7mel\u00b7te", "r\u00fcck\u00b7w\u00e4rts", "lei\u00b7chen\u00b7fahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sein Panzerstahl,", "tokens": ["Sein", "Pan\u00b7zer\u00b7stahl", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Sein Helm und sein Schild zerschmolzen.\u00ab", "tokens": ["Sein", "Helm", "und", "sein", "Schild", "zer\u00b7schmol\u00b7zen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Evadne vernimmt's: sie verhaucht kein Ach;", "tokens": ["E\u00b7vad\u00b7ne", "ver\u00b7nimmt's", ":", "sie", "ver\u00b7haucht", "kein", "Ach", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Stumm liegt sie am Boden im Trauergemach,", "tokens": ["Stumm", "liegt", "sie", "am", "Bo\u00b7den", "im", "Trau\u00b7er\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Umringt von den sorgenden Frauen.", "tokens": ["Um\u00b7ringt", "von", "den", "sor\u00b7gen\u00b7den", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Von Theben nahte der Trauerzug,", "tokens": ["Von", "The\u00b7ben", "nah\u00b7te", "der", "Trau\u00b7er\u00b7zug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der den blitzerschlagenen Helden trug;", "tokens": ["Der", "den", "blit\u00b7zer\u00b7schla\u00b7ge\u00b7nen", "Hel\u00b7den", "trug", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sie aber schlug", "tokens": ["Sie", "a\u00b7ber", "schlug"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Das Auge nicht auf, ihn zu schauen.", "tokens": ["Das", "Au\u00b7ge", "nicht", "auf", ",", "ihn", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "PTKVZ", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Ihren fl\u00fcstern: \u00bbWeil sie nicht klagt,", "tokens": ["Die", "Ih\u00b7ren", "fl\u00fcs\u00b7tern", ":", "\u00bb", "Weil", "sie", "nicht", "klagt", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "$(", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Weil stumm ihr der Jammer am Herzen nagt,", "tokens": ["Weil", "stumm", "ihr", "der", "Jam\u00b7mer", "am", "Her\u00b7zen", "nagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Verk\u00fcndet B\u00f6ses ihr Br\u00fcten.", "tokens": ["Ver\u00b7k\u00fcn\u00b7det", "B\u00f6\u00b7ses", "ihr", "Br\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Damit sie nicht rasche That ver\u00fcbt", "tokens": ["Da\u00b7mit", "sie", "nicht", "ra\u00b7sche", "That", "ver\u00b7\u00fcbt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJA", "NN", "VVFIN"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Und dem zu folgen, den sie geliebt,", "tokens": ["Und", "dem", "zu", "fol\u00b7gen", ",", "den", "sie", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PTKZU", "VVINF", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Den Tod sich giebt,", "tokens": ["Den", "Tod", "sich", "giebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "La\u00dft uns sie achtsam beh\u00fcten!\u00ab", "tokens": ["La\u00dft", "uns", "sie", "acht\u00b7sam", "be\u00b7h\u00fc\u00b7ten", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Im Hof wird Kapaneus aufgebahrt;", "tokens": ["Im", "Hof", "wird", "Ka\u00b7pa\u00b7neus", "auf\u00b7ge\u00b7bahrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "NN", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch sie, als h\u00e4tte sie nichts gewahrt,", "tokens": ["Doch", "sie", ",", "als", "h\u00e4t\u00b7te", "sie", "nichts", "ge\u00b7wahrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "KOKOM", "VAFIN", "PPER", "PIS", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Liegt selbst f\u00fcr tot im Gemache.", "tokens": ["Liegt", "selbst", "f\u00fcr", "tot", "im", "Ge\u00b7ma\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJD", "APPRART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Da pl\u00f6tzlich am Morgen erwacht sie und spricht:", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "am", "Mor\u00b7gen", "er\u00b7wacht", "sie", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "VVFIN", "PPER", "KON", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "\u00bbo Mutter, mein Haupt mit dem Kranz umflicht!", "tokens": ["\u00bb", "o", "Mut\u00b7ter", ",", "mein", "Haupt", "mit", "dem", "Kranz", "um\u00b7flicht", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Mir ward ein Gesicht,", "tokens": ["Mir", "ward", "ein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Aus dem ich in Freuden erwache.", "tokens": ["Aus", "dem", "ich", "in", "Freu\u00b7den", "er\u00b7wa\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Vernimm! In der Rechten den Thyrsusstab,", "tokens": ["Ver\u00b7nimm", "!", "In", "der", "Rech\u00b7ten", "den", "Thyr\u00b7sus\u00b7stab", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Stieg Bacchus in meinen Traum herab,", "tokens": ["Stieg", "Bac\u00b7chus", "in", "mei\u00b7nen", "Traum", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Von himmlischem Glanz umflossen;", "tokens": ["Von", "himm\u00b7li\u00b7schem", "Glanz", "um\u00b7flos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sein dunkles Antlitz leuchtete hold;", "tokens": ["Sein", "dunk\u00b7les", "Ant\u00b7litz", "leuch\u00b7te\u00b7te", "hold", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Der rebenbekr\u00e4nzten Locken Gold", "tokens": ["Der", "re\u00b7ben\u00b7be\u00b7kr\u00e4nz\u00b7ten", "Lo\u00b7cken", "Gold"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "War niedergerollt", "tokens": ["War", "nie\u00b7der\u00b7ge\u00b7rollt"], "token_info": ["word", "word"], "pos": ["VAFIN", "VVPP"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Um den schwellenden Nacken ergossen.\u00ab", "tokens": ["Um", "den", "schwel\u00b7len\u00b7den", "Na\u00b7cken", "er\u00b7gos\u00b7sen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.9": {"line.1": {"text": "\u00bbschon,\u00ab sprach er, \u00bbreift in den Trauben der Saft;", "tokens": ["\u00bb", "schon", ",", "\u00ab", "sprach", "er", ",", "\u00bb", "reift", "in", "den", "Trau\u00b7ben", "der", "Saft", ";"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was z\u00f6gerst du? Auf! Dich emporgerafft!", "tokens": ["Was", "z\u00f6\u00b7gerst", "du", "?", "Auf", "!", "Dich", "em\u00b7por\u00b7ge\u00b7rafft", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "APPR", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn dich zur Dienerin will ich!", "tokens": ["Denn", "dich", "zur", "Die\u00b7ne\u00b7rin", "will", "ich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VMFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Stirn umschling mit dem Epheukranz,", "tokens": ["Die", "Stirn", "um\u00b7schling", "mit", "dem", "E\u00b7pheu\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "F\u00fchr an die M\u00e4naden bei Fackelglanz", "tokens": ["F\u00fchr", "an", "die", "M\u00e4\u00b7na\u00b7den", "bei", "Fa\u00b7ckel\u00b7glanz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Zum bacchischen Tanz,", "tokens": ["Zum", "bac\u00b7chi\u00b7schen", "Tanz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und alle Leiden dir still' ich!\u00ab", "tokens": ["Und", "al\u00b7le", "Lei\u00b7den", "dir", "still'", "ich", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIAT", "NN", "PPER", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "So k\u00fcndet Evadne des Gottes Gehei\u00df", "tokens": ["So", "k\u00fcn\u00b7det", "E\u00b7vad\u00b7ne", "des", "Got\u00b7tes", "Ge\u00b7hei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "ART", "NN", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und eilt von dannen; der Weiber Kreis", "tokens": ["Und", "eilt", "von", "dan\u00b7nen", ";", "der", "Wei\u00b7ber", "Kreis"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADV", "$.", "ART", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In freudigem Staunen umringt sie;", "tokens": ["In", "freu\u00b7di\u00b7gem", "Stau\u00b7nen", "um\u00b7ringt", "sie", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Stirn bekr\u00e4nzt sie mit Epheu schnell;", "tokens": ["Die", "Stirn", "be\u00b7kr\u00e4nzt", "sie", "mit", "E\u00b7pheu", "schnell", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "In der Rechten flammt ihr die Fackel hell,", "tokens": ["In", "der", "Rech\u00b7ten", "flammt", "ihr", "die", "Fa\u00b7ckel", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und der Hindin Fell", "tokens": ["Und", "der", "Hin\u00b7din", "Fell"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Um die blendenden Schultern schlingt sie.", "tokens": ["Um", "die", "blen\u00b7den\u00b7den", "Schul\u00b7tern", "schlingt", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.11": {"line.1": {"text": "\u00bbevadne!\u00ab rufen die Ihren, \u00bbKind!", "