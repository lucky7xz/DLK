, "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und in Jubel wandelst die Klagen!\u00ab", "tokens": ["Und", "in", "Ju\u00b7bel", "wan\u00b7delst", "die", "Kla\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "NN", "$.", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Bald fa\u00dft der Taumel die ganze Schar;", "tokens": ["Bald", "fa\u00dft", "der", "Tau\u00b7mel", "die", "gan\u00b7ze", "Schar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie geben dem Winde das flatternde Haar,", "tokens": ["Sie", "ge\u00b7ben", "dem", "Win\u00b7de", "das", "flat\u00b7tern\u00b7de", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Durchflochten mit Rebenzweigen;", "tokens": ["Durch\u00b7floch\u00b7ten", "mit", "Re\u00b7ben\u00b7zwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den Thyrsus schwingend, durch Schluchten und Wald", "tokens": ["Den", "Thyr\u00b7sus", "schwin\u00b7gend", ",", "durch", "Schluch\u00b7ten", "und", "Wald"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Hinbrausen sie jauchzend; die Pauke schallt,", "tokens": ["Hin\u00b7brau\u00b7sen", "sie", "jauch