{"textgrid.poem.25379": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich stand in Nacht. Ich rang versteinert.", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich stand in Nacht. Ich rang versteinert.", "tokens": ["Ich", "stand", "in", "Nacht", ".", "Ich", "rang", "ver\u00b7stei\u00b7nert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$.", "PPER", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Fand in W\u00fcsten irrend deine Seele nicht.", "tokens": ["Fand", "in", "W\u00fcs\u00b7ten", "ir\u00b7rend", "dei\u00b7ne", "See\u00b7le", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "VVPP", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Die Wege lagen endlos mir versch\u00fcttet,", "tokens": ["Die", "We\u00b7ge", "la\u00b7gen", "end\u00b7los", "mir", "ver\u00b7sch\u00fct\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "die zu deiner Schwelle liefen.", "tokens": ["die", "zu", "dei\u00b7ner", "Schwel\u00b7le", "lie\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich war ganz fern. Du sprachst zu mir.", "tokens": ["Ich", "war", "ganz", "fern", ".", "Du", "sprachst", "zu", "mir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$.", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich stand mit abgewandtem Herzen und Gesicht.", "tokens": ["Ich", "stand", "mit", "ab\u00b7ge\u00b7wand\u00b7tem", "Her\u00b7zen", "und", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie Sterbeglocken rauschten mir die Worte,", "tokens": ["Wie", "Ster\u00b7be\u00b7glo\u00b7cken", "rauschten", "mir", "die", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "die mich zu dir riefen.", "tokens": ["die", "mich", "zu", "dir", "rie\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Ich lauschte dumpf der Stimme.", "tokens": ["Ich", "lauschte", "dumpf", "der", "Stim\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Wie erstarrt. Sie kam", "tokens": ["Wie", "er\u00b7starrt", ".", "Sie", "kam"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAV", "VVPP", "$.", "PPER", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Aus Fernen: still; dem\u00fctig, aber fest;", "tokens": ["Aus", "Fer\u00b7nen", ":", "still", ";", "de\u00b7m\u00fc\u00b7tig", ",", "a\u00b7ber", "fest", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$.", "PTKVZ", "$.", "ADJD", "$,", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "nachtwandelnd und im Glanze ihres Schicksals,", "tokens": ["nacht\u00b7wan\u00b7delnd", "und", "im", "Glan\u00b7ze", "ih\u00b7res", "Schick\u00b7sals", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.13": {"text": "und sie drang in meinen Traum.", "tokens": ["und", "sie", "drang", "in", "mei\u00b7nen", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Da war's, da\u00df in mein Herz das Wunder brach.", "tokens": ["Da", "wa\u00b7r's", ",", "da\u00df", "in", "mein", "Herz", "das", "Wun\u00b7der", "brach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOUS", "APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.15": {"text": "Ich wachte auf. In j\u00e4her Scham", "tokens": ["Ich", "wach\u00b7te", "auf", ".", "In", "j\u00e4\u00b7her", "Scham"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Sah ich mich selbst. Sah deine Seele, wie sie stumm,", "tokens": ["Sah", "ich", "mich", "selbst", ".", "Sah", "dei\u00b7ne", "See\u00b7le", ",", "wie", "sie", "stumm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$.", "VVFIN", "PPOSAT", "NN", "$,", "PWAV", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "mit schweren Lidern, vor mir stand,", "tokens": ["mit", "schwe\u00b7ren", "Li\u00b7dern", ",", "vor", "mir", "stand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Nackend. Sah ihre lange Qual,", "tokens": ["Na\u00b7ckend", ".", "Sah", "ih\u00b7re", "lan\u00b7ge", "Qual", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "und wie sie durch die vielen, vielen N\u00e4chte", "tokens": ["und", "wie", "sie", "durch", "die", "vie\u00b7len", ",", "vie\u00b7len", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWAV", "PPER", "APPR", "ART", "PIAT", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Mich so gesucht, die Augen still in mich gekehrt,", "tokens": ["Mich", "so", "ge\u00b7sucht", ",", "die", "Au\u00b7gen", "still", "in", "mich", "ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,", "ART", "NN", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "und mich doch nimmer fand,", "tokens": ["und", "mich", "doch", "nim\u00b7mer", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Indes ich blind", "tokens": ["In\u00b7des", "ich", "blind"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.23": {"text": "in wilden Zonen irrte", "tokens": ["in", "wil\u00b7den", "Zo\u00b7nen", "irr\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Und meines Herzens Heimwehruf", "tokens": ["Und", "mei\u00b7nes", "Her\u00b7zens", "Heim\u00b7weh\u00b7ruf"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "verbannte.", "tokens": ["ver\u00b7bann\u00b7te", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.26": {"text": "Sah, wie ihr reiner Spiegel", "tokens": ["Sah", ",", "wie", "ihr", "rei\u00b7ner", "Spie\u00b7gel"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "sich mit Dunkel wirrte,", "tokens": ["sich", "mit", "Dun\u00b7kel", "wirr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.28": {"text": "Und j\u00e4h gereckt die Gier,", "tokens": ["Und", "j\u00e4h", "ge\u00b7reckt", "die", "Gier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "wie sie sich selbst zum Opfer br\u00e4chte,", "tokens": ["wie", "sie", "sich", "selbst", "zum", "Op\u00b7fer", "br\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Grausam, im eignen Blut die Qualen l\u00f6schend,", "tokens": ["Grau\u00b7sam", ",", "im", "eig\u00b7nen", "Blut", "die", "Qua\u00b7len", "l\u00f6\u00b7schend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPRART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.31": {"text": "und mit Weh ihr Weh ert\u00f6te,", "tokens": ["und", "mit", "Weh", "ihr", "Weh", "er\u00b7t\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Im Opfer ihres Leibes. Und ich sah dich bleich,", "tokens": ["Im", "Op\u00b7fer", "ih\u00b7res", "Lei\u00b7bes", ".", "Und", "ich", "sah", "dich", "bleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$.", "KON", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "mit nackten F\u00fc\u00dfen auf dem B\u00fc\u00dferberg", "tokens": ["mit", "nack\u00b7ten", "F\u00fc\u00b7\u00dfen", "auf", "dem", "B\u00fc\u00b7\u00dfer\u00b7berg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "und \u00fcber deiner Brust die R\u00f6te", "tokens": ["und", "\u00fc\u00b7ber", "dei\u00b7ner", "Brust", "die", "R\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Der Wunden, die ich dir geschlagen.", "tokens": ["Der", "Wun\u00b7den", ",", "die", "ich", "dir", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Sah dich matt und blo\u00df", "tokens": ["Sah", "dich", "matt", "und", "blo\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.37": {"text": "Und schwach.", "tokens": ["Und", "schwach", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.38": {"text": "Doch \u00fcber Nacht und Leid", "tokens": ["Doch", "\u00fc\u00b7ber", "Nacht", "und", "Leid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.39": {"text": "Strahlte dein heiliges Herz. Ich sah den Glorienschein,", "tokens": ["Strahl\u00b7te", "dein", "hei\u00b7li\u00b7ges", "Herz", ".", "Ich", "sah", "den", "Glo\u00b7ri\u00b7en\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+-+---", "measure": "dactylic.di.plus"}, "line.40": {"text": "der j\u00e4hlings \u00fcber deinem Scheitel brannte", "tokens": ["der", "j\u00e4h\u00b7lings", "\u00fc\u00b7ber", "dei\u00b7nem", "Schei\u00b7tel", "brann\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Und mich bego\u00df.", "tokens": ["Und", "mich", "be\u00b7go\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.42": {"text": "Oh, immer will ich stehn und schauen, schauen", "tokens": ["Oh", ",", "im\u00b7mer", "will", "ich", "stehn", "und", "schau\u00b7en", ",", "schau\u00b7en"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ITJ", "$,", "ADV", "VMFIN", "PPER", "VVINF", "KON", "VVINF", "$,", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Und warten, du Geliebte,", "tokens": ["Und", "war\u00b7ten", ",", "du", "Ge\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "da\u00df dein Antlitz mir ein L\u00e4cheln schenke.", "tokens": ["da\u00df", "dein", "Ant\u00b7litz", "mir", "ein", "L\u00e4\u00b7cheln", "schen\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.45": {"text": "Ich wei\u00df, ich hab an dir ges\u00fcndigt.", "tokens": ["Ich", "wei\u00df", ",", "ich", "hab", "an", "dir", "ge\u00b7s\u00fcn\u00b7digt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Sieh, ich will dein Kleid", "tokens": ["Sieh", ",", "ich", "will", "dein", "Kleid"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VMFIN", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.47": {"text": "Blo\u00df fassen, so wie M\u00fctter tun mit kranken Kindern", "tokens": ["Blo\u00df", "fas\u00b7sen", ",", "so", "wie", "M\u00fct\u00b7ter", "tun", "mit", "kran\u00b7ken", "Kin\u00b7dern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "$,", "ADV", "KOKOM", "NN", "VVINF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "vor dem Bild der lieben Frauen \u2013", "tokens": ["vor", "dem", "Bild", "der", "lie\u00b7ben", "Frau\u00b7en", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Nur l\u00e4chle wieder,", "tokens": ["Nur", "l\u00e4ch\u00b7le", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.50": {"text": "du, in deren Scho\u00df", "tokens": ["du", ",", "in", "de\u00b7ren", "Scho\u00df"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "APPR", "PRELAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.51": {"text": "Ich wie in klares Wasser", "tokens": ["Ich", "wie", "in", "kla\u00b7res", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "KOKOM", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.52": {"text": "meines Lebens dunkles Opfer senke.", "tokens": ["mei\u00b7nes", "Le\u00b7bens", "dunk\u00b7les", "Op\u00b7fer", "sen\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}