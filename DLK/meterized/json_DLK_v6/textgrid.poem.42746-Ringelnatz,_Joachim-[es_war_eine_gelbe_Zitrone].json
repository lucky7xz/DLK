{"textgrid.poem.42746": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "[es war eine gelbe Zitrone]", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war eine gelbe Zitrone,", "tokens": ["Es", "war", "ei\u00b7ne", "gel\u00b7be", "Zit\u00b7ro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die lag unter einer Kanone,", "tokens": ["Die", "lag", "un\u00b7ter", "ei\u00b7ner", "Ka\u00b7no\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und deshalb bildete sie sich ein,", "tokens": ["Und", "des\u00b7halb", "bil\u00b7de\u00b7te", "sie", "sich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Eine Kanonenkugel zu sein.", "tokens": ["Ei\u00b7ne", "Ka\u00b7no\u00b7nen\u00b7ku\u00b7gel", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VAINF", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Der Kanonier im ersten Glied,", "tokens": ["Der", "Ka\u00b7no\u00b7nier", "im", "ers\u00b7ten", "Glied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der merkte aber den Unterschied.", "tokens": ["Der", "merk\u00b7te", "a\u00b7ber", "den", "Un\u00b7ter\u00b7schied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}, "line.8": {"text": "Bemerkt sei noch zu diesem Lied,", "tokens": ["Be\u00b7merkt", "sei", "noch", "zu", "die\u00b7sem", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ein Unterschied ist kein Oberschied.", "tokens": ["Ein", "Un\u00b7ter\u00b7schied", "ist", "kein", "O\u00b7ber\u00b7schied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}}}}}