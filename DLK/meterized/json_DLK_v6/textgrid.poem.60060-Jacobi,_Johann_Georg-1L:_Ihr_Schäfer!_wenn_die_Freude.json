{"textgrid.poem.60060": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr Sch\u00e4fer! wenn die Freude", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Sch\u00e4fer! wenn die Freude", "tokens": ["Ihr", "Sch\u00e4\u00b7fer", "!", "wenn", "die", "Freu\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom H\u00fcgel niederschwebt,", "tokens": ["Vom", "H\u00fc\u00b7gel", "nie\u00b7der\u00b7schwebt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sich das Gr\u00fcn der Weide", "tokens": ["Und", "sich", "das", "Gr\u00fcn", "der", "Wei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Veilchen unterwebt;", "tokens": ["Mit", "Veil\u00b7chen", "un\u00b7ter\u00b7webt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und ihr, in bunten Reihen,", "tokens": ["Und", "ihr", ",", "in", "bun\u00b7ten", "Rei\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Euch um die Blumen setzt,", "tokens": ["Euch", "um", "die", "Blu\u00b7men", "setzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Fl\u00f6ten und Schallmeyen", "tokens": ["Mit", "Fl\u00f6\u00b7ten", "und", "Schall\u00b7me\u00b7yen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den nahen Wald erg\u00f6tzt;", "tokens": ["Den", "na\u00b7hen", "Wald", "er\u00b7g\u00f6tzt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und eure M\u00e4dchen liegen", "tokens": ["Und", "eu\u00b7re", "M\u00e4d\u00b7chen", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf zarten Rasen, weich,", "tokens": ["Auf", "zar\u00b7ten", "Ra\u00b7sen", ",", "weich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am Bl\u00fcthenbaum, und schmiegen", "tokens": ["Am", "Bl\u00fc\u00b7then\u00b7baum", ",", "und", "schmie\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vertrauter sich an euch;", "tokens": ["Ver\u00b7trau\u00b7ter", "sich", "an", "euch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PPER", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Und fern von euren Ch\u00f6ren", "tokens": ["Und", "fern", "von", "eu\u00b7ren", "Ch\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erschallt der Fl\u00f6te Klang;", "tokens": ["Er\u00b7schallt", "der", "Fl\u00f6\u00b7te", "Klang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Chloe kommt, zu h\u00f6ren", "tokens": ["Und", "Chloe", "kommt", ",", "zu", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NE", "VVFIN", "$,", "PTKZU", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Den lockenden Gesang;", "tokens": ["Den", "lo\u00b7cken\u00b7den", "Ge\u00b7sang", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O dann \u2013 die G\u00f6tter geben", "tokens": ["O", "dann", "\u2013", "die", "G\u00f6t\u00b7ter", "ge\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "ADV", "$(", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Euch s\u00fc\u00dfen Lohn daf\u00fcr! \u2013", "tokens": ["Euch", "s\u00fc\u00b7\u00dfen", "Lohn", "da\u00b7f\u00fcr", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJA", "NN", "PAV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann singt vom reinen Leben", "tokens": ["Dann", "singt", "vom", "rei\u00b7nen", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der sch\u00f6nen Unschuld ihr.", "tokens": ["Der", "sch\u00f6\u00b7nen", "Un\u00b7schuld", "ihr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dann singt ihr von der Wei\u00dfe", "tokens": ["Dann", "singt", "ihr", "von", "der", "Wei\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Lilien im Thal;", "tokens": ["Der", "Li\u00b7li\u00b7en", "im", "Thal", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von kleiner Bienen Flei\u00dfe,", "tokens": ["Von", "klei\u00b7ner", "Bie\u00b7nen", "Flei\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bey fr\u00fchem Sonnenstrahl;", "tokens": ["Bey", "fr\u00fc\u00b7hem", "Son\u00b7nen\u00b7strahl", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Von K\u00fcssen ohne Reue,", "tokens": ["Von", "K\u00fcs\u00b7sen", "oh\u00b7ne", "Reu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die man dem Sch\u00e4fer gab;", "tokens": ["Die", "man", "dem", "Sch\u00e4\u00b7fer", "gab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom Glauben an die Treue", "tokens": ["Vom", "Glau\u00b7ben", "an", "die", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bis in das finstre Grab.", "tokens": ["Bis", "in", "das", "finst\u00b7re", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Gelobt's, ihr jungen Hirten,", "tokens": ["Ge\u00b7lobt's", ",", "ihr", "jun\u00b7gen", "Hir\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem Fr\u00fchling! \u2013 Ach, kein Lied,", "tokens": ["Dem", "Fr\u00fch\u00b7ling", "!", "\u2013", "Ach", ",", "kein", "Lied", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "ITJ", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vor dem in ihre Mirthen", "tokens": ["Vor", "dem", "in", "ih\u00b7re", "Mir\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die keusche Liebe flieht!", "tokens": ["Die", "keu\u00b7sche", "Lie\u00b7be", "flieht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Denn ohne Falsch geblieben", "tokens": ["Denn", "oh\u00b7ne", "Falsch", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist noch das M\u00e4dchen. Ach!", "tokens": ["Ist", "noch", "das", "M\u00e4d\u00b7chen", ".", "Ach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$.", "ITJ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wollt ihr die Quelle tr\u00fcben", "tokens": ["Wollt", "ihr", "die", "Quel\u00b7le", "tr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem lautern Silber-Bach?", "tokens": ["Dem", "lau\u00b7tern", "Sil\u00b7ber\u00b7Bach", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Denn Chloens innre G\u00fcte", "tokens": ["Denn", "Chloens", "inn\u00b7re", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Bestrahlt ihr Angesicht:", "tokens": ["Be\u00b7strahlt", "ihr", "An\u00b7ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O, tilgt die erste Bl\u00fcthe", "tokens": ["O", ",", "tilgt", "die", "ers\u00b7te", "Bl\u00fc\u00b7the"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von M\u00e4dchen-Tugend nicht!", "tokens": ["Von", "M\u00e4d\u00b7chen\u00b7Tu\u00b7gend", "nicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}