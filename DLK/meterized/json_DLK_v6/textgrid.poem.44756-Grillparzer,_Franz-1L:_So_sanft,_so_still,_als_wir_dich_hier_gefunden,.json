{"textgrid.poem.44756": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: So sanft, so still, als wir dich hier gefunden,", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So sanft, so still, als wir dich hier gefunden,", "tokens": ["So", "sanft", ",", "so", "still", ",", "als", "wir", "dich", "hier", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "KOUS", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O m\u00f6gest dus im ganzen Leben sein.", "tokens": ["O", "m\u00f6\u00b7gest", "dus", "im", "gan\u00b7zen", "Le\u00b7ben", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "NE", "APPRART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und w\u00e4r dein eigen Bild dir je entschwunden,", "tokens": ["Und", "w\u00e4r", "dein", "ei\u00b7gen", "Bild", "dir", "je", "ent\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "ADJD", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denk an die Schwesterb\u00e4che von Gastein.", "tokens": ["Denk", "an", "die", "Schwes\u00b7ter\u00b7b\u00e4\u00b7che", "von", "Gas\u00b7tein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wies st\u00fcndlich dort gleich M\u00fchlenr\u00e4dern klappert,", "tokens": ["Wies", "st\u00fcnd\u00b7lich", "dort", "gleich", "M\u00fch\u00b7len\u00b7r\u00e4\u00b7dern", "klap\u00b7pert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und mit dem L\u00e4rm die Ruh das Amt geteilt:", "tokens": ["Und", "mit", "dem", "L\u00e4rm", "die", "Ruh", "das", "Amt", "ge\u00b7teilt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Wasserfall zerst\u00f6rt, bespritzt und plappert,", "tokens": ["Der", "Was\u00b7ser\u00b7fall", "zer\u00b7st\u00f6rt", ",", "be\u00b7spritzt", "und", "plap\u00b7pert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die warme Quelle rieselt still und heilt.", "tokens": ["Die", "war\u00b7me", "Quel\u00b7le", "rie\u00b7selt", "still", "und", "heilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}