{"dta.poem.3563": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Tanzreime .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ueber dem Wald, \u00fcber dem Wald,               ", "tokens": ["Ue\u00b7ber", "dem", "Wald", ",", "\u00fc\u00b7ber", "dem", "Wald", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+--++--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Hats nen sch\u00f6nen Reifen;", "tokens": ["Hats", "nen", "sch\u00f6\u00b7nen", "Rei\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Dem M\u00e4dle sind die Ohren kalt,", "tokens": ["Dem", "M\u00e4d\u00b7le", "sind", "die", "Oh\u00b7ren", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Buben wollens greifen.", "tokens": ["Die", "Bu\u00b7ben", "wol\u00b7lens", "grei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}