{"textgrid.poem.24437": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf Sauffenum, und dessen Wasser Trincken", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Leute sind wohl schlim/ die dir den Tadel geben:", "tokens": ["Die", "Leu\u00b7te", "sind", "wohl", "schlim", "/", "die", "dir", "den", "Ta\u00b7del", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$(", "PRELS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die G\u00fcter h\u00e4ttest du im Luder durchgebracht.", "tokens": ["Die", "G\u00fc\u00b7ter", "h\u00e4t\u00b7test", "du", "im", "Lu\u00b7der", "durch\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zu Wasser hast du sie deswegen nur gemacht/", "tokens": ["Zu", "Was\u00b7ser", "hast", "du", "sie", "des\u00b7we\u00b7gen", "nur", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PPER", "PAV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von deinen G\u00fctern so/ dieweil du lebst/ zu leben.", "tokens": ["Von", "dei\u00b7nen", "G\u00fc\u00b7tern", "so", "/", "die\u00b7weil", "du", "lebst", "/", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$(", "KOUS", "PPER", "VVFIN", "$(", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}