{"textgrid.poem.61201": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df ab, o M\u00e4dchen, diese Zeiten sind", "tokens": ["La\u00df", "ab", ",", "o", "M\u00e4d\u00b7chen", ",", "die\u00b7se", "Zei\u00b7ten", "sind"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$,", "FM", "NN", "$,", "PDAT", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr Lieb' und Rosenlauben nicht geschaffen;", "tokens": ["F\u00fcr", "Lieb'", "und", "Ro\u00b7sen\u00b7lau\u00b7ben", "nicht", "ge\u00b7schaf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht darf in s\u00fc\u00dfem Spiel der Arm erschlaffen;", "tokens": ["Nicht", "darf", "in", "s\u00fc\u00b7\u00dfem", "Spiel", "der", "Arm", "er\u00b7schlaf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "APPR", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Darum la\u00df ab, la\u00df ab von mir, mein Kind.", "tokens": ["Da\u00b7rum", "la\u00df", "ab", ",", "la\u00df", "ab", "von", "mir", ",", "mein", "Kind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVIMP", "PTKVZ", "$,", "VVIMP", "PTKVZ", "APPR", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Trompetenkl\u00e4nge flattern hoch im Wind,", "tokens": ["Trom\u00b7pe\u00b7ten\u00b7kl\u00e4n\u00b7ge", "flat\u00b7tern", "hoch", "im", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von Wunden redend, die schon morgen klaffen;", "tokens": ["Von", "Wun\u00b7den", "re\u00b7dend", ",", "die", "schon", "mor\u00b7gen", "klaf\u00b7fen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "PRELS", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es dr\u00f6hnt das Lager, und der Gott der Waffen", "tokens": ["Es", "dr\u00f6hnt", "das", "La\u00b7ger", ",", "und", "der", "Gott", "der", "Waf\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist wie der Gott der Liebe rasch und blind.", "tokens": ["Ist", "wie", "der", "Gott", "der", "Lie\u00b7be", "rasch", "und", "blind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Vielleicht ist schon gesch\u00e4rft die Lanzenspitze,", "tokens": ["Viel\u00b7leicht", "ist", "schon", "ge\u00b7sch\u00e4rft", "die", "Lan\u00b7zens\u00b7pit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die mich durchbohren soll in Mordbegier,", "tokens": ["Die", "mich", "durch\u00b7boh\u00b7ren", "soll", "in", "Mord\u00b7be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VMFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und diese Stirne bald ein Ziel der Blitze.", "tokens": ["Und", "die\u00b7se", "Stir\u00b7ne", "bald", "ein", "Ziel", "der", "Blit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Fahr wohl, da\u00df nicht der Stahl, gez\u00fcckt nach mir,", "tokens": ["Fahr", "wohl", ",", "da\u00df", "nicht", "der", "Stahl", ",", "ge\u00b7z\u00fcckt", "nach", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PTKNEG", "ART", "NN", "$,", "VVPP", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auch deine Brust, auch deine Schulter ritze!", "tokens": ["Auch", "dei\u00b7ne", "Brust", ",", "auch", "dei\u00b7ne", "Schul\u00b7ter", "rit\u00b7ze", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fahr wohl, fahr wohl! Und Friede sei mit dir!", "tokens": ["Fahr", "wohl", ",", "fahr", "wohl", "!", "Und", "Frie\u00b7de", "sei", "mit", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "VVFIN", "ADV", "$.", "KON", "NN", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}