{"textgrid.poem.49926": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Hermippus", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seltsames wird von Hermippus, dem r\u00f6mischen Weisen, dem Pfleger", "tokens": ["Selt\u00b7sa\u00b7mes", "wird", "von", "Her\u00b7mip\u00b7pus", ",", "dem", "r\u00f6\u00b7mi\u00b7schen", "Wei\u00b7sen", ",", "dem", "Pfle\u00b7ger"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VAFIN", "APPR", "NE", "$,", "ART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "+--+-+-+-+--+--+-", "measure": "iambic.septa.invert"}, "line.2": {"text": "Weiblicher Jugend, erz\u00e4hlt, Glaubliches doch, wie mir deucht.", "tokens": ["Weib\u00b7li\u00b7cher", "Ju\u00b7gend", ",", "er\u00b7z\u00e4hlt", ",", "Glaub\u00b7li\u00b7ches", "doch", ",", "wie", "mir", "deucht", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,", "NN", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.3": {"text": "Hundertundf\u00fcnfzehn Jahre, so liest man, vom st\u00e4rkenden Anhauch", "tokens": ["Hun\u00b7der\u00b7tund\u00b7f\u00fcnf\u00b7zehn", "Jah\u00b7re", ",", "so", "liest", "man", ",", "vom", "st\u00e4r\u00b7ken\u00b7den", "An\u00b7hauch"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["CARD", "NN", "$,", "ADV", "VVFIN", "PIS", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Kindlicher Lippen gen\u00e4hrt, lebte der treffliche Greis.", "tokens": ["Kind\u00b7li\u00b7cher", "Lip\u00b7pen", "ge\u00b7n\u00e4hrt", ",", "leb\u00b7te", "der", "treff\u00b7li\u00b7che", "Greis", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.5": {"text": "Dort in geschlossener Halle, die er zur Schule den M\u00e4dchen", "tokens": ["Dort", "in", "ge\u00b7schlos\u00b7se\u00b7ner", "Hal\u00b7le", ",", "die", "er", "zur", "Schu\u00b7le", "den", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Selber gegr\u00fcndet, auch wohl \u00f6fter im G\u00e4rtchen am Haus", "tokens": ["Sel\u00b7ber", "ge\u00b7gr\u00fcn\u00b7det", ",", "auch", "wohl", "\u00f6f\u00b7ter", "im", "G\u00e4rt\u00b7chen", "am", "Haus"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "$,", "ADV", "ADV", "ADV", "APPRART", "NN", "APPRART", "NN"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Sah man ihn Tag f\u00fcr Tag, vom Morgen zum Abende t\u00e4tig,", "tokens": ["Sah", "man", "ihn", "Tag", "f\u00fcr", "Tag", ",", "vom", "Mor\u00b7gen", "zum", "A\u00b7ben\u00b7de", "t\u00e4\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "NN", "APPR", "NN", "$,", "APPRART", "NN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Bei dem bescheidenen Brot seiner Minerva vergn\u00fcgt.", "tokens": ["Bei", "dem", "be\u00b7schei\u00b7de\u00b7nen", "Brot", "sei\u00b7ner", "Mi\u00b7ner\u00b7va", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--++-+-+-+", "measure": "iambic.septa.relaxed"}, "line.9": {"text": "Rundum zu F\u00fc\u00dfen ihm sa\u00df, in pergamentenen Rollen", "tokens": ["Run\u00b7dum", "zu", "F\u00fc\u00b7\u00dfen", "ihm", "sa\u00df", ",", "in", "per\u00b7ga\u00b7men\u00b7te\u00b7nen", "Rol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "APPR", "NN", "PPER", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "+--+--+---+--+-", "measure": "dactylic.tri.plus"}, "line.10": {"text": "Lesend ein Teil, ein Teil still mit dem Griffel bem\u00fcht.", "tokens": ["Le\u00b7send", "ein", "Teil", ",", "ein", "Teil", "still", "mit", "dem", "Grif\u00b7fel", "be\u00b7m\u00fcht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "ART", "NN", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.11": {"text": "Aber der kleineren eins hielt er in holder Umarmung", "tokens": ["A\u00b7ber", "der", "klei\u00b7ne\u00b7ren", "eins", "hielt", "er", "in", "hol\u00b7der", "Um\u00b7ar\u00b7mung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "PIS", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Allzeit selbst auf dem Schlo\u00df (immer das \u00e4rmste zuerst).", "tokens": ["All\u00b7zeit", "selbst", "auf", "dem", "Schlo\u00df", "(", "im\u00b7mer", "das", "\u00e4rms\u00b7te", "zu\u00b7erst", ")", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$(", "ADV", "ART", "ADJA", "ADV", "$(", "$."], "meter": "-+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "Goldene Spr\u00fcche der Alten und liebliche Rhythmen der Dichter,", "tokens": ["Gol\u00b7de\u00b7ne", "Spr\u00fc\u00b7che", "der", "Al\u00b7ten", "und", "lieb\u00b7li\u00b7che", "Rhyth\u00b7men", "der", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "KON", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Die es gelernt, h\u00f6rt' er, leis ihm der Reihe nach ab.", "tokens": ["Die", "es", "ge\u00b7lernt", ",", "h\u00f6rt'", "er", ",", "leis", "ihm", "der", "Rei\u00b7he", "nach", "ab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "VVFIN", "PPER", "$,", "ADJD", "PPER", "ART", "NN", "APPR", "PTKVZ", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.15": {"text": "Und vom Munde des M\u00e4dchens den Hauch, wie Fr\u00fchlingsatem", "tokens": ["Und", "vom", "Mun\u00b7de", "des", "M\u00e4d\u00b7chens", "den", "Hauch", ",", "wie", "Fr\u00fch\u00b7lin\u00b7gsa\u00b7tem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "ART", "NN", "$,", "PWAV", "NN"], "meter": "--+--+--+-+-+-", "measure": "anapaest.tri.plus"}, "line.16": {"text": "Herzerfrischend, empfing er in die welkende Brust.", "tokens": ["Her\u00b7zer\u00b7fri\u00b7schend", ",", "emp\u00b7fing", "er", "in", "die", "wel\u00b7ken\u00b7de", "Brust", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Also fristet' Asklepios ihm die gesegneten Tage.", "tokens": ["Al\u00b7so", "fris\u00b7tet'", "As\u00b7kle\u00b7pi\u00b7os", "ihm", "die", "ge\u00b7se\u00b7gne\u00b7ten", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "PPER", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Aber der Parze zuletzt weicht auch der Himmlischen Rat.", "tokens": ["A\u00b7ber", "der", "Par\u00b7ze", "zu\u00b7letzt", "weicht", "auch", "der", "Himm\u00b7li\u00b7schen", "Rat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.19": {"text": "\u2013 \u2013 Als er nun tot im Portikus sa\u00df in dem steinernen Sessel,", "tokens": ["\u2013", "\u2013", "Als", "er", "nun", "tot", "im", "Por\u00b7ti\u00b7kus", "sa\u00df", "in", "dem", "stei\u00b7ner\u00b7nen", "Ses\u00b7sel", ","], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "KOUS", "PPER", "ADV", "ADJD", "APPRART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "Noch vom Mantel, den er gestern getragen, umh\u00fcllt,", "tokens": ["Noch", "vom", "Man\u00b7tel", ",", "den", "er", "ge\u00b7stern", "ge\u00b7tra\u00b7gen", ",", "um\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,", "VVFIN", "$,"], "meter": "--+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Kamen aus jedem Quartiere der Stadt unm\u00fcndige Kinder,", "tokens": ["Ka\u00b7men", "aus", "je\u00b7dem", "Quar\u00b7tie\u00b7re", "der", "Stadt", "un\u00b7m\u00fcn\u00b7di\u00b7ge", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-++--+-+--+-", "measure": "iambic.septa.invert"}, "line.22": {"text": "Jungfraun, M\u00fctter, in Eil, edle Matronen, herbei,", "tokens": ["Jung\u00b7fraun", ",", "M\u00fct\u00b7ter", ",", "in", "Eil", ",", "ed\u00b7le", "Mat\u00b7ro\u00b7nen", ",", "her\u00b7bei", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "APPR", "NN", "$,", "ADJA", "NN", "$,", "PTKVZ", "$,"], "meter": "+-+--+--++--+", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "Ihren Hermippus noch einmal zu sehn, den Geweihten der G\u00f6tter,", "tokens": ["Ih\u00b7ren", "Her\u00b7mip\u00b7pus", "noch", "ein\u00b7mal", "zu", "sehn", ",", "den", "Ge\u00b7weih\u00b7ten", "der", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "PTKZU", "VVINF", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.24": {"text": "Kamen und standen von fern, sonder Entsetzen, um ihn,", "tokens": ["Ka\u00b7men", "und", "stan\u00b7den", "von", "fern", ",", "son\u00b7der", "Ent\u00b7set\u00b7zen", ",", "um", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPR", "ADJD", "$,", "KON", "NN", "$,", "KOUI", "PPER", "$,"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.25": {"text": "Ehrend so heiligen Schlaf mit Schweigen. Und einige kr\u00e4nzten", "tokens": ["Eh\u00b7rend", "so", "hei\u00b7li\u00b7gen", "Schlaf", "mit", "Schwei\u00b7gen", ".", "Und", "ei\u00b7ni\u00b7ge", "kr\u00e4nz\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "ADJA", "NN", "APPR", "NN", "$.", "KON", "PIAT", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.26": {"text": "Mit Hyazinthen sein Haupt, Veilchen auch deckten den Scho\u00df.", "tokens": ["Mit", "Hy\u00b7a\u00b7zin\u00b7then", "sein", "Haupt", ",", "Veil\u00b7chen", "auch", "deck\u00b7ten", "den", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$,", "NN", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Lieblicher war nicht Homerus geschm\u00fcckt von den Fingern der Musen,", "tokens": ["Lieb\u00b7li\u00b7cher", "war", "nicht", "Ho\u00b7me\u00b7rus", "ge\u00b7schm\u00fcckt", "von", "den", "Fin\u00b7gern", "der", "Mu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PTKNEG", "NE", "VVPP", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+---+--+--+-", "measure": "iambic.hexa.invert"}, "line.28": {"text": "Milderes Have war keinem hinuntergefolgt.", "tokens": ["Mil\u00b7de\u00b7res", "Ha\u00b7ve", "war", "kei\u00b7nem", "hin\u00b7un\u00b7ter\u00b7ge\u00b7folgt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "PIS", "VVPP", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}, "stanza.2": {"line.1": {"text": "Aber wozu ", "tokens": ["A\u00b7ber", "wo\u00b7zu"], "token_info": ["word", "word"], "pos": ["KON", "PWAV"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Lang ist die Kunst, und lang messe dein Leben der Gott!", "tokens": ["Lang", "ist", "die", "Kunst", ",", "und", "lang", "mes\u00b7se", "dein", "Le\u00b7ben", "der", "Gott", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "KON", "ADJD", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Zwar noch ist es nicht eben an dem gar, da\u00df du der K\u00fcnste", "tokens": ["Zwar", "noch", "ist", "es", "nicht", "e\u00b7ben", "an", "dem", "gar", ",", "da\u00df", "du", "der", "K\u00fcns\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "APPR", "ART", "ADV", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Unseres R\u00f6mers bedarfst, aber sie kommt dir, die Zeit,", "tokens": ["Un\u00b7se\u00b7res", "R\u00f6\u00b7mers", "be\u00b7darfst", ",", "a\u00b7ber", "sie", "kommt", "dir", ",", "die", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KON", "PPER", "VVFIN", "PPER", "$,", "ART", "NN", "$,"], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.5": {"text": "La\u00df mich's hoffen! \u2013 gewi\u00df. Dann, wenn die Locke dir schneewei\u00df", "tokens": ["La\u00df", "mich's", "hof\u00b7fen", "!", "\u2013", "ge\u00b7wi\u00df", ".", "Dann", ",", "wenn", "die", "Lo\u00b7cke", "dir", "schnee\u00b7wei\u00df"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVFIN", "$.", "$(", "ADV", "$.", "ADV", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN"], "meter": "+-+--+-+-+-+++", "measure": "trochaic.octa.plus.relaxed"}, "line.6": {"text": "H\u00e4ngt und der Bart, wer ist besser geborgen als du?", "tokens": ["H\u00e4ngt", "und", "der", "Bart", ",", "wer", "ist", "bes\u00b7ser", "ge\u00b7bor\u00b7gen", "als", "du", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ART", "NN", "$,", "PWS", "VAFIN", "ADJD", "VVPP", "KOUS", "PPER", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.7": {"text": "Doch ich seh es im Geist, du wirst an W\u00fcrden und Ehren", "tokens": ["Doch", "ich", "seh", "es", "im", "Geist", ",", "du", "wirst", "an", "W\u00fcr\u00b7den", "und", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,", "PPER", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Reich, vor den Neunzigen schon heiterer Ruhe dich freun.", "tokens": ["Reich", ",", "vor", "den", "Neun\u00b7zi\u00b7gen", "schon", "hei\u00b7te\u00b7rer", "Ru\u00b7he", "dich", "freun", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "ADV", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Still im eigenen Haus hast du, im eigenen G\u00e4rtlein", "tokens": ["Still", "im", "ei\u00b7ge\u00b7nen", "Haus", "hast", "du", ",", "im", "ei\u00b7ge\u00b7nen", "G\u00e4rt\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "VAFIN", "PPER", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Sitzend, ein bl\u00fchendes, lernlustiges H\u00e4ufchen zur Hand.", "tokens": ["Sit\u00b7zend", ",", "ein", "bl\u00fc\u00b7hen\u00b7des", ",", "lern\u00b7lus\u00b7ti\u00b7ges", "H\u00e4uf\u00b7chen", "zur", "Hand", "."], "token_info": ["word", "punct