{"dta.poem.16059": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "113.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein wandelbares Ich, das ist und wird und war,", "tokens": ["Mein", "wan\u00b7del\u00b7ba\u00b7res", "Ich", ",", "das", "ist", "und", "wird", "und", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "PPER", "$,", "PDS", "VAFIN", "KON", "VAFIN", "KON", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ergreift im Dein'gen sich, das ist unwandelbar.", "tokens": ["Er\u00b7greift", "im", "Dein'\u00b7gen", "sich", ",", "das", "ist", "un\u00b7wan\u00b7del\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PRF", "$,", "PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn du bist, der du warst, und bist, der seyn wirst, du!", "tokens": ["Denn", "du", "bist", ",", "der", "du", "warst", ",", "und", "bist", ",", "der", "seyn", "wirst", ",", "du", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,", "PRELS", "PPER", "VAFIN", "$,", "KON", "VAFIN", "$,", "PRELS", "VAINF", "VAFIN", "$,", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es str\u00f6mt aus deinem Seyn mein Seyn dem deinen zu.", "tokens": ["Es", "str\u00f6mt", "aus", "dei\u00b7nem", "Seyn", "mein", "Seyn", "dem", "dei\u00b7nen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "ART", "PPOSAT", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich h\u00e4tt' in jeder Nacht mich, der ich war, verloren,", "tokens": ["Ich", "h\u00e4tt'", "in", "je\u00b7der", "Nacht", "mich", ",", "der", "ich", "war", ",", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "PPER", "$,", "PRELS", "PPER", "VAFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und w\u00e4r' an jedem Tag, als der nicht war, geboren,", "tokens": ["Und", "w\u00e4r'", "an", "je\u00b7dem", "Tag", ",", "als", "der", "nicht", "war", ",", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PIAT", "NN", "$,", "KOUS", "ART", "PTKNEG", "VAFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "H\u00e4tt' ich mich nicht, da\u00df ich derselbe bin, begriffen,", "tokens": ["H\u00e4tt'", "ich", "mich", "nicht", ",", "da\u00df", "ich", "der\u00b7sel\u00b7be", "bin", ",", "be\u00b7grif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "$,", "KOUS", "PPER", "PDS", "VAFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil ich in dir, der ist, bin ewig inbegriffen.", "tokens": ["Weil", "ich", "in", "dir", ",", "der", "ist", ",", "bin", "e\u00b7wig", "in\u00b7be\u00b7grif\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "$,", "PRELS", "VAFIN", "$,", "VAFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}