{"textgrid.poem.33916": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Herbstgef\u00fchl", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gr\u00fcnen, Bl\u00fchen, Duften, Gl\u00e4nzen,", "tokens": ["Gr\u00fc\u00b7nen", ",", "Bl\u00fc\u00b7hen", ",", "Duf\u00b7ten", ",", "Gl\u00e4n\u00b7zen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reichstes Leben ohne Gr\u00e4nzen,", "tokens": ["Reichs\u00b7tes", "Le\u00b7ben", "oh\u00b7ne", "Gr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles steigernd, nirgends stockend,", "tokens": ["Al\u00b7les", "stei\u00b7gernd", ",", "nir\u00b7gends", "sto\u00b7ckend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVPP", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selbst die k\u00fchnsten W\u00fcnsche lockend:", "tokens": ["Selbst", "die", "k\u00fchns\u00b7ten", "W\u00fcn\u00b7sche", "lo\u00b7ckend", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ja, da kann ich wohl zerflie\u00dfen,", "tokens": ["Ja", ",", "da", "kann", "ich", "wohl", "zer\u00b7flie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber nimmermehr genie\u00dfen;", "tokens": ["A\u00b7ber", "nim\u00b7mer\u00b7mehr", "ge\u00b7nie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Solche Fl\u00fcgel tragen weiter,", "tokens": ["Sol\u00b7che", "Fl\u00fc\u00b7gel", "tra\u00b7gen", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als zur n\u00e4chsten Kirschbaum-Leiter.", "tokens": ["Als", "zur", "n\u00e4chs\u00b7ten", "Kirschbaum\u00b7Lei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Doch, wenn roth die Bl\u00e4tter fallen,", "tokens": ["Doch", ",", "wenn", "roth", "die", "Bl\u00e4t\u00b7ter", "fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fchl die Nebelhauche wallen,", "tokens": ["K\u00fchl", "die", "Ne\u00b7bel\u00b7hau\u00b7che", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leis durchschauernd, nicht erfrischend,", "tokens": ["Leis", "durch\u00b7schau\u00b7ernd", ",", "nicht", "er\u00b7fri\u00b7schend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den warmen Wind sich mischend:", "tokens": ["In", "den", "war\u00b7men", "Wind", "sich", "mi\u00b7schend", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dann vom Endlos-Ungeheuren", "tokens": ["Dann", "vom", "End\u00b7los\u00b7Un\u00b7ge\u00b7heu\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fl\u00fccht' ich gern zum Menschlich-Theuren,", "tokens": ["Fl\u00fccht'", "ich", "gern", "zum", "Menschlich\u00b7Theu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und in einer ersten Traube", "tokens": ["Und", "in", "ei\u00b7ner", "ers\u00b7ten", "Trau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieht die Frucht der Welt mein Glaube.", "tokens": ["Sieht", "die", "Frucht", "der", "Welt", "mein", "Glau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}