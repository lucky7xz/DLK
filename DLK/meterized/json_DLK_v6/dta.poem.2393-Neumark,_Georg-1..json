{"dta.poem.2393": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.85", "af:0.14"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Pakke dich Feindin! weg von mir Dione!", "tokens": ["Pak\u00b7ke", "dich", "Fein\u00b7din", "!", "weg", "von", "mir", "Dio\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$.", "ADV", "APPR", "PPER", "NE", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Pakke dich Venus! weg mit deinem Sohne!", "tokens": ["Pak\u00b7ke", "dich", "Ve\u00b7nus", "!", "weg", "mit", "dei\u00b7nem", "Soh\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$.", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Deine verfluchte ", "tokens": ["Dei\u00b7ne", "ver\u00b7fluch\u00b7te"], "token_info": ["word", "word"], "pos": ["PPOSAT", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Hab\u2019 ich ein grauen.", "tokens": ["Hab'", "ich", "ein", "grau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Hebe dich weg ", "tokens": ["He\u00b7be", "dich", "weg"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKVZ"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Wehe dem Menschen/ der in deinem Banne", "tokens": ["We\u00b7he", "dem", "Men\u00b7schen", "/", "der", "in", "dei\u00b7nem", "Ban\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$(", "ART", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Feste verkn\u00fcpfft/ der dir sein junges Leben", "tokens": ["Fes\u00b7te", "ver\u00b7kn\u00fcpfft", "/", "der", "dir", "sein", "jun\u00b7ges", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$(", "PRELS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Sch\u00e4ndlich ergeben.", "tokens": ["Sch\u00e4nd\u00b7lich", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Denn ", "tokens": ["Denn"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Nichtes gewissers als ein heimlichs ", "tokens": ["Nich\u00b7tes", "ge\u00b7wis\u00b7sers", "als", "ein", "heim\u00b7lichs"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "ADV", "KOUS", "ART", "ADJA"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Nach dem mit ", "tokens": ["Nach", "dem", "mit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Bittere Schmertzen.", "tokens": ["Bit\u00b7te\u00b7re", "Schmert\u00b7zen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "J\u00e4mmerlich fellt durch dich die zarte Jugend/", "tokens": ["J\u00e4m\u00b7mer\u00b7lich", "fellt", "durch", "dich", "die", "zar\u00b7te", "Ju\u00b7gend", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Liederlich stirbt durch dich die \u00e4dle Tugend/", "tokens": ["Lie\u00b7der\u00b7lich", "stirbt", "durch", "dich", "die", "\u00e4d\u00b7le", "Tu\u00b7gend", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Falscher Kupido/ du hast lust mit L\u00fcgen", "tokens": ["Fal\u00b7scher", "Ku\u00b7pi\u00b7do", "/", "du", "hast", "lust", "mit", "L\u00fc\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$(", "PPER", "VAFIN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Menschen zu triegen.", "tokens": ["Men\u00b7schen", "zu", "trie\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Numehro bin ich mit mir einig worden/", "tokens": ["Nu\u00b7meh\u00b7ro", "bin", "ich", "mit", "mir", "ei\u00b7nig", "wor\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "ADJD", "VAPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gantz zu entsagen deinem geilen Orden/", "tokens": ["Gantz", "zu", "ent\u00b7sa\u00b7gen", "dei\u00b7nem", "gei\u00b7len", "Or\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "PPOSAT", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Welcher mit Hauffen in sich h\u00e4lt verborgen/", "tokens": ["Wel\u00b7cher", "mit", "Hauf\u00b7fen", "in", "sich", "h\u00e4lt", "ver\u00b7bor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPR", "NN", "APPR", "PRF", "VVFIN", "VVPP", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Allerley Sorgen.", "tokens": ["Al\u00b7ler\u00b7ley", "Sor\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Weder dein ", "tokens": ["We\u00b7der", "dein"], "token_info": ["word", "word"], "pos": ["KON", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Wil ich nicht achten/ noch zu Hertzen fassen;", "tokens": ["Wil", "ich", "nicht", "ach\u00b7ten", "/", "noch", "zu", "Hert\u00b7zen", "fas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$(", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ob du wirst lachen/ oder auff mich klagen/", "tokens": ["Ob", "du", "wirst", "la\u00b7chen", "/", "o\u00b7der", "auff", "mich", "kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "VVINF", "$(", "KON", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wil ich nicht fragen.", "tokens": ["Wil", "ich", "nicht", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}