{"textgrid.poem.34938": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Posaunenruf erf\u00fcllt die Luft,", "tokens": ["Po\u00b7sau\u00b7nen\u00b7ruf", "er\u00b7f\u00fcllt", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und furchtbar schallt es wider;", "tokens": ["Und", "furcht\u00b7bar", "schallt", "es", "wi\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Toten steigen aus der Gruft,", "tokens": ["Die", "To\u00b7ten", "stei\u00b7gen", "aus", "der", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sch\u00fctteln und r\u00fctteln die Glieder.", "tokens": ["Und", "sch\u00fct\u00b7teln", "und", "r\u00fct\u00b7teln", "die", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Was Beine hat, das trollt sich fort,", "tokens": ["Was", "Bei\u00b7ne", "hat", ",", "das", "trollt", "sich", "fort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "$,", "PDS", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es wallen die wei\u00dfen Gestalten,", "tokens": ["Es", "wal\u00b7len", "die", "wei\u00b7\u00dfen", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Nach Josaphat, dem Sammelort,", "tokens": ["Nach", "Jo\u00b7sa\u00b7phat", ",", "dem", "Sam\u00b7mel\u00b7ort", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dort wird Gericht gehalten.", "tokens": ["Dort", "wird", "Ge\u00b7richt", "ge\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Als Freigraf sitzet Christus dort", "tokens": ["Als", "Frei\u00b7graf", "sit\u00b7zet", "Chris\u00b7tus", "dort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVFIN", "NE", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In seiner Apostel Kreise.", "tokens": ["In", "sei\u00b7ner", "A\u00b7pos\u00b7tel", "Krei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie sind die Sch\u00f6ppen, ihr Spruch und Wort", "tokens": ["Sie", "sind", "die", "Sch\u00f6p\u00b7pen", ",", "ihr", "Spruch", "und", "Wort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ist minniglich und weise.", "tokens": ["Ist", "min\u00b7nig\u00b7lich", "und", "wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie urteln nicht vermummten Gesichts;", "tokens": ["Sie", "ur\u00b7teln", "nicht", "ver\u00b7mumm\u00b7ten", "Ge\u00b7sichts", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die Maske l\u00e4\u00dft jeder fallen", "tokens": ["Die", "Mas\u00b7ke", "l\u00e4\u00dft", "je\u00b7der", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIS", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Am hellen Tage des J\u00fcngsten Gerichts,", "tokens": ["Am", "hel\u00b7len", "Ta\u00b7ge", "des", "J\u00fcng\u00b7sten", "Ge\u00b7richts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wenn die Posaunen schallen.", "tokens": ["Wenn", "die", "Po\u00b7sau\u00b7nen", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das ist zu Josaphat im Tal,", "tokens": ["Das", "ist", "zu", "Jo\u00b7sa\u00b7phat", "im", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NE", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da stehn die geladenen Scharen,", "tokens": ["Da", "stehn", "die", "ge\u00b7la\u00b7de\u00b7nen", "Scha\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und weil zu gro\u00df der Beklagten Zahl,", "tokens": ["Und", "weil", "zu", "gro\u00df", "der", "Be\u00b7klag\u00b7ten", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PTKA", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wird hier summarisch verfahren.", "tokens": ["Wird", "hier", "sum\u00b7ma\u00b7risch", "ver\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Das B\u00f6cklein zur Linken, zur Rechten das Schaf,", "tokens": ["Das", "B\u00f6c\u00b7klein", "zur", "Lin\u00b7ken", ",", "zur", "Rech\u00b7ten", "das", "Schaf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Geschieden sind sie schnelle;", "tokens": ["Ge\u00b7schie\u00b7den", "sind", "sie", "schnel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Himmel dem Sch\u00e4fchen fromm und brav,", "tokens": ["Der", "Him\u00b7mel", "dem", "Sch\u00e4f\u00b7chen", "fromm", "und", "brav", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dem geilen Bock die H\u00f6lle!", "tokens": ["Dem", "gei\u00b7len", "Bock", "die", "H\u00f6l\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}