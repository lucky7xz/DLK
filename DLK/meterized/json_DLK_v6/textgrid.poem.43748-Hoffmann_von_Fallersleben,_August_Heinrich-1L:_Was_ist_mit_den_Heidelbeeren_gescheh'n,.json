{"textgrid.poem.43748": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was ist mit den Heidelbeeren gescheh'n,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist mit den Heidelbeeren gescheh'n,", "tokens": ["Was", "ist", "mit", "den", "Hei\u00b7del\u00b7bee\u00b7ren", "ge\u00b7scheh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da\u00df sie so blau sich lassen seh'n?", "tokens": ["Da\u00df", "sie", "so", "blau", "sich", "las\u00b7sen", "seh'n", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PRF", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Frau Sonne wohl \u00fcber die Heide kam", "tokens": ["Frau", "Son\u00b7ne", "wohl", "\u00fc\u00b7ber", "die", "Hei\u00b7de", "kam"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "ADV", "APPR", "ART", "NE", "VVFIN"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.4": {"text": "Und hat sie gek\u00fc\u00dft mit hei\u00dfer Glut,", "tokens": ["Und", "hat", "sie", "ge\u00b7k\u00fc\u00dft", "mit", "hei\u00b7\u00dfer", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Da sind sie blau geworden vor Scham", "tokens": ["Da", "sind", "sie", "blau", "ge\u00b7wor\u00b7den", "vor", "Scham"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VAPP", "APPR", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Und blau verwandelt hat sich ihr Blut", "tokens": ["Und", "blau", "ver\u00b7wan\u00b7delt", "hat", "sich", "ihr", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "VAFIN", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Wir wollen uns b\u00fccken", "tokens": ["Wir", "wol\u00b7len", "uns", "b\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Und flei\u00dfig pfl\u00fccken", "tokens": ["Und", "flei\u00b7\u00dfig", "pfl\u00fc\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Die Heidelbeeren,", "tokens": ["Die", "Hei\u00b7del\u00b7bee\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Und lustig verzehren", "tokens": ["Und", "lus\u00b7tig", "ver\u00b7zeh\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Frau Sonne zu Ehren", "tokens": ["Frau", "Son\u00b7ne", "zu", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Die Heidelbeeren.", "tokens": ["Die", "Hei\u00b7del\u00b7bee\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Die lieben blauen Heidelbeeren!", "tokens": ["Die", "lie\u00b7ben", "blau\u00b7en", "Hei\u00b7del\u00b7bee\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}