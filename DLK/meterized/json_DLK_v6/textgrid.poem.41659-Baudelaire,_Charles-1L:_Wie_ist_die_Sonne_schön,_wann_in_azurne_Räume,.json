{"textgrid.poem.41659": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie ist die Sonne sch\u00f6n, wann in azurne R\u00e4ume,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ist die Sonne sch\u00f6n, wann in azurne R\u00e4ume,", "tokens": ["Wie", "ist", "die", "Son\u00b7ne", "sch\u00f6n", ",", "wann", "in", "a\u00b7zur\u00b7ne", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADJD", "$,", "PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein flammendes Gescho\u00df, sie gr\u00fc\u00dfend aufw\u00e4rts steigt!", "tokens": ["Ein", "flam\u00b7men\u00b7des", "Ge\u00b7scho\u00df", ",", "sie", "gr\u00fc\u00b7\u00dfend", "auf\u00b7w\u00e4rts", "steigt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gl\u00fcckselig, wessen Herz sich ihr in Liebe neigt", "tokens": ["Gl\u00fcck\u00b7se\u00b7lig", ",", "wes\u00b7sen", "Herz", "sich", "ihr", "in", "Lie\u00b7be", "neigt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "PWAT", "NN", "PRF", "PPER", "APPR", "NN", "VVFIN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Bei ihrem Untergang, der prangender als Tr\u00e4ume!", "tokens": ["Bei", "ih\u00b7rem", "Un\u00b7ter\u00b7gang", ",", "der", "pran\u00b7gen\u00b7der", "als", "Tr\u00e4u\u00b7me", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich denke dran! ... Ich sah Quell, Blume, Furche, Feld,", "tokens": ["Ich", "den\u00b7ke", "dran", "!", "...", "Ich", "sah", "Quell", ",", "Blu\u00b7me", ",", "Fur\u00b7che", ",", "Feld", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "$(", "PPER", "VVFIN", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gleichwie ein zitternd Herz vergehn vor ihren Pfeilen ...", "tokens": ["Gleich\u00b7wie", "ein", "zit\u00b7ternd", "Herz", "ver\u00b7gehn", "vor", "ih\u00b7ren", "Pfei\u00b7len", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "NN", "VVINF", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf nach dem Horizont, es dunkelt, la\u00dft uns eilen,", "tokens": ["Auf", "nach", "dem", "Ho\u00b7ri\u00b7zont", ",", "es", "dun\u00b7kelt", ",", "la\u00dft", "uns", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "$,", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Damit auf unser Haupt ein sp\u00e4ter Strahl noch f\u00e4llt!", "tokens": ["Da\u00b7mit", "auf", "un\u00b7ser", "Haupt", "ein", "sp\u00e4\u00b7ter", "Strahl", "noch", "f\u00e4llt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch folge ich umsonst dem Gotte, der entschwindet;", "tokens": ["Doch", "fol\u00b7ge", "ich", "um\u00b7sonst", "dem", "Got\u00b7te", ",", "der", "ent\u00b7schwin\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die allgewaltge Nacht hat schon ihr Reich begr\u00fcndet,", "tokens": ["Die", "all\u00b7ge\u00b7walt\u00b7ge", "Nacht", "hat", "schon", "ihr", "Reich", "be\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die schwarz und unheilvoll das Herz in Schauern l\u00f6st;", "tokens": ["Die", "schwarz", "und", "un\u00b7heil\u00b7voll", "das", "Herz", "in", "Schau\u00b7ern", "l\u00f6st", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ein Grabesdunst enthaucht des Dunkels feuchtem Flore,", "tokens": ["Ein", "Gra\u00b7bes\u00b7dunst", "ent\u00b7haucht", "des", "Dun\u00b7kels", "feuch\u00b7tem", "Flo\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Indes mein banger Fu\u00df am Rand der finstren Moore", "tokens": ["In\u00b7des", "mein", "ban\u00b7ger", "Fu\u00df", "am", "Rand", "der", "finst\u00b7ren", "Moo\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf kalte Schnecken und gescheuchte Kr\u00f6ten st\u00f6\u00dft.", "tokens": ["Auf", "kal\u00b7te", "Schne\u00b7cken", "und", "ge\u00b7scheuch\u00b7te", "Kr\u00f6\u00b7ten", "st\u00f6\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}