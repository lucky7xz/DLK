{"textgrid.poem.43759": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier sind heute viele Leute", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier sind heute viele Leute", "tokens": ["Hier", "sind", "heu\u00b7te", "vie\u00b7le", "Leu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PIAT", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Auf dem Markt vereint:", "tokens": ["Auf", "dem", "Markt", "ver\u00b7eint", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Gaffen wie die Narren,", "tokens": ["Gaf\u00b7fen", "wie", "die", "Nar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Stehen still und harren,", "tokens": ["Ste\u00b7hen", "still", "und", "har\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Bis Hanswurst erscheint,", "tokens": ["Bis", "Hans\u00b7wurst", "er\u00b7scheint", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Bidi bum bum!", "tokens": ["Bi\u00b7di", "bum", "bum", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["XY", "XY", "XY", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Bis Hanswurst erscheint.", "tokens": ["Bis", "Hans\u00b7wurst", "er\u00b7scheint", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Endlich singet, tanzt und springet", "tokens": ["End\u00b7lich", "sin\u00b7get", ",", "tanzt", "und", "sprin\u00b7get"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dann Hanswurst umher:", "tokens": ["Dann", "Hans\u00b7wurst", "um\u00b7her", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Auf dem Seile munter", "tokens": ["Auf", "dem", "Sei\u00b7le", "mun\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Bald hinauf, hinunter,", "tokens": ["Bald", "hin\u00b7auf", ",", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Bald die Kreuz, die Quer,", "tokens": ["Bald", "die", "Kreuz", ",", "die", "Quer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Bidi bum bum!", "tokens": ["Bi\u00b7di", "bum", "bum", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["XY", "XY", "XY", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Bald die Kreuz, die Quer.", "tokens": ["Bald", "die", "Kreuz", ",", "die", "Quer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbhab' ich allen dann gefallen,", "tokens": ["\u00bb", "hab'", "ich", "al\u00b7len", "dann", "ge\u00b7fal\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "PIAT", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt ein Kunstst\u00fcck noch:", "tokens": ["Kommt", "ein", "Kunst\u00b7st\u00fcck", "noch", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Legt auf einen Teller", "tokens": ["Legt", "auf", "ei\u00b7nen", "Tel\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Eure letzten Heller,", "tokens": ["Eu\u00b7re", "letz\u00b7ten", "Hel\u00b7ler", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Die verschluck' ich grad,", "tokens": ["Die", "ver\u00b7schluck'", "ich", "grad", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Bidi bum bum!", "tokens": ["Bi\u00b7di", "bum", "bum", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["XY", "XY", "XY", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "So wie Krautsalat.", "tokens": ["So", "wie", "Kraut\u00b7sa\u00b7lat", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Nun versteht Ihr: es gehet", "tokens": ["Nun", "ver\u00b7steht", "Ihr", ":", "es", "ge\u00b7het"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PPER", "VVFIN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Alle Kunst nach Brot.", "tokens": ["Al\u00b7le", "Kunst", "nach", "Brot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Denn umsonst ist heute", "tokens": ["Denn", "um\u00b7sonst", "ist", "heu\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ADV"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Nichts, ihr lieben Leute,", "tokens": ["Nichts", ",", "ihr", "lie\u00b7ben", "Leu\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Nichts als nur der Tod,", "tokens": ["Nichts", "als", "nur", "der", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ADV", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Bidi bum bum!", "tokens": ["Bi\u00b7di", "bum", "bum", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["XY", "XY", "XY", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Nichts als nur der Tod.\u00ab", "tokens": ["Nichts", "als", "nur", "der", "Tod", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "KOKOM", "ADV", "ART", "NN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}