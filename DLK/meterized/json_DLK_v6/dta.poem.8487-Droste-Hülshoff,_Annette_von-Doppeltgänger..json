{"dta.poem.8487": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Doppeltg\u00e4nger.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "'s war eine Nacht, vom Thaue wachgek&#252;\u00dft,", "tokens": ["'s", "war", "ei\u00b7ne", "Nacht", ",", "vom", "Thau\u00b7e", "wach\u00b7gek", "&#252;", "\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "APPRART", "NN", "NE", "$(", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Das Dunkel f\u00fchlt\u2019 ich k\u00fchl wie zarten Regen", "tokens": ["Das", "Dun\u00b7kel", "f\u00fchlt'", "ich", "k\u00fchl", "wie", "zar\u00b7ten", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An meine Wange gleiten. Das Ger\u00fcst", "tokens": ["An", "mei\u00b7ne", "Wan\u00b7ge", "glei\u00b7ten", ".", "Das", "Ge\u00b7r\u00fcst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$.", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Vorhangs schien sich schaukelnd zu bewegen,", "tokens": ["Des", "Vor\u00b7hangs", "schien", "sich", "schau\u00b7kelnd", "zu", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "\u2019s war eine Nacht, wo man am Morgen denkt:", "tokens": ["'s", "war", "ei\u00b7ne", "Nacht", ",", "wo", "man", "am", "Mor\u00b7gen", "denkt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PWAV", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "++--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Ward Dasein jetzt dir, oder dort geschenkt?", "tokens": ["Ward", "Da\u00b7sein", "jetzt", "dir", ",", "o\u00b7der", "dort", "ge\u00b7schenkt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADV", "PPER", "$,", "KON", "ADV", "VVPP", "$."], "meter": "-----+-+-+", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Mir war so wohl und federleicht zu Muth,", "tokens": ["Mir", "war", "so", "wohl", "und", "fe\u00b7der\u00b7leicht", "zu", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "KON", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So schwimmend nun die Wimper halb geschlossen;", "tokens": ["So", "schwim\u00b7mend", "nun", "die", "Wim\u00b7per", "halb", "ge\u00b7schlos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verlorne Funken zuckten durch mein Blut,", "tokens": ["Ver\u00b7lor\u00b7ne", "Fun\u00b7ken", "zuck\u00b7ten", "durch", "mein", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von fernen Lauten w\u00e4hnt\u2019 ich mich umflossen;", "tokens": ["Von", "fer\u00b7nen", "Lau\u00b7ten", "w\u00e4hnt'", "ich", "mich", "um\u00b7flos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "\u2019s war eine Nacht, wo man am Morgen fragt:", "tokens": ["'s", "war", "ei\u00b7ne", "Nacht", ",", "wo", "man", "am", "Mor\u00b7gen", "fragt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PWAV", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "+---+-+-+-+", "measure": "dactylic.init"}, "line.6": {"text": "Hat\u2019s damals, oder hat es jetzt getagt?", "tokens": ["Hat's", "da\u00b7mals", ",", "o\u00b7der", "hat", "es", "jetzt", "ge\u00b7tagt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "KON", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und immer heller ward der s\u00fc\u00dfe Klang,", "tokens": ["Und", "im\u00b7mer", "hel\u00b7ler", "ward", "der", "s\u00fc\u00b7\u00dfe", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das liebe Lachen, es begann zu schwimmen", "tokens": ["Das", "lie\u00b7be", "La\u00b7chen", ",", "es", "be\u00b7gann", "zu", "schwim\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie Bilder von Daguerre die Deck\u2019 entlang,", "tokens": ["Wie", "Bil\u00b7der", "von", "Da\u00b7guer\u00b7re", "die", "Deck'", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NE", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Gleich Feuerw\u00fcrmern sah ich Augen glimmen,", "tokens": ["Gleich", "Feu\u00b7er\u00b7w\u00fcr\u00b7mern", "sah", "ich", "Au\u00b7gen", "glim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dann wurden feucht sie, blau und lind,", "tokens": ["Dann", "wur\u00b7den", "feucht", "sie", ",", "blau", "und", "lind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "PPER", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und mir zu F\u00fc\u00dfen sa\u00df ein sch\u00f6nes Kind.", "tokens": ["Und", "mir", "zu", "F\u00fc\u00b7\u00dfen", "sa\u00df", "ein", "sch\u00f6\u00b7nes", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Das sah zu mir empor, so ernst gespannt,", "tokens": ["Das", "sah", "zu", "mir", "em\u00b7por", ",", "so", "ernst", "ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "PTKVZ", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als quelle ihm die Seele aus den Blicken,", "tokens": ["Als", "quel\u00b7le", "ihm", "die", "See\u00b7le", "aus", "den", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bald schlo\u00df es, schmerzlich zuckend, seine Hand,", "tokens": ["Bald", "schlo\u00df", "es", ",", "schmerz\u00b7lich", "zu\u00b7ckend", ",", "sei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "VVPP", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bald sch\u00fcttelt es sie funkelnd vor Entz\u00fccken,", "tokens": ["Bald", "sch\u00fct\u00b7telt", "es", "sie", "fun\u00b7kelnd", "vor", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und horchend, horchend klomm es sacht heran", "tokens": ["Und", "hor\u00b7chend", ",", "hor\u00b7chend", "klomm", "es", "sacht", "he\u00b7ran"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "$,", "ADJD", "VVFIN", "PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zu meiner Schulter \u2014 und wo blieb es dann? \u2014", "tokens": ["Zu", "mei\u00b7ner", "Schul\u00b7ter", "und", "wo", "blieb", "es", "dann", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "KON", "PWAV", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "O w\u00e4ren\u2019s Geisterstimmen aus der Luft,", "tokens": ["O", "w\u00e4\u00b7ren's", "Geis\u00b7ters\u00b7tim\u00b7men", "aus", "der", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die sich wie Vogelzwitschern um mich reihten!", "tokens": ["Die", "sich", "wie", "Vo\u00b7gel\u00b7zwit\u00b7schern", "um", "mich", "reih\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "KOKOM", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "W\u00e4r\u2019 Grabesbrodem nur der leise Duft,", "tokens": ["W\u00e4r'", "Gra\u00b7bes\u00b7bro\u00b7dem", "nur", "der", "lei\u00b7se", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der mich umseufzte aus verschollnen Zeiten.", "tokens": ["Der", "mich", "um\u00b7seufz\u00b7te", "aus", "ver\u00b7scholl\u00b7nen", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch nur mein Herz ist eure stille Gruft,", "tokens": ["Doch", "nur", "mein", "Herz", "ist", "eu\u00b7re", "stil\u00b7le", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und meine Heil\u2019gen, meine einst Geweihten,", "tokens": ["Und", "mei\u00b7ne", "Heil'\u00b7gen", ",", "mei\u00b7ne", "einst", "Ge\u00b7weih\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPOSAT", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sie leben alle, wandeln allzumal.", "tokens": ["Sie", "le\u00b7ben", "al\u00b7le", ",", "wan\u00b7deln", "all\u00b7zu\u00b7mal", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Vielleicht zum Segen sich, doch mir zur Qual.", "tokens": ["Viel\u00b7leicht", "zum", "Se\u00b7gen", "sich", ",", "doch", "mir", "zur", "Qual", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PRF", "$,", "KON", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}