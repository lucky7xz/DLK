{"textgrid.poem.51137": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Schlechte Jahreszeit", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo ist der sch\u00f6ne Blumenflor,", "tokens": ["Wo", "ist", "der", "sch\u00f6\u00b7ne", "Blu\u00b7men\u00b7flor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den wir so treu gehegt?", "tokens": ["Den", "wir", "so", "treu", "ge\u00b7hegt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom Hoffen und vom Gr\u00fcnen sind", "tokens": ["Vom", "Hof\u00b7fen", "und", "vom", "Gr\u00fc\u00b7nen", "sind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Herz, Garten kahlgefegt!", "tokens": ["Herz", ",", "Gar\u00b7ten", "kahl\u00b7ge\u00b7fegt", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wie in ", "tokens": ["Und", "wie", "in"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWAV", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Ein ungl\u00fcckselig Haupt,", "tokens": ["Ein", "un\u00b7gl\u00fcck\u00b7se\u00b7lig", "Haupt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Hat sich heut nacht das Vaterland", "tokens": ["Hat", "sich", "heut", "nacht", "das", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PRF", "ADV", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gesch\u00fcttelt und entlaubt!", "tokens": ["Ge\u00b7sch\u00fct\u00b7telt", "und", "ent\u00b7laubt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Rhein entf\u00fchrt ins Niederland", "tokens": ["Der", "Rhein", "ent\u00b7f\u00fchrt", "ins", "Nie\u00b7der\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die welke Sommerlust,", "tokens": ["Die", "wel\u00b7ke", "Som\u00b7mer\u00b7lust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "L\u00e4\u00dft \u00f6d und fahl die Felder uns,", "tokens": ["L\u00e4\u00dft", "\u00f6d", "und", "fahl", "die", "Fel\u00b7der", "uns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Frost in unsrer Brust.", "tokens": ["Den", "Frost", "in", "uns\u00b7rer", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Silberfirnen h\u00fcllen sich", "tokens": ["Die", "Sil\u00b7ber\u00b7fir\u00b7nen", "h\u00fcl\u00b7len", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In dunkle Wolken ein;", "tokens": ["In", "dunk\u00b7le", "Wol\u00b7ken", "ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch bald wird jeder Kehricht nun", "tokens": ["Doch", "bald", "wird", "je\u00b7der", "Keh\u00b7richt", "nun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PIAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein blanker Schneeberg sein!", "tokens": ["Ein", "blan\u00b7ker", "Schnee\u00b7berg", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und alles wird so klein, so nah,", "tokens": ["Und", "al\u00b7les", "wird", "so", "klein", ",", "so", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So dumpf und eingezw\u00e4ngt;", "tokens": ["So", "dumpf", "und", "ein\u00b7ge\u00b7zw\u00e4ngt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie dr\u00fcckend ob dem Scheitel uns", "tokens": ["Wie", "dr\u00fc\u00b7ckend", "ob", "dem", "Schei\u00b7tel", "uns"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "KOUS", "ART", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der graue Himmel h\u00e4ngt!", "tokens": ["Der", "grau\u00b7e", "Him\u00b7mel", "h\u00e4ngt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auf jedem Kreuzweg sitzt ein Feind \u2013", "tokens": ["Auf", "je\u00b7dem", "Kreuz\u00b7weg", "sitzt", "ein", "Feind", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es ist ein harter Stand:", "tokens": ["Es", "ist", "ein", "har\u00b7ter", "Stand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mit Schurken atmen gleiche Luft", "tokens": ["Mit", "Schur\u00b7ken", "at\u00b7men", "glei\u00b7che", "Luft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Im engen Vaterland!", "tokens": ["Im", "en\u00b7gen", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}