{"textgrid.poem.47476": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[es sind die Wege Gottes wunderbar]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sind die Wege Gottes wunderbar,", "tokens": ["Es", "sind", "die", "We\u00b7ge", "Got\u00b7tes", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und ich erprob' es in besonderm Grade;", "tokens": ["Und", "ich", "er\u00b7prob'", "es", "in", "be\u00b7son\u00b7derm", "Gra\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch sp\u00fcr' ich auch im Wirbel der Gefahr,", "tokens": ["Doch", "sp\u00fcr'", "ich", "auch", "im", "Wir\u00b7bel", "der", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der lichtberaubten, seine reiche Gnade;", "tokens": ["Der", "licht\u00b7be\u00b7raub\u00b7ten", ",", "sei\u00b7ne", "rei\u00b7che", "Gna\u00b7de", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es irrt mein Herz in deinem Lockenhaar,", "tokens": ["Es", "irrt", "mein", "Herz", "in", "dei\u00b7nem", "Lo\u00b7cken\u00b7haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da leitet es der Herr die dunkeln Pfade.", "tokens": ["Da", "lei\u00b7tet", "es", "der", "Herr", "die", "dun\u00b7keln", "Pfa\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}