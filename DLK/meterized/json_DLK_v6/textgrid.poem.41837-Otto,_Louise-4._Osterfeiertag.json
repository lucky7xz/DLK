{"textgrid.poem.41837": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "4. Osterfeiertag", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Turme t\u00f6nt in stiller Sabbathfr\u00fche", "tokens": ["Vom", "Tur\u00b7me", "t\u00f6nt", "in", "stil\u00b7ler", "Sab\u00b7bath\u00b7fr\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Posaunengru\u00df: der Herr ist auferstanden!", "tokens": ["Po\u00b7sau\u00b7nen\u00b7gru\u00df", ":", "der", "Herr", "ist", "auf\u00b7er\u00b7stan\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er liegt nicht mehr in finstern Grabesbanden;", "tokens": ["Er", "liegt", "nicht", "mehr", "in", "fins\u00b7tern", "Gra\u00b7bes\u00b7ban\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da wars, als wenn der Himmel purpurn gl\u00fche.", "tokens": ["Da", "wars", ",", "als", "wenn", "der", "Him\u00b7mel", "pur\u00b7purn", "gl\u00fc\u00b7he", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOKOM", "KOUS", "ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Allm\u00e4hlich schiens, als ob er Funken spr\u00fche,", "tokens": ["All\u00b7m\u00e4h\u00b7lich", "schiens", ",", "als", "ob", "er", "Fun\u00b7ken", "spr\u00fc\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOKOM", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Lerchen aufw\u00e4rts Jubelgr\u00fc\u00dfe sandten,", "tokens": ["Die", "Ler\u00b7chen", "auf\u00b7w\u00e4rts", "Ju\u00b7bel\u00b7gr\u00fc\u00b7\u00dfe", "sand\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Veilchenaug' sich goldne Tropfen fanden,", "tokens": ["Im", "Veil\u00b7che\u00b7naug'", "sich", "gold\u00b7ne", "Trop\u00b7fen", "fan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und jede Knospe tr\u00e4umte, da\u00df sie bl\u00fche.", "tokens": ["Und", "je\u00b7de", "Knos\u00b7pe", "tr\u00e4um\u00b7te", ",", "da\u00df", "sie", "bl\u00fc\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Solch eine Feier mahnt beklommne Herzen,", "tokens": ["Solch", "ei\u00b7ne", "Fei\u00b7er", "mahnt", "be\u00b7klomm\u00b7ne", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So bl\u00fchend, gl\u00fchend, und so sonnenhaft", "tokens": ["So", "bl\u00fc\u00b7hend", ",", "gl\u00fc\u00b7hend", ",", "und", "so", "son\u00b7nen\u00b7haft"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVPP", "$,", "ADJD", "$,", "KON", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein neues Leben freudig zu beginnen.", "tokens": ["Ein", "neu\u00b7es", "Le\u00b7ben", "freu\u00b7dig", "zu", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Das Grab, das Kreuz und alle bange Schmerzen", "tokens": ["Das", "Grab", ",", "das", "Kreuz", "und", "al\u00b7le", "ban\u00b7ge", "Schmer\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind \u00fcberwunden von der Gottheit Kraft.", "tokens": ["Sind", "\u00fc\u00b7berw\u00b7un\u00b7den", "von", "der", "Got\u00b7theit", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Triumph erschallt und Freudenthr\u00e4nen rinnen.", "tokens": ["Tri\u00b7umph", "er\u00b7schallt", "und", "Freu\u00b7dent\u00b7hr\u00e4\u00b7nen", "rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}