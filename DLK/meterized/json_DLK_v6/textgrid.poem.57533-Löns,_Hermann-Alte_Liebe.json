{"textgrid.poem.57533": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Alte Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die stillen, dunklen Erlenteiche", "tokens": ["Die", "stil\u00b7len", ",", "dunk\u00b7len", "Er\u00b7len\u00b7tei\u00b7che"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hab' ich geliebt von Jugend an,", "tokens": ["Hab'", "ich", "ge\u00b7liebt", "von", "Ju\u00b7gend", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In ihrem d\u00fcstern M\u00e4rchenreiche", "tokens": ["In", "ih\u00b7rem", "d\u00fcs\u00b7tern", "M\u00e4r\u00b7chen\u00b7rei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich manchen sch\u00f6nen Traum ersann.", "tokens": ["Ich", "man\u00b7chen", "sch\u00f6\u00b7nen", "Traum", "er\u00b7sann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Tr\u00e4ume sind in Nichts zerflogen,", "tokens": ["Die", "Tr\u00e4u\u00b7me", "sind", "in", "Nichts", "zer\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein frohes Antlitz wurde bleich,", "tokens": ["Mein", "fro\u00b7hes", "Ant\u00b7litz", "wur\u00b7de", "bleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch f\u00fchl ich stets mich hingezogen", "tokens": ["Doch", "f\u00fchl", "ich", "stets", "mich", "hin\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum stillen, dunklen Erlenteich.", "tokens": ["Zum", "stil\u00b7len", ",", "dunk\u00b7len", "Er\u00b7len\u00b7teich", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}