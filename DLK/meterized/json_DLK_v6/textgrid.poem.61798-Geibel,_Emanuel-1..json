{"textgrid.poem.61798": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich jung war, da trieb's mich", "tokens": ["Als", "ich", "jung", "war", ",", "da", "trie\u00b7b's", "mich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,", "KOUS", "NE", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00dcber Land, \u00fcber Meer,", "tokens": ["\u00dc\u00b7ber", "Land", ",", "\u00fc\u00b7ber", "Meer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Mit den Schwalben zu wandern", "tokens": ["Mit", "den", "Schwal\u00b7ben", "zu", "wan\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "War all mein Begehr.", "tokens": ["War", "all", "mein", "Be\u00b7gehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Und das Land der Zitronen", "tokens": ["Und", "das", "Land", "der", "Zit\u00b7ro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und die marmornen H\u00f6hn", "tokens": ["Und", "die", "mar\u00b7mor\u00b7nen", "H\u00f6hn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und die Palmen von Hellas", "tokens": ["Und", "die", "Pal\u00b7men", "von", "Hel\u00b7las"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NE"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Nur deuchten mir sch\u00f6n.", "tokens": ["Nur", "deuch\u00b7ten", "mir", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Doch die Unrast der Jugend,", "tokens": ["Doch", "die", "Un\u00b7rast", "der", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wie schwand sie dahin!", "tokens": ["Wie", "schwand", "sie", "da\u00b7hin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PAV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Heimkehrte der Mann", "tokens": ["Heim\u00b7kehr\u00b7te", "der", "Mann"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Mit verwandeltem Sinn.", "tokens": ["Mit", "ver\u00b7wan\u00b7del\u00b7tem", "Sinn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Jetzt wei\u00df ich, was tiefer", "tokens": ["Jetzt", "wei\u00df", "ich", ",", "was", "tie\u00b7fer"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Gen\u00fcgen mir schafft:", "tokens": ["Ge\u00b7n\u00fc\u00b7gen", "mir", "schafft", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "In den Boden gewurzelt", "tokens": ["In", "den", "Bo\u00b7den", "ge\u00b7wur\u00b7zelt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Zu \u00fcben die Kraft,", "tokens": ["Zu", "\u00fc\u00b7ben", "die", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Zum Gesange zu reifen,", "tokens": ["Zum", "Ge\u00b7san\u00b7ge", "zu", "rei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Was still mich durchgl\u00fcht,", "tokens": ["Was", "still", "mich", "durch\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und ein Echo zu wecken", "tokens": ["Und", "ein", "E\u00b7cho", "zu", "we\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PTKZU", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Im deutschen Gem\u00fct.", "tokens": ["Im", "deut\u00b7schen", "Ge\u00b7m\u00fct", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Und ob ich im Lied wohl", "tokens": ["Und", "ob", "ich", "im", "Lied", "wohl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPRART", "NN", "ADV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Fremde noch gr\u00fc\u00df',", "tokens": ["Die", "Frem\u00b7de", "noch", "gr\u00fc\u00df'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Doch ist wie die Heimat", "tokens": ["Doch", "ist", "wie", "die", "Hei\u00b7mat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "KOKOM", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Kein Land mir so s\u00fc\u00df.", "tokens": ["Kein", "Land", "mir", "so", "s\u00fc\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Wo der Buchenwald rauscht", "tokens": ["Wo", "der", "Bu\u00b7chen\u00b7wald", "rauscht"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN"], "meter": "--+-++", "measure": "anapaest.init"}, "line.2": {"text": "Und der Dorn bl\u00fcht am Zaun", "tokens": ["Und", "der", "Dorn", "bl\u00fcht", "am", "Zaun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und ins Meer geht die Trave,", "tokens": ["Und", "ins", "Meer", "geht", "die", "Tra\u00b7ve", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+-++-", "measure": "anapaest.init"}, "line.4": {"text": "La\u00dft H\u00fctten mich baun!", "tokens": ["La\u00dft", "H\u00fct\u00b7ten", "mich", "baun", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "PPER", "VVINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}