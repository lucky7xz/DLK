{"textgrid.poem.34825": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie hatten sich beide so herzlich lieb,", "tokens": ["Sie", "hat\u00b7ten", "sich", "bei\u00b7de", "so", "herz\u00b7lich", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "PIS", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Spitzb\u00fcbin war sie, er war ein Dieb.", "tokens": ["Spitz\u00b7b\u00fc\u00b7bin", "war", "sie", ",", "er", "war", "ein", "Dieb", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "+-----+-+", "measure": "dactylic.init"}, "line.3": {"text": "Wenn er Schelmenstreiche machte,", "tokens": ["Wenn", "er", "Schel\u00b7men\u00b7strei\u00b7che", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie warf sich aufs Bette und lachte.", "tokens": ["Sie", "warf", "sich", "aufs", "Bet\u00b7te", "und", "lach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "KON", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Der Tag verging in Freud und Lust,", "tokens": ["Der", "Tag", "ver\u00b7ging", "in", "Freud", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Nachts lag sie an seiner Brust.", "tokens": ["Des", "Nachts", "lag", "sie", "an", "sei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Als man ins Gef\u00e4ngnis ihn brachte,", "tokens": ["Als", "man", "ins", "Ge\u00b7f\u00e4ng\u00b7nis", "ihn", "brach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sie stand am Fenster und lachte.", "tokens": ["Sie", "stand", "am", "Fens\u00b7ter", "und", "lach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Er lie\u00df ihr sagen: \u00bbO komm zu mir,", "tokens": ["Er", "lie\u00df", "ihr", "sa\u00b7gen", ":", "\u00bb", "O", "komm", "zu", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$.", "$(", "NE", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich sehne mich so sehr nach dir,", "tokens": ["Ich", "seh\u00b7ne", "mich", "so", "sehr", "nach", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich rufe nach dir, ich schmachte\u00ab \u2013", "tokens": ["Ich", "ru\u00b7fe", "nach", "dir", ",", "ich", "schmach\u00b7te", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "PPER", "VVFIN", "$(", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sie sch\u00fcttelt' das Haupt und lachte.", "tokens": ["Sie", "sch\u00fct\u00b7telt'", "das", "Haupt", "und", "lach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Um sechse des Morgens ward er gehenkt,", "tokens": ["Um", "sech\u00b7se", "des", "Mor\u00b7gens", "ward", "er", "ge\u00b7henkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ART", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Um sieben ward er ins Grab gesenkt;", "tokens": ["Um", "sie\u00b7ben", "ward", "er", "ins", "Grab", "ge\u00b7senkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie aber schon um achte", "tokens": ["Sie", "a\u00b7ber", "schon", "um", "ach\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Trank roten Wein und lachte.", "tokens": ["Trank", "ro\u00b7ten", "Wein", "und", "lach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}