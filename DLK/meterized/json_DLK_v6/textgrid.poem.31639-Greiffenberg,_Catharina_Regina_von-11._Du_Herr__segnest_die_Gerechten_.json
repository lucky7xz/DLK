{"textgrid.poem.31639": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "11. Du Herr / segnest die Gerechten /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df nur fluchen / w\u00fcten / toben / Belial und seine Schaar /", "tokens": ["La\u00df", "nur", "flu\u00b7chen", "/", "w\u00fc\u00b7ten", "/", "to\u00b7ben", "/", "Be\u00b7li\u00b7al", "und", "sei\u00b7ne", "Schaar", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "VVINF", "$(", "VVFIN", "$(", "VVFIN", "$(", "NN", "KON", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "wann Gott die Gerechten segnet / hat es mit uns kein Gefahr:", "tokens": ["wann", "Gott", "die", "Ge\u00b7rech\u00b7ten", "seg\u00b7net", "/", "hat", "es", "mit", "uns", "kein", "Ge\u00b7fahr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "VVFIN", "$(", "VAFIN", "PPER", "APPR", "PPER", "PIAT", "NN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "dann in Ihm seynd wir Gerecht / und derhalb in seinem Segen.", "tokens": ["dann", "in", "Ihm", "seynd", "wir", "Ge\u00b7recht", "/", "und", "der\u00b7halb", "in", "sei\u00b7nem", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VAFIN", "PPER", "NN", "$(", "KON", "PAV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "mag sich nun ein ganzes Heer wider uns ger\u00fcstet legen:", "tokens": ["mag", "sich", "nun", "ein", "gan\u00b7zes", "Heer", "wi\u00b7der", "uns", "ge\u00b7r\u00fcs\u00b7tet", "le\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADV", "ART", "ADJA", "NN", "APPR", "PPER", "VVPP", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Gottes Gnaden- starker Schild / schirmet unser Haupt im Streit /", "tokens": ["Got\u00b7tes", "Gna\u00b7den", "star\u00b7ker", "Schild", "/", "schir\u00b7met", "un\u00b7ser", "Haupt", "im", "Streit", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "TRUNC", "ADJA", "NN", "$(", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$("], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "macht die Bo\u00dfheit-Pfeil zerspringen / schenket Sieg-und Friedens-Freud.", "tokens": ["macht", "die", "Bo\u00df\u00b7heit\u00b7Pfeil", "zer\u00b7sprin\u00b7gen", "/", "schen\u00b7ket", "Sieg\u00b7\u00b7und", "Frie\u00b7dens\u00b7Freud", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "$(", "VVFIN", "NE", "NE", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}