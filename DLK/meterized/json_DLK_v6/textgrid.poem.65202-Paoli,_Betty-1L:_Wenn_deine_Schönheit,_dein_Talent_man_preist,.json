{"textgrid.poem.65202": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn deine Sch\u00f6nheit, dein Talent man preist,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn deine Sch\u00f6nheit, dein Talent man preist,", "tokens": ["Wenn", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", ",", "dein", "Ta\u00b7lent", "man", "preist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sei der Gedanke stets dir gegenw\u00e4rtig:", "tokens": ["Sei", "der", "Ge\u00b7dan\u00b7ke", "stets", "dir", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Leben wird mit allem, allem fertig,", "tokens": ["Das", "Le\u00b7ben", "wird", "mit", "al\u00b7lem", ",", "al\u00b7lem", "fer\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PIS", "$,", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wie das Antlitz altert auch der Geist.", "tokens": ["Und", "wie", "das", "Ant\u00b7litz", "al\u00b7tert", "auch", "der", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du meinst: \u00bbVerschmerzen l\u00e4\u00dft sich der Verlust,", "tokens": ["Du", "meinst", ":", "\u00bb", "Ver\u00b7schmer\u00b7zen", "l\u00e4\u00dft", "sich", "der", "Ver\u00b7lust", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$.", "$(", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Zeit mag ihres strengen Amtes walten,", "tokens": ["Die", "Zeit", "mag", "ih\u00b7res", "stren\u00b7gen", "Am\u00b7tes", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bleibt mir nur eins, das K\u00f6stlichste, erhalten:", "tokens": ["Bleibt", "mir", "nur", "eins", ",", "das", "K\u00f6st\u00b7lichs\u00b7te", ",", "er\u00b7hal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIS", "$,", "ART", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die tiefe Liebeskraft in meiner Brust!\u00ab", "tokens": ["Die", "tie\u00b7fe", "Lie\u00b7bes\u00b7kraft", "in", "mei\u00b7ner", "Brust", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So wisse: m\u00fcd, ersch\u00f6pft und abgehetzt", "tokens": ["So", "wis\u00b7se", ":", "m\u00fcd", ",", "er\u00b7sch\u00f6pft", "und", "ab\u00b7ge\u00b7hetzt"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$.", "ADJD", "$,", "VVPP", "KON", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fchlst du dereinst auch diese Kraft dir schwinden,", "tokens": ["F\u00fchlst", "du", "de\u00b7reinst", "auch", "die\u00b7se", "Kraft", "dir", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PDAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein Herz vertrocknet, stumpf wird dein Empfinden,", "tokens": ["Dein", "Herz", "ver\u00b7trock\u00b7net", ",", "stumpf", "wird", "dein", "Emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht lieben kannst du mehr \u2013 \u2013 was bleibt zuletzt?!", "tokens": ["Nicht", "lie\u00b7ben", "kannst", "du", "mehr", "\u2013", "\u2013", "was", "bleibt", "zu\u00b7letzt", "?!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "PPER", "ADV", "$(", "$(", "PWS", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}