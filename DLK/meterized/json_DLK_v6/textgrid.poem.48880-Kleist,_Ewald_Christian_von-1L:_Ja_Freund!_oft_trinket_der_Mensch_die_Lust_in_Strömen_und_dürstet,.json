{"textgrid.poem.48880": {"metadata": {"author": {"name": "Kleist, Ewald Christian von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja Freund! oft trinket der Mensch die Lust in Str\u00f6men und d\u00fcrstet,", "genre": "verse", "period": "N.A.", "pub_year": 1737, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja Freund! oft trinket der Mensch die Lust in Str\u00f6men und d\u00fcrstet,", "tokens": ["Ja", "Freund", "!", "oft", "trin\u00b7ket", "der", "Mensch", "die", "Lust", "in", "Str\u00f6\u00b7men", "und", "d\u00fcrs\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der Gl\u00fccklichste stirbt unter W\u00fcnschen; ein Tropfen Kummers verbittert", "tokens": ["Der", "Gl\u00fcck\u00b7lichs\u00b7te", "stirbt", "un\u00b7ter", "W\u00fcn\u00b7schen", ";", "ein", "Trop\u00b7fen", "Kum\u00b7mers", "ver\u00b7bit\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$.", "ART", "NN", "NN", "VVPP"], "meter": "-+--+--+--+-+--+-", "measure": "amphibrach.tetra.plus"}, "line.3": {"text": "Ihm ganze Meere von Freude. Die Einbildung spornt seine Triebe,", "tokens": ["Ihm", "gan\u00b7ze", "Mee\u00b7re", "von", "Freu\u00b7de", ".", "Die", "Ein\u00b7bil\u00b7dung", "spornt", "sei\u00b7ne", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "NN", "$.", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Wie R\u00f6sse reissen sie aus, die Zwang und Z\u00fcgel verachten,", "tokens": ["Wie", "R\u00f6s\u00b7se", "reis\u00b7sen", "sie", "aus", ",", "die", "Zwang", "und", "Z\u00fc\u00b7gel", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Und ziehn ihn mit sich zum Abgrund. Sein Stolz zielt immer gen Himmel.", "tokens": ["Und", "ziehn", "ihn", "mit", "sich", "zum", "Ab\u00b7grund", ".", "Sein", "Stolz", "zielt", "im\u00b7mer", "gen", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PRF", "APPRART", "NN", "$.", "PPOSAT", "NN", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+--+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Bald schilt er die Vorsicht, die ihn in Purpur und Reichthum verabs\u00e4umt,", "tokens": ["Bald", "schilt", "er", "die", "Vor\u00b7sicht", ",", "die", "ihn", "in", "Pur\u00b7pur", "und", "Reicht\u00b7hum", "ver\u00b7ab\u00b7s\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+--+--+-+--+--+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Bald d\u00fcnkt er sich selber zu schwach und tadelt die Weisheit der Sch\u00f6pfung:", "tokens": ["Bald", "d\u00fcnkt", "er", "sich", "sel\u00b7ber", "zu", "schwach", "und", "ta\u00b7delt", "die", "Weis\u00b7heit", "der", "Sch\u00f6p\u00b7fung", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "PTKA", "ADJD", "KON", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+--+--+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Das Feuer haucht Plagen f\u00fcr ihn, ihm bl\u00fcht auf Auen das Ungl\u00fcck,", "tokens": ["Das", "Feu\u00b7er", "haucht", "Pla\u00b7gen", "f\u00fcr", "ihn", ",", "ihm", "bl\u00fcht", "auf", "Au\u00b7en", "das", "Un\u00b7gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "APPR", "PPER", "$,", "PPER", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+-+--+-", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Und eilt mit Fluthen heran, die Wind' umwehn ihn mit Schmerzen.", "tokens": ["Und", "eilt", "mit", "Flut\u00b7hen", "he\u00b7ran", ",", "die", "Wind'", "um\u00b7wehn", "ihn", "mit", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Wohin verwegnes Gesch\u00f6pfe? Denkst du wie Riesen der Fabel", "tokens": ["Wo\u00b7hin", "ver\u00b7weg\u00b7nes", "Ge\u00b7sch\u00f6p\u00b7fe", "?", "Denkst", "du", "wie", "Rie\u00b7sen", "der", "Fa\u00b7bel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "$.", "VVFIN", "PPER", "KOKOM", "NN", "ART", "NN"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Auf Felsen Felsen zu h\u00e4ufen, und durch den Unsinn bewafnet,", "tokens": ["Auf", "Fel\u00b7sen", "Fel\u00b7sen", "zu", "h\u00e4u\u00b7fen", ",", "und", "durch", "den", "Un\u00b7sinn", "be\u00b7waf\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PTKZU", "VVINF", "$,", "KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Den Sitz der Gottheit zu st\u00fcrmen? Will ein Gef\u00e4sse von Leimen", "tokens": ["Den", "Sitz", "der", "Got\u00b7theit", "zu", "st\u00fcr\u00b7men", "?", "Will", "ein", "Ge\u00b7f\u00e4s\u00b7se", "von", "Lei\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$.", "VMFIN", "ART", "NN", "APPR", "NN"], "meter": "-+--+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Sich wider den T\u00f6pfer emp\u00f6ren? Durchfleuch erst die blauen Gefilde", "tokens": ["Sich", "wi\u00b7der", "den", "T\u00f6p\u00b7fer", "em\u00b7p\u00f6\u00b7ren", "?", "Durch\u00b7fleuch", "erst", "die", "blau\u00b7en", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN", "VVINF", "$.", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-+-+--+-", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Mit Sonnen und Erden durchs\u00e4t, den milchfarbnen G\u00fcrtel des Himmels,", "tokens": ["Mit", "Son\u00b7nen", "und", "Er\u00b7den", "durch\u00b7s\u00e4t", ",", "den", "milch\u00b7farb\u00b7nen", "G\u00fcr\u00b7tel", "des", "Him\u00b7mels", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+--+--+-", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "Die Luftsph\u00e4r jeglichen Sterns, betrachte des Ganzen Verbindung,", "tokens": ["Die", "Luft\u00b7sph\u00e4r", "jeg\u00b7li\u00b7chen", "Sterns", ",", "be\u00b7trach\u00b7te", "des", "Gan\u00b7zen", "Ver\u00b7bin\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Sammt allen Federn der R\u00e4der und andrer Planeten Naturen,", "tokens": ["Sammt", "al\u00b7len", "Fe\u00b7dern", "der", "R\u00e4\u00b7der", "und", "an\u00b7drer", "Pla\u00b7ne\u00b7ten", "Na\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ART", "NN", "KON", "ADJA", "NN", "NN", "$,"], "meter": "+--+--+--+-+-+-+-", "measure": "dactylic.tri.plus"}, "line.17": {"text": "Die Arten ihrer Bewohner, ihr Thun und Stufengefolge,", "tokens": ["Die", "Ar\u00b7ten", "ih\u00b7rer", "Be\u00b7woh\u00b7ner", ",", "ihr", "Thun", "und", "Stu\u00b7fen\u00b7ge\u00b7fol\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Ergr\u00fcnd mit k\u00fchnem Gefieder des dunkeln Geisterreichs Tiefe.", "tokens": ["Er\u00b7gr\u00fcnd", "mit", "k\u00fch\u00b7nem", "Ge\u00b7fie\u00b7der", "des", "dun\u00b7keln", "Geis\u00b7ter\u00b7reichs", "Tie\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Sieh Wesen ohne Gestalten, merk ihre Abh\u00e4ng und Kr\u00e4fte,", "tokens": ["Sieh", "We\u00b7sen", "oh\u00b7ne", "Ge\u00b7stal\u00b7ten", ",", "merk", "ih\u00b7re", "Ab\u00b7h\u00e4ng", "und", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "NN", "$,", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Steig auf der Leiter der Dinge selbst bis zum Throne der Gottheit;", "tokens": ["Steig", "auf", "der", "Lei\u00b7ter", "der", "Din\u00b7ge", "selbst", "bis", "zum", "Thro\u00b7ne", "der", "Got\u00b7theit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "ADV", "APPR", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+--+-+-+", "measure": "dactylic.di.plus"}, "line.21": {"text": "Dann strafe, woferne du kanst, die Vorsicht und Ordnung der Erde.", "tokens": ["Dann", "stra\u00b7fe", ",", "wo\u00b7fer\u00b7ne", "du", "kanst", ",", "die", "Vor\u00b7sicht", "und", "Ord\u00b7nung", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "VMFIN", "$,", "ART", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+--+--+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 Willst du die Ursach erforschen,", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "Willst", "du", "die", "Ur\u00b7sach", "er\u00b7for\u00b7schen", ","], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Warum du kein Seraphim wurdest? Entdeck erst Stolzer! weswegen", "tokens": ["Wa\u00b7rum", "du", "kein", "Se\u00b7ra\u00b7phim", "wur\u00b7dest", "?", "Ent\u00b7deck", "erst", "Stol\u00b7zer", "!", "wes\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "PPER", "PIAT", "NN", "VAFIN", "$.", "NN", "ADV", "NN", "$.", "PWAV"], "meter": "-+--+--+--+-+--+-", "measure": "amphibrach.tetra.plus"}, "line.3": {"text": "Du keine Milbe geworden. Soll deiner Thorheit zum Vortheil", "tokens": ["Du", "kei\u00b7ne", "Mil\u00b7be", "ge\u00b7wor\u00b7den", ".", "Soll", "dei\u00b7ner", "Thor\u00b7heit", "zum", "Vor\u00b7theil"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "PIAT", "NN", "VAPP", "$.", "VMFIN", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die grosse Weltkette brechen, und tausend Planeten und Sonnen", "tokens": ["Die", "gros\u00b7se", "Welt\u00b7ket\u00b7te", "bre\u00b7chen", ",", "und", "tau\u00b7send", "Pla\u00b7ne\u00b7ten", "und", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,", "KON", "CARD", "NN", "KON", "NN"], "meter": "-+-+--+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Aus ihren Gleisen ger\u00fcckt, in einen Klumpen zerfallen?", "tokens": ["Aus", "ih\u00b7ren", "Glei\u00b7sen", "ge\u00b7r\u00fcckt", ",", "in", "ei\u00b7nen", "Klum\u00b7pen", "zer\u00b7fal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Soll bis zum Throne des H\u00f6chsten des Himmels Vorhang zerreissen?", "tokens": ["Soll", "bis", "zum", "Thro\u00b7ne", "des", "H\u00f6chs\u00b7ten", "des", "Him\u00b7mels", "Vor\u00b7hang", "zer\u00b7reis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "APPRART", "NN", "ART", "NN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Und endlich die ganze Natur ersch\u00fcttert zum Innersten seufzen?", "tokens": ["Und", "end\u00b7lich", "die", "gan\u00b7ze", "Na\u00b7tur", "er\u00b7sch\u00fct\u00b7tert", "zum", "In\u00b7ners\u00b7ten", "seuf\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VVPP", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Die\u00df wilst du, wenn du verlangst, was mit der Weltordnung streitet.", "tokens": ["Die\u00df", "wilst", "du", ",", "wenn", "du", "ver\u00b7langst", ",", "was", "mit", "der", "Wel\u00b7tord\u00b7nung", "strei\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Sey deiner Neigungen Herr, so wirst du das Ungl\u00fcck beherrschen,", "tokens": ["Sey", "dei\u00b7ner", "Nei\u00b7gun\u00b7gen", "Herr", ",", "so", "wirst", "du", "das", "Un\u00b7gl\u00fcck", "be\u00b7herr\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$,", "ADV", "VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-++--+-", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Der Sch\u00f6pfer ist Liebe und Huld, nur die sind deine Tyrannen.", "tokens": ["Der", "Sch\u00f6p\u00b7fer", "ist", "Lie\u00b7be", "und", "Huld", ",", "nur", "die", "sind", "dei\u00b7ne", "Ty\u00b7ran\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "KON", "NN", "$,", "ADV", "ART", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+-+--+-", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Was baut ihr H\u00e4user auf Wellen, ihr Diebe der indischen Berge,", "tokens": ["Was", "baut", "ihr", "H\u00e4u\u00b7ser", "auf", "Wel\u00b7len", ",", "ihr", "Die\u00b7be", "der", "in\u00b7di\u00b7schen", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "Verdammt euch Jahre lang, nichts als nasse Gr\u00e4ber zu sehen,", "tokens": ["Ver\u00b7dammt", "euch", "Jah\u00b7re", "lang", ",", "nichts", "als", "nas\u00b7se", "Gr\u00e4\u00b7ber", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADJD", "$,", "PIS", "KOKOM", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Und in den Wolken den Tod? Du Untersucher der Gr\u00fcnde,", "tokens": ["Und", "in", "den", "Wol\u00b7ken", "den", "Tod", "?", "Du", "Un\u00b7ter\u00b7su\u00b7cher", "der", "Gr\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$.", "PPER", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Was blickst du hohnl\u00