{"textgrid.poem.43521": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mein Hans und mein Peter", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Hans und mein Peter sind beide so brav,", "tokens": ["Mein", "Hans", "und", "mein", "Pe\u00b7ter", "sind", "bei\u00b7de", "so", "brav", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "KON", "PPOSAT", "NN", "VAFIN", "PIS", "ADV", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Es giebt doch auf Erden kein braveres Schaf.", "tokens": ["Es", "giebt", "doch", "auf", "Er\u00b7den", "kein", "bra\u00b7ve\u00b7res", "Schaf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "PIAT", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Mein Hans und mein Peter, die sind nicht so dumm:", "tokens": ["Mein", "Hans", "und", "mein", "Pe\u00b7ter", ",", "die", "sind", "nicht", "so", "dumm", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "KON", "PPOSAT", "NE", "$,", "PRELS", "VAFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sobald ich sie locke, so dreh'n sie sich um.", "tokens": ["So\u00b7bald", "ich", "sie", "lo\u00b7cke", ",", "so", "dreh'n", "sie", "sich", "um", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Mein Hans und mein Peter erscheinen im Nu,", "tokens": ["Mein", "Hans", "und", "mein", "Pe\u00b7ter", "er\u00b7schei\u00b7nen", "im", "Nu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "KON", "PPOSAT", "NE", "VVFIN", "APPRART", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sie tanzen und l\u00e4uten die Gl\u00f6ckchen dazu.", "tokens": ["Sie", "tan\u00b7zen", "und", "l\u00e4u\u00b7ten", "die", "Gl\u00f6ck\u00b7chen", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Mein Hans und mein Peter sind artig und fein,", "tokens": ["Mein", "Hans", "und", "mein", "Pe\u00b7ter", "sind", "ar\u00b7tig", "und", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "KON", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Denn lauf' ich, so folgen sie flink hinterdrein.", "tokens": ["Denn", "lauf'", "ich", ",", "so", "fol\u00b7gen", "sie", "flink", "hin\u00b7ter\u00b7drein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Mein Hans und mein Peter, die sind nicht zu Kauf,", "tokens": ["Mein", "Hans", "und", "mein", "Pe\u00b7ter", ",", "die", "sind", "nicht", "zu", "Kauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "KON", "PPOSAT", "NE", "$,", "PRELS", "VAFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und bietet ihr hundert Dukaten darauf!", "tokens": ["Und", "bie\u00b7tet", "ihr", "hun\u00b7dert", "Du\u00b7ka\u00b7ten", "da\u00b7rauf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "CARD", "NN", "PAV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}