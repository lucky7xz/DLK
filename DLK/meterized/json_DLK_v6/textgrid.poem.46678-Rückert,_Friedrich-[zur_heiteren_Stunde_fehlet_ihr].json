{"textgrid.poem.46678": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[zur heiteren Stunde fehlet ihr]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zur heiteren Stunde fehlet ihr,", "tokens": ["Zur", "hei\u00b7te\u00b7ren", "Stun\u00b7de", "feh\u00b7let", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zum fr\u00f6hlichen Bunde fehlet ihr.", "tokens": ["Zum", "fr\u00f6h\u00b7li\u00b7chen", "Bun\u00b7de", "feh\u00b7let", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Den Sommer k\u00fcndigen Schwalben an,", "tokens": ["Den", "Som\u00b7mer", "k\u00fcn\u00b7di\u00b7gen", "Schwal\u00b7ben", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der freudigen Kunde fehlet ihr.", "tokens": ["Der", "freu\u00b7di\u00b7gen", "Kun\u00b7de", "feh\u00b7let", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die Blumen im Wiesengrunde bl\u00fchn,", "tokens": ["Die", "Blu\u00b7men", "im", "Wie\u00b7sen\u00b7grun\u00b7de", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Dem bl\u00fchenden Grunde fehlet ihr.", "tokens": ["Dem", "bl\u00fc\u00b7hen\u00b7den", "Grun\u00b7de", "feh\u00b7let", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Mit lachendem Mund gehn Rosen auf,", "tokens": ["Mit", "la\u00b7chen\u00b7dem", "Mund", "gehn", "Ro\u00b7sen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Mit lachendem Munde fehlet ihr.", "tokens": ["Mit", "la\u00b7chen\u00b7dem", "Mun\u00b7de", "feh\u00b7let", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Gefunden hat Gl\u00fcck und Lust die Welt,", "tokens": ["Ge\u00b7fun\u00b7den", "hat", "Gl\u00fcck", "und", "Lust", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Zum gl\u00fccklichen Funde fehlet ihr.", "tokens": ["Zum", "gl\u00fcck\u00b7li\u00b7chen", "Fun\u00b7de", "feh\u00b7let", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Die Br\u00fcder schlingen den Reihentanz;", "tokens": ["Die", "Br\u00fc\u00b7der", "schlin\u00b7gen", "den", "Rei\u00b7hen\u00b7tanz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Warum in der Runde fehlet ihr?", "tokens": ["Wa\u00b7rum", "in", "der", "Run\u00b7de", "feh\u00b7let", "ihr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Die Mutter erz\u00e4hlt ein M\u00e4hrchen sch\u00f6n;", "tokens": ["Die", "Mut\u00b7ter", "er\u00b7z\u00e4hlt", "ein", "M\u00e4hr\u00b7chen", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Warum bei der Kunde fehlet ihr?", "tokens": ["Wa\u00b7rum", "bei", "der", "Kun\u00b7de", "feh\u00b7let", "ihr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Ihr fehlt, ich wei\u00df nicht, warum ihr fehlt;", "tokens": ["Ihr", "fehlt", ",", "ich", "wei\u00df", "nicht", ",", "wa\u00b7rum", "ihr", "fehlt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Aus nichtigem Grunde fehlet ihr.", "tokens": ["Aus", "nich\u00b7ti\u00b7gem", "Grun\u00b7de", "feh\u00b7let", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Ihr fehlt uns in jedem Augenblick,", "tokens": ["Ihr", "fehlt", "uns", "in", "je\u00b7dem", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "In jeder Secunde fehlet ihr.", "tokens": ["In", "je\u00b7der", "Se\u00b7cun\u00b7de", "feh\u00b7let", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Ihr fehlet an jedem Ort, nur nie", "tokens": ["Ihr", "feh\u00b7let", "an", "je\u00b7dem", "Ort", ",", "nur", "nie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,", "ADV", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Dem Herzen als Wunde fehlet ihr.", "tokens": ["Dem", "Her\u00b7zen", "als", "Wun\u00b7de", "feh\u00b7let", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "NN", "VVFIN", "PPER", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Was fehlt dem Herzen? ihr fehlet ihm,", "tokens": ["Was", "fehlt", "dem", "Her\u00b7zen", "?", "ihr", "feh\u00b7let", "ihm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Damit es gesunde, fehlet ihr.", "tokens": ["Da\u00b7mit", "es", "ge\u00b7sun\u00b7de", ",", "feh\u00b7let", "ihr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "$,", "VVFIN", "PPER", "$."], "meter": "----+-+-+", "measure": "unknown.measure.tri"}}}}}