{"textgrid.poem.64357": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "79. Elegie auf Vater Bodmer", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ruhe dir im Scho\u00dfe der Erde, heiliger S\u00e4nger!", "tokens": ["Ru\u00b7he", "dir", "im", "Scho\u00b7\u00dfe", "der", "Er\u00b7de", ",", "hei\u00b7li\u00b7ger", "S\u00e4n\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Fernher t\u00f6net dein Lob, wie an der Limmat es t\u00f6nt!", "tokens": ["Fern\u00b7her", "t\u00f6\u00b7net", "dein", "Lob", ",", "wie", "an", "der", "Lim\u00b7mat", "es", "t\u00f6nt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "PWAV", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+----+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Keine Klag' erschall' an der Gruft des heiligen S\u00e4ngers,", "tokens": ["Kei\u00b7ne", "Klag'", "er\u00b7schall'", "an", "der", "Gruft", "des", "hei\u00b7li\u00b7gen", "S\u00e4n\u00b7gers", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PIS", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Denn reif senkte sein Haupt sich wie die goldene Frucht.", "tokens": ["Denn", "reif", "senk\u00b7te", "sein", "Haupt", "sich", "wie", "die", "gol\u00b7de\u00b7ne", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "PRF", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-++--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Trocknet, trocknet die Thr\u00e4nen, und schnell, ihr Freunde des Greises,", "tokens": ["Trock\u00b7net", ",", "trock\u00b7net", "die", "Thr\u00e4\u00b7nen", ",", "und", "schnell", ",", "ihr", "Freun\u00b7de", "des", "Grei\u00b7ses", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "NN", "$,", "KON", "ADJD", "$,", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Denn sein freudiger Geist schwebt um die n\u00e4chtliche Gruft.", "tokens": ["Denn", "sein", "freu\u00b7di\u00b7ger", "Geist", "schwebt", "um", "die", "n\u00e4cht\u00b7li\u00b7che", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "H\u00f6rt ihr, seht ihr ihn nicht? Wie weht der rauschende Fittich!", "tokens": ["H\u00f6rt", "ihr", ",", "seht", "ihr", "ihn", "nicht", "?", "Wie", "weht", "der", "rau\u00b7schen\u00b7de", "Fit\u00b7tich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "VVFIN", "PPER", "PPER", "PTKNEG", "$.", "PWAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Gleich des Sirius Strahl flammt der Unsterblichen Blick.", "tokens": ["Gleich", "des", "Si\u00b7rius", "Strahl", "flammt", "der", "U\u00b7nsterb\u00b7li\u00b7chen", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+---+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Lavater, siehst du ihn nicht mit scharfem Auge des Sehers?", "tokens": ["La\u00b7va\u00b7ter", ",", "siehst", "du", "ihn", "nicht", "mit", "schar\u00b7fem", "Au\u00b7ge", "des", "Se\u00b7hers", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Sehn des z\u00e4rtlichen He\u00df redliche Augen ihn nicht?", "tokens": ["Sehn", "des", "z\u00e4rt\u00b7li\u00b7chen", "He\u00df", "red\u00b7li\u00b7che", "Au\u00b7gen", "ihn", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJA", "NN", "PPER", "PTKNEG", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Singt entflammten Gesang, auf da\u00df der Himmlische weile;", "tokens": ["Singt", "ent\u00b7flamm\u00b7ten", "Ge\u00b7sang", ",", "auf", "da\u00df", "der", "Himm\u00b7li\u00b7sche", "wei\u00b7le", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,", "APPR", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.12": {"text": "T\u00e4uschet den Sonnendurst, der ihn auch itzt noch durchgl\u00fcht.", "tokens": ["T\u00e4u\u00b7schet", "den", "Son\u00b7nen\u00b7durst", ",", "der", "ihn", "auch", "itzt", "noch", "durch\u00b7gl\u00fcht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.13": {"text": "Mich auch h\u00f6rt er vielleicht, wiewohl sich zwischen der Eider", "tokens": ["Mich", "auch", "h\u00f6rt", "er", "viel\u00b7leicht", ",", "wie\u00b7wohl", "sich", "zwi\u00b7schen", "der", "Ei\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PRF", "APPR", "ART", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Und der Elbe mein Lied, fern von den Alpen erhebt.", "tokens": ["Und", "der", "El\u00b7be", "mein", "Lied", ",", "fern", "von", "den", "Al\u00b7pen", "er\u00b7hebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "PPOSAT", "NN", "$,", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.15": {"text": "S\u00e4nger, wenn du mich h\u00f6rst, so umrausche die Schl\u00e4fen des Mannes,", "tokens": ["S\u00e4n\u00b7ger", ",", "wenn", "du", "mich", "h\u00f6rst", ",", "so", "um\u00b7rau\u00b7sche", "die", "Schl\u00e4\u00b7fen", "des", "Man\u00b7nes", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Welcher von Herzen dich ehrt, welcher von Herzen dich liebt.", "tokens": ["Wel\u00b7cher", "von", "Her\u00b7zen", "dich", "ehrt", ",", "wel\u00b7cher", "von", "Her\u00b7zen", "dich", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPR", "NN", "PPER", "VVFIN", "$,", "PRELS", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Dem als J\u00fcngling die Thr\u00e4ne der Freude von gl\u00fchenden Wangen", "tokens": ["Dem", "als", "J\u00fcng\u00b7ling", "die", "Thr\u00e4\u00b7ne", "der", "Freu\u00b7de", "von", "gl\u00fc\u00b7hen\u00b7den", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "KOUS", "NN", "ART", "NN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.18": {"text": "St\u00fcrzte, da er dich sah, da er umarmte den Greis.", "tokens": ["St\u00fcrz\u00b7te", ",", "da", "er", "dich", "sah", ",", "da", "er", "um\u00b7arm\u00b7te", "den", "Greis", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Damals hatten dich schon, gleich einer schattenden Eiche,", "tokens": ["Da\u00b7mals", "hat\u00b7ten", "dich", "schon", ",", "gleich", "ei\u00b7ner", "schat\u00b7ten\u00b7den", "Ei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "--+--+---+--+-", "measure": "anapaest.di.plus"}, "line.20": {"text": "Siebenundsiebenzigmal rollende Sonnen erhellt.", "tokens": ["Sie\u00b7be\u00b7nund\u00b7sie\u00b7ben\u00b7zig\u00b7mal", "rol\u00b7len\u00b7de", "Son\u00b7nen", "er\u00b7hellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+--+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.21": {"text": "Sieben w\u00e4rmten seitdem die wei\u00dfere Scheitel des Greises;", "tokens": ["Sie\u00b7ben", "w\u00e4rm\u00b7ten", "seit\u00b7dem", "die", "wei\u00b7\u00dfe\u00b7re", "Schei\u00b7tel", "des", "Grei\u00b7ses", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VAFIN", "PAV", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.22": {"text": "Leise pfl\u00fcckte der Tod dich, wie mit sch\u00fcchterner Hand.", "tokens": ["Lei\u00b7se", "pfl\u00fcck\u00b7te", "der", "Tod", "dich", ",", "wie", "mit", "sch\u00fcch\u00b7ter\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPER", "$,", "PWAV", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "Herzlich freut' ich mich oft ob deines Lebens, und freue", "tokens": ["Herz\u00b7lich", "freut'", "ich", "mich", "oft", "ob", "dei\u00b7nes", "Le\u00b7bens", ",", "und", "freu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "ADV", "KOUS", "PPOSAT", "NN", "$,", "KON", "VVFIN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.24": {"text": "Deines Todes mich auch; denn du entschlummertest sanft.", "tokens": ["Dei\u00b7nes", "To\u00b7des", "mich", "auch", ";", "denn", "du", "ent\u00b7schlum\u00b7mer\u00b7test", "sanft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADV", "$.", "KON", "PPER", "VVFIN", "ADJD", "$."], "meter": "+-+----+-+--+", "measure": "iambic.penta.chol"}, "line.25": {"text": "Aber es schmerzet mich eins: da\u00df du, mit bebender Rechte,", "tokens": ["A\u00b7ber", "es", "schmer\u00b7zet", "mich", "eins", ":", "da\u00df", "du", ",", "mit", "be\u00b7ben\u00b7der", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PIS", "$.", "KOUS", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-++--+-+-+-", "measure": "iambic.septa.invert"}, "line.26": {"text": "Gegen den, der dich liebt, zuckend den Bogen ergriffst,", "tokens": ["Ge\u00b7gen", "den", ",", "der", "dich", "liebt", ",", "zu\u00b7ckend", "den", "Bo\u00b7gen", "er\u00b7griffst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVPP", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.27": {"text": "Zwar du trafst mich nicht, vom irrenden Alter get\u00e4uschet,", "tokens": ["Zwar", "du", "trafst", "mich", "nicht", ",", "vom", "ir\u00b7ren\u00b7den", "Al\u00b7ter", "ge\u00b7t\u00e4u\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.28": {"text": "Doch du spanntest, und das that in der Seele mir weh.", "tokens": ["Doch", "du", "spann\u00b7test", ",", "und", "das", "that", "in", "der", "See\u00b7le", "mir", "weh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KON", "PDS", "VVFIN", "APPR", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "F\u00fcrder z\u00fcrnest du nicht, und ich werde wieder dich sehen,", "tokens": ["F\u00fcr\u00b7der", "z\u00fcr\u00b7nest", "du", "nicht", ",", "und", "ich", "wer\u00b7de", "wie\u00b7der", "dich", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "$,", "KON", "PPER", "VAFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.30": {"text": "Wo uns Vater Homer, ob du auch z\u00fcrnest, vers\u00f6hnt.", "tokens": ["Wo", "uns", "Va\u00b7ter", "Ho\u00b7mer", ",", "ob", "du", "auch", "z\u00fcr\u00b7nest", ",", "ver\u00b7s\u00f6hnt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "NE", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,", "VVPP", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.31": {"text": "Wo die heiligen S\u00e4nger von allen Zeiten und Zungen", "tokens": ["Wo", "die", "hei\u00b7li\u00b7gen", "S\u00e4n\u00b7ger", "von", "al\u00b7len", "Zei\u00b7ten", "und", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "KON", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.32": {"text": "Sich versammlen, und wo ewiger Lorbeer sie kr\u00e4nzt.", "tokens": ["Sich", "ver\u00b7samm\u00b7len", ",", "und", "wo", "e\u00b7wi\u00b7ger", "Lor\u00b7beer", "sie", "kr\u00e4nzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "$,", "KON", "PWAV", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "Dort wird Bodmer mit mir dem Celten Ossian lauschen,", "tokens": ["Dort", "wird", "Bod\u00b7mer", "mit", "mir", "dem", "Cel\u00b7ten", "Os\u00b7si\u00b7an", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "APPR", "PPER", "ART", "NN", "NE", "VVINF", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.34": {"text": "Und den Barden verzeihn, da\u00df sie nicht Ph\u00f6bos beseelt.", "tokens": ["Und", "den", "Bar\u00b7den", "ver\u00b7zeihn", ",", "da\u00df", "sie", "nicht", "Ph\u00f6\u00b7bos", "be\u00b7seelt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$,", "KOUS", "PPER", "PTKNEG", "NN", "VVPP", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}}}}}