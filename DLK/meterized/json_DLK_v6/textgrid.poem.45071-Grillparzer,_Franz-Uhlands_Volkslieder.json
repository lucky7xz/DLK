{"textgrid.poem.45071": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Uhlands Volkslieder", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was f\u00fchrst du selber M\u00f6rtel und Sand?", "tokens": ["Was", "f\u00fchrst", "du", "sel\u00b7ber", "M\u00f6r\u00b7tel", "und", "Sand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Zu h\u00f6hern Werken berufen und sch\u00f6nern.", "tokens": ["Zu", "h\u00f6\u00b7hern", "Wer\u00b7ken", "be\u00b7ru\u00b7fen", "und", "sch\u00f6\u00b7nern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "KON", "VVINF", "$."], "meter": "-+----+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wer bauen kann, bau auf eigne Hand", "tokens": ["Wer", "bau\u00b7en", "kann", ",", "bau", "auf", "eig\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVINF", "VMFIN", "$,", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und lasse den Karren den Tagel\u00f6hnern.", "tokens": ["Und", "las\u00b7se", "den", "Kar\u00b7ren", "den", "Ta\u00b7ge\u00b7l\u00f6h\u00b7nern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}}}}