{"textgrid.poem.35540": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "N\u00fcrnberger Tand", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im fremden Gasthofszimmer,", "tokens": ["Im", "frem\u00b7den", "Gast\u00b7hofs\u00b7zim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das uns\u00e4glich n\u00fcchtern und kalt,", "tokens": ["Das", "un\u00b7s\u00e4g\u00b7lich", "n\u00fcch\u00b7tern", "und", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Knistert's von seidenem Schimmer,", "tokens": ["Knis\u00b7tert's", "von", "sei\u00b7de\u00b7nem", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Perlt's auf von flirrendem Flimmer \u2013", "tokens": ["Perlt's", "auf", "von", "flir\u00b7ren\u00b7dem", "Flim\u00b7mer", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "APPR", "ADJA", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Nacht, deine geliebte Gestalt!", "tokens": ["Nacht", ",", "dei\u00b7ne", "ge\u00b7lieb\u00b7te", "Ge\u00b7stalt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "L\u00e4ngst schon ist die Nacht gekommen,", "tokens": ["L\u00e4ngst", "schon", "ist", "die", "Nacht", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat alles schwarz verh\u00e4ngt \u2013", "tokens": ["Hat", "al\u00b7les", "schwarz", "ver\u00b7h\u00e4ngt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Str\u00f6mend ist's da erglommen ...", "tokens": ["Str\u00f6\u00b7mend", "ist's", "da", "er\u00b7glom\u00b7men", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "VVINF", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und was du mir jemals genommen,", "tokens": ["Und", "was", "du", "mir", "je\u00b7mals", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Hast du alles mir wiedergeschenkt!", "tokens": ["Hast", "du", "al\u00b7les", "mir", "wie\u00b7der\u00b7ge\u00b7schenkt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "PPER", "VVFIN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich hab' mich herumgetrieben", "tokens": ["Ich", "hab'", "mich", "her\u00b7um\u00b7ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In der fremden, verworrenen Stadt \u2013", "tokens": ["In", "der", "frem\u00b7den", ",", "ver\u00b7wor\u00b7re\u00b7nen", "Stadt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Ich wollte dich nicht mehr lieben \u2013", "tokens": ["Ich", "woll\u00b7te", "dich", "nicht", "mehr", "lie\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da war mir treu geblieben,", "tokens": ["Da", "war", "mir", "treu", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die mich verlassen hat ...", "tokens": ["Die", "mich", "ver\u00b7las\u00b7sen", "hat", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}