{"dta.poem.22484": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "30. Auf die Krippe JEsu.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Hier liegt da\u00df wehrte Kind/ der Jungfrau erste", "tokens": ["Hier", "liegt", "da\u00df", "wehr\u00b7te", "Kind", "/", "der", "Jung\u00b7frau", "ers\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KOUS", "ADJA", "NN", "$(", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Blum/", "tokens": ["Blum", "/"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Der Engel Freud und Lust/ der Menschen Prei\u00df und", "tokens": ["Der", "En\u00b7gel", "Freud", "und", "Lust", "/", "der", "Men\u00b7schen", "Prei\u00df", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN", "$(", "ART", "NN", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ruhm.", "tokens": ["Ruhm", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Sol Er dein Heyland seyn/ und dich zu GOtt erheben/", "tokens": ["Sol", "Er", "dein", "Hey\u00b7land", "seyn", "/", "und", "dich", "zu", "Gott", "er\u00b7he\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VAINF", "$(", "KON", "PPER", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So mustu nicht sehr weit von seiner