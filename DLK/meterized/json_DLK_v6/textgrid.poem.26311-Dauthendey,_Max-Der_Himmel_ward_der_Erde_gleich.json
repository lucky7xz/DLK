{"textgrid.poem.26311": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Der Himmel ward der Erde gleich", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun wird es wieder abendstill,", "tokens": ["Nun", "wird", "es", "wie\u00b7der", "a\u00b7bend\u00b7still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wind noch einmal atmen will.", "tokens": ["Der", "Wind", "noch", "ein\u00b7mal", "at\u00b7men", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er biegt die B\u00e4ume hin und her.", "tokens": ["Er", "biegt", "die", "B\u00e4u\u00b7me", "hin", "und", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sonne schwand. Die Luft ist leer.", "tokens": ["Die", "Son\u00b7ne", "schwand", ".", "Die", "Luft", "ist", "leer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nur gelbe Wolken strahlen leicht.", "tokens": ["Nur", "gel\u00b7be", "Wol\u00b7ken", "strah\u00b7len", "leicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Baumwelt dunkelt und verbleicht.", "tokens": ["Die", "Baum\u00b7welt", "dun\u00b7kelt", "und", "ver\u00b7bleicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wolken gl\u00e4nzen um das Haus, \u2013", "tokens": ["Die", "Wol\u00b7ken", "gl\u00e4n\u00b7zen", "um", "das", "Haus", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie ziehn den Blick mir weit hinaus.", "tokens": ["Sie", "ziehn", "den", "Blick", "mir", "weit", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich schaue hin von meinem Tisch.", "tokens": ["Ich", "schau\u00b7e", "hin", "von", "mei\u00b7nem", "Tisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wind verzischt. Die Luft wird frisch.", "tokens": ["Der", "Wind", "ver\u00b7zischt", ".", "Die", "Luft", "wird", "frisch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wolken wandelt tiefes Rot.", "tokens": ["Die", "Wol\u00b7ken", "wan\u00b7delt", "tie\u00b7fes", "Rot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Haus versinkt \u2013 und mir w\u00e4chst Not.", "tokens": ["Das", "Haus", "ver\u00b7sinkt", "\u2013", "und", "mir", "w\u00e4chst", "Not", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$(", "KON", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Himmel ward der Erde gleich:", "tokens": ["Der", "Him\u00b7mel", "ward", "der", "Er\u00b7de", "gleich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein gro\u00dfes totes Dunkelreich.", "tokens": ["Ein", "gro\u00b7\u00dfes", "to\u00b7tes", "Dun\u00b7kel\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ich allein mit meinem Blut", "tokens": ["Und", "ich", "al\u00b7lein", "mit", "mei\u00b7nem", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und in mir all der Wolken Glut.", "tokens": ["Und", "in", "mir", "all", "der", "Wol\u00b7ken", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PIAT", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Nacht mir um die Schultern h\u00e4ngt.", "tokens": ["Die", "Nacht", "mir", "um", "die", "Schul\u00b7tern", "h\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Nacht mich nicht so sehr bedr\u00e4ngt", "tokens": ["Die", "Nacht", "mich", "nicht", "so", "sehr", "be\u00b7dr\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "PTKNEG", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Ruf um Ruf, den ich erstickt", "tokens": ["Wie", "Ruf", "um", "Ruf", ",", "den", "ich", "er\u00b7stickt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "NN", "$,", "PRELS", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Blut, das in die Leere blickt.", "tokens": ["Im", "Blut", ",", "das", "in", "die", "Lee\u00b7re", "blickt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}