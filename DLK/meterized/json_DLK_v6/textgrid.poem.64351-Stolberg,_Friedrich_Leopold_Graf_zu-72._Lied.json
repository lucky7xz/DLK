{"textgrid.poem.64351": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "72. Lied", "genre": "verse", "period": "N.A.", "pub_year": 1781, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ging unter Erlen am k\u00fchligen Bach,", "tokens": ["Ich", "ging", "un\u00b7ter", "Er\u00b7len", "am", "k\u00fch\u00b7li\u00b7gen", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und dachte wohl manchem und manchem wohl nach:", "tokens": ["Und", "dach\u00b7te", "wohl", "man\u00b7chem", "und", "man\u00b7chem", "wohl", "nach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "KON", "PIAT", "ADV", "APPR", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Es war mir im Herzen so leicht und so wohl;", "tokens": ["Es", "war", "mir", "im", "Her\u00b7zen", "so", "leicht", "und", "so", "wohl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPRART", "NN", "ADV", "ADJD", "KON", "ADV", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Doch wurden von Thr\u00e4nen die Augen mir voll.", "tokens": ["Doch", "wur\u00b7den", "von", "Thr\u00e4\u00b7nen", "die", "Au\u00b7gen", "mir", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Es entschwebte den s\u00e4uselnden Wellen das Bild", "tokens": ["Es", "ent\u00b7schweb\u00b7te", "den", "s\u00e4u\u00b7seln\u00b7den", "Wel\u00b7len", "das", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Von meiner Geliebten, holdselig und mild;", "tokens": ["Von", "mei\u00b7ner", "Ge\u00b7lieb\u00b7ten", ",", "hold\u00b7se\u00b7lig", "und", "mild", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da sank ich ans Ufer ins schwellende Moos,", "tokens": ["Da", "sank", "ich", "ans", "U\u00b7fer", "ins", "schwel\u00b7len\u00b7de", "Moos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Mir st\u00fcrzten die Thr\u00e4nen hinab in den Scho\u00df.", "tokens": ["Mir", "st\u00fcrz\u00b7ten", "die", "Thr\u00e4\u00b7nen", "hin\u00b7ab", "in", "den", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Nun lag ich im Schatten am k\u00fchligen Bach,", "tokens": ["Nun", "lag", "ich", "im", "Schat\u00b7ten", "am", "k\u00fch\u00b7li\u00b7gen", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und dachte wohl manchem und manchem wohl nach:", "tokens": ["Und", "dach\u00b7te", "wohl", "man\u00b7chem", "und", "man\u00b7chem", "wohl", "nach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "KON", "PIAT", "ADV", "APPR", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die Nachtigall sang, und es rauschte der Bach;", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "sang", ",", "und", "es", "rauschte", "der", "Bach", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ich dachte dem einen und einen nur nach.", "tokens": ["Ich", "dach\u00b7te", "dem", "ei\u00b7nen", "und", "ei\u00b7nen", "nur", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ART", "KON", "ART", "ADV", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Schon flammten die Wolken im r\u00f6tenden Strahl,", "tokens": ["Schon", "flamm\u00b7ten", "die", "Wol\u00b7ken", "im", "r\u00f6\u00b7ten\u00b7den", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Schon senkten sich br\u00e4unere Schatten ins Thal,", "tokens": ["Schon", "senk\u00b7ten", "sich", "br\u00e4u\u00b7ne\u00b7re", "Schat\u00b7ten", "ins", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Schon bebte durch Erlen der Mond auf dem Bach;", "tokens": ["Schon", "beb\u00b7te", "durch", "Er\u00b7len", "der", "Mond", "auf", "dem", "Bach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ich dachte dem einen und einen nur nach.", "tokens": ["Ich", "dach\u00b7te", "dem", "ei\u00b7nen", "und", "ei\u00b7nen", "nur", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ART", "KON", "ART", "ADV", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Nun wankt' ich von dannen mit weinendem Blick,", "tokens": ["Nun", "wankt'", "ich", "von", "dan\u00b7nen", "mit", "wei\u00b7nen\u00b7dem", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und sah nach dem Bach und den Erlen zur\u00fcck:", "tokens": ["Und", "sah", "nach", "dem", "Bach", "und", "den", "Er\u00b7len", "zu\u00b7r\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "KON", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Sie schwanden; es schwand nicht das liebliche Bild,", "tokens": ["Sie", "schwan\u00b7den", ";", "es", "schwand", "nicht", "das", "lieb\u00b7li\u00b7che", "Bild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Das immer und immer die Seele mir f\u00fcllt.", "tokens": ["Das", "im\u00b7mer", "und", "im\u00b7mer", "die", "See\u00b7le", "mir", "f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "KON", "ADV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}