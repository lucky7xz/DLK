{"textgrid.poem.42559": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf der \u00dcberfahrt", "genre": "verse", "period": "N.A.", "pub_year": 1823, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber diesen Strom, vor Jahren,", "tokens": ["\u00dc\u00b7ber", "die\u00b7sen", "Strom", ",", "vor", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bin ich einmal schon gefahren.", "tokens": ["Bin", "ich", "ein\u00b7mal", "schon", "ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hier die Burg im Abendschimmer,", "tokens": ["Hier", "die", "Burg", "im", "A\u00b7bend\u00b7schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dr\u00fcben rauscht das Wehr, wie immer.", "tokens": ["Dr\u00fc\u00b7ben", "rauscht", "das", "Wehr", ",", "wie", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PWAV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und von diesem Kahn umschlossen", "tokens": ["Und", "von", "die\u00b7sem", "Kahn", "um\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Waren mit mir zween Genossen:", "tokens": ["Wa\u00b7ren", "mit", "mir", "zween", "Ge\u00b7nos\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! ein Freund, ein vatergleicher,", "tokens": ["Ach", "!", "ein", "Freund", ",", "ein", "va\u00b7ter\u00b7glei\u00b7cher", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ein junger, hoffnungsreicher.", "tokens": ["Und", "ein", "jun\u00b7ger", ",", "hoff\u00b7nungs\u00b7rei\u00b7cher", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jener wirkte still hienieden,", "tokens": ["Je\u00b7ner", "wirk\u00b7te", "still", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "VVFIN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und so ist er auch geschieden,", "tokens": ["Und", "so", "ist", "er", "auch", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieser, brausend vor uns allen,", "tokens": ["Die\u00b7ser", ",", "brau\u00b7send", "vor", "uns", "al\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ADJD", "APPR", "PPER", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist in Kampf und Sturm gefallen.", "tokens": ["Ist", "in", "Kampf", "und", "Sturm", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "So, wenn ich vergangner Tage,", "tokens": ["So", ",", "wenn", "ich", "ver\u00b7gang\u00b7ner", "Ta\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gl\u00fccklicher, zu denken wage,", "tokens": ["Gl\u00fcck\u00b7li\u00b7cher", ",", "zu", "den\u00b7ken", "wa\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00df ich stets Genossen missen,", "tokens": ["Mu\u00df", "ich", "stets", "Ge\u00b7nos\u00b7sen", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Teure, die der Tod entrissen.", "tokens": ["Teu\u00b7re", ",", "die", "der", "Tod", "ent\u00b7ris\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch was alle Freundschaft bindet,", "tokens": ["Doch", "was", "al\u00b7le", "Freund\u00b7schaft", "bin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist, wenn Geist zu Geist sich findet;", "tokens": ["Ist", ",", "wenn", "Geist", "zu", "Geist", "sich", "fin\u00b7det", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "NN", "APPR", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Geistig waren jene Stunden,", "tokens": ["Geis\u00b7tig", "wa\u00b7ren", "je\u00b7ne", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Geistern bin ich noch verbunden. \u2013", "tokens": ["Geis\u00b7tern", "bin", "ich", "noch", "ver\u00b7bun\u00b7den", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Nimm nur, F\u00e4hrmann, nimm die Miete,", "tokens": ["Nimm", "nur", ",", "F\u00e4hr\u00b7mann", ",", "nimm", "die", "Mie\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "NN", "$,", "VVIMP", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ich gerne dreifach biete!", "tokens": ["Die", "ich", "ger\u00b7ne", "drei\u00b7fach", "bie\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PAV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zween, die mit mir \u00fcberfuhren,", "tokens": ["Zween", ",", "die", "mit", "mir", "\u00fc\u00b7ber\u00b7fuh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Waren geistige Naturen.", "tokens": ["Wa\u00b7ren", "geis\u00b7ti\u00b7ge", "Na\u00b7tu\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}