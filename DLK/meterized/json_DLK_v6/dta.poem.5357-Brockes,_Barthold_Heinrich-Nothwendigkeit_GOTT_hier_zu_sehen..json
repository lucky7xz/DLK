{"dta.poem.5357": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Nothwendigkeit GOTT hier zu sehen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wilst du den grossen Sch\u00f6pfer sehen,", "tokens": ["Wilst", "du", "den", "gros\u00b7sen", "Sch\u00f6p\u00b7fer", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So mu\u00df es im Gesch\u00f6pf geschehen:", "tokens": ["So", "mu\u00df", "es", "im", "Ge\u00b7sch\u00f6pf", "ge\u00b7sche\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Weil man ihn sonst nicht sehen kann.", "tokens": ["Weil", "man", "ihn", "sonst", "nicht", "se\u00b7hen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er will von uns gesehen werden;", "tokens": ["Er", "will", "von", "uns", "ge\u00b7se\u00b7hen", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Drum schuf er Himmel, Meer und Erden,", "tokens": ["Drum", "schuf", "er", "Him\u00b7mel", ",", "Meer", "und", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und spricht: ", "tokens": ["Und", "spricht", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Jhr aber h\u00f6rt nicht was er spricht,", "tokens": ["Ihr", "a\u00b7ber", "h\u00f6rt", "nicht", "was", "er", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PTKNEG", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er zeigt sich, und ihr seht ihn nicht.", "tokens": ["Er", "zeigt", "sich", ",", "und", "ihr", "seht", "ihn", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zwar hofft ihr k\u00fcnftig ihn zu sehen;", "tokens": ["Zwar", "hofft", "ihr", "k\u00fcnf\u00b7tig", "ihn", "zu", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Allein ihr werdet schlecht bestehen,", "tokens": ["Al\u00b7lein", "ihr", "wer\u00b7det", "schlecht", "be\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Da ihr, in seiner Wercke Pracht,", "tokens": ["Da", "ihr", ",", "in", "sei\u00b7ner", "Wer\u00b7cke", "Pracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Jhn hier zu sehn nicht wehrt geacht\u2019t.", "tokens": ["Jhn", "hier", "zu", "sehn", "nicht", "wehrt", "ge\u00b7acht'", "t."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "PTKNEG", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Hat er, da\u00df ihr ihn m\u00f6gtet fassen,", "tokens": ["Hat", "er", ",", "da\u00df", "ihr", "ihn", "m\u00f6g\u00b7tet", "fas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Sich hier nicht unbezeugt gelassen:", "tokens": ["Sich", "hier", "nicht", "un\u00b7be\u00b7zeugt", "ge\u00b7las\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PTKNEG", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "M\u00fc\u00dft ihr auch hier, zu seinen Ehren,", "tokens": ["M\u00fc\u00dft", "ihr", "auch", "hier", ",", "zu", "sei\u00b7nen", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Jhn schmecken, f\u00fchlen, sehn und h\u00f6ren.", "tokens": ["Jhn", "schme\u00b7cken", ",", "f\u00fch\u00b7len", ",", "sehn", "und", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}