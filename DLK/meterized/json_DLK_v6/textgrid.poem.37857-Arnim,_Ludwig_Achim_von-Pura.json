{"textgrid.poem.37857": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Pura", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich gen Antiocha kam,", "tokens": ["Als", "ich", "gen", "An\u00b7ti\u00b7o\u00b7cha", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Jungfrau, Pura war ihr Nahm,", "tokens": ["Ein", "Jung\u00b7frau", ",", "Pu\u00b7ra", "war", "ihr", "Nahm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NE", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Christin ward gefunden,", "tokens": ["Ein", "Chris\u00b7tin", "ward", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die ward vor den Kaiser bracht,", "tokens": ["Die", "ward", "vor", "den", "Kai\u00b7ser", "bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der sprach zur b\u00f6sen Stunde:", "tokens": ["Der", "sprach", "zur", "b\u00f6\u00b7sen", "Stun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbgeht, f\u00fchrt sie in ein Schandhau\u00df ein,", "tokens": ["\u00bb", "geht", ",", "f\u00fchrt", "sie", "in", "ein", "Schand\u00b7hau\u00df", "ein", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Jungfrau z\u00fcchtig, keusch und rein,", "tokens": ["Die", "Jung\u00b7frau", "z\u00fcch\u00b7tig", ",", "keusch", "und", "rein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Spott und Schmach zu sch\u00e4nden.\u00ab", "tokens": ["In", "Spott", "und", "Schmach", "zu", "sch\u00e4n\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Jungfrau rief in dieser Noth", "tokens": ["Die", "Jung\u00b7frau", "rief", "in", "die\u00b7ser", "Noth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu Gott, und wandt die H\u00e4nde.", "tokens": ["Zu", "Gott", ",", "und", "wandt", "die", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bberrette mich, du Sohn David!", "tokens": ["\u00bb", "er\u00b7ret\u00b7te", "mich", ",", "du", "Sohn", "Da\u00b7vid", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "PPER", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor Schand und S\u00fcnd, Herr mich beh\u00fct,", "tokens": ["Vor", "Schand", "und", "S\u00fcnd", ",", "Herr", "mich", "be\u00b7h\u00fct", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df dich meins Leids erbarmen!", "tokens": ["La\u00df", "dich", "meins", "Leids", "er\u00b7bar\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das bitt ich dich durch Jesum Christ,", "tokens": ["Das", "bitt", "ich", "dich", "durch", "Je\u00b7sum", "Christ", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PRF", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Komm bald zu H\u00fclf mir Armen!\u00ab", "tokens": ["Komm", "bald", "zu", "H\u00fclf", "mir", "Ar\u00b7men", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "PPER", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Klag erh\u00f6rt ein Engel fein,", "tokens": ["Die", "Klag", "er\u00b7h\u00f6rt", "ein", "En\u00b7gel", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als J\u00fcngling ging er zu ihr ein,", "tokens": ["Als", "J\u00fcng\u00b7ling", "ging", "er", "zu", "ihr", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sprach: \u00bbJungfrau sey ohn' Sorgen,", "tokens": ["Sprach", ":", "\u00bb", "Jung\u00b7frau", "sey", "ohn'", "Sor\u00b7gen", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von mir sollst bleiben unber\u00fchrt,", "tokens": ["Von", "mir", "sollst", "blei\u00b7ben", "un\u00b7be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "VVINF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wart mit Geduld bis Morgen,", "tokens": ["Wart", "mit", "Ge\u00b7duld", "bis", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "So will ich helfen dir davon,", "tokens": ["So", "will", "ich", "hel\u00b7fen", "dir", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "PPER", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bald leg du meine Kleider an,", "tokens": ["Bald", "leg", "du", "mei\u00b7ne", "Klei\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und geh aus diesem Hause.\u00ab", "tokens": ["Und", "geh", "aus", "die\u00b7sem", "Hau\u00b7se", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPR", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So tauschten sie denn ihr Gewand,", "tokens": ["So", "tauschten", "sie", "denn", "ihr", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sie gieng, er blieb ohn Grausen.", "tokens": ["Sie", "gieng", ",", "er", "blieb", "ohn", "Grau\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Betrunken in des Kaisers Wein,", "tokens": ["Be\u00b7trun\u00b7ken", "in", "des", "Kai\u00b7sers", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Trat bald ein Kriegsknecht zu ihm ein,", "tokens": ["Trat", "bald", "ein", "Kriegs\u00b7knecht", "zu", "ihm", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Th\u00e4t s\u00fcndlig auf ihn dringen,", "tokens": ["Th\u00e4t", "s\u00fcnd\u00b7lig", "auf", "ihn", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der J\u00fcngling rang in Gotteskraft,", "tokens": ["Der", "J\u00fcng\u00b7ling", "rang", "in", "Got\u00b7tes\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und th\u00e4t ihn niederringen.", "tokens": ["Und", "th\u00e4t", "ihn", "nie\u00b7der\u00b7rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Des ward der Kaiser sehr ergrimmt,", "tokens": ["Des", "ward", "der", "Kai\u00b7ser", "sehr", "er\u00b7grimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als er vom Knecht die Klag vernimmt,", "tokens": ["Als", "er", "vom", "Knecht", "die", "Klag", "ver\u00b7nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "L\u00e4\u00dft greifen sie und binden.", "tokens": ["L\u00e4\u00dft", "grei\u00b7fen", "sie", "und", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "PPER", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O Wunder gro\u00df! O Wunder gro\u00df!", "tokens": ["O", "Wun\u00b7der", "gro\u00df", "!", "O", "Wun\u00b7der", "gro\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "$.", "NE", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein J\u00fcngling th\u00e4t er finden.", "tokens": ["Ein", "J\u00fcng\u00b7ling", "th\u00e4t", "er", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbbist du ein Christ?\u00ab der Kaiser fragt,", "tokens": ["\u00bb", "bist", "du", "ein", "Christ", "?", "\u00ab", "der", "Kai\u00b7ser", "fragt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "ART", "NN", "$.", "$(", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbich bin getauft,\u00ab der J\u00fcngling sagt,", "tokens": ["\u00bb", "ich", "bin", "ge\u00b7tauft", ",", "\u00ab", "der", "J\u00fcng\u00b7ling", "sagt", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "VVPP", "$,", "$(", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbvon ihr bin ich getaufet.", "tokens": ["\u00bb", "von", "ihr", "bin", "ich", "ge\u00b7tau\u00b7fet", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sie gehet frey und unber\u00fchrt,", "tokens": ["Sie", "ge\u00b7het", "frey", "und", "un\u00b7be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Euch Heiden all zu taufen.\u00ab", "tokens": ["Euch", "Hei\u00b7den", "all", "zu", "tau\u00b7fen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "NN", "PIAT", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der Kaiser bald das Urtheil sprach,", "tokens": ["Der", "Kai\u00b7ser", "bald", "das", "Ur\u00b7theil", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df man ihn tauf, in Flammen nach,", "tokens": ["Da\u00df", "man", "ihn", "tauf", ",", "in", "Flam\u00b7men", "nach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "$,", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ward bald dem Henker geben;", "tokens": ["Ward", "bald", "dem", "Hen\u00b7ker", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der f\u00fchrt sogleich ihn aus der Stadt,", "tokens": ["Der", "f\u00fchrt", "sog\u00b7leich", "ihn", "aus", "der", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wollt nehmen ihm sein Leben.", "tokens": ["Wollt", "neh\u00b7men", "ihm", "sein", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Da nun ersieht die Pura fromm,", "tokens": ["Da", "nun", "er\u00b7sieht", "die", "Pu\u00b7ra", "fromm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df man ihn da wollt bringen um,", "tokens": ["Da\u00df", "man", "ihn", "da", "wollt", "brin\u00b7gen", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "VMFIN", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lief sie in diesen N\u00f6then,", "tokens": ["Lief", "sie", "in", "die\u00b7sen", "N\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In schneller Eil auf die Richtstadt,", "tokens": ["In", "schnel\u00b7ler", "Eil", "auf", "die", "Richt\u00b7stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wollt ihren Freund erretten.", "tokens": ["Wollt", "ih\u00b7ren", "Freund", "er\u00b7ret\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "\u00bbich schuldig bin an deinem Tod!\u00ab", "tokens": ["\u00bb", "ich", "schul\u00b7dig", "bin", "an", "dei\u00b7nem", "Tod", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "ADJD", "VAFIN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach diese Jungfrau in der Noth,", "tokens": ["Sprach", "die\u00b7se", "Jung\u00b7frau", "in", "der", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "\u00bbherzlieber Bruder meine!", "tokens": ["\u00bb", "herz\u00b7lie\u00b7ber", "Bru\u00b7der", "mei\u00b7ne", "!"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NN", "PPOSAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Darum f\u00fcr dich ich sterben will,", "tokens": ["Da\u00b7rum", "f\u00fcr", "dich", "ich", "ster\u00b7ben", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PPER", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "i