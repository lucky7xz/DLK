{"dta.poem.5892": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allein,", "tokens": ["Al\u00b7lein", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Der Grund-Satz, der so allgemein,", "tokens": ["Der", "Grun\u00b7dSatz", ",", "der", "so", "all\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erweckte doch auch zu derselben Zeit", "tokens": ["Er\u00b7weck\u00b7te", "doch", "auch", "zu", "der\u00b7sel\u00b7ben", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verschiedne Secten, Zanck und Streit.", "tokens": ["Ver\u00b7schied\u00b7ne", "Sec\u00b7ten", ",", "Zanck", "und", "Streit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn ", "tokens": ["Denn"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Best\u00e4nde", "tokens": ["Be\u00b7st\u00e4n\u00b7de"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Aus ", "tokens": ["Aus"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Wenn, sprach er, diese C\u00f6rperlein,", "tokens": ["Wenn", ",", "sprach", "er", ",", "die\u00b7se", "C\u00f6r\u00b7per\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "$,", "VVFIN", "PPER", "$,", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die allenthalben zwar, jedoch verborgen seyn,", "tokens": ["Die", "al\u00b7len\u00b7thal\u00b7ben", "zwar", ",", "je\u00b7doch", "ver\u00b7bor\u00b7gen", "seyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "$,", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "In deren jeglichen Figur und Form zu finden,", "tokens": ["In", "de\u00b7ren", "jeg\u00b7li\u00b7chen", "Fi\u00b7gur", "und", "Form", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "PIAT", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "In gleicher Meng und Gr\u00f6sse sich verbinden;", "tokens": ["In", "glei\u00b7cher", "Meng", "und", "Gr\u00f6s\u00b7se", "sich", "ver\u00b7bin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Sodann entstehen", "tokens": ["So\u00b7dann", "ent\u00b7ste\u00b7hen"], "token_info": ["word", "word"], "pos": ["ADV", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Die Dinge, die wir k\u00f6nnen sehen.", "tokens": ["Die", "Din\u00b7ge", ",", "die", "wir", "k\u00f6n\u00b7nen", "se\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und nichts, wie er vermeinte, w\u00fcrde neu.", "tokens": ["Und", "nichts", ",", "wie", "er", "ver\u00b7mein\u00b7te", ",", "w\u00fcr\u00b7de", "neu", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PWAV", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Ver\u00e4nderten die Theilgen ihren Ort;", "tokens": ["Ver\u00b7\u00e4n\u00b7der\u00b7ten", "die", "Theil\u00b7gen", "ih\u00b7ren", "Ort", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.16": {"text": "So lehrt\u2019 er, da\u00df alsdenn so fort", "tokens": ["So", "lehrt'", "er", ",", "da\u00df", "als\u00b7denn", "so", "fort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ADV", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Das Wesen auch ver\u00e4ndert sey.", "tokens": ["Das", "We\u00b7sen", "auch", "ver\u00b7\u00e4n\u00b7dert", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Wir k\u00f6nnen nehmlich Wasser sehen,", "tokens": ["Wir", "k\u00f6n\u00b7nen", "nehm\u00b7lich", "Was\u00b7ser", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Wenn Wasser-Theilgen, die vorher", "tokens": ["Wenn", "Was\u00b7ser\u00b7T\u00b7heil\u00b7gen", ",", "die", "vor\u00b7her"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KOUS", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Mit andern C\u00f6rperchen vermischt, von ungefehr", "tokens": ["Mit", "an\u00b7dern", "C\u00f6r\u00b7per\u00b7chen", "ver\u00b7mischt", ",", "von", "un\u00b7ge\u00b7fehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,", "APPR", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Sich binden und zusammen gehen.", "tokens": ["Sich", "bin\u00b7den", "und", "zu\u00b7sam\u00b7men", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Er lehrte, da\u00df das Feur sich schiene zu entz\u00fcnden,", "tokens": ["Er", "lehr\u00b7te", ",", "da\u00df", "das", "Feur", "sich", "schie\u00b7ne", "zu", "ent\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "PRF", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wenn viele Funcken sich verbinden:", "tokens": ["Wenn", "vie\u00b7le", "Fun\u00b7cken", "sich", "ver\u00b7bin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Und da\u00df auch so das Gold, verstreuet in der Erde,", "tokens": ["Und", "da\u00df", "auch", "so", "das", "Gold", ",", "ver\u00b7streu\u00b7et", "in", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ADV", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wenn seine Theilgen sich vereinen, Gold erst werde.", "tokens": ["Wenn", "sei\u00b7ne", "Theil\u00b7gen", "sich", "ver\u00b7ei\u00b7nen", ",", "Gold", "erst", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "VVINF", "$,", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}