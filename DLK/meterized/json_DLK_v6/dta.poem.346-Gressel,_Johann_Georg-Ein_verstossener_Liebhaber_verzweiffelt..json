{"dta.poem.346": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Ein verstossener Liebhaber verzweiffelt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "So ist es denn mit mir geschehn/", "tokens": ["So", "ist", "es", "denn", "mit", "mir", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich soll und mu\u00df vergehen.", "tokens": ["Ich", "soll", "und", "mu\u00df", "ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Jhr Geister aus den dunckeln Gr\u00fcfften", "tokens": ["Ihr", "Geis\u00b7ter", "aus", "den", "dun\u00b7ckeln", "Gr\u00fcff\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kommt nehmet meinen Geist zu euch;", "tokens": ["Kommt", "neh\u00b7met", "mei\u00b7nen", "Geist", "zu", "euch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die/ so mein Leben solte heissen/", "tokens": ["Die", "/", "so", "mein", "Le\u00b7ben", "sol\u00b7te", "heis\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ADV", "PPOSAT", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die wird durch keine Thr\u00e4nen weich/", "tokens": ["Die", "wird", "durch", "kei\u00b7ne", "Thr\u00e4\u00b7nen", "weich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PIAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie will ihr solt mich nur zerreissen", "tokens": ["Sie", "will", "ihr", "solt", "mich", "nur", "zer\u00b7reis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ein erb\u00e4rmlich Schau-Spiel stifften.", "tokens": ["Und", "ein", "er\u00b7b\u00e4rm\u00b7lich", "Schau\u00b7Spiel", "stiff\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Jhr Geister aus den dunckeln Gr\u00fcfften", "tokens": ["Ihr", "Geis\u00b7ter", "aus", "den", "dun\u00b7ckeln", "Gr\u00fcff\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Kommt nehmet meinen Geist zu euch.", "tokens": ["Kommt", "neh\u00b7met", "mei\u00b7nen", "Geist", "zu", "euch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Komm ", "tokens": ["Komm"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Den Lebens-Drat ab/", "tokens": ["Den", "Le\u00b7bens\u00b7Drat", "ab", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ich finde nur Freude", "tokens": ["Ich", "fin\u00b7de", "nur", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Jm finsteren Grab.", "tokens": ["Jm", "fins\u00b7te\u00b7ren", "Grab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}