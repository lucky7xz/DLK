{"textgrid.poem.61409": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer sich selbst zu bescheiden vermag aus Liebe zum Ganzen,", "tokens": ["Wer", "sich", "selbst", "zu", "be\u00b7schei\u00b7den", "ver\u00b7mag", "aus", "Lie\u00b7be", "zum", "Gan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADV", "PTKA", "ADJD", "VVFIN", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Den vor allen im Staat preis' ich als gro\u00df und als frei.", "tokens": ["Den", "vor", "al\u00b7len", "im", "Staat", "preis'", "ich", "als", "gro\u00df", "und", "als", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "APPRART", "NN", "VVFIN", "PPER", "KOUS", "ADJD", "KON", "KOUS", "ADJD", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Denn ihm ward das Gesetz zum eigenen Willen, und freudig", "tokens": ["Denn", "ihm", "ward", "das", "Ge\u00b7setz", "zum", "ei\u00b7ge\u00b7nen", "Wil\u00b7len", ",", "und", "freu\u00b7dig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,", "KON", "ADJD"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "\u00dcbt er aus innerstem Trieb, was ihn begl\u00fccket, das Ma\u00df.", "tokens": ["\u00dcbt", "er", "aus", "in\u00b7ners\u00b7tem", "Trieb", ",", "was", "ihn", "be\u00b7gl\u00fc\u00b7cket", ",", "das", "Ma\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.5": {"text": "Jeglichem leistet er gern das Geb\u00fchrende, da\u00df er es selber", "tokens": ["Jeg\u00b7li\u00b7chem", "leis\u00b7tet", "er", "gern", "das", "Ge\u00b7b\u00fch\u00b7ren\u00b7de", ",", "da\u00df", "er", "es", "sel\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "PPER", "ADV"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Wieder empfange, vom Recht, dem er sich beuget, besch\u00fctzt.", "tokens": ["Wie\u00b7der", "emp\u00b7fan\u00b7ge", ",", "vom", "Recht", ",", "dem", "er", "sich", "beu\u00b7get", ",", "be\u00b7sch\u00fctzt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPRART", "NN", "$,", "PRELS", "PPER", "PRF", "VVFIN", "$,", "VVPP", "$."], "meter": "---+--+---+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Lebte jeglicher so vom K\u00f6nig herunter zum Bauern:", "tokens": ["Leb\u00b7te", "jeg\u00b7li\u00b7cher", "so", "vom", "K\u00f6\u00b7nig", "her\u00b7un\u00b7ter", "zum", "Bau\u00b7ern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADV", "APPRART", "NN", "APZR", "APPRART", "NN", "$."], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Ach, kein bitterer Zwist spaltete schm\u00e4hlich das Land,", "tokens": ["Ach", ",", "kein", "bit\u00b7te\u00b7rer", "Zwist", "spal\u00b7te\u00b7te", "schm\u00e4h\u00b7lich", "das", "Land", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIAT", "ADJA", "NN", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Sondern wir st\u00e4nden vereint, wie ein Forst hochragender Eichen,", "tokens": ["Son\u00b7dern", "wir", "st\u00e4n\u00b7den", "ver\u00b7eint", ",", "wie", "ein", "Forst", "hoch\u00b7ra\u00b7gen\u00b7der", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "VVPP", "$,", "PWAV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Auf uns selber, dem Feind schrecklich und gl\u00fccklich am Herd.", "tokens": ["Auf", "uns", "sel\u00b7ber", ",", "dem", "Feind", "schreck\u00b7lich", "und", "gl\u00fcck\u00b7lich", "am", "Herd", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "$,", "ART", "NN", "ADJD", "KON", "ADJD", "APPRART", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}}}}}