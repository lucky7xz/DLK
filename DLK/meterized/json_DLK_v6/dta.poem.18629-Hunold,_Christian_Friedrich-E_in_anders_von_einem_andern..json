{"dta.poem.18629": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "E in anders von einem andern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Hier wird ein R\u00e4tzel seyn/ ihr Jungfern rahtet doch/", "tokens": ["Hier", "wird", "ein", "R\u00e4t\u00b7zel", "seyn", "/", "ihr", "Jung\u00b7fern", "rah\u00b7tet", "doch", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VAINF", "$(", "PPOSAT", "NN", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch d\u00fcrfft ihr ja dabey nicht etwas arges meinen:", "tokens": ["Doch", "d\u00fcrfft", "ihr", "ja", "da\u00b7bey", "nicht", "et\u00b7was", "ar\u00b7ges", "mei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PAV", "PTKNEG", "ADV", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es ist ein weisser Leib der hat ein rundes Loch", "tokens": ["Es", "ist", "ein", "weis\u00b7ser", "Leib", "der", "hat", "ein", "run\u00b7des", "Loch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem Frauenzimmer sieht mans zwischen ihren Beinen/", "tokens": ["Dem", "Frau\u00b7en\u00b7zim\u00b7mer", "sieht", "mans", "zwi\u00b7schen", "ih\u00b7ren", "Bei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man steckt ein Ding hinein/ das hin und wieder f\u00e4hrt/", "tokens": ["Man", "steckt", "ein", "Ding", "hin\u00b7ein", "/", "das", "hin", "und", "wie\u00b7der", "f\u00e4hrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "PTKVZ", "$(", "PDS", "PTKVZ", "KON", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bi\u00df da\u00df der weisse Safft zusammen ist geronnen/", "tokens": ["Bi\u00df", "da\u00df", "der", "weis\u00b7se", "Safft", "zu\u00b7sam\u00b7men", "ist", "ge\u00b7ron\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "ADJA", "NN", "VVPP", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der in dem Bauche sich in einem Klump verkehrt/", "tokens": ["Der", "in", "dem", "Bau\u00b7che", "sich", "in", "ei\u00b7nem", "Klump", "ver\u00b7kehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die das errathen kan/ die hat den Prei\u00df gewonnen.", "tokens": ["Die", "das", "er\u00b7ra\u00b7then", "kan", "/", "die", "hat", "den", "Prei\u00df", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "VVINF", "VMFIN", "$(", "PDS", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "(ein Butterfa\u00df)", "tokens": ["(", "ein", "But\u00b7ter\u00b7fa\u00df", ")"], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}}}}}