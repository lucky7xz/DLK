{"textgrid.poem.60282": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Waldestrost", "genre": "verse", "period": "N.A.", "pub_year": 1832, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Walde schleicht ein alter Mann,", "tokens": ["Im", "Wal\u00b7de", "schleicht", "ein", "al\u00b7ter", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Allein mit seinem Leid,", "tokens": ["Al\u00b7lein", "mit", "sei\u00b7nem", "Leid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er ist so \u00e4rmlich angetan", "tokens": ["Er", "ist", "so", "\u00e4rm\u00b7lich", "an\u00b7ge\u00b7tan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit einem Lodenkleid.", "tokens": ["Mit", "ei\u00b7nem", "Lo\u00b7den\u00b7kleid", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er blickt so traurig um sich her,", "tokens": ["Er", "blickt", "so", "trau\u00b7rig", "um", "sich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An seinen Stab gelehnt;", "tokens": ["An", "sei\u00b7nen", "Stab", "ge\u00b7lehnt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem Manne ists im Herzen schwer,", "tokens": ["Dem", "Man\u00b7ne", "ists", "im", "Her\u00b7zen", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wonach er wohl sich sehnt?", "tokens": ["Wo\u00b7nach", "er", "wohl", "sich", "sehnt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Den B\u00e4umen nimmt der Herbst das Laub,", "tokens": ["Den", "B\u00e4u\u00b7men", "nimmt", "der", "Herbst", "das", "Laub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Tod im Walde tost,", "tokens": ["Der", "Tod", "im", "Wal\u00b7de", "tost", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Alte starret in den Staub,", "tokens": ["Der", "Al\u00b7te", "star\u00b7ret", "in", "den", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als sucht' er dort sich Trost.", "tokens": ["Als", "sucht'", "er", "dort", "sich", "Trost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "PRF", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Vom Dickicht rauscht vor ihn ein Reh", "tokens": ["Vom", "Di\u00b7ckicht", "rauscht", "vor", "ihn", "ein", "Reh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und h\u00e4lt und will nicht fliehn,", "tokens": ["Und", "h\u00e4lt", "und", "will", "nicht", "fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als w\u00e4rs ger\u00fchrt von seinem Weh,", "tokens": ["Als", "w\u00e4rs", "ge\u00b7r\u00fchrt", "von", "sei\u00b7nem", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als wollt es tr\u00f6sten ihn.", "tokens": ["Als", "wollt", "es", "tr\u00f6s\u00b7ten", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Schau tief dem Reh, du armer Mann,", "tokens": ["Schau", "tief", "dem", "Reh", ",", "du", "ar\u00b7mer", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In seinen Kindesblick,", "tokens": ["In", "sei\u00b7nen", "Kin\u00b7des\u00b7blick", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vielleicht der Blick dir lindern kann", "tokens": ["Viel\u00b7leicht", "der", "Blick", "dir", "lin\u00b7dern", "kann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein trauriges Geschick!", "tokens": ["Dein", "trau\u00b7ri\u00b7ges", "Ge\u00b7schick", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}