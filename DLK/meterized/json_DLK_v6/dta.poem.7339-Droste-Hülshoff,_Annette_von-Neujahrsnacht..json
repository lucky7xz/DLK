{"dta.poem.7339": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Neujahrsnacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Im grauen Schneegest\u00f6ber blassen", "tokens": ["Im", "grau\u00b7en", "Schnee\u00b7ge\u00b7st\u00f6\u00b7ber", "blas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Formen, es zerflie\u00dft der Raum,", "tokens": ["Die", "For\u00b7men", ",", "es", "zer\u00b7flie\u00dft", "der", "Raum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Laternen schwimmen durch die Gassen,", "tokens": ["La\u00b7ter\u00b7nen", "schwim\u00b7men", "durch", "die", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und leise knistert es im Flaum;", "tokens": ["Und", "lei\u00b7se", "knis\u00b7tert", "es", "im", "Flaum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schon naht des Jahres letzte Stunde,", "tokens": ["Schon", "naht", "des", "Jah\u00b7res", "letz\u00b7te", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und dr\u00fcben, wo der matte Schein", "tokens": ["Und", "dr\u00fc\u00b7ben", ",", "wo", "der", "mat\u00b7te", "Schein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Haucht aus den Fenstern der Rotunde,", "tokens": ["Haucht", "aus", "den", "Fens\u00b7tern", "der", "Ro\u00b7tun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Dort ziehn die frommen Beter ein.", "tokens": ["Dort", "ziehn", "die", "from\u00b7men", "Be\u00b7ter", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie zu dem Richter der Bedr\u00e4ngte,", "tokens": ["Wie", "zu", "dem", "Rich\u00b7ter", "der", "Be\u00b7dr\u00e4ng\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ob dessen Haupt die Wage neigt,", "tokens": ["Ob", "des\u00b7sen", "Haupt", "die", "Wa\u00b7ge", "neigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch einmal schleicht eh der verh\u00e4ngte,", "tokens": ["Noch", "ein\u00b7mal", "schleicht", "eh", "der", "ver\u00b7h\u00e4ng\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "KOUS", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der schwere Tag im Osten steigt,", "tokens": ["Der", "schwe\u00b7re", "Tag", "im", "Os\u00b7ten", "steigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch einmal faltet seine H\u00e4nde", "tokens": ["Noch", "ein\u00b7mal", "fal\u00b7tet", "sei\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Um milden Spruch, so knien sie dort,", "tokens": ["Um", "mil\u00b7den", "Spruch", ",", "so", "kni\u00b7en", "sie", "dort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Still gl\u00e4ubig, da\u00df ihr Flehen wende", "tokens": ["Still", "gl\u00e4u\u00b7big", ",", "da\u00df", "ihr", "Fle\u00b7hen", "wen\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des Jahres ernstes Losungswort.", "tokens": ["Des", "Jah\u00b7res", "erns\u00b7tes", "Lo\u00b7sungs\u00b7wort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich sehe unter meinem Fenster", "tokens": ["Ich", "se\u00b7he", "un\u00b7ter", "mei\u00b7nem", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie gleiten durch den Nebelrauch,", "tokens": ["Sie", "glei\u00b7ten", "durch", "den", "Ne\u00b7bel\u00b7rauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verh\u00fcllt und lautlos wie Gespenster,", "tokens": ["Ver\u00b7h\u00fcllt", "und", "laut\u00b7los", "wie", "Ge\u00b7spens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor ihrer Lippe flirrt der Hauch;", "tokens": ["Vor", "ih\u00b7rer", "Lip\u00b7pe", "flirrt", "der", "Hauch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein blasser Kreis zu ihren F\u00fc\u00dfen", "tokens": ["Ein", "blas\u00b7ser", "Kreis", "zu", "ih\u00b7ren", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zieht \u00fcber den verschneiten Grund,", "tokens": ["Zieht", "\u00fc\u00b7ber", "den", "ver\u00b7schnei\u00b7ten", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Lichtfunken blitzen auf und schie\u00dfen", "tokens": ["Licht\u00b7fun\u00b7ken", "blit\u00b7zen", "auf", "und", "schie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Um der Laterne dunstig Rund.", "tokens": ["Um", "der", "La\u00b7ter\u00b7ne", "duns\u00b7tig", "Rund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Was m\u00f6gen sie im Herzen tragen,", "tokens": ["Was", "m\u00f6\u00b7gen", "sie", "im", "Her\u00b7zen", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie manche Hoffnung, still bewacht!", "tokens": ["Wie", "man\u00b7che", "Hoff\u00b7nung", ",", "still", "be\u00b7wacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie mag es unterm Vlie\u00dfe schlagen", "tokens": ["Wie", "mag", "es", "un\u00b7term", "Vlie\u00b7\u00dfe", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So hei\u00df in dieser kalten Nacht!", "tokens": ["So", "hei\u00df", "in", "die\u00b7ser", "kal\u00b7ten", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Fort keuchen sie, als m\u00f6ge fallen", "tokens": ["Fort", "keu\u00b7chen", "sie", ",", "als", "m\u00f6\u00b7ge", "fal\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "KOUS", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Hammer, eh sie sich gebeugt,", "tokens": ["Der", "Ham\u00b7mer", ",", "eh", "sie", "sich", "ge\u00b7beugt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bevor sie an des Thrones Hallen", "tokens": ["Be\u00b7vor", "sie", "an", "des", "Thro\u00b7nes", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die letzte Bittschrift eingereicht.", "tokens": ["Die", "letz\u00b7te", "Bitt\u00b7schrift", "ein\u00b7ge\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dort h\u00f6r ich eine Angel rauschen,", "tokens": ["Dort", "h\u00f6r", "ich", "ei\u00b7ne", "An\u00b7gel", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vernehmlich wird des Kindes Schreyn,", "tokens": ["Ver\u00b7nehm\u00b7lich", "wird", "des", "Kin\u00b7des", "Schreyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und die Gestalt \u2014 sie scheint zu lauschen,", "tokens": ["Und", "die", "Ge\u00b7stalt", "sie", "scheint", "zu", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$(", "PPER", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dann f\u00fcrder schwimmt der Lampe Schein;", "tokens": ["Dann", "f\u00fcr\u00b7der", "schwimmt", "der", "Lam\u00b7pe", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch einmal steigt sie, l\u00e4\u00dft die Schimmer", "tokens": ["Noch", "ein\u00b7mal", "steigt", "sie", ",", "l\u00e4\u00dft", "die", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verzittern an des Fensters Rand,", "tokens": ["Ver\u00b7zit\u00b7tern", "an", "des", "Fens\u00b7ters", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gewi\u00df, sie tr\u00e4gt ein Frauenzimmer,", "tokens": ["Ge\u00b7wi\u00df", ",", "sie", "tr\u00e4gt", "ein", "Frau\u00b7en\u00b7zim\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und einer Mutter fromme Hand!", "tokens": ["Und", "ei\u00b7ner", "Mut\u00b7ter", "from\u00b7me", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Nun stampft es r\u00fcstig durch die Gasse,", "tokens": ["Nun", "stampft", "es", "r\u00fcs\u00b7tig", "durch", "die", "Gas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Decke kracht vom schweren Tritt,", "tokens": ["Die", "De\u00b7cke", "kracht", "vom", "schwe\u00b7ren", "Tritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Kr\u00e4mer schleppt die S\u00fcndenmasse", "tokens": ["Der", "Kr\u00e4\u00b7mer", "schleppt", "die", "S\u00fcn\u00b7den\u00b7mas\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der b\u00f6sen Zahler keuchend mit;", "tokens": ["Der", "b\u00f6\u00b7sen", "Zah\u00b7ler", "keu\u00b7chend", "mit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und hinter ihm wie eine Docke", "tokens": ["Und", "hin\u00b7ter", "ihm", "wie", "ei\u00b7ne", "Do\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein armes Kind im Flitterstaat,", "tokens": ["Ein", "ar\u00b7mes", "Kind", "im", "Flit\u00b7ter\u00b7staat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit seidnem F\u00e4hnchen, seidner Locke,", "tokens": ["Mit", "seid\u00b7nem", "F\u00e4hn\u00b7chen", ",", "seid\u00b7ner", "Lo\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Huscht frierend durch den engen Pfad.", "tokens": ["Huscht", "frie\u00b7rend", "durch", "den", "en\u00b7gen", "Pfad", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ha, Schellenklingeln l\u00e4ngs der Stiege!", "tokens": ["Ha", ",", "Schel\u00b7len\u00b7klin\u00b7geln", "l\u00e4ngs", "der", "Stie\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Glutaugen richtend in die H\u00f6h',", "tokens": ["Glu\u00b7tau\u00b7gen", "rich\u00b7tend", "in", "die", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "'ne kolossale Feuerfliege,", "tokens": ["'ne", "ko\u00b7los\u00b7sa\u00b7le", "Feu\u00b7er\u00b7flie\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rauscht die Karosse durch den Schnee;", "tokens": ["Rauscht", "die", "Ka\u00b7ros\u00b7se", "durch", "den", "Schnee", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Und D\u00e4mpfe qualmen auf und schlagen", "tokens": ["Und", "D\u00e4mp\u00b7fe", "qual\u00b7men", "auf", "und", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PTKVZ", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zur\u00fcck vom Wirbel des Gespanns;", "tokens": ["Zu\u00b7r\u00fcck", "vom", "Wir\u00b7bel", "des", "Ge\u00b7spanns", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ja, schwere B\u00fcrde tr\u00e4gt der Wagen,", "tokens": ["Ja", ",", "schwe\u00b7re", "B\u00fcr\u00b7de", "tr\u00e4gt", "der", "Wa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die W\u00fcnsche eines reichen Manns!", "tokens": ["Die", "W\u00fcn\u00b7sche", "ei\u00b7nes", "rei\u00b7chen", "Manns", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und hinter ihm ein Licht so schwankend,", "tokens": ["Und", "hin\u00b7ter", "ihm", "ein", "Licht", "so", "schwan\u00b7kend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Tr\u00e4ger tritt so sachte auf,", "tokens": ["Der", "Tr\u00e4\u00b7ger", "tritt", "so", "sach\u00b7te", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun lehnt er an der Mauer, wankend,", "tokens": ["Nun", "lehnt", "er", "an", "der", "Mau\u00b7er", ",", "wan\u00b7kend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein hohler Husten schallt hinauf;", "tokens": ["Sein", "hoh\u00b7ler", "Hus\u00b7ten", "schallt", "hin\u00b7auf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er \u00f6ffnet der Laterne Reifen,", "tokens": ["Er", "\u00f6ff\u00b7net", "der", "La\u00b7ter\u00b7ne", "Rei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Es zupfen Finger lang und fahl", "tokens": ["Es", "zup\u00b7fen", "Fin\u00b7ger", "lang", "und", "fahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Am Dochte, Odemz\u00fcge pfeifen, \u2014", "tokens": ["Am", "Doch\u00b7te", ",", "O\u00b7dem\u00b7z\u00fc\u00b7ge", "pfei\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "$,", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Du, Armer, kniest zum letztenmal.", "tokens": ["Du", ",", "Ar\u00b7mer", ",", "kniest", "zum", "letz\u00b7ten\u00b7mal", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "APPRART", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Dann Licht an Lichtern l\u00e4ngs der Mauer,", "tokens": ["Dann", "Licht", "an", "Lich\u00b7tern", "l\u00e4ngs", "der", "Mau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Meteore irr geschaart,", "tokens": ["Wie", "Me\u00b7te\u00b7o\u00b7re", "irr", "ge\u00b7schaart", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NE", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein krankes Weib, in tiefer Trauer,", "tokens": ["Ein", "kran\u00b7kes", "Weib", ",", "in", "tie\u00b7fer", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Husaren mit bereiftem Bart,", "tokens": ["Hu\u00b7sa\u00b7ren", "mit", "be\u00b7reif\u00b7tem", "Bart", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In Filz und Kittel st\u00e4mmge Bauern,", "tokens": ["In", "Filz", "und", "Kit\u00b7tel", "st\u00e4mm\u00b7ge", "Bau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Rosenkranz in starrer Faust,", "tokens": ["Den", "Ro\u00b7sen\u00b7kranz", "in", "star\u00b7rer", "Faust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und M\u00e4dchen die wie Falken lauern,", "tokens": ["Und", "M\u00e4d\u00b7chen", "die", "wie", "Fal\u00b7ken", "lau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "KOKOM", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von Mantels Fittigen umsaust.", "tokens": ["Von", "Man\u00b7tels", "Fit\u00b7ti\u00b7gen", "um\u00b7saust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Wie oft hab' ich als Kind im Spiele", "tokens": ["Wie", "oft", "hab'", "ich", "als", "Kind", "im", "Spie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "KOUS", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gelauscht den Funken im Papier,", "tokens": ["Ge\u00b7lauscht", "den", "Fun\u00b7ken", "im", "Pa\u00b7pier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sternchen zitterndem Gew\u00fchle,", "tokens": ["Der", "Stern\u00b7chen", "zit\u00b7tern\u00b7dem", "Ge\u00b7w\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und: \u201eKircheng\u00e4nger!\u201c sagten wir;", "tokens": ["Und", ":", "\u201e", "Kir\u00b7chen\u00b7g\u00e4n\u00b7ger", "!", "\u201c", "sag\u00b7ten", "wir", ";"], "token_info": ["word", "punct", "punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "$.", "$(", "NN", "$.", "$(", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So seh' ichs wimmeln um die Wette", "tokens": ["So", "seh'", "ichs", "wim\u00b7meln", "um", "die", "Wet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und l\u00f6schen, wo der Pfad sich eint,", "tokens": ["Und", "l\u00f6\u00b7schen", ",", "wo", "der", "Pfad", "sich", "eint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PWAV", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nachz\u00fcgler noch, dann grau die St\u00e4tte,", "tokens": ["Nach\u00b7z\u00fcg\u00b7ler", "noch", ",", "dann", "grau", "die", "St\u00e4t\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nur einsam die Rotunde scheint.", "tokens": ["Nur", "ein\u00b7sam", "die", "Ro\u00b7tun\u00b7de", "scheint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.11": {"line.1": {"text": "Und m\u00e4hlig schwellen Orgelkl\u00e4nge", "tokens": ["Und", "m\u00e4h\u00b7lig", "schwel\u00b7len", "Or\u00b7gel\u00b7kl\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Heroldsrufe an mein Ohr:", "tokens": ["Wie", "He\u00b7rolds\u00b7ru\u00b7fe", "an", "mein", "Ohr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Knie nieder, L\u00e4ssiger, und dr\u00e4nge", "tokens": ["Knie", "nie\u00b7der", ",", "L\u00e4s\u00b7si\u00b7ger", ",", "und", "dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auch deines Herzens Wunsch hervor!", "tokens": ["Auch", "dei\u00b7nes", "Her\u00b7zens", "Wunsch", "her\u00b7vor", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u201edu, dem Jahrtausende verrollen", "tokens": ["\u201e", "du", ",", "dem", "Jahr\u00b7tau\u00b7sen\u00b7de", "ver\u00b7rol\u00b7len"], "token_info": ["punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "$,", "ART", "NN", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Secundengleich, erhalte mir", "tokens": ["Se\u00b7cun\u00b7den\u00b7gleich", ",", "er\u00b7hal\u00b7te", "mir"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADJD", "$,", "VVFIN", "PPER"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Ein muthig Herz, ein redlich Wollen,", "tokens": ["Ein", "mut\u00b7hig", "Herz", ",", "ein", "red\u00b7lich", "Wol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und Fassung an des Grabes Th\u00fcr.\u201c", "tokens": ["Und", "Fas\u00b7sung", "an", "des", "Gra\u00b7bes", "Th\u00fcr.", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "abbreviation", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "NE", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Da, horch! \u2014 es summt durch Wind und Schlossen,", "tokens": ["Da", ",", "horch", "!", "es", "summt", "durch", "Wind", "und", "Schlos\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVIMP", "$.", "$(", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Gott gnade uns, hin ist das Jahr!", "tokens": ["Gott", "gna\u00b7de", "uns", ",", "hin", "ist", "das", "Jahr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Schneegest\u00e4ub' wie Schnee zerflossen,", "tokens": ["Im", "Schnee\u00b7ge\u00b7st\u00e4ub'", "wie", "Schnee", "zer\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KOKOM", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zuk\u00fcnftiges wird offenbar;", "tokens": ["Zu\u00b7k\u00fcnf\u00b7ti\u00b7ges", "wird", "of\u00b7fen\u00b7bar", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von allen Th\u00fcrmen um die Wette", "tokens": ["Von", "al\u00b7len", "Th\u00fcr\u00b7men", "um", "die", "Wet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der H\u00e4mmer Schl\u00e4ge, da\u00df es schallt,", "tokens": ["Der", "H\u00e4m\u00b7mer", "Schl\u00e4\u00b7ge", ",", "da\u00df", "es", "schallt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und mit dem letzten ist die St\u00e4tte", "tokens": ["Und", "mit", "dem", "letz\u00b7ten", "ist", "die", "St\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gelichtet f\u00fcr den neuen Wald.", "tokens": ["Ge\u00b7lich\u00b7tet", "f\u00fcr", "den", "neu\u00b7en", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}