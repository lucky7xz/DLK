{"dta.poem.1621": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der edle Schecken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wie schade/ da\u00df ich hier nunmehr darnieder liege!", "tokens": ["Wie", "scha\u00b7de", "/", "da\u00df", "ich", "hier", "nun\u00b7mehr", "dar\u00b7nie\u00b7der", "lie\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$(", "KOUS", "PPER", "ADV", "ADV", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich zeugte Zimber-Land/ ein Schiff war meine Wiege/", "tokens": ["Mich", "zeug\u00b7te", "Zim\u00b7ber\u00b7Land", "/", "ein", "Schiff", "war", "mei\u00b7ne", "Wie\u00b7ge", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$(", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Kleid war Schnee und Sammt/ mein Auge voller", "tokens": ["Mein", "Kleid", "war", "Schnee", "und", "Sammt", "/", "mein", "Au\u00b7ge", "vol\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "KON", "NN", "$(", "PPOSAT", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Glutt/", "tokens": ["Glutt", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Mein Schenckel voller Krafft/ mein Hertze voller Mutt.", "tokens": ["Mein", "Schen\u00b7ckel", "vol\u00b7ler", "Krafft", "/", "mein", "Hert\u00b7ze", "vol\u00b7ler", "Mutt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$(", "PPOSAT", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mein spielend Maul voll Schaum; mein wohl-gew\u00f6lbter", "tokens": ["Mein", "spie\u00b7lend", "Maul", "voll", "Schaum", ";", "mein", "wohl\u00b7ge\u00b7w\u00f6lb\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN", "ADJD", "NN", "$.", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "R\u00fccken/", "tokens": ["R\u00fc\u00b7cken", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Trug jeden starcken Mann: Ich wuste nichts von T\u00fccken/", "tokens": ["Trug", "je\u00b7den", "star\u00b7cken", "Mann", ":", "Ich", "wus\u00b7te", "nichts", "von", "T\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "$.", "PPER", "VVFIN", "PIS", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die unanst\u00e4ndig seyn; nur gerne war ich frey/", "tokens": ["Die", "un\u00b7an\u00b7st\u00e4n\u00b7dig", "seyn", ";", "nur", "ger\u00b7ne", "war", "ich", "frey", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAINF", "$.", "ADV", "ADV", "VAFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und lie\u00df mir nicht den Schmid von hinten kommen bey.", "tokens": ["Und", "lie\u00df", "mir", "nicht", "den", "Schmid", "von", "hin\u00b7ten", "kom\u00b7men", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "APPR", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Piastens lezter Sohn hat mich mit Lust beschritten/", "tokens": ["Pi\u00b7as\u00b7tens", "lez\u00b7ter", "Sohn", "hat", "mich", "mit", "Lust", "be\u00b7schrit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VAFIN", "PRF", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn er den Kopff gef\u00e4llt/ wenn er den Ring erritten.", "tokens": ["Wenn", "er", "den", "Kopff", "ge\u00b7f\u00e4llt", "/", "wenn", "er", "den", "Ring", "er\u00b7rit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$(", "KOUS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mein Gl\u00fccke fiel mit ihm/ wies an den H\u00f6fen geht/", "tokens": ["Mein", "Gl\u00fc\u00b7cke", "fiel", "mit", "ihm", "/", "wies", "an", "den", "H\u00f6\u00b7fen", "geht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$(", "VVFIN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Wo auff des Herren Heyl der Diener Wohlfart steht.", "tokens": ["Wo", "auff", "des", "Her\u00b7ren", "Heyl", "der", "Die\u00b7ner", "Wohl\u00b7fart", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Doch fiel mir nicht der Mutt: wer sich nur wohl gehalten/", "tokens": ["Doch", "fiel", "mir", "nicht", "der", "Mutt", ":", "wer", "sich", "nur", "wohl", "ge\u00b7hal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$.", "PWS", "PRF", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und redlich hat gedient/ kan doch mit Ehren alten/", "tokens": ["Und", "red\u00b7lich", "hat", "ge\u00b7dient", "/", "kan", "doch", "mit", "Eh\u00b7ren", "al\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "VVPP", "$(", "VMFIN", "ADV", "APPR", "NN", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die Liebe/ die mein Herr den Seinen hinterlie\u00df/", "tokens": ["Die", "Lie\u00b7be", "/", "die", "mein", "Herr", "den", "Sei\u00b7nen", "hin\u00b7ter\u00b7lie\u00df", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "PPOSAT", "NN", "ART", "PPOSS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die Treue/ die ich ihm nach meiner Pflicht erwie\u00df/", "tokens": ["Die", "Treu\u00b7e", "/", "die", "ich", "ihm", "nach", "mei\u00b7ner", "Pflicht", "er\u00b7wie\u00df", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Hat mir noch so viel Gunst bey Lebens-Zeit erworben/", "tokens": ["Hat", "mir", "noch", "so", "viel", "Gunst", "bey", "Le\u00b7bens\u00b7Zeit", "er\u00b7wor\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PIAT", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.20": {"text": "Da\u00df ich/ der Jahre satt/ im Herren Dienst gestorben.", "tokens": ["Da\u00df", "ich", "/", "der", "Jah\u00b7re", "satt", "/", "im", "Her\u00b7ren", "Dienst", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "ART", "NN", "ADJD", "$(", "APPRART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Zieht manch veraltet Pferd den Karn und Glocken-Strang/", "tokens": ["Zieht", "manch", "ver\u00b7al\u00b7tet", "Pferd", "den", "Karn", "und", "Glo\u00b7cken\u00b7Strang", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "VVFIN", "NN", "ART", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Dar\u00fcber klag ich nicht/ und wei\u00df dem Herren Danck/", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "klag", "ich", "nicht", "/", "und", "wei\u00df", "dem", "Her\u00b7ren", "Danck", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "$(", "KON", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der mich bey Ruhe/ Lust und Futter lassen leben/", "tokens": ["Der", "mich", "bey", "Ru\u00b7he", "/", "Lust", "und", "Fut\u00b7ter", "las\u00b7sen", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "$(", "NN", "KON", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und keiner schn\u00f6den Hand zu Diensten untergeben.", "tokens": ["Und", "kei\u00b7ner", "schn\u00f6\u00b7den", "Hand", "zu", "Diens\u00b7ten", "un\u00b7ter\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der mich/ so alt als ie nicht leicht ein Pferd/ beklagt/", "tokens": ["Der", "mich", "/", "so", "alt", "als", "ie", "nicht", "leicht", "ein", "Pferd", "/", "be\u00b7klagt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "$(", "ADV", "ADJD", "KOKOM", "ADV", "PTKNEG", "ADJD", "ART", "NN", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und mehre Kosten noch auff mein Ged\u00e4chtnis wagt.", "tokens": ["Und", "meh\u00b7re", "Kos\u00b7ten", "noch", "auff", "mein", "Ge\u00b7d\u00e4cht\u00b7nis", "wagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Wer saget aber/ da\u00df ich hier darnieder liege?", "tokens": ["Wer", "sa\u00b7get", "a\u00b7ber", "/", "da\u00df", "ich", "hier", "dar\u00b7nie\u00b7der", "lie\u00b7ge", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "$(", "KOUS", "PPER", "ADV", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ich liegen? stand ich nicht in meiner ersten Wiege?", "tokens": ["Ich", "lie\u00b7gen", "?", "stand", "ich", "nicht", "in", "mei\u00b7ner", "ers\u00b7ten", "Wie\u00b7ge", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wer mich gesehen hat wird noch ein Zeuge seyn/", "tokens": ["Wer", "mich", "ge\u00b7se\u00b7hen", "hat", "wird", "noch", "ein", "Zeu\u00b7ge", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VAFIN", "VAFIN", "ADV", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wie sich mein Haubt zum Schlaff im stehn gewieget ein.", "tokens": ["Wie", "sich", "mein", "Haubt", "zum", "Schlaff", "im", "stehn", "ge\u00b7wie\u00b7get", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PPOSAT", "NN", "APPRART", "NN", "APPRART", "VVINF", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Nach muntrer Fohlen Art scherzt ich mit Sprung und", "tokens": ["Nach", "mun\u00b7trer", "Foh\u00b7len", "Art", "scherzt", "ich", "mit", "Sprung", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN", "VVFIN", "PPER", "APPR", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Streiche/", "tokens": ["Strei\u00b7che", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.33": {"text": "Kein Riese Pferdes-Art th\u00e4t mirs an K\u00fcnheit gleiche/", "tokens": ["Kein", "Rie\u00b7se", "Pfer\u00b7des\u00b7Art", "th\u00e4t", "mirs", "an", "K\u00fcn\u00b7heit", "glei\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVFIN", "NE", "APPR", "NN", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Bi\u00df Reuter/ Flei\u00df und Kunst mich in die Ordnung bracht/", "tokens": ["Bi\u00df", "Reu\u00b7ter", "/", "Flei\u00df", "und", "Kunst", "mich", "in", "die", "Ord\u00b7nung", "bracht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$(", "NN", "KON", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und ein bequemes Ro\u00df voll Mutt aus mir gemacht.", "tokens": ["Und", "ein", "be\u00b7que\u00b7mes", "Ro\u00df", "voll", "Mutt", "aus", "mir", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJD", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Die Jahre wolten mir zwar das Verm\u00f6gen brechen;", "tokens": ["Die", "Jah\u00b7re", "wol\u00b7ten", "mir", "zwar", "das", "Ver\u00b7m\u00f6\u00b7gen", "bre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Sie konten nicht das Hertz/ als wie die Lunge/ schw\u00e4chen:", "tokens": ["Sie", "kon\u00b7ten", "nicht", "das", "Hertz", "/", "als", "wie", "die", "Lun\u00b7ge", "/", "schw\u00e4\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ART", "NN", "$(", "KOUS", "KOKOM", "ART", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ich schritt und drabte frey: und wenn mich nicht die Macht", "tokens": ["Ich", "schritt", "und", "drab\u00b7te", "frey", ":", "und", "wenn", "mich", "nicht", "die", "Macht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "$.", "KON", "KOUS", "PPER", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Des Todes unverhofft in seine Stricke bracht/", "tokens": ["Des", "To\u00b7des", "un\u00b7ver\u00b7hofft", "in", "sei\u00b7ne", "Stri\u00b7cke", "bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ich w\u00e4r ihm noch zulezt aus Stall und Stand gesprungen.", "tokens": ["Ich", "w\u00e4r", "ihm", "noch", "zu\u00b7lezt", "aus", "Stall", "und", "Stand", "ge\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ist mir der lezte Sprung nun gleich nicht wohl gelungen/", "tokens": ["Ist", "mir", "der", "lez\u00b7te", "Sprung", "nun", "gleich", "nicht", "wohl", "ge\u00b7lun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "ADV", "ADV", "PTKNEG", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "So fehl