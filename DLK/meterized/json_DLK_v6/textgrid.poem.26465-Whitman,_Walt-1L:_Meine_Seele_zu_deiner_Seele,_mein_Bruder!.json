{"textgrid.poem.26465": {"metadata": {"author": {"name": "Whitman, Walt", "birth": "N.A.", "death": "N.A."}, "title": "1L: Meine Seele zu deiner Seele, mein Bruder!", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Seele zu deiner Seele, mein Bruder!", "tokens": ["Mei\u00b7ne", "See\u00b7le", "zu", "dei\u00b7ner", "See\u00b7le", ",", "mein", "Bru\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Z\u00fcrne du nicht, weil die vielen, die laut deinen Namen rufen,", "tokens": ["Z\u00fcr\u00b7ne", "du", "nicht", ",", "weil", "die", "vie\u00b7len", ",", "die", "laut", "dei\u00b7nen", "Na\u00b7men", "ru\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "ART", "PIAT", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Dich nicht verstehen;", "tokens": ["Dich", "nicht", "ver\u00b7ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ich nenne deinen Namen nicht laut,", "tokens": ["Ich", "nen\u00b7ne", "dei\u00b7nen", "Na\u00b7men", "nicht", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Aber ich verstehe dich.", "tokens": ["A\u00b7ber", "ich", "ver\u00b7ste\u00b7he", "dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich erkenne dich jubelnd, du mein Gef\u00e4hrte!", "tokens": ["Ich", "er\u00b7ken\u00b7ne", "dich", "ju\u00b7belnd", ",", "du", "mein", "Ge\u00b7f\u00e4hr\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "$,", "PPER", "PPOSAT", "NN", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Ich gr\u00fc\u00dfe dich und alle, die mit dir sind,", "tokens": ["Ich", "gr\u00fc\u00b7\u00dfe", "dich", "und", "al\u00b7le", ",", "die", "mit", "dir", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "PIS", "$,", "PRELS", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Vorher und nachher, auch die, die da kommen werden,", "tokens": ["Vor\u00b7her", "und", "nach\u00b7her", ",", "auch", "die", ",", "die", "da", "kom\u00b7men", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "$,", "ADV", "ART", "$,", "PRELS", "ADV", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Auf da\u00df wir alle miteinander, f\u00fcreinander,", "tokens": ["Auf", "da\u00df", "wir", "al\u00b7le", "mi\u00b7tein\u00b7an\u00b7der", ",", "f\u00fc\u00b7rein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PIS", "ADV", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nacheinander arbeiten und das anvertraute Pfand", "tokens": ["Na\u00b7ch\u00b7ein\u00b7an\u00b7der", "ar\u00b7bei\u00b7ten", "und", "das", "an\u00b7ver\u00b7trau\u00b7te", "Pfand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "KON", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Weitergeben,", "tokens": ["Wei\u00b7ter\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Wir wenigen Gleichartigen,", "tokens": ["Wir", "we\u00b7ni\u00b7gen", "Gleich\u00b7ar\u00b7ti\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Unabh\u00e4ngig von L\u00e4ndern und Zeiten,", "tokens": ["Un\u00b7ab\u00b7h\u00e4n\u00b7gig", "von", "L\u00e4n\u00b7dern", "und", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Alle Staaten, V\u00f6lker, Rassen und Klassen umfassend,", "tokens": ["Al\u00b7le", "Staa\u00b7ten", ",", "V\u00f6l\u00b7ker", ",", "Ras\u00b7sen", "und", "Klas\u00b7sen", "um\u00b7fas\u00b7send", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVPP", "$,"], "meter": "--+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Alle theologischen Lehren duldend,", "tokens": ["Al\u00b7le", "theo\u00b7lo\u00b7gi\u00b7schen", "Leh\u00b7ren", "dul\u00b7dend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Mitf\u00fchlende, Mitsehende, Berichterstatter der Menschen:", "tokens": ["Mit\u00b7f\u00fch\u00b7len\u00b7de", ",", "Mit\u00b7se\u00b7hen\u00b7de", ",", "Be\u00b7rich\u00b7ter\u00b7stat\u00b7ter", "der", "Men\u00b7schen", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "ART", "NN", "$."], "meter": "-+--+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Wir wandern schweigend zwischen", "tokens": ["Wir", "wan\u00b7dern", "schwei\u00b7gend", "zwi\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Streitfragen und Behauptungen,", "tokens": ["Streit\u00b7fra\u00b7gen", "und", "Be\u00b7haup\u00b7tun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Wir weisen die Streitenden nicht zur\u00fcck,", "tokens": ["Wir", "wei\u00b7sen", "die", "Strei\u00b7ten\u00b7den", "nicht", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Noch die Behauptungen;", "tokens": ["Noch", "die", "Be\u00b7haup\u00b7tun\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "+--++-", "measure": "dactylic.init"}, "line.5": {"text": "Wir h\u00f6ren das Schreien und Schimpfen,", "tokens": ["Wir", "h\u00f6\u00b7ren", "das", "Schrei\u00b7en", "und", "Schimp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Wir werden umstellt von allen Seiten,", "tokens": ["Wir", "wer\u00b7den", "um\u00b7stellt", "von", "al\u00b7len", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Von Parteien, Eifers\u00fcchteleien, Anma\u00dfungen \u2013", "tokens": ["Von", "Par\u00b7tei\u00b7en", ",", "Ei\u00b7fer\u00b7s\u00fcch\u00b7te\u00b7lei\u00b7en", ",", "An\u00b7ma\u00b7\u00dfun\u00b7gen", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "$,", "NN", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.9": {"line.1": {"text": "Sie dringen auf uns ein, umkreisen uns, mein Kamerad,", "tokens": ["Sie", "drin\u00b7gen", "auf", "uns", "ein", ",", "um\u00b7krei\u00b7sen", "uns", ",", "mein", "Ka\u00b7me\u00b7rad", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$,", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.10": {"line.1": {"text": "Dennoch wandern wir ungehemmt und frei", "tokens": ["Den\u00b7noch", "wan\u00b7dern", "wir", "un\u00b7ge\u00b7hemmt", "und", "frei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "\u00dcber die ganze Welt,", "tokens": ["\u00dc\u00b7ber", "die", "gan\u00b7ze", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ziehen hin und her,", "tokens": ["Zie\u00b7hen", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Bis unsre unausl\u00f6schlichen Spuren sich eingraben", "tokens": ["Bis", "uns\u00b7re", "un\u00b7aus\u00b7l\u00f6schli\u00b7chen", "Spu\u00b7ren", "sich", "ein\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In Zeiten und Entwickelungen,", "tokens": ["In", "Zei\u00b7ten", "und", "Ent\u00b7wi\u00b7cke\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bis wir die Zeiten und Entwickelungen", "tokens": ["Bis", "wir", "die", "Zei\u00b7ten", "und", "Ent\u00b7wi\u00b7cke\u00b7lun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ges\u00e4ttigt haben,", "tokens": ["Ge\u00b7s\u00e4t\u00b7tigt", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Und die M\u00e4nner und Frauen", "tokens": ["Und", "die", "M\u00e4n\u00b7ner", "und", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Kommender Zeiten und Rassen", "tokens": ["Kom\u00b7men\u00b7der", "Zei\u00b7ten", "und", "Ras\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.10": {"text": "Br\u00fcder und Liebende werden,", "tokens": ["Br\u00fc\u00b7der", "und", "Lie\u00b7ben\u00b7de", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.11": {"text": "Wie wir.", "tokens": ["Wie", "wir", "."], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "PPER", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}