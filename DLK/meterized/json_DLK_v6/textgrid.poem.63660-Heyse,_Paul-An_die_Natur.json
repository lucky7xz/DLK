{"textgrid.poem.63660": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "An die Natur", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Bilderbuch, Allmutter Natur,", "tokens": ["Dein", "Bil\u00b7der\u00b7buch", ",", "A\u00b7llmut\u00b7ter", "Na\u00b7tur", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Drin Jahreszeiten", "tokens": ["Drin", "Jah\u00b7res\u00b7zei\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADV", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und Sternenheere,", "tokens": ["Und", "Ster\u00b7nen\u00b7hee\u00b7re", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "L\u00e4nder und Meere", "tokens": ["L\u00e4n\u00b7der", "und", "Mee\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Vor\u00fcbergleiten,", "tokens": ["Vor\u00b7\u00fc\u00b7berg\u00b7lei\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Gibst du den gro\u00dfen,", "tokens": ["Gibst", "du", "den", "gro\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Ewig unm\u00fcndigen", "tokens": ["E\u00b7wig", "un\u00b7m\u00fcn\u00b7di\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADJD", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Kindern zu schauen,", "tokens": ["Kin\u00b7dern", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Bis ihnen sp\u00e4t im Abendgrauen", "tokens": ["Bis", "ih\u00b7nen", "sp\u00e4t", "im", "A\u00b7bend\u00b7grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Vom Bl\u00e4ttern matt", "tokens": ["Vom", "Bl\u00e4t\u00b7tern", "matt"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Die Hand hinsinkt auf das letzte Blatt.", "tokens": ["Die", "Hand", "hin\u00b7sinkt", "auf", "das", "letz\u00b7te", "Blatt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Aber der Dichter, der gro\u00dfen Kinder", "tokens": ["A\u00b7ber", "der", "Dich\u00b7ter", ",", "der", "gro\u00b7\u00dfen", "Kin\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Eigensinnigstes, wunderlichstes,", "tokens": ["Ei\u00b7gen\u00b7sin\u00b7nigs\u00b7tes", ",", "wun\u00b7der\u00b7lichs\u00b7tes", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJA", "$,", "ADJA", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Am Meeresstrand", "tokens": ["Am", "Mee\u00b7res\u00b7strand"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Sitzt er und h\u00e4lt in tr\u00e4umender Hand", "tokens": ["Sitzt", "er", "und", "h\u00e4lt", "in", "tr\u00e4u\u00b7men\u00b7der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Die bunte Muschel und horcht mit Sinnen", "tokens": ["Die", "bun\u00b7te", "Mu\u00b7schel", "und", "horcht", "mit", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "APPR", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Dem Brausen drinnen.", "tokens": ["Dem", "Brau\u00b7sen", "drin\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Dann versucht er, im kleinen Rund", "tokens": ["Dann", "ver\u00b7sucht", "er", ",", "im", "klei\u00b7nen", "Rund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPRART", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Auszusch\u00f6pfen den Meeresgrund,", "tokens": ["Aus\u00b7zu\u00b7sch\u00f6p\u00b7fen", "den", "Mee\u00b7res\u00b7grund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.9": {"text": "Indes mit Hohngel\u00e4chter die andern", "tokens": ["In\u00b7des", "mit", "Hohn\u00b7ge\u00b7l\u00e4ch\u00b7ter", "die", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "ART", "ADJA"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Vor\u00fcberwandern:", "tokens": ["Vor\u00b7\u00fc\u00b7ber\u00b7wan\u00b7dern", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Seht den Toren,", "tokens": ["Seht", "den", "To\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "In sein vergebliches Spiel verloren!", "tokens": ["In", "sein", "ver\u00b7geb\u00b7li\u00b7ches", "Spiel", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Du aber, hehre Mutter,", "tokens": ["Du", "a\u00b7ber", ",", "heh\u00b7re", "Mut\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Blickst milde l\u00e4chelnd", "tokens": ["Blickst", "mil\u00b7de", "l\u00e4\u00b7chelnd"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADJD", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Auf dein Scho\u00dfkind,", "tokens": ["Auf", "dein", "Scho\u00df\u00b7kind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Und in den Schaum, der verspr\u00fcht im Sand,", "tokens": ["Und", "in", "den", "Schaum", ",", "der", "ver\u00b7spr\u00fcht", "im", "Sand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "VVFIN", "APPRART", "NN", "$,"], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Streut deine Hand", "tokens": ["Streut", "dei\u00b7ne", "Hand"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Perlen, mit denen entz\u00fcckt", "tokens": ["Per\u00b7len", ",", "mit", "de\u00b7nen", "ent\u00b7z\u00fcckt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "PRELS", "VVPP"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Er seiner Liebsten Haupt und Busen schm\u00fcckt.", "tokens": ["Er", "sei\u00b7ner", "Liebs\u00b7ten", "Haupt", "und", "Bu\u00b7sen", "schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}