{"textgrid.poem.65902": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Eleusis", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit deinen Fackeln, Demeter, entsteigen", "tokens": ["Mit", "dei\u00b7nen", "Fa\u00b7ckeln", ",", "De\u00b7me\u00b7ter", ",", "ent\u00b7stei\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "$,", "VVFIN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir Sterblichen, dem Hades zu, der Erde.", "tokens": ["Wir", "Sterb\u00b7li\u00b7chen", ",", "dem", "Ha\u00b7des", "zu", ",", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ART", "NN", "PTKVZ", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Fieberfinsterung beschnuppern Pferde", "tokens": ["In", "Fie\u00b7ber\u00b7fins\u00b7te\u00b7rung", "be\u00b7schnup\u00b7pern", "Pfer\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Hei\u00dfverheimlichten, bei Heil und Schweigen,", "tokens": ["Der", "Hei\u00df\u00b7ver\u00b7heim\u00b7lich\u00b7ten", ",", "bei", "Heil", "und", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun meine Schenkel wohl; die Schultern neigen", "tokens": ["Nun", "mei\u00b7ne", "Schen\u00b7kel", "wohl", ";", "die", "Schul\u00b7tern", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Kopf, voll Blutlast, zwischen die Beschwerde", "tokens": ["Den", "Kopf", ",", "voll", "Blut\u00b7last", ",", "zwi\u00b7schen", "die", "Be\u00b7schwer\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADJD", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bem\u00fchter Mutiger um Plutos Herde:", "tokens": ["Be\u00b7m\u00fch\u00b7ter", "Mu\u00b7ti\u00b7ger", "um", "Plu\u00b7tos", "Her\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gespensterte umglasten uns im Reigen.", "tokens": ["Ge\u00b7spens\u00b7ter\u00b7te", "um\u00b7glas\u00b7ten", "uns", "im", "Rei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wir werden stumm: uns Zukunft zu erfahren!", "tokens": ["Wir", "wer\u00b7den", "stumm", ":", "uns", "Zu\u00b7kunft", "zu", "er\u00b7fah\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$.", "PPER", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hier wallt die Welt: ich warte bei Enthauchten,", "tokens": ["Hier", "wallt", "die", "Welt", ":", "ich", "war\u00b7te", "bei", "Ent\u00b7hauch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erkennbar noch am Urgeruch von Haaren;", "tokens": ["Er\u00b7kenn\u00b7bar", "noch", "am", "Ur\u00b7ge\u00b7ruch", "von", "Haa\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPRART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nun findet mich mein Hund: die mir enttauchten", "tokens": ["Nun", "fin\u00b7det", "mich", "mein", "Hund", ":", "die", "mir", "ent\u00b7tauch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bekannten aber kann kein Sinn, in Scharen", "tokens": ["Be\u00b7kann\u00b7ten", "a\u00b7ber", "kann", "kein", "Sinn", ",", "in", "Scha\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "VMFIN", "PIAT", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der schon Erlauchten suchend, wo gewahren.", "tokens": ["Der", "schon", "Er\u00b7lauch\u00b7ten", "su\u00b7chend", ",", "wo", "ge\u00b7wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VVPP", "$,", "PWAV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}