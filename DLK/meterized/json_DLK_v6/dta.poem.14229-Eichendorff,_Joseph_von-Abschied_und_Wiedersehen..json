{"dta.poem.14229": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Abschied und Wiedersehen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Was sind's f\u00fcr Kl\u00e4nge, die an's Fenster flogen?", "tokens": ["Was", "sin\u00b7d's", "f\u00fcr", "Kl\u00e4n\u00b7ge", ",", "die", "an's", "Fens\u00b7ter", "flo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "NE", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So altbekannt verlocken diese Lieder,", "tokens": ["So", "alt\u00b7be\u00b7kannt", "ver\u00b7lo\u00b7cken", "die\u00b7se", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein S\u00e4nger steht im schwanken D\u00e4mmerschein.", "tokens": ["Ein", "S\u00e4n\u00b7ger", "steht", "im", "schwan\u00b7ken", "D\u00e4m\u00b7mer\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}