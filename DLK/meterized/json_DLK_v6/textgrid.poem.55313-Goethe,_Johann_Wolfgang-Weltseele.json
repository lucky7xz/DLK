{"textgrid.poem.55313": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Weltseele", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verteilet euch nach allen Regionen", "tokens": ["Ver\u00b7tei\u00b7let", "euch", "nach", "al\u00b7len", "Re\u00b7gi\u00b7o\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von diesem heil'gen Schmaus!", "tokens": ["Von", "die\u00b7sem", "heil'\u00b7gen", "Schmaus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Begeistert rei\u00dft euch durch die n\u00e4chsten Zonen", "tokens": ["Be\u00b7geis\u00b7tert", "rei\u00dft", "euch", "durch", "die", "n\u00e4chs\u00b7ten", "Zo\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ins All und f\u00fcllt es aus!", "tokens": ["Ins", "All", "und", "f\u00fcllt", "es", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Schon schwebet ihr in ungeme\u00dfnen Fernen", "tokens": ["Schon", "schwe\u00b7bet", "ihr", "in", "un\u00b7ge\u00b7me\u00df\u00b7nen", "Fer\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den sel'gen G\u00f6ttertraum", "tokens": ["Den", "sel'\u00b7gen", "G\u00f6t\u00b7ter\u00b7traum"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und leuchtet neu, gesellig, unter Sternen", "tokens": ["Und", "leuch\u00b7tet", "neu", ",", "ge\u00b7sel\u00b7lig", ",", "un\u00b7ter", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "$,", "ADJD", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im lichtbes\u00e4ten Raum.", "tokens": ["Im", "licht\u00b7be\u00b7s\u00e4\u00b7ten", "Raum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dann treibt ihr euch, gewaltige Kometen,", "tokens": ["Dann", "treibt", "ihr", "euch", ",", "ge\u00b7wal\u00b7ti\u00b7ge", "Ko\u00b7me\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ins Weit' und Weitr' hinan.", "tokens": ["Ins", "Weit'", "und", "Weit\u00b7r'", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Labyrinth der Sonnen und Planeten", "tokens": ["Das", "La\u00b7by\u00b7rinth", "der", "Son\u00b7nen", "und", "Pla\u00b7ne\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "KON", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Durchschneidet eure Bahn.", "tokens": ["Durch\u00b7schnei\u00b7det", "eu\u00b7re", "Bahn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ihr greifet rasch nach ungeformten Erden", "tokens": ["Ihr", "grei\u00b7fet", "rasch", "nach", "un\u00b7ge\u00b7form\u00b7ten", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wirket sch\u00f6pfrisch jung,", "tokens": ["Und", "wir\u00b7ket", "sch\u00f6pf\u00b7risch", "jung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df sie belebt und stets belebter werden", "tokens": ["Da\u00df", "sie", "be\u00b7lebt", "und", "stets", "be\u00b7leb\u00b7ter", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "KON", "ADV", "ADJD", "VAINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im abgeme\u00dfnen Schwung.", "tokens": ["Im", "ab\u00b7ge\u00b7me\u00df\u00b7nen", "Schwung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und kreisend f\u00fchrt ihr in bewegen L\u00fcften", "tokens": ["Und", "krei\u00b7send", "f\u00fchrt", "ihr", "in", "be\u00b7we\u00b7gen", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den wandelbaren Flor", "tokens": ["Den", "wan\u00b7del\u00b7ba\u00b7ren", "Flor"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schreibt dem Stein in allen seinen Gr\u00fcften", "tokens": ["Und", "schreibt", "dem", "Stein", "in", "al\u00b7len", "sei\u00b7nen", "Gr\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die festen Formen vor.", "tokens": ["Die", "fes\u00b7ten", "For\u00b7men", "vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nun alles sich mit g\u00f6ttlichem Erk\u00fchnen", "tokens": ["Nun", "al\u00b7les", "sich", "mit", "g\u00f6tt\u00b7li\u00b7chem", "Er\u00b7k\u00fch\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu \u00fcbertreffen strebt;", "tokens": ["Zu", "\u00fc\u00b7bert\u00b7ref\u00b7fen", "strebt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Wasser will, das unfruchtbare, gr\u00fcnen,", "tokens": ["Das", "Was\u00b7ser", "will", ",", "das", "un\u00b7frucht\u00b7ba\u00b7re", ",", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$,", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und jedes St\u00e4ubchen lebt.", "tokens": ["Und", "je\u00b7des", "St\u00e4ub\u00b7chen", "lebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und so verdr\u00e4ngt mit liebevollem Streiten", "tokens": ["Und", "so", "ver\u00b7dr\u00e4ngt", "mit", "lie\u00b7be\u00b7vol\u00b7lem", "Strei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der feuchten Qualme Nacht;", "tokens": ["Der", "feuch\u00b7ten", "Qual\u00b7me", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun gl\u00fchen schon des Paradieses Weiten", "tokens": ["Nun", "gl\u00fc\u00b7hen", "schon", "des", "Pa\u00b7ra\u00b7die\u00b7ses", "Wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In \u00fcberbunter Pracht.", "tokens": ["In", "\u00fc\u00b7berb\u00b7un\u00b7ter", "Pracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wie regt sich bald, ein holdes Licht zu schauen,", "tokens": ["Wie", "regt", "sich", "bald", ",", "ein", "hol\u00b7des", "Licht", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ADV", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gestaltenreiche Schar,", "tokens": ["Ge\u00b7stal\u00b7ten\u00b7rei\u00b7che", "Schar", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ihr erstaunt auf den begl\u00fcckten Auen", "tokens": ["Und", "ihr", "er\u00b7staunt", "auf", "den", "be\u00b7gl\u00fcck\u00b7ten", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nun als das erste Paar,", "tokens": ["Nun", "als", "das", "ers\u00b7te", "Paar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und bald verlischt ein unbegrenztes Streben", "tokens": ["Und", "bald", "ver\u00b7lischt", "ein", "un\u00b7be\u00b7grenz\u00b7tes", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im sel'gen Wechselblick.", "tokens": ["Im", "sel'\u00b7gen", "Wech\u00b7sel\u00b7blick", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und so empfangt mit Dank das sch\u00f6nste Leben", "tokens": ["Und", "so", "emp\u00b7fangt", "mit", "Dank", "das", "sch\u00f6ns\u00b7te", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom All ins All zur\u00fcck.", "tokens": ["Vom", "All", "ins", "All", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}