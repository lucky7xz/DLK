{"textgrid.poem.49212": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du g\u00fcldene Freyheit du, mein W\u00fcndschen unnd Begehren,", "tokens": ["Du", "g\u00fcl\u00b7de\u00b7ne", "Frey\u00b7heit", "du", ",", "mein", "W\u00fcnd\u00b7schen", "unnd", "Be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPER", "$,", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wie wol doch were mir, im Fall ich jederzeit", "tokens": ["Wie", "wol", "doch", "we\u00b7re", "mir", ",", "im", "Fall", "ich", "je\u00b7der\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "VAFIN", "PPER", "$,", "APPRART", "NN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein selber m\u00f6chte seyn, und were gantz befreyt", "tokens": ["Mein", "sel\u00b7ber", "m\u00f6ch\u00b7te", "seyn", ",", "und", "we\u00b7re", "gantz", "be\u00b7freyt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADV", "VMFIN", "VAINF", "$,", "KON", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Liebe, die noch nie sich wollen von mir kehren,", "tokens": ["Der", "Lie\u00b7be", ",", "die", "noch", "nie", "sich", "wol\u00b7len", "von", "mir", "keh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADV", "PRF", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wiewol ich offte mich bedacht bin zu erwehren.", "tokens": ["Wie\u00b7wol", "ich", "off\u00b7te", "mich", "be\u00b7dacht", "bin", "zu", "er\u00b7weh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "VVPP", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch, lieb' ich gleichwol nicht, so bin ich wie ein Scheit,", "tokens": ["Doch", ",", "lieb'", "ich", "gleich\u00b7wol", "nicht", ",", "so", "bin", "ich", "wie", "ein", "Scheit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "ADV", "VAFIN", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Stock und raues Bley; die freye Dinstbarkeit,", "tokens": ["Ein", "Stock", "und", "rau\u00b7es", "Bley", ";", "die", "frey\u00b7e", "Dinst\u00b7bar\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die sichere Gefahr, das tr\u00f6stliche Beschweren", "tokens": ["Die", "si\u00b7che\u00b7re", "Ge\u00b7fahr", ",", "das", "tr\u00f6st\u00b7li\u00b7che", "Be\u00b7schwe\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ermuntert meinen Geist, da\u00df er sich h\u00f6her schwingt", "tokens": ["Er\u00b7mun\u00b7tert", "mei\u00b7nen", "Geist", ",", "da\u00df", "er", "sich", "h\u00f6\u00b7her", "schwingt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PRF", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als wo der P\u00f6fel kreucht, und durch die Wolcken dringt,", "tokens": ["Als", "wo", "der", "P\u00f6\u00b7fel", "kreucht", ",", "und", "durch", "die", "Wol\u00b7cken", "dringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "ART", "NN", "VVFIN", "$,", "KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gefl\u00fcgelt mit Vernunfft und muthigen Gedancken.", "tokens": ["Ge\u00b7fl\u00fc\u00b7gelt", "mit", "Ver\u00b7nunfft", "und", "mut\u00b7hi\u00b7gen", "Ge\u00b7dan\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Drumb geh' es, wie es wil, und mu\u00df ich gleich darvon,", "tokens": ["Drumb", "geh'", "es", ",", "wie", "es", "wil", ",", "und", "mu\u00df", "ich", "gleich", "dar\u00b7von", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$,", "KON", "VMFIN", "PPER", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So \u00fcberschreit' ich doch de\u00df Lebens enge Schrancken;", "tokens": ["So", "\u00fc\u00b7ber\u00b7schreit'", "ich", "doch", "de\u00df", "Le\u00b7bens", "en\u00b7ge", "Schran\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Name, der mir folgt, ist meiner Sorgen Lohn.", "tokens": ["Der", "Na\u00b7me", ",", "der", "mir", "folgt", ",", "ist", "mei\u00b7ner", "Sor\u00b7gen", "Lohn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}