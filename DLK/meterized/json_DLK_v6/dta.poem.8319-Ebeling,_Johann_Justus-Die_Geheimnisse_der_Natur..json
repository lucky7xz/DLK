{"dta.poem.8319": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n Geheimnisse der Natur.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer denket alles zu ergr\u00fcnden,", "tokens": ["Wer", "den\u00b7ket", "al\u00b7les", "zu", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wird noch vieles dunkel finden", "tokens": ["Wird", "noch", "vie\u00b7les", "dun\u00b7kel", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PIS", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem Reiche der Natur,", "tokens": ["In", "dem", "Rei\u00b7che", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo in der verstekten Spur", "tokens": ["Wo", "in", "der", "vers\u00b7tek\u00b7ten", "Spur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Vieles \u00fcberal zu sehen", "tokens": ["Vie\u00b7les", "\u00fc\u00b7be\u00b7ral", "zu", "se\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "ADJD", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dessen Grund wir nicht verstehen.", "tokens": ["Des\u00b7sen", "Grund", "wir", "nicht", "ver\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Tohren die dar\u00fcber lachen,", "tokens": ["Toh\u00b7ren", "die", "da\u00b7r\u00fc\u00b7ber", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PAV", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Wenn man will Geheimnis machen,", "tokens": ["Wenn", "man", "will", "Ge\u00b7heim\u00b7nis", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zeiget uns erst deutlich an,", "tokens": ["Zei\u00b7get", "uns", "erst", "deut\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo des Windes schnelle Bahn,", "tokens": ["Wo", "des", "Win\u00b7des", "schnel\u00b7le", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jhren Ursprung kl\u00e4rlich weiset,", "tokens": ["Ih\u00b7ren", "Ur\u00b7sprung", "kl\u00e4r\u00b7lich", "wei\u00b7set", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie er durch den Luftkreis reiset.", "tokens": ["Wie", "er", "durch", "den", "Luft\u00b7kreis", "rei\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wisset ihr, wo er entstehet,", "tokens": ["Wis\u00b7set", "ihr", ",", "wo", "er", "ent\u00b7ste\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie er brau\u00dft, wohin er gehet,", "tokens": ["Wie", "er", "brau\u00dft", ",", "wo\u00b7hin", "er", "ge\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie er wiederum verfliegt,", "tokens": ["Wie", "er", "wie\u00b7de\u00b7rum", "ver\u00b7fliegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In verborgne H\u00f6len kriecht?", "tokens": ["In", "ver\u00b7borg\u00b7ne", "H\u00f6\u00b7len", "kriecht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nein! ihr h\u00f6rt der Winde Sausen,", "tokens": ["Nein", "!", "ihr", "h\u00f6rt", "der", "Win\u00b7de", "Sau\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wisset nicht woher sie brausen.", "tokens": ["Wis\u00b7set", "nicht", "wo\u00b7her", "sie", "brau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wer kan deutlich uns erkl\u00e4ren,", "tokens": ["Wer", "kan", "deut\u00b7lich", "uns", "er\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADJD", "PPER", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Wie in denen Fl\u00fcssen, Meeren", "tokens": ["Wie", "in", "de\u00b7nen", "Fl\u00fcs\u00b7sen", ",", "Mee\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "APPR", "PRELS", "NN", "$,", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Ebbe und die Flut entspringt,", "tokens": ["Eb\u00b7be", "und", "die", "Flut", "ent\u00b7springt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie das Wasser abwerts dringt,", "tokens": ["Wie", "das", "Was\u00b7ser", "ab\u00b7werts", "dringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und in den bestimmten Stunden,", "tokens": ["Und", "in", "den", "be\u00b7stimm\u00b7ten", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wiederum sich eingefunden?", "tokens": ["Wie\u00b7de\u00b7rum", "sich", "ein\u00b7ge\u00b7fun\u00b7den", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wer kan uns den Grund angeben,", "tokens": ["Wer", "kan", "uns", "den", "Grund", "an\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von dem lenkenden Bestreben,", "tokens": ["Von", "dem", "len\u00b7ken\u00b7den", "Be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df die Nadel von Magnet,", "tokens": ["Da\u00df", "die", "Na\u00b7del", "von", "Mag\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NE", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Sich nach ihrem Nordpol dreht,", "tokens": ["Sich", "nach", "ih\u00b7rem", "Nord\u00b7pol", "dreht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df sie Eisen an sich f\u00fcget,", "tokens": ["Da\u00df", "sie", "Ei\u00b7sen", "an", "sich", "f\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dessen schwere Last aufwieget?", "tokens": ["Des\u00b7sen", "schwe\u00b7re", "Last", "auf\u00b7wie\u00b7get", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Keiner von der Zahl der Weisen,", "tokens": ["Kei\u00b7ner", "von", "der", "Zahl", "der", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deren Wiz wir billig preisen,", "tokens": ["De\u00b7ren", "Wiz", "wir", "bil\u00b7lig", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat uns kl\u00e4rlich \u00fcberf\u00fchrt,", "tokens": ["Hat", "uns", "kl\u00e4r\u00b7lich", "\u00fc\u00b7berf\u00b7\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie dies alles wird regiert;", "tokens": ["Wie", "dies", "al\u00b7les", "wird", "re\u00b7giert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "PIS", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Keiner, obs gleich viele wagen,", "tokens": ["Kei\u00b7ner", ",", "obs", "gleich", "vie\u00b7le", "wa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "ADV", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kan davon den Grund ansagen.", "tokens": ["Kan", "da\u00b7von", "den", "Grund", "an\u00b7sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PAV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Dennoch m\u00fcssen wir gestehen,", "tokens": ["Den\u00b7noch", "m\u00fcs\u00b7sen", "wir", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Es sey wahr, was man gesehen,", "tokens": ["Es", "sey", "wahr", ",", "was", "man", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PRELS", "PIS", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ob man gleich den Grund nicht sieht,", "tokens": ["Ob", "man", "gleich", "den", "Grund", "nicht", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie sich dieses alles zieht:", "tokens": ["Wie", "sich", "die\u00b7ses", "al\u00b7les", "zieht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PDAT", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Darum mu\u00df man sich bequemen,", "tokens": ["Da\u00b7rum", "mu\u00df", "man", "sich", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PIS", "PRF", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auch Geheimnis anzunehmen.", "tokens": ["Auch", "Ge\u00b7heim\u00b7nis", "an\u00b7zu\u00b7neh\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Warum will man sich beschweren,", "tokens": ["Wa\u00b7rum", "will", "man", "sich", "be\u00b7schwe\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PIS", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber die Geheimnis-Lehren,", "tokens": ["Ue\u00b7ber", "die", "Ge\u00b7heim\u00b7nis\u00b7Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die das Gnadenreich uns zeigt,", "tokens": ["Die", "das", "Gna\u00b7den\u00b7reich", "uns", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da man hiezu stille schweigt,", "tokens": ["Da", "man", "hie\u00b7zu", "stil\u00b7le", "schweigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PAV", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df auf unsrer Erden-Scheiben,", "tokens": ["Da\u00df", "auf", "uns\u00b7rer", "Er\u00b7den\u00b7Schei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So viel Dinge dunkel bleiben?", "tokens": ["So", "viel", "Din\u00b7ge", "dun\u00b7kel", "blei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Warum will man das nicht gl\u00e4uben,", "tokens": ["Wa\u00b7rum", "will", "man", "das", "nicht", "gl\u00e4u\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PIS", "PDS", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was die heilgen M\u00e4nner schreiben:", "tokens": ["Was", "die", "heil\u00b7gen", "M\u00e4n\u00b7ner", "schrei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die des Geistes Trieb belebt?", "tokens": ["Die", "des", "Geis\u00b7tes", "Trieb", "be\u00b7lebt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Darum, weil man sich bestrebt,", "tokens": ["Da\u00b7rum", ",", "weil", "man", "sich", "be\u00b7strebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PIS", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch ein \u00fcbereiltes Spotten,", "tokens": ["Durch", "ein", "\u00fc\u00b7be\u00b7reil\u00b7tes", "Spot\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Tohren! lernet anders denken,", "tokens": ["Toh\u00b7ren", "!", "ler\u00b7net", "an\u00b7ders", "den\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lernt den Wiz recht einzuschr\u00e4nken,", "tokens": ["Lernt", "den", "Wiz", "recht", "ein\u00b7zu\u00b7schr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Z\u00e4umet euren Frevel-Geist,", "tokens": ["Z\u00e4u\u00b7met", "eu\u00b7ren", "Fre\u00b7vel\u00b7Geist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der ein flatternd Jrrlicht heist,", "tokens": ["Der", "ein", "flat\u00b7ternd", "Jrr\u00b7licht", "heist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das Naturreich giebt zu lesen,", "tokens": ["Das", "Na\u00b7tur\u00b7reich", "giebt", "zu", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}