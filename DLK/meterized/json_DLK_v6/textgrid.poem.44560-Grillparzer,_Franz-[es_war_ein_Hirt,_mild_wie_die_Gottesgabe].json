{"textgrid.poem.44560": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[es war ein Hirt, mild wie die Gottesgabe]", "genre": "verse", "period": "N.A.", "pub_year": 1848, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war ein Hirt, mild wie die Gottesgabe,", "tokens": ["Es", "war", "ein", "Hirt", ",", "mild", "wie", "die", "Got\u00b7tes\u00b7ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein netter Mann und elegant dabei;", "tokens": ["Ein", "net\u00b7ter", "Mann", "und", "e\u00b7le\u00b7gant", "da\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADJD", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein blaues Band an seinem Sch\u00e4ferstabe,", "tokens": ["Ein", "blau\u00b7es", "Band", "an", "sei\u00b7nem", "Sch\u00e4\u00b7fer\u00b7sta\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor allem blies er lieblich die Schalmei.", "tokens": ["Vor", "al\u00b7lem", "blies", "er", "lieb\u00b7lich", "die", "Schal\u00b7mei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Der folgt der Herde nach mit leisem Tritte", "tokens": ["Der", "folgt", "der", "Her\u00b7de", "nach", "mit", "lei\u00b7sem", "Trit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und statt zu f\u00fchren ward er selbst gef\u00fchrt.", "tokens": ["Und", "statt", "zu", "f\u00fch\u00b7ren", "ward", "er", "selbst", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKZU", "VVINF", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein jedes Bl\u00f6ken schien ihm eine Bitte,", "tokens": ["Ein", "je\u00b7des", "Bl\u00f6\u00b7ken", "schien", "ihm", "ei\u00b7ne", "Bit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von jeder Bitte war er gleich ger\u00fchrt.", "tokens": ["Von", "je\u00b7der", "Bit\u00b7te", "war", "er", "gleich", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Vor allem, wenn mit flehender Geb\u00e4rde", "tokens": ["Vor", "al\u00b7lem", ",", "wenn", "mit", "fle\u00b7hen\u00b7der", "Ge\u00b7b\u00e4r\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein t\u00fcchtger Widder ihm die H\u00f6rner wies.", "tokens": ["Ein", "t\u00fccht\u00b7ger", "Wid\u00b7der", "ihm", "die", "H\u00f6r\u00b7ner", "wies", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Drum wollt er rechts, so ging nach links die Herde,", "tokens": ["Drum", "wollt", "er", "rechts", ",", "so", "ging", "nach", "links", "die", "Her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "APPR", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er nahm sein Wort zur\u00fcck und sang und blies.", "tokens": ["Er", "nahm", "sein", "Wort", "zu\u00b7r\u00fcck", "und", "sang", "und", "blies", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "KON", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da brachen sie denn rings in alle Raine,", "tokens": ["Da", "bra\u00b7chen", "sie", "denn", "rings", "in", "al\u00b7le", "Rai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Nachbars Saat den Tieren wohl behagt;", "tokens": ["Des", "Nach\u00b7bars", "Saat", "den", "Tie\u00b7ren", "wohl", "be\u00b7hagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie \u00fcberkletterten die Schirmungsz\u00e4une,", "tokens": ["Sie", "\u00fc\u00b7berk\u00b7let\u00b7ter\u00b7ten", "die", "Schir\u00b7mungs\u00b7z\u00e4u\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der jungen B\u00e4umchen Rinde ward benagt.", "tokens": ["Der", "jun\u00b7gen", "B\u00e4um\u00b7chen", "Rin\u00b7de", "ward", "be\u00b7nagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Er schien daraus nicht allzuviel zu machen,", "tokens": ["Er", "schien", "da\u00b7raus", "nicht", "all\u00b7zu\u00b7viel", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PTKNEG", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Nachbar ohnehin war er nicht hold,", "tokens": ["Dem", "Nach\u00b7bar", "oh\u00b7ne\u00b7hin", "war", "er", "nicht", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und stie\u00dfen auch die St\u00e4rkern nach den Schwachen,", "tokens": ["Und", "stie\u00b7\u00dfen", "auch", "die", "St\u00e4r\u00b7kern", "nach", "den", "Schwa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verzeihung um Verzeihn ist Liebessold.", "tokens": ["Ver\u00b7zei\u00b7hung", "um", "Ver\u00b7zeihn", "ist", "Lie\u00b7bes\u00b7sold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Da scheint der Schwarm mit eins Gefahr zu wittern \u2013", "tokens": ["Da", "scheint", "der", "Schwarm", "mit", "eins", "Ge\u00b7fahr", "zu", "wit\u00b7tern", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "CARD", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Wolf! der Wolf! der allgemeine Feind \u2013", "tokens": ["Der", "Wolf", "!", "der", "Wolf", "!", "der", "all\u00b7ge\u00b7mei\u00b7ne", "Feind", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$.", "ART", "NE", "$.", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den guten Hirten \u00fcberf\u00e4llt ein Zittern,", "tokens": ["Den", "gu\u00b7ten", "Hir\u00b7ten", "\u00fc\u00b7berf\u00b7\u00e4llt", "ein", "Zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er sinkt auf seine Knie und flennt und weint.", "tokens": ["Er", "sinkt", "auf", "sei\u00b7ne", "Knie", "und", "flennt", "und", "weint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Doch will ers mit der Tonkunst noch probieren,", "tokens": ["Doch", "will", "ers", "mit", "der", "Ton\u00b7kunst", "noch", "pro\u00b7bie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "APPR", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Mit blassem Munde bl\u00e4st er die Schalmei;", "tokens": ["Mit", "blas\u00b7sem", "Mun\u00b7de", "bl\u00e4st", "er", "die", "Schal\u00b7mei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Den Wolf mag solches Mundwerk wenig r\u00fchren,", "tokens": ["Den", "Wolf", "mag", "sol\u00b7ches", "Mund\u00b7werk", "we\u00b7nig", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VMFIN", "PIAT", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schon raschelts im Geb\u00fcsch, er kommt herbei.", "tokens": ["Schon", "ra\u00b7schelts", "im", "Ge\u00b7b\u00fcsch", ",", "er", "kommt", "her\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Da f\u00e4llt ein Schu\u00df, und wo der Waldweg offen,", "tokens": ["Da", "f\u00e4llt", "ein", "Schu\u00df", ",", "und", "wo", "der", "Wald\u00b7weg", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "PWAV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erscheint ein zweiter Hirt voll ernstem Mut,", "tokens": ["Er\u00b7scheint", "ein", "zwei\u00b7ter", "Hirt", "voll", "erns\u00b7tem", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In einer Hand die B\u00fcchse, die getroffen,", "tokens": ["In", "ei\u00b7ner", "Hand", "die", "B\u00fcch\u00b7se", ",", "die", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die andre schleppt den Wolf in seinem Blut.", "tokens": ["Die", "and\u00b7re", "schleppt", "den", "Wolf", "in", "sei\u00b7nem", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "NE", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Er wirft das Tier zu des Erschrocknen F\u00fc\u00dfen,", "tokens": ["Er", "wirft", "das", "Tier", "zu", "des", "Er\u00b7schrock\u00b7nen", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbhier ist der Feind\u00ab, ruft er, \u00bbden ich bestand,", "tokens": ["\u00bb", "hier", "ist", "der", "Feind", "\u00ab", ",", "ruft", "er", ",", "\u00bb", "den", "ich", "be\u00b7stand", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "ART", "NN", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mir hat er selbst ein teures Lamm zerrissen,", "tokens": ["Mir", "hat", "er", "selbst", "ein", "teu\u00b7res", "Lamm", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch bebt mein Herz, allein nicht meine Hand.\u00ab", "tokens": ["Noch", "bebt", "mein", "Herz", ",", "al\u00b7lein", "nicht", "mei\u00b7ne", "Hand", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "PTKNEG", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "\u00bbdoch willst du k\u00fcnftig wieder L\u00e4mmer weiden,", "tokens": ["\u00bb", "doch", "willst", "du", "k\u00fcnf\u00b7tig", "wie\u00b7der", "L\u00e4m\u00b7mer", "wei\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "ADJD", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So sch\u00fctze sie vor sich und vor Gefahr,", "tokens": ["So", "sch\u00fct\u00b7ze", "sie", "vor", "sich", "und", "vor", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PRF", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Schw\u00e4che liebt in Sanftmut sich zu kleiden,", "tokens": ["Die", "Schw\u00e4\u00b7che", "liebt", "in", "Sanft\u00b7mut", "sich", "zu", "klei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Mut erst macht des Lenkers Worte wahr.\u00ab", "tokens": ["Der", "Mut", "erst", "macht", "des", "Len\u00b7kers", "Wor\u00b7te", "wahr", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}