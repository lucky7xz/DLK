{"textgrid.poem.57245": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich sah, o sagt mir, sah ich, was jetzt geschieht?", "genre": "verse", "period": "N.A.", "pub_year": 1752, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah, o sagt mir, sah ich, was jetzt geschieht?", "tokens": ["Ich", "sah", ",", "o", "sagt", "mir", ",", "sah", "ich", ",", "was", "jetzt", "ge\u00b7schieht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Erblickt' ich Zukunft? mit der britannischen", "tokens": ["Er\u00b7blickt'", "ich", "Zu\u00b7kunft", "?", "mit", "der", "bri\u00b7tan\u00b7ni\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "$.", "APPR", "ART", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sah ich in Streitlauf Deutschlands Muse", "tokens": ["Sah", "ich", "in", "Streit\u00b7lauf", "Deutschlands", "Mu\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Heiss zu den kr\u00f6nenden Zielen fliegen.", "tokens": ["Heiss", "zu", "den", "kr\u00f6\u00b7nen\u00b7den", "Zie\u00b7len", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Zwey Ziele gr\u00e4nzten, wo sich der Blick verlor,", "tokens": ["Zwey", "Zie\u00b7le", "gr\u00e4nz\u00b7ten", ",", "wo", "sich", "der", "Blick", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "$,", "PWAV", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dort an die Laufbahn. Eichen beschatteten", "tokens": ["Dort", "an", "die", "Lauf\u00b7bahn", ".", "Ei\u00b7chen", "be\u00b7schat\u00b7te\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$.", "NN", "VVFIN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Des Hains das eine; nah dem andren", "tokens": ["Des", "Hains", "das", "ei\u00b7ne", ";", "nah", "dem", "an\u00b7dren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ART", "$.", "ADJD", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weheten Palmen im Abendschimmer.", "tokens": ["We\u00b7he\u00b7ten", "Pal\u00b7men", "im", "A\u00b7bend\u00b7schim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Gewohnt des Streitlaufs, trat die von Albion", "tokens": ["Ge\u00b7wohnt", "des", "Streit\u00b7laufs", ",", "trat", "die", "von", "Al\u00b7bi\u00b7on"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "ART", "NN", "$,", "VVFIN", "ART", "APPR", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Stolz in die Schranken, so wie sie kam, da sie", "tokens": ["Stolz", "in", "die", "Schran\u00b7ken", ",", "so", "wie", "sie", "kam", ",", "da", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ADV", "KOKOM", "PPER", "VVFIN", "$,", "KOUS", "PPER"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Einst mit der M\u00e4onid', und jener", "tokens": ["Einst", "mit", "der", "M\u00e4o\u00b7nid'", ",", "und", "je\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "KON", "PDS"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Am Kapitol in den heissen Sand trat.", "tokens": ["Am", "Ka\u00b7pi\u00b7tol", "in", "den", "heis\u00b7sen", "Sand", "trat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Sie sah die junge bebende Streiterin;", "tokens": ["Sie", "sah", "die", "jun\u00b7ge", "be\u00b7ben\u00b7de", "Strei\u00b7te\u00b7rin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch diese bebte m\u00e4nnlich, und gl\u00fchende", "tokens": ["Doch", "die\u00b7se", "beb\u00b7te", "m\u00e4nn\u00b7lich", ",", "und", "gl\u00fc\u00b7hen\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "ADJD", "$,", "KON", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Siegswerthe R\u00f6then \u00fcberstr\u00f6mten", "tokens": ["Siegs\u00b7wert\u00b7he", "R\u00f6\u00b7then", "\u00fc\u00b7ber\u00b7str\u00f6m\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NE", "NN", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Flammend die Wang', und ihr goldnes Haar flog.", "tokens": ["Flam\u00b7mend", "die", "Wang'", ",", "und", "ihr", "gold\u00b7nes", "Haar", "flog", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Schon hielt sie m\u00fchsam in der emp\u00f6rten Brust", "tokens": ["Schon", "hielt", "sie", "m\u00fch\u00b7sam", "in", "der", "em\u00b7p\u00f6r\u00b7ten", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den engen Athem; hing schon hervorgebeugt", "tokens": ["Den", "en\u00b7gen", "A\u00b7them", ";", "hing", "schon", "her\u00b7vor\u00b7ge\u00b7beugt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "VVFIN", "ADV", "VVPP"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dein Ziele zu; schon hub der Herold", "tokens": ["Dein", "Zie\u00b7le", "zu", ";", "schon", "hub", "der", "He\u00b7rold"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKVZ", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ihr die Drommet', und ihr trunkner Blick schwamm.", "tokens": ["Ihr", "die", "Drom\u00b7met'", ",", "und", "ihr", "trunk\u00b7ner", "Blick", "schwamm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+---+-+-", "measure": "unknown.measure.tetra"}}, "stanza.6": {"line.1": {"text": "Stolz auf die k\u00fchne, stolzer auf sich, bemass", "tokens": ["Stolz", "auf", "die", "k\u00fch\u00b7ne", ",", "stol\u00b7zer", "auf", "sich", ",", "be\u00b7mass"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "$,", "ADJA", "APPR", "PRF", "$,", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Die hohe Brittin, aber mit edlem Blick,", "tokens": ["Die", "ho\u00b7he", "Brit\u00b7tin", ",", "a\u00b7ber", "mit", "ed\u00b7lem", "Blick", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dich, Thuiskone: Ja bey Barden", "tokens": ["Dich", ",", "Thu\u00b7is\u00b7ko\u00b7ne", ":", "Ja", "bey", "Bar\u00b7den"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "NE", "$.", "PTKANT", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wuchs ich mit dir in dem Eichenhain auf;", "tokens": ["Wuchs", "ich", "mit", "dir", "in", "dem", "Ei\u00b7chen\u00b7hain", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.7": {"line.1": {"text": "Allein die Sage kam mir, du seyst nicht mehr!", "tokens": ["Al\u00b7lein", "die", "Sa\u00b7ge", "kam", "mir", ",", "du", "seyst", "nicht", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verzeih, o Muse, wenn du unsterblich bist,", "tokens": ["Ver\u00b7zeih", ",", "o", "Mu\u00b7se", ",", "wenn", "du", "uns\u00b7terb\u00b7lich", "bist", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Verzeih, dass ichs erst jetzo lerne;", "tokens": ["Ver\u00b7zeih", ",", "dass", "ichs", "erst", "jet\u00b7zo", "ler\u00b7ne", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PIS", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch an dem Ziele nur will ichs lernen!", "tokens": ["Doch", "an", "dem", "Zie\u00b7le", "nur", "will", "ichs", "ler\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "VMFIN", "PIS", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Dort steht es! Aber siehst du das weitere,", "tokens": ["Dort", "steht", "es", "!", "A\u00b7ber", "siehst", "du", "das", "wei\u00b7te\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "KON", "VVFIN", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und seine Kron' auch? Diesen gehaltnen Muth,", "tokens": ["Und", "sei\u00b7ne", "Kron'", "auch", "?", "Die\u00b7sen", "ge\u00b7halt\u00b7nen", "Muth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "$.", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Diess stolze Schweigen, diesen Blick, der", "tokens": ["Diess", "stol\u00b7ze", "Schwei\u00b7gen", ",", "die\u00b7sen", "Blick", ",", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PDAT", "ADJA", "NN", "$,", "PDAT", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Feurig zur Erde sich senkt, die kenn' ich!", "tokens": ["Feu\u00b7rig", "zur", "Er\u00b7de", "sich", "senkt", ",", "die", "kenn'", "ich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "PRF", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Doch w\u00e4g's noch Einmal, eh zu gefahrvoll dir", "tokens": ["Doch", "w\u00e4g's", "noch", "Ein\u00b7mal", ",", "eh", "zu", "ge\u00b7fahr\u00b7voll", "dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NE", "ADV", "ADV", "$,", "KOUS", "PTKA", "ADJD", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Herold t\u00f6net. War es nicht ich, die schon", "tokens": ["Der", "He\u00b7rold", "t\u00f6\u00b7net", ".", "War", "es", "nicht", "ich", ",", "die", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "VAFIN", "PPER", "PTKNEG", "PPER", "$,", "PRELS", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit der an Thermopyl die Bahn mass?", "tokens": ["Mit", "der", "an", "Ther\u00b7mo\u00b7pyl", "die", "Bahn", "mass", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mit der hohen der sieben H\u00fcgel?", "tokens": ["Und", "mit", "der", "ho\u00b7hen", "der", "sie\u00b7ben", "H\u00fc\u00b7gel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Sie sprachs. Der ernste, richtende Augenblick", "tokens": ["Sie", "sprachs", ".", "Der", "erns\u00b7te", ",", "rich\u00b7ten\u00b7de", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Kam mit dem Herold n\u00e4her. Ich liebe dich!", "tokens": ["Kam", "mit", "dem", "He\u00b7rold", "n\u00e4\u00b7her", ".", "Ich", "lie\u00b7be", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ADJD", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Sprach schnell mit Flammenblick Teutona,", "tokens": ["Sprach", "schnell", "mit", "Flam\u00b7men\u00b7blick", "Teu\u00b7to\u00b7na", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "NN", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Brittin, ich liebe dich mit Bewundrung!", "tokens": ["Brit\u00b7tin", ",", "ich", "lie\u00b7be", "dich", "mit", "Be\u00b7wund\u00b7rung", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Doch dich nicht heisser, als die Unsterblichkeit,", "tokens": ["Doch", "dich", "nicht", "heis\u00b7ser", ",", "als", "die", "U\u00b7nsterb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "ADJD", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und jene Palmen! R\u00fchre, dein Genius", "tokens": ["Und", "je\u00b7ne", "Pal\u00b7men", "!", "R\u00fch\u00b7re", ",", "dein", "Ge\u00b7nius"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "PDAT", "NN", "$.", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gebeut ers, sie vor mir; doch fass' ich,", "tokens": ["Ge\u00b7beut", "ers", ",", "sie", "vor", "mir", ";", "doch", "fass'", "ich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "$,", "PPER", "APPR", "PPER", "$.", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wenn du sie fassest, dann gleich die Kron' auch.", "tokens": ["Wenn", "du", "sie", "fas\u00b7sest", ",", "dann", "gleich", "die", "Kron'", "auch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "ADV", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "Und, o wie beb' ich! o ihr Unsterblichen!", "tokens": ["Und", ",", "o", "wie", "beb'", "ich", "!", "o", "ihr", "U\u00b7nsterb\u00b7li\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "FM", "KOKOM", "VVFIN", "PPER", "$.", "FM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vielleicht erreich' ich fr\u00fcher das hohe Ziel!", "tokens": ["Viel\u00b7leicht", "er\u00b7reich'", "ich", "fr\u00fc\u00b7her", "das", "ho\u00b7he", "Ziel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dann mag, o dann an meine leichte", "tokens": ["Dann", "mag", ",", "o", "dann", "an", "mei\u00b7ne", "leich\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "$,", "FM", "ADV", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fliegende Locke dein Athem hauchen!", "tokens": ["Flie\u00b7gen\u00b7de", "Lo\u00b7cke", "dein", "A\u00b7them", "hau\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.13": {"line.1": {"text": "Der Herold klang! Sie flogen mit Adlereil.", "tokens": ["Der", "He\u00b7rold", "klang", "!", "Sie", "flo\u00b7gen", "mit", "Ad\u00b7ler\u00b7eil", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die weite Laufbahn st\u00e4ubte, wie Wolken, auf.", "tokens": ["Die", "wei\u00b7te", "Lauf\u00b7bahn", "st\u00e4ub\u00b7te", ",", "wie", "Wol\u00b7ken", ",", "auf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "PWAV", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ich sah: Vorbey der Eiche wehte", "tokens": ["Ich", "sah", ":", "Vor\u00b7bey", "der", "Ei\u00b7che", "weh\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dunkler der Staub, und mein Blick verlor sie!", "tokens": ["Dunk\u00b7ler", "der", "Staub", ",", "und", "mein", "Blick", "ver\u00b7lor", "sie", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}