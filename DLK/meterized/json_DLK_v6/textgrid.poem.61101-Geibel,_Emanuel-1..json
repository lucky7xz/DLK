{"textgrid.poem.61101": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die du die Burg dort oben bewohnst, blau\u00e4ugige Pallas,", "tokens": ["Die", "du", "die", "Burg", "dort", "o\u00b7ben", "be\u00b7wohnst", ",", "blau\u00b7\u00e4u\u00b7gi\u00b7ge", "Pal\u00b7las", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADV", "ADV", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Schau mit segnendem Blick auch auf den S\u00e4nger herab!", "tokens": ["Schau", "mit", "seg\u00b7nen\u00b7dem", "Blick", "auch", "auf", "den", "S\u00e4n\u00b7ger", "her\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Zwar mir zeigte sich Eros geneigt, und der rosige Bakchos", "tokens": ["Zwar", "mir", "zeig\u00b7te", "sich", "E\u00b7ros", "ge\u00b7neigt", ",", "und", "der", "ro\u00b7si\u00b7ge", "Bakc\u00b7hos"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "PRF", "NE", "VVPP", "$,", "KON", "ART", "ADJA", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Blickt' aus dem Efeukranz schalkhaft verlockend mich an!", "tokens": ["Blickt'", "aus", "dem", "E\u00b7feu\u00b7kranz", "schalk\u00b7haft", "ver\u00b7lo\u00b7ckend", "mich", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJD", "VVPP", "PPER", "PTKVZ", "$."], "meter": "+--+---+-+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Doch du, G\u00f6ttin, verleih zu dem S\u00fc\u00dfen das Ma\u00df und die Weisheit,", "tokens": ["Doch", "du", ",", "G\u00f6t\u00b7tin", ",", "ver\u00b7leih", "zu", "dem", "S\u00fc\u00b7\u00dfen", "das", "Ma\u00df", "und", "die", "Weis\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NN", "$,", "VVIMP", "APPR", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Gib mir das stille Gem\u00fct, recht zu genie\u00dfen, dabei.", "tokens": ["Gib", "mir", "das", "stil\u00b7le", "Ge\u00b7m\u00fct", ",", "recht", "zu", "ge\u00b7nie\u00b7\u00dfen", ",", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$,", "ADJD", "PTKZU", "VVINF", "$,", "PAV", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Liebt auch die Jugend den feurigen Rausch und den Taumel der Wonne,", "tokens": ["Liebt", "auch", "die", "Ju\u00b7gend", "den", "feu\u00b7ri\u00b7gen", "Rausch", "und", "den", "Tau\u00b7mel", "der", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Ach, wie teuer erkauft oft sich die fl\u00fcchtige Lust!", "tokens": ["Ach", ",", "wie", "teu\u00b7er", "er\u00b7kauft", "oft", "sich", "die", "fl\u00fcch\u00b7ti\u00b7ge", "Lust", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADJD", "VVFIN", "ADV", "PRF", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Doch wenn du die Begier mit l\u00e4chelndem Ernste bes\u00e4nftigst,", "tokens": ["Doch", "wenn", "du", "die", "Be\u00b7gier", "mit", "l\u00e4\u00b7cheln\u00b7dem", "Erns\u00b7te", "be\u00b7s\u00e4nf\u00b7tigst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Wie mit frommer Musik Orpheus den L\u00f6wen gez\u00e4hmt,", "tokens": ["Wie", "mit", "from\u00b7mer", "Mu\u00b7sik", "Or\u00b7pheus", "den", "L\u00f6\u00b7wen", "ge\u00b7z\u00e4hmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "NE", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Nimmer entheiligt das Mahl alsdann der vergossene Becher,", "tokens": ["Nim\u00b7mer", "en\u00b7thei\u00b7ligt", "das", "Mahl", "als\u00b7dann", "der", "ver\u00b7gos\u00b7se\u00b7ne", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Nimmer betroffenen Blicks gl\u00fchen die M\u00e4dchen vor Scham,", "tokens": ["Nim\u00b7mer", "be\u00b7trof\u00b7fe\u00b7nen", "Blicks", "gl\u00fc\u00b7hen", "die", "M\u00e4d\u00b7chen", "vor", "Scham", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.13": {"text": "Sondern es wandelt im Kreis mit Blumen umwunden die Zither,", "tokens": ["Son\u00b7dern", "es", "wan\u00b7delt", "im", "Kreis", "mit", "Blu\u00b7men", "um\u00b7wun\u00b7den", "die", "Zi\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Und um das freundliche Fest schlingt sich der Grazien Tanz.", "tokens": ["Und", "um", "das", "freund\u00b7li\u00b7che", "Fest", "schlingt", "sich", "der", "Gra\u00b7zi\u00b7en", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN", "NN", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Dann erst wird der Genu\u00df zum Genu\u00df, und die Bl\u00fcte der Freude", "tokens": ["Dann", "erst", "wird", "der", "Ge\u00b7nu\u00df", "zum", "Ge\u00b7nu\u00df", ",", "und", "die", "Bl\u00fc\u00b7te", "der", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "$,", "KON", "ART", "NN", "ART", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.16": {"text": "Treibt als schwellende Frucht manches begeisterte Lied.", "tokens": ["Treibt", "als", "schwel\u00b7len\u00b7de", "Frucht", "man\u00b7ches", "be\u00b7geis\u00b7ter\u00b7te", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ADJA", "NN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}}}}}