{"textgrid.poem.55413": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Einzelne", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hebe selbst die Hindernisse,", "tokens": ["He\u00b7be", "selbst", "die", "Hin\u00b7der\u00b7nis\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Neige dich herab, Zypresse!", "tokens": ["Nei\u00b7ge", "dich", "her\u00b7ab", ",", "Zyp\u00b7res\u00b7se", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Da\u00df ich deinen Gipfel k\u00fcsse", "tokens": ["Da\u00df", "ich", "dei\u00b7nen", "Gip\u00b7fel", "k\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das Leben dran vergesse.", "tokens": ["Und", "das", "Le\u00b7ben", "dran", "ver\u00b7ges\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PAV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eure G\u00e4rtnerei zu lernen,", "tokens": ["Eu\u00b7re", "G\u00e4rt\u00b7ne\u00b7rei", "zu", "ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00f6nnte nimmermehr verlangen;", "tokens": ["K\u00f6nn\u00b7te", "nim\u00b7mer\u00b7mehr", "ver\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mein Jasmin ist fortgegangen,", "tokens": ["Mein", "Jas\u00b7min", "ist", "fort\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Meine Rose weilt im Fernen.", "tokens": ["Mei\u00b7ne", "Ro\u00b7se", "weilt", "im", "Fer\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Nachtigall, sie war entfernt,", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", ",", "sie", "war", "ent\u00b7fernt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Fr\u00fchling lockt sie wieder;", "tokens": ["Der", "Fr\u00fch\u00b7ling", "lockt", "sie", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was Neues hat sie nicht gelernt,", "tokens": ["Was", "Neu\u00b7es", "hat", "sie", "nicht", "ge\u00b7lernt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Singt alte liebe Lieder.", "tokens": ["Singt", "al\u00b7te", "lie\u00b7be", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Luna, solcher hohen Stelle", "tokens": ["Lu\u00b7na", ",", "sol\u00b7cher", "ho\u00b7hen", "Stel\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weiten Umblick neid ich dir;", "tokens": ["Wei\u00b7ten", "Um\u00b7blick", "neid", "ich", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sei auch der Entfernten helle,", "tokens": ["Sei", "auch", "der", "Ent\u00b7fern\u00b7ten", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Aber \u00e4ugle nicht mit ihr.", "tokens": ["A\u00b7ber", "\u00e4ug\u00b7le", "nicht", "mit", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Liebevoll und frank und frei", "tokens": ["Lie\u00b7be\u00b7voll", "und", "frank", "und", "frei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Riefst du mich heran;", "tokens": ["Riefst", "du", "mich", "he\u00b7ran", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Langsam geh ich nun vorbei,", "tokens": ["Lang\u00b7sam", "geh", "ich", "nun", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Siehst du mich denn an?", "tokens": ["Siehst", "du", "mich", "denn", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Ringlein kauft! geschwind, ihr Fraun!", "tokens": ["Rin\u00b7glein", "kauft", "!", "ge\u00b7schwind", ",", "ihr", "Fraun", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "ADJD", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6cht nicht weiterwandeln;", "tokens": ["M\u00f6cht", "nicht", "wei\u00b7ter\u00b7wan\u00b7deln", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Gegen Aug und Augenbraun", "tokens": ["Ge\u00b7gen", "Aug", "und", "Au\u00b7gen\u00b7braun"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wollt ich sie verhandeln.", "tokens": ["Wollt", "ich", "sie", "ver\u00b7han\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Ach, Zypresse, hoch zu schauen,", "tokens": ["Ach", ",", "Zyp\u00b7res\u00b7se", ",", "hoch", "zu", "schau\u00b7en", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NE", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "M\u00f6gest du dich zu mir neigen;", "tokens": ["M\u00f6\u00b7gest", "du", "dich", "zu", "mir", "nei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Habe dir was zu vertrauen,", "tokens": ["Ha\u00b7be", "dir", "was", "zu", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIS", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dann will ich ewig schweigen.", "tokens": ["Und", "dann", "will", "ich", "e\u00b7wig", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Harre lieblich im Kyanenkranze,", "tokens": ["Har\u00b7re", "lieb\u00b7lich", "im", "Ky\u00b7a\u00b7nen\u00b7kran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Blondes M\u00e4dchen, bleib er unverletzt,", "tokens": ["Blon\u00b7des", "M\u00e4d\u00b7chen", ",", "bleib", "er", "un\u00b7ver\u00b7letzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Auch wenn Luna in Orions", "tokens": ["Auch", "wenn", "Lu\u00b7na", "in", "O\u00b7rions"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "NN", "APPR", "NE"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.4": {"text": "Glanze Wechselscheinend sich ergetzt.", "tokens": ["Glan\u00b7ze", "Wech\u00b7sel\u00b7schei\u00b7nend", "sich", "er\u00b7getzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PRF", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Wei\u00df ich doch, zu welchem Gl\u00fcck", "tokens": ["Wei\u00df", "ich", "doch", ",", "zu", "wel\u00b7chem", "Gl\u00fcck"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "APPR", "PWAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00e4dchen mir emporbl\u00fcht,", "tokens": ["M\u00e4d\u00b7chen", "mir", "em\u00b7por\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wenn der feurig schwarze Blick", "tokens": ["Wenn", "der", "feu\u00b7rig", "schwar\u00b7ze", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus der Milch hervorsieht.", "tokens": ["Aus", "der", "Milch", "her\u00b7vor\u00b7sieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "Von der Rose meines Herzens", "tokens": ["Von", "der", "Ro\u00b7se", "mei\u00b7nes", "Her\u00b7zens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Pfl\u00fccktest Bl\u00e4tter nach Gefallen,", "tokens": ["Pfl\u00fcck\u00b7test", "Bl\u00e4t\u00b7ter", "nach", "Ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind vor Glut des Scheideschmerzens", "tokens": ["Sind", "vor", "Glut", "des", "Schei\u00b7de\u00b7schmer\u00b7zens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "All die andern abgefallen.", "tokens": ["All", "die", "an\u00b7dern", "ab\u00b7ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Liebt ich dich als Kleine, Kleine,", "tokens": ["Liebt", "ich", "dich", "als", "Klei\u00b7ne", ",", "Klei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "KOUS", "ADJA", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jungfrau warst du mir versagt;", "tokens": ["Jung\u00b7frau", "warst", "du", "mir", "ver\u00b7sagt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wirst doch endlich noch die Meine,", "tokens": ["Wirst", "doch", "end\u00b7lich", "noch", "die", "Mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "ART", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn der Freund die Witwe fragt.", "tokens": ["Wenn", "der", "Freund", "die", "Wit\u00b7we", "fragt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}