{"textgrid.poem.40117": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Sternzauber", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wallst du ", "tokens": ["Wallst", "du"], "token_info": ["word", "word"], "pos": ["VAFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Nacht, mit deinem Silberkranz?", "tokens": ["Nacht", ",", "mit", "dei\u00b7nem", "Sil\u00b7ber\u00b7kranz", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "zieht in deine Ewigkeiten", "tokens": ["zieht", "in", "dei\u00b7ne", "E\u00b7wig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Als ob treue Augen winken:", "tokens": ["Als", "ob", "treu\u00b7e", "Au\u00b7gen", "win\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "alle Liebe sei enth\u00fcllt!", "tokens": ["al\u00b7le", "Lie\u00b7be", "sei", "ent\u00b7h\u00fcllt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "als ob sanfte Gr\u00fc\u00dfe sinken:", "tokens": ["als", "ob", "sanf\u00b7te", "Gr\u00fc\u00b7\u00dfe", "sin\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "alle Sehnsucht sei erf\u00fcllt \u2013", "tokens": ["al\u00b7le", "Sehn\u00b7sucht", "sei", "er\u00b7f\u00fcllt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "will ein Stern sich nun entbreiten,", "tokens": ["will", "ein", "Stern", "sich", "nun", "ent\u00b7brei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "in mein schwimmend Aug' er taucht,", "tokens": ["in", "mein", "schwim\u00b7mend", "Aug'", "er", "taucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "seligste Versunkenheiten", "tokens": ["se\u00b7ligs\u00b7te", "Ver\u00b7sun\u00b7ken\u00b7hei\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "in mein schwellend Herz er haucht.", "tokens": ["in", "mein", "schwel\u00b7lend", "Herz", "er", "haucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Zu ihm heben mich Gewalten,", "tokens": ["Zu", "ihm", "he\u00b7ben", "mich", "Ge\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wie zu ", "tokens": ["wie", "zu"], "token_info": ["word", "word"], "pos": ["PWAV", "PTKZU"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "und ein Quellen, ein Entfalten", "tokens": ["und", "ein", "Quel\u00b7len", ",", "ein", "Ent\u00b7fal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "seines Scheines mich umschlingt", "tokens": ["sei\u00b7nes", "Schei\u00b7nes", "mich", "um\u00b7schlingt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "und entf\u00fchrt mich in die Zeiten,", "tokens": ["und", "ent\u00b7f\u00fchrt", "mich", "in", "die", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "da noch keine Menschen sahn", "tokens": ["da", "noch", "kei\u00b7ne", "Men\u00b7schen", "sahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "dieses F\u00fchlen, dieses Gleiten,", "tokens": ["die\u00b7ses", "F\u00fch\u00b7len", ",", "die\u00b7ses", "Glei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "dieses r\u00e4tselvolle Nahn.", "tokens": ["die\u00b7ses", "r\u00e4t\u00b7sel\u00b7vol\u00b7le", "Nahn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}