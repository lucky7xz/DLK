{"dta.poem.11619": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Iv.  \n  Der Zauberleuchtthurm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Des Zauberers sein M\u00e4gdlein sa\u00df", "tokens": ["Des", "Zau\u00b7be\u00b7rers", "sein", "M\u00e4gd\u00b7lein", "sa\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In ihrem Saale, rund von Glas.", "tokens": ["In", "ih\u00b7rem", "Saa\u00b7le", ",", "rund", "von", "Glas", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie spann beim hellen Kerzenschein,", "tokens": ["Sie", "spann", "beim", "hel\u00b7len", "Ker\u00b7zen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sang so glockenhell darein;", "tokens": ["Und", "sang", "so", "glo\u00b7cken\u00b7hell", "da\u00b7rein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Saal, als eine Kugel klar,", "tokens": ["Der", "Saal", ",", "als", "ei\u00b7ne", "Ku\u00b7gel", "klar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In L\u00fcften aufgehangen war", "tokens": ["In", "L\u00fcf\u00b7ten", "auf\u00b7ge\u00b7han\u00b7gen", "war"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "An einem Thurm auf Felsenh\u00f6h',", "tokens": ["An", "ei\u00b7nem", "Thurm", "auf", "Fel\u00b7sen\u00b7h\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bei Nacht hoch ob der wilden See,", "tokens": ["Bei", "Nacht", "hoch", "ob", "der", "wil\u00b7den", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und hing in Sturm und Wettergraus", "tokens": ["Und", "hing", "in", "Sturm", "und", "Wet\u00b7ter\u00b7graus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "An einem langen Arm hinaus.", "tokens": ["An", "ei\u00b7nem", "lan\u00b7gen", "Arm", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wenn nun ein Schiff in N\u00e4chten schwer", "tokens": ["Wenn", "nun", "ein", "Schiff", "in", "N\u00e4ch\u00b7ten", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sah weder Rath noch Rettung mehr,", "tokens": ["Sah", "we\u00b7der", "Rath", "noch", "Ret\u00b7tung", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "NN", "ADV", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Der Lootse zog die Achsel schief,", "tokens": ["Der", "Loot\u00b7se", "zog", "die", "Ach\u00b7sel", "schief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Der Hauptmann alle Teufel rief,", "tokens": ["Der", "Haupt\u00b7mann", "al\u00b7le", "Teu\u00b7fel", "rief", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Auch der Matrose wollt' verzagen:", "tokens": ["Auch", "der", "Mat\u00b7ro\u00b7se", "wollt'", "ver\u00b7za\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "O weh mir armen Schwartenmagen!", "tokens": ["O", "weh", "mir", "ar\u00b7men", "Schwar\u00b7ten\u00b7ma\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Auf einmal scheint ein Licht von fern", "tokens": ["Auf", "ein\u00b7mal", "scheint", "ein", "Licht", "von", "fern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN", "ART", "NN", "APPR", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Als wie ein heller Morgenstern;", "tokens": ["Als", "wie", "ein", "hel\u00b7ler", "Mor\u00b7gens\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.19": {"text": "Die Mannschaft jauchzet \u00fcberlaut:", "tokens": ["Die", "Mann\u00b7schaft", "jauch\u00b7zet", "\u00fc\u00b7berl\u00b7aut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Heida! jezt gilt es trockne Haut!", "tokens": ["Hei\u00b7da", "!", "jezt", "gilt", "es", "trock\u00b7ne", "Haut", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.21": {"text": "Aus allen Kr\u00e4ften steuert man", "tokens": ["Aus", "al\u00b7len", "Kr\u00e4f\u00b7ten", "steu\u00b7ert", "man"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Jezt nach dem theuren Licht hinan,", "tokens": ["Jezt", "nach", "dem", "theu\u00b7ren", "Licht", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Das w\u00e4chst und w\u00e4chst und leuchtet fast", "tokens": ["Das", "w\u00e4chst", "und", "w\u00e4chst", "und", "leuch\u00b7tet", "fast"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Wie einer Zaubersonne Glast,", "tokens": ["Wie", "ei\u00b7ner", "Zau\u00b7ber\u00b7son\u00b7ne", "Glast", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Darin ein M\u00e4gdlein sizt und spinnt,", "tokens": ["Da\u00b7rin", "ein", "M\u00e4gd\u00b7lein", "sizt", "und", "spinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Sich beuget ihr Gesang im Wind;", "tokens": ["Sich", "beu\u00b7get", "ihr", "Ge\u00b7sang", "im", "Wind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Die M\u00e4nner stehen wie verz\u00fcckt,", "tokens": ["Die", "M\u00e4n\u00b7ner", "ste\u00b7hen", "wie", "ver\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Ein Jeder nach dem Wunder blickt", "tokens": ["Ein", "Je\u00b7der", "nach", "dem", "Wun\u00b7der", "blickt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Und horcht und staunet unverwandt,", "tokens": ["Und", "horcht", "und", "stau\u00b7net", "un\u00b7ver\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Dem Steuermann entsinkt die Hand,", "tokens": ["Dem", "Steu\u00b7er\u00b7mann", "ent\u00b7sinkt", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Hat Keiner auf das Schiff mehr Acht,", "tokens": ["Hat", "Kei\u00b7ner", "auf", "das", "Schiff", "mehr", "Acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "ART", "NN", "ADV", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Bis es am Felsenriffe kracht.", "tokens": ["Bis", "es", "am", "Fel\u00b7sen\u00b7rif\u00b7fe", "kracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Die Luft zerrei\u00dft ein Jammerschrei:", "tokens": ["Die", "Luft", "zer\u00b7rei\u00dft", "ein", "Jam\u00b7mer\u00b7schrei", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Herr Gott im Himmel, steh' uns bei!", "tokens": ["Herr", "Gott", "im", "Him\u00b7mel", ",", "steh'", "uns", "bei", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Da l\u00f6scht die Zauberin ihr Licht;", "tokens": ["Da", "l\u00f6scht", "die", "Zau\u00b7be\u00b7rin", "ihr", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Noch einmal aus der Tiefe bricht", "tokens": ["Noch", "ein\u00b7mal", "aus", "der", "Tie\u00b7fe", "bricht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Verhallend Weh aus Einem Munde:", "tokens": ["Ver\u00b7hal\u00b7lend", "Weh", "aus", "Ei\u00b7nem", "Mun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Da zuckt das Schiff und sinkt zu Grunde.", "tokens": ["Da", "zuckt", "das", "Schiff", "und", "sinkt", "zu", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}