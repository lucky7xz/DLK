{"textgrid.poem.41315": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zum Dichter machten dich die Lieb' und die Natur.", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum Dichter machten dich die Lieb' und die Natur.", "tokens": ["Zum", "Dich\u00b7ter", "mach\u00b7ten", "dich", "die", "Lieb'", "und", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O w\u00e4rst du dieser stets, wie Opitz, treu gewesen!", "tokens": ["O", "w\u00e4rst", "du", "die\u00b7ser", "stets", ",", "wie", "O\u00b7pitz", ",", "treu", "ge\u00b7we\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PDS", "ADV", "$,", "PWAV", "NE", "$,", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du w\u00fcrdest noch mit Ruhm gelesen:", "tokens": ["Du", "w\u00fcr\u00b7dest", "noch", "mit", "Ruhm", "ge\u00b7le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jetzt kennt man deinen Schwulst und deine Fehler nur.", "tokens": ["Jetzt", "kennt", "man", "dei\u00b7nen", "Schwulst", "und", "dei\u00b7ne", "Feh\u00b7ler", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hat sonst dein Reiz auch Lehrer oft verf\u00fchret,", "tokens": ["Hat", "sonst", "dein", "Reiz", "auch", "Leh\u00b7rer", "oft", "ver\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "ADV", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So wirst du jetzt von Sch\u00fclern kaum ber\u00fchret.", "tokens": ["So", "wirst", "du", "jetzt", "von", "Sch\u00fc\u00b7lern", "kaum", "be\u00b7r\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Allein, wie viele sind von denen, die dich schm\u00e4hn,", "tokens": ["Al\u00b7lein", ",", "wie", "vie\u00b7le", "sind", "von", "de\u00b7nen", ",", "die", "dich", "schm\u00e4hn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PIS", "VAFIN", "APPR", "PRELS", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Zu metaphysisch schwach, wie du, sich zu vergehn!", "tokens": ["Zu", "me\u00b7ta\u00b7phy\u00b7sisch", "schwach", ",", "wie", "du", ",", "sich", "zu", "ver\u00b7gehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "$,", "PWAV", "PPER", "$,", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}