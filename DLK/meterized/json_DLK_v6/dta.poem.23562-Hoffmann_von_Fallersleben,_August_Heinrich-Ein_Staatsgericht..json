{"dta.poem.23562": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein Staatsgericht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es hat die Welt wohl ihre Mucken,", "tokens": ["Es", "hat", "die", "Welt", "wohl", "ih\u00b7re", "Mu\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch leider ihre Mucker auch;", "tokens": ["Doch", "lei\u00b7der", "ih\u00b7re", "Mu\u00b7cker", "auch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Mucken k\u00f6nntest du verschlucken,", "tokens": ["Die", "Mu\u00b7cken", "k\u00f6nn\u00b7test", "du", "ver\u00b7schlu\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Mucker platzte dir der Bauch.", "tokens": ["Vom", "Mu\u00b7cker", "platz\u00b7te", "dir", "der", "Bauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch w\u00e4r' ein Staatsbauch mir beschieden,", "tokens": ["Doch", "w\u00e4r'", "ein", "Staats\u00b7bauch", "mir", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O weh der armen Muckerschaar!", "tokens": ["O", "weh", "der", "ar\u00b7men", "Mu\u00b7cker\u00b7schaar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein einz'ger Mucker blieb' in Frieden,", "tokens": ["Kein", "einz'\u00b7ger", "Mu\u00b7cker", "blieb'", "in", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich fr\u00e4\u00dfe sie mit Haut und Haar.", "tokens": ["Ich", "fr\u00e4\u00b7\u00dfe", "sie", "mit", "Haut", "und", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}