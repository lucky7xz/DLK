{"dta.poem.87": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als er  Urona  verlassen muste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Du Sam\u0303el-Platz der Lust/ du Wohn-Haus grosser Freuden/", "tokens": ["Du", "Sam\u0303el\u00b7Platz", "der", "Lust", "/", "du", "Wohn\u00b7Haus", "gros\u00b7ser", "Freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$(", "PPER", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Mein Schicksahl zwinget mich von deiner Pracht zu", "tokens": ["Mein", "Schick\u00b7sahl", "zwin\u00b7get", "mich", "von", "dei\u00b7ner", "Pracht", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "PTKZU"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "scheiden/", "tokens": ["schei\u00b7den", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Es rafft ein Unfall mich von deinem Feste hin/", "tokens": ["Es", "rafft", "ein", "Un\u00b7fall", "mich", "von", "dei\u00b7nem", "Fes\u00b7te", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das mit beliebter Lust erquicket Hertz und Sinn.", "tokens": ["Das", "mit", "be\u00b7lieb\u00b7ter", "Lust", "er\u00b7quic\u00b7ket", "Hertz", "und", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADJA", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ob gleich die ", "tokens": ["Ob", "gleich", "die"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADV", "ART"], "meter": "++-", "measure": "unknown.measure.di"}, "line.7": {"text": "Und ein Atlassen Kleid von Schnee und Eyse tr\u00e4get/", "tokens": ["Und", "ein", "At\u00b7las\u00b7sen", "Kleid", "von", "Schnee", "und", "Ey\u00b7se", "tr\u00e4\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So l\u00e4st die Lustbarkeit dich doch nicht \u00f6de seyn/", "tokens": ["So", "l\u00e4st", "die", "Lust\u00b7bar\u00b7keit", "dich", "doch", "nicht", "\u00f6\u00b7de", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADV", "PTKNEG", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und was Vergn\u00fcgen heist geht nicht im Winter ein.", "tokens": ["Und", "was", "Ver\u00b7gn\u00fc\u00b7gen", "heist", "geht", "nicht", "im", "Win\u00b7ter", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "NN", "VVFIN", "VVFIN", "PTKNEG", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Es kan die Schlitten-Fahrt die Garten-Lust ersetzen/", "tokens": ["Es", "kan", "die", "Schlit\u00b7ten\u00b7Fahrt", "die", "Gar\u00b7ten\u00b7Lust", "er\u00b7set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.12": {"text": "Es l\u00e4st die Anmuht uns auf Zucker-Rosen gehn/", "tokens": ["Es", "l\u00e4st", "die", "An\u00b7muht", "uns", "auf", "Zu\u00b7cke\u00b7rRo\u00b7sen", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und s\u00fcsse Mandeln-Milch in g\u00fcldnen Schaalen stehn.", "tokens": ["Und", "s\u00fcs\u00b7se", "Man\u00b7deln\u00b7Milch", "in", "g\u00fcld\u00b7nen", "Schaa\u00b7len", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Blumen deiner Lust bestricken Aug\u2019 und Hertzen/", "tokens": ["Die", "Blu\u00b7men", "dei\u00b7ner", "Lust", "be\u00b7stri\u00b7cken", "Aug'", "und", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Jhr lieblich Angesicht vertreibet alle Schmertzen/", "tokens": ["Ihr", "lieb\u00b7lich", "An\u00b7ge\u00b7sicht", "ver\u00b7trei\u00b7bet", "al\u00b7le", "Schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und ihrer Sch\u00f6nheits-Pracht nichts irrdsches \u00fcberwiegt/", "tokens": ["Und", "ih\u00b7rer", "Sch\u00f6n\u00b7heits\u00b7Pracht", "nichts", "irrd\u00b7sches", "\u00fc\u00b7berw\u00b7iegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PIS", "ADJA", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Sie sind der sch\u00f6nste Theil von dem was uns vergn\u00fcgt.", "tokens": ["Sie", "sind", "der", "sch\u00f6ns\u00b7te", "Theil", "von", "dem", "was", "uns", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "ART", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die holden Kindergens den sch\u00f6nsten Engeln gleichen/", "tokens": ["Die", "hol\u00b7den", "Kin\u00b7der\u00b7gens", "den", "sch\u00f6ns\u00b7ten", "En\u00b7geln", "glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Dian' und Venus mu\u00df vor ihren Blicken weichen:", "tokens": ["Dian'", "und", "Ve\u00b7nus", "mu\u00df", "vor", "ih\u00b7ren", "Bli\u00b7cken", "wei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.20": {"text": "Die Glieder sind wie Schnee/ wie Wolle zart und wei\u00df/", "tokens": ["Die", "Glie\u00b7der", "sind", "wie", "Schnee", "/", "wie", "Wol\u00b7le", "zart", "und", "wei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "NN", "$(", "KOKOM", "NN", "ADJD", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Sie sind dein bester Schatz/ der Sch\u00f6nheit Ehren-Prei\u00df.", "tokens": ["Sie", "sind", "dein", "bes\u00b7ter", "Schatz", "/", "der", "Sch\u00f6n\u00b7heit", "Eh\u00b7ren\u00b7Prei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$(", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die Wangen sind annoch im ersten Fr\u00fchlings-Jahren/", "tokens": ["Die", "Wan\u00b7gen", "sind", "an\u00b7noch", "im", "ers\u00b7ten", "Fr\u00fch\u00b7lings\u00b7Jah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPRART", "ADJA", "NN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.23": {"text": "Auf welchen Milch und Blut in sch\u00f6nster Lust sich paaren/", "tokens": ["Auf", "wel\u00b7chen", "Milch", "und", "Blut", "in", "sch\u00f6ns\u00b7ter", "Lust", "sich", "paa\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "KON", "NN", "APPR", "ADJA", "NN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der Augen helles Licht den lichten Tag gebiehrt/", "tokens": ["Der", "Au\u00b7gen", "hel\u00b7les", "Licht", "den", "lich\u00b7ten", "Tag", "ge\u00b7biehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und dich/ du sch\u00f6ner Ort/ mit vielen Sternen ziert.", "tokens": ["Und", "dich", "/", "du", "sch\u00f6\u00b7ner", "Ort", "/", "mit", "vie\u00b7len", "Ster\u00b7nen", "ziert", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "PPER", "ADJA", "NN", "$(", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Jhr sch\u00f6nes Antlitzt ist ein kostbahr ", "tokens": ["Ihr", "sch\u00f6\u00b7nes", "Ant\u00b7litzt", "ist", "ein", "kost\u00b7bahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "Darinnen Ros\u2019 und Lilg sich durch einander karten/", "tokens": ["Da\u00b7rin\u00b7nen", "Ros'", "und", "Lilg", "sich", "durch", "ein\u00b7an\u00b7der", "kar\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "PRF", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die Lippen cr\u00f6nt Corall/ den Zucker-Mund Rubin/", "tokens": ["Die", "Lip\u00b7pen", "cr\u00f6nt", "Co\u00b7rall", "/", "den", "Zu\u00b7cke\u00b7rMund", "Ru\u00b7bin", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "$(", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Jhr Athen \u00fcbersteigt den ", "tokens": ["Ihr", "A\u00b7then", "\u00fc\u00b7bers\u00b7teigt", "den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Der wohl-gew\u00f6lbten Brust ihr Spiegel glattes Pflaster", "tokens": ["Der", "wohl\u00b7ge\u00b7w\u00f6lb\u00b7ten", "Brust", "ihr", "Spie\u00b7gel", "glat\u00b7tes", "Pflas\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "An Klarheit macht besch\u00e4mt den wei\u00dften Alabaster;", "tokens": ["An", "Klar\u00b7heit", "macht", "be\u00b7sch\u00e4mt", "den", "wei\u00df\u00b7ten", "A\u00b7la\u00b7bas\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Granaten bl\u00fchn im Schnee den blauer T\u00fcrcks durchflicht", "tokens": ["Gra\u00b7na\u00b7ten", "bl\u00fchn", "im", "Schnee", "den", "blau\u00b7er", "T\u00fcrcks", "durch\u00b7flicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Es gleicht des Himmels Glantz den ", "tokens": ["Es", "gleicht", "des", "Him\u00b7mels", "Glantz", "den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Sie sind der Sorgen-Pest/ der Lust Geb\u00e4hrerinnen", "tokens": ["Sie", "sind", "der", "Sor\u00b7gen\u00b7Pest", "/", "der", "Lust", "Ge\u00b7b\u00e4h\u00b7re\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Es l\u00e4st ihr holder Mund des ", "tokens": ["Es", "l\u00e4st", "ihr", "hol\u00b7der", "Mund", "des"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Jhr sch\u00f6nes Wesen zeugt stets neue Lieblichkeit/", "tokens": ["Ihr", "sch\u00f6\u00b7nes", "We\u00b7sen", "zeugt", "stets", "neu\u00b7e", "Lieb\u00b7lich\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die bitter Wermuth wird nie in die Lust gestreut.", "tokens": ["Die", "bit\u00b7ter", "Wer\u00b7muth", "wird", "nie", "in", "die", "Lust", "ge\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.38": {"text": "Dis alles zwinget mich mein Schicksahl zu verlassen/", "tokens": ["Dis", "al\u00b7les", "zwin\u00b7get", "mich", "mein", "Schick\u00b7sahl", "zu", "ver\u00b7las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Es schenckt/ vor s\u00fcssen Most/ mir Galle in die ", "tokens": ["Es", "schenckt", "/", "vor", "s\u00fcs\u00b7sen", "Most", "/", "mir", "Gal\u00b7le", "in", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "APPR", "ADJA", "NN", "$(", "PPER", "NN", "APPR", "ART"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.40": {"text": "An statt des wahren Lichts reicht es den Schatten dar/", "tokens": ["An", "statt", "des", "wah\u00b7ren", "Lichts", "reicht", "es", "den", "Schat\u00b7ten", "dar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Und macht/ ein solches Spiel/ mir gleich zum Neuen-Jahr.", "tokens": ["Und", "macht", "/", "ein", "sol\u00b7ches", "Spiel", "/", "mir", "gleich", "zum", "Neu\u00b7en\u00b7Jahr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "ART", "PIAT", "NN", "$(", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}