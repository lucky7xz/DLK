{"textgrid.poem.28893": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Keine Schand-, ein Ehren-Hure soll man, Pornia, dich nennen;", "tokens": ["Kei\u00b7ne", "Schan\u00b7d", ",", "ein", "Eh\u00b7ren\u00b7Hu\u00b7re", "soll", "man", ",", "Por\u00b7nia", ",", "dich", "nen\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "TRUNC", "$,", "ART", "NN", "VMFIN", "PIS", "$,", "NE", "$,", "PPER", "VVINF", "$."], "meter": "+-+--+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Weil dich nicht verachte Leute, sondern die geehrten kennen.", "tokens": ["Weil", "dich", "nicht", "ver\u00b7ach\u00b7te", "Leu\u00b7te", ",", "son\u00b7dern", "die", "ge\u00b7ehr\u00b7ten", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJA", "NN", "$,", "KON", "ART", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}