{"dta.poem.23689": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Einladung zur Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "M&#228;dchen, wolt ihr mich nicht lieben?", "tokens": ["M", "&#228;", "dchen", ",", "wolt", "ihr", "mich", "nicht", "lie\u00b7ben", "?"], "token_info": ["word", "XML_entity", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVINF", "$,", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seht, hier lieg ich in dem Schatten!", "tokens": ["Seht", ",", "hier", "lieg", "ich", "in", "dem", "Schat\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seht mich nur, ihr m\u00fc\u00dft mich lieben!", "tokens": ["Seht", "mich", "nur", ",", "ihr", "m\u00fc\u00dft", "mich", "lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rosen bl\u00fchen auf den Wangen,", "tokens": ["Ro\u00b7sen", "bl\u00fc\u00b7hen", "auf", "den", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In den Adern gl\u00fchet Feuer,", "tokens": ["In", "den", "A\u00b7dern", "gl\u00fc\u00b7het", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In den Minen lacht Vergn\u00fcgen,", "tokens": ["In", "den", "Mi\u00b7nen", "lacht", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In den Augen lokket Liebe,", "tokens": ["In", "den", "Au\u00b7gen", "lok\u00b7ket", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und bewegen sich die Lippen,", "tokens": ["Und", "be\u00b7we\u00b7gen", "sich", "die", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "So bewegt sie Scherz und Freude.", "tokens": ["So", "be\u00b7wegt", "sie", "Scherz", "und", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "M\u00e4dchen, wolt ihr mich nicht lieben?", "tokens": ["M\u00e4d\u00b7chen", ",", "wolt", "ihr", "mich", "nicht", "lie\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Seht, hier lieg ich in dem Schatten,", "tokens": ["Seht", ",", "hier", "lieg", "ich", "in", "dem", "Schat\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "M\u00e4dchen seht, wie sch\u00f6n ich liege!", "tokens": ["M\u00e4d\u00b7chen", "seht", ",", "wie", "sch\u00f6n", "ich", "lie\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}