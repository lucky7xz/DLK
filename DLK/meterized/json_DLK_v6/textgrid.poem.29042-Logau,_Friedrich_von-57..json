{"textgrid.poem.29042": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "57.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der deutschen Tichter Helena, de\u00df Opitz seine Leyer,", "tokens": ["Der", "deut\u00b7schen", "Tich\u00b7ter", "He\u00b7le\u00b7na", ",", "de\u00df", "O\u00b7pitz", "sei\u00b7ne", "Le\u00b7yer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.2": {"text": "Hat zwar viel Buhler stets gehabt und, wie man meint, auch Freyer.", "tokens": ["Hat", "zwar", "viel", "Buh\u00b7ler", "stets", "ge\u00b7habt", "und", ",", "wie", "man", "meint", ",", "auch", "Frey\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "ADV", "VAPP", "KON", "$,", "PWAV", "PIS", "VVFIN", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Mich d\u00fcnckt, da\u00df ihre Jungferschafft noch richtig sey und rein,", "tokens": ["Mich", "d\u00fcnckt", ",", "da\u00df", "ih\u00b7re", "Jung\u00b7fer\u00b7schafft", "noch", "rich\u00b7tig", "sey", "und", "rein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "ADV", "ADJD", "VAFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Und der, der ihr geh\u00f6ren wird, wird noch von dannen seyn.", "tokens": ["Und", "der", ",", "der", "ihr", "ge\u00b7h\u00f6\u00b7ren", "wird", ",", "wird", "noch", "von", "dan\u00b7nen", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "PPER", "VVINF", "VAFIN", "$,", "VAFIN", "ADV", "APPR", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}