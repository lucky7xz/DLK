{"textgrid.poem.67372": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Weisen eines Landes sahen einst", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Weisen eines Landes sahen einst", "tokens": ["Die", "Wei\u00b7sen", "ei\u00b7nes", "Lan\u00b7des", "sa\u00b7hen", "einst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Annahen eine Constellation,", "tokens": ["An\u00b7na\u00b7hen", "ei\u00b7ne", "Con\u00b7stel\u00b7la\u00b7ti\u00b7on", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Jedermann den Sinn verr\u00fcckte. Sie", "tokens": ["Die", "Je\u00b7der\u00b7mann", "den", "Sinn", "ver\u00b7r\u00fcck\u00b7te", ".", "Sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "PIS", "ART", "NN", "VVFIN", "$.", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Entflohn der Gegend, um nachher den Wahnsinn", "tokens": ["Ent\u00b7flohn", "der", "Ge\u00b7gend", ",", "um", "nach\u00b7her", "den", "Wahn\u00b7sinn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "KOUI", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der kranken Br\u00fcder mit Vernunft zu heilen.", "tokens": ["Der", "kran\u00b7ken", "Br\u00fc\u00b7der", "mit", "Ver\u00b7nunft", "zu", "hei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der b\u00f6se Stern erschien: die Krankheit kam;", "tokens": ["Der", "b\u00f6\u00b7se", "Stern", "er\u00b7schien", ":", "die", "Krank\u00b7heit", "kam", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Vielartig raseten die Menschen. Da", "tokens": ["Viel\u00b7ar\u00b7tig", "ra\u00b7se\u00b7ten", "die", "Men\u00b7schen", ".", "Da"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$.", "KOUS"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Trat freundlich ihnen zu der Weisen Rath,", "tokens": ["Trat", "freund\u00b7lich", "ih\u00b7nen", "zu", "der", "Wei\u00b7sen", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "In Sitten, Kleidern, Nahrung doch nach alter", "tokens": ["In", "Sit\u00b7ten", ",", "Klei\u00b7dern", ",", "Nah\u00b7rung", "doch", "nach", "al\u00b7ter"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Vern\u00fcnft'ger Art zu leben: All umsonst;", "tokens": ["Ver\u00b7n\u00fcnft'\u00b7ger", "Art", "zu", "le\u00b7ben", ":", "All", "um\u00b7sonst", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$.", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Mit Schlag und Sto\u00df vergalt man ihre G\u00fcte.", "tokens": ["Mit", "Schlag", "und", "Sto\u00df", "ver\u00b7galt", "man", "ih\u00b7re", "G\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Was war zu thun? Dem Tode zu entgehn", "tokens": ["Was", "war", "zu", "thun", "?", "Dem", "To\u00b7de", "zu", "ent\u00b7gehn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PTKZU", "VVINF", "$.", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "(denn der Gewaltigste war stets der gr\u00f6\u00dfte,", "tokens": ["(", "denn", "der", "Ge\u00b7wal\u00b7tigs\u00b7te", "war", "stets", "der", "gr\u00f6\u00df\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ART", "NN", "VAFIN", "ADV", "ART", "ADJA", "$,"], "meter": "+--+---+-+-", "measure": "dactylic.di.plus"}, "line.14": {"text": "Furchtbarste Narr), entschlossen sie sich kl\u00fcglich,", "tokens": ["Furcht\u00b7bars\u00b7te", "Narr", ")", ",", "ent\u00b7schlos\u00b7sen", "sie", "sich", "kl\u00fcg\u00b7lich", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "$,", "VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Mit Thoren nach der Thoren Art zu leben.", "tokens": ["Mit", "Tho\u00b7ren", "nach", "der", "Tho\u00b7ren", "Art", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Nur bei verschlossnen Th\u00fcren durften sie", "tokens": ["Nur", "bei", "ver\u00b7schloss\u00b7nen", "Th\u00fc\u00b7ren", "durf\u00b7ten", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "VMFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Vern\u00fcnftig sein; von au\u00dfen, vor der Welt", "tokens": ["Ver\u00b7n\u00fcnf\u00b7tig", "sein", ";", "von", "au\u00b7\u00dfen", ",", "vor", "der", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAINF", "$.", "APPR", "ADV", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Machten sie n\u00e4rrisch jede Narrheit mit.", "tokens": ["Mach\u00b7ten", "sie", "n\u00e4r\u00b7risch", "je\u00b7de", "Nar\u00b7rheit", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PIAT", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}