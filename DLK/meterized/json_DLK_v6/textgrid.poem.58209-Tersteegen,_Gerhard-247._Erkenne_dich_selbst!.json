{"textgrid.poem.58209": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "247. Erkenne dich selbst!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie kann ein Mensch noch \u00fcber ", "tokens": ["Wie", "kann", "ein", "Mensch", "noch", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ART", "NN", "ADV", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir fehlt Geduld, da\u00df ich ", "tokens": ["Mir", "fehlt", "Ge\u00b7duld", ",", "da\u00df", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "KOUS", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer tief sich kennt und sich nur f\u00fchlet recht,", "tokens": ["Wer", "tief", "sich", "kennt", "und", "sich", "nur", "f\u00fch\u00b7let", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PRF", "VVFIN", "KON", "PRF", "ADV", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Will gerne sein der gr\u00f6\u00dften S\u00fcnder Knecht.", "tokens": ["Will", "ger\u00b7ne", "sein", "der", "gr\u00f6\u00df\u00b7ten", "S\u00fcn\u00b7der", "Knecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}