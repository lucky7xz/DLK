{"textgrid.poem.64832": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Begeisterndes Gl\u00fcck!", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Begeisterndes Gl\u00fcck!", "tokens": ["Be\u00b7geis\u00b7tern\u00b7des", "Gl\u00fcck", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Es sank von den schmerzlichen Werken,", "tokens": ["Es", "sank", "von", "den", "schmerz\u00b7li\u00b7chen", "Wer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Von allen den qualvollen M\u00fch'n", "tokens": ["Von", "al\u00b7len", "den", "qual\u00b7vol\u00b7len", "M\u00fch'n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Die irdische H\u00fclle in Schlummer dahin.", "tokens": ["Die", "ir\u00b7di\u00b7sche", "H\u00fcl\u00b7le", "in", "Schlum\u00b7mer", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "PAV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Und ich darf zur\u00fcck,", "tokens": ["Und", "ich", "darf", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Im himmlischen See mich zu st\u00e4rken!", "tokens": ["Im", "himm\u00b7li\u00b7schen", "See", "mich", "zu", "st\u00e4r\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Am Tage da h\u00e4lt mich mein Kerker fest,", "tokens": ["Am", "Ta\u00b7ge", "da", "h\u00e4lt", "mich", "mein", "Ker\u00b7ker", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da bin ich zur\u00fcck in den Busen gepre\u00dft,", "tokens": ["Da", "bin", "ich", "zu\u00b7r\u00fcck", "in", "den", "Bu\u00b7sen", "ge\u00b7pre\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Zur Qual und zum Weh f\u00fcr uns Beide;", "tokens": ["Zur", "Qual", "und", "zum", "Weh", "f\u00fcr", "uns", "Bei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "APPR", "PPER", "PIS", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Denn Psyche ertr\u00e4gt die Gefangenschaft nicht,", "tokens": ["Denn", "Psy\u00b7che", "er\u00b7tr\u00e4gt", "die", "Ge\u00b7fan\u00b7gen\u00b7schaft", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Und ich r\u00fcttle am Gitter, bis da\u00df es zerbricht,", "tokens": ["Und", "ich", "r\u00fctt\u00b7le", "am", "Git\u00b7ter", ",", "bis", "da\u00df", "es", "zer\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "$,", "KON", "KOUS", "PPER", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Wenn im Kerker ich allzuviel leide.", "tokens": ["Wenn", "im", "Ker\u00b7ker", "ich", "all\u00b7zu\u00b7viel", "lei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PPER", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Da bin ich versenkt in den blutigen Strom,", "tokens": ["Da", "bin", "ich", "ver\u00b7senkt", "in", "den", "blu\u00b7ti\u00b7gen", "Strom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ich, die in der Himmel helleuchtendem Dom", "tokens": ["Ich", ",", "die", "in", "der", "Him\u00b7mel", "hel\u00b7leuch\u00b7ten\u00b7dem", "Dom"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Gewohnt bin, auf Strahlen zu schweben!", "tokens": ["Ge\u00b7wohnt", "bin", ",", "auf", "Strah\u00b7len", "zu", "schwe\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Befleckt bin ich da von zerfallendem Staub,", "tokens": ["Be\u00b7fleckt", "bin", "ich", "da", "von", "zer\u00b7fal\u00b7len\u00b7dem", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Ich, der einst die Sterne als goldenes Laub", "tokens": ["Ich", ",", "der", "einst", "die", "Ster\u00b7ne", "als", "gol\u00b7de\u00b7nes", "Laub"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ADV", "ART", "NN", "KOUS", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Verkl\u00e4rend die Stirne umgeben!", "tokens": ["Ver\u00b7kl\u00e4\u00b7rend", "die", "Stir\u00b7ne", "um\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Drum bin ich so selig, wenn endlich die Nacht,", "tokens": ["Drum", "bin", "ich", "so", "se\u00b7lig", ",", "wenn", "end\u00b7lich", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "ADJD", "$,", "KOUS", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Vermittelnd die Pforten der Erde bewacht,", "tokens": ["Ver\u00b7mit\u00b7telnd", "die", "Pfor\u00b7ten", "der", "Er\u00b7de", "be\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wenn alle die H\u00fcllen entschlafen.", "tokens": ["Wenn", "al\u00b7le", "die", "H\u00fcl\u00b7len", "ent\u00b7schla\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wenn vorbei erst des Tages so peinliche Frist,", "tokens": ["Wenn", "vor\u00b7bei", "erst", "des", "Ta\u00b7ges", "so", "pein\u00b7li\u00b7che", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ART", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Da werd' ich von Freiheit auf Stunden begr\u00fc\u00dft,", "tokens": ["Da", "werd'", "ich", "von", "Frei\u00b7heit", "auf", "Stun\u00b7den", "be\u00b7gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Da eil' ich zum heimischen Hafen!", "tokens": ["Da", "eil'", "ich", "zum", "hei\u00b7mi\u00b7schen", "Ha\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "In Aether mich badend durchflieg' ich die Welt,", "tokens": ["In", "A\u00b7e\u00b7ther", "mich", "ba\u00b7dend", "durch\u00b7flieg'", "ich", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Das prangende raumlose Himmelsgezelt,", "tokens": ["Das", "pran\u00b7gen\u00b7de", "raum\u00b7lo\u00b7se", "Him\u00b7mels\u00b7ge\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Vom Sonnensysteme zum andern.", "tokens": ["Vom", "Son\u00b7nen\u00b7sys\u00b7te\u00b7me", "zum", "an\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich lausche der Harfe in Seraphshand,", "tokens": ["Ich", "lau\u00b7sche", "der", "Har\u00b7fe", "in", "Se\u00b7raph\u00b7shand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Ich nipp' an der himmlischen Blumen Rand \u2013", "tokens": ["Ich", "nipp'", "an", "der", "himm\u00b7li\u00b7schen", "Blu\u00b7men", "Rand", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "O selig entz\u00fcckendes Wandern!", "tokens": ["O", "se\u00b7lig", "ent\u00b7z\u00fc\u00b7cken\u00b7des", "Wan\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADJA", "NN", "$."], "meter": "----+--+-", "measure": "iambic.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Ich gr\u00fc\u00dfe die Geister, die vor mir befreit,", "tokens": ["Ich", "gr\u00fc\u00b7\u00dfe", "die", "Geis\u00b7ter", ",", "die", "vor", "mir", "be\u00b7freit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "F\u00fcr die schon vorbei der Verbannung Zeit;", "tokens": ["F\u00fcr", "die", "schon", "vor\u00b7bei", "der", "Ver\u00b7ban\u00b7nung", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADV", "ART", "NN", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ich habe sie wieder gefunden,", "tokens": ["Ich", "ha\u00b7be", "sie", "wie\u00b7der", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und sag' ihnen nun, wie ich ihrer gedacht,", "tokens": ["Und", "sag'", "ih\u00b7nen", "nun", ",", "wie", "ich", "ih\u00b7rer", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "PPOSAT", "VVPP", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "In Thr\u00e4nen die nachtvollen Tage durchwacht", "tokens": ["In", "Thr\u00e4\u00b7nen", "die", "nacht\u00b7vol\u00b7len", "Ta\u00b7ge", "durch\u00b7wacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "VVPP"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und wie nun geheilt meine Wunden!", "tokens": ["Und", "wie", "nun", "ge\u00b7heilt", "mei\u00b7ne", "Wun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Ich sehe das Auge, das jenseits der Zeit", "tokens": ["Ich", "se\u00b7he", "das", "Au\u00b7ge", ",", "das", "jen\u00b7seits", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+--+---+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Uns sch\u00fctzend bewacht f\u00fcr die Ewigkeit,", "tokens": ["Uns", "sch\u00fct\u00b7zend", "be\u00b7wacht", "f\u00fcr", "die", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und das Herz voll unendlicher Liebe!", "tokens": ["Und", "das", "Herz", "voll", "un\u00b7end\u00b7li\u00b7cher", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Ich ahne der Sch\u00f6pfung hochheiligen Grund,", "tokens": ["Ich", "ah\u00b7ne", "der", "Sch\u00f6p\u00b7fung", "hoch\u00b7hei\u00b7li\u00b7gen", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Die G\u00fcte mit Weisheit und Allmacht im Bund \u2013", "tokens": ["Die", "G\u00fc\u00b7te", "mit", "Weis\u00b7heit", "und", "All\u00b7macht", "im", "Bund", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "APPRART", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "O da\u00df es doch ewig so bliebe!", "tokens": ["O", "da\u00df", "es", "doch", "e\u00b7wig", "so", "blie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "ADV", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Doch wehe! da hebt sich das Morgenroth", "tokens": ["Doch", "we\u00b7he", "!", "da", "hebt", "sich", "das", "Mor\u00b7gen\u00b7roth"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$.", "ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und ich mu\u00df zur\u00fcck in das Leben, den Tod,", "tokens": ["Und", "ich", "mu\u00df", "zu\u00b7r\u00fcck", "in", "das", "Le\u00b7ben", ",", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "APPR", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Bis dieser einst Leben mir spendet.", "tokens": ["Bis", "die\u00b7ser", "einst", "Le\u00b7ben", "mir", "spen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADV", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "O strahlende Heimath, so fahre denn hin!", "tokens": ["O", "strah\u00b7len\u00b7de", "Hei\u00b7math", ",", "so", "fah\u00b7re", "denn", "hin", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "ADV", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "So segelt, ihr eilenden Welten, wohin", "tokens": ["So", "se\u00b7gelt", ",", "ihr", "ei\u00b7len\u00b7den", "Wel\u00b7ten", ",", "wo\u00b7hin"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,", "PWAV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Der g\u00f6ttliche Wille euch sendet!", "tokens": ["Der", "g\u00f6tt\u00b7li\u00b7che", "Wil\u00b7le", "euch", "sen\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+----", "measure": "iambic.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Mich sendet er fort aus dem himmlischen Land,", "tokens": ["Mich", "sen\u00b7det", "er", "fort", "aus", "dem", "himm\u00b7li\u00b7schen", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+---+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Mich hat er in dr\u00fcckende Fesseln gebannt", "tokens": ["Mich", "hat", "er", "in", "dr\u00fc\u00b7cken\u00b7de", "Fes\u00b7seln", "ge\u00b7bannt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Auf jenem unsel'gen Planeten!", "tokens": ["Auf", "je\u00b7nem", "un\u00b7sel'\u00b7gen", "Pla\u00b7ne\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Heimkehr auf immer ist mir noch verwehrt,", "tokens": ["Die", "Heim\u00b7kehr", "auf", "im\u00b7mer", "ist", "mir", "noch", "ver\u00b7wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und was mir im Kerker an Tr\u00f6stung bescheert,", "tokens": ["Und", "was", "mir", "im", "Ker\u00b7ker", "an", "Tr\u00f6s\u00b7tung", "be\u00b7scheert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "APPRART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Ist Lieben und Dichten und Beten.", "tokens": ["Ist", "Lie\u00b7ben", "und", "Dich\u00b7ten", "und", "Be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "KON", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}