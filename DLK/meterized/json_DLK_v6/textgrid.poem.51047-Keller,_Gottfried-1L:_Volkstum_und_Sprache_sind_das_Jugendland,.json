{"textgrid.poem.51047": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Volkstum und Sprache sind das Jugendland,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Volkstum und Sprache sind das Jugendland,", "tokens": ["Volks\u00b7tum", "und", "Spra\u00b7che", "sind", "das", "Ju\u00b7gend\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Darin die V\u00f6lker wachsen und gedeihen,", "tokens": ["Da\u00b7rin", "die", "V\u00f6l\u00b7ker", "wach\u00b7sen", "und", "ge\u00b7dei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Mutterhaus, nach dem sie sehnend schreien,", "tokens": ["Das", "Mut\u00b7ter\u00b7haus", ",", "nach", "dem", "sie", "seh\u00b7nend", "schrei\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn sie verschlagen sind auf fremden Strand.", "tokens": ["Wenn", "sie", "ver\u00b7schla\u00b7gen", "sind", "auf", "frem\u00b7den", "Strand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch manchmal werden sie zum G\u00e4ngelband,", "tokens": ["Doch", "manch\u00b7mal", "wer\u00b7den", "sie", "zum", "G\u00e4n\u00b7gel\u00b7band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sogar zur Kette um den Hals der Freien;", "tokens": ["So\u00b7gar", "zur", "Ket\u00b7te", "um", "den", "Hals", "der", "Frei\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann treiben L\u00e4ngsterwachsne Spielereien,", "tokens": ["Dann", "trei\u00b7ben", "L\u00e4ngs\u00b7ter\u00b7wachs\u00b7ne", "Spie\u00b7le\u00b7rei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Genarrt von der Tyrannen schlauer Hand.", "tokens": ["Ge\u00b7narrt", "von", "der", "Ty\u00b7ran\u00b7nen", "schlau\u00b7er", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hier trenne sich der lang vereinte Strom!", "tokens": ["Hier", "tren\u00b7ne", "sich", "der", "lang", "ver\u00b7ein\u00b7te", "Strom", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Versiegend schwinde ", "tokens": ["Ver\u00b7sie\u00b7gend", "schwin\u00b7de"], "token_info": ["word", "word"], "pos": ["ADJD", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Der andre breche sich ein neues Bette!", "tokens": ["Der", "and\u00b7re", "bre\u00b7che", "sich", "ein", "neu\u00b7es", "Bet\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Denn ", "tokens": ["Denn"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Das ist die Freiheit, der polit'sche Glaube,", "tokens": ["Das", "ist", "die", "Frei\u00b7heit", ",", "der", "po\u00b7lit'\u00b7sche", "Glau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der l\u00f6st und bindet jede Seelenkette!", "tokens": ["Der", "l\u00f6st", "und", "bin\u00b7det", "je\u00b7de", "See\u00b7len\u00b7ket\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}