{"textgrid.poem.39930": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der Bau des Reissensteins", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Droben von dem Berge hoch", "tokens": ["Dro\u00b7ben", "von", "dem", "Ber\u00b7ge", "hoch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaut herab das Felsenloch,", "tokens": ["Schaut", "her\u00b7ab", "das", "Fel\u00b7sen\u00b7loch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drin aus seiner langen Nacht", "tokens": ["Drin", "aus", "sei\u00b7ner", "lan\u00b7gen", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist der Riese Heim erwacht.", "tokens": ["Ist", "der", "Rie\u00b7se", "Heim", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Streckt das zott'ge Haupt hervor,", "tokens": ["Streckt", "das", "zott'\u00b7ge", "Haupt", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Luget durch sein schwarzes Thor;", "tokens": ["Lu\u00b7get", "durch", "sein", "schwar\u00b7zes", "Thor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihm gef\u00e4llt das tiefe Thal,", "tokens": ["Ihm", "ge\u00b7f\u00e4llt", "das", "tie\u00b7fe", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der gew\u00f6lbte Riesensaal.", "tokens": ["Der", "ge\u00b7w\u00f6lb\u00b7te", "Rie\u00b7sen\u00b7saal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und er sehnt sich nach dem Licht,", "tokens": ["Und", "er", "sehnt", "sich", "nach", "dem", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weilt in seinem Steine nicht;", "tokens": ["Weilt", "in", "sei\u00b7nem", "Stei\u00b7ne", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bald mit einem Schritt er stand", "tokens": ["Bald", "mit", "ei\u00b7nem", "Schritt", "er", "stand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf der andern Felsenwand.", "tokens": ["Auf", "der", "an\u00b7dern", "Fel\u00b7sen\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie am Berg der Donner grollt,", "tokens": ["Wie", "am", "Berg", "der", "Don\u00b7ner", "grollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "So sein Wort zu Thale rollt:", "tokens": ["So", "sein", "Wort", "zu", "Tha\u00b7le", "rollt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbzwerglein! menschliches Geschlecht,", "tokens": ["\u00bb", "zwerg\u00b7lein", "!", "menschli\u00b7ches", "Ge\u00b7schlecht", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Diene mir beim Bau als Knecht!\u00ab", "tokens": ["Die\u00b7ne", "mir", "beim", "Bau", "als", "Knecht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "KOUS", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wimmelnd kommen sie heran,", "tokens": ["Wim\u00b7melnd", "kom\u00b7men", "sie", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Maurer, Steinmetz, Zimmermann;", "tokens": ["Mau\u00b7rer", ",", "Stein\u00b7metz", ",", "Zim\u00b7mer\u00b7mann", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bauen all' auf sein Gehei\u00df", "tokens": ["Bau\u00b7en", "all'", "auf", "sein", "Ge\u00b7hei\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PIS", "APPR", "PPOSAT", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "In des Angesichtes Schwei\u00df.", "tokens": ["In", "des", "An\u00b7ge\u00b7sich\u00b7tes", "Schwei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Fertig steht der Riesenstein,", "tokens": ["Fer\u00b7tig", "steht", "der", "Rie\u00b7sen\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wurzelt in dem Felsen ein,", "tokens": ["Wur\u00b7zelt", "in", "dem", "Fel\u00b7sen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00f6lbt den Saal zu Lust und Ruh,", "tokens": ["W\u00f6lbt", "den", "Saal", "zu", "Lust", "und", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Streckt den Thurm dem Himmel zu.", "tokens": ["Streckt", "den", "Thurm", "dem", "Him\u00b7mel", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "An dem h\u00f6chsten Fensterloch", "tokens": ["An", "dem", "h\u00f6chs\u00b7ten", "Fens\u00b7ter\u00b7loch"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Fehlt ein einz'ger Nagel noch,", "tokens": ["Fehlt", "ein", "einz'\u00b7ger", "Na\u00b7gel", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NE", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Schlosser zagend spricht:", "tokens": ["Und", "der", "Schlos\u00b7ser", "za\u00b7gend", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbda hinaus gelang' ich nicht!", "tokens": ["\u00bb", "da", "hin\u00b7aus", "ge\u00b7lang'", "ich", "nicht", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APZR", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Schad' ist's doch um das Geb\u00e4u,", "tokens": ["Schad'", "ist's", "doch", "um", "das", "Ge\u00b7b\u00e4u", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn es steht so frank und frei,", "tokens": ["Denn", "es", "steht", "so", "frank", "und", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn der Wandrer es beschaut,", "tokens": ["Wenn", "der", "Wand\u00b7rer", "es", "be\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Spricht: Es ist nicht ausgebaut.\u00ab", "tokens": ["Spricht", ":", "Es", "ist", "nicht", "aus\u00b7ge\u00b7baut", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$.", "PPER", "VAFIN", "PTKNEG", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Doch der Ries' im Augenblick", "tokens": ["Doch", "der", "Ries'", "im", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Nimmt den Knecht bei dem Genick,", "tokens": ["Nimmt", "den", "Knecht", "bei", "dem", "Ge\u00b7nick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Streckt zum Fenster den hinaus,", "tokens": ["Streckt", "zum", "Fens\u00b7ter", "den", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df es Allen ist ein Graus.", "tokens": ["Da\u00df", "es", "Al\u00b7len", "ist", "ein", "Graus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "\u00bbh\u00e4mmre! meine Hand ist fest,", "tokens": ["\u00bb", "h\u00e4mm\u00b7re", "!", "mei\u00b7ne", "Hand", "ist", "fest", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df sie dich nicht sinken l\u00e4\u00dft!", "tokens": ["Da\u00df", "sie", "dich", "nicht", "sin\u00b7ken", "l\u00e4\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlag den Nagel in den Stein", "tokens": ["Schlag", "den", "Na\u00b7gel", "in", "den", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NE", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zwischen Erd' und Himmel ein!\u00ab", "tokens": ["Zwi\u00b7schen", "Erd'", "und", "Him\u00b7mel", "ein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Drau\u00dfen h\u00e4ngt er so mit Schreck,", "tokens": ["Drau\u00b7\u00dfen", "h\u00e4ngt", "er", "so", "mit", "Schreck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch er wagts und h\u00e4mmert keck,", "tokens": ["Doch", "er", "wagts", "und", "h\u00e4m\u00b7mert", "keck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "ADJD", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Nieder l\u00e4\u00dft der Heim ihn sacht:", "tokens": ["Nie\u00b7der", "l\u00e4\u00dft", "der", "Heim", "ihn", "sacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbzwerg du hast es wohl gemacht!\u00ab", "tokens": ["\u00bb", "zwerg", "du", "hast", "es", "wohl", "ge\u00b7macht", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Schreitet aus dem hohen Saal", "tokens": ["Schrei\u00b7tet", "aus", "dem", "ho\u00b7hen", "Saal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00e4chtig \u00fcber Berg und Thal,", "tokens": ["M\u00e4ch\u00b7tig", "\u00fc\u00b7ber", "Berg", "und", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Langt aus seiner H\u00f6hle Thor", "tokens": ["Langt", "aus", "sei\u00b7ner", "H\u00f6h\u00b7le", "Thor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen goldnen Schatz hervor.", "tokens": ["Ei\u00b7nen", "gold\u00b7nen", "Schatz", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Auf dem hellen Heimenstein", "tokens": ["Auf", "dem", "hel\u00b7len", "Hei\u00b7men\u00b7stein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nehmen sie den Baulohn ein,", "tokens": ["Neh\u00b7men", "sie", "den", "Bau\u00b7lohn", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Maurer, Steinmetz, Zimmerknecht,", "tokens": ["Mau\u00b7rer", ",", "Stein\u00b7metz", ",", "Zim\u00b7mer\u00b7knecht", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedem widerf\u00e4hrt sein Recht.", "tokens": ["Je\u00b7dem", "wi\u00b7der\u00b7f\u00e4hrt", "sein", "Recht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Doch zum Schlosser spricht er: \u00bbSohn,", "tokens": ["Doch", "zum", "Schlos\u00b7ser", "spricht", "er", ":", "\u00bb", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "$.", "$(", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimm du hin den reichsten Lohn!", "tokens": ["Nimm", "du", "hin", "den", "reichs\u00b7ten", "Lohn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Halt dich an den Boden fest,", "tokens": ["Halt", "dich", "an", "den", "Bo\u00b7den", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4mmre gut dein Zwergennest!\u00ab", "tokens": ["H\u00e4mm\u00b7re", "gut", "dein", "Zwer\u00b7gen\u00b7nest", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}