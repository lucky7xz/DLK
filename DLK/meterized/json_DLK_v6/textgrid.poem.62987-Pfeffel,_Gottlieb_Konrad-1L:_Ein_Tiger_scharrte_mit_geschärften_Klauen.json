{"textgrid.poem.62987": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Tiger scharrte mit gesch\u00e4rften Klauen", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Tiger scharrte mit gesch\u00e4rften Klauen", "tokens": ["Ein", "Ti\u00b7ger", "scharr\u00b7te", "mit", "ge\u00b7sch\u00e4rf\u00b7ten", "Klau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Leichnam eines Bramen aus,", "tokens": ["Den", "Leich\u00b7nam", "ei\u00b7nes", "Bra\u00b7men", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem hinter seinem Palmenhaus", "tokens": ["Dem", "hin\u00b7ter", "sei\u00b7nem", "Pal\u00b7men\u00b7haus"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Sohn ein Grab in weichen Tuff gehauen.", "tokens": ["Sein", "Sohn", "ein", "Grab", "in", "wei\u00b7chen", "Tuff", "ge\u00b7hau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die\u00df sah der Redliche: mit bangem Grauen", "tokens": ["Die\u00df", "sah", "der", "Red\u00b7li\u00b7che", ":", "mit", "ban\u00b7gem", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Kam er herbey, vom schauerlichen Schmaus", "tokens": ["Kam", "er", "her\u00b7bey", ",", "vom", "schau\u00b7er\u00b7li\u00b7chen", "Schmaus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPER", "PTKVZ", "$,", "APPRART", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Durch frommes Flehn das Unthier abzuziehen.", "tokens": ["Durch", "from\u00b7mes", "Flehn", "das", "Un\u00b7thier", "ab\u00b7zu\u00b7zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "La\u00df, rief er, la\u00df das heilige Gebein!", "tokens": ["La\u00df", ",", "rief", "er", ",", "la\u00df", "das", "hei\u00b7li\u00b7ge", "Ge\u00b7bein", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VVFIN", "PPER", "$,", "VVIMP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Hier ist ein bessrer Raub. Des Tigers Augen gl\u00fchen;", "tokens": ["Hier", "ist", "ein", "bess\u00b7rer", "Raub", ".", "Des", "Ti\u00b7gers", "Au\u00b7gen", "gl\u00fc\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$.", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Er st\u00fcrzet auf den J\u00fcngling ein", "tokens": ["Er", "st\u00fcr\u00b7zet", "auf", "den", "J\u00fcng\u00b7ling", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und rei\u00dft das Herz, das Brama nun belohnet,", "tokens": ["Und", "rei\u00dft", "das", "Herz", ",", "das", "Bra\u00b7ma", "nun", "be\u00b7loh\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Ihm aus der Brust. Der B\u00f6sewicht!", "tokens": ["Ihm", "aus", "der", "Brust", ".", "Der", "B\u00f6\u00b7se\u00b7wicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Doch wer die Todten nicht verschonet,", "tokens": ["Doch", "wer", "die", "Tod\u00b7ten", "nicht", "ver\u00b7scho\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Verschont die Lebenden auch nicht.", "tokens": ["Ver\u00b7schont", "die", "Le\u00b7ben\u00b7den", "auch", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}