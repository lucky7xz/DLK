{"textgrid.poem.57234": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Feind ist da! Die Schlacht beginnt!", "genre": "verse", "period": "N.A.", "pub_year": 1749, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Feind ist da! Die Schlacht beginnt!", "tokens": ["Der", "Feind", "ist", "da", "!", "Die", "Schlacht", "be\u00b7ginnt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohlauf zum Sieg' herbey!", "tokens": ["Wohl\u00b7auf", "zum", "Sieg'", "her\u00b7bey", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es f\u00fchret uns der beste Mann", "tokens": ["Es", "f\u00fch\u00b7ret", "uns", "der", "bes\u00b7te", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im ganzen Vaterland!", "tokens": ["Im", "gan\u00b7zen", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Heut f\u00fchlet er die Krankheit nicht,", "tokens": ["Heut", "f\u00fch\u00b7let", "er", "die", "Krank\u00b7heit", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dort tragen sie ihn her!", "tokens": ["Dort", "tra\u00b7gen", "sie", "ihn", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Heil, Heinrich! heil dir Held und Mann", "tokens": ["Heil", ",", "Hein\u00b7rich", "!", "heil", "dir", "Held", "und", "Mann"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NE", "$.", "KOUS", "PPER", "NN", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Im eisernen Gefild", "tokens": ["Im", "ei\u00b7ser\u00b7nen", "Ge\u00b7fild"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+---+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Sein Antlitz gl\u00fcht vor Ehrbegier,", "tokens": ["Sein", "Ant\u00b7litz", "gl\u00fcht", "vor", "Ehr\u00b7be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und herscht den Sieg herbey!", "tokens": ["Und", "herscht", "den", "Sieg", "her\u00b7bey", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schon ist um ihn der Edlen Helm", "tokens": ["Schon", "ist", "um", "ihn", "der", "Ed\u00b7len", "Helm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Feindesblut bespritzt!", "tokens": ["Mit", "Fein\u00b7des\u00b7blut", "be\u00b7spritzt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Streu furchtbar Strahlen um dich her,", "tokens": ["Streu", "furcht\u00b7bar", "Strah\u00b7len", "um", "dich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Schwert in des Kaisers Hand,", "tokens": ["Schwert", "in", "des", "Kai\u00b7sers", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Dass alles t\u00f6dtliche Geschoss", "tokens": ["Dass", "al\u00b7les", "t\u00f6dt\u00b7li\u00b7che", "Ge\u00b7schoss"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Weg vor\u00fcbergeh!", "tokens": ["Den", "Weg", "vor\u00b7\u00fc\u00b7ber\u00b7geh", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Willkommen Tod f\u00fcrs Vaterland!", "tokens": ["Will\u00b7kom\u00b7men", "Tod", "f\u00fcrs", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn unser sinkend Haupt", "tokens": ["Wenn", "un\u00b7ser", "sin\u00b7kend", "Haupt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sch\u00f6n Blut bedeckt, dann sterben wir", "tokens": ["Sch\u00f6n", "Blut", "be\u00b7deckt", ",", "dann", "ster\u00b7ben", "wir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "VVPP", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Ruhm f\u00fcrs Vaterland!", "tokens": ["Mit", "Ruhm", "f\u00fcrs", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wenn vor uns wird ein ofnes Feld", "tokens": ["Wenn", "vor", "uns", "wird", "ein", "of\u00b7nes", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wir nur Todte sehn", "tokens": ["Und", "wir", "nur", "Tod\u00b7te", "sehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "NN", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weit um uns her, dann siegen wir", "tokens": ["Weit", "um", "uns", "her", ",", "dann", "sie\u00b7gen", "wir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Ruhm f\u00fcrs Vaterland!", "tokens": ["Mit", "Ruhm", "f\u00fcrs", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Dann treten wir mit hohem Schritt", "tokens": ["Dann", "tre\u00b7ten", "wir", "mit", "ho\u00b7hem", "Schritt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Leichnamen daher!", "tokens": ["Auf", "Leich\u00b7na\u00b7men", "da\u00b7her", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PAV", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dann jauchzen wir im Siegsgeschrey!", "tokens": ["Dann", "jauch\u00b7zen", "wir", "im", "Siegs\u00b7ge\u00b7schrey", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das geht durch Mark und Bein!", "tokens": ["Das", "geht", "durch", "Mark", "und", "Bein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Uns preist mit frohem Ungest\u00fcm", "tokens": ["Uns", "preist", "mit", "fro\u00b7hem", "Un\u00b7ge\u00b7st\u00fcm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Br\u00e4utgam und die Braut;", "tokens": ["Der", "Br\u00e4ut\u00b7gam", "und", "die", "Braut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er sieht die hohen Fahnen wehn,", "tokens": ["Er", "sieht", "die", "ho\u00b7hen", "Fah\u00b7nen", "wehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dr\u00fcckt ihr sanft die Hand,", "tokens": ["Und", "dr\u00fcckt", "ihr", "sanft", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und spricht zu ihr: Da kommen sie,", "tokens": ["Und", "spricht", "zu", "ihr", ":", "Da", "kom\u00b7men", "sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "$.", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Kriegesg\u00f6tter, her!", "tokens": ["Die", "Krie\u00b7ges\u00b7g\u00f6t\u00b7ter", ",", "her", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie stritten in der heissen Schlacht", "tokens": ["Sie", "strit\u00b7ten", "in", "der", "heis\u00b7sen", "Schlacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch f\u00fcr uns beide mit!", "tokens": ["Auch", "f\u00fcr", "uns", "bei\u00b7de", "mit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "PIS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Uns preist der Freudenthr\u00e4nen voll", "tokens": ["Uns", "preist", "der", "Freu\u00b7dent\u00b7hr\u00e4\u00b7nen", "voll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Mutter, und ihr Kind!", "tokens": ["Die", "Mut\u00b7ter", ",", "und", "ihr", "Kind", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie dr\u00fcckt den Knaben an ihr Herz,", "tokens": ["Sie", "dr\u00fcckt", "den", "Kna\u00b7ben", "an", "ihr", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sieht dem Kaiser nach.", "tokens": ["Und", "sieht", "dem", "Kai\u00b7ser", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Uns folgt ein Ruhm, der ewig bleibt,", "tokens": ["Uns", "folgt", "ein", "Ruhm", ",", "der", "e\u00b7wig", "bleibt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn wir gestorben sind,", "tokens": ["Wenn", "wir", "ge\u00b7stor\u00b7ben", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gestorben f\u00fcr das Vaterland", "tokens": ["Ge\u00b7stor\u00b7ben", "f\u00fcr", "das", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den ehrenvollen Tod!", "tokens": ["Den", "eh\u00b7ren\u00b7vol\u00b7len", "Tod", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}