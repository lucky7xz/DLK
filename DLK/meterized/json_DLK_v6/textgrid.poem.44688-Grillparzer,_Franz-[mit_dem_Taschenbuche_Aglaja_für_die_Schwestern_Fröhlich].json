{"textgrid.poem.44688": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[mit dem Taschenbuche Aglaja f\u00fcr die Schwestern Fr\u00f6hlich]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aglaja nennt man mich; mit also heidnischem Namen", "tokens": ["Ag\u00b7la\u00b7ja", "nennt", "man", "mich", ";", "mit", "al\u00b7so", "heid\u00b7ni\u00b7schem", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PIS", "PRF", "$.", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Pa\u00dft freilich man nicht gut zu einem Weihnachtfest,", "tokens": ["Pa\u00dft", "frei\u00b7lich", "man", "nicht", "gut", "zu", "ei\u00b7nem", "Weih\u00b7nacht\u00b7fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "PTKNEG", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Am wenigsten zu einem, gefeiert von Damen;", "tokens": ["Am", "we\u00b7nigs\u00b7ten", "zu", "ei\u00b7nem", ",", "ge\u00b7fei\u00b7ert", "von", "Da\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "PIS", "APPR", "ART", "$,", "VVPP", "APPR", "NN", "$."], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Denn h\u00e4lt bei Herren gleich von Vielg\u00f6tterei noch ein Rest,", "tokens": ["Denn", "h\u00e4lt", "bei", "Her\u00b7ren", "gleich", "von", "Viel\u00b7g\u00f6t\u00b7te\u00b7rei", "noch", "ein", "Rest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ADV", "APPR", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "So wei\u00df man doch, da\u00df Damen gar christlich gesinnt", "tokens": ["So", "wei\u00df", "man", "doch", ",", "da\u00df", "Da\u00b7men", "gar", "christ\u00b7lich", "ge\u00b7sinnt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$,", "KOUS", "NN", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "F\u00fcr ", "tokens": ["F\u00fcr"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Und dennoch tret ich ein voll Zuversicht,", "tokens": ["Und", "den\u00b7noch", "tret", "ich", "ein", "voll", "Zu\u00b7ver\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Denn, Sch\u00f6nheit singend und der Liebe holde Weise,", "tokens": ["Denn", ",", "Sch\u00f6n\u00b7heit", "sin\u00b7gend", "und", "der", "Lie\u00b7be", "hol\u00b7de", "Wei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "ADJD", "KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Fehlt sicher an Verehrern nicht", "tokens": ["Fehlt", "si\u00b7cher", "an", "Ver\u00b7eh\u00b7rern", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und nicht an Stoff es mir in eurem Kreise.", "tokens": ["Und", "nicht", "an", "Stoff", "es", "mir", "in", "eu\u00b7rem", "Krei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}