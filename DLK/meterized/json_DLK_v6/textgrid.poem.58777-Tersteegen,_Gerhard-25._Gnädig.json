{"textgrid.poem.58777": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "25. Gn\u00e4dig", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Gn\u00e4digstfreundlicher uns liebst,", "tokens": ["Du", "Gn\u00e4\u00b7digst\u00b7freund\u00b7li\u00b7cher", "uns", "liebst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Uns gern f\u00fcr Recht nur Gnade gibst;", "tokens": ["Uns", "gern", "f\u00fcr", "Recht", "nur", "Gna\u00b7de", "gibst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch Gnade bin ich tief gezogen,", "tokens": ["Durch", "Gna\u00b7de", "bin", "ich", "tief", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur Gnade tilget Schuld und S\u00fcnd',", "tokens": ["Nur", "Gna\u00b7de", "til\u00b7get", "Schuld", "und", "S\u00fcnd'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durch sie bin ich dein Gnadenkind.", "tokens": ["Durch", "sie", "bin", "ich", "dein", "Gna\u00b7den\u00b7kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Dein Herz bleibt unverdient gewogen;", "tokens": ["Dein", "Herz", "bleibt", "un\u00b7ver\u00b7di\u00b7ent", "ge\u00b7wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ich ess' und trink' nur Gnade hier,", "tokens": ["Ich", "ess'", "und", "trink'", "nur", "Gna\u00b7de", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von Gnade sing' ich ewig dir.", "tokens": ["Von", "Gna\u00b7de", "sing'", "ich", "e\u00b7wig", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJD", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Halleluja! Halleluja!", "tokens": ["Hal\u00b7le\u00b7lu\u00b7ja", "!", "Hal\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "ITJ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}