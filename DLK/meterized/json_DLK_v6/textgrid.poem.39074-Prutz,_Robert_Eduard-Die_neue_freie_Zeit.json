{"textgrid.poem.39074": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Die neue freie Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun freuet euch, ihr Frommen,", "tokens": ["Nun", "freu\u00b7et", "euch", ",", "ihr", "From\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "nun ist der Tag gekommen,", "tokens": ["nun", "ist", "der", "Tag", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "den ihr so lange erharrt:", "tokens": ["den", "ihr", "so", "lan\u00b7ge", "er\u00b7harrt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Durch Beten und durch Glauben,", "tokens": ["Durch", "Be\u00b7ten", "und", "durch", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "nun wird man r\u00fcckw\u00e4rts schrauben", "tokens": ["nun", "wird", "man", "r\u00fcck\u00b7w\u00e4rts", "schrau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "die schn\u00f6de Gegenwart.", "tokens": ["die", "schn\u00f6\u00b7de", "Ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Trotz Widerspruch und Schnarchen,", "tokens": ["Trotz", "Wi\u00b7der\u00b7spruch", "und", "Schnar\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "die Zeit der Patriarchen,", "tokens": ["die", "Zeit", "der", "Pat\u00b7ri\u00b7ar\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "schon kehrt sie uns zur\u00fcck:", "tokens": ["schon", "kehrt", "sie", "uns", "zu\u00b7r\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Modern sind wir gewesen,", "tokens": ["Mo\u00b7dern", "sind", "wir", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VAPP", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "nun werden wir Chinesen \u2013", "tokens": ["nun", "wer\u00b7den", "wir", "Chi\u00b7ne\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$("], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.6": {"text": "Chinesen, welch ein Gl\u00fcck!", "tokens": ["Chi\u00b7ne\u00b7sen", ",", "welch", "ein", "Gl\u00fcck", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAT", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "O China, Reich der Sitte,", "tokens": ["O", "Chi\u00b7na", ",", "Reich", "der", "Sit\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Reich der gerechten Mitte,", "tokens": ["Reich", "der", "ge\u00b7rech\u00b7ten", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "du Reich der Majest\u00e4t:", "tokens": ["du", "Reich", "der", "Ma\u00b7jes\u00b7t\u00e4t", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wo niemand braucht zu sorgen,", "tokens": ["Wo", "nie\u00b7mand", "braucht", "zu", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "wo alles heut wie morgen,", "tokens": ["wo", "al\u00b7les", "heut", "wie", "mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "KOKOM", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "in ew'gen Gleisen geht!", "tokens": ["in", "ew'\u00b7gen", "Glei\u00b7sen", "geht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dein Beispiel soll uns lehren,", "tokens": ["Dein", "Bei\u00b7spiel", "soll", "uns", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "zur Einfalt zu bekehren", "tokens": ["zur", "Ein\u00b7falt", "zu", "be\u00b7keh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "das s\u00fcndige Geschlecht:", "tokens": ["das", "s\u00fcn\u00b7di\u00b7ge", "Ge\u00b7schlecht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nun scheren wir die K\u00f6pfe,", "tokens": ["Nun", "sche\u00b7ren", "wir", "die", "K\u00f6p\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "nun salben wir die Z\u00f6pfe,", "tokens": ["nun", "sal\u00b7ben", "wir", "die", "Z\u00f6p\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "der dickste Zopf hat recht!", "tokens": ["der", "dicks\u00b7te", "Zopf", "hat", "recht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nun m\u00fc\u00dft ihr schweigend sitzen", "tokens": ["Nun", "m\u00fc\u00dft", "ihr", "schwei\u00b7gend", "sit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und auf die Nasenspitzen", "tokens": ["und", "auf", "die", "Na\u00b7sen\u00b7spit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "in stiller Andacht sehn:", "tokens": ["in", "stil\u00b7ler", "An\u00b7dacht", "sehn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So wird die Menge preisend", "tokens": ["So", "wird", "die", "Men\u00b7ge", "prei\u00b7send"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "und mit dem Finger weisend", "tokens": ["und", "mit", "dem", "Fin\u00b7ger", "wei\u00b7send"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "euch demutvoll umstehn.", "tokens": ["euch", "de\u00b7mut\u00b7voll", "um\u00b7stehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Nun gegen Strau\u00df und Bauer,", "tokens": ["Nun", "ge\u00b7gen", "Strau\u00df", "und", "Bau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "nun baut man eine Mauer", "tokens": ["nun", "baut", "man", "ei\u00b7ne", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "rings um das Reich herum:", "tokens": ["rings", "um", "das", "Reich", "he\u00b7rum", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "drauf stehn mit stolzen Mienen", "tokens": ["drauf", "stehn", "mit", "stol\u00b7zen", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "die Herren Mandarinen", "tokens": ["die", "Her\u00b7ren", "Man\u00b7da\u00b7ri\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "und nicken und \u2013 sind stumm.", "tokens": ["und", "ni\u00b7cken", "und", "\u2013", "sind", "stumm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVINF", "KON", "$(", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Das Schreiben und das Sprechen", "tokens": ["Das", "Schrei\u00b7ben", "und", "das", "Spre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "das gilt nun als Verbrechen,", "tokens": ["das", "gilt", "nun", "als", "Ver\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "KOUS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "denn nur der Kaiser spricht!", "tokens": ["denn", "nur", "der", "Kai\u00b7ser", "spricht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nun, m\u00e4chtiger und weiser", "tokens": ["Nun", ",", "m\u00e4ch\u00b7ti\u00b7ger", "und", "wei\u00b7ser"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "ADJA", "KON", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "als unser Herr, der Kaiser,", "tokens": ["als", "un\u00b7ser", "Herr", ",", "der", "Kai\u00b7ser", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "ist selbst der Herrgott nicht.", "tokens": ["ist", "selbst", "der", "Herr\u00b7gott", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und will das Fleisch sich regen,", "tokens": ["Und", "will", "das", "Fleisch", "sich", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PRF", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und fragen wir, weswegen?", "tokens": ["und", "fra\u00b7gen", "wir", ",", "wes\u00b7we\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O dann dem Kaiser Preis:", "tokens": ["O", "dann", "dem", "Kai\u00b7ser", "Preis", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dann kriegen wir als Kinder,", "tokens": ["Dann", "krie\u00b7gen", "wir", "als", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "bald st\u00e4rker, bald gelinder,", "tokens": ["bald", "st\u00e4r\u00b7ker", ",", "bald", "ge\u00b7lin\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "die Rute auf den Stei\u00df.", "tokens": ["die", "Ru\u00b7te", "auf", "den", "Stei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "So bilden wir mit Ehren", "tokens": ["So", "bil\u00b7den", "wir", "mit", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "als ob wir's selber w\u00e4ren,", "tokens": ["als", "ob", "wir's", "sel\u00b7ber", "w\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PIS", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "den Mittelpunkt der Welt!", "tokens": ["den", "Mit\u00b7tel\u00b7punkt", "der", "Welt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Was schert in unsrer Glorie,", "tokens": ["Was", "schert", "in", "uns\u00b7rer", "Glo\u00b7rie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "was schert uns die Historie,", "tokens": ["was", "schert", "uns", "die", "His\u00b7to\u00b7rie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "wenn's nur zusammenh\u00e4lt?", "tokens": ["wenn's", "nur", "zu\u00b7sam\u00b7men\u00b7h\u00e4lt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Drum immer frisch geschoben,", "tokens": ["Drum", "im\u00b7mer", "frisch", "ge\u00b7scho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "gehoben und geschroben,", "tokens": ["ge\u00b7ho\u00b7ben", "und", "ge\u00b7schro\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "nach China frisch herum!", "tokens": ["nach", "Chi\u00b7na", "frisch", "he\u00b7rum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJD", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Doch wollt ihr's recht vollenden,", "tokens": ["Doch", "wollt", "ih\u00b7r's", "recht", "voll\u00b7en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "o dann mit gn\u00e4d'gen H\u00e4nden,", "tokens": ["o", "dann", "mit", "gn\u00e4d'\u00b7gen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.6": {"text": "o gebt uns Opium!", "tokens": ["o", "gebt", "uns", "O\u00b7pium", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}