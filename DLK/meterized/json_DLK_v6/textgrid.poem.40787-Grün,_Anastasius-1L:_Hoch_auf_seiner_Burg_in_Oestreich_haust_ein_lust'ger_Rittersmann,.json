{"textgrid.poem.40787": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hoch auf seiner Burg in Oestreich haust ein lust'ger Rittersmann,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoch auf seiner Burg in Oestreich haust ein lust'ger Rittersmann,", "tokens": ["Hoch", "auf", "sei\u00b7ner", "Burg", "in", "O\u00b7e\u00b7streich", "haust", "ein", "lust'\u00b7ger", "Rit\u00b7ters\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "APPR", "NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Hold des frommen Manns Lutheri neuen Lehren zugethan,", "tokens": ["Hold", "des", "from\u00b7men", "Manns", "Lu\u00b7the\u00b7ri", "neu\u00b7en", "Leh\u00b7ren", "zu\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "NE", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Die aus dumpfen Klostermauern frei und leuchtend einst entstiegen,", "tokens": ["Die", "aus", "dum\u00b7pfen", "Klos\u00b7ter\u00b7mau\u00b7ern", "frei", "und", "leuch\u00b7tend", "einst", "ent\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "ADJD", "KON", "ADJD", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wie aus schwarzen Felsgekl\u00fcften Schaaren wei\u00dfer Tauben fliegen.", "tokens": ["Wie", "aus", "schwar\u00b7zen", "Fels\u00b7ge\u00b7kl\u00fcf\u00b7ten", "Schaa\u00b7ren", "wei\u00b7\u00dfer", "Tau\u00b7ben", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Und sie flogen bald auch siegreich \u00fcber Oestreichs Fluren hin,", "tokens": ["Und", "sie", "flo\u00b7gen", "bald", "auch", "sieg\u00b7reich", "\u00fc\u00b7ber", "O\u00b7e\u00b7streichs", "Flu\u00b7ren", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV", "ADJD", "APPR", "NE", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Die Verwegnen sah mit Z\u00fcrnen Kaiser Ferdinandus ziehn,", "tokens": ["Die", "Ver\u00b7weg\u00b7nen", "sah", "mit", "Z\u00fcr\u00b7nen", "Kai\u00b7ser", "Fer\u00b7di\u00b7nan\u00b7dus", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "NE", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Und Edikte lie\u00df zermalmend \u00fcber sie vom Thron er fallen,", "tokens": ["Und", "E\u00b7dik\u00b7te", "lie\u00df", "zer\u00b7mal\u00b7mend", "\u00fc\u00b7ber", "sie", "vom", "Thron", "er", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "VVPP", "APPR", "PPER", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wie von hohen Felsenhorsten Geier mit den scharfen Krallen.", "tokens": ["Wie", "von", "ho\u00b7hen", "Fel\u00b7sen\u00b7hors\u00b7ten", "Gei\u00b7er", "mit", "den", "schar\u00b7fen", "Kral\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Sonntags fr\u00fch, als die Gemeinde Glockenklang zur Kirche ruft,", "tokens": ["Sonn\u00b7tags", "fr\u00fch", ",", "als", "die", "Ge\u00b7mein\u00b7de", "Glo\u00b7cken\u00b7klang", "zur", "Kir\u00b7che", "ruft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "ART", "NN", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wallt im gr\u00fcnen Forst der Ritter, freuend sich an Laub und Duft:", "tokens": ["Wallt", "im", "gr\u00fc\u00b7nen", "Forst", "der", "Rit\u00b7ter", ",", "freu\u00b7end", "sich", "an", "Laub", "und", "Duft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "$,", "ADJD", "PRF", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "\u00bbwer den Herrn nicht kann im Walde, kann ihn auch im Dom nicht ehren,", "tokens": ["\u00bb", "wer", "den", "Herrn", "nicht", "kann", "im", "Wal\u00b7de", ",", "kann", "ihn", "auch", "im", "Dom", "nicht", "eh\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ART", "NN", "PTKNEG", "VMFIN", "APPRART", "NN", "$,", "VMFIN", "PPER", "ADV", "APPRART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und wen nicht die frommen Blumen, wird kein Pf\u00e4fflein auch bekehren.\u00ab", "tokens": ["Und", "wen", "nicht", "die", "from\u00b7men", "Blu\u00b7men", ",", "wird", "kein", "Pf\u00e4f\u00b7flein", "auch", "be\u00b7keh\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PWS", "PTKNEG", "ART", "ADJA", "NN", "$,", "VAFIN", "PIAT", "NN", "ADV", "VVINF", "$.", "$("], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}}, "stanza.4": {"line.1": {"text": "Sieh, da rauscht' aus Busch und Dickicht stolz ein Edelhirsch empor,", "tokens": ["Sieh", ",", "da", "rauscht'", "aus", "Busch", "und", "Di\u00b7ckicht", "stolz", "ein", "E\u00b7del\u00b7hirsch", "em\u00b7por", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "APPR", "NN", "KON", "NN", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Doch es streckte schnell zu Boden ihn des Ritters Feuerrohr:", "tokens": ["Doch", "es", "streck\u00b7te", "schnell", "zu", "Bo\u00b7den", "ihn", "des", "Rit\u00b7ters", "Feu\u00b7er\u00b7rohr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "APPR", "NN", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "\u00bbwer da zu Mittag des Sonntags seinen Braten will genie\u00dfen,", "tokens": ["\u00bb", "wer", "da", "zu", "Mit\u00b7tag", "des", "Sonn\u00b7tags", "sei\u00b7nen", "Bra\u00b7ten", "will", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ADV", "APPR", "NN", "ART", "NN", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Ei, der wird dazu das Wildpret doch wohl auch sich d\u00fcrfen schie\u00dfen.\u00ab", "tokens": ["Ei", ",", "der", "wird", "da\u00b7zu", "das", "Wild\u00b7pret", "doch", "wohl", "auch", "sich", "d\u00fcr\u00b7fen", "schie\u00b7\u00dfen", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PRELS", "VAFIN", "PAV", "ART", "NN", "ADV", "ADV", "ADV", "PRF", "VMFIN", "VVINF", "$.", "$("], "meter": "+-+-+-+--+--+-+-", "measure": "trochaic.septa.relaxed"}}, "stanza.5": {"line.1": {"text": "Als der Ritter kehrt zum Schlosse, steht der Pfarrer vor dem Thor,", "tokens": ["Als", "der", "Rit\u00b7ter", "kehrt", "zum", "Schlos\u00b7se", ",", "steht", "der", "Pfar\u00b7rer", "vor", "dem", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPRART", "NN", "$,", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Stolz, wie im Triumphe, haltend hoch ein Pergament empor:", "tokens": ["Stolz", ",", "wie", "im", "Tri\u00b7um\u00b7phe", ",", "hal\u00b7tend", "hoch", "ein", "Per\u00b7ga\u00b7ment", "em\u00b7por", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "APPRART", "NN", "$,", "ADJD", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "\u00bbwer des Sonntags, statt der Messe, Feld- und Waidwerks sich beflissen,", "tokens": ["\u00bb", "wer", "des", "Sonn\u00b7tags", ",", "statt", "der", "Mes\u00b7se", ",", "Feld", "und", "Waid\u00b7werks", "sich", "be\u00b7flis\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ART", "NN", "$,", "KOUI", "ART", "NN", "$,", "TRUNC", "KON", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Soll's mit hundert Golddukaten in den Schatz des Kaisers b\u00fc\u00dfen!", "tokens": ["Soll's", "mit", "hun\u00b7dert", "Gold\u00b7du\u00b7ka\u00b7ten", "in", "den", "Schatz", "des", "Kai\u00b7sers", "b\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "CARD", "NN", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "W\u00e4hrend ihr in W\u00e4ldern Hirsche, oder B\u00f6cke schie\u00dft vielmehr,", "tokens": ["W\u00e4h\u00b7rend", "ihr", "in", "W\u00e4l\u00b7dern", "Hir\u00b7sche", ",", "o\u00b7der", "B\u00f6\u00b7cke", "schie\u00dft", "viel\u00b7mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "NN", "$,", "KON", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ward verk\u00fcndet von der Kanzel die\u00df Edikt so inhaltschwer.", "tokens": ["Ward", "ver\u00b7k\u00fcn\u00b7det", "von", "der", "Kan\u00b7zel", "die\u00df", "E\u00b7dikt", "so", "in\u00b7halt\u00b7schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "ART", "NN", "PDS", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "M\u00f6gt verzeihen, edler Ritter, wenn ich's euch bedauernd sage,", "tokens": ["M\u00f6gt", "ver\u00b7zei\u00b7hen", ",", "ed\u00b7ler", "Rit\u00b7ter", ",", "wenn", "ich's", "euch", "be\u00b7dau\u00b7ernd", "sa\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "ADJA", "NN", "$,", "KOUS", "PIS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Da\u00df das Me\u00df- und Predigtschw\u00e4nzen selten goldne Fr\u00fcchte trage!\u00ab", "tokens": ["Da\u00df", "das", "Me\u00df", "und", "Pre\u00b7digt\u00b7schw\u00e4n\u00b7zen", "sel\u00b7ten", "gold\u00b7ne", "Fr\u00fcch\u00b7te", "tra\u00b7ge", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "TRUNC", "KON", "NN", "ADJD", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "\u00bbdie\u00dfmal,\u00ab sprach der Ritter l\u00e4chelnd, \u00bbtrug's doch Gold, wenn auch nicht mir!", "tokens": ["\u00bb", "die\u00df\u00b7mal", ",", "\u00ab", "sprach", "der", "Rit\u00b7ter", "l\u00e4\u00b7chelnd", ",", "\u00bb", "trug's", "doch", "Gold", ",", "wenn", "auch", "nicht", "mir", "!"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "$(", "VVFIN", "ART", "NN", "ADJD", "$,", "$(", "VVFIN", "ADV", "NN", "$,", "KOUS", "ADV", "PTKNEG", "PPER", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Doch mir bleibt die Haut des Hirsches: im Edikt steht nichts von ihr!", "tokens": ["Doch", "mir", "bleibt", "die", "Haut", "des", "Hir\u00b7sches", ":", "im", "E\u00b7dikt", "steht", "nichts", "von", "ihr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$.", "APPRART", "NN", "VVFIN", "PIS", "APPR", "PPER", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Heil dem \u00fcbergn\u00e4d'gen Kaiser, der uns doch die Haut will lassen!", "tokens": ["Heil", "dem", "\u00fc\u00b7ber\u00b7gn\u00e4d'\u00b7gen", "Kai\u00b7ser", ",", "der", "uns", "doch", "die", "Haut", "will", "las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Seht, vielleicht zu einem Wamse oder sonst was kann sie passen!\u00ab \u2013 \u2013", "tokens": ["Seht", ",", "viel\u00b7leicht", "zu", "ei\u00b7nem", "Wam\u00b7se", "o\u00b7der", "sonst", "was", "kann", "sie", "pas\u00b7sen", "!", "\u00ab", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["VVFIN", "$,", "ADV", "APPR", "ART", "NN", "KON", "ADV", "PWS", "VMFIN", "PPER", "VVINF", "$.", "$(", "$(", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.8": {"line.1": {"text": "Einst nach Jahren, als der Kaiser heim von ernster Fahrt gekehrt,", "tokens": ["Einst", "nach", "Jah\u00b7ren", ",", "als", "der", "Kai\u00b7ser", "heim", "von", "erns\u00b7ter", "Fahrt", "ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "KOUS", "ART", "NN", "PTKVZ", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Lud er vor den Thron zu Hofe seine Edlen, treu und werth:", "tokens": ["Lud", "er", "vor", "den", "Thron", "zu", "Ho\u00b7fe", "sei\u00b7ne", "Ed\u00b7len", ",", "treu", "und", "werth", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ART", "NN", "APPR", "NN", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Jeder m\u00f6g' in seinem Kleide dann des Landes Farben f\u00fchren,", "tokens": ["Je\u00b7der", "m\u00f6g'", "in", "sei\u00b7nem", "Klei\u00b7de", "dann", "des", "Lan\u00b7des", "Far\u00b7ben", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PPOSAT", "NN", "ADV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Oder sonst mit seinem sch\u00f6nsten, k\u00f6stlichsten Gewand sich zieren!", "tokens": ["O\u00b7der", "sonst", "mit", "sei\u00b7nem", "sch\u00f6ns\u00b7ten", ",", "k\u00f6st\u00b7lichs\u00b7ten", "Ge\u00b7wand", "sich", "zie\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+---+-+-", "measure": "unknown.measure.septa"}}, "stanza.9": {"line.1": {"text": "In dem Kaisersaale wimmelt's von Gew\u00e4ndern, roth und wei\u00df,", "tokens": ["In", "dem", "Kai\u00b7ser\u00b7saa\u00b7le", "wim\u00b7melt's", "von", "Ge\u00b7w\u00e4n\u00b7dern", ",", "roth", "und", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPR", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Sammt und Perlen, Gold und Demant gl\u00fchn und strahlen rings im Kreis,", "tokens": ["Sammt", "und", "Per\u00b7len", ",", "Gold", "und", "De\u00b7mant", "gl\u00fchn", "und", "strah\u00b7len", "r