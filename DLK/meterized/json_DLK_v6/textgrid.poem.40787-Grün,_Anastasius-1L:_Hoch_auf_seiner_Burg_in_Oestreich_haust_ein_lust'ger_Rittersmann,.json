m,", "tokens": ["Wack\u00b7rer", "Rit\u00b7ter", ",", "aus", "dem", "Him\u00b7mel", "blickst", "du", "nun", "auf", "ird'\u00b7schen", "Kram", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wo so g\u00e4nzlich aus der Mode deine Lederhose kam,", "tokens": ["Wo", "so", "g\u00e4nz\u00b7lich", "aus", "der", "Mo\u00b7de", "dei\u00b7ne", "Le\u00b7der\u00b7ho\u00b7se", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wo in Seid' und Sammt wir prunken! \u2013 L\u00e4chelnd doch siehst du die Gecken", "tokens": ["Wo", "in", "Seid'", "und", "Sammt", "wir", "prun\u00b7ken", "!", "\u2013", "L\u00e4\u00b7chelnd", "doch", "siehst", "du", "die", "Ge\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NE", "KON", "VVFIN", "PPER", "VVINF", "$.", "$(", "ADJD", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "--+-+-+-+-++--+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Unbewu\u00dft, bis an den Kragen, tief in Lederhosen stecken.", "tokens": ["Un\u00b7be\u00b7wu\u00dft", ",", "bis", "an", "den", "Kra\u00b7gen", ",", "tief", "in", "Le\u00b7der\u00b7ho\u00b7sen", "ste\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "APPR", "ART", "NN", "$,", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}