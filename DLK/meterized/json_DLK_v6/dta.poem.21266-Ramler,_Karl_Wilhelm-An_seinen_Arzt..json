{"dta.poem.21266": {"metadata": {"author": {"name": "Ramler, Karl Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An seinen Arzt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20476-3", "language": ["de:0.99"], "booktitle": "Ramler, Karl Wilhelm: Oden. Berlin, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Arzt, mein Freund, o! lass mich\nihn entsiegeln,", "tokens": ["Mein", "Arzt", ",", "mein", "Freund", ",", "o", "!", "lass", "mich", "ihn", "ent\u00b7sie\u00b7geln", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "FM", "$.", "VVFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Hochheims edle Kelter zwang,", "tokens": ["Den", "Hoch\u00b7heims", "ed\u00b7le", "Kel\u00b7ter", "zwang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und jenen, alt als ich, der einst auf Tar-", "tokens": ["Und", "je\u00b7nen", ",", "alt", "als", "ich", ",", "der", "einst", "auf", "Ta\u00b7r"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PDS", "$,", "ADJD", "KOKOM", "PPER", "$,", "PRELS", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Morgensonne trank!", "tokens": ["Die", "Mor\u00b7gen\u00b7son\u00b7ne", "trank", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dass ich diess thrazisch kalte Fieber", "tokens": ["Dass", "ich", "diess", "thra\u00b7zisch", "kal\u00b7te", "Fie\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDS", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das um mein Eingeweide schleicht,", "tokens": ["Das", "um", "mein", "Ein\u00b7ge\u00b7wei\u00b7de", "schleicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "(denn ", "tokens": ["(", "denn"], "token_info": ["punct", "word"], "pos": ["$(", "KON"], "meter": "-", "measure": "single.down"}}, "stanza.3": {"line.1": {"text": "Heut seiner Jahre Mittag, den Phalangen", "tokens": ["Heut", "sei\u00b7ner", "Jah\u00b7re", "Mit\u00b7tag", ",", "den", "Pha\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "NN", "$,", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Europens nicht, auch nicht der Wut", "tokens": ["Eu\u00b7ro\u00b7pens", "nicht", ",", "auch", "nicht", "der", "Wut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PTKNEG", "$,", "ADV", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Horden Asiens bezwinglich, noch den", "tokens": ["Der", "Hor\u00b7den", "A\u00b7siens", "be\u00b7zwing\u00b7lich", ",", "noch", "den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,", "ADV", "ART"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Eumenidenbrut;)", "tokens": ["Der", "Eu\u00b7me\u00b7ni\u00b7den\u00b7brut", ";)"], "token_info": ["word", "word", "emoticon"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und trunkne Jubel jauchze, dass von", "tokens": ["Und", "trunk\u00b7ne", "Ju\u00b7bel", "jauch\u00b7ze", ",", "dass", "von"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$,", "KOUS", "APPR"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Feindinnen nur Theresia", "tokens": ["Fein\u00b7din\u00b7nen", "nur", "The\u00b7re\u00b7sia"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "NE"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Noch trotzen darf; dass Tanaquil j\u00fcngsthin", "tokens": ["Noch", "trot\u00b7zen", "darf", ";", "dass", "Ta\u00b7na\u00b7quil", "j\u00fcng\u00b7sthin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVINF", "VMFIN", "$.", "KOUS", "NE", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und nun Kleopatra.", "tokens": ["Und", "nun", "Kleo\u00b7pat\u00b7ra", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}