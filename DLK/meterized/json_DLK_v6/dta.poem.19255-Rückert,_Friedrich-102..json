{"dta.poem.19255": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "102.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vom Abhang rollt ein Stein in jedem Nu hernieder,", "tokens": ["Vom", "Ab\u00b7hang", "rollt", "ein", "Stein", "in", "je\u00b7dem", "Nu", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "APPR", "PIS", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von allen aber kommt zur H\u00f6he keiner wieder.", "tokens": ["Von", "al\u00b7len", "a\u00b7ber", "kommt", "zur", "H\u00f6\u00b7he", "kei\u00b7ner", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "VVFIN", "APPRART", "NN", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So m\u00fc\u00dfen nach und nach die Th\u00e4ler h\u00f6her werden,", "tokens": ["So", "m\u00fc\u00b7\u00dfen", "nach", "und", "nach", "die", "Th\u00e4\u00b7ler", "h\u00f6\u00b7her", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "KON", "APPR", "ART", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Berge niedriger und alles flach auf Erden.", "tokens": ["Die", "Ber\u00b7ge", "nied\u00b7ri\u00b7ger", "und", "al\u00b7les", "flach", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "PIS", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch scheinen innerlich die Berge nachzusprie\u00dfen,", "tokens": ["Doch", "schei\u00b7nen", "in\u00b7ner\u00b7lich", "die", "Ber\u00b7ge", "nach\u00b7zu\u00b7sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Alswie die Wasser, die aus ihnen sich ergie\u00dfen.", "tokens": ["Als\u00b7wie", "die", "Was\u00b7ser", ",", "die", "aus", "ih\u00b7nen", "sich", "er\u00b7gie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und ewig bleibt die Welt in ihrem Gleichgewicht;", "tokens": ["Und", "e\u00b7wig", "bleibt", "die", "Welt", "in", "ih\u00b7rem", "Gleich\u00b7ge\u00b7wicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du f\u00fchle, wer sie h\u00e4lt, und zittre f\u00fcr sie nicht!", "tokens": ["Du", "f\u00fch\u00b7le", ",", "wer", "sie", "h\u00e4lt", ",", "und", "zitt\u00b7re", "f\u00fcr", "sie", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "KON", "VVFIN", "APPR", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}