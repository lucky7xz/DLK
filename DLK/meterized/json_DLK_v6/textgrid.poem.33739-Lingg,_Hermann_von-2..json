{"textgrid.poem.33739": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Wille, Schuld und S\u00fchne sind verbunden,", "tokens": ["Wie", "Wil\u00b7le", ",", "Schuld", "und", "S\u00fch\u00b7ne", "sind", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "NN", "KON", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lag vor dem Blick des allgewalt'gen Dichters,", "tokens": ["Lag", "vor", "dem", "Blick", "des", "all\u00b7ge\u00b7walt'\u00b7gen", "Dich\u00b7ters", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der mit dem Scharfblick eines Seelenrichters", "tokens": ["Der", "mit", "dem", "Scharf\u00b7blick", "ei\u00b7nes", "See\u00b7len\u00b7rich\u00b7ters"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Der Menschheit Herz gekannt und mitempfunden.", "tokens": ["Der", "Menschheit", "Herz", "ge\u00b7kannt", "und", "mi\u00b7temp\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "KON", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wer hat, wie er, geschaut die tiefsten Wunden,", "tokens": ["Wer", "hat", ",", "wie", "er", ",", "ge\u00b7schaut", "die", "tiefs\u00b7ten", "Wun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PWAV", "PPER", "$,", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Wahn und D\u00fcnkel des Alltagsgelichters", "tokens": ["Den", "Wahn", "und", "D\u00fcn\u00b7kel", "des", "All\u00b7tags\u00b7ge\u00b7lich\u00b7ters"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Zerschmettert mit dem Spotte des Vernichters,", "tokens": ["Zer\u00b7schmet\u00b7tert", "mit", "dem", "Spot\u00b7te", "des", "Ver\u00b7nich\u00b7ters", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcrs H\u00f6chste, wie f\u00fcrs Zartste Wort gefunden?", "tokens": ["F\u00fcrs", "H\u00f6chs\u00b7te", ",", "wie", "f\u00fcrs", "Zarts\u00b7te", "Wort", "ge\u00b7fun\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "$,", "PWAV", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zum Dasein rief voll sch\u00f6pfungsreicher F\u00fclle", "tokens": ["Zum", "Da\u00b7sein", "rief", "voll", "sch\u00f6p\u00b7fungs\u00b7rei\u00b7cher", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein Genius die m\u00e4chtigsten Gestalten,", "tokens": ["Sein", "Ge\u00b7nius", "die", "m\u00e4ch\u00b7tigs\u00b7ten", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+---+-", "measure": "anapaest.init"}, "line.3": {"text": "Und, selbst ein Ariel im Sturmgebr\u00fclle,", "tokens": ["Und", ",", "selbst", "ein", "A\u00b7riel", "im", "Sturm\u00b7ge\u00b7br\u00fcl\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Gebot er H\u00f6llennacht und Lichtgewalten.", "tokens": ["Ge\u00b7bot", "er", "H\u00f6l\u00b7len\u00b7nacht", "und", "Licht\u00b7ge\u00b7wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es schien, der Weltgeist lie\u00df in dieser H\u00fclle", "tokens": ["Es", "schien", ",", "der", "Welt\u00b7geist", "lie\u00df", "in", "die\u00b7ser", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das R\u00e4tsel seines Schaffens sich entfalten.", "tokens": ["Das", "R\u00e4t\u00b7sel", "sei\u00b7nes", "Schaf\u00b7fens", "sich", "ent\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}