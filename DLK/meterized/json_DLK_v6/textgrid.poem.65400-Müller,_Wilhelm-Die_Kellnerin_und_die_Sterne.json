{"textgrid.poem.65400": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Kellnerin und die Sterne", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Himmels Sterne gehen auf und unter,", "tokens": ["Des", "Him\u00b7mels", "Ster\u00b7ne", "ge\u00b7hen", "auf", "und", "un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKVZ", "KON", "APPR", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und deine Augen leuchten immer munter,", "tokens": ["Und", "dei\u00b7ne", "Au\u00b7gen", "leuch\u00b7ten", "im\u00b7mer", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vom fr\u00fchen Morgen bis zur sp\u00e4ten Nacht:", "tokens": ["Vom", "fr\u00fc\u00b7hen", "Mor\u00b7gen", "bis", "zur", "sp\u00e4\u00b7ten", "Nacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das hat die Sterne b\u00f6s' auf dich gemacht.", "tokens": ["Das", "hat", "die", "Ster\u00b7ne", "b\u00f6s'", "auf", "dich", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie wollen einen alten Mann dir geben,", "tokens": ["Sie", "wol\u00b7len", "ei\u00b7nen", "al\u00b7ten", "Mann", "dir", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf da\u00df du lernest nach den Stunden leben,", "tokens": ["Auf", "da\u00df", "du", "ler\u00b7nest", "nach", "den", "Stun\u00b7den", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und schlafen in der Nacht, wie sich's geh\u00f6rt,", "tokens": ["Und", "schla\u00b7fen", "in", "der", "Nacht", ",", "wie", "sich's", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn Keiner dich in deiner Ruhe st\u00f6rt.", "tokens": ["Wenn", "Kei\u00b7ner", "dich", "in", "dei\u00b7ner", "Ru\u00b7he", "st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}