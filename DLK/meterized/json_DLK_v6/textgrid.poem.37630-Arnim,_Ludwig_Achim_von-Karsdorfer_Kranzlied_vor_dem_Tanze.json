{"textgrid.poem.37630": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Karsdorfer Kranzlied vor dem Tanze", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frisch auf, ihr M\u00e4nner, f\u00fchrt die Fraun,", "tokens": ["Frisch", "auf", ",", "ihr", "M\u00e4n\u00b7ner", ",", "f\u00fchrt", "die", "Fraun", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Tanzplatz sch\u00f6n geschm\u00fccket,", "tokens": ["Zum", "Tanz\u00b7platz", "sch\u00f6n", "ge\u00b7schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Viel tausend Blumen sind zu schaun,", "tokens": ["Viel", "tau\u00b7send", "Blu\u00b7men", "sind", "zu", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu einem Kranz gepfl\u00fccket,", "tokens": ["Zu", "ei\u00b7nem", "Kranz", "ge\u00b7pfl\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auf, windet lustig an dem Kranz", "tokens": ["Auf", ",", "win\u00b7det", "lus\u00b7tig", "an", "dem", "Kranz"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "$,", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In diesem hellen Morgenglanz.", "tokens": ["In", "die\u00b7sem", "hel\u00b7len", "Mor\u00b7gen\u00b7glanz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zur Arbeit brannte manchen Tag", "tokens": ["Zur", "Ar\u00b7beit", "brann\u00b7te", "man\u00b7chen", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sonn auf unsern R\u00fccken,", "tokens": ["Die", "Sonn", "auf", "un\u00b7sern", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Heut schaut sie milde zum Gelag", "tokens": ["Heut", "schaut", "sie", "mil\u00b7de", "zum", "Ge\u00b7lag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit allen Liebesblicken,", "tokens": ["Mit", "al\u00b7len", "Lie\u00b7bes\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hellstrahlend heut ihr Angesicht", "tokens": ["Hell\u00b7strah\u00b7lend", "heut", "ihr", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch alle tr\u00fcbe Wolken bricht.", "tokens": ["Durch", "al\u00b7le", "tr\u00fc\u00b7be", "Wol\u00b7ken", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die H\u00fcthe schwenkt zu Gottes Ehr,", "tokens": ["Die", "H\u00fc\u00b7the", "schwenkt", "zu", "Got\u00b7tes", "Ehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schaut an die fernen Auen,", "tokens": ["Schaut", "an", "die", "fer\u00b7nen", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kein Feld, das nicht gesegnet w\u00e4r,", "tokens": ["Kein", "Feld", ",", "das", "nicht", "ge\u00b7seg\u00b7net", "w\u00e4r", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PTKNEG", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "K\u00f6nnt ihr vom Buchberg schauen,", "tokens": ["K\u00f6nnt", "ihr", "vom", "Buch\u00b7berg", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und \u00fcber uns der Wald erklingt,", "tokens": ["Und", "\u00fc\u00b7ber", "uns", "der", "Wald", "er\u00b7klingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu Gottes Ehr der Vogel singt.", "tokens": ["Zu", "Got\u00b7tes", "Ehr", "der", "Vo\u00b7gel", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Zu Gottes Ehr der Tauben Schaar", "tokens": ["Zu", "Got\u00b7tes", "Ehr", "der", "Tau\u00b7ben", "Schaar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erschwingt sich durch den Himmel,", "tokens": ["Er\u00b7schwingt", "sich", "durch", "den", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu seiner Ehr den See so klar", "tokens": ["Zu", "sei\u00b7ner", "Ehr", "den", "See", "so", "klar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durchrauscht der Fisch' Get\u00fcmmel,", "tokens": ["Durc\u00b7hrauscht", "der", "Fisch'", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Wiesen Gr\u00fcn das Auge st\u00e4rkt,", "tokens": ["Der", "Wie\u00b7sen", "Gr\u00fcn", "das", "Au\u00b7ge", "st\u00e4rkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df es auf Gottes Gnade merkt.", "tokens": ["Da\u00df", "es", "auf", "Got\u00b7tes", "Gna\u00b7de", "merkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Er schuf die Welt, hat diesen Ort", "tokens": ["Er", "schuf", "die", "Welt", ",", "hat", "die\u00b7sen", "Ort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VAFIN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf gr\u00fcnem Berg gegr\u00fcndet,", "tokens": ["Auf", "gr\u00fc\u00b7nem", "Berg", "ge\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df einst gelehret, was sein Wort", "tokens": ["Da\u00df", "einst", "ge\u00b7leh\u00b7ret", ",", "was", "sein", "Wort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "VVPP", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und frohe Lust verk\u00fcndet:", "tokens": ["Und", "fro\u00b7he", "Lust", "ver\u00b7k\u00fcn\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch wer zuerst den Ort erkannt,", "tokens": ["Doch", "wer", "zu\u00b7erst", "den", "Ort", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den ehre rings das frohe Land.", "tokens": ["Den", "eh\u00b7re", "rings", "das", "fro\u00b7he", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dem Herrn geb\u00fchrt der Ehrentrank,", "tokens": ["Dem", "Herrn", "ge\u00b7b\u00fchrt", "der", "Eh\u00b7ren\u00b7trank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er hat den Platz umgeben", "tokens": ["Er", "hat", "den", "Platz", "um\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit bunten D\u00e4chern, Tisch und Bank,", "tokens": ["Mit", "bun\u00b7ten", "D\u00e4\u00b7chern", ",", "Tisch", "und", "Bank", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gott lass' ihn lange leben;", "tokens": ["Gott", "lass'", "ihn", "lan\u00b7ge", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er giebt uns Speis', er giebt uns Trank,", "tokens": ["Er", "giebt", "uns", "Speis'", ",", "er", "giebt", "uns", "Trank", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NE", "$,", "PPER", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihm schalle jubelnd unser Dank.", "tokens": ["Ihm", "schal\u00b7le", "ju\u00b7belnd", "un\u00b7ser", "Dank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ihr wisset noch, wie w\u00fcst es war,", "tokens": ["Ihr", "wis\u00b7set", "noch", ",", "wie", "w\u00fcst", "es", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "VVFIN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Eh' unser Herr gekommen,", "tokens": ["Eh'", "un\u00b7ser", "Herr", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Heut geht hieher manch Liebespaar,", "tokens": ["Heut", "geht", "hie\u00b7her", "manch", "Lie\u00b7be\u00b7spaar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PAV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So froh und so beklommen,", "tokens": ["So", "froh", "und", "so", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und wird vom Pred'ger eingeweiht,", "tokens": ["Und", "wird", "vom", "Pre\u00b7d'\u00b7ger", "ein\u00b7ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Zum Ehestand bei Lust und Freud.", "tokens": ["Zum", "E\u00b7hes\u00b7tand", "bei", "Lust", "und", "Freud", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Sie sind begl\u00fcckt in ihrer Brust,", "tokens": ["Sie", "sind", "be\u00b7gl\u00fcckt", "in", "ih\u00b7rer", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sitzen still beiseite,", "tokens": ["Und", "sit\u00b7zen", "still", "bei\u00b7sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein jedes Alter hat hier Lust,", "tokens": ["Ein", "je\u00b7des", "Al\u00b7ter", "hat", "hier", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Kinder schaukeln heute,", "tokens": ["Die", "Kin\u00b7der", "schau\u00b7keln", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es tr\u00e4gt der Greis das Kleinste her", "tokens": ["Es", "tr\u00e4gt", "der", "Greis", "das", "Kleins\u00b7te", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und findet seine Last nicht schwer.", "tokens": ["Und", "fin\u00b7det", "sei\u00b7ne", "Last", "nicht", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Aus Brunnen springt empor der Trank,", "tokens": ["Aus", "Brun\u00b7nen", "springt", "em\u00b7por", "der", "Trank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PTKVZ", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den uns der Herr geschenket,", "tokens": ["Den", "uns", "der", "Herr", "ge\u00b7schen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zum Himmel steiget unser Dank,", "tokens": ["Zum", "Him\u00b7mel", "stei\u00b7get", "un\u00b7ser", "Dank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der uns den Herrn geschenket.", "tokens": ["Der", "uns", "den", "Herrn", "ge\u00b7schen\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es pflanzte seine Th\u00e4tigkeit", "tokens": ["Es", "pflanz\u00b7te", "sei\u00b7ne", "Th\u00e4\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hier manchen Baum zur Fr\u00f6hlichkeit.", "tokens": ["Hier", "man\u00b7chen", "Baum", "zur", "Fr\u00f6h\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Daf\u00fcr gedeiht, was er ges\u00e4t,", "tokens": ["Da\u00b7f\u00fcr", "ge\u00b7deiht", ",", "was", "er", "ge\u00b7s\u00e4t", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und was er baut, das bleibet,", "tokens": ["Und", "was", "er", "baut", ",", "das", "blei\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und was er ordnet, das ger\u00e4th,", "tokens": ["Und", "was", "er", "ord\u00b7net", ",", "das", "ge\u00b7r\u00e4\u00b7th", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "PRELS", "ADJD", "$,"], "