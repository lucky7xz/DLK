{"textgrid.poem.38402": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Morgenlied von den Sch\u00e4fchen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlaf, Kindlein, schlaf,", "tokens": ["Schlaf", ",", "Kin\u00b7dlein", ",", "schlaf", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der Vater h\u00fct die Schaaf,", "tokens": ["Der", "Va\u00b7ter", "h\u00fct", "die", "Schaaf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Mutter sch\u00fcttelts B\u00e4umelein,", "tokens": ["Die", "Mut\u00b7ter", "sch\u00fct\u00b7telts", "B\u00e4u\u00b7me\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da f\u00e4llt herab ein Tr\u00e4umelein,", "tokens": ["Da", "f\u00e4llt", "her\u00b7ab", "ein", "Tr\u00e4u\u00b7me\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schlaf, Kindlein, schlaf.", "tokens": ["Schlaf", ",", "Kin\u00b7dlein", ",", "schlaf", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Schlaf, Kindlein, schlaf,", "tokens": ["Schlaf", ",", "Kin\u00b7dlein", ",", "schlaf", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Am Himmel ziehn die Schaaf,", "tokens": ["Am", "Him\u00b7mel", "ziehn", "die", "Schaaf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Sternlein sind die L\u00e4mmerlein,", "tokens": ["Die", "Stern\u00b7lein", "sind", "die", "L\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Mond der ist das Sch\u00e4ferlein,", "tokens": ["Der", "Mond", "der", "ist", "das", "Sch\u00e4\u00b7fer\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schlaf, Kindlein, schlaf.", "tokens": ["Schlaf", ",", "Kin\u00b7dlein", ",", "schlaf", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Schlaf, Kindlein, schlaf,", "tokens": ["Schlaf", ",", "Kin\u00b7dlein", ",", "schlaf", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Christkindlein hat ein Schaaf,", "tokens": ["Christ\u00b7kin\u00b7dlein", "hat", "ein", "Schaaf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist selbst das liebe Gotteslamm,", "tokens": ["Ist", "selbst", "das", "lie\u00b7be", "Got\u00b7tes\u00b7lamm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das um uns all zu Tode kam,", "tokens": ["Das", "um", "uns", "all", "zu", "To\u00b7de", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPER", "PIAT", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schlaf, Kindlein, schlaf!", "tokens": ["Schlaf", ",", "Kin\u00b7dlein", ",", "schlaf", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Schlaf, Kindlein, schlaf,", "tokens": ["Schlaf", ",", "Kin\u00b7dlein", ",", "schlaf", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "So schenk ich dir ein Schaaf,", "tokens": ["So", "schenk", "ich", "dir", "ein", "Schaaf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit einer goldnen Schelle fein,", "tokens": ["Mit", "ei\u00b7ner", "gold\u00b7nen", "Schel\u00b7le", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das soll dein Spielgeselle seyn,", "tokens": ["Das", "soll", "dein", "Spiel\u00b7ge\u00b7sel\u00b7le", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schlaf, Kindlein, schlaf!", "tokens": ["Schlaf", ",", "Kin\u00b7dlein", ",", "schlaf", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Schlaf, Kindlein, schlaf,", "tokens": ["Schlaf", ",", "Kin\u00b7dlein", ",", "schlaf", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und bl\u00f6ck nicht wie ein Schaaf,", "tokens": ["Und", "bl\u00f6ck", "nicht", "wie", "ein", "Schaaf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sonst k\u00f6mmt des Sch\u00e4fers H\u00fcndelein,", "tokens": ["Sonst", "k\u00f6mmt", "des", "Sch\u00e4\u00b7fers", "H\u00fcn\u00b7del\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bei\u00dft mein b\u00f6ses Kindelein,", "tokens": ["Und", "bei\u00dft", "mein", "b\u00f6\u00b7ses", "Kin\u00b7de\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schlaf, Kindlein, schlaf.", "tokens": ["Schlaf", ",", "Kin\u00b7dlein", ",", "schlaf", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Schlaf, Kindlein, schlaf,", "tokens": ["Schlaf", ",", "Kin\u00b7dlein", ",", "schlaf", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Geh fort und h\u00fct die Schaaf,", "tokens": ["Geh", "fort", "und", "h\u00fct", "die", "Schaaf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Geh fort du schwarzes H\u00fcndelein,", "tokens": ["Geh", "fort", "du", "schwar\u00b7zes", "H\u00fcn\u00b7del\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und weck mir nicht mein Kindelein,", "tokens": ["Und", "weck", "mir", "nicht", "mein", "Kin\u00b7de\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schlaf, Kindlein, schlaf.", "tokens": ["Schlaf", ",", "Kin\u00b7dlein", ",", "schlaf", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}