{"textgrid.poem.55703": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u2013 \u2013 Und ich sah Dich wieder,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u2013 \u2013 Und ich sah Dich wieder,", "tokens": ["\u2013", "\u2013", "Und", "ich", "sah", "Dich", "wie\u00b7der", ","], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "KON", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sah Dein Haar ergraut", "tokens": ["Sah", "Dein", "Haar", "er\u00b7graut"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "In schwerer Pflichterf\u00fcllung,", "tokens": ["In", "schwe\u00b7rer", "Pflich\u00b7ter\u00b7f\u00fcl\u00b7lung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sah die Linien,", "tokens": ["Und", "sah", "die", "Li\u00b7ni\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die herber Trotz", "tokens": ["Die", "her\u00b7ber", "Trotz"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und bittre Menschenverachtung", "tokens": ["Und", "bitt\u00b7re", "Men\u00b7schen\u00b7ver\u00b7ach\u00b7tung"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Um Deine schmalen,", "tokens": ["Um", "Dei\u00b7ne", "schma\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Blutlosen Lippen gezogen.", "tokens": ["Blut\u00b7lo\u00b7sen", "Lip\u00b7pen", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Mit halbverschleierter", "tokens": ["Mit", "halb\u00b7ver\u00b7schlei\u00b7er\u00b7ter"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "++-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "M\u00fcder Stimme,", "tokens": ["M\u00fc\u00b7der", "Stim\u00b7me", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "In der nur selten", "tokens": ["In", "der", "nur", "sel\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ein lichter Ton der Freude", "tokens": ["Ein", "lich\u00b7ter", "Ton", "der", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sch\u00fcchtern emporklang,", "tokens": ["Sch\u00fcch\u00b7tern", "em\u00b7por\u00b7klang", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Erz\u00e4hltest Du mir", "tokens": ["Er\u00b7z\u00e4hl\u00b7test", "Du", "mir"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Von Deines Lebens", "tokens": ["Von", "Dei\u00b7nes", "Le\u00b7bens"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Dornigen Wanderz\u00fcgen", "tokens": ["Dor\u00b7ni\u00b7gen", "Wan\u00b7der\u00b7z\u00fc\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und qualvollen K\u00e4mpfen.", "tokens": ["Und", "qual\u00b7vol\u00b7len", "K\u00e4mp\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich und Du,", "tokens": ["Ich", "und", "Du", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Wir h\u00e4tten gerne", "tokens": ["Wir", "h\u00e4t\u00b7ten", "ger\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Von gemeinsam verlebten", "tokens": ["Von", "ge\u00b7mein\u00b7sam", "ver\u00b7leb\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJD", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Seligen Stunden gesprochen,", "tokens": ["Se\u00b7li\u00b7gen", "Stun\u00b7den", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Von unvergessenen,", "tokens": ["Von", "un\u00b7ver\u00b7ges\u00b7se\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "ADJA", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.6": {"text": "Liebesschw\u00fclen Tagen, \u2013", "tokens": ["Lie\u00b7bes\u00b7schw\u00fc\u00b7len", "Ta\u00b7gen", ",", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Aber keines von uns Beiden", "tokens": ["A\u00b7ber", "kei\u00b7nes", "von", "uns", "Bei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "APPR", "PPER", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Fand das hei\u00dfersehnte,", "tokens": ["Fand", "das", "hei\u00b7\u00dfer\u00b7sehn\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Das richtige Wort.", "tokens": ["Das", "rich\u00b7ti\u00b7ge", "Wort", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}