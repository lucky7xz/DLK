{"dta.poem.6751": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Stoff formirt das Werckzeug, anders nichts:", "tokens": ["Der", "Stoff", "for\u00b7mirt", "das", "Wer\u00b7ck\u00b7zeug", ",", "an\u00b7ders", "nichts", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "ART", "NN", "$,", "ADV", "PIS", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und den verschiednen Druck formirt der Strahl des Lichts.", "tokens": ["Und", "den", "ver\u00b7schied\u00b7nen", "Druck", "for\u00b7mirt", "der", "Strahl", "des", "Lichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Br\u00fcche, welche sich ver\u00e4ndern f\u00fcr und f\u00fcr,", "tokens": ["Die", "Br\u00fc\u00b7che", ",", "wel\u00b7che", "sich", "ver\u00b7\u00e4n\u00b7dern", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "VVINF", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sind zarte Pinselchen, wodurch von allen Bildern", "tokens": ["Sind", "zar\u00b7te", "Pin\u00b7sel\u00b7chen", ",", "wo\u00b7durch", "von", "al\u00b7len", "Bil\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJA", "NN", "$,", "PWAV", "APPR", "PIAT", "NN"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "Solch eine Menge sich auf der Retina schildern,", "tokens": ["Solch", "ei\u00b7ne", "Men\u00b7ge", "sich", "auf", "der", "Re\u00b7ti\u00b7na", "schil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "PRF", "APPR", "ART", "NE", "VVINF", "$,"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Mit allen ihren Linien", "tokens": ["Mit", "al\u00b7len", "ih\u00b7ren", "Li\u00b7ni\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und Farben, selbst mit den Bewegungen.", "tokens": ["Und", "Far\u00b7ben", ",", "selbst", "mit", "den", "Be\u00b7we\u00b7gun\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}