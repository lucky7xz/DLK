{"dta.poem.12943": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie die hand vor ihr gesichte  \n hielt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weil deine finger schnee, und deiner augen licht", "tokens": ["Weil", "dei\u00b7ne", "fin\u00b7ger", "schnee", ",", "und", "dei\u00b7ner", "au\u00b7gen", "licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$,", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zwey heisse sonnen seyn,", "tokens": ["Zwey", "heis\u00b7se", "son\u00b7nen", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach so vermaure doch mit deiner weichen hand", "tokens": ["Ach", "so", "ver\u00b7mau\u00b7re", "doch", "mit", "dei\u00b7ner", "wei\u00b7chen", "hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein helles antlitz nicht.", "tokens": ["Dein", "hel\u00b7les", "ant\u00b7litz", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zeuch sie, eh\u2019 deine blicke", "tokens": ["Zeuch", "sie", ",", "eh'", "dei\u00b7ne", "bli\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den heissen strahl darauf gewandt,", "tokens": ["Den", "heis\u00b7sen", "strahl", "da\u00b7rauf", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch bald zur\u00fccke,", "tokens": ["Doch", "bald", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKVZ", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Weil deinem Damon zum verdru\u00df", "tokens": ["Weil", "dei\u00b7nem", "Da\u00b7mon", "zum", "ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Bey diesen sonnen sonst ihr schnee zerschmeltzen mu\u00df.", "tokens": ["Bey", "die\u00b7sen", "son\u00b7nen", "sonst", "ihr", "schnee", "zer\u00b7schmelt\u00b7zen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "ADV", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}