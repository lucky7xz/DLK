{"dta.poem.14488": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Morgenlied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und ehe sich alle erhoben", "tokens": ["Und", "e\u00b7he", "sich", "al\u00b7le", "er\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PRF", "PIS", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Des Tages Freuden und Weh,", "tokens": ["Des", "Ta\u00b7ges", "Freu\u00b7den", "und", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Will ich Herr Gott Dich loben", "tokens": ["Will", "ich", "Herr", "Gott", "Dich", "lo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "NN", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hier einsam in stiller H\u00f6h. \u2014", "tokens": ["Hier", "ein\u00b7sam", "in", "stil\u00b7ler", "H\u00f6h", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}