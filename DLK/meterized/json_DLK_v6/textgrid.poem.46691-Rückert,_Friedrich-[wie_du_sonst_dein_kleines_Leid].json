{"textgrid.poem.46691": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wie du sonst dein kleines Leid]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie du sonst dein kleines Leid", "tokens": ["Wie", "du", "sonst", "dein", "klei\u00b7nes", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kindisch pflegtest mir zu klagen,", "tokens": ["Kin\u00b7disch", "pfleg\u00b7test", "mir", "zu", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tr\u00f6stet' ich dich m\u00fctterlich.", "tokens": ["Tr\u00f6s\u00b7tet'", "ich", "dich", "m\u00fct\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Also mein nicht kleines Leid", "tokens": ["Al\u00b7so", "mein", "nicht", "klei\u00b7nes", "Leid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "PTKNEG", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df ich dir mit Klagen sagen", "tokens": ["Mu\u00df", "ich", "dir", "mit", "Kla\u00b7gen", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun, o Kindchen, tr\u00f6ste mich!", "tokens": ["Nun", ",", "o", "Kind\u00b7chen", ",", "tr\u00f6s\u00b7te", "mich", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "FM", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Also wechselt Lust und Leid,", "tokens": ["Al\u00b7so", "wech\u00b7selt", "Lust", "und", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab' am Herzen dich getragen,", "tokens": ["Hab'", "am", "Her\u00b7zen", "dich", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Trage nun den Schmerz um dich.", "tokens": ["Tra\u00b7ge", "nun", "den", "Schmerz", "um", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}