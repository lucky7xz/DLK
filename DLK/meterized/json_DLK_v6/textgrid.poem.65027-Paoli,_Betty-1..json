{"textgrid.poem.65027": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fort trieb mich's an den Busen der Natur", "tokens": ["Fort", "trieb", "mich's", "an", "den", "Bu\u00b7sen", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PIS", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus meiner Zelle, d'rin ich lang verschlossen,", "tokens": ["Aus", "mei\u00b7ner", "Zel\u00b7le", ",", "d'\u00b7rin", "ich", "lang", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PWAV", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und staunend sah mein Auge Wald und Flur", "tokens": ["Und", "stau\u00b7nend", "sah", "mein", "Au\u00b7ge", "Wald", "und", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von Sonnenstrahlen goldig \u00fcbergossen.", "tokens": ["Von", "Son\u00b7nen\u00b7strah\u00b7len", "gol\u00b7dig", "\u00fc\u00b7ber\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es sprach das Licht: O sag! was trauerst du?", "tokens": ["Es", "sprach", "das", "Licht", ":", "O", "sag", "!", "was", "trau\u00b7erst", "du", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "NE", "VVFIN", "$.", "PWS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kann meine Macht dein Dunkel nicht vers\u00fc\u00dfen?", "tokens": ["Kann", "mei\u00b7ne", "Macht", "dein", "Dun\u00b7kel", "nicht", "ver\u00b7s\u00fc\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und liebvoll s\u00e4uselte die Luft mir zu:", "tokens": ["Und", "lieb\u00b7voll", "s\u00e4u\u00b7sel\u00b7te", "die", "Luft", "mir", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich will die Thr\u00e4ne dir vom Auge k\u00fcssen!", "tokens": ["Ich", "will", "die", "Thr\u00e4\u00b7ne", "dir", "vom", "Au\u00b7ge", "k\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der rasche Strom sang mir ein brausend Lied:", "tokens": ["Der", "ra\u00b7sche", "Strom", "sang", "mir", "ein", "brau\u00b7send", "Lied", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Lebens Hast magst du in meiner sehen!", "tokens": ["Des", "Le\u00b7bens", "Hast", "magst", "du", "in", "mei\u00b7ner", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "PPER", "APPR", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und als die Sonne fern von hinnen schied,", "tokens": ["Und", "als", "die", "Son\u00b7ne", "fern", "von", "hin\u00b7nen", "schied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sprach tr\u00f6stend sie von Sterben und Vergehen! \u2013", "tokens": ["Sprach", "tr\u00f6s\u00b7tend", "sie", "von", "Ster\u00b7ben", "und", "Ver\u00b7ge\u00b7hen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADJD", "PPER", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}