{"textgrid.poem.55977": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nah ist nur Innres; alles andre fern.", "tokens": ["Nah", "ist", "nur", "Inn\u00b7res", ";", "al\u00b7les", "and\u00b7re", "fern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "NN", "$.", "PIS", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und dieses Innere gedr\u00e4ngt und t\u00e4glich", "tokens": ["Und", "die\u00b7ses", "In\u00b7ne\u00b7re", "ge\u00b7dr\u00e4ngt", "und", "t\u00e4g\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "VVPP", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "mit allem \u00fcberf\u00fcllt und ganz uns\u00e4glich.", "tokens": ["mit", "al\u00b7lem", "\u00fc\u00b7berf\u00b7\u00fcllt", "und", "ganz", "un\u00b7s\u00e4g\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVPP", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Insel ist wie ein zu kleiner Stern", "tokens": ["Die", "In\u00b7sel", "ist", "wie", "ein", "zu", "klei\u00b7ner", "Stern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "ART", "PTKZU", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "welchen der Raum nicht merkt und stumm zerst\u00f6rt", "tokens": ["wel\u00b7chen", "der", "Raum", "nicht", "merkt", "und", "stumm", "zer\u00b7st\u00f6rt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAT", "ART", "NN", "PTKNEG", "VVFIN", "KON", "ADJD", "VVPP"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "in seinem unbewu\u00dften Furchtbarsein,", "tokens": ["in", "sei\u00b7nem", "un\u00b7be\u00b7wu\u00df\u00b7ten", "Furcht\u00b7bar\u00b7sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "so da\u00df er, unerhellt und \u00fcberh\u00f6rt,", "tokens": ["so", "da\u00df", "er", ",", "un\u00b7er\u00b7hellt", "und", "\u00fc\u00b7ber\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "$,", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "allein", "tokens": ["al\u00b7lein"], "token_info": ["word"], "pos": ["ADV"], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "damit dies alles doch ein Ende nehme", "tokens": ["da\u00b7mit", "dies", "al\u00b7les", "doch", "ein", "En\u00b7de", "neh\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDS", "PIS", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "dunkel auf einer selbsterfundnen Bahn", "tokens": ["dun\u00b7kel", "auf", "ei\u00b7ner", "selbs\u00b7ter\u00b7fund\u00b7nen", "Bahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "versucht zu gehen, blindlings, nicht im Plan", "tokens": ["ver\u00b7sucht", "zu", "ge\u00b7hen", ",", "blind\u00b7lings", ",", "nicht", "im", "Plan"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "PTKZU", "VVINF", "$,", "ADV", "$,", "PTKNEG", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "der Wandelsterne, Sonnen und Systeme.", "tokens": ["der", "Wan\u00b7dels\u00b7ter\u00b7ne", ",", "Son\u00b7nen", "und", "Sys\u00b7te\u00b7me", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+---+--+--", "measure": "iambic.tri.relaxed"}}}}}