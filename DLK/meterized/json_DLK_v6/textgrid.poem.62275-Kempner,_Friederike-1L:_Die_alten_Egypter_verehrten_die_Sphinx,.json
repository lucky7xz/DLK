{"textgrid.poem.62275": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die alten Egypter verehrten die Sphinx,", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die alten Egypter verehrten die Sphinx,", "tokens": ["Die", "al\u00b7ten", "E\u00b7gyp\u00b7ter", "ver\u00b7ehr\u00b7ten", "die", "Sphinx", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Sphinx \u2013 das R\u00e4tsel des Lebens \u2013", "tokens": ["Die", "Sphinx", "\u2013", "das", "R\u00e4t\u00b7sel", "des", "Le\u00b7bens", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das R\u00e4tsel des Menschen \u2013 das R\u00e4tsel der Welt \u2013", "tokens": ["Das", "R\u00e4t\u00b7sel", "des", "Men\u00b7schen", "\u2013", "das", "R\u00e4t\u00b7sel", "der", "Welt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die L\u00f6sung sucht man vergebens.", "tokens": ["Die", "L\u00f6\u00b7sung", "sucht", "man", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Grieche, grazi\u00f6ser die Psyche er schuf,", "tokens": ["Der", "Grie\u00b7che", ",", "gra\u00b7zi\u00b7\u00f6\u00b7ser", "die", "Psy\u00b7che", "er", "schuf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Psyche \u2013 das Sinnbild der Seele \u2013", "tokens": ["Die", "Psy\u00b7che", "\u2013", "das", "Sinn\u00b7bild", "der", "See\u00b7le", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "In Marmor grub er die Sch\u00f6nheit hinein,", "tokens": ["In", "Mar\u00b7mor", "grub", "er", "die", "Sch\u00f6n\u00b7heit", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da\u00df Jeder sie sehe und w\u00e4hle. \u2013", "tokens": ["Da\u00df", "Je\u00b7der", "sie", "se\u00b7he", "und", "w\u00e4h\u00b7le", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "KON", "VVFIN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Doch unsre Zeit dem \u00e4u\u00dfern Schein \u2013", "tokens": ["Doch", "uns\u00b7re", "Zeit", "dem", "\u00e4u\u00b7\u00dfern", "Schein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Schatten der Wahrheit ergeben \u2013", "tokens": ["Dem", "Schat\u00b7ten", "der", "Wahr\u00b7heit", "er\u00b7ge\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Verk\u00fcndet und lehrt das moralische Nichts,", "tokens": ["Ver\u00b7k\u00fcn\u00b7det", "und", "lehrt", "das", "mo\u00b7ra\u00b7li\u00b7sche", "Nichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Kein Sinnbild wird sie erheben! \u2013", "tokens": ["Kein", "Sinn\u00b7bild", "wird", "sie", "er\u00b7he\u00b7ben", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}