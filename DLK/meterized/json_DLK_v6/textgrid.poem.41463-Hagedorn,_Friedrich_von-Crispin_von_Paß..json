{"textgrid.poem.41463": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Crispin von Pa\u00df.", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein kleiner Eigensinn sei K\u00fcnstlern gern verziehen!", "tokens": ["Ein", "klei\u00b7ner", "Ei\u00b7gen\u00b7sinn", "sei", "K\u00fcnst\u00b7lern", "gern", "ver\u00b7zie\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich setze mit Bedacht: ein kleiner Eigensinn;", "tokens": ["Ich", "set\u00b7ze", "mit", "Be\u00b7dacht", ":", "ein", "klei\u00b7ner", "Ei\u00b7gen\u00b7sinn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn allen, die sich nicht um Kunst und Witz bem\u00fchen,", "tokens": ["Denn", "al\u00b7len", ",", "die", "sich", "nicht", "um", "Kunst", "und", "Witz", "be\u00b7m\u00fc\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PRF", "PTKNEG", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem groben Theil der Welt, geh' auch der gr\u00f6\u00dfte hin!", "tokens": ["Dem", "gro\u00b7ben", "Theil", "der", "Welt", ",", "geh'", "auch", "der", "gr\u00f6\u00df\u00b7te", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,", "VVFIN", "ADV", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein K\u00fcnstler, welcher sich des Griffels Ruhm erworben,", "tokens": ["Ein", "K\u00fcnst\u00b7ler", ",", "wel\u00b7cher", "sich", "des", "Grif\u00b7fels", "Ruhm", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der einen Ridinger, und Schmidt, und Prei\u00dfler ziert,", "tokens": ["Der", "ei\u00b7nen", "Ri\u00b7din\u00b7ger", ",", "und", "Schmidt", ",", "und", "Prei\u00df\u00b7ler", "ziert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "$,", "KON", "NE", "$,", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Entwarf nicht leicht das Bild der F\u00fcrsten, die verstorben,", "tokens": ["Ent\u00b7warf", "nicht", "leicht", "das", "Bild", "der", "F\u00fcrs\u00b7ten", ",", "die", "ver\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADJD", "ART", "NN", "ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Noch der Gelehrten Bild, eh' sie der Tod entf\u00fchrt.", "tokens": ["Noch", "der", "Ge\u00b7lehr\u00b7ten", "Bild", ",", "eh'", "sie", "der", "Tod", "ent\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die meisten wu\u00dften nicht die Ursach' anzugeben,", "tokens": ["Die", "meis\u00b7ten", "wu\u00df\u00b7ten", "nicht", "die", "Ur\u00b7sach'", "an\u00b7zu\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PTKNEG", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Bis einst, ich wei\u00df nicht wer, sie von ihm selbst erfuhr:", "tokens": ["Bis", "einst", ",", "ich", "wei\u00df", "nicht", "wer", ",", "sie", "von", "ihm", "selbst", "er\u00b7fuhr", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "PPER", "VVFIN", "PTKNEG", "ADJD", "$,", "PPER", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der F\u00fcrsten achtet man nicht l\u00e4nger, als sie leben,", "tokens": ["Der", "F\u00fcrs\u00b7ten", "ach\u00b7tet", "man", "nicht", "l\u00e4n\u00b7ger", ",", "als", "sie", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "PTKNEG", "ADJD", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und der Gelehrte gilt nach seinem Tode nur.", "tokens": ["Und", "der", "Ge\u00b7lehr\u00b7te", "gilt", "nach", "sei\u00b7nem", "To\u00b7de", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}