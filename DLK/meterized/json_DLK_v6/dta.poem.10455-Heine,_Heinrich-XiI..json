{"dta.poem.10455": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XiI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.71", "da:0.28"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Der Abend kommt gezogen,", "tokens": ["Der", "A\u00b7bend", "kommt", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Nebel bedeckt die See;", "tokens": ["Der", "Ne\u00b7bel", "be\u00b7deckt", "die", "See", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Geheimni\u00dfvoll rauschen die Wogen,", "tokens": ["Ge\u00b7heim\u00b7ni\u00df\u00b7voll", "rau\u00b7schen", "die", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Da steigt es wei\u00df in die H\u00f6h'.", "tokens": ["Da", "steigt", "es", "wei\u00df", "in", "die", "H\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Die Meerfrau steigt aus den Wellen,", "tokens": ["Die", "Meer\u00b7frau", "steigt", "aus", "den", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und setzt sich zu mir, am Strand;", "tokens": ["Und", "setzt", "sich", "zu", "mir", ",", "am", "Strand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPER", "$,", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die wei\u00dfen Br\u00fcste quellen", "tokens": ["Die", "wei\u00b7\u00dfen", "Br\u00fcs\u00b7te", "quel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hervor aus dem Schleiergewand.", "tokens": ["Her\u00b7vor", "aus", "dem", "Schlei\u00b7er\u00b7ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Sie dr\u00fcckt mich und sie pre\u00dft mich", "tokens": ["Sie", "dr\u00fcckt", "mich", "und", "sie", "pre\u00dft", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KON", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und thut mir fast ein Weh';", "tokens": ["Und", "thut", "mir", "fast", "ein", "Weh'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du dr\u00fcckst ja viel zu fest mich,", "tokens": ["Du", "dr\u00fcckst", "ja", "viel", "zu", "fest", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PTKA", "ADJD", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du sch\u00f6ne Wasserfee!", "tokens": ["Du", "sch\u00f6\u00b7ne", "Was\u00b7ser\u00b7fee", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u201eich presse dich, in meinen Armen,", "tokens": ["\u201e", "ich", "pres\u00b7se", "dich", ",", "in", "mei\u00b7nen", "Ar\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und dr\u00fccke dich mit Gewalt;", "tokens": ["Und", "dr\u00fc\u00b7cke", "dich", "mit", "Ge\u00b7walt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich will bei dir erwarmen,", "tokens": ["Ich", "will", "bei", "dir", "er\u00b7war\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Abend ist gar zu kalt.\u201c", "tokens": ["Der", "A\u00b7bend", "ist", "gar", "zu", "kalt", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Der Mond schaut immer blasser", "tokens": ["Der", "Mond", "schaut", "im\u00b7mer", "blas\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus d\u00e4mmriger Wolkenh\u00f6h';", "tokens": ["Aus", "d\u00e4mm\u00b7ri\u00b7ger", "Wol\u00b7ken\u00b7h\u00f6h'", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dein Auge wird tr\u00fcber und nasser,", "tokens": ["Dein", "Au\u00b7ge", "wird", "tr\u00fc\u00b7ber", "und", "nas\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Du sch\u00f6ne Wasserfee!", "tokens": ["Du", "sch\u00f6\u00b7ne", "Was\u00b7ser\u00b7fee", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u201ees wird nicht tr\u00fcber und nasser,", "tokens": ["\u201e", "es", "wird", "nicht", "tr\u00fc\u00b7ber", "und", "nas\u00b7ser", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PTKNEG", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mein Aug' ist na\u00df und tr\u00fcb',", "tokens": ["Mein", "Aug'", "ist", "na\u00df", "und", "tr\u00fcb'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil, als ich stieg aus dem Wasser,", "tokens": ["Weil", ",", "als", "ich", "stieg", "aus", "dem", "Was\u00b7ser", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Ein Tropfen im Auge blieb.\u201c", "tokens": ["Ein", "Trop\u00b7fen", "im", "Au\u00b7ge", "blieb", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Die M\u00f6ven schrillen kl\u00e4glich,", "tokens": ["Die", "M\u00f6\u00b7ven", "schril\u00b7len", "kl\u00e4g\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es grollt und brandet die See;", "tokens": ["Es", "grollt", "und", "bran\u00b7det", "die", "See", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dein Herz pocht wild beweglich,", "tokens": ["Dein", "Herz", "pocht", "wild", "be\u00b7weg\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du sch\u00f6ne Wasserfee!", "tokens": ["Du", "sch\u00f6\u00b7ne", "Was\u00b7ser\u00b7fee", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u201emein Herz pocht wild beweglich,", "tokens": ["\u201e", "mein", "Herz", "pocht", "wild", "be\u00b7weg\u00b7lich", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es pocht beweglich wild;", "tokens": ["Es", "pocht", "be\u00b7weg\u00b7lich", "wild", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil ich dich liebe uns\u00e4glich,", "tokens": ["Weil", "ich", "dich", "lie\u00b7be", "un\u00b7s\u00e4g\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Du liebes Menschenbild!\u201c", "tokens": ["Du", "lie\u00b7bes", "Men\u00b7schen\u00b7bild", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}