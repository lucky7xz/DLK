{"dta.poem.18975": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "2.  \n  Jhrer Sch\u00f6nheit wunderliche  \n W\u00fcrckung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Wer sein betr\u00fcbtes aug ab aller G\u00f6tter pracht/", "tokens": ["Wer", "sein", "be\u00b7tr\u00fcb\u00b7tes", "aug", "ab", "al\u00b7ler", "G\u00f6t\u00b7ter", "pracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd ab der Natur kunst zu erquicken begehret/", "tokens": ["Vnd", "ab", "der", "Na\u00b7tur", "kunst", "zu", "er\u00b7qui\u00b7cken", "be\u00b7ge\u00b7hret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der kom vnd schaw die sonn/ die mit g\u00f6tlicher", "tokens": ["Der", "kom", "vnd", "schaw", "die", "sonn", "/", "die", "mit", "g\u00f6t\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "ART", "NN", "$(", "ART", "APPR", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "macht", "tokens": ["macht"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Mich/ ja die f\u00fcnstre welt des liebe\u0304 liechts gewehret.", "tokens": ["Mich", "/", "ja", "die", "f\u00fcnst\u00b7re", "welt", "des", "lie\u00b7b\u0113", "liechts", "ge\u00b7weh\u00b7ret", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "ADV", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch kom Er (seelig) bald: Dan mit zu fr\u00fcher nacht", "tokens": ["Doch", "kom", "Er", "(", "see\u00b7lig", ")", "bald", ":", "Dan", "mit", "zu", "fr\u00fc\u00b7her", "nacht"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$(", "ADJD", "$(", "ADV", "$.", "ADV", "APPR", "PTKA", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der tod/ sparend was b\u00f6\u00df/ dz best allzeit beschweret:", "tokens": ["Der", "tod", "/", "spa\u00b7rend", "was", "b\u00f6\u00df", "/", "dz", "best", "all\u00b7zeit", "be\u00b7schwe\u00b7ret", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KON", "PWS", "VVFIN", "$(", "KOUS", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Vnd dise G\u00f6ttin wirt mit eyfer vnd obacht", "tokens": ["Vnd", "di\u00b7se", "G\u00f6t\u00b7tin", "wirt", "mit", "ey\u00b7fer", "vnd", "o\u00b7bacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der G\u00f6tter/ als die zierd des himmels/ schon ge-", "tokens": ["Der", "G\u00f6t\u00b7ter", "/", "als", "die", "zierd", "des", "him\u00b7mels", "/", "schon", "ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$(", "KOUS", "ART", "NN", "ART", "NN", "$(", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "ehret.", "tokens": ["eh\u00b7ret", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Ein wunderreiches werck/ da lieblichkeit mit ehr/", "tokens": ["Ein", "wun\u00b7der\u00b7rei\u00b7ches", "werck", "/", "da", "lieb\u00b7lich\u00b7keit", "mit", "ehr", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KOUS", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da tugent mit sch\u00f6nheit/ in einem leib verm\u00e4hlet", "tokens": ["Da", "tu\u00b7gent", "mit", "sch\u00f6n\u00b7heit", "/", "in", "ei\u00b7nem", "leib", "ver\u00b7m\u00e4h\u00b7let"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$(", "APPR", "ART", "NN", "VVFIN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Soll segnen sein gesicht mit lust/ sein haupt mit", "tokens": ["Soll", "seg\u00b7nen", "sein", "ge\u00b7sicht", "mit", "lust", "/", "sein", "haupt", "mit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "PPOSAT", "VVPP", "APPR", "NN", "$(", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "lehr;", "tokens": ["lehr", ";"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "Da\u00df Er gestehen mu\u00df/ da\u00df mein gesang weit fehlet/", "tokens": ["Da\u00df", "Er", "ge\u00b7ste\u00b7hen", "mu\u00df", "/", "da\u00df", "mein", "ge\u00b7sang", "weit", "feh\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$(", "KOUS", "PPOSAT", "ADJD", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dem (bestutzet) ich jhr lob nicht gnug vermehr/", "tokens": ["In", "dem", "(", "be\u00b7stut\u00b7zet", ")", "ich", "jhr", "lob", "nicht", "gnug", "ver\u00b7mehr", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$(", "VVFIN", "$(", "PPER", "PPOSAT", "NN", "PTKNEG", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil mich jhr aug zugleich entsehlet vnd besehlet.", "tokens": ["Weil", "mich", "jhr", "aug", "zu\u00b7gleich", "ent\u00b7seh\u00b7let", "vnd", "be\u00b7seh\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}