{"dta.poem.3838": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen alten feinen B\u00fcrger,  \n  G. Schniegel.  1734.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Geist sucht nun sein Ararat,", "tokens": ["Mein", "Geist", "sucht", "nun", "sein", "Ar\u00b7a\u00b7rat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der hier mit Ernst gek\u00e4mpffet hat,", "tokens": ["Der", "hier", "mit", "Ernst", "ge\u00b7k\u00e4mpf\u00b7fet", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NE", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und meine abgelebten Glieder", "tokens": ["Und", "mei\u00b7ne", "ab\u00b7ge\u00b7leb\u00b7ten", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erfahren ihren Ursprung wieder.", "tokens": ["Er\u00b7fah\u00b7ren", "ih\u00b7ren", "Ur\u00b7sprung", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich lasse diese Jammer-Welt,", "tokens": ["Ich", "las\u00b7se", "die\u00b7se", "Jam\u00b7mer\u00b7Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich suche jenes Freuden-Zelt,", "tokens": ["Ich", "su\u00b7che", "je\u00b7nes", "Freu\u00b7den\u00b7Zelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo meine Lust soll ewig w\u00e4hren;", "tokens": ["Wo", "mei\u00b7ne", "Lust", "soll", "e\u00b7wig", "w\u00e4h\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Betr\u00fcbte! hemmet eure Z\u00e4hren.", "tokens": ["Be\u00b7tr\u00fcb\u00b7te", "!", "hem\u00b7met", "eu\u00b7re", "Z\u00e4h\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}