{"textgrid.poem.43885": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: So wenig eine junge Rebe", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So wenig eine junge Rebe", "tokens": ["So", "we\u00b7nig", "ei\u00b7ne", "jun\u00b7ge", "Re\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Ulmbaums H\u00fclfe mi\u00dfen kan,", "tokens": ["Des", "Ulm\u00b7baums", "H\u00fcl\u00b7fe", "mi\u00b7\u00dfen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So wenig ficht der Neid mich an,", "tokens": ["So", "we\u00b7nig", "ficht", "der", "Neid", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df meine Brust dir Abschied gebe.", "tokens": ["Da\u00df", "mei\u00b7ne", "Brust", "dir", "Ab\u00b7schied", "ge\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mein treues Herz ist ein Magnet,", "tokens": ["Mein", "treu\u00b7es", "Herz", "ist", "ein", "Mag\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Der nur nach einem Pole steht,", "tokens": ["Der", "nur", "nach", "ei\u00b7nem", "Po\u00b7le", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dein Nordstern leitet meine Liebe;", "tokens": ["Dein", "Nords\u00b7tern", "lei\u00b7tet", "mei\u00b7ne", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich leb und sterbe dir getreu,", "tokens": ["Ich", "leb", "und", "ster\u00b7be", "dir", "ge\u00b7treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wenn gleich der Schickung Tyranney", "tokens": ["Wenn", "gleich", "der", "Schi\u00b7ckung", "Ty\u00b7ran\u00b7ney"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mich heute noch ins Elend triebe.", "tokens": ["Mich", "heu\u00b7te", "noch", "ins", "E\u00b7lend", "trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er\u00f6fne mir das Feld der Br\u00fcste,", "tokens": ["Er\u00b7\u00f6f\u00b7ne", "mir", "das", "Feld", "der", "Br\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entschleus die wollustschwangre Schoos,", "tokens": ["Ent\u00b7schleus", "die", "wol\u00b7lust\u00b7schwang\u00b7re", "Schoos", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gieb mir die sch\u00f6nen Lenden blos,", "tokens": ["Gieb", "mir", "die", "sch\u00f6\u00b7nen", "Len\u00b7den", "blos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis sich des Monden Neid entr\u00fcste!", "tokens": ["Bis", "sich", "des", "Mon\u00b7den", "Neid", "ent\u00b7r\u00fcs\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Nacht ist unsrer Lust bequem,", "tokens": ["Die", "Nacht", "ist", "uns\u00b7rer", "Lust", "be\u00b7quem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Sternen schimmern angenehm", "tokens": ["Die", "Ster\u00b7nen", "schim\u00b7mern", "an\u00b7ge\u00b7nehm"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und buhlen uns nur zum Exempel;", "tokens": ["Und", "buh\u00b7len", "uns", "nur", "zum", "Ex\u00b7em\u00b7pel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Drum gieb mir der Verliebten Kost,", "tokens": ["Drum", "gieb", "mir", "der", "Ver\u00b7lieb\u00b7ten", "Kost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ich schencke dir der Wollust Most", "tokens": ["Ich", "schen\u00b7cke", "dir", "der", "Wol\u00b7lust", "Most"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Zum Opfer in der Keuschheit Tempel.", "tokens": ["Zum", "Op\u00b7fer", "in", "der", "Keuschheit", "Tem\u00b7pel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Zeit kommt nimmermehr zur\u00fccke,", "tokens": ["Die", "Zeit", "kommt", "nim\u00b7mer\u00b7mehr", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sie schon einmahl sich verkreucht,", "tokens": ["Wenn", "sie", "schon", "ein\u00b7mahl", "sich", "ver\u00b7kreucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und die Gelegenheit entweicht", "tokens": ["Und", "die", "Ge\u00b7le\u00b7gen\u00b7heit", "ent\u00b7weicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In einem kurzen Augenblicke.", "tokens": ["In", "ei\u00b7nem", "kur\u00b7zen", "Au\u00b7gen\u00b7bli\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wer weis, wer dich in einer Frist", "tokens": ["Wer", "weis", ",", "wer", "dich", "in", "ei\u00b7ner", "Frist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PTKVZ", "$,", "PWS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von vierundzwanzig Wochen k\u00fcst.", "tokens": ["Von", "vie\u00b7rund\u00b7zwan\u00b7zig", "Wo\u00b7chen", "k\u00fcst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie bald kan mich ein Stahl entleiben,", "tokens": ["Wie", "bald", "kan", "mich", "ein", "Stahl", "ent\u00b7lei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dann wird dein angenehmer Mund,", "tokens": ["Dann", "wird", "dein", "an\u00b7ge\u00b7neh\u00b7mer", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der meiner Sehnsucht ofen stund,", "tokens": ["Der", "mei\u00b7ner", "Sehn\u00b7sucht", "o\u00b7fen", "stund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mit andren sich die Zeit vertreiben.", "tokens": ["Mit", "an\u00b7dren", "sich", "die", "Zeit", "ver\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Jedoch soll mich der Tod entrei\u00dfen,", "tokens": ["Je\u00b7doch", "soll", "mich", "der", "Tod", "ent\u00b7rei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du aber meine Leiche sehn,", "tokens": ["Du", "a\u00b7ber", "mei\u00b7ne", "Lei\u00b7che", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So soll mir doch der Wuntsch geschehn,", "tokens": ["So", "soll", "mir", "doch", "der", "Wunt\u00b7sch", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dir in der Gruft getreu zu hei\u00dfen;", "tokens": ["Dir", "in", "der", "Gruft", "ge\u00b7treu", "zu", "hei\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Blut soll dir best\u00e4ndig seyn,", "tokens": ["Mein", "Blut", "soll", "dir", "be\u00b7st\u00e4n\u00b7dig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und meines C\u00f6rpers Leichenstein", "tokens": ["Und", "mei\u00b7nes", "C\u00f6r\u00b7pers", "Lei\u00b7chen\u00b7stein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wird diese Grabschrift nie verlieren:", "tokens": ["Wird", "die\u00b7se", "Grab\u00b7schrift", "nie", "ver\u00b7lie\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hier schl\u00e4ft, mein Kind, dein ander Ich,", "tokens": ["Hier", "schl\u00e4ft", ",", "mein", "Kind", ",", "dein", "an\u00b7der", "Ich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "ADJD", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dem wenig, glaub es sicherlich,", "tokens": ["Dem", "we\u00b7nig", ",", "glaub", "es", "si\u00b7cher\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Den Preis der Redligkeit entf\u00fchren.", "tokens": ["Den", "Preis", "der", "Red\u00b7lig\u00b7keit", "ent\u00b7f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}