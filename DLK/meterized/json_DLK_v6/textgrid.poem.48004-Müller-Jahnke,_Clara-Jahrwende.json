{"textgrid.poem.48004": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Jahrwende", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am altersgrauen Baum der Zeit", "tokens": ["Am", "al\u00b7ters\u00b7grau\u00b7en", "Baum", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ist eine Blume abgebl\u00fcht,", "tokens": ["ist", "ei\u00b7ne", "Blu\u00b7me", "ab\u00b7ge\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und eine Knospe tut sich auf.", "tokens": ["und", "ei\u00b7ne", "Knos\u00b7pe", "tut", "sich", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Menschheit seufzt in gleicher Fron;", "tokens": ["Die", "Menschheit", "seufzt", "in", "glei\u00b7cher", "Fron", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "von ihrer m\u00fcden Stirne f\u00e4llt", "tokens": ["von", "ih\u00b7rer", "m\u00fc\u00b7den", "Stir\u00b7ne", "f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "der Schwei\u00df in Tropfen erdenw\u00e4rts.", "tokens": ["der", "Schwei\u00df", "in", "Trop\u00b7fen", "er\u00b7den\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Ihr Glaube aber tr\u00e4umt im Licht:", "tokens": ["Ihr", "Glau\u00b7be", "a\u00b7ber", "tr\u00e4umt", "im", "Licht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "vor ihren Sehnsuchtsblicken schwimmt", "tokens": ["vor", "ih\u00b7ren", "Sehn\u00b7suchts\u00b7bli\u00b7cken", "schwimmt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "das Morgenrot des neuen Tags.", "tokens": ["das", "Mor\u00b7gen\u00b7rot", "des", "neu\u00b7en", "Tags", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie auch die Kette klirrt und dr\u00fcckt,", "tokens": ["Wie", "auch", "die", "Ket\u00b7te", "klirrt", "und", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "der Zukunft Sturm zerbricht sie doch, \u2013", "tokens": ["der", "Zu\u00b7kunft", "Sturm", "zer\u00b7bricht", "sie", "doch", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "ADV", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und jedes Jahr l\u00f6st einen Ring.", "tokens": ["und", "je\u00b7des", "Jahr", "l\u00f6st", "ei\u00b7nen", "Ring", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und jede Knospe, die erbl\u00fcht", "tokens": ["Und", "je\u00b7de", "Knos\u00b7pe", ",", "die", "er\u00b7bl\u00fcht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "am altersgrauen Baum der Zeit,", "tokens": ["am", "al\u00b7ters\u00b7grau\u00b7en", "Baum", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "birgt einen Keim der k\u00fcnftigen Frucht.", "tokens": ["birgt", "ei\u00b7nen", "Keim", "der", "k\u00fcnf\u00b7ti\u00b7gen", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "So gr\u00fc\u00df ich dich, du neues Jahr;", "tokens": ["So", "gr\u00fc\u00df", "ich", "dich", ",", "du", "neu\u00b7es", "Jahr", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "du junge Knospe tu dich auf,", "tokens": ["du", "jun\u00b7ge", "Knos\u00b7pe", "tu", "dich", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und bl\u00fch' in lichtem Rosenrot!", "tokens": ["und", "bl\u00fch'", "in", "lich\u00b7tem", "Ro\u00b7sen\u00b7rot", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Des Friedens milder Maienwind", "tokens": ["Des", "Frie\u00b7dens", "mil\u00b7der", "Mai\u00b7en\u00b7wind"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "umspiele deinen vollen Scho\u00df,", "tokens": ["um\u00b7spie\u00b7le", "dei\u00b7nen", "vol\u00b7len", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "der Liebe Geist befruchte dich!", "tokens": ["der", "Lie\u00b7be", "Geist", "be\u00b7fruch\u00b7te", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und deine D\u00fcfte gie\u00dfe aus, \u2013", "tokens": ["Und", "dei\u00b7ne", "D\u00fcf\u00b7te", "gie\u00b7\u00dfe", "aus", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mit Bl\u00fctenbl\u00e4ttern kr\u00e4nze du", "tokens": ["mit", "Bl\u00fc\u00b7ten\u00b7bl\u00e4t\u00b7tern", "kr\u00e4n\u00b7ze", "du"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "der Menschheit tiefgefurchte Stirn.", "tokens": ["der", "Menschheit", "tief\u00b7ge\u00b7furch\u00b7te", "Stirn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.9": {"line.1": {"text": "In des Jahrhunderts Niedergang", "tokens": ["In", "des", "Jahr\u00b7hun\u00b7derts", "Nie\u00b7der\u00b7gang"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "sei du ein lichter Zukunftstraum,", "tokens": ["sei", "du", "ein", "lich\u00b7ter", "Zu\u00b7kunft\u00b7straum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "sei du ein Gru\u00df der neuen Zeit!", "tokens": ["sei", "du", "ein", "Gru\u00df", "der", "neu\u00b7en", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}