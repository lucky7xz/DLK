{"dta.poem.13757": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Gebet   .  \n 1810.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df die Ketten mich zerschlagen,", "tokens": ["La\u00df", "die", "Ket\u00b7ten", "mich", "zer\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Frei zum sch\u00f6nen Gottesstreit", "tokens": ["Frei", "zum", "sch\u00f6\u00b7nen", "Got\u00b7tes\u00b7streit"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine hellen Waffen tragen,", "tokens": ["Dei\u00b7ne", "hel\u00b7len", "Waf\u00b7fen", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fr\u00f6hlich beten, herrlich wagen,", "tokens": ["Fr\u00f6h\u00b7lich", "be\u00b7ten", ",", "herr\u00b7lich", "wa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gieb zur Kraft die Freudigkeit!", "tokens": ["Gieb", "zur", "Kraft", "die", "Freu\u00b7dig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}