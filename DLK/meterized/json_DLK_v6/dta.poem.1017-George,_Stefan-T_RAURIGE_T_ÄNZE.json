{"dta.poem.1017": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "T RAURIGE   T \u00c4NZE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Ob deine augen dich trogen               ", "tokens": ["Ob", "dei\u00b7ne", "au\u00b7gen", "dich", "tro\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Durch fallender &#228;ste hauf               ", "tokens": ["Durch", "fal\u00b7len\u00b7der", "&#228;", "ste", "hauf"], "token_info": ["word", "word", "XML_entity", "word", "word"], "pos": ["APPR", "ADJA", "$(", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Treiben die k&#228;mpfenden wogen               ", "tokens": ["Trei\u00b7ben", "die", "k", "&#228;", "mp\u00b7fen\u00b7den", "wo\u00b7gen"], "token_info": ["word", "word", "word", "XML_entity", "word", "word"], "pos": ["NN", "ART", "NN", "$(", "ADJA", "NN"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Den strom hinauf?             ", "tokens": ["Den", "strom", "hin\u00b7auf", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Du jagest nach und sie steigen               ", "tokens": ["Du", "ja\u00b7gest", "nach", "und", "sie", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "KON", "PPER", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von fremden kr&#228;ften erfasst               ", "tokens": ["Von", "frem\u00b7den", "kr", "&#228;", "ften", "er\u00b7fasst"], "token_info": ["word", "word", "word", "XML_entity", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$(", "VVFIN", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wirbelndem rieselndem reigen               ", "tokens": ["Wir\u00b7beln\u00b7dem", "rie\u00b7seln\u00b7dem", "rei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Folgt die begehrende hast             ", "tokens": ["Folgt", "die", "be\u00b7geh\u00b7ren\u00b7de", "hast"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "H&#252;te dich f&#252;hre nicht weiter               ", "tokens": ["H", "&#252;", "te", "dich", "f", "&#252;", "hre", "nicht", "wei\u00b7ter"], "token_info": ["word", "XML_entity", "word", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["XY", "$(", "VVFIN", "PPER", "VVFIN", "$(", "VVFIN", "PTKNEG", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das spiel mit schwerem kauf &#8212;               ", "tokens": ["Das", "spiel", "mit", "schwe\u00b7rem", "kauf", "&#8212;"], "token_info": ["word", "word", "word", "word", "word", "XML_entity"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ziehen nicht deine begleiter               ", "tokens": ["Zie\u00b7hen", "nicht", "dei\u00b7ne", "be\u00b7glei\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Schon ihren alten lauf?             ", "tokens": ["Schon", "ih\u00b7ren", "al\u00b7ten", "lauf", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}