{"textgrid.poem.40819": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein greiser M\u00f6nch schleicht durch des Kreuzgangs Hallen,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein greiser M\u00f6nch schleicht durch des Kreuzgangs Hallen,", "tokens": ["Ein", "grei\u00b7ser", "M\u00f6nch", "schleicht", "durch", "des", "Kreuz\u00b7gangs", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Horch, Fl\u00fcche seiner bleichen Lipp' entwallen,", "tokens": ["Horch", ",", "Fl\u00fc\u00b7che", "sei\u00b7ner", "blei\u00b7chen", "Lipp'", "ent\u00b7wal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie aus zerfall'nen Tempeln in der W\u00fcste", "tokens": ["Wie", "aus", "zer\u00b7fall'\u00b7nen", "Tem\u00b7peln", "in", "der", "W\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Schwarm von Panthern springt mit Mordgel\u00fcste!", "tokens": ["Ein", "Schwarm", "von", "Pan\u00b7thern", "springt", "mit", "Mord\u00b7ge\u00b7l\u00fcs\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich lauscht', und Fluch um Fluch entbot der Alte", "tokens": ["Ich", "lauscht'", ",", "und", "Fluch", "um", "Fluch", "ent\u00b7bot", "der", "Al\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "NN", "APPR", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "All' dem, was heilig, lieb und gro\u00df ich halte;", "tokens": ["All'", "dem", ",", "was", "hei\u00b7lig", ",", "lieb", "und", "gro\u00df", "ich", "hal\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "$,", "PRELS", "ADJD", "$,", "ADJD", "KON", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir war's, als schleudert' er mit Hohn, zerrissen,", "tokens": ["Mir", "wa\u00b7r's", ",", "als", "schleu\u00b7dert'", "er", "mit", "Hohn", ",", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "APPR", "NN", "$,", "VVPP", "$,"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Mir meiner Freuden Bl\u00fcthenkranz zu F\u00fc\u00dfen!", "tokens": ["Mir", "mei\u00b7ner", "Freu\u00b7den", "Bl\u00fc\u00b7then\u00b7kranz", "zu", "F\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Als ob er an der Wand zu Tr\u00fcmmern w\u00fcrfe", "tokens": ["Als", "ob", "er", "an", "der", "Wand", "zu", "Tr\u00fcm\u00b7mern", "w\u00fcr\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "ART", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Goldpokal, draus ich Begeist'rung schl\u00fcrfe!", "tokens": ["Den", "Gold\u00b7po\u00b7kal", ",", "draus", "ich", "Be\u00b7geist'\u00b7rung", "schl\u00fcr\u00b7fe", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als ob der Geifer seines Munds bespeie", "tokens": ["Als", "ob", "der", "Gei\u00b7fer", "sei\u00b7nes", "Munds", "be\u00b7spei\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das heil'ge Banner, dem ich stolz mich reihe!", "tokens": ["Das", "heil'\u00b7ge", "Ban\u00b7ner", ",", "dem", "ich", "stolz", "mich", "rei\u00b7he", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Halt an! \u2013 Mein Schwert sollt' aus der Scheide klirren,", "tokens": ["Halt", "an", "!", "\u2013", "Mein", "Schwert", "sollt'", "aus", "der", "Schei\u00b7de", "klir\u00b7ren", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$.", "$(", "PPOSAT", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Pfeile, z\u00fccht'gend, aus dem K\u00f6cher schwirren,", "tokens": ["Die", "Pfei\u00b7le", ",", "z\u00fccht'\u00b7gend", ",", "aus", "dem", "K\u00f6\u00b7cher", "schwir\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "$,", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn dich die wei\u00dfen Haare nicht, die milden", "tokens": ["Wenn", "dich", "die", "wei\u00b7\u00dfen", "Haa\u00b7re", "nicht", ",", "die", "mil\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PTKNEG", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcrsprecher, deckten mit den Silberschilden!", "tokens": ["F\u00fcr\u00b7spre\u00b7cher", ",", "deck\u00b7ten", "mit", "den", "Sil\u00b7ber\u00b7schil\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sie sind des heil'gen Stromes wei\u00dfe Wellen,", "tokens": ["Sie", "sind", "des", "heil'\u00b7gen", "Stro\u00b7mes", "wei\u00b7\u00dfe", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die sanft ein schroffes Inselhaupt umquellen;", "tokens": ["Die", "sanft", "ein", "schrof\u00b7fes", "In\u00b7sel\u00b7haupt", "um\u00b7quel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Silberlocken Brandung heiligt, schirmet", "tokens": ["Der", "Sil\u00b7berl\u00b7o\u00b7cken", "Bran\u00b7dung", "hei\u00b7ligt", ",", "schir\u00b7met"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Wahnes Tempel selbst, der drauf sich th\u00fcrmet.", "tokens": ["Des", "Wah\u00b7nes", "Tem\u00b7pel", "selbst", ",", "der", "drauf", "sich", "th\u00fcr\u00b7met", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "PRELS", "PAV", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}