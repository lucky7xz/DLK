{"textgrid.poem.67944": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "12. Der verschm\u00e4hete J\u00fcngling", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Umschifft hab' ich Sicilien,", "tokens": ["Um\u00b7schifft", "hab'", "ich", "Si\u00b7ci\u00b7li\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da waren wir M\u00e4nner!", "tokens": ["Da", "wa\u00b7ren", "wir", "M\u00e4n\u00b7ner", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Das braune Schif gieng eilig,", "tokens": ["Das", "brau\u00b7ne", "Schif", "gieng", "ei\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach W\u00fcnschen mit uns M\u00e4nnern!", "tokens": ["Nach", "W\u00fcn\u00b7schen", "mit", "uns", "M\u00e4n\u00b7nern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie da, so hoft ich, sollte", "tokens": ["Wie", "da", ",", "so", "hoft", "ich", ",", "soll\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ADV", "$,", "ADV", "VVFIN", "PPER", "$,", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mein Schif, mir immer laufen; \u2013", "tokens": ["Mein", "Schif", ",", "mir", "im\u00b7mer", "lau\u00b7fen", ";", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und dennoch verschm\u00e4ht mich", "tokens": ["Und", "den\u00b7noch", "ver\u00b7schm\u00e4ht", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Das Ru\u00dfische M\u00e4dchen.", "tokens": ["Das", "Ru\u00b7\u00dfi\u00b7sche", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Schlacht gab es bei Drontheim,", "tokens": ["Schlacht", "gab", "es", "bei", "Dron\u00b7theim", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gr\u00f6sser war ihr Heer da:", "tokens": ["Gr\u00f6s\u00b7ser", "war", "ihr", "Heer", "da", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Das Treffen, das wir gaben,", "tokens": ["Das", "Tref\u00b7fen", ",", "das", "wir", "ga\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "War grausend blutig.", "tokens": ["War", "grau\u00b7send", "blu\u00b7tig", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Gefallen der K\u00f6nig,", "tokens": ["Ge\u00b7fal\u00b7len", "der", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ich nur entkommen \u2013", "tokens": ["Ich", "nur", "ent\u00b7kom\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Und dennoch verschm\u00e4ht mich", "tokens": ["Und", "den\u00b7noch", "ver\u00b7schm\u00e4ht", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Das Ru\u00dfische M\u00e4dchen.", "tokens": ["Das", "Ru\u00b7\u00dfi\u00b7sche", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Sechzehn sassen unser", "tokens": ["Sech\u00b7zehn", "sas\u00b7sen", "un\u00b7ser"], "token_info": ["word", "word", "word"], "pos": ["CARD", "VVFIN", "PPOSAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Auf vier Ruderb\u00e4nken:", "tokens": ["Auf", "vier", "Ru\u00b7der\u00b7b\u00e4n\u00b7ken", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Des Meeres Sturm ward grimmig,", "tokens": ["Des", "Mee\u00b7res", "Sturm", "ward", "grim\u00b7mig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Schif ersank im Wasser:", "tokens": ["Das", "Schif", "er\u00b7sank", "im", "Was\u00b7ser", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wir sch\u00f6pften alle freudig;", "tokens": ["Wir", "sch\u00f6pf\u00b7ten", "al\u00b7le", "freu\u00b7dig", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So sollts immer gehen; \u2013", "tokens": ["So", "sollts", "im\u00b7mer", "ge\u00b7hen", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "ADV", "VVINF", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Und dennoch verschm\u00e4ht mich", "tokens": ["Und", "den\u00b7noch", "ver\u00b7schm\u00e4ht", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Das Ru\u00dfische M\u00e4dchen.", "tokens": ["Das", "Ru\u00b7\u00dfi\u00b7sche", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "K\u00fcnste kann ich achte,", "tokens": ["K\u00fcns\u00b7te", "kann", "ich", "ach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wei\u00df tapfer zu fechten,", "tokens": ["Wei\u00df", "tap\u00b7fer", "zu", "fech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Edel zu reiten,", "tokens": ["E\u00b7del", "zu", "rei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zu schwimmen k\u00fcnstlich,", "tokens": ["Zu", "schwim\u00b7men", "k\u00fcnst\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Schrittschuh, zu laufen,", "tokens": ["Schritt\u00b7schuh", ",", "zu", "lau\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Zu schleudern, zu rudern \u2013", "tokens": ["Zu", "schleu\u00b7dern", ",", "zu", "ru\u00b7dern", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Und dennoch verschm\u00e4ht mich", "tokens": ["Und", "den\u00b7noch", "ver\u00b7schm\u00e4ht", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Das Ru\u00dfische M\u00e4dchen.", "tokens": ["Das", "Ru\u00b7\u00dfi\u00b7sche", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "M\u00e4dchen oder Wittwe! \u2013", "tokens": ["M\u00e4d\u00b7chen", "o\u00b7der", "Witt\u00b7we", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "NN", "$.", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Als fern im Ostland", "tokens": ["Als", "fern", "im", "Ost\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Warme Schlacht wir gaben;", "tokens": ["War\u00b7me", "Schlacht", "wir", "ga\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Da dr\u00e4ngt ich fr\u00fch zur Stadt hin,", "tokens": ["Da", "dr\u00e4ngt", "ich", "fr\u00fch", "zur", "Stadt", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Brauchte frisch die Waffen,", "tokens": ["Brauch\u00b7te", "frisch", "die", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Da sind noch unsre Spuren \u2013", "tokens": ["Da", "sind", "noch", "uns\u00b7re", "Spu\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und dennoch verschm\u00e4ht mich", "tokens": ["Und", "den\u00b7noch", "ver\u00b7schm\u00e4ht", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Das Ru\u00dfische M\u00e4dchen.", "tokens": ["Das", "Ru\u00b7\u00dfi\u00b7sche", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Gebohren an den K\u00fcsten", "tokens": ["Ge\u00b7boh\u00b7ren", "an", "den", "K\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo sie Bogen spannen,", "tokens": ["Wo", "sie", "Bo\u00b7gen", "span\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Trieb ich Feindes Schiffe", "tokens": ["Trieb", "ich", "Fein\u00b7des", "Schif\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Oft auf Meeres Klippen,", "tokens": ["Oft", "auf", "Mee\u00b7res", "Klip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ackert fern von Menschen", "tokens": ["A\u00b7ckert", "fern", "von", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJD", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Das Meer allein mit Rudern \u2013", "tokens": ["Das", "Meer", "al\u00b7lein", "mit", "Ru\u00b7dern", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und dennoch verschm\u00e4ht mich", "tokens": ["Und", "den\u00b7noch", "ver\u00b7schm\u00e4ht", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Das Ru\u00dfische M\u00e4dchen.", "tokens": ["Das", "Ru\u00b7\u00dfi\u00b7sche", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}