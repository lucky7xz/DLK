{"textgrid.poem.53578": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Er verheiratet sie", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Reicht mir den Kranz, reicht mir den Myrtenschleier!", "tokens": ["Reicht", "mir", "den", "Kranz", ",", "reicht", "mir", "den", "Myr\u00b7ten\u00b7schlei\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Der Unschuld gr\u00fcnes Kr\u00e4nzlein tragt herbei!", "tokens": ["Der", "Un\u00b7schuld", "gr\u00fc\u00b7nes", "Kr\u00e4nz\u00b7lein", "tragt", "her\u00b7bei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So schick ich Clairen an Direktor Meyer \u2013 \u2013", "tokens": ["So", "schick", "ich", "Clai\u00b7ren", "an", "Di\u00b7rek\u00b7tor", "Me\u00b7yer", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "NN", "NE", "$(", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "(mitgift anbei).", "tokens": ["(", "mit\u00b7gift", "an\u00b7bei", ")", "."], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "FM.la", "FM.la", "$(", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Bedenk: Du schreitest nun ins wilde Leben!", "tokens": ["Be\u00b7denk", ":", "Du", "schrei\u00b7test", "nun", "ins", "wil\u00b7de", "Le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbzum ersten Mal\u00ab \u2013 ein schwerer Schritt, mein Kind!", "tokens": ["\u00bb", "zum", "ers\u00b7ten", "Mal", "\u00ab", "\u2013", "ein", "schwe\u00b7rer", "Schritt", ",", "mein", "Kind", "!"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "APPRART", "ADJA", "NN", "$(", "$(", "ART", "ADJA", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was fr\u00fcher war, Gott, das vergi\u00dft man eben . . .", "tokens": ["Was", "fr\u00fc\u00b7her", "war", ",", "Gott", ",", "das", "ver\u00b7gi\u00dft", "man", "e\u00b7ben", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "NN", "$,", "PDS", "VVFIN", "PIS", "ADV", "$.", "$.", "$."], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "und er \u2013", "tokens": ["und", "er", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["KON", "PPER", "$("], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "ist blind.", "tokens": ["ist", "blind", "."], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Sein Tastsinn sei ein wenig unentwickelt,", "tokens": ["Sein", "Tast\u00b7sinn", "sei", "ein", "we\u00b7nig", "un\u00b7ent\u00b7wi\u00b7ckelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und tobt er br\u00fcllend wie ein br\u00fcnstiger Leu:", "tokens": ["und", "tobt", "er", "br\u00fcl\u00b7lend", "wie", "ein", "br\u00fcns\u00b7ti\u00b7ger", "Leu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "dann gl\u00e4nzt die liebe Unschuld frisch vernickelt", "tokens": ["dann", "gl\u00e4nzt", "die", "lie\u00b7be", "Un\u00b7schuld", "frisch", "ver\u00b7ni\u00b7ckelt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "so gut wie neu . . .", "tokens": ["so", "gut", "wie", "neu", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADJD", "$.", "$.", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "So zeuch denn hin, du liebe Maculata!", "tokens": ["So", "zeuch", "denn", "hin", ",", "du", "lie\u00b7be", "Ma\u00b7cu\u00b7la\u00b7ta", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "PTKVZ", "$,", "PPER", "VVFIN", "NE", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zart \u00fcberhaucht von br\u00e4utlich rosa Gl\u00fcck . . .", "tokens": ["Zart", "\u00fc\u00b7ber\u00b7haucht", "von", "br\u00e4ut\u00b7lich", "ro\u00b7sa", "Gl\u00fcck", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ADJD", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich h\u00e4tt gelacht? Todernst. Wie eine Fata", "tokens": ["Ich", "h\u00e4tt", "ge\u00b7lacht", "?", "To\u00b7dernst", ".", "Wie", "ei\u00b7ne", "Fa\u00b7ta"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "NN", "$.", "PWAV", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Morgana verschwindest du \u2013 \u2013", "tokens": ["Mor\u00b7ga\u00b7na", "ver\u00b7schwin\u00b7dest", "du", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "PPER", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "ich gr\u00fc\u00dfe leicht zur\u00fcck.", "tokens": ["ich", "gr\u00fc\u00b7\u00dfe", "leicht", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}