{"textgrid.poem.65982": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie manches Weib umfing ich schon in meinen Tr\u00e4umen,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie manches Weib umfing ich schon in meinen Tr\u00e4umen,", "tokens": ["Wie", "man\u00b7ches", "Weib", "um\u00b7fing", "ich", "schon", "in", "mei\u00b7nen", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das zu umarmen lie\u00df am Tag die Scheu mich s\u00e4umen.", "tokens": ["Das", "zu", "um\u00b7ar\u00b7men", "lie\u00df", "am", "Tag", "die", "Scheu", "mich", "s\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKZU", "VVINF", "VVFIN", "APPRART", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gelegenheit verflog, die Frucht blieb ungepfl\u00fcckt,", "tokens": ["Ge\u00b7le\u00b7gen\u00b7heit", "ver\u00b7flog", ",", "die", "Frucht", "blieb", "un\u00b7ge\u00b7pfl\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was half's, dass mich im Schlaf ihr Schattenbild begl\u00fcckt?", "tokens": ["Was", "ha\u00b7lf's", ",", "dass", "mich", "im", "Schlaf", "ihr", "Schat\u00b7ten\u00b7bild", "be\u00b7gl\u00fcckt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "APPRART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "Dich aber sah noch nie im Traum ich, wie im Wachen.", "tokens": ["Dich", "a\u00b7ber", "sah", "noch", "nie", "im", "Traum", "ich", ",", "wie", "im", "Wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ADV", "APPRART", "NN", "PPER", "$,", "PWAV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wo kamst du Hohe her, von welchem Strand den Nachen", "tokens": ["Wo", "kamst", "du", "Ho\u00b7he", "her", ",", "von", "wel\u00b7chem", "Strand", "den", "Na\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "NE", "PTKVZ", "$,", "APPR", "PWAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Triebst du durchs tiefe Blau des L\u00fcfteozeans?", "tokens": ["Triebst", "du", "durchs", "tie\u00b7fe", "Blau", "des", "L\u00fcf\u00b7teo\u00b7ze\u00b7ans", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Ein bl\u00e4ulich bleiches Licht war Herold deines Nah'ns.", "tokens": ["Ein", "bl\u00e4u\u00b7lich", "blei\u00b7ches", "Licht", "war", "He\u00b7rold", "dei\u00b7nes", "Nah'ns", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VAFIN", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ein leises Zittern ging vor dir durchs \u00c4thermeer,", "tokens": ["Ein", "lei\u00b7ses", "Zit\u00b7tern", "ging", "vor", "dir", "durchs", "\u00c4\u00b7ther\u00b7meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dann schwebtest du heran, ein Leuchten um dich her.", "tokens": ["Dann", "schweb\u00b7test", "du", "he\u00b7ran", ",", "ein", "Leuch\u00b7ten", "um", "dich", "her", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wer bist du, stolz Gebild, im Sternenfunkelkranz?", "tokens": ["Wer", "bist", "du", ",", "stolz", "Ge\u00b7bild", ",", "im", "Ster\u00b7nen\u00b7fun\u00b7kel\u00b7kranz", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "ADJD", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dein Leib \u2013 Licht oder Luft? \u2013 schien nur durchsichtiger Glanz,", "tokens": ["Dein", "Leib", "\u2013", "Licht", "o\u00b7der", "Luft", "?", "\u2013", "schien", "nur", "durch\u00b7sich\u00b7ti\u00b7ger", "Glanz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "NN", "KON", "NN", "$.", "$(", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-++--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Und doch hob sich mein Arm, ihn br\u00fcnstig zu umfangen.", "tokens": ["Und", "doch", "hob", "sich", "mein", "Arm", ",", "ihn", "br\u00fcns\u00b7tig", "zu", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$,", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Bist Schein du nur, ein Trug, was weckst du mein Verlangen?", "tokens": ["Bist", "Schein", "du", "nur", ",", "ein", "Trug", ",", "was", "weckst", "du", "mein", "Ver\u00b7lan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPER", "ADV", "$,", "ART", "NN", "$,", "PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Vor deinem Angesicht m\u00fcsst' sich ein Engel beugen,", "tokens": ["Vor", "dei\u00b7nem", "An\u00b7ge\u00b7sicht", "m\u00fcsst'", "sich", "ein", "En\u00b7gel", "beu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die reinere Himmelsglut dir demutvoll bezeugen.", "tokens": ["Die", "rei\u00b7ne\u00b7re", "Him\u00b7mels\u00b7glut", "dir", "de\u00b7mut\u00b7voll", "be\u00b7zeu\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Doch schickt von seinem Thron des Fl\u00fcgelheers Befehler", "tokens": ["Doch", "schickt", "von", "sei\u00b7nem", "Thron", "des", "Fl\u00fc\u00b7gel\u00b7heers", "Be\u00b7feh\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die frommen Boten als Verf\u00fchrer aus und Qu\u00e4ler?", "tokens": ["Die", "from\u00b7men", "Bo\u00b7ten", "als", "Ver\u00b7f\u00fch\u00b7rer", "aus", "und", "Qu\u00e4\u00b7ler", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KOUS", "NN", "PTKVZ", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "H\u00e4tt' Satan dich gezeugt, k\u00e4mst du von seinem Hofe,", "tokens": ["H\u00e4tt'", "Sa\u00b7tan", "dich", "ge\u00b7zeugt", ",", "k\u00e4mst", "du", "von", "sei\u00b7nem", "Ho\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "VVPP", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der H\u00f6lle listig Kind, der S\u00fcnde saubere Zofe?", "tokens": ["Der", "H\u00f6l\u00b7le", "lis\u00b7tig", "Kind", ",", "der", "S\u00fcn\u00b7de", "sau\u00b7be\u00b7re", "Zo\u00b7fe", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Nun lauf' ich durch den Tag ein wacher Tr\u00e4umer hin,", "tokens": ["Nun", "lauf'", "ich", "durch", "den", "Tag", "ein", "wa\u00b7cher", "Tr\u00e4u\u00b7mer", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Begierdekrank das Blut, vergiftet jeder Sinn.", "tokens": ["Be\u00b7gier\u00b7de\u00b7krank", "das", "Blut", ",", "ver\u00b7gif\u00b7tet", "je\u00b7der", "Sinn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wie eine Melodie uns peinigt und nicht scheidet,", "tokens": ["Wie", "ei\u00b7ne", "Me\u00b7lo\u00b7die", "uns", "pei\u00b7nigt", "und", "nicht", "schei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVFIN", "KON", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So l\u00e4sst dein Bild mich nicht, das alles mir verleidet.", "tokens": ["So", "l\u00e4sst", "dein", "Bild", "mich", "nicht", ",", "das", "al\u00b7les", "mir", "ver\u00b7lei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "PTKNEG", "$,", "PRELS", "PIS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Schlie\u00df' ich die Augen, stehst du vor mir, herrlich Weib,", "tokens": ["Schlie\u00df'", "ich", "die", "Au\u00b7gen", ",", "stehst", "du", "vor", "mir", ",", "herr\u00b7lich", "Weib", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "VVFIN", "PPER", "APPR", "PPER", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ge\u00f6ffnet suchen sie in jeder Dirn den Leib,", "tokens": ["Ge\u00b7\u00f6ff\u00b7net", "su\u00b7chen", "sie", "in", "je\u00b7der", "Dirn", "den", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der so mich hat entbrannt, und wenden ekel sich,", "tokens": ["Der", "so", "mich", "hat", "ent\u00b7brannt", ",", "und", "wen\u00b7den", "e\u00b7kel", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VAFIN", "VVPP", "$,", "KON", "VVFIN", "ADJD", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wenn dir die sch\u00f6nste selbst wie Nacht dem Tage glich.", "tokens": ["Wenn", "dir", "die", "sch\u00f6ns\u00b7te", "selbst", "wie", "Nacht", "dem", "Ta\u00b7ge", "glich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "ADV", "KOKOM", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der heilige Anton war wahrlich schlimm daran,", "tokens": ["Der", "hei\u00b7li\u00b7ge", "An\u00b7ton", "war", "wahr\u00b7lich", "schlimm", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VAFIN", "ADV", "ADJD", "PAV", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.30": {"text": "Ihn griff der Teufel mit verst\u00e4rkten Kr\u00e4ften an.", "tokens": ["Ihn", "griff", "der", "Teu\u00b7fel", "mit", "ver\u00b7st\u00e4rk\u00b7ten", "Kr\u00e4f\u00b7ten", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "War eine einzige nur von jenen Huldgestalten", "tokens": ["War", "ei\u00b7ne", "ein\u00b7zi\u00b7ge", "nur", "von", "je\u00b7nen", "Huld\u00b7ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "So sch\u00f6n wie du, woher kam soviel Kraft dem Alten?", "tokens": ["So", "sch\u00f6n", "wie", "du", ",", "wo\u00b7her", "kam", "so\u00b7viel", "Kraft", "dem", "Al\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "$,", "PWAV", "VVFIN", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ich h\u00e4tte schwerlich wohl so standhaft k\u00f6nnen sein,", "tokens": ["Ich", "h\u00e4t\u00b7te", "schwer\u00b7lich", "wohl", "so", "stand\u00b7haft", "k\u00f6n\u00b7nen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "ADV", "ADJD", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "H\u00e4tt' Keuschheit eingeb\u00fc\u00dft dabei und Heiligenschein.", "tokens": ["H\u00e4tt'", "Keuschheit", "ein\u00b7ge\u00b7b\u00fc\u00dft", "da\u00b7bei", "und", "Hei\u00b7li\u00b7gen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "VVFIN", "PAV", "KON", "NN", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}}}}}