{"dta.poem.147": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Sie sucht durch seine Verbannung bey der  \n  Venus  Gnade.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Geh fort/ Verfluchter geh/ fort geh mir aus den Augen/", "tokens": ["Geh", "fort", "/", "Ver\u00b7fluch\u00b7ter", "geh", "/", "fort", "geh", "mir", "aus", "den", "Au\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$(", "NN", "VVFIN", "$(", "PTKVZ", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich kan/ und will dich nicht hinf\u00fchro vor mir sehn/", "tokens": ["Ich", "kan", "/", "und", "will", "dich", "nicht", "hin\u00b7f\u00fch\u00b7ro", "vor", "mir", "sehn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "KON", "VMFIN", "PPER", "PTKNEG", "ADV", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein Schmeicheln kan zu nichts als zum Verderben taugen", "tokens": ["Dein", "Schmei\u00b7cheln", "kan", "zu", "nichts", "als", "zum", "Ver\u00b7der\u00b7ben", "tau\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "APPR", "PIS", "KOKOM", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer dir Geh\u00f6re giebt/ mit solchem ists geschehn.", "tokens": ["Wer", "dir", "Ge\u00b7h\u00f6\u00b7re", "giebt", "/", "mit", "sol\u00b7chem", "ists", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "VVFIN", "$(", "APPR", "PIAT", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich mu\u00df der ", "tokens": ["Ich", "mu\u00df", "der"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Dein F\u00fcrwitz hat mich in mein Ungel\u00fcck gebracht", "tokens": ["Dein", "F\u00fcr\u00b7witz", "hat", "mich", "in", "mein", "Un\u00b7ge\u00b7l\u00fcck", "ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Allein du solt davor mein Angesicht vermeiden/", "tokens": ["Al\u00b7lein", "du", "solt", "da\u00b7vor", "mein", "An\u00b7ge\u00b7sicht", "ver\u00b7mei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PAV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich gebe dir hiemit auf ewig gute Nacht.", "tokens": ["Ich", "ge\u00b7be", "dir", "hie\u00b7mit", "auf", "e\u00b7wig", "gu\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PAV", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du solt mein Angesicht hinf\u00fchre nicht mehr schauen/", "tokens": ["Du", "solt", "mein", "An\u00b7ge\u00b7sicht", "hin\u00b7f\u00fch\u00b7re", "nicht", "mehr", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVFIN", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich geb dir keinen Blick/ ich achte dich nicht mehr/", "tokens": ["Ich", "geb", "dir", "kei\u00b7nen", "Blick", "/", "ich", "ach\u00b7te", "dich", "nicht", "mehr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Du magst dein Elend nun als ein Versto\u00dfner bauen/", "tokens": ["Du", "magst", "dein", "E\u00b7lend", "nun", "als", "ein", "Ver\u00b7sto\u00df\u00b7ner", "bau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "ADV", "KOUS", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und wenn ich dich geliebt/ so ha\u00df/ ich dich nun sehr.", "tokens": ["Und", "wenn", "ich", "dich", "ge\u00b7liebt", "/", "so", "ha\u00df", "/", "ich", "dich", "nun", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVPP", "$(", "ADV", "VVFIN", "$(", "PPER", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dein Lob hat es gemacht/ da\u00df ich mich drob vergangen/", "tokens": ["Dein", "Lob", "hat", "es", "ge\u00b7macht", "/", "da\u00df", "ich", "mich", "drob", "ver\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$(", "KOUS", "PPER", "PRF", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Du hast mich zu den Stoltz und Aberwitz bewegt;", "tokens": ["Du", "hast", "mich", "zu", "den", "Stoltz", "und", "A\u00b7ber\u00b7witz", "be\u00b7wegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich zierte wie ein Pfau die auffgebla\u00dfnen Wangen", "tokens": ["Ich", "zier\u00b7te", "wie", "ein", "Pfau", "die", "auff\u00b7ge\u00b7bla\u00df\u00b7nen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die Brust hat einen Stoltz/ der all zu gro\u00df/ gehegt.", "tokens": ["Die", "Brust", "hat", "ei\u00b7nen", "Stoltz", "/", "der", "all", "zu", "gro\u00df", "/", "ge\u00b7hegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$(", "ART", "PIAT", "PTKA", "ADJD", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Allein wie lie\u00df ich bald die stoltzen Fl\u00fcgel fallen", "tokens": ["Al\u00b7lein", "wie", "lie\u00df", "ich", "bald", "die", "stolt\u00b7zen", "Fl\u00fc\u00b7gel", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Als ", "tokens": ["Als"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.19": {"text": "Ich solte fort von ihr ins herbe Elend wallen", "tokens": ["Ich", "sol\u00b7te", "fort", "von", "ihr", "ins", "her\u00b7be", "E\u00b7lend", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKVZ", "APPR", "PPER", "APPRART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die Straffe wurde mir mit Rechte zuerkannt.", "tokens": ["Die", "Straf\u00b7fe", "wur\u00b7de", "mir", "mit", "Rech\u00b7te", "zu\u00b7er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Doch ihre G\u00fctigkeit/ die lie\u00df mich Gnade finden", "tokens": ["Doch", "ih\u00b7re", "G\u00fc\u00b7tig\u00b7keit", "/", "die", "lie\u00df", "mich", "Gna\u00b7de", "fin\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$(", "ART", "VVFIN", "PPER", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Mir solte mein Versehn nicht zugerechnet seyn/", "tokens": ["Mir", "sol\u00b7te", "mein", "Ver\u00b7sehn", "nicht", "zu\u00b7ge\u00b7rech\u00b7net", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Sie wolte keine Ruht\u2019 vor meinen R\u00fccken binden/", "tokens": ["Sie", "wol\u00b7te", "kei\u00b7ne", "Ruht'", "vor", "mei\u00b7nen", "R\u00fc\u00b7cken", "bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wenn ich insk\u00fcnfftige das Unrecht w\u00fcrde scheun.", "tokens": ["Wenn", "ich", "ins\u00b7k\u00fcnff\u00b7ti\u00b7ge", "das", "Un\u00b7recht", "w\u00fcr\u00b7de", "scheun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ich werde diese Gunst so leichte nicht verschertzen/", "tokens": ["Ich", "wer\u00b7de", "die\u00b7se", "Gunst", "so", "leich\u00b7te", "nicht", "ver\u00b7schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDAT", "NN", "ADV", "ADJA", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Verfluchter geh nur fort/ du hast mich einst gest\u00fcrtzt/", "tokens": ["Ver\u00b7fluch\u00b7ter", "geh", "nur", "fort", "/", "du", "hast", "mich", "einst", "ge\u00b7st\u00fcrtzt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PTKVZ", "$(", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Allein dein Frevel soll dich in der Seelen schmertzen", "tokens": ["Al\u00b7lein", "dein", "Fre\u00b7vel", "soll", "dich", "in", "der", "See\u00b7len", "schmert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "PRF", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Verachtung hat das Band der Liebe abgek\u00fcrtzt.", "tokens": ["Ver\u00b7ach\u00b7tung", "hat", "das", "Band", "der", "Lie\u00b7be", "ab\u00b7ge\u00b7k\u00fcrtzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ich werde dich hinfort mit keinem Blicke lieben/", "tokens": ["Ich", "wer\u00b7de", "dich", "hin\u00b7fort", "mit", "kei\u00b7nem", "Bli\u00b7cke", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Du bist nun gantz und gar aus meiner Gunst verbannt.", "tokens": ["Du", "bist", "nun", "gantz", "und", "gar", "aus", "mei\u00b7ner", "Gunst", "ver\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "KON", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Du bist nun in das Buch der Feinde eingeschrieben/", "tokens": ["Du", "bist", "nun", "in", "das", "Buch", "der", "Fein\u00b7de", "ein\u00b7ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ich habe nun mein Hertz gantz von dir weggewandt.", "tokens": ["Ich", "ha\u00b7be", "nun", "mein", "Hertz", "gantz", "von", "dir", "weg\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ach ", "tokens": ["Ach"], "token_info": ["word"], "pos": ["ITJ"], "meter": "+", "measure": "single.up"}, "line.34": {"text": "Mit Schaam und Furcht erf\u00fcllt/ die Augen weinen Blut/", "tokens": ["Mit", "Schaam", "und", "Furcht", "er\u00b7f\u00fcllt", "/", "die", "Au\u00b7gen", "wei\u00b7nen", "Blut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Das matte Hertz zerspringt/ dis sind die Reue-Fr\u00fcchte/", "tokens": ["Das", "mat\u00b7te", "Hertz", "zer\u00b7springt", "/", "dis", "sind", "die", "Reu\u00b7e\u00b7Fr\u00fcch\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$(", "PDS", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Zu dem so bin ich auch dem Buhler nicht mehr gut.", "tokens": ["Zu", "dem", "so", "bin", "ich", "auch", "dem", "Buh\u00b7ler", "nicht", "mehr", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ich werde deine Macht gantz unterth\u00e4nig ehren/", "tokens": ["Ich", "wer\u00b7de", "dei\u00b7ne", "Macht", "gantz", "un\u00b7ter\u00b7th\u00e4\u00b7nig", "eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ich k\u00fcsse K\u00f6nigin Fu\u00df-f\u00e4llig deinen Fu\u00df/", "tokens": ["Ich", "k\u00fcs\u00b7se", "K\u00f6\u00b7ni\u00b7gin", "Fu\u00df\u00b7f\u00e4l\u00b7lig", "dei\u00b7nen", "Fu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "NE", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ich bitte/ da\u00df du magst den Zorn in Gnade kehren/", "tokens": ["Ich", "bit\u00b7te", "/", "da\u00df", "du", "magst", "den", "Zorn", "in", "Gna\u00b7de", "keh\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "VMFIN", "ART", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Denn sieh\u2019 ich thue ja mit wahrer Reue Bu\u00df.", "tokens": ["Denn", "sieh'", "ich", "thue", "ja", "mit", "wah\u00b7rer", "Reu\u00b7e", "Bu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}}}}