{"textgrid.poem.47012": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kleide dich mit einem sch\u00f6nen Kleide,", "tokens": ["Ich", "klei\u00b7de", "dich", "mit", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Darin du sollst wie eine F\u00fcrstin prangen;", "tokens": ["Da\u00b7rin", "du", "sollst", "wie", "ei\u00b7ne", "F\u00fcrs\u00b7tin", "pran\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VMFIN", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Lieb' ist das Kleid, das rings dich soll umfangen;", "tokens": ["Lieb'", "ist", "das", "Kleid", ",", "das", "rings", "dich", "soll", "um\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wen Liebe schm\u00fcckt, bedarf der Gold und Seide?", "tokens": ["Wen", "Lie\u00b7be", "schm\u00fcckt", ",", "be\u00b7darf", "der", "Gold", "und", "Sei\u00b7de", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich schm\u00fccke dich mit k\u00f6stlichem Geschmeide,", "tokens": ["Ich", "schm\u00fc\u00b7cke", "dich", "mit", "k\u00f6st\u00b7li\u00b7chem", "Ge\u00b7schmei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das um dich soll in goldner Windung hangen;", "tokens": ["Das", "um", "dich", "soll", "in", "gold\u00b7ner", "Win\u00b7dung", "han\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPER", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Goldgeschmeid' ist Hoffnung und Verlangen,", "tokens": ["Das", "Gold\u00b7ge\u00b7schmeid'", "ist", "Hoff\u00b7nung", "und", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie sind der Liebe goldne Kettlein beide.", "tokens": ["Sie", "sind", "der", "Lie\u00b7be", "gold\u00b7ne", "Ket\u00b7tlein", "bei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "PIS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich bau' dir eine sanftgew\u00f6lbte H\u00fctte,", "tokens": ["Ich", "bau'", "dir", "ei\u00b7ne", "sanft\u00b7ge\u00b7w\u00f6lb\u00b7te", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verschlungen aus dem Schatten dreier \u00c4ste,", "tokens": ["Ver\u00b7schlun\u00b7gen", "aus", "dem", "Schat\u00b7ten", "drei\u00b7er", "\u00c4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die drei sind Treue, G'n\u00fcgsamkeit und Sitte.", "tokens": ["Die", "drei", "sind", "Treu\u00b7e", ",", "G'\u00b7n\u00fcg\u00b7sam\u00b7keit", "und", "Sit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "VAFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Und wenn du mit mir willst zum stillen Feste", "tokens": ["Und", "wenn", "du", "mit", "mir", "willst", "zum", "stil\u00b7len", "Fes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPER", "VMFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Einziehn und wohnen in des H\u00fcttleins Mitte,", "tokens": ["Ein\u00b7ziehn", "und", "woh\u00b7nen", "in", "des", "H\u00fctt\u00b7leins", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So wird es uns zum sch\u00f6nsten der Pal\u00e4ste.", "tokens": ["So", "wird", "es", "uns", "zum", "sch\u00f6ns\u00b7ten", "der", "Pa\u00b7l\u00e4s\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "APPRART", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}