{"dta.poem.21722": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Edmunds Nachtgesang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nein, es ist kein t&#228;uschend Sehnen,", "tokens": ["Nein", ",", "es", "ist", "kein", "t", "&#228;", "usc\u00b7hend", "Seh\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PIAT", "NN", "$(", "ADJD", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Nein, mich neckt kein eitler Traum.", "tokens": ["Nein", ",", "mich", "neckt", "kein", "eit\u00b7ler", "Traum", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wohl vermag ich Seyn und W\u00e4hnen,", "tokens": ["Wohl", "ver\u00b7mag", "ich", "Seyn", "und", "W\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wohl zu scheiden Zeit und Raum.", "tokens": ["Wohl", "zu", "schei\u00b7den", "Zeit", "und", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Pr\u00e4gt nicht itzt noch diesen Boden", "tokens": ["Pr\u00e4gt", "nicht", "itzt", "noch", "die\u00b7sen", "Bo\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ADV", "ADV", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihres Trittes Rehenspur?", "tokens": ["Ih\u00b7res", "Trit\u00b7tes", "Re\u00b7hen\u00b7spur", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00fcrzt nicht ihr Ambrosiaodem", "tokens": ["W\u00fcrzt", "nicht", "ihr", "A\u00b7mbro\u00b7si\u00b7a\u00b7o\u00b7dem"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Rings die amaranthne Flur?", "tokens": ["Rings", "die", "a\u00b7ma\u00b7ranth\u00b7ne", "Flur", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "F\u00fchlt' ich nicht, wie leis' und bange", "tokens": ["F\u00fchlt'", "ich", "nicht", ",", "wie", "leis'", "und", "ban\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "ADJD", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich ihr Lilienarm umwand?", "tokens": ["Mich", "ihr", "Li\u00b7li\u00b7en\u00b7arm", "um\u00b7wand", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Flammt nicht noch auf dieser Wange", "tokens": ["Flammt", "nicht", "noch", "auf", "die\u00b7ser", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihrer Wange keuscher Brand?", "tokens": ["Ih\u00b7rer", "Wan\u00b7ge", "keu\u00b7scher", "Brand", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bin ich nicht des Weins noch trunken,", "tokens": ["Bin", "ich", "nicht", "des", "Weins", "noch", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der auf ihren Lippen gl\u00fcht?", "tokens": ["Der", "auf", "ih\u00b7ren", "Lip\u00b7pen", "gl\u00fcht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dessen Gluthstrom Lebensfunken", "tokens": ["Des\u00b7sen", "Gluth\u00b7strom", "Le\u00b7bens\u00b7fun\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mir durch Mark und Adern spr\u00fcht?", "tokens": ["Mir", "durch", "Mark", "und", "A\u00b7dern", "spr\u00fcht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sch\u00e4umt nicht noch der Becher \u00fcber,", "tokens": ["Sch\u00e4umt", "nicht", "noch", "der", "Be\u00b7cher", "\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "ART", "NN", "APPR", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dess ich bis zum Taumeln trank?", "tokens": ["Dess", "ich", "bis", "zum", "Tau\u00b7meln", "trank", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bebt nicht noch in Nerv' und Fiber", "tokens": ["Bebt", "nicht", "noch", "in", "Ner\u00b7v'", "und", "Fi\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ADV", "APPR", "NE", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Des Entz\u00fcckens \u00dcberschwang?", "tokens": ["Des", "Ent\u00b7z\u00fc\u00b7ckens", "\u00dc\u00b7bersc\u00b7hwang", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nein, mich tr\u00fcgt kein t\u00e4uschend Sehnen;", "tokens": ["Nein", ",", "mich", "tr\u00fcgt", "kein", "t\u00e4u\u00b7schend", "Seh\u00b7nen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PIAT", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nein, mich neckt kein nicht'ger Traum.", "tokens": ["Nein", ",", "mich", "neckt", "kein", "nicht'\u00b7ger", "Traum", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Noch vermag ich Seyn und W\u00e4hnen,", "tokens": ["Noch", "ver\u00b7mag", "ich", "Seyn", "und", "W\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Noch zu scheiden Zeit und Raum.", "tokens": ["Noch", "zu", "schei\u00b7den", "Zeit", "und", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und so w\u00e4r' ein Kranz errungen,", "tokens": ["Und", "so", "w\u00e4r'", "ein", "Kranz", "er\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie er keinen noch gekr\u00f6nt?", "tokens": ["Wie", "er", "kei\u00b7nen", "noch", "ge\u00b7kr\u00f6nt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die M\u00f6re w\u00e4r' bezwungen,", "tokens": ["Und", "die", "M\u00f6\u00b7re", "w\u00e4r'", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Nemesis vers\u00f6hnt?", "tokens": ["Und", "die", "Ne\u00b7me\u00b7sis", "ver\u00b7s\u00f6hnt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihn, den Matten, ihn, den Kranken,", "tokens": ["Ihn", ",", "den", "Mat\u00b7ten", ",", "ihn", ",", "den", "Kran\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$,", "PPER", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lezte Labsal, reich und k\u00fchl,", "tokens": ["Lez\u00b7te", "Lab\u00b7sal", ",", "reich", "und", "k\u00fchl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und nach k\u00fchn durchmessnen Schranken", "tokens": ["Und", "nach", "k\u00fchn", "durch\u00b7mess\u00b7nen", "Schran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "W\u00e4r' erreicht der Ziele Ziel?", "tokens": ["W\u00e4r'", "er\u00b7reicht", "der", "Zie\u00b7le", "Ziel", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Dennoch h\u00fcllt mich leise Wehmuth,", "tokens": ["Den\u00b7noch", "h\u00fcllt", "mich", "lei\u00b7se", "Weh\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich umflort Melancholie.", "tokens": ["Mich", "um\u00b7flort", "Me\u00b7lan\u00b7cho\u00b7lie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich versink' in Schaam und Demuth.", "tokens": ["Ich", "ver\u00b7sink'", "in", "Schaam", "und", "De\u00b7muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Edle, dich verdien' ich nie \u2014 \u2014", "tokens": ["Ed\u00b7le", ",", "dich", "ver\u00b7dien'", "ich", "nie"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "$,", "PPER", "VVFIN", "PPER", "ADV", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lass, Geliebte, lass gew\u00e4hren \u2014 \u2014", "tokens": ["Lass", ",", "Ge\u00b7lieb\u00b7te", ",", "lass", "ge\u00b7w\u00e4h\u00b7ren"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "VVINF", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nieder sink ich kraftberaubt,", "tokens": ["Nie\u00b7der", "sink", "ich", "kraft\u00b7be\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und gebadet gar in Z\u00e4hren,", "tokens": ["Und", "ge\u00b7ba\u00b7det", "gar", "in", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Neigt verzagend sich das Haupt \u2014 \u2014", "tokens": ["Neigt", "ver\u00b7za\u00b7gend", "sich", "das", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVPP", "PRF", "ART", "NN", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Weg jedoch mit feigen Thr\u00e4nen!", "tokens": ["Weg", "je\u00b7doch", "mit", "fei\u00b7gen", "Thr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Genius, g\u00fcrte dich zum Streit!", "tokens": ["Ge\u00b7nius", ",", "g\u00fcr\u00b7te", "dich", "zum", "Streit", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Spanne die erschlafften Sehnen,", "tokens": ["Span\u00b7ne", "die", "er\u00b7schlaff\u00b7ten", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ringe nach Vortrefflichkeit.", "tokens": ["Rin\u00b7ge", "nach", "Vor\u00b7treff\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weggeschmelzt sey jede Schlacke,", "tokens": ["Weg\u00b7ge\u00b7schmelzt", "sey", "je\u00b7de", "Schla\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die dein reines Gold versehrt!", "tokens": ["Die", "dein", "rei\u00b7nes", "Gold", "ver\u00b7sehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "K\u00fchn erklommen jede Zacke,", "tokens": ["K\u00fchn", "er\u00b7klom\u00b7men", "je\u00b7de", "Za\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die dem Flug des Adlers wehrt.", "tokens": ["Die", "dem", "Flug", "des", "Ad\u00b7lers", "wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nein, Erhabne, nie err\u00f6then", "tokens": ["Nein", ",", "Er\u00b7hab\u00b7ne", ",", "nie", "er\u00b7r\u00f6\u00b7then"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "NN", "$,", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sollst du \u00fcber deinen Freund!", "tokens": ["Sollst", "du", "\u00fc\u00b7ber", "dei\u00b7nen", "Freund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mag Apollons Pfeil mich t\u00f6dten,", "tokens": ["Mag", "A\u00b7pol\u00b7lons", "Pfeil", "mich", "t\u00f6d\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Eh dein Liebling dich verneint,", "tokens": ["Eh", "dein", "Lieb\u00b7ling", "dich", "ver\u00b7neint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eh der Treffliche mich tadelt,", "tokens": ["Eh", "der", "Treff\u00b7li\u00b7che", "mich", "ta\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eh sich Pflicht und Ich entzweyn \u2014", "tokens": ["Eh", "sich", "Pflicht", "und", "Ich", "ent\u00b7zweyn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NN", "KON", "PPER", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der, den Ida's Wahl geadelt,", "tokens": ["Der", ",", "den", "I\u00b7da's", "Wahl", "ge\u00b7a\u00b7delt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ART", "NE", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Muss der Menschen Erster seyn.", "tokens": ["Muss", "der", "Men\u00b7schen", "Ers\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Sinken nur, lass nimmer sinken,", "tokens": ["Sin\u00b7ken", "nur", ",", "lass", "nim\u00b7mer", "sin\u00b7ken", ","], "token_info": ["word"