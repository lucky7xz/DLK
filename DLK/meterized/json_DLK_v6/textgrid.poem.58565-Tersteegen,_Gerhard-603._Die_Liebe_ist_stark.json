{"textgrid.poem.58565": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "603. Die Liebe ist stark", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00df oder sauer, Lieb' oder Leid", "tokens": ["S\u00fc\u00df", "o\u00b7der", "sau\u00b7er", ",", "Lieb'", "o\u00b7der", "Leid"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "$,", "NN", "KON", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "In diesem Jammertal der Zeit,", "tokens": ["In", "die\u00b7sem", "Jam\u00b7mer\u00b7tal", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was ist's \u2013 ist nur die Liebe rein,", "tokens": ["Was", "ist's", "\u2013", "ist", "nur", "die", "Lie\u00b7be", "rein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$(", "VAFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dies Feu'r verzehret Freud' und Pein!", "tokens": ["Dies", "Feu'r", "ver\u00b7zeh\u00b7ret", "Freud'", "und", "Pein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Liebe kann ertragen", "tokens": ["Lie\u00b7be", "kann", "er\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "VMFIN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Vieles Kreuz ohn' Klagen,", "tokens": ["Vie\u00b7les", "Kreuz", "ohn'", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Liebe greift zum Werke,", "tokens": ["Lie\u00b7be", "greift", "zum", "Wer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Gibt dem Vorsatz St\u00e4rke,", "tokens": ["Gibt", "dem", "Vor\u00b7satz", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Liebe sucht in allen", "tokens": ["Lie\u00b7be", "sucht", "in", "al\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PIAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Gott nur zu gefallen,", "tokens": ["Gott", "nur", "zu", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Liebe acht't sich selbst gar klein", "tokens": ["Lie\u00b7be", "acht't", "sich", "selbst", "gar", "klein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "ADV", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Und bewahrt das Herze rein.", "tokens": ["Und", "be\u00b7wahrt", "das", "Her\u00b7ze", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}