], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "ja der Wiesenglantz", "tokens": ["ja", "der", "Wie\u00b7sen\u00b7glantz"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "ist verwelcket gantz/", "tokens": ["ist", "ver\u00b7wel\u00b7cket", "gantz", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "ADV", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Es betranren dich die fisch\u2019 in den tieffen gr\u00fcnden/", "tokens": ["Es", "be\u00b7tran\u00b7ren", "dich", "die", "fisch'", "in", "den", "tief\u00b7fen", "gr\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "ja die g\u00fcldne Sonne selbst zihet an ein schwartzes kleid/", "tokens": ["ja", "die", "g\u00fcld\u00b7ne", "Son\u00b7ne", "selbst", "zi\u00b7het", "an", "ein", "schwart\u00b7zes", "kleid", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "gr\u00fcne W\u00e4lder", "tokens": ["gr\u00fc\u00b7ne", "W\u00e4l\u00b7der"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "bunte Felder", "tokens": ["bun\u00b7te", "Fel\u00b7der"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "selbst verlieren jhren schmuck/ jhren glantz un\u0303 liebligkeit.", "tokens": ["selbst", "ver\u00b7lie\u00b7ren", "jhren", "schmuck", "/", "jhren", "glantz", "u\u00