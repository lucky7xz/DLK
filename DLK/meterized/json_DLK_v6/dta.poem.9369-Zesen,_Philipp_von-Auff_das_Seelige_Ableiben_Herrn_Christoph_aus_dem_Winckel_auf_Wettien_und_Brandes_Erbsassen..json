"trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O Du Licht des gantzen Landes/", "tokens": ["O", "Du", "Licht", "des", "gant\u00b7zen", "Lan\u00b7des", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "der du warest f\u00fcr und f\u00fcr", "tokens": ["der", "du", "wa\u00b7rest", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VAFIN", "APPR", "KON", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "unser aller Ruhm und Zier/", "tokens": ["un\u00b7ser", "al\u00b7ler", "Ruhm", "und", "Zier", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PIAT", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "ja des Winckelischen Standes", "tokens": ["ja", "des", "Win\u00b7cke\u00b7li\u00b7schen", "Stan\u00b7des"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "starcker Trost und Ehrenkron/", "tokens": ["star\u00b7cker", "Trost", "und", "Eh\u00b7ren\u00b7kron", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "wiltu dann nun reisen schon", "tokens": ["wil\u00b7tu", "dann", "nun", "rei\u00b7sen", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "an den s\u00fc\u00dfen ort der freuden", "tokens": ["an", "den", "s\u00fc\u00b7\u00dfen", "ort", "der", "freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter