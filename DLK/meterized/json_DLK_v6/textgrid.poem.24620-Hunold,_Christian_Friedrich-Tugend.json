{"textgrid.poem.24620": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Tugend", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Sinnen haltet ein/ die eitler Sch\u00f6nheit voll.", "tokens": ["Ihr", "Sin\u00b7nen", "hal\u00b7tet", "ein", "/", "die", "eit\u00b7ler", "Sch\u00f6n\u00b7heit", "voll", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "$(", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Menschen wohnt ein Geist/ der mich verfechten soll:", "tokens": ["Im", "Men\u00b7schen", "wohnt", "ein", "Geist", "/", "der", "mich", "ver\u00b7fech\u00b7ten", "soll", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$(", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Sch\u00f6nheit/ die nicht stirbt/ erwecket seine Triebe.", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "/", "die", "nicht", "stirbt", "/", "er\u00b7we\u00b7cket", "sei\u00b7ne", "Trie\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PTKNEG", "VVFIN", "$(", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Tugend ist die Braut der allerreinsten Liebe.", "tokens": ["Die", "Tu\u00b7gend", "ist", "die", "Braut", "der", "al\u00b7ler\u00b7reins\u00b7ten", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}