{"textgrid.poem.31610": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Durch diese holde Blum / riech' ich des Sch\u00f6pffers Liebe.", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch diese holde Blum / riech' ich des Sch\u00f6pffers Liebe.", "tokens": ["Durch", "die\u00b7se", "hol\u00b7de", "Blum", "/", "riech'", "ich", "des", "Sch\u00f6pf\u00b7fers", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NE", "$(", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "aus jener hohen Farb / strahlt seiner Sch\u00f6nheit Schein.", "tokens": ["aus", "je\u00b7ner", "ho\u00b7hen", "Farb", "/", "strahlt", "sei\u00b7ner", "Sch\u00f6n\u00b7heit", "Schein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$(", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er hauchet / mit dem West / mir seine S\u00fc\u00dfheit ein.", "tokens": ["Er", "hau\u00b7chet", "/", "mit", "dem", "West", "/", "mir", "sei\u00b7ne", "S\u00fc\u00df\u00b7heit", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "APPR", "ART", "NN", "$(", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf Rosenbl\u00e4ttern / er sein sanfftes Herz beschriebe.", "tokens": ["Auf", "Ro\u00b7sen\u00b7bl\u00e4t\u00b7tern", "/", "er", "sein", "sanff\u00b7tes", "Herz", "be\u00b7schrie\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sein G\u00fcte / mir zu gut / sich durch die Scho\u00df austriebe.", "tokens": ["Sein", "G\u00fc\u00b7te", "/", "mir", "zu", "gut", "/", "sich", "durch", "die", "Scho\u00df", "aus\u00b7trie\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPER", "PTKA", "ADJD", "$(", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es fliest aus seiner Brust der Seeler-K\u00e4hle Wein /", "tokens": ["Es", "fliest", "aus", "sei\u00b7ner", "Brust", "der", "See\u00b7ler\u00b7K\u00e4h\u00b7le", "Wein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "macht alle Lieblichkeit vollkommen gut und rein /", "tokens": ["macht", "al\u00b7le", "Lieb\u00b7lich\u00b7keit", "voll\u00b7kom\u00b7men", "gut", "und", "rein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADJD", "ADJD", "KON", "ADJD", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.8": {"text": "die ohn den Lebensgeist ein tode Lust nur bliebe.", "tokens": ["die", "ohn", "den", "Le\u00b7bens\u00b7geist", "ein", "to\u00b7de", "Lust", "nur", "blie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er macht ein Wasserwerk / begiest mit Gnaden Safft", "tokens": ["Er", "macht", "ein", "Was\u00b7ser\u00b7werk", "/", "be\u00b7giest", "mit", "Gna\u00b7den", "Safft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "VVFIN", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "die Sternen-Schal / und machts in uns aus ihnen springen:", "tokens": ["die", "Ster\u00b7nen\u00b7Schal", "/", "und", "machts", "in", "uns", "aus", "ih\u00b7nen", "sprin\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KON", "VVFIN", "APPR", "PRF", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "von uns / im Jubel-Thron / durch hohe Danckes-Krafft /", "tokens": ["von", "uns", "/", "im", "Ju\u00b7bel\u00b7Thron", "/", "durch", "ho\u00b7he", "Dan\u00b7ckes\u00b7Krafft", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$(", "APPRART", "NN", "$(", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "die Gl\u00fcck -vermehrten Str\u00f6m empor auch wider dringen.", "tokens": ["die", "Gl\u00fcck", "Str\u00f6m", "em\u00b7por", "auch", "wi\u00b7der", "drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKVZ", "ADV", "APPR", "VVINF", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.13": {"text": "Es ist die ewig Ruh allein dahin bem\u00fcht /", "tokens": ["Es", "ist", "die", "e\u00b7wig", "Ruh", "al\u00b7lein", "da\u00b7hin", "be\u00b7m\u00fcht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "ADV", "PAV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "da\u00df uns in allem Ding ihr Lieb erscheint und bl\u00fcht.", "tokens": ["da\u00df", "uns", "in", "al\u00b7lem", "Ding", "ihr", "Lieb", "er\u00b7scheint", "und", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIS", "NN", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}