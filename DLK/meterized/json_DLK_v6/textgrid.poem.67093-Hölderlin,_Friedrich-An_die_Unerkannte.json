{"textgrid.poem.67093": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An die Unerkannte", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kennst du sie, die selig, wie die Sterne,", "tokens": ["Kennst", "du", "sie", ",", "die", "se\u00b7lig", ",", "wie", "die", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "PRELS", "ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Von des Lebens dunkler Woge ferne", "tokens": ["Von", "des", "Le\u00b7bens", "dunk\u00b7ler", "Wo\u00b7ge", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wandellos in stiller Sch\u00f6ne lebt,", "tokens": ["Wan\u00b7del\u00b7los", "in", "stil\u00b7ler", "Sch\u00f6\u00b7ne", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Die des Herzens l\u00f6wenk\u00fchne Siege,", "tokens": ["Die", "des", "Her\u00b7zens", "l\u00f6\u00b7wen\u00b7k\u00fch\u00b7ne", "Sie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Des Gedankens fesselfreie Fl\u00fcge,", "tokens": ["Des", "Ge\u00b7dan\u00b7kens", "fes\u00b7sel\u00b7frei\u00b7e", "Fl\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wie der Tag den Adler, \u00fcberschwebt?", "tokens": ["Wie", "der", "Tag", "den", "Ad\u00b7ler", ",", "\u00fc\u00b7bersc\u00b7hwebt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Die uns trifft mit ihren Mittagsstrahlen,", "tokens": ["Die", "uns", "trifft", "mit", "ih\u00b7ren", "Mit\u00b7tags\u00b7strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Uns entflammt mit ihren Idealen,", "tokens": ["Uns", "ent\u00b7flammt", "mit", "ih\u00b7ren", "I\u00b7dea\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wie vom Himmel, uns Gebote schickt,", "tokens": ["Wie", "vom", "Him\u00b7mel", ",", "uns", "Ge\u00b7bo\u00b7te", "schickt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "$,", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Die die Weisen nach dem Wege fragen,", "tokens": ["Die", "die", "Wei\u00b7sen", "nach", "dem", "We\u00b7ge", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Stumm und ernst, wie von dem Sturm verschlagen", "tokens": ["Stumm", "und", "ernst", ",", "wie", "von", "dem", "Sturm", "ver\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "$,", "PWAV", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Nach dem Orient der Schiffer blickt?", "tokens": ["Nach", "dem", "O\u00b7rient", "der", "Schif\u00b7fer", "blickt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Die das Beste gibt aus sch\u00f6ner F\u00fclle,", "tokens": ["Die", "das", "Bes\u00b7te", "gibt", "aus", "sch\u00f6\u00b7ner", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn aus ihr die Riesenkraft der Wille", "tokens": ["Wenn", "aus", "ihr", "die", "Rie\u00b7sen\u00b7kraft", "der", "Wil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und der Geist sein stilles Urteil nimmt,", "tokens": ["Und", "der", "Geist", "sein", "stil\u00b7les", "Ur\u00b7teil", "nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Die dem Lebensliede seine Weise,", "tokens": ["Die", "dem", "Le\u00b7bens\u00b7lie\u00b7de", "sei\u00b7ne", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Die das Ma\u00df der Ruhe, wie dem Flei\u00dfe", "tokens": ["Die", "das", "Ma\u00df", "der", "Ru\u00b7he", ",", "wie", "dem", "Flei\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Durch den Mittler, unsern Geist, bestimmt?", "tokens": ["Durch", "den", "Mitt\u00b7ler", ",", "un\u00b7sern", "Geist", ",", "be\u00b7stimmt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Die, wenn uns des Lebens Leere t\u00f6tet,", "tokens": ["Die", ",", "wenn", "uns", "des", "Le\u00b7bens", "Lee\u00b7re", "t\u00f6\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Magisch uns die welken Schl\u00e4fe r\u00f6tet,", "tokens": ["Ma\u00b7gisch", "uns", "die", "wel\u00b7ken", "Schl\u00e4\u00b7fe", "r\u00f6\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Uns mit Hoffnungen das Herz verj\u00fcngt,", "tokens": ["Uns", "mit", "Hoff\u00b7nun\u00b7gen", "das", "Herz", "ver\u00b7j\u00fcngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Die den Dulder, den der Sturm zertr\u00fcmmert,", "tokens": ["Die", "den", "Dul\u00b7der", ",", "den", "der", "Sturm", "zer\u00b7tr\u00fcm\u00b7mert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Den sein fernes Ithaka bek\u00fcmmert,", "tokens": ["Den", "sein", "fer\u00b7nes", "I\u00b7tha\u00b7ka", "be\u00b7k\u00fcm\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NE", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "In Alcinous Gefilde bringt?", "tokens": ["In", "Al\u00b7ci\u00b7nous", "Ge\u00b7fil\u00b7de", "bringt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Kennst du sie, die uns mit Lorbeerkronen,", "tokens": ["Kennst", "du", "sie", ",", "die", "uns", "mit", "Lor\u00b7beer\u00b7kro\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "PRELS", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mit der Freude be\u00dfrer Regionen,", "tokens": ["Mit", "der", "Freu\u00b7de", "be\u00df\u00b7rer", "Re\u00b7gi\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ehe wir zu Grabe gehn, vergilt,", "tokens": ["E\u00b7he", "wir", "zu", "Gra\u00b7be", "gehn", ",", "ver\u00b7gilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVINF", "$,", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Die der Liebe g\u00f6ttlichstes Verlangen,", "tokens": ["Die", "der", "Lie\u00b7be", "g\u00f6tt\u00b7lichs\u00b7tes", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Die das Sch\u00f6nste, was wir angefangen,", "tokens": ["Die", "das", "Sch\u00f6ns\u00b7te", ",", "was", "wir", "an\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "M\u00fchelos im Augenblick erf\u00fcllt?", "tokens": ["M\u00fc\u00b7he\u00b7los", "im", "Au\u00b7gen\u00b7blick", "er\u00b7f\u00fcllt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Die der Kindheit Wiederkehr beschleunigt,", "tokens": ["Die", "der", "Kind\u00b7heit", "Wie\u00b7der\u00b7kehr", "be\u00b7schleu\u00b7nigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die den Halbgott, unsern Geist, vereinigt", "tokens": ["Die", "den", "Halb\u00b7gott", ",", "un\u00b7sern", "Geist", ",", "ver\u00b7ei\u00b7nigt"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Mit den G\u00f6ttern, die er k\u00fchn verst\u00f6\u00dft,", "tokens": ["Mit", "den", "G\u00f6t\u00b7tern", ",", "die", "er", "k\u00fchn", "ver\u00b7st\u00f6\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Die des Schicksals ehrne Schl\u00fcsse mildert,", "tokens": ["Die", "des", "Schick\u00b7sals", "ehr\u00b7ne", "Schl\u00fcs\u00b7se", "mil\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und im Kampfe, wenn das Herz verwildert,", "tokens": ["Und", "im", "Kamp\u00b7fe", ",", "wenn", "das", "Herz", "ver\u00b7wil\u00b7dert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "$,", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Uns bes\u00e4nftigend den Harnisch l\u00f6st?", "tokens": ["Uns", "be\u00b7s\u00e4nf\u00b7ti\u00b7gend", "den", "Har\u00b7nisch", "l\u00f6st", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Die das Eine, das im Raum der Sterne,", "tokens": ["Die", "das", "Ei\u00b7ne", ",", "das", "im", "Raum", "der", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Das du suchst in aller Zeiten Ferne", "tokens": ["Das", "du", "suchst", "in", "al\u00b7ler", "Zei\u00b7ten", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "APPR", "PIAT", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Unter St\u00fcrmen, auf verwegner Fahrt,", "tokens": ["Un\u00b7ter", "St\u00fcr\u00b7men", ",", "auf", "ver\u00b7weg\u00b7ner", "Fahrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Das kein sterblicher Verstand ersonnen,", "tokens": ["Das", "kein", "sterb\u00b7li\u00b7cher", "Ver\u00b7stand", "er\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Keine, keine Tugend noch gewonnen,", "tokens": ["Kei\u00b7ne", ",", "kei\u00b7ne", "Tu\u00b7gend", "noch", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "$,", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Die des Friedens goldne Frucht bewahrt?", "tokens": ["Die", "des", "Frie\u00b7dens", "gold\u00b7ne", "Frucht", "be\u00b7wahrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}