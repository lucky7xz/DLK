{"textgrid.poem.44888": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Rubini, Malibran, Fodor, Lablache,", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rubini, Malibran, Fodor, Lablache,", "tokens": ["Ru\u00b7bi\u00b7ni", ",", "Ma\u00b7li\u00b7bran", ",", "Fo\u00b7dor", ",", "Lab\u00b7la\u00b7che", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "$,", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "So ging denn eure sch\u00f6ne Kunst verloren?", "tokens": ["So", "ging", "denn", "eu\u00b7re", "sch\u00f6\u00b7ne", "Kunst", "ver\u00b7lo\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Oper wird zum Melodram. Gl\u00fcck auf!", "tokens": ["Die", "O\u00b7per", "wird", "zum", "Me\u00b7lo\u00b7dram", ".", "Gl\u00fcck", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NE", "$.", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr weiche Herzen und f\u00fcr harte Ohren.", "tokens": ["F\u00fcr", "wei\u00b7che", "Her\u00b7zen", "und", "f\u00fcr", "har\u00b7te", "Oh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Rubini, Malibran, Fodor, Lablache,", "tokens": ["Ru\u00b7bi\u00b7ni", ",", "Ma\u00b7li\u00b7bran", ",", "Fo\u00b7dor", ",", "Lab\u00b7la\u00b7che", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "$,", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Non ebbero successor vostri talenti puri", "tokens": ["Non", "eb\u00b7be\u00b7ro", "suc\u00b7ces\u00b7sor", "vost\u00b7ri", "ta\u00b7len\u00b7ti", "pu\u00b7ri"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Si cangia in Melodrama l'Opera. Al pr\u00f2", "tokens": ["Si", "can\u00b7gia", "in", "Me\u00b7lo\u00b7dra\u00b7ma", "l'\u00b7Ope\u00b7ra", ".", "Al", "pr\u00f2"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "FM.la", "FM.la"], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Dei cuori teneri e degli' orechi duri.", "tokens": ["Dei", "cuo\u00b7ri", "te\u00b7ne\u00b7ri", "e", "deg\u00b7li'", "o\u00b7re\u00b7chi", "du\u00b7ri", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+--+-+-+-+--+", "measure": "iambic.hexa.relaxed"}}}}}