{"textgrid.poem.59002": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "221. Liebe Gott!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott lieben ist der Seelen Leben,", "tokens": ["Gott", "lie\u00b7ben", "ist", "der", "See\u00b7len", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gott lieben ist der Sinne Tod;", "tokens": ["Gott", "lie\u00b7ben", "ist", "der", "Sin\u00b7ne", "Tod", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lieb Gott, doch liebe nichts daneben,", "tokens": ["Lieb", "Gott", ",", "doch", "lie\u00b7be", "nichts", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADV", "VVFIN", "PIS", "PAV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sonst schmeckst du nicht dies Himmelbrot!", "tokens": ["Sonst", "schmeckst", "du", "nicht", "dies", "Him\u00b7mel\u00b7brot", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "PDS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}