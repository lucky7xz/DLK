{"dta.poem.8448": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingslied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Die kleinen Maienklocken bl\u00fchn,", "tokens": ["Die", "klei\u00b7nen", "Mai\u00b7en\u00b7klo\u00b7cken", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Schl\u00fcsselblumen drunter;", "tokens": ["Und", "Schl\u00fcs\u00b7sel\u00b7blu\u00b7men", "drun\u00b7ter", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "PAV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Wiesengrund", "tokens": ["Der", "Wie\u00b7sen\u00b7grund"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ist schon so bunt,", "tokens": ["Ist", "schon", "so", "bunt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und malt sich t\u00e4glich bunter.", "tokens": ["Und", "malt", "sich", "t\u00e4g\u00b7lich", "bun\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Drum komme, wem der Mai gef\u00e4llt,", "tokens": ["Drum", "kom\u00b7me", ",", "wem", "der", "Mai", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "PWS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und freue sich der sch\u00f6nen Welt", "tokens": ["Und", "freu\u00b7e", "sich", "der", "sch\u00f6\u00b7nen", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Gottes Vaterg\u00fcte,", "tokens": ["Und", "Got\u00b7tes", "Va\u00b7ter\u00b7g\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die solche Pracht", "tokens": ["Die", "sol\u00b7che", "Pracht"], "token_info": ["word", "word", "word"], "pos": ["ART", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Hervorgebracht,", "tokens": ["Her\u00b7vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Den Baum und seine Bl\u00fcte.", "tokens": ["Den", "Baum", "und", "sei\u00b7ne", "Bl\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}