{"textgrid.poem.45764": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[sonst haben meine beiden Onkels f\u00fcr mich intrigiert]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sonst haben meine beiden Onkels f\u00fcr mich intrigiert,", "tokens": ["Sonst", "ha\u00b7ben", "mei\u00b7ne", "bei\u00b7den", "On\u00b7kels", "f\u00fcr", "mich", "int\u00b7ri\u00b7giert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "PIAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Und ich steckte den Profit ein ungeniert,", "tokens": ["Und", "ich", "steck\u00b7te", "den", "Pro\u00b7fit", "ein", "un\u00b7ge\u00b7niert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "ADJD", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Jetzt, da die beiden Schnapph\u00e4hne nicht mehr am Leben,", "tokens": ["Jetzt", ",", "da", "die", "bei\u00b7den", "Schnap\u00b7ph\u00e4h\u00b7ne", "nicht", "mehr", "am", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "PIAT", "NN", "PTKNEG", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Mu\u00df ich mir schon selber die M\u00fche geben.", "tokens": ["Mu\u00df", "ich", "mir", "schon", "sel\u00b7ber", "die", "M\u00fc\u00b7he", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}}}}