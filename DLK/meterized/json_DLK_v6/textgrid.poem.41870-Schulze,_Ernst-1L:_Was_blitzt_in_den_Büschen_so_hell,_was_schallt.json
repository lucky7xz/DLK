{"textgrid.poem.41870": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was blitzt in den B\u00fcschen so hell, was schallt", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was blitzt in den B\u00fcschen so hell, was schallt", "tokens": ["Was", "blitzt", "in", "den", "B\u00fc\u00b7schen", "so", "hell", ",", "was", "schallt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "ADV", "ADJD", "$,", "PWS", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "In dem gr\u00fcnen Gehege so munter?", "tokens": ["In", "dem", "gr\u00fc\u00b7nen", "Ge\u00b7he\u00b7ge", "so", "mun\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Was zieht hervor aus dem dunkelen Wald", "tokens": ["Was", "zieht", "her\u00b7vor", "aus", "dem", "dun\u00b7ke\u00b7len", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKVZ", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und fern von den Bergen herunter?", "tokens": ["Und", "fern", "von", "den", "Ber\u00b7gen", "her\u00b7un\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Wir sind die J\u00e4ger, wir ziehn von Haus", "tokens": ["Wir", "sind", "die", "J\u00e4\u00b7ger", ",", "wir", "ziehn", "von", "Haus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und wollen zum Feind in das Feld hinaus,", "tokens": ["Und", "wol\u00b7len", "zum", "Feind", "in", "das", "Feld", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPRART", "NN", "APPR", "ART", "NN", "APZR", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Zum Krieg,", "tokens": ["Zum", "Krieg", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Zum Sieg", "tokens": ["Zum", "Sieg"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Und zum Siegesschmaus.", "tokens": ["Und", "zum", "Sie\u00b7ges\u00b7schmaus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Von dem lustigen Harzwald kommen wir her,", "tokens": ["Von", "dem", "lus\u00b7ti\u00b7gen", "Harz\u00b7wald", "kom\u00b7men", "wir", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wo nach Gold und nach Silber sie graben.", "tokens": ["Wo", "nach", "Gold", "und", "nach", "Sil\u00b7ber", "sie", "gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "KON", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Uns frommet das Gold und das Silber nicht mehr,", "tokens": ["Uns", "from\u00b7met", "das", "Gold", "und", "das", "Sil\u00b7ber", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Nur die Freyheit wollen wir haben.", "tokens": ["Nur", "die", "Frey\u00b7heit", "wol\u00b7len", "wir", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "PPER", "VAFIN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Drum lie\u00dfen wir Andern den th\u00f6rigten Wahn", "tokens": ["Drum", "lie\u00b7\u00dfen", "wir", "An\u00b7dern", "den", "th\u00f6\u00b7rig\u00b7ten", "Wahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADJA", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und haben mit Eisen uns angethan;", "tokens": ["Und", "ha\u00b7ben", "mit", "Ei\u00b7sen", "uns", "an\u00b7ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Nur das Schwert", "tokens": ["Nur", "das", "Schwert"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Hat Werth", "tokens": ["Hat", "Werth"], "token_info": ["word", "word"], "pos": ["VAFIN", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Auf der blutigen Bahn.", "tokens": ["Auf", "der", "blu\u00b7ti\u00b7gen", "Bahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6n ist's, zu lieben, zu trinken sch\u00f6n,", "tokens": ["Sch\u00f6n", "ist's", ",", "zu", "lie\u00b7ben", ",", "zu", "trin\u00b7ken", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sch\u00f6n ist's zu schlummern im Gr\u00fcnen;", "tokens": ["Sch\u00f6n", "ist's", "zu", "schlum\u00b7mern", "im", "Gr\u00fc\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKZU", "VVINF", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch fr\u00f6hlicher ist's in der Schlacht zu stehn", "tokens": ["Doch", "fr\u00f6h\u00b7li\u00b7cher", "ist's", "in", "der", "Schlacht", "zu", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Und sich Beut' und Kranz zu verdienen;", "tokens": ["Und", "sich", "Beut'", "und", "Kranz", "zu", "ver\u00b7die\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVFIN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Hell lodert wie Liebe des Kampfes Gluth,", "tokens": ["Hell", "lo\u00b7dert", "wie", "Lie\u00b7be", "des", "Kamp\u00b7fes", "Gluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KOKOM", "NN", "ART", "NN", "NN", "$,"], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Und wo Viele schlummern, da schl\u00e4ft sich's gut,", "tokens": ["Und", "wo", "Vie\u00b7le", "schlum\u00b7mern", ",", "da", "schl\u00e4ft", "sich's", "gut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "VVINF", "$,", "ADV", "VVFIN", "PIS", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Und es trinkt", "tokens": ["Und", "es", "trinkt"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Wer sinkt,", "tokens": ["Wer", "sinkt", ","], "token_info": ["word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Sey's Wein, sey's Blut.", "tokens": ["Sey's", "Wein", ",", "sey's", "Blut", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Oft haben wir wohl in der dunkeln Nacht", "tokens": ["Oft", "ha\u00b7ben", "wir", "wohl", "in", "der", "dun\u00b7keln", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Bey St\u00fcrmen und Regenschauern", "tokens": ["Bey", "St\u00fcr\u00b7men", "und", "Re\u00b7gen\u00b7schau\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hoch auf dem Fels und in Schluchten gewacht,", "tokens": ["Hoch", "auf", "dem", "Fels", "und", "in", "Schluch\u00b7ten", "ge\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "KON", "APPR", "NN", "VVPP", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Um das streifende Wild zu belauern;", "tokens": ["Um", "das", "strei\u00b7fen\u00b7de", "Wild", "zu", "be\u00b7lau\u00b7ern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Jetzt ziehen wir muthig im Sonnenlicht", "tokens": ["Jetzt", "zie\u00b7hen", "wir", "mut\u00b7hig", "im", "Son\u00b7nen\u00b7licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und sehen dem Feind in das Angesicht.", "tokens": ["Und", "se\u00b7hen", "dem", "Feind", "in", "das", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Sey's Jagd,", "tokens": ["Sey's", "Jagd", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Sey's Schlacht,", "tokens": ["Sey's", "Schlacht", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Uns k\u00fcmmert's nicht.", "tokens": ["Uns", "k\u00fcm\u00b7mert's", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Mag fliehen der Feige durch Wald und Feld,", "tokens": ["Mag", "flie\u00b7hen", "der", "Fei\u00b7ge", "durch", "Wald", "und", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wenn die st\u00e4rkere Zahl ihn bestreitet;", "tokens": ["Wenn", "die", "st\u00e4r\u00b7ke\u00b7re", "Zahl", "ihn", "be\u00b7strei\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Wo das Wild uns in Schaaren entgegenprellt,", "tokens": ["Wo", "das", "Wild", "uns", "in", "Schaa\u00b7ren", "ent\u00b7ge\u00b7gen\u00b7prellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Da wird was Rechtes erbeutet.", "tokens": ["Da", "wird", "was", "Rech\u00b7tes", "er\u00b7beu\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und wenn auch unz\u00e4hlig der Feind uns droht,", "tokens": ["Und", "wenn", "auch", "un\u00b7z\u00e4h\u00b7lig", "der", "Feind", "uns", "droht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ADJD", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.6": {"text": "Uns blitzt aus den H\u00e4nden der sichere Tod.", "tokens": ["Uns", "blitzt", "aus", "den", "H\u00e4n\u00b7den", "der", "si\u00b7che\u00b7re", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Ein Knall", "tokens": ["Ein", "Knall"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Ein Fall,", "tokens": ["Ein", "Fall", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Das ist J\u00e4gergebot.", "tokens": ["Das", "ist", "J\u00e4\u00b7ger\u00b7ge\u00b7bot", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Drum haltet zusammen und stehet fest,", "tokens": ["Drum", "hal\u00b7tet", "zu\u00b7sam\u00b7men", "und", "ste\u00b7het", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der Eine den Andern zu decken;", "tokens": ["Der", "Ei\u00b7ne", "den", "An\u00b7dern", "zu", "de\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wenn nur vom Freunde der Freund nicht l\u00e4\u00dft,", "tokens": ["Wenn", "nur", "vom", "Freun\u00b7de", "der", "Freund", "nicht", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPRART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Kann wenig der Feind uns erschrecken.", "tokens": ["Kann", "we\u00b7nig", "der", "Feind", "uns", "er\u00b7schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Doch steht dein Nam' auf dem t\u00f6dtlichen Bley,", "tokens": ["Doch", "steht", "dein", "Nam'", "auf", "dem", "t\u00f6dt\u00b7li\u00b7chen", "Bley", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "So fliegt dir auch nimmer die Kugel vorbey;", "tokens": ["So", "fliegt", "dir", "auch", "nim\u00b7mer", "die", "Ku\u00b7gel", "vor\u00b7bey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Vom Freund,", "tokens": ["Vom", "Freund", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Vom Feind,", "tokens": ["Vom", "Feind", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Es ist einerley.", "tokens": ["Es", "ist", "ei\u00b7ner\u00b7ley", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Denn der gr\u00f6\u00dfte J\u00e4gersmann ist der Tod,", "tokens": ["Denn", "der", "gr\u00f6\u00df\u00b7te", "J\u00e4\u00b7gers\u00b7mann", "ist", "der", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der will an der Lust nur sich laben;", "tokens": ["Der", "will", "an", "der", "Lust", "nur", "sich", "la\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "ART", "NN", "ADV", "PRF", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wohl f\u00e4rbt er mit Blute die Haiden roth,", "tokens": ["Wohl", "f\u00e4rbt", 