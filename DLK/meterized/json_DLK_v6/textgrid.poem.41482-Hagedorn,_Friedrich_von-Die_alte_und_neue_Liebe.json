{"textgrid.poem.41482": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die alte und neue Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Heiligen der alten Zeit,", "tokens": ["Ihr", "Hei\u00b7li\u00b7gen", "der", "al\u00b7ten", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Treu', Ehrfurcht und Verschwiegenheit,", "tokens": ["Treu'", ",", "Ehr\u00b7furcht", "und", "Ver\u00b7schwie\u00b7gen\u00b7heit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und du, o wahre Z\u00e4rtlichkeit!", "tokens": ["Und", "du", ",", "o", "wah\u00b7re", "Z\u00e4rt\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr lehrtet uns dem Liebreiz fr\u00f6hnen.", "tokens": ["Ihr", "lehr\u00b7tet", "uns", "dem", "Lieb\u00b7reiz", "fr\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nun ist die Treue nur verstellt,", "tokens": ["Nun", "ist", "die", "Treu\u00b7e", "nur", "ver\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und die Verschwiegenheit entf\u00e4llt,", "tokens": ["Und", "die", "Ver\u00b7schwie\u00b7gen\u00b7heit", "ent\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn ja die Ehrfurcht Gunst erh\u00e4lt.", "tokens": ["Wenn", "ja", "die", "Ehr\u00b7furcht", "Gunst", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wer liebt nicht sich in seinen Sch\u00f6nen?", "tokens": ["Wer", "liebt", "nicht", "sich", "in", "sei\u00b7nen", "Sch\u00f6\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Von seiner Phyllis ferne sein,", "tokens": ["Von", "sei\u00b7ner", "Phyl\u00b7lis", "fer\u00b7ne", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NE", "ADJA", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr dennoch hei\u00dfe Seufzer weihn,", "tokens": ["Ihr", "den\u00b7noch", "hei\u00b7\u00dfe", "Seuf\u00b7zer", "weihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und diese Seufzer nicht bereun:", "tokens": ["Und", "die\u00b7se", "Seuf\u00b7zer", "nicht", "be\u00b7re\u00b7un", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Das war die Lust des Sch\u00e4ferlebens.", "tokens": ["Das", "war", "die", "Lust", "des", "Sch\u00e4\u00b7fer\u00b7le\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das Seufzen ist uns unbewu\u00dft.", "tokens": ["Das", "Seuf\u00b7zen", "ist", "uns", "un\u00b7be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Man seufzet, aber nur vor Lust,", "tokens": ["Man", "seuf\u00b7zet", ",", "a\u00b7ber", "nur", "vor", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "An einer nahen Phyllis Brust,", "tokens": ["An", "ei\u00b7ner", "na\u00b7hen", "Phyl\u00b7lis", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und seufzet da nicht leicht vergebens.", "tokens": ["Und", "seuf\u00b7zet", "da", "nicht", "leicht", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Fessel k\u00fcssen, die man tr\u00e4gt,", "tokens": ["Die", "Fes\u00b7sel", "k\u00fcs\u00b7sen", ",", "die", "man", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die uns ein M\u00e4dchen angelegt,", "tokens": ["Die", "uns", "ein", "M\u00e4d\u00b7chen", "an\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das reizend Mund und Augen regt:", "tokens": ["Das", "rei\u00b7zend", "Mund", "und", "Au\u00b7gen", "regt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVPP", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das war die Kunst der ersten Zeiten.", "tokens": ["Das", "war", "die", "Kunst", "der", "ers\u00b7ten", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Fessel und die Knechtschaft fliehn", "tokens": ["Die", "Fes\u00b7sel", "und", "die", "Knecht\u00b7schaft", "fliehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und, wo nur sch\u00f6ne Wangen bl\u00fchn,", "tokens": ["Und", ",", "wo", "nur", "sch\u00f6\u00b7ne", "Wan\u00b7gen", "bl\u00fchn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Um sch\u00f6ne Wangen sich bem\u00fchn:", "tokens": ["Um", "sch\u00f6\u00b7ne", "Wan\u00b7gen", "sich", "be\u00b7m\u00fchn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das nennt man jetzo Z\u00e4rtlichkeiten.", "tokens": ["Das", "nennt", "man", "jet\u00b7zo", "Z\u00e4rt\u00b7lich\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "ADV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Durch mehr als j\u00e4hrigen Bestand", "tokens": ["Durch", "mehr", "als", "j\u00e4h\u00b7ri\u00b7gen", "Be\u00b7stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verehren, was man artig fand,", "tokens": ["Ver\u00b7eh\u00b7ren", ",", "was", "man", "ar\u00b7tig", "fand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und unsre Treu' oft nicht erkannt:", "tokens": ["Und", "uns\u00b7re", "Treu'", "oft", "nicht", "er\u00b7kannt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das war den V\u00e4tern vorgeschrieben.", "tokens": ["Das", "war", "den", "V\u00e4\u00b7tern", "vor\u00b7ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Erw\u00e4hlen was nur Sch\u00f6nheit schm\u00fcckt,", "tokens": ["Er\u00b7w\u00e4h\u00b7len", "was", "nur", "Sch\u00f6n\u00b7heit", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Genie\u00dfen was uns oft entz\u00fcckt,", "tokens": ["Ge\u00b7nie\u00b7\u00dfen", "was", "uns", "oft", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Verlassen was uns sonst begl\u00fcckt:", "tokens": ["Ver\u00b7las\u00b7sen", "was", "uns", "sonst", "be\u00b7gl\u00fcckt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das ist der Enkel Art zu lieben.", "tokens": ["Das", "ist", "der", "En\u00b7kel", "Art", "zu", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}