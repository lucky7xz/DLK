{"textgrid.poem.64684": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "311. Beygelegter Streit", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In mir lebt' er zuerst, der Vater der Poeten!", "tokens": ["In", "mir", "lebt'", "er", "zu\u00b7erst", ",", "der", "Va\u00b7ter", "der", "Po\u00b7et\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Das war vordem der Zank von sieben St\u00e4dten.", "tokens": ["Das", "war", "vor\u00b7dem", "der", "Zank", "von", "sie\u00b7ben", "St\u00e4d\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Kritikus vereinigt sie:", "tokens": ["Der", "Kri\u00b7ti\u00b7kus", "ver\u00b7ei\u00b7nigt", "sie", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um den ihr zankt, der lebte nie.", "tokens": ["Um", "den", "ihr", "zankt", ",", "der", "leb\u00b7te", "nie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}