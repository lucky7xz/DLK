{"dta.poem.11821": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "107.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du sagst, dir sei zu weit die drei\u00dfigst\u00fcnd'ge Reise,", "tokens": ["Du", "sagst", ",", "dir", "sei", "zu", "weit", "die", "drei\u00b7\u00dfig\u00b7st\u00fcn\u00b7d'\u00b7ge", "Rei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PTKA", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und drehest jeden Tag dich stundenlang im Kreise.", "tokens": ["Und", "dre\u00b7hest", "je\u00b7den", "Tag", "dich", "stun\u00b7den\u00b7lang", "im", "Krei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Stunde dehnest du, alswie ein m\u00fc\u00df'ger Reiter,", "tokens": ["Die", "Stun\u00b7de", "deh\u00b7nest", "du", ",", "als\u00b7wie", "ein", "m\u00fc\u00df'\u00b7ger", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Haus zur\u00fcck zum Haus, und r\u00fcckst dabei nicht weiter.", "tokens": ["Vom", "Haus", "zu\u00b7r\u00fcck", "zum", "Haus", ",", "und", "r\u00fcckst", "da\u00b7bei", "nicht", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "APPRART", "NN", "$,", "KON", "VVFIN", "PAV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Setz' einen Monat lang zusammen nur die Stunden", "tokens": ["Setz'", "ei\u00b7nen", "Mo\u00b7nat", "lang", "zu\u00b7sam\u00b7men", "nur", "die", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJD", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In grader Linie zum Ziel, so ists gefunden.", "tokens": ["In", "gra\u00b7der", "Li\u00b7nie", "zum", "Ziel", ",", "so", "ists", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "$,", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Mit solchem Kunstst\u00fcck kommt die Schnecke selbst zum Zwecke,", "tokens": ["Mit", "sol\u00b7chem", "Kunst\u00b7st\u00fcck", "kommt", "die", "Schne\u00b7cke", "selbst", "zum", "Zwe\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ohne solches auch das Rennthier nicht vom Flecke.", "tokens": ["Und", "oh\u00b7ne", "sol\u00b7ches", "auch", "das", "Renn\u00b7thier", "nicht", "vom", "Fle\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "ADV", "ART", "NN", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}