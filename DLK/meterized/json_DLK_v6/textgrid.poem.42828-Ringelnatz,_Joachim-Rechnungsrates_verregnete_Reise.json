{"textgrid.poem.42828": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Rechnungsrates verregnete Reise", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wie ich vom Regen begossen", "tokens": ["Und", "wie", "ich", "vom", "Re\u00b7gen", "be\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Wegweiser las,", "tokens": ["Die", "Weg\u00b7wei\u00b7ser", "las", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da lag ein Hemd, wie erschossen,", "tokens": ["Da", "lag", "ein", "Hemd", ",", "wie", "er\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PWAV", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zum Bleichen im Gras.", "tokens": ["Zum", "Blei\u00b7chen", "im", "Gras", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ich dachte: Zweifellos leben", "tokens": ["Ich", "dach\u00b7te", ":", "Zwei\u00b7fel\u00b7los", "le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "NN", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hier Menschen und leben nicht schlecht.", "tokens": ["Hier", "Men\u00b7schen", "und", "le\u00b7ben", "nicht", "schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "VVFIN", "PTKNEG", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und sah das Hemd und daneben", "tokens": ["Und", "sah", "das", "Hemd", "und", "da\u00b7ne\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "PAV"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Ein Haus. Also hatte ich recht.", "tokens": ["Ein", "Haus", ".", "Al\u00b7so", "hat\u00b7te", "ich", "recht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Ich wollte mich selber beklagen,", "tokens": ["Ich", "woll\u00b7te", "mich", "sel\u00b7ber", "be\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Zog bitter mein Los in Vergleich,", "tokens": ["Zog", "bit\u00b7ter", "mein", "Los", "in", "Ver\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Doch machte das Mi\u00dfbehagen", "tokens": ["Doch", "mach\u00b7te", "das", "Mi\u00df\u00b7be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Im Regen mich weich.", "tokens": ["Im", "Re\u00b7gen", "mich", "weich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Man soll sich nichts selber verleiden.", "tokens": ["Man", "soll", "sich", "nichts", "sel\u00b7ber", "ver\u00b7lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "PIS", "ADV", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und Mi\u00dfgunst ist immer wie Rost.", "tokens": ["Und", "Mi\u00df\u00b7gunst", "ist", "im\u00b7mer", "wie", "Rost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "KOKOM", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich gab unter Schwierigkeiten", "tokens": ["Ich", "gab", "un\u00b7ter", "Schwie\u00b7rig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Eine Depesche zur n\u00e4chsten Post:", "tokens": ["Ei\u00b7ne", "De\u00b7pe\u00b7sche", "zur", "n\u00e4chs\u00b7ten", "Post", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "\u00bberwarte f\u00fcr morgen Montag fr\u00fch", "tokens": ["\u00bb", "er\u00b7war\u00b7te", "f\u00fcr", "mor\u00b7gen", "Mon\u00b7tag", "fr\u00fch"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "APPR", "ADJA", "NN", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Den Mann mit dem accent aigu.\u00ab", "tokens": ["Den", "Mann", "mit", "dem", "ac\u00b7cent", "ai\u00b7gu", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NE", "NE", "$.", "$("], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Zwar ist es im Grunde ein kleiner", "tokens": ["Zwar", "ist", "es", "im", "Grun\u00b7de", "ein", "klei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN", "ART", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Umstand, aber er qu\u00e4lt,", "tokens": ["Um\u00b7stand", ",", "a\u00b7ber", "er", "qu\u00e4lt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da\u00df nun seit Jahren schon meiner", "tokens": ["Da\u00df", "nun", "seit", "Jah\u00b7ren", "schon", "mei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "NN", "ADV", "PPOSAT"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schreibmaschine der Rechtskn\u00fcppel fehlt.", "tokens": ["Schreib\u00b7ma\u00b7schi\u00b7ne", "der", "Rechts\u00b7kn\u00fcp\u00b7pel", "fehlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Man mu\u00df die Natur nur erfassen,", "tokens": ["Man", "mu\u00df", "die", "Na\u00b7tur", "nur", "er\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wie immer das Wetter auch sei.", "tokens": ["Wie", "im\u00b7mer", "das", "Wet\u00b7ter", "auch", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Bewu\u00dft, den Zug zu verpassen,", "tokens": ["Be\u00b7wu\u00dft", ",", "den", "Zug", "zu", "ver\u00b7pas\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "War ich doch ruhig dabei.", "tokens": ["War", "ich", "doch", "ru\u00b7hig", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "PAV", "$."], "meter": "---+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Ich fuhr also heim. Denn, was blieb mir", "tokens": ["Ich", "fuhr", "al\u00b7so", "heim", ".", "Denn", ",", "was", "blieb", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$.", "KON", "$,", "PWS", "VVFIN", "PPER"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sonst \u00fcbrig? Man ist nicht Herr seiner Zeit.", "tokens": ["Sonst", "\u00fcb\u00b7rig", "?", "Man", "ist", "nicht", "Herr", "sei\u00b7ner", "Zeit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "PIS", "VAFIN", "PTKNEG", "NN", "PPOSAT", "NN", "$."], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Meine Stiefnichte schrieb mir:", "tokens": ["Mei\u00b7ne", "Stief\u00b7nich\u00b7te", "schrieb", "mir", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Es habe in Bozen sogar geschneit.", "tokens": ["Es", "ha\u00b7be", "in", "Bo\u00b7zen", "so\u00b7gar", "ge\u00b7schneit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NE", "ADV", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}