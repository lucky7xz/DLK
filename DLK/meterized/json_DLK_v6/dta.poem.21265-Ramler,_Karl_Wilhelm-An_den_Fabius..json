{"dta.poem.21265": {"metadata": {"author": {"name": "Ramler, Karl Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An den Fabius.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20476-3", "language": ["de:0.99"], "booktitle": "Ramler, Karl Wilhelm: Oden. Berlin, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "O Fabius! gereut dich nach drey\nIahren", "tokens": ["O", "Fa\u00b7bi\u00b7us", "!", "ge\u00b7reut", "dich", "nach", "drey", "I\u00b7ah\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$.", "VVFIN", "PRF", "APPR", "CARD", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dein gl\u00fcckliches Verziehn?", "tokens": ["Dein", "gl\u00fcck\u00b7li\u00b7ches", "Ver\u00b7ziehn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo waren deine Felsen? Waren", "tokens": ["Wo", "wa\u00b7ren", "dei\u00b7ne", "Fel\u00b7sen", "?", "Wa\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "$.", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Felsen nicht mehr steil f\u00fcr ihn?", "tokens": ["Die", "Fel\u00b7sen", "nicht", "mehr", "steil", "f\u00fcr", "ihn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vergissest du, wie man bey Nacht dem", "tokens": ["Ver\u00b7gis\u00b7sest", "du", ",", "wie", "man", "bey", "Nacht", "dem"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PIS", "APPR", "NN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ins m\u00fcde Lager streift?", "tokens": ["Ins", "m\u00fc\u00b7de", "La\u00b7ger", "streift", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wie man eine Hand voll Krieger", "tokens": ["Und", "wie", "man", "ei\u00b7ne", "Hand", "voll", "Krie\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PIS", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit einem Ocean ers\u00e4uft?", "tokens": ["Mit", "ei\u00b7nem", "O\u00b7cean", "er\u00b7s\u00e4uft", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Und wie man bundsverwandte Na-", "tokens": ["Und", "wie", "man", "bunds\u00b7ver\u00b7wand\u00b7te", "Na"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PIS", "ADJA", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bequem zur Schlachtbank schickt,", "tokens": ["Be\u00b7quem", "zur", "Schlacht\u00b7bank", "schickt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Indessen man, sein Heer zu schonen,", "tokens": ["In\u00b7des\u00b7sen", "man", ",", "sein", "Heer", "zu", "scho\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von sichrer H\u00f6h weit um sich blickt?", "tokens": ["Von", "sich\u00b7rer", "H\u00f6h", "weit", "um", "sich", "blickt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wer nimmt sich nun der Diener armer", "tokens": ["Wer", "nimmt", "sich", "nun", "der", "Die\u00b7ner", "ar\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PRF", "ADV", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der hohen Bassen an,", "tokens": ["Der", "ho\u00b7hen", "Bas\u00b7sen", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und straft den stolzen Potentaten,", "tokens": ["Und", "straft", "den", "stol\u00b7zen", "Po\u00b7ten\u00b7ta\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der selbst regieren will, und kann?", "tokens": ["Der", "selbst", "re\u00b7gie\u00b7ren", "will", ",", "und", "kann", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "VMFIN", "$,", "KON", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wer r\u00e4cht die Feldherrn, die nach", "tokens": ["Wer", "r\u00e4cht", "die", "Feld\u00b7herrn", ",", "die", "nach"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Nach Beute l\u00fcstern sind,", "tokens": ["Nach", "Beu\u00b7te", "l\u00fcs\u00b7tern", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An diesem wunderbaren F\u00fcrsten,", "tokens": ["An", "die\u00b7sem", "wun\u00b7der\u00b7ba\u00b7ren", "F\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der seine Schlachten selbst gewinnt?", "tokens": ["Der", "sei\u00b7ne", "Schlach\u00b7ten", "selbst", "ge\u00b7winnt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und ach! wer r\u00e4cht die Zunft der", "tokens": ["Und", "ach", "!", "wer", "r\u00e4cht", "die", "Zunft", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "XY", "$.", "PWS", "VVFIN", "ART", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun du geschlagen bist,", "tokens": ["Nun", "du", "ge\u00b7schla\u00b7gen", "bist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An einem K\u00f6nige, der Meister", "tokens": ["An", "ei\u00b7nem", "K\u00f6\u00b7ni\u00b7ge", ",", "der", "Meis\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In allen ihren K\u00fcnsten ist?", "tokens": ["In", "al\u00b7len", "ih\u00b7ren", "K\u00fcns\u00b7ten", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Weh deinem Pontifex, der stets die", "tokens": ["Weh", "dei\u00b7nem", "Pon\u00b7ti\u00b7fex", ",", "der", "stets", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "PRELS", "ADV", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Wundern hintergeht!", "tokens": ["Mit", "Wun\u00b7dern", "hin\u00b7ter\u00b7geht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er kann ja keinen Degen weihen,", "tokens": ["Er", "kann", "ja", "kei\u00b7nen", "De\u00b7gen", "wei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der wider Pallas Helm besteht.", "tokens": ["Der", "wi\u00b7der", "Pal\u00b7las", "Helm", "be\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}