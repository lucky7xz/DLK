{"dta.poem.978": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "V ERSTATTET  D IES  S PIEL:  E URE  F L\u00dcCHTIG  \n  G ESCHNITTENEN  S CHATTEN  Z UM  S CHMUCK  \n  F \u00dcR  M EINER  A NGEDENKEN  S AAL", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Die du ein gl&#252;ck vermehrst auch nicht es teilend               ", "tokens": ["Die", "du", "ein", "gl", "&#252;", "ck", "ver\u00b7mehrst", "auch", "nicht", "es", "tei\u00b7lend"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NE", "$(", "NE", "VVFIN", "ADV", "PTKNEG", "PPER", "VVPP"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "F&#252;r schmerzen balsam bist auch kaum sie fassend               ", "tokens": ["F", "&#252;", "r", "schmer\u00b7zen", "bal\u00b7sam", "bist", "auch", "kaum", "sie", "fas\u00b7send"], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$(", "NE", "VVFIN", "ADJD", "VAFIN", "ADV", "ADV", "PPER", "VVPP"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Und gar aus schlimmen zeichen sch\u00f6nes r&#228;tst               ", "tokens": ["Und", "gar", "aus", "schlim\u00b7men", "zei\u00b7chen", "sch\u00f6\u00b7nes", "r", "&#228;", "tst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "ADJA", "NN", "$(", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Erfinderisch und gross im reich der g&#252;te               ", "tokens": ["Er\u00b7fin\u00b7de\u00b7risch", "und", "gross", "im", "reich", "der", "g", "&#252;", "te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["ADJD", "KON", "ADJD", "APPRART", "ADJD", "ART", "NN", "$(", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du darfst dich r&#252;hmen: manchen geist am strand               ", "tokens": ["Du", "darfst", "dich", "r", "&#252;", "hmen", ":", "man\u00b7chen", "geist", "am", "strand"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "NE", "$(", "VVINF", "$.", "PIAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der nach dem schiffbruch hingeschleudert wurde               ", "tokens": ["Der", "nach", "dem", "schiff\u00b7bruch", "hin\u00b7ge\u00b7schleu\u00b7dert", "wur\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Den g\u00f6tter und genossen liegen liessen               ", "tokens": ["Den", "g\u00f6t\u00b7ter", "und", "ge\u00b7nos\u00b7sen", "lie\u00b7gen", "lies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "KON", "VVPP", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich jenes m&#228;dchen hab ihn aufgerichtet.             ", "tokens": ["Ich", "je\u00b7nes", "m", "&#228;", "dchen", "hab", "ihn", "auf\u00b7ge\u00b7rich\u00b7tet", "."], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "$(", "VVINF", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}