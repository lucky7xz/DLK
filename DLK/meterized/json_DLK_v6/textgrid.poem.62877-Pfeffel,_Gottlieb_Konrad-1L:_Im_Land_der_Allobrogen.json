{"textgrid.poem.62877": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Land der Allobrogen", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Land der Allobrogen", "tokens": ["Im", "Land", "der", "Al\u00b7lo\u00b7bro\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Lebt Fr\u00e4ulein Isabell:", "tokens": ["Lebt", "Fr\u00e4u\u00b7lein", "I\u00b7sa\u00b7bell", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie scho\u00df mit Pfeil und Bogen", "tokens": ["Sie", "scho\u00df", "mit", "Pfeil", "und", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So gut als Wilhelm Tell:", "tokens": ["So", "gut", "als", "Wil\u00b7helm", "Tell", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "War jung und sch\u00f6n, doch spr\u00f6de", "tokens": ["War", "jung", "und", "sch\u00f6n", ",", "doch", "spr\u00f6\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und kalt f\u00fcr Lieb und Scherz;", "tokens": ["Und", "kalt", "f\u00fcr", "Lieb", "und", "Scherz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Auch gieng im Land die Rede,", "tokens": ["Auch", "gieng", "im", "Land", "die", "Re\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sie hab ein st\u00e4hlern Herz.", "tokens": ["Sie", "hab", "ein", "st\u00e4h\u00b7lern", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Held aus den Cevennen,", "tokens": ["Ein", "Held", "aus", "den", "Ce\u00b7ven\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Mit Namen Odoard,", "tokens": ["Mit", "Na\u00b7men", "O\u00b7doard", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Bey einem Ringelrennen", "tokens": ["Bey", "ei\u00b7nem", "Rin\u00b7gel\u00b7ren\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von ihr entz\u00fcndet ward.", "tokens": ["Von", "ihr", "ent\u00b7z\u00fcn\u00b7det", "ward", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er girrt ihr seine Plage", "tokens": ["Er", "girrt", "ihr", "sei\u00b7ne", "Pla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bey Nacht und Tage vor:", "tokens": ["Bey", "Nacht", "und", "Ta\u00b7ge", "vor", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Umsonst! des Buhlen Klage", "tokens": ["Um\u00b7sonst", "!", "des", "Buh\u00b7len", "Kla\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$.", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Schallt in ein taubes Ohr.", "tokens": ["Schallt", "in", "ein", "tau\u00b7bes", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er bringt ihr Papagoyen,", "tokens": ["Er", "bringt", "ihr", "Pa\u00b7pa\u00b7go\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gekauft in Trapezunt:", "tokens": ["Ge\u00b7kauft", "in", "Tra\u00b7pe\u00b7zunt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er holt ihr aus Savoyen", "tokens": ["Er", "holt", "ihr", "aus", "Sa\u00b7vo\u00b7yen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den besten Wachtelhund:", "tokens": ["Den", "bes\u00b7ten", "Wach\u00b7tel\u00b7hund", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er sendet ihr ein F\u00fcllen", "tokens": ["Er", "sen\u00b7det", "ihr", "ein", "F\u00fcl\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.6": {"text": "Mit einem Reigerstrau\u00df;", "tokens": ["Mit", "ei\u00b7nem", "Rei\u00b7ger\u00b7strau\u00df", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch nichts beugt ihren Willen;", "tokens": ["Doch", "nichts", "beugt", "ih\u00b7ren", "Wil\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sie schl\u00e4gt die Gaben aus.", "tokens": ["Sie", "schl\u00e4gt", "die", "Ga\u00b7ben", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Als nach dem Wettgefechte", "tokens": ["Als", "nach", "dem", "Wett\u00b7ge\u00b7fech\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie ihm den Preis gereicht,", "tokens": ["Sie", "ihm", "den", "Preis", "ge\u00b7reicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ergriff er ihre Rechte", "tokens": ["Er\u00b7griff", "er", "ih\u00b7re", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und seufzte tief erweicht:", "tokens": ["Und", "seufz\u00b7te", "tief", "er\u00b7weicht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach, lindre meine Schmerzen", "tokens": ["Ach", ",", "lind\u00b7re", "mei\u00b7ne", "Schmer\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auch nur mit einem Blick.", "tokens": ["Auch", "nur", "mit", "ei\u00b7nem", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Allein mit stolzem Herzen", "tokens": ["Al\u00b7lein", "mit", "stol\u00b7zem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zog sie die Hand zur\u00fcck.", "tokens": ["Zog", "sie", "die", "Hand", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Fahr hin! rief er, mit Z\u00e4hren", "tokens": ["Fahr", "hin", "!", "rief", "er", ",", "mit", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "VVFIN", "PPER", "$,", "APPR", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Wirst du den Trotz bereun;", "tokens": ["Wirst", "du", "den", "Trotz", "be\u00b7re\u00b7un", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Doch dann werd ich nicht h\u00f6ren,", "tokens": ["Doch", "dann", "werd", "ich", "nicht", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht f\u00fchlen deine Pein.", "tokens": ["Nicht", "f\u00fch\u00b7len", "dei\u00b7ne", "Pein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie lacht der eiteln Worte,", "tokens": ["Sie", "lacht", "der", "ei\u00b7teln", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und Odoard entflieht,", "tokens": ["Und", "O\u00b7doard", "ent\u00b7flieht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "An einem \u00f6den Orte", "tokens": ["An", "ei\u00b7nem", "\u00f6\u00b7den", "Or\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zu weinen. Was geschieht?", "tokens": ["Zu", "wei\u00b7nen", ".", "Was", "ge\u00b7schieht", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$.", "PWS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Einst ritt auf ihrer Schecke", "tokens": ["Einst", "ritt", "auf", "ih\u00b7rer", "Sche\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die J\u00e4gerin durchs Holz;", "tokens": ["Die", "J\u00e4\u00b7ge\u00b7rin", "durchs", "Holz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da kam aus dunkler Hecke", "tokens": ["Da", "kam", "aus", "dunk\u00b7ler", "He\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein B\u00e4r mit ernstem Stolz", "tokens": ["Ein", "B\u00e4r", "mit", "erns\u00b7tem", "Stolz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Stracks auf sie los. In Eile", "tokens": ["Stracks", "auf", "sie", "los", ".", "In", "Ei\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "PPER", "PTKVZ", "$.", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Fa\u00dft sich das k\u00fchne Weib,", "tokens": ["Fa\u00dft", "sich", "das", "k\u00fch\u00b7ne", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und schie\u00dft mit einem Pfeile", "tokens": ["Und", "schie\u00dft", "mit", "ei\u00b7nem", "Pfei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Unthier durch den Leib.", "tokens": ["Das", "Un\u00b7thier", "durch", "den", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Schnell, wie der Flug des Barden,", "tokens": ["Schnell", ",", "wie", "der", "Flug", "des", "Bar\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Rennt sie zum todten Wild,", "tokens": ["Rennt", "sie", "zum", "tod\u00b7ten", "Wild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und findet Odoarden", "tokens": ["Und", "fin\u00b7det", "O\u00b7doar\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "NE"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "In B\u00e4renhaut verh\u00fcllt.", "tokens": ["In", "B\u00e4\u00b7ren\u00b7haut", "ver\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er konnte nicht mehr sprechen,", "tokens": ["Er", "konn\u00b7te", "nicht", "mehr", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sein Auge deckt ein Flor,", "tokens": ["Sein", "Au\u00b7ge", "deckt", "ein", "Flor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch warfs ihr noch im Brechen", "tokens": ["Doch", "warfs", "ihr", "noch", "im", "Bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ihr Unrecht z\u00e4rtlich vor.", "tokens": ["Ihr", "Un\u00b7recht", "z\u00e4rt\u00b7lich", "vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Mit grauenvollem Harme", "tokens": ["Mit", "grau\u00b7en\u00b7vol\u00b7lem", "Har\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "St\u00fcrzt sich die Magd auf ihn,", "tokens": ["St\u00fcrzt", "sich", "die", "Magd", "auf", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und fa\u00dft ihn in die Arme;", "tokens": ["Und", "fa\u00dft", "ihn", "in", "die", "Ar\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Umsonst! er war dahin.", "tokens": ["Um\u00b7sonst", "!", "er", "war", "da\u00b7hin", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VAFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie heult, sie st\u00f6hnt, sie klaget,", "tokens": ["Sie", "heult", ",", "sie", "st\u00f6hnt", ",", "sie", "kla\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie rauft das Haar sich aus,", "tokens": ["Sie", "rauft", "das", "Haar", "sich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Springt auf ihr Pferd und jaget,", "tokens": ["Springt", "auf", "ihr", "Pferd", "und", "ja\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bla\u00df wie der Tod, nach Haus.", "tokens": ["Bla\u00df", "wie", "der", "Tod", ",", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"]