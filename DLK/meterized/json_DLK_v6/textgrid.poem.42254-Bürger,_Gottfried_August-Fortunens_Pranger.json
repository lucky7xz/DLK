{"textgrid.poem.42254": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Fortunens Pranger", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nieten? Nieten? Nichts als kahle Nieten? \u2013", "tokens": ["Nie\u00b7ten", "?", "Nie\u00b7ten", "?", "Nichts", "als", "kah\u00b7le", "Nie\u00b7ten", "?", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$.", "NN", "$.", "PIS", "KOKOM", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nun so niete dich denn satt und matt! \u2013", "tokens": ["Nun", "so", "nie\u00b7te", "dich", "denn", "satt", "und", "matt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Zur Vergeltung will ich dir auch bieten,", "tokens": ["Zur", "Ver\u00b7gel\u00b7tung", "will", "ich", "dir", "auch", "bie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Was noch keiner dir geboten hat.", "tokens": ["Was", "noch", "kei\u00b7ner", "dir", "ge\u00b7bo\u00b7ten", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PIS", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Nicht mit Erbsen mu\u00df man nach dir schnellen,", "tokens": ["Nicht", "mit", "Erb\u00b7sen", "mu\u00df", "man", "nach", "dir", "schnel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VMFIN", "PIS", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie ein Lustigmacher etwa schnellt:", "tokens": ["Wie", "ein", "Lus\u00b7tig\u00b7ma\u00b7cher", "et\u00b7wa", "schnellt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "An den Pranger, und in Eisenschellen,", "tokens": ["An", "den", "Pran\u00b7ger", ",", "und", "in", "Ei\u00b7sen\u00b7schel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sei, Fortuna, schimpflich ausgestellt! \u2013", "tokens": ["Sei", ",", "For\u00b7tu\u00b7na", ",", "schimpf\u00b7lich", "aus\u00b7ge\u00b7stellt", "!", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VAFIN", "$,", "NN", "$,", "ADJD", "VVPP", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "R\u00fcstig, ihr Verwandten meiner Leier,", "tokens": ["R\u00fcs\u00b7tig", ",", "ihr", "Ver\u00b7wand\u00b7ten", "mei\u00b7ner", "Lei\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Satyrbuben, auf! Verschont sie nicht!", "tokens": ["Sa\u00b7tyr\u00b7bu\u00b7ben", ",", "auf", "!", "Ver\u00b7schont", "sie", "nicht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKVZ", "$.", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Alle faulen \u00c4pfel \u2013 puh! \u2013 und Eier", "tokens": ["Al\u00b7le", "fau\u00b7len", "\u00c4p\u00b7fel", "\u2013", "puh", "!", "\u2013", "und", "Ei\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$(", "ADJD", "$.", "$(", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Werft der B\u00fcbin in da\u00df Angesicht!", "tokens": ["Werft", "der", "B\u00fc\u00b7bin", "in", "da\u00df", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "KOUS", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Denn sie ist, sie ist die Ehrenlose,", "tokens": ["Denn", "sie", "ist", ",", "sie", "ist", "die", "Eh\u00b7ren\u00b7lo\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die das \u00e4rgste Schandgesindel liebt,", "tokens": ["Die", "das", "\u00e4rgs\u00b7te", "Schand\u00b7ge\u00b7sin\u00b7del", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und nur selten ihrer Wollust Rose", "tokens": ["Und", "nur", "sel\u00b7ten", "ih\u00b7rer", "Wol\u00b7lust", "Ro\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "PPOSAT", "NN", "NE"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Einem Biedermann zu kosten gibt.", "tokens": ["Ei\u00b7nem", "Bie\u00b7der\u00b7mann", "zu", "kos\u00b7ten", "gibt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Ha, der Frechen! die so unverhohlen,", "tokens": ["Ha", ",", "der", "Fre\u00b7chen", "!", "die", "so", "un\u00b7ver\u00b7hoh\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "$.", "ART", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mir nichts, dir nichts! falsche M\u00fcnzen schl\u00e4gt,", "tokens": ["Mir", "nichts", ",", "dir", "nichts", "!", "fal\u00b7sche", "M\u00fcn\u00b7zen", "schl\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "$,", "PPER", "PIS", "$.", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und aus Lumpenkupfer die Pistolen,", "tokens": ["Und", "aus", "Lum\u00b7pen\u00b7kup\u00b7fer", "die", "Pis\u00b7to\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und aus Gold die Lumpenheller pr\u00e4gt!", "tokens": ["Und", "aus", "Gold", "die", "Lum\u00b7pen\u00b7hel\u00b7ler", "pr\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "O wie manchem edlen Tugendsohne", "tokens": ["O", "wie", "man\u00b7chem", "ed\u00b7len", "Tu\u00b7gend\u00b7soh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "G\u00f6nnte sie kaum seinen Bettelstab,", "tokens": ["G\u00f6nn\u00b7te", "sie", "kaum", "sei\u00b7nen", "Bet\u00b7tel\u00b7stab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sie, die dennoch Zepter, Reich und Krone", "tokens": ["Sie", ",", "die", "den\u00b7noch", "Zep\u00b7ter", ",", "Reich", "und", "Kro\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ADV", "NN", "$,", "NN", "KON", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Oft dem tollsten Oran-Utan gab!", "tokens": ["Oft", "dem", "tolls\u00b7ten", "O\u00b7ran\u00b7U\u00b7tan", "gab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Mit dem R\u00e4uber zieht sie aus zum Raube;", "tokens": ["Mit", "dem", "R\u00e4u\u00b7ber", "zieht", "sie", "aus", "zum", "Rau\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Selbst dem M\u00f6rder f\u00fchrt sie oft den Stahl.", "tokens": ["Selbst", "dem", "M\u00f6r\u00b7der", "f\u00fchrt", "sie", "oft", "den", "Stahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wie sie rupft dem Habicht Lamm und Taube,", "tokens": ["Wie", "sie", "rupft", "dem", "Ha\u00b7bicht", "Lamm", "und", "Tau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Zupft sie jenem Wais' und Witwe kahl.", "tokens": ["Zupft", "sie", "je\u00b7nem", "Wais'", "und", "Wit\u00b7we", "kahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "KON", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Seht, wie sie beim Beutelschneider stehet,", "tokens": ["Seht", ",", "wie", "sie", "beim", "Beu\u00b7tel\u00b7schnei\u00b7der", "ste\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und dem Gauner, den der W\u00fcrfel n\u00e4hrt,", "tokens": ["Und", "dem", "Gau\u00b7ner", ",", "den", "der", "W\u00fcr\u00b7fel", "n\u00e4hrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Zum Gewinn die Schinderknochen drehet,", "tokens": ["Zum", "Ge\u00b7winn", "die", "Schin\u00b7der\u00b7kno\u00b7chen", "dre\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und dem frommen Tropf die Taschen leert!", "tokens": ["Und", "dem", "from\u00b7men", "Tropf", "die", "Ta\u00b7schen", "leert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Wie sie dort den Mann von Treu' und Glauben", "tokens": ["Wie", "sie", "dort", "den", "Mann", "von", "Treu'", "und", "Glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In der Heuchlerlarve fein beschnellt,", "tokens": ["In", "der", "Heuch\u00b7ler\u00b7lar\u00b7ve", "fein", "be\u00b7schnellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und, ihm vollends Rock und Hemd zu rauben,", "tokens": ["Und", ",", "ihm", "vol\u00b7lends", "Rock", "und", "Hemd", "zu", "rau\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "ADV", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nachts dem Diebe gar die Leiter h\u00e4lt!", "tokens": ["Nachts", "dem", "Die\u00b7be", "gar", "die", "Lei\u00b7ter", "h\u00e4lt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Ha, mit Treue wei\u00df sie umzuspringen,", "tokens": ["Ha", ",", "mit", "Treu\u00b7e", "wei\u00df", "sie", "um\u00b7zu\u00b7sprin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "NN", "VVFIN", "PPER", "VVIZU", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie die Katze mit der armen Maus!", "tokens": ["Wie", "die", "Kat\u00b7ze", "mit", "der", "ar\u00b7men", "Maus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wahrheit kann von ihr ein Liedchen singen,", "tokens": ["Wahr\u00b7heit", "kann", "von", "ihr", "ein", "Lied\u00b7chen", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "PPOSAT", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wahrheit, oft verjagt von Amt und Haus!", "tokens": ["Wahr\u00b7heit", ",", "oft", "ver\u00b7jagt", "von", "Amt", "und", "Haus", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Doch den Auswurf von den \u00e4rgsten Schelmen", "tokens": ["Doch", "den", "Aus\u00b7wurf", "von", "den", "\u00e4rgs\u00b7ten", "Schel\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Lohnte sie, f\u00fcr seine Heuchelkunst,", "tokens": ["Lohn\u00b7te", "sie", ",", "f\u00fcr", "sei\u00b7ne", "Heu\u00b7chel\u00b7kunst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Oft mit Sternen, oft mit Ritterhelmen", "tokens": ["Oft", "mit", "Ster\u00b7nen", ",", "oft", "mit", "Rit\u00b7ter\u00b7hel\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "$,", "ADV", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und mit \u00dcberschwang von F\u00fcrstengunst. \u2013", "tokens": ["Und", "mit", "\u00dc\u00b7bersc\u00b7hwang", "von", "F\u00fcrs\u00b7ten\u00b7gunst", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "NN", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Wird sie stets zum Tapfern sich gesellen,", "tokens": ["Wird", "sie", "stets", "zum", "Tap\u00b7fern", "sich", "ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der f\u00fcr die gerechte Sache kriegt? \u2013", "tokens": ["Der", "f\u00fcr", "die", "ge\u00b7rech\u00b7te", "Sa\u00b7che", "kriegt", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u00d6fter haben Schurken und Rebellen,", "tokens": ["\u00d6f\u00b7ter", "ha\u00b7ben", "Schur\u00b7ken", "und", "Re\u00b7bel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ohne Recht, durch ihre Hand gesiegt. \u2013", "tokens": ["Oh\u00b7ne", "Recht", ",", "durch", "ih\u00b7re", "Hand", "ge\u00b7siegt", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "Dennoch wird im kurzen alle Gnade", "tokens": ["Den\u00b7noch", "wird", "im", "kur\u00b7zen", "al\u00b7le", "Gna\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "ADJA", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ihren Buhlen oft zum Ungewinn;", "tokens": ["Ih\u00b7ren", "Buh\u00b7len", "oft", "zum", "Un\u00b7ge\u00b7winn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wie im M\u00e4rchen der Scheherezade", "tokens": ["Wie", "im", "M\u00e4r\u00b7chen", "der", "Sche\u00b7he\u00b7re\u00b7za\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Von der geilen Zauberk\u00f6nigin.", "tokens": ["Von", "der", "gei\u00b7len", "Zau\u00b7ber\u00b7k\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.14": {"line.1": {"text": "Labe hie\u00df sie. Buhlerisch gewogen", "tokens": ["La\u00b7be", "hie\u00df", "sie", ".", "Buh\u00b7le\u00b7risch", "ge\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$.", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "War sie manchem jungen sch\u00f6nen Mann!", "tokens": ["War", "sie", "man\u00b7chem", "jun\u00b7gen", "sch\u00f6\u00b7nen", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Doch, sobald sie satt der Lust gepflogen,", "tokens": ["Doch", ",", "so\u00b7bald", "sie", "satt", "der", "Lust", "ge\u00b7pflo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Spie sie hui und pfui! sein Antlitz an.", "tokens": ["Spie", "sie", "hui", "und", "pfui", "!", "sein", "Ant\u00b7litz", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.15": {"line.1": {"text": "Hui und pfui! ward er zum Ungeheuer,", "tokens": ["Hui", "und", "pfui", "!", "ward", "er", "zum", "Un\u00b7ge\u00b7heu\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "KON", "ITJ", "$.", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dessen Namen ihre Zunge sprach.", "tokens": ["Des\u00b7sen", "Na\u00b7men", "ih\u00b7re", "Zun\u00b7ge", "sprach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ihren Kitzel stillte bald ein Neuer:", "tokens": ["Ih\u00b7ren", "Kit\u00b7zel", "still\u00b7te", "bald", "ein", "Neu\u00b7er", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Aber immer traf ihn gleiche Schmach.", "tokens": ["A\u00b7ber", "im\u00b7mer", "traf", "ihn", "glei\u00b7che", "Schmach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.16": {"line.1": {"text": "Eben so schon tausendmal gehandelt", "tokens": ["E\u00b7ben", "so", "schon", "tau\u00b7send\u00b7mal", "ge\u00b7han\u00b7delt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ADV", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hat die B\u00fcbin, die wir ausgestellt.", "tokens": ["Hat", "die", "B\u00fc\u00b7bin", ",", "die", "wir", "aus\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Oft ihr liebster Liebling wird verwandelt", "tokens": ["Oft", "ihr", "liebs\u00b7ter", "Lieb\u00b7ling", "wird", "ver\u00b7wan\u00b7delt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Durch die Zauberst\u00e4bchen, Ehr' und Geld.", "tokens": ["Durch", "die", "Zau\u00b7ber\u00b7st\u00e4b\u00b7chen", ",", "Ehr'", "und", "Geld", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.17": {"line.1": {"text": "Ihro Hoch- Hochehr- und Wohlehrw\u00fcrden", "tokens": ["Ih\u00b7ro", "Hoch", "Hoch\u00b7ehr", "und", "Woh\u00b7lehr\u00b7w\u00fcr\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "TRUNC", "TRUNC", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schaffet sie zu Hammeln, fett und dumm,", "tokens": ["Schaf\u00b7fet", "sie", "zu", "Ham\u00b7meln", ",", "fett", "und", "dumm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Bl\u00f6kend, wie die Br\u00fcder in den H\u00fcrden,", "tokens": ["Bl\u00f6\u00b7kend", ",", "wie", "die", 