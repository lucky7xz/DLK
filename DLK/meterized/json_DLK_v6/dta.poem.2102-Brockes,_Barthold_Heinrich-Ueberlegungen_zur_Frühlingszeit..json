{"dta.poem.2102": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ueberlegungen zur Fr\u00fchlingszeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Gott! der, j\u00fcngst noch, d\u00fcrre Baum ist ganz", "tokens": ["Mein", "Gott", "!", "der", ",", "j\u00fcngst", "noch", ",", "d\u00fcr\u00b7re", "Baum", "ist", "ganz"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "ART", "$,", "ADV", "ADV", "$,", "ADJA", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "mit gr\u00fcnem Laub bedecket,", "tokens": ["mit", "gr\u00fc\u00b7nem", "Laub", "be\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es haben sich die nackten Aeste in gr\u00fcne Bl\u00e4tter ganz", "tokens": ["Es", "ha\u00b7ben", "sich", "die", "nack\u00b7ten", "A\u00b7es\u00b7te", "in", "gr\u00fc\u00b7ne", "Bl\u00e4t\u00b7ter", "ganz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "verstecket,", "tokens": ["ver\u00b7ste\u00b7cket", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Was todt schien, scheinet itzt zu leben, sein ganzes Wesen", "tokens": ["Was", "todt", "schien", ",", "schei\u00b7net", "itzt", "zu", "le\u00b7ben", ",", "sein", "gan\u00b7zes", "We\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ADJD", "VVFIN", "$,", "VVFIN", "ADV", "PTKZU", "VVINF", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "scheint verwandelt.", "tokens": ["scheint", "ver\u00b7wan\u00b7delt", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Woher entstehet die Ver\u00e4ndrung? Wer hat die B\u00e4ume", "tokens": ["Wo\u00b7her", "ent\u00b7ste\u00b7het", "die", "Ver\u00b7\u00e4n\u00b7drung", "?", "Wer", "hat", "die", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$.", "PWS", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "so behandelt?", "tokens": ["so", "be\u00b7han\u00b7delt", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Wie geht die\u00df zu? fragt die Vernunft: doch kann sie", "tokens": ["Wie", "geht", "die\u00df", "zu", "?", "fragt", "die", "Ver\u00b7nunft", ":", "doch", "kann", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PDS", "PTKVZ", "$.", "VVFIN", "ART", "NN", "$.", "ADV", "VMFIN", "PPER"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "dieses nicht ergr\u00fcnden,", "tokens": ["die\u00b7ses", "nicht", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Nur die Erfahrung blo\u00df allein, die gro\u00dfe Lehrerinn,", "tokens": ["Nur", "die", "Er\u00b7fah\u00b7rung", "blo\u00df", "al\u00b7lein", ",", "die", "gro\u00b7\u00dfe", "Leh\u00b7re\u00b7rinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.12": {"text": "kann finden,", "tokens": ["kann", "fin\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Da es im Lenzen stets geschieht, da\u00df es durch keine", "tokens": ["Da", "es", "im", "Len\u00b7zen", "stets", "ge\u00b7schieht", ",", "da\u00df", "es", "durch", "kei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADV", "VVFIN", "$,", "KOUS", "PPER", "APPR", "PIAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Zauberey,", "tokens": ["Zau\u00b7be\u00b7rey", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Wie es jedoch das Ansehn hat, gewirket und entstanden", "tokens": ["Wie", "es", "je\u00b7doch", "das", "An\u00b7sehn", "hat", ",", "ge\u00b7wir\u00b7ket", "und", "ent\u00b7stan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VAFIN", "$,", "VVPP", "KON", "ADJA"], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.16": {"text": "sey.", "tokens": ["sey", "."], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "+", "measure": "single.up"}, "line.17": {"text": "So bleibt denn der Vernunft nichts \u00fcbrig, wenn sie es,", "tokens": ["So", "bleibt", "denn", "der", "Ver\u00b7nunft", "nichts", "\u00fcb\u00b7rig", ",", "wenn", "sie", "es", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "PIS", "PTKVZ", "$,", "KOUS", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "wie sie soll, betrachtet,", "tokens": ["wie", "sie", "soll", ",", "be\u00b7trach\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "$,", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "Als da\u00df sie dieses f\u00fcr ein Wunder des wirkenden Natur-", "tokens": ["Als", "da\u00df", "sie", "die\u00b7ses", "f\u00fcr", "ein", "Wun\u00b7der", "des", "wir\u00b7ken\u00b7den", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "PDAT", "APPR", "ART", "NN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.20": {"text": "geists achtet.", "tokens": ["geists", "ach\u00b7tet", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "Allein, was ist denn der Naturgeist? wird man sie", "tokens": ["Al\u00b7lein", ",", "was", "ist", "denn", "der", "Na\u00b7tur\u00b7geist", "?", "wird", "man", "sie"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PWS", "VAFIN", "ADV", "ART", "NN", "$.", "VAFIN", "PIS", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "darauf billig fragen.", "tokens": ["da\u00b7rauf", "bil\u00b7lig", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.23": {"text": "Hierauf wird, wenn sie redlich beichtet, sie dir nichts", "tokens": ["Hier\u00b7auf", "wird", ",", "wenn", "sie", "red\u00b7lich", "beich\u00b7tet", ",", "sie", "dir", "nichts"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VAFIN", "$,", "KOUS", "PPER", "ADJD", "VVPP", "$,", "PPER", "PPER", "PIS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "anders k\u00f6nnen sagen,", "tokens": ["an\u00b7ders", "k\u00f6n\u00b7nen", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.25": {"text": "Als: Er mu\u00df eine rege Kraft, die ordentlich verf\u00e4hret,", "tokens": ["Als", ":", "Er", "mu\u00df", "ei\u00b7ne", "re\u00b7ge", "Kraft", ",", "die", "or\u00b7dent\u00b7lich", "ver\u00b7f\u00e4h\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$.", "PPER", "VMFIN", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.26": {"text": "seyn;", "tokens": ["seyn", ";"], "token_info": ["word", "punct"], "pos": ["VAINF", "$."], "meter": "+", "measure": "single.up"}, "line.27": {"text": "(denn alles, was sie wirkt und formt, stimmt mit ein-", "tokens": ["(", "denn", "al\u00b7les", ",", "was", "sie", "wirkt", "und", "formt", ",", "stimmt", "mit", "ein"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "KON", "PIS", "$,", "PRELS", "PPER", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "APPR", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.28": {"text": "ander \u00fcberein", "tokens": ["an\u00b7der", "\u00fc\u00b7be\u00b7re\u00b7in"], "token_info": ["word", "word"], "pos": ["ADJD", "PTKVZ"], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}