{"dta.poem.13563": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Rettung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ich spielt', ein frohes Kind, im Morgenscheine,", "tokens": ["Ich", "spielt'", ",", "ein", "fro\u00b7hes", "Kind", ",", "im", "Mor\u00b7gen\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Fr\u00fchling schlug die Augen auf so helle,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "schlug", "die", "Au\u00b7gen", "auf", "so", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hinunter reisten Str\u00f6m' und Wolken schnelle,", "tokens": ["Hin\u00b7un\u00b7ter", "reis\u00b7ten", "Str\u00f6m'", "und", "Wol\u00b7ken", "schnel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich streckt' die Arme nach in's Blaue, Reine.", "tokens": ["Ich", "streckt'", "die", "Ar\u00b7me", "nach", "in's", "Blau\u00b7e", ",", "Rei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "APPRART", "ADJA", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Noch wu\u00dft' ich's selbst nicht, was das alles meine:", "tokens": ["Noch", "wu\u00dft'", "ich's", "selbst", "nicht", ",", "was", "das", "al\u00b7les", "mei\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "PTKNEG", "$,", "PRELS", "ART", "PIS", "PPOSAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Lerch', der Wald, der L\u00fcfte blaue Welle,", "tokens": ["Die", "Ler\u00b7ch'", ",", "der", "Wald", ",", "der", "L\u00fcf\u00b7te", "blau\u00b7e", "Wel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und tr\u00e4umend stand ich an des Fr\u00fchlings Schwelle,", "tokens": ["Und", "tr\u00e4u\u00b7mend", "stand", "ich", "an", "des", "Fr\u00fch\u00b7lings", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von fern rief's immer fort: Ich bin die Deine!", "tokens": ["Von", "fern", "rie\u00b7f's", "im\u00b7mer", "fort", ":", "Ich", "bin", "die", "Dei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVFIN", "ADV", "PTKVZ", "$.", "PPER", "VAFIN", "ART", "PPOSAT", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Da kam ein alter Mann gegangen", "tokens": ["Da", "kam", "ein", "al\u00b7ter", "Mann", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit hohlen Augen und bleichen Wangen,", "tokens": ["Mit", "hoh\u00b7len", "Au\u00b7gen", "und", "blei\u00b7chen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er schlich gebogen und schien so krank;", "tokens": ["Er", "schlich", "ge\u00b7bo\u00b7gen", "und", "schien", "so", "krank", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich gr\u00fc\u00dft' ihn sch\u00f6n, doch f\u00fcr den Dank", "tokens": ["Ich", "gr\u00fc\u00dft'", "ihn", "sch\u00f6n", ",", "doch", "f\u00fcr", "den", "Dank"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Fa\u00dft' er mich t\u00fcckisch schnell von hinten,", "tokens": ["Fa\u00dft'", "er", "mich", "t\u00fc\u00b7ckisch", "schnell", "von", "hin\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "ADJD", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Schlang um die Arme mir dreifache Binden,", "tokens": ["Schlang", "um", "die", "Ar\u00b7me", "mir", "drei\u00b7fa\u00b7che", "Bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPER", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.7": {"text": "Und wie ich rang und um H\u00fclfe rief,", "tokens": ["Und", "wie", "ich", "rang", "und", "um", "H\u00fcl\u00b7fe", "rief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "KON", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Geschwind noch ein andrer zum Alten lief,", "tokens": ["Ge\u00b7schwind", "noch", "ein", "an\u00b7drer", "zum", "Al\u00b7ten", "lief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und von allen Seiten kamen Menschen gelaufen,", "tokens": ["Und", "von", "al\u00b7len", "Sei\u00b7ten", "ka\u00b7men", "Men\u00b7schen", "ge\u00b7lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVFIN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Ein dunkelverworr'ner, tr\u00fcbseeliger Haufen.", "tokens": ["Ein", "dun\u00b7kel\u00b7ver\u00b7worr'\u00b7ner", ",", "tr\u00fcb\u00b7see\u00b7li\u00b7ger", "Hau\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+-++--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Die dr\u00e4ngten mich gar t\u00fcckisch in ihre Mitte,", "tokens": ["Die", "dr\u00e4ng\u00b7ten", "mich", "gar", "t\u00fc\u00b7ckisch", "in", "ih\u00b7re", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "F\u00fchrten durch's Land mich mit eiligem Schritte.", "tokens": ["F\u00fchr\u00b7ten", "durch's", "Land", "mich", "mit", "ei\u00b7li\u00b7gem", "Schrit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.13": {"text": "Wie wandt' ich sehnend mich oft zur\u00fccke!", "tokens": ["Wie", "wandt'", "ich", "seh\u00b7nend", "mich", "oft", "zu\u00b7r\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Die Heimath schickte mir Abschiedsblicke;", "tokens": ["Die", "Hei\u00b7math", "schick\u00b7te", "mir", "Ab\u00b7schieds\u00b7bli\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Die B\u00fcsche langten nach mir mit gr\u00fcnen Armen,", "tokens": ["Die", "B\u00fc\u00b7sche", "lang\u00b7ten", "nach", "mir", "mit", "gr\u00fc\u00b7nen", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Es schrie'n alle V\u00f6glein recht zum Erbarmen.", "tokens": ["Es", "schrie'n", "al\u00b7le", "V\u00f6\u00b7glein", "recht", "zum", "Er\u00b7bar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Doch die Alten h\u00f6rten nicht die fernen Lieder,", "tokens": ["Doch", "die", "Al\u00b7ten", "h\u00f6r\u00b7ten", "nicht", "die", "fer\u00b7nen", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.18": {"text": "Sumsten d\u00fcstere Worte nur hin und wieder,", "tokens": ["Sum\u00b7sten", "d\u00fcs\u00b7te\u00b7re", "Wor\u00b7te", "nur", "hin", "und", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "ADV", "PTKVZ", "KON", "ADV", "$,"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "F\u00fchrten mich endlich in ein altes Haus,", "tokens": ["F\u00fchr\u00b7ten", "mich", "end\u00b7lich", "in", "ein", "al\u00b7tes", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Da wogt' es unten in Nacht und Graus,", "tokens": ["Da", "wogt'", "es", "un\u00b7ten", "in", "Nacht", "und", "Graus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da war ein H\u00e4mmern, ein Schachern und Rumoren,", "tokens": ["Da", "war", "ein", "H\u00e4m\u00b7mern", ",", "ein", "Scha\u00b7chern", "und", "Ru\u00b7mo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Als h\u00e4tte das Chaos noch nicht ausgegohren.", "tokens": ["Als", "h\u00e4t\u00b7te", "das", "Chaos", "noch", "nicht", "aus\u00b7ge\u00b7goh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Hier hielt der Alte w\u00fcrdig und breit:", "tokens": ["Hier", "hielt", "der", "Al\u00b7te", "w\u00fcr\u00b7dig", "und", "breit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Mein Sohn, sprach er zu mir, das ist die N\u00fctzlichkeit!", "tokens": ["Mein", "Sohn", ",", "sprach", "er", "zu", "mir", ",", "das", "ist", "die", "N\u00fctz\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "APPR", "PPER", "$,", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die haben wir so zum gemeinen Besten erfunden.", "tokens": ["Die", "ha\u00b7ben", "wir", "so", "zum", "ge\u00b7mei\u00b7nen", "Bes\u00b7ten", "er\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Das betrachte h\u00fcbsch flei\u00dfig und sei gescheut. \u2014", "tokens": ["Das", "be\u00b7trach\u00b7te", "h\u00fcbsch", "flei\u00b7\u00dfig", "und", "sei", "ge\u00b7scheut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "ADJD", "KON", "VAFIN", "VVPP", "$.", "$("], "meter": "--+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "So lie\u00dfen sie mich Armen allein und gebunden.", "tokens": ["So", "lie\u00b7\u00dfen", "sie", "mich", "Ar\u00b7men", "al\u00b7lein", "und", "ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "NN", "ADV", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Da schaut' ich weinend aus meinem Kerker", "tokens": ["Da", "schaut'", "ich", "wei\u00b7nend", "aus", "mei\u00b7nem", "Ker\u00b7ker"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hinaus in das Leben durch d\u00fcstern Erker,", "tokens": ["Hin\u00b7aus", "in", "das", "Le\u00b7ben", "durch", "d\u00fcs\u00b7tern", "Er\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und unten sah ich den Lenz sich breiten,", "tokens": ["Und", "un\u00b7ten", "sah", "ich", "den", "Lenz", "sich", "brei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "PRF", "ADJA", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bl\u00fchende Tr\u00e4ume \u00fcber die Berge schreiten,", "tokens": ["Bl\u00fc\u00b7hen\u00b7de", "Tr\u00e4u\u00b7me", "\u00fc\u00b7ber", "die", "Ber\u00b7ge", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Dr\u00fcber die blauen unendlichen Weiten.", "tokens": ["Dr\u00fc\u00b7ber", "die", "blau\u00b7en", "un\u00b7end\u00b7li\u00b7chen", "Wei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Durch's farbige Land auf blauen Fl\u00fcssen", "tokens": ["Durch's", "far\u00b7bi\u00b7ge", "Land", "auf", "blau\u00b7en", "Fl\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Zogen bunte Schifflein, die wollten mich gr\u00fc\u00dfen.", "tokens": ["Zo\u00b7gen", "bun\u00b7te", "Schif\u00b7flein", ",", "die", "woll\u00b7ten", "mich", "gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,", "PRELS", "VMFIN", "PRF", "VVINF", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Vor\u00fcber kamen die Wolken gezogen,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "ka\u00b7men", "die", "Wol\u00b7ken", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Vor\u00fcber singende V\u00f6glein geflogen;", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "sin\u00b7gen\u00b7de", "V\u00f6\u00b7glein", "ge\u00b7flo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Es wollt' der gro\u00dfe Zug mich mit fassen,", "tokens": ["Es", "wollt'", "der", "gro\u00b7\u00dfe", "Zug", "mich", "mit", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "PRF", "APPR", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Ach! Menschen, wan