{"textgrid.poem.59399": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du sollst mich doch nicht fangen,", "tokens": ["Du", "sollst", "mich", "doch", "nicht", "fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Duftschw\u00fcle Zaubernacht!", "tokens": ["Duft\u00b7schw\u00fc\u00b7le", "Zau\u00b7ber\u00b7nacht", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Es stehn mit goldnem Prangen", "tokens": ["Es", "stehn", "mit", "gold\u00b7nem", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Stern auf stiller Wacht,", "tokens": ["Die", "Stern", "auf", "stil\u00b7ler", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und machen \u00fcberm Grunde,", "tokens": ["Und", "ma\u00b7chen", "\u00fc\u00b7berm", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo du verirret bist,", "tokens": ["Wo", "du", "ver\u00b7ir\u00b7ret", "bist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Getreu die alte Runde \u2013", "tokens": ["Ge\u00b7treu", "die", "al\u00b7te", "Run\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gelobt sei Jesus Christ!", "tokens": ["Ge\u00b7lobt", "sei", "Je\u00b7sus", "Christ", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie bald in allen B\u00e4umen", "tokens": ["Wie", "bald", "in", "al\u00b7len", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geht nun die Morgenluft,", "tokens": ["Geht", "nun", "die", "Mor\u00b7gen\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie sch\u00fctteln sich in Tr\u00e4umen,", "tokens": ["Sie", "sch\u00fct\u00b7teln", "sich", "in", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und durch den roten Duft", "tokens": ["Und", "durch", "den", "ro\u00b7ten", "Duft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Eine fromme Lerche steiget,", "tokens": ["Ei\u00b7ne", "from\u00b7me", "Ler\u00b7che", "stei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn alles still noch ist,", "tokens": ["Wenn", "al\u00b7les", "still", "noch", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "ADV", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den rechten Weg dir zeiget \u2013", "tokens": ["Den", "rech\u00b7ten", "Weg", "dir", "zei\u00b7get", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gelobt sei Jesus Christ!", "tokens": ["Ge\u00b7lobt", "sei", "Je\u00b7sus", "Christ", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}