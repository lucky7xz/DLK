{"dta.poem.9691": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf Lupam.  \n * * v. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vormahls wards f\u00fcr eine s\u00fcnde von dem jungfer-volck", "tokens": ["Vor\u00b7mahls", "wards", "f\u00fcr", "ei\u00b7ne", "s\u00fcn\u00b7de", "von", "dem", "jung\u00b7fer\u00b7volck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ART", "ADJA", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "geacht/", "tokens": ["ge\u00b7acht", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Wenn sie noch in ihrem stande m\u00e4nner h\u00e4tten angelacht.", "tokens": ["Wenn", "sie", "noch", "in", "ih\u00b7rem", "stan\u00b7de", "m\u00e4n\u00b7ner", "h\u00e4t\u00b7ten", "an\u00b7ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "ADJA", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Lupa meint; es w\u00e4re s\u00fcnd/ wof\u00fcr sie m\u00fcst ewig b\u00fcssen/", "tokens": ["Lu\u00b7pa", "meint", ";", "es", "w\u00e4\u00b7re", "s\u00fcnd", "/", "wo\u00b7f\u00fcr", "sie", "m\u00fcst", "e\u00b7wig", "b\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "PPER", "VAFIN", "ADJD", "$(", "PWAV", "PPER", "VMFIN", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Wenn sie nicht/ was m\u00e4nn