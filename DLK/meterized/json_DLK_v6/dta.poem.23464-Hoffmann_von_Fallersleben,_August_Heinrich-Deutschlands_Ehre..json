{"dta.poem.23464": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Deutschlands Ehre.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lande hab' ich viel gesehen,", "tokens": ["Lan\u00b7de", "hab'", "ich", "viel", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach den Besten blickt' ich allerw\u00e4rts:", "tokens": ["Nach", "den", "Bes\u00b7ten", "blickt'", "ich", "al\u00b7ler\u00b7w\u00e4rts", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Uebel m\u00f6ge mir geschehn,", "tokens": ["Ue\u00b7bel", "m\u00f6\u00b7ge", "mir", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn sich je bereden lie\u00df mein Herz,", "tokens": ["Wenn", "sich", "je", "be\u00b7re\u00b7den", "lie\u00df", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "VVINF", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Da\u00df ihm wohlgefalle", "tokens": ["Da\u00df", "ihm", "wohl\u00b7ge\u00b7fal\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Fremder Lande Brauch:", "tokens": ["Frem\u00b7der", "Lan\u00b7de", "Brauch", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Wenn ich l\u00fcgen wollte, lohnte mir es auch?", "tokens": ["Wenn", "ich", "l\u00fc\u00b7gen", "woll\u00b7te", ",", "lohn\u00b7te", "mir", "es", "auch", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$,", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Deutsche Zucht geht \u00fcber Alle.", "tokens": ["Deut\u00b7sche", "Zucht", "geht", "\u00fc\u00b7ber", "Al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Von der Elbe bis zum Rhein", "tokens": ["Von", "der", "El\u00b7be", "bis", "zum", "Rhein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "APPR", "APPRART", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und zur\u00fcck bis an der Ungern Land,", "tokens": ["Und", "zu\u00b7r\u00fcck", "bis", "an", "der", "Un\u00b7gern", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Da m\u00f6gen wohl die Besten sein,", "tokens": ["Da", "m\u00f6\u00b7gen", "wohl", "die", "Bes\u00b7ten", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die ich irgend auf der Erden fand.", "tokens": ["Die", "ich", "ir\u00b7gend", "auf", "der", "Er\u00b7den", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wei\u00df ich recht zu schauen", "tokens": ["Wei\u00df", "ich", "recht", "zu", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "PTKZU", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Sch\u00f6nheit, Huld und Zier,", "tokens": ["Sch\u00f6n\u00b7heit", ",", "Huld", "und", "Zier", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Hilf mir Gott, so schw\u00f6r' ich, sie sind besser hier", "tokens": ["Hilf", "mir", "Gott", ",", "so", "schw\u00f6r'", "ich", ",", "sie", "sind", "bes\u00b7ser", "hier"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPER", "NN", "$,", "ADV", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "ADJD", "ADV"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Als der andern L\u00e4nder Frauen.", "tokens": ["Als", "der", "an\u00b7dern", "L\u00e4n\u00b7der", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Z\u00fcchtig ist der deutsche Mann,", "tokens": ["Z\u00fcch\u00b7tig", "ist", "der", "deut\u00b7sche", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deutsche Frau'n sind engelsch\u00f6n und rein;", "tokens": ["Deut\u00b7sche", "Frau'n", "sind", "en\u00b7gel\u00b7sch\u00f6n", "und", "rein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Th\u00f6richt, wer sie schelten kann,", "tokens": ["Th\u00f6\u00b7richt", ",", "wer", "sie", "schel\u00b7ten", "kann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Anders wahrlich mag es nimmer sein:", "tokens": ["An\u00b7ders", "wahr\u00b7lich", "mag", "es", "nim\u00b7mer", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "ADV", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Zucht und reine Minne,", "tokens": ["Zucht", "und", "rei\u00b7ne", "Min\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wer die sucht und liebt,", "tokens": ["Wer", "die", "sucht", "und", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.7": {"text": "Komm in unser Land, wo es noch beide giebt;", "tokens": ["Komm", "in", "un\u00b7ser", "Land", ",", "wo", "es", "noch", "bei\u00b7de", "giebt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,", "PWAV", "PPER", "ADV", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Lebt' ich lange nur darinne!", "tokens": ["Lebt'", "ich", "lan\u00b7ge", "nur", "da\u00b7rin\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PAV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}