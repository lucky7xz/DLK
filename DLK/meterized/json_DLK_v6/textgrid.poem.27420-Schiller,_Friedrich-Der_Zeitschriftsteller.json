{"textgrid.poem.27420": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Zeitschriftsteller", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bald ist die Menge ges\u00e4ttigt von demokratischem Futter,", "tokens": ["Bald", "ist", "die", "Men\u00b7ge", "ge\u00b7s\u00e4t\u00b7tigt", "von", "de\u00b7mo\u00b7kra\u00b7ti\u00b7schem", "Fut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und ich wette, du steckst irgendein anderes auf.", "tokens": ["Und", "ich", "wet\u00b7te", ",", "du", "steckst", "ir\u00b7gend\u00b7ein", "an\u00b7de\u00b7res", "auf", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "PIS", "PTKVZ", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Schlechtes zu fertigen ist doch so leicht, und selber das Schlechte", "tokens": ["Schlech\u00b7tes", "zu", "fer\u00b7ti\u00b7gen", "ist", "doch", "so", "leicht", ",", "und", "sel\u00b7ber", "das", "Schlech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKZU", "VVINF", "VAFIN", "ADV", "ADV", "ADJD", "$,", "KON", "ADV", "ART", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Ist ihm zu schwer; sein Buch wird nur durch Stehlen gef\u00fcllt.", "tokens": ["Ist", "ihm", "zu", "schwer", ";", "sein", "Buch", "wird", "nur", "durch", "Steh\u00b7len", "ge\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKA", "ADJD", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}}}}