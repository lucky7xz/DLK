{"dta.poem.20569": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Zweiter Tag der Schlacht von Bautzen,  \n  am  21.  Mai  1813.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1848", "urn": "urn:nbn:de:kobv:b4-25129-1", "language": ["de:0.99"], "booktitle": "[N. N.]: Erinnerungen eines freiwilligen reitenden J\u00e4gers aus den Kriegsjahren 1813\u20131815. Berlin, 1848."}, "poem": {"stanza.1": {"line.1": {"text": "Es hat der Selbstsucht und der Ehrsucht Schimmer", "tokens": ["Es", "hat", "der", "Selbst\u00b7sucht", "und", "der", "Ehr\u00b7sucht", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dir Herz und Sinn, ", "tokens": ["Dir", "Herz", "und", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Doch sinken wird Dein weites Reich in Tr\u00fcmmer,", "tokens": ["Doch", "sin\u00b7ken", "wird", "Dein", "wei\u00b7tes", "Reich", "in", "Tr\u00fcm\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn Dich kein and\u2019rer h\u00f6h\u2019rer Leitstern f\u00fchrt.", "tokens": ["Wenn", "Dich", "kein", "an\u00b7d'\u00b7rer", "h\u00f6h'\u00b7rer", "Leits\u00b7tern", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Du, der mich hob, bedenke: Menschenthaten", "tokens": ["Du", ",", "der", "mich", "hob", ",", "be\u00b7den\u00b7ke", ":", "Men\u00b7schen\u00b7tha\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$.", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nur unter Gottes Beistand wohl gerathen.", "tokens": ["Nur", "un\u00b7ter", "Got\u00b7tes", "Bei\u00b7stand", "wohl", "ge\u00b7ra\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "La\u00df Jahre Dir, la\u00df auch Armeen schwinden,", "tokens": ["La\u00df", "Jah\u00b7re", "Dir", ",", "la\u00df", "auch", "Ar\u00b7me\u00b7en", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "PPER", "$,", "VVIMP", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du hilfst dem ab durch Deines Genius Macht.", "tokens": ["Du", "hilfst", "dem", "ab", "durch", "Dei\u00b7nes", "Ge\u00b7nius", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PTKVZ", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mag bei Dir Ungl\u00fcck Trost und H\u00fclfe finden,", "tokens": ["Mag", "bei", "Dir", "Un\u00b7gl\u00fcck", "Trost", "und", "H\u00fcl\u00b7fe", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "NN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Dir, mit Welt schlie\u00df Frieden! \u2014 Sei bedacht,", "tokens": ["Mit", "Dir", ",", "mit", "Welt", "schlie\u00df", "Frie\u00b7den", "!", "Sei", "be\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "NN", "VVFIN", "NN", "$.", "$(", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Statt zu erobern, kr\u00e4ftig zu erhalten,", "tokens": ["Statt", "zu", "er\u00b7o\u00b7bern", ",", "kr\u00e4f\u00b7tig", "zu", "er\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dann wird sich sch\u00f6nes Frankreich neu gestalten.", "tokens": ["Dann", "wird", "sich", "sch\u00f6\u00b7nes", "Fran\u00b7kreich", "neu", "ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "ADJA", "NE", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Herr willst Du werden von dem Erdenkreise;", "tokens": ["Herr", "willst", "Du", "wer\u00b7den", "von", "dem", "Er\u00b7den\u00b7krei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "C'est &#231;a, Mon Empereur, was Dich durchblitzt!", "tokens": ["C'\u00b7est", "&#231;", "a", ",", "Mon", "Em\u00b7pe\u00b7reur", ",", "was", "Dich", "durch\u00b7blitzt", "!"], "token_info": ["word", "XML_entity", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "$,", "FM", "FM", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Nie wird dies Ziel Dir in begonn\u2019ner Weise,", "tokens": ["Nie", "wird", "dies", "Ziel", "Dir", "in", "be\u00b7gonn'\u00b7ner", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDS", "NN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Jrrlicht nur des Nachruhms Dich erhitzt", "tokens": ["Das", "Jrr\u00b7licht", "nur", "des", "Nach\u00b7ruhms", "Dich", "er\u00b7hitzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Liebe nur der Menschen k\u00f6nnt\u2019 erf\u00fcllen", "tokens": ["Die", "Lie\u00b7be", "nur", "der", "Men\u00b7schen", "k\u00f6nnt'", "er\u00b7f\u00fcl\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dir jenen Wunsch, w\u00e4r\u2019s anders Gottes Willen.", "tokens": ["Dir", "je\u00b7nen", "Wunsch", ",", "w\u00e4r's", "an\u00b7ders", "Got\u00b7tes", "Wil\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "$,", "VAFIN", "ADV", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Am Bette sa\u00df der Kaiser in Gedanken,", "tokens": ["Am", "Bet\u00b7te", "sa\u00df", "der", "Kai\u00b7ser", "in", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sah dankbar ", "tokens": ["Sah", "dank\u00b7bar"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Ehre gebeut, drum reite, bitt\u2019 ich, fort! \u2014", "tokens": ["Eh\u00b7re", "ge\u00b7beut", ",", "drum", "rei\u00b7te", ",", "bitt'", "ich", ",", "fort", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "$,", "PAV", "VVFIN", "$,", "VVFIN", "PPER", "$,", "PTKVZ", "$.", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Der Kaiser ritt. \u2014 Nachts lebt in wilden Tr\u00e4umen", "tokens": ["Der", "Kai\u00b7ser", "ritt", ".", "Nachts", "lebt", "in", "wil\u00b7den", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Er bald im Orkus, bald in Aethers R\u00e4umen.", "tokens": ["Er", "bald", "im", "Or\u00b7kus", ",", "bald", "in", "A\u00b7e\u00b7thers", "R\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "$,", "ADV", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}