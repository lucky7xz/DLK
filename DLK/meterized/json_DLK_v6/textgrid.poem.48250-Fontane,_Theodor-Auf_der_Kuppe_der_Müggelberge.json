{"textgrid.poem.48250": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Auf der Kuppe der M\u00fcggelberge", "genre": "verse", "period": "N.A.", "pub_year": 1895, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber den M\u00fcggelsee setzt mich der Ferge.", "tokens": ["\u00dc\u00b7ber", "den", "M\u00fcg\u00b7gel\u00b7see", "setzt", "mich", "der", "Fer\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Nun erklettr' ich die M\u00fcggelberge,", "tokens": ["Nun", "er\u00b7klett\u00b7r'", "ich", "die", "M\u00fcg\u00b7gel\u00b7ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Mir zu H\u00e4upten rauschen die Kronen", "tokens": ["Mir", "zu", "H\u00e4up\u00b7ten", "rau\u00b7schen", "die", "Kro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wie zu Zeiten der ", "tokens": ["Wie", "zu", "Zei\u00b7ten", "der"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "ART"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Unsrer Urahnen, die hier im Eichwaldsschatten", "tokens": ["Uns\u00b7rer", "Ur\u00b7ah\u00b7nen", ",", "die", "hier", "im", "Eich\u00b7walds\u00b7schat\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "APPRART", "NN"], "meter": "+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Ihre Gottheitsst\u00e4tten hatten.", "tokens": ["Ih\u00b7re", "Got\u00b7theits\u00b7st\u00e4t\u00b7ten", "hat\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und die Spree hinauf, an Buchten und Seen,", "tokens": ["Und", "die", "Spree", "hin\u00b7auf", ",", "an", "Buch\u00b7ten", "und", "Seen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "PTKVZ", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Seh' ich wieder ihre Lager stehn,", "tokens": ["Seh'", "ich", "wie\u00b7der", "ih\u00b7re", "La\u00b7ger", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wie damals beim Aufbruch. Tausende ziehn", "tokens": ["Wie", "da\u00b7mals", "beim", "Auf\u00b7bruch", ".", "Tau\u00b7sen\u00b7de", "ziehn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADV", "APPRART", "NN", "$.", "NN", "VVINF"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hin \u00fcber die Dahme ... Der Vollmond schien.", "tokens": ["Hin", "\u00fc\u00b7ber", "die", "Dah\u00b7me", "...", "Der", "Voll\u00b7mond", "schien", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$(", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Am Eierh\u00e4uschen hebt es an:", "tokens": ["Am", "Ei\u00b7er\u00b7h\u00e4usc\u00b7hen", "hebt", "es", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Eine Vorhut, etliche drei\u00dfig Mann,", "tokens": ["Ei\u00b7ne", "Vor\u00b7hut", ",", "et\u00b7li\u00b7che", "drei\u00b7\u00dfig", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PIAT", "CARD", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ein Bardentrupp folgt von Friedrichshagen,", "tokens": ["Ein", "Bar\u00b7den\u00b7trupp", "folgt", "von", "Fried\u00b7richs\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wo noch jetzt Nachkommen die Harfe schlagen,", "tokens": ["Wo", "noch", "jetzt", "Nach\u00b7kom\u00b7men", "die", "Har\u00b7fe", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Bei Kiekemal und bei Kiekebusch", "tokens": ["Bei", "Kie\u00b7ke\u00b7mal", "und", "bei", "Kie\u00b7ke\u00b7busch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "APPR", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Blasen H\u00f6rner den Abschiedstusch;", "tokens": ["Bla\u00b7sen", "H\u00f6r\u00b7ner", "den", "Ab\u00b7schieds\u00b7tu\u00b7sch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Auf Fl\u00f6\u00dfen kommen andre geschwommen,", "tokens": ["Auf", "Fl\u00f6\u00b7\u00dfen", "kom\u00b7men", "and\u00b7re", "ge\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Haben den Weg bis Schm\u00f6ckwitz genommen,", "tokens": ["Ha\u00b7ben", "den", "Weg", "bis", "Schm\u00f6c\u00b7kwitz", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.9": {"text": "Bis Schm\u00f6ckwitz, wo, Wandel der Epochen,", "tokens": ["Bis", "Schm\u00f6c\u00b7kwitz", ",", "wo", ",", "Wan\u00b7del", "der", "E\u00b7po\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "$,", "NN", "ART", "NN", "$,"], "meter": "-++-+-+-+-", "measure": "unknown.measure.penta"}, "line.10": {"text": "Jetzt Familien Kaffee kochen.", "tokens": ["Jetzt", "Fa\u00b7mi\u00b7li\u00b7en", "Kaf\u00b7fee", "ko\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Aus der \u00bbWuhlheide\u00ab treten, wirr und verwundert,", "tokens": ["Aus", "der", "\u00bb", "Wuhl\u00b7hei\u00b7de", "\u00ab", "tre\u00b7ten", ",", "wirr", "und", "ver\u00b7wun\u00b7dert", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$(", "NN", "$(", "VVINF", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "---+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Geschwindschritts immer neue Hundert,", "tokens": ["Ge\u00b7schwind\u00b7schritts", "im\u00b7mer", "neu\u00b7e", "Hun\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und bei Woltersdorf und am D\u00e4meritz-See", "tokens": ["Und", "bei", "Wol\u00b7ters\u00b7dorf", "und", "am", "D\u00e4\u00b7me\u00b7ritz\u00b7See"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "KON", "APPRART", "NN"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Sammelt sich schon das Corps d'arm\u00e9e.", "tokens": ["Sam\u00b7melt", "sich", "schon", "das", "Corps", "d'\u00b7ar\u00b7m\u00e9e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ART", "NN", "NE", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Jetzt aber \u2013 der D\u00e4meritz ist \u00fcberschritten \u2013", "tokens": ["Jetzt", "a\u00b7ber", "\u2013", "der", "D\u00e4\u00b7me\u00b7ritz", "ist", "\u00fc\u00b7bersc\u00b7hrit\u00b7ten", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "An des Zuges Ausgang und inmitten", "tokens": ["An", "des", "Zu\u00b7ges", "Aus\u00b7gang", "und", "in\u00b7mit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Erblick' ich M\u00e4dchen, erblick' ich Fraun,", "tokens": ["Er\u00b7blick'", "ich", "M\u00e4d\u00b7chen", ",", "er\u00b7blick'", "ich", "Fraun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,", "VVFIN", "PPER", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Alle thusneldisch anzuschaun,", "tokens": ["Al\u00b7le", "thus\u00b7nel\u00b7disch", "an\u00b7zu\u00b7schaun", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "VVIZU", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.5": {"text": "Alle mit Butten, alle mit Hucken,", "tokens": ["Al\u00b7le", "mit", "But\u00b7ten", ",", "al\u00b7le", "mit", "Hu\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "NN", "$,", "PIS", "APPR", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Draus blond die kleinen Germanen kucken \u2013", "tokens": ["Draus", "blond", "die", "klei\u00b7nen", "Ger\u00b7ma\u00b7nen", "ku\u00b7cken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "So ziehen sie s\u00fcdw\u00e4rts mit Kiepen und Kobern,", "tokens": ["So", "zie\u00b7hen", "sie", "s\u00fcd\u00b7w\u00e4rts", "mit", "Kie\u00b7pen", "und", "Ko\u00b7bern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Von der M\u00fcggel aus die Welt zu erobern.", "tokens": ["Von", "der", "M\u00fcg\u00b7gel", "aus", "die", "Welt", "zu", "er\u00b7o\u00b7bern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}