{"textgrid.poem.61620": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O sieh, wie hinterm Waldgebirge sacht", "tokens": ["O", "sieh", ",", "wie", "hin\u00b7term", "Wald\u00b7ge\u00b7bir\u00b7ge", "sacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "PWAV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein sel'ger Schein emporquillt in die Nacht!", "tokens": ["Ein", "sel'\u00b7ger", "Schein", "em\u00b7por\u00b7quillt", "in", "die", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dort, in der Pinienwipfel Finsternis,", "tokens": ["Dort", ",", "in", "der", "Pi\u00b7ni\u00b7en\u00b7wip\u00b7fel", "Fins\u00b7ter\u00b7nis", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Den fl\u00fccht'gen Wagen hemmt jetzt Artemis", "tokens": ["Den", "fl\u00fccht'\u00b7gen", "Wa\u00b7gen", "hemmt", "jetzt", "Ar\u00b7te\u00b7mis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und steigt in Glanz geh\u00fcllt am Felsenhang", "tokens": ["Und", "steigt", "in", "Glanz", "ge\u00b7h\u00fcllt", "am", "Fel\u00b7sen\u00b7hang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "VVPP", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zum J\u00fcngling nieder, der ihr Herz bezwang.", "tokens": ["Zum", "J\u00fcng\u00b7ling", "nie\u00b7der", ",", "der", "ihr", "Herz", "be\u00b7zwang", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Er schlummert ahnungslos; sie weckt ihn nicht,", "tokens": ["Er", "schlum\u00b7mert", "ah\u00b7nungs\u00b7los", ";", "sie", "weckt", "ihn", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So lieblich gl\u00fcht vom Traum sein Angesicht;", "tokens": ["So", "lieb\u00b7lich", "gl\u00fcht", "vom", "Traum", "sein", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Versunken l\u00e4\u00dft sie in entz\u00fccktes Schaun", "tokens": ["Ver\u00b7sun\u00b7ken", "l\u00e4\u00dft", "sie", "in", "ent\u00b7z\u00fcck\u00b7tes", "Schaun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Auf Wang' und Stirn ihm leise K\u00fcsse taun. \u2013", "tokens": ["Auf", "Wang'", "und", "Stirn", "ihm", "lei\u00b7se", "K\u00fcs\u00b7se", "taun", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Wohl harren Erd' und Himmel unerhellt,", "tokens": ["Wohl", "har\u00b7ren", "Erd'", "und", "Him\u00b7mel", "un\u00b7er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Doch wer vergi\u00dft nicht, wenn er liebt, die Welt!", "tokens": ["Doch", "wer", "ver\u00b7gi\u00dft", "nicht", ",", "wenn", "er", "liebt", ",", "die", "Welt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da schnauben k\u00fchl vom Tau die Zelter schon,", "tokens": ["Da", "schnau\u00b7ben", "k\u00fchl", "vom", "Tau", "die", "Zel\u00b7ter", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "APPRART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie rei\u00dft sich los: \u00bbFahr wohl, Endymion!\u00ab", "tokens": ["Sie", "rei\u00dft", "sich", "los", ":", "\u00bb", "Fahr", "wohl", ",", "En\u00b7dy\u00b7mi\u00b7on", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$.", "$(", "VVIMP", "ADV", "$,", "NN", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ein einz'ger Ku\u00df noch, und mit sichrer Hand", "tokens": ["Ein", "einz'\u00b7ger", "Ku\u00df", "noch", ",", "und", "mit", "sich\u00b7rer", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Z\u00fcgel fa\u00dft sie, halb zur\u00fcckgewandt,", "tokens": ["Die", "Z\u00fc\u00b7gel", "fa\u00dft", "sie", ",", "halb", "zu\u00b7r\u00fcck\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und sanft vom Hang sich l\u00f6send, \u00fcberm Tann", "tokens": ["Und", "sanft", "vom", "Hang", "sich", "l\u00f6\u00b7send", ",", "\u00fc\u00b7berm", "Tann"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN", "PRF", "ADJD", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ins Blaue, zaudernd, schwebt ihr Lichtgespann.", "tokens": ["Ins", "Blau\u00b7e", ",", "zau\u00b7dernd", ",", "schwebt", "ihr", "Licht\u00b7ge\u00b7spann", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "VVPP", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}