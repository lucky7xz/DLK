{"textgrid.poem.28266": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Carmesin-roth h\u00e4lt man werth;", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Carmesin-roth h\u00e4lt man werth;", "tokens": ["Car\u00b7me\u00b7sin\u00b7roth", "h\u00e4lt", "man", "werth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Reines Wei\u00df wird offt begehrt;", "tokens": ["Rei\u00b7nes", "Wei\u00df", "wird", "offt", "be\u00b7gehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Purpur hat nicht schlechten Ruhm;", "tokens": ["Pur\u00b7pur", "hat", "nicht", "schlech\u00b7ten", "Ruhm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gold begehrt das Eigenthum;", "tokens": ["Gold", "be\u00b7gehrt", "das", "Ei\u00b7gen\u00b7thum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Billich aber wird geacht", "tokens": ["Bil\u00b7lich", "a\u00b7ber", "wird", "ge\u00b7acht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Farbe, die die Tugend macht.", "tokens": ["Far\u00b7be", ",", "die", "die", "Tu\u00b7gend", "macht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}