{"textgrid.poem.37704": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Mit dem Dolch r\u00fchr' ich die Zither", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbmit dem Dolch r\u00fchr' ich die Zither,", "tokens": ["\u00bb", "mit", "dem", "Dolch", "r\u00fchr'", "ich", "die", "Zi\u00b7ther", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gift ist meiner Stimme Hauchen;", "tokens": ["Gift", "ist", "mei\u00b7ner", "Stim\u00b7me", "Hau\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch sie tobt, nicht wie Gewitter,", "tokens": ["Doch", "sie", "tobt", ",", "nicht", "wie", "Ge\u00b7wit\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PTKNEG", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bebt nicht, wie Vulkanes Rauchen:", "tokens": ["Bebt", "nicht", ",", "wie", "Vul\u00b7ka\u00b7nes", "Rau\u00b7chen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "NE", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Lieblich wei\u00df sich in den T\u00f6nen", "tokens": ["Lieb\u00b7lich", "wei\u00df", "sich", "in", "den", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zorn und Rache zu vers\u00f6hnen.", "tokens": ["Zorn", "und", "Ra\u00b7che", "zu", "ver\u00b7s\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sinke Schlummer auf Entz\u00fcckte!", "tokens": ["Sin\u00b7ke", "Schlum\u00b7mer", "auf", "Ent\u00b7z\u00fcck\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, dies w\u00fcnschet der Ver\u00fcckte;", "tokens": ["Ach", ",", "dies", "w\u00fcn\u00b7schet", "der", "Ver\u00b7\u00fcck\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PDS", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Dies Erheben im Vergeben", "tokens": ["Dies", "Er\u00b7he\u00b7ben", "im", "Ver\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "NN", "APPRART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Kann Verrath euch nicht erstreben", "tokens": ["Kann", "Ver\u00b7rath", "euch", "nicht", "er\u00b7stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "NN", "PPER", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der Liebe, die sich so verkl\u00e4rt,", "tokens": ["Und", "der", "Lie\u00b7be", ",", "die", "sich", "so", "ver\u00b7kl\u00e4rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Wird noch h\u00f6h're Lust gew\u00e4hrt.\u00ab", "tokens": ["Wird", "noch", "h\u00f6h'\u00b7re", "Lust", "ge\u00b7w\u00e4hrt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbnur die Lust der Melodieen,", "tokens": ["\u00bb", "nur", "die", "Lust", "der", "Me\u00b7lo\u00b7di\u00b7e\u00b7en", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Nicht des Worts verhaltne Schmerzen", "tokens": ["Nicht", "des", "Worts", "ver\u00b7halt\u00b7ne", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dringen durch der K\u00fcsse Gl\u00fchen;", "tokens": ["Drin\u00b7gen", "durch", "der", "K\u00fcs\u00b7se", "Gl\u00fc\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn sie liebt nicht mit dem Herzen.", "tokens": ["Denn", "sie", "liebt", "nicht", "mit", "dem", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja, ihr geht es, wie dem Kinde,", "tokens": ["Ja", ",", "ihr", "geht", "es", ",", "wie", "dem", "Kin\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihr verfliegt das Wort im Winde.", "tokens": ["Ihr", "ver\u00b7fliegt", "das", "Wort", "im", "Win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Keinem ist die Sch\u00f6nheit eigen,", "tokens": ["Kei\u00b7nem", "ist", "die", "Sch\u00f6n\u00b7heit", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Allen m\u00f6chte sie sich zeigen,", "tokens": ["Al\u00b7len", "m\u00f6ch\u00b7te", "sie", "sich", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So in Worten wie in Werken,", "tokens": ["So", "in", "Wor\u00b7ten", "wie", "in", "Wer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KOKOM", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um durch Beifall sich zu st\u00e4rken;", "tokens": ["Um", "durch", "Bei\u00b7fall", "sich", "zu", "st\u00e4r\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lobst du sie, so ist sie doppelt sch\u00f6n,", "tokens": ["Lobst", "du", "sie", ",", "so", "ist", "sie", "dop\u00b7pelt", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "ADV", "VAFIN", "PPER", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Sie ist nichtig, wenn sie ungesehn.\u00ab", "tokens": ["Sie", "ist", "nich\u00b7tig", ",", "wenn", "sie", "un\u00b7ge\u00b7sehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "KOUS", "PPER", "ADJD", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}