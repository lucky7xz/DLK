{"textgrid.poem.65275": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Je h\u00f6her die Glocke,", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Je h\u00f6her die Glocke,", "tokens": ["Je", "h\u00f6\u00b7her", "die", "Glo\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Je heller der Klang:", "tokens": ["Je", "hel\u00b7ler", "der", "Klang", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Je ferner das M\u00e4dchen,", "tokens": ["Je", "fer\u00b7ner", "das", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Je lieber der Gang.", "tokens": ["Je", "lie\u00b7ber", "der", "Gang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Der Fr\u00fchling will kommen,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "will", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "O Fr\u00fchling, meine Freud'!", "tokens": ["O", "Fr\u00fch\u00b7ling", ",", "mei\u00b7ne", "Freud'", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun mach' ich meine Schuhe", "tokens": ["Nun", "mach'", "ich", "mei\u00b7ne", "Schu\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Wandern bereit.", "tokens": ["Zum", "Wan\u00b7dern", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Wohlauf durch die W\u00e4lder,", "tokens": ["Wohl\u00b7auf", "durch", "die", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wo die Nachtigall singt!", "tokens": ["Wo", "die", "Nach\u00b7ti\u00b7gall", "singt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wohlauf durch die Berge,", "tokens": ["Wohl\u00b7auf", "durch", "die", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wo 's Gemsb\u00f6cklein springt!", "tokens": ["Wo", "'s", "Gems\u00b7b\u00f6c\u00b7klein", "springt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Zwei schneewei\u00dfe T\u00e4ubchen,", "tokens": ["Zwei", "schnee\u00b7wei\u00b7\u00dfe", "T\u00e4ub\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die fliegen voraus,", "tokens": ["Die", "flie\u00b7gen", "vo\u00b7raus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und setzen sich schn\u00e4belnd", "tokens": ["Und", "set\u00b7zen", "sich", "schn\u00e4\u00b7belnd"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Auf der Hirtin ihr Haus.", "tokens": ["Auf", "der", "Hir\u00b7tin", "ihr", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Ei bist du schon munter,", "tokens": ["Ei", "bist", "du", "schon", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und bist schon so blank?", "tokens": ["Und", "bist", "schon", "so", "blank", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Gott gr\u00fc\u00df' dich, sch\u00f6n's Dirnel!", "tokens": ["Gott", "gr\u00fc\u00df'", "dich", ",", "sch\u00f6n's", "Dir\u00b7nel", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ach, der Winter war lang!", "tokens": ["Ach", ",", "der", "Win\u00b7ter", "war", "lang", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Zwei Augen wie Kirschkern',", "tokens": ["Zwei", "Au\u00b7gen", "wie", "Kirschkern'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "KOKOM", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Die Z\u00e4hne schneewei\u00df,", "tokens": ["Die", "Z\u00e4h\u00b7ne", "schnee\u00b7wei\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die Wangen wie R\u00f6slein", "tokens": ["Die", "Wan\u00b7gen", "wie", "R\u00f6s\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KOKOM", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Betracht' ich mit Flei\u00df.", "tokens": ["Be\u00b7tracht'", "ich", "mit", "Flei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Ein Mieder von Scharlach,", "tokens": ["Ein", "Mie\u00b7der", "von", "Schar\u00b7lach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ganz funkelnagelneu,", "tokens": ["Ganz", "fun\u00b7kel\u00b7na\u00b7gel\u00b7neu", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und unter dem Mieder", "tokens": ["Und", "un\u00b7ter", "dem", "Mie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ein Herzlein so treu!", "tokens": ["Ein", "Herz\u00b7lein", "so", "treu", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Und ihr Lippen, ihr Lippen,", "tokens": ["Und", "ihr", "Lip\u00b7pen", ",", "ihr", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wie preis' ich denn euch?", "tokens": ["Wie", "preis'", "ich", "denn", "euch", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "So wie ich will sprechen,", "tokens": ["So", "wie", "ich", "will", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "So k\u00fc\u00dft ihr mich gleich!", "tokens": ["So", "k\u00fc\u00dft", "ihr", "mich", "gleich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Ei Winter, ei Winter,", "tokens": ["Ei", "Win\u00b7ter", ",", "ei", "Win\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Bist immer noch hier?", "tokens": ["Bist", "im\u00b7mer", "noch", "hier", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "So darf ich doch wandern", "tokens": ["So", "darf", "ich", "doch", "wan\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "In Gedanken zu ihr.", "tokens": ["In", "Ge\u00b7dan\u00b7ken", "zu", "ihr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Auf Siebenmeilenstiefeln", "tokens": ["Auf", "Sie\u00b7ben\u00b7mei\u00b7len\u00b7stie\u00b7feln"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geht's flink von der Stell',", "tokens": ["Geht's", "flink", "von", "der", "Stell'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Auf Liebesgedanken", "tokens": ["Auf", "Lie\u00b7bes\u00b7ge\u00b7dan\u00b7ken"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Geht's siebenmal so schnell.", "tokens": ["Geht's", "sie\u00b7ben\u00b7mal", "so", "schnell", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}