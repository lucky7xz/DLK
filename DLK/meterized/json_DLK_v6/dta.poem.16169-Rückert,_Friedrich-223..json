{"dta.poem.16169": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "223.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Weiser, einst gefragt, wozu sei nutz das Leben", "tokens": ["Ein", "Wei\u00b7ser", ",", "einst", "ge\u00b7fragt", ",", "wo\u00b7zu", "sei", "nutz", "das", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "VVPP", "$,", "PWAV", "VAFIN", "NN", "ART", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf Erden?, sprach: um sich zum Himmel zu erheben.", "tokens": ["Auf", "Er\u00b7den", "?", ",", "sprach", ":", "um", "sich", "zum", "Him\u00b7mel", "zu", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "$,", "VVFIN", "$.", "APPR", "PRF", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zum Himmel wollen hier sich alle lebenden", "tokens": ["Zum", "Him\u00b7mel", "wol\u00b7len", "hier", "sich", "al\u00b7le", "le\u00b7ben\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VMFIN", "ADV", "PRF", "PIAT", "ADJA"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Erheben, alle wie verschieden strebenden.", "tokens": ["Er\u00b7he\u00b7ben", ",", "al\u00b7le", "wie", "ver\u00b7schie\u00b7den", "stre\u00b7ben\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PIS", "KOKOM", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Zum Himmel heben will der eine sich durch Ruhm,", "tokens": ["Zum", "Him\u00b7mel", "he\u00b7ben", "will", "der", "ei\u00b7ne", "sich", "durch", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "VMFIN", "ART", "ART", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der andere durch Macht und h\u00f6chstes Herscherthum;", "tokens": ["Der", "an\u00b7de\u00b7re", "durch", "Macht", "und", "h\u00f6chs\u00b7tes", "Her\u00b7scher\u00b7thum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ein dritter durch Genu\u00df der G\u00fcter dieser Erde,", "tokens": ["Ein", "drit\u00b7ter", "durch", "Ge\u00b7nu\u00df", "der", "G\u00fc\u00b7ter", "die\u00b7ser", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "NN", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein vierter durch die Flucht vor M\u00fchsal und Beschwerde;", "tokens": ["Ein", "vier\u00b7ter", "durch", "die", "Flucht", "vor", "M\u00fch\u00b7sal", "und", "Be\u00b7schwer\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ein andrer wiederum durch Duldmuth und Ertragung,", "tokens": ["Ein", "an\u00b7drer", "wie\u00b7de\u00b7rum", "durch", "Duld\u00b7muth", "und", "Er\u00b7tra\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und endlich einer durch Gebet und Weltentsagung.", "tokens": ["Und", "end\u00b7lich", "ei\u00b7ner", "durch", "Ge\u00b7bet", "und", "Wel\u00b7ten\u00b7tsa\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}}, "stanza.6": {"line.1": {"text": "Der Weise sieht die buntgetheilten Lebenskreise,", "tokens": ["Der", "Wei\u00b7se", "sieht", "die", "bunt\u00b7ge\u00b7theil\u00b7ten", "Le\u00b7bens\u00b7krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und freut sich, da\u00df soviel mit ihm auf gleicher Reise", "tokens": ["Und", "freut", "sich", ",", "da\u00df", "so\u00b7viel", "mit", "ihm", "auf", "glei\u00b7cher", "Rei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "$,", "KOUS", "PIS", "APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Verschiedne Wege gehn, er l\u00e4\u00dft sie gehn auf ihren,", "tokens": ["Ver\u00b7schied\u00b7ne", "We\u00b7ge", "gehn", ",", "er", "l\u00e4\u00dft", "sie", "gehn", "auf", "ih\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,", "PPER", "VVFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sorget im Gedr\u00e4ng nicht seinen zu verlieren.", "tokens": ["Und", "sor\u00b7get", "im", "Ge\u00b7dr\u00e4ng", "nicht", "sei\u00b7nen", "zu", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKNEG", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}