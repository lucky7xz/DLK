{"textgrid.poem.37253": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[ach, wie geht's dem heilgen Vater]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, wie geht's dem heilgen Vater!", "tokens": ["Ach", ",", "wie", "geht's", "dem", "heil\u00b7gen", "Va\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gro\u00df und schwer sind seine Lasten,", "tokens": ["Gro\u00df", "und", "schwer", "sind", "sei\u00b7ne", "Las\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Drum, o Joseph, trag den Gulden", "tokens": ["Drum", ",", "o", "Jo\u00b7se\u00b7ph", ",", "trag", "den", "Gul\u00b7den"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "$,", "FM", "NE", "$,", "VVFIN", "ART", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "In Sankt Peters Sammelkasten!", "tokens": ["In", "Sankt", "Pe\u00b7ters", "Sam\u00b7mel\u00b7kas\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "So sprach im Seelentrauerton", "tokens": ["So", "sprach", "im", "See\u00b7len\u00b7trau\u00b7er\u00b7ton"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Mutter zu dem frommen Sohn.", "tokens": ["Die", "Mut\u00b7ter", "zu", "dem", "from\u00b7men", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Joseph, nach empfangner Summe,", "tokens": ["Der", "Jo\u00b7se\u00b7ph", ",", "nach", "emp\u00b7fang\u00b7ner", "Sum\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+----+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Eilt auch sogleich ums Eck herumme,", "tokens": ["Eilt", "auch", "sog\u00b7leich", "ums", "Eck", "her\u00b7um\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Bis er das Tor des Hauses fand,", "tokens": ["Bis", "er", "das", "Tor", "des", "Hau\u00b7ses", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo eines Bockes Bildnis stand,", "tokens": ["Wo", "ei\u00b7nes", "Bo\u00b7ckes", "Bild\u00b7nis", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was man dahin gemalt mit Flei\u00df", "tokens": ["Was", "man", "da\u00b7hin", "ge\u00b7malt", "mit", "Flei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PIS", "PAV", "VVPP", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Zeichen, da\u00df hier Bockverschlei\u00df.", "tokens": ["Zum", "Zei\u00b7chen", ",", "da\u00df", "hier", "Bock\u00b7ver\u00b7schlei\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Allhier in einen k\u00fchlen Hof", "tokens": ["All\u00b7hier", "in", "ei\u00b7nen", "k\u00fch\u00b7len", "Hof"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Setzt sich der Joseph hin und soff;", "tokens": ["Setzt", "sich", "der", "Jo\u00b7se\u00b7ph", "hin", "und", "soff", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NE", "PTKVZ", "KON", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Und a\u00df dazu, je nach Bedarf,", "tokens": ["Und", "a\u00df", "da\u00b7zu", ",", "je", "nach", "Be\u00b7darf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "$,", "ADV", "APPR", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Die gute Wurst, den Radi scharf,", "tokens": ["Die", "gu\u00b7te", "Wurst", ",", "den", "Ra\u00b7di", "scharf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Bis er, was nicht gar lange w\u00e4hrt,", "tokens": ["Bis", "er", ",", "was", "nicht", "gar", "lan\u00b7ge", "w\u00e4hrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "PTKNEG", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sankt Peters Gulden aufgezehrt.", "tokens": ["Sankt", "Pe\u00b7ters", "Gul\u00b7den", "auf\u00b7ge\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun wird's ihm trauriglich zu Sinn", "tokens": ["Nun", "wird's", "ihm", "trau\u00b7rig\u00b7lich", "zu", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und stille singt er vor sich hin:", "tokens": ["Und", "stil\u00b7le", "singt", "er", "vor", "sich", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVFIN", "PPER", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach, der Tugend sch\u00f6ne Werke,", "tokens": ["Ach", ",", "der", "Tu\u00b7gend", "sch\u00f6\u00b7ne", "Wer\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gerne m\u00f6cht ich sie erwischen,", "tokens": ["Ger\u00b7ne", "m\u00f6cht", "ich", "sie", "er\u00b7wi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch ich merke, doch ich merke,", "tokens": ["Doch", "ich", "mer\u00b7ke", ",", "doch", "ich", "mer\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Immer kommt mir was dazwischen.", "tokens": ["Im\u00b7mer", "kommt", "mir", "was", "da\u00b7zwi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}