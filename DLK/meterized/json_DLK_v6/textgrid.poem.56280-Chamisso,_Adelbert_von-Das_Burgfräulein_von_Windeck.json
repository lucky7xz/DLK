{"textgrid.poem.56280": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Das Burgfr\u00e4ulein von Windeck", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Halt an den schnaubenden Rappen,", "tokens": ["Halt", "an", "den", "schnau\u00b7ben\u00b7den", "Rap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Verblendeter Rittersmann!", "tokens": ["Ver\u00b7blen\u00b7de\u00b7ter", "Rit\u00b7ters\u00b7mann", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gen Windeck fleucht, dich verlockend,", "tokens": ["Gen", "Win\u00b7deck", "fleucht", ",", "dich", "ver\u00b7lo\u00b7ckend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$,", "PPER", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der luftige Hirsch hinan.", "tokens": ["Der", "luf\u00b7ti\u00b7ge", "Hirsch", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und vor den m\u00e4chtigen T\u00fcrmen,", "tokens": ["Und", "vor", "den", "m\u00e4ch\u00b7ti\u00b7gen", "T\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vom \u00e4u\u00dfern verfallenen Tor", "tokens": ["Vom", "\u00e4u\u00b7\u00dfern", "ver\u00b7fal\u00b7le\u00b7nen", "Tor"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Durchschweifte sein Auge die Tr\u00fcmmer,", "tokens": ["Durch\u00b7schweif\u00b7te", "sein", "Au\u00b7ge", "die", "Tr\u00fcm\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Worunter das Wild sich verlor.", "tokens": ["Wo\u00b7run\u00b7ter", "das", "Wild", "sich", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Da war es so einsam und stille,", "tokens": ["Da", "war", "es", "so", "ein\u00b7sam", "und", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Es brannte die Sonne so hei\u00df,", "tokens": ["Es", "brann\u00b7te", "die", "Son\u00b7ne", "so", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Er trocknete tiefaufatmend", "tokens": ["Er", "trock\u00b7ne\u00b7te", "tie\u00b7fauf\u00b7at\u00b7mend"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Von seiner Stirne den Schwei\u00df.", "tokens": ["Von", "sei\u00b7ner", "Stir\u00b7ne", "den", "Schwei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "\u00bbwer br\u00e4chte des k\u00f6stlichen Weines", "tokens": ["\u00bb", "wer", "br\u00e4ch\u00b7te", "des", "k\u00f6st\u00b7li\u00b7chen", "Wei\u00b7nes"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PWS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Mir nur ein Trinkhorn voll,", "tokens": ["Mir", "nur", "ein", "Trink\u00b7horn", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den hier der versch\u00fcttete Keller", "tokens": ["Den", "hier", "der", "ver\u00b7sch\u00fct\u00b7te\u00b7te", "Kel\u00b7ler"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Verborgen noch hegen soll?\u00ab", "tokens": ["Ver\u00b7bor\u00b7gen", "noch", "he\u00b7gen", "soll", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "VVINF", "VMFIN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Kaum war das Wort befl\u00fcgelt", "tokens": ["Kaum", "war", "das", "Wort", "be\u00b7fl\u00fc\u00b7gelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von seinen Lippen entflohn,", "tokens": ["Von", "sei\u00b7nen", "Lip\u00b7pen", "ent\u00b7flohn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "So bog um die Efeu-Mauer", "tokens": ["So", "bog", "um", "die", "E\u00b7feu\u00b7Mau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die sorgende Schaffnerin schon.", "tokens": ["Die", "sor\u00b7gen\u00b7de", "Schaff\u00b7ne\u00b7rin", "schon", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Die zarte, die herrliche Jungfrau,", "tokens": ["Die", "zar\u00b7te", ",", "die", "herr\u00b7li\u00b7che", "Jung\u00b7frau", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In blendend wei\u00dfem Gewand,", "tokens": ["In", "blen\u00b7dend", "wei\u00b7\u00dfem", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Den Schl\u00fcsselbund im G\u00fcrtel,", "tokens": ["Den", "Schl\u00fcs\u00b7sel\u00b7bund", "im", "G\u00fcr\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Trinkhorn hoch in der Hand.", "tokens": ["Das", "Trink\u00b7horn", "hoch", "in", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Er schl\u00fcrfte mit gierigem Munde", "tokens": ["Er", "schl\u00fcrf\u00b7te", "mit", "gie\u00b7ri\u00b7gem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Den w\u00fcrzig k\u00f6stlichen Wein,", "tokens": ["Den", "w\u00fcr\u00b7zig", "k\u00f6st\u00b7li\u00b7chen", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Er schl\u00fcrfte verzehrende Flammen", "tokens": ["Er", "schl\u00fcrf\u00b7te", "ver\u00b7zeh\u00b7ren\u00b7de", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "In seinen Busen hinein.", "tokens": ["In", "sei\u00b7nen", "Bu\u00b7sen", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Des Auges klare Tiefe!", "tokens": ["Des", "Au\u00b7ges", "kla\u00b7re", "Tie\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Locken fl\u00fcssiges Gold! \u2013", "tokens": ["Der", "Lo\u00b7cken", "fl\u00fcs\u00b7si\u00b7ges", "Gold", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Es falteten seine H\u00e4nde", "tokens": ["Es", "fal\u00b7te\u00b7ten", "sei\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sich flehend um Minnesold.", "tokens": ["Sich", "fle\u00b7hend", "um", "Min\u00b7ne\u00b7sold", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Sie sah ihn an mitleidig", "tokens": ["Sie", "sah", "ihn", "an", "mit\u00b7lei\u00b7dig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und ernst und wunderbar,", "tokens": ["Und", "ernst", "und", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und war so schnell verschwunden,", "tokens": ["Und", "war", "so", "schnell", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie schnell sie erschienen war.", "tokens": ["Wie", "schnell", "sie", "er\u00b7schie\u00b7nen", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Er hat seit dieser Stunde,", "tokens": ["Er", "hat", "seit", "die\u00b7ser", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An Windecks Tr\u00fcmmer gebannt,", "tokens": ["An", "Win\u00b7decks", "Tr\u00fcm\u00b7mer", "ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nicht Ruh, nicht Rast gefunden,", "tokens": ["Nicht", "Ruh", ",", "nicht", "Rast", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PTKNEG", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und keine Hoffnung gekannt.", "tokens": ["Und", "kei\u00b7ne", "Hoff\u00b7nung", "ge\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "Er schlich im wachen Traume,", "tokens": ["Er", "schlich", "im", "wa\u00b7chen", "Trau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gespenstig, siech und bleich,", "tokens": ["Ge\u00b7spens\u00b7tig", ",", "siech", "und", "bleich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVIMP", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu sterben nicht verm\u00f6gend,", "tokens": ["Zu", "ster\u00b7ben", "nicht", "ver\u00b7m\u00f6\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und keinem Lebendigen gleich.", "tokens": ["Und", "kei\u00b7nem", "Le\u00b7ben\u00b7di\u00b7gen", "gleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Sie sagen: sie sei ihm zum andern", "tokens": ["Sie", "sa\u00b7gen", ":", "sie", "sei", "ihm", "zum", "an\u00b7dern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "$.", "PPER", "VAFIN", "PPER", "APPRART", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Erschienen nach langer Zeit,", "tokens": ["Er\u00b7schie\u00b7nen", "nach", "lan\u00b7ger", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und hab ihn gek\u00fc\u00dft auf die Lippen,", "tokens": ["Und", "hab", "ihn", "ge\u00b7k\u00fc\u00dft", "auf", "die", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und so ihn vom Leben befreit.", "tokens": ["Und", "so", "ihn", "vom", "Le\u00b7ben", "be\u00b7freit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}