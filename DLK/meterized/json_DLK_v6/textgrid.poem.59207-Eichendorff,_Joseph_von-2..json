{"textgrid.poem.59207": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sage mir mein Herz, was willst du?", "tokens": ["Sa\u00b7ge", "mir", "mein", "Herz", ",", "was", "willst", "du", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$,", "PWS", "VMFIN", "PPER", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Unstet schweift dein bunter Will;", "tokens": ["Un\u00b7stet", "schweift", "dein", "bun\u00b7ter", "Will", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "ADJD", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Manches andre Herz wohl stillst du,", "tokens": ["Man\u00b7ches", "and\u00b7re", "Herz", "wohl", "stillst", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJA", "NN", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Nur du selbst wirst niemals still.", "tokens": ["Nur", "du", "selbst", "wirst", "nie\u00b7mals", "still", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbeben, wenn ich munter singe,", "tokens": ["\u00bb", "e\u00b7ben", ",", "wenn", "ich", "mun\u00b7ter", "sin\u00b7ge", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um die Angst mir zu zerstreun,", "tokens": ["Um", "die", "Angst", "mir", "zu", "zer\u00b7streun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruh und Frieden manchen bringe,", "tokens": ["Ruh", "und", "Frie\u00b7den", "man\u00b7chen", "brin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sich viele still erfreun:", "tokens": ["Da\u00df", "sich", "vie\u00b7le", "still", "er\u00b7freun", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PIS", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Fa\u00dft mich erst recht tief Verlangen", "tokens": ["Fa\u00dft", "mich", "erst", "recht", "tief", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach viel andrer, be\u00dfrer Lust,", "tokens": ["Nach", "viel", "an\u00b7drer", ",", "be\u00df\u00b7rer", "Lust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Die die T\u00f6ne nicht erlangen \u2013", "tokens": ["Die", "die", "T\u00f6\u00b7ne", "nicht", "er\u00b7lan\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, wer sprengt die m\u00fcde Brust?\u00ab", "tokens": ["Ach", ",", "wer", "sprengt", "die", "m\u00fc\u00b7de", "Brust", "?", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "PWS", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}