{"dta.poem.2129": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gott in seinen Werken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit einer fast halb selgen Lust kann man, in Gottes", "tokens": ["Mit", "ei\u00b7ner", "fast", "halb", "sel\u00b7gen", "Lust", "kann", "man", ",", "in", "Got\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJD", "ADJA", "NN", "VMFIN", "PIS", "$,", "APPR", "NN"], "meter": "-+---+-++--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Kreaturen,", "tokens": ["Kre\u00b7a\u00b7tu\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Pracht, Zierlichkeit, Glanz, Licht und Schatten, zumalen", "tokens": ["Pracht", ",", "Zier\u00b7lich\u00b7keit", ",", "Glanz", ",", "Licht", "und", "Schat\u00b7ten", ",", "zu\u00b7ma\u00b7len"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,", "VVINF"], "meter": "-+--++-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Farben und Figuren,", "tokens": ["Far\u00b7ben", "und", "Fi\u00b7gu\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Die alle sich mit vieler Aendrung einander noch erh\u00f6hn", "tokens": ["Die", "al\u00b7le", "sich", "mit", "vie\u00b7ler", "A\u00b7en\u00b7drung", "ein\u00b7an\u00b7der", "noch", "er\u00b7h\u00f6hn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "PRF", "APPR", "PIAT", "NN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.6": {"text": "und schm\u00fccken,", "tokens": ["und", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "(wann unsrer Seelen Auge sie, wie es sie sehen sollt\u2019) er-", "tokens": ["(", "wann", "uns\u00b7rer", "See\u00b7len", "Au\u00b7ge", "sie", ",", "wie", "es", "sie", "se\u00b7hen", "sollt'", ")", "er"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["$(", "PWAV", "PPOSAT", "NN", "NN", "PPER", "$,", "PWAV", "PPER", "PPER", "VVINF", "VMFIN", "$(", "TRUNC"], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "blicken.", "tokens": ["bli\u00b7cken", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Wobey, wenn uns die wesentlich\u2019 und ordentliche Sch\u00f6n-", "tokens": ["Wo\u00b7bey", ",", "wenn", "uns", "die", "we\u00b7sent\u00b7lich'", "und", "or\u00b7dent\u00b7li\u00b7che", "Sch\u00f6n"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "ART", "NN", "KON", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "heit r\u00fchrt,", "tokens": ["heit", "r\u00fchrt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Ein jedes Theil, ein jedes Ganze uns unvermerkt zum", "tokens": ["Ein", "je\u00b7des", "Theil", ",", "ein", "je\u00b7des", "Gan\u00b7ze", "uns", "un\u00b7ver\u00b7merkt", "zum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "$,", "ART", "PIAT", "NN", "PPER", "ADV", "APPRART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Ursprung f\u00fchrt.", "tokens": ["Ur\u00b7sprung", "f\u00fchrt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}