{"dta.poem.11491": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Aria von der einsamkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Einsamkeit! mein aufenthalt!", "tokens": ["Ein\u00b7sam\u00b7keit", "!", "mein", "auf\u00b7ent\u00b7halt", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tieffe th\u00e4ler! hohe berge!", "tokens": ["Tief\u00b7fe", "th\u00e4\u00b7ler", "!", "ho\u00b7he", "ber\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwerer sorgen gr\u00fcfft\u2019 und s\u00e4rge!", "tokens": ["Schwe\u00b7rer", "sor\u00b7gen", "gr\u00fcfft'", "und", "s\u00e4r\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und du auserlesner wald,", "tokens": ["Und", "du", "au\u00b7ser\u00b7les\u00b7ner", "wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo so viel vergn\u00fcgung bl\u00fchet!", "tokens": ["Wo", "so", "viel", "ver\u00b7gn\u00fc\u00b7gung", "bl\u00fc\u00b7het", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In euch liegt der magnet, der mein gem\u00fcthe ziehet.", "tokens": ["In", "euch", "liegt", "der", "mag\u00b7net", ",", "der", "mein", "ge\u00b7m\u00fc\u00b7the", "zie\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "VVFIN", "$,", "PRELS", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Viel gemeinschafft, viel verdru\u00df!", "tokens": ["Viel", "ge\u00b7mein\u00b7schafft", ",", "viel", "ver\u00b7dru\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Grosse titel lass\u2019 ich gerne,", "tokens": ["Gros\u00b7se", "ti\u00b7tel", "lass'", "ich", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Es sind schalen ohne kerne,", "tokens": ["Es", "sind", "scha\u00b7len", "oh\u00b7ne", "ker\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die man theuer zahlen mu\u00df.", "tokens": ["Die", "man", "theu\u00b7er", "zah\u00b7len", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In der demuth lebt man sicher:", "tokens": ["In", "der", "de\u00b7muth", "lebt", "man", "si\u00b7cher", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Der scharlach kehrt sich offt in klag- und thr\u00e4nen-t\u00fccher.", "tokens": ["Der", "schar\u00b7lach", "kehrt", "sich", "offt", "in", "klag", "und", "thr\u00e4\u00b7nen\u00b7t\u00fc\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "PRF", "ADV", "APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Aber du, o einsamkeit!", "tokens": ["A\u00b7ber", "du", ",", "o", "ein\u00b7sam\u00b7keit", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Machst uns nicht mit sorgen m\u00fcde:", "tokens": ["Machst", "uns", "nicht", "mit", "sor\u00b7gen", "m\u00fc\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Du erquickest uns mit friede,", "tokens": ["Du", "er\u00b7qui\u00b7ckest", "uns", "mit", "frie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den kein harter sturm zerstreut.", "tokens": ["Den", "kein", "har\u00b7ter", "sturm", "zer\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unter deinen lorber-bl\u00e4ttern", "tokens": ["Un\u00b7ter", "dei\u00b7nen", "lor\u00b7ber\u00b7bl\u00e4t\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bef\u00fcrchtet sich mein geist vor keinen donner-wettern.", "tokens": ["Be\u00b7f\u00fcrch\u00b7tet", "sich", "mein", "geist", "vor", "kei\u00b7nen", "don\u00b7ner\u00b7wet\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Eyfersucht, verleumdung, neid", "tokens": ["Ey\u00b7fer\u00b7sucht", ",", "ver\u00b7leum\u00b7dung", ",", "neid"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nasen nur auf hohe zinnen:", "tokens": ["Na\u00b7sen", "nur", "auf", "ho\u00b7he", "zin\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihr gifftiges beginnen", "tokens": ["Und", "ihr", "giff\u00b7ti\u00b7ges", "be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Steht nach keiner einsamkeit;", "tokens": ["Steht", "nach", "kei\u00b7ner", "ein\u00b7sam\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn ein haus von schlechter stirne", "tokens": ["Denn", "ein", "haus", "von", "schlech\u00b7ter", "stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kommt solchen Furien nicht leichtlich ins gehirne.", "tokens": ["Kommt", "sol\u00b7chen", "Fu\u00b7ri\u00b7en", "nicht", "leicht\u00b7lich", "ins", "ge\u00b7hir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKNEG", "ADJD", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ich bek\u00fcmmre mich nicht viel,", "tokens": ["Ich", "be\u00b7k\u00fcmm\u00b7re", "mich", "nicht", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob mein einsames gefilde", "tokens": ["Ob", "mein", "ein\u00b7sa\u00b7mes", "ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Einem andern gleich zu wilde,", "tokens": ["Ei\u00b7nem", "an\u00b7dern", "gleich", "zu", "wil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und zu fruchtbar scheinen will;", "tokens": ["Und", "zu", "frucht\u00b7bar", "schei\u00b7nen", "will", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKA", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil ihm vor dem orte grauet,", "tokens": ["Weil", "ihm", "vor", "dem", "or\u00b7te", "grau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da man nicht viel gepr\u00e4ng\u2019 und viel gesellschafft", "tokens": ["Da", "man", "nicht", "viel", "ge\u00b7pr\u00e4ng'", "und", "viel", "ge\u00b7sell\u00b7schafft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PTKNEG", "ADV", "VVPP", "KON", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Gleichwohl macht die einsamkeit", "tokens": ["Gleich\u00b7wohl", "macht", "die", "ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meinem hertzen niemahls bange:", "tokens": ["Mei\u00b7nem", "hert\u00b7zen", "nie\u00b7mahls", "ban\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Keine stunde w\u00e4hrt zu lange,", "tokens": ["Kei\u00b7ne", "stun\u00b7de", "w\u00e4hrt", "zu", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "VVFIN", "APPR", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Man vertreibt sich schon die zeit;", "tokens": ["Man", "ver\u00b7treibt", "sich", "schon", "die", "zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn ein buch von guten sachen", "tokens": ["Denn", "ein", "buch", "von", "gu\u00b7ten", "sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kan auch den l\u00e4ngsten tag zum augenblicke mathen.", "tokens": ["Kan", "auch", "den", "l\u00e4ngs\u00b7ten", "tag", "zum", "au\u00b7gen\u00b7bli\u00b7cke", "ma\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Unsre regung, fleisch und blut", "tokens": ["Uns\u00b7re", "re\u00b7gung", ",", "fleisch", "und", "blut"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "KON", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Recht vern\u00fcnfftig zu bekriegen,", "tokens": ["Recht", "ver\u00b7n\u00fcnff\u00b7tig", "zu", "be\u00b7krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihm selber obzusiegen,", "tokens": ["Und", "ihm", "sel\u00b7ber", "ob\u00b7zu\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lernt sich nirgends sonst so gut,", "tokens": ["Lernt", "sich", "nir\u00b7gends", "sonst", "so", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als in abgelegnen b\u00fcschen,", "tokens": ["Als", "in", "ab\u00b7ge\u00b7leg\u00b7nen", "b\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo keine schmeichler uns was in die ohren zischen.", "tokens": ["Wo", "kei\u00b7ne", "schmeich\u00b7ler", "uns", "was", "in", "die", "oh\u00b7ren", "zi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "PPER", "PIS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "O gew\u00fcnschtes paradies!", "tokens": ["O", "ge\u00b7w\u00fcnschtes", "pa\u00b7ra\u00b7dies", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wo uns keine b\u00f6se schlange", "tokens": ["Wo", "uns", "kei\u00b7ne", "b\u00f6\u00b7se", "schlan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und kein schrecken-bild macht bange:", "tokens": ["Und", "kein", "schre\u00b7cken\u00b7bild", "macht", "ban\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJD", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie vergn\u00fcgt ist dein genie\u00df!", "tokens": ["Wie", "ver\u00b7gn\u00fcgt", "ist", "dein", "ge\u00b7nie\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn der zucker deiner fr\u00fcchte", "tokens": ["Denn", "der", "zu\u00b7cker", "dei\u00b7ner", "fr\u00fcch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Macht alle saure pein durch seine krafft zu nichte.", "tokens": ["Macht", "al\u00b7le", "sau\u00b7re", "pein", "durch", "sei\u00b7ne", "krafft", "zu", "nich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "APPR", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Schwermuth, unruh, grimm und spott", "tokens": ["Schwer\u00b7muth", ",", "un\u00b7ruh", ",", "grimm", "und", "spott"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Machen hier gantz kein get\u00fcmmel:", "tokens": ["Ma\u00b7chen", "hier", "gantz", "kein", "ge\u00b7t\u00fcm\u00b7mel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Friede bauet hier den himmel,", "tokens": ["Frie\u00b7de", "bau\u00b7et", "hier", "den", "him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn, wo fried ist, da ist GOtt,", "tokens": ["Denn", ",", "wo", "fried", "ist", ",", "da", "ist", "Gott", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "NN", "VAFIN", "$,", "ADV", "VAFIN", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Der auch unter schlechten kiefern", "tokens": ["Der", "auch", "un\u00b7ter", "schlech\u00b7ten", "kie\u00b7fern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Den schatten sauffter ruh uns h\u00e4uffig wei\u00df zu liefern.", "tokens": ["Den", "schat\u00b7ten", "sauff\u00b7ter", "ruh", "uns", "h\u00e4uf\u00b7fig", "wei\u00df", "zu", "lie\u00b7fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "PPER", "ADJD", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "O vergn\u00fcgtes einsam-seyn!", "tokens": ["O", "ver\u00b7gn\u00fcg\u00b7tes", "ein\u00b7sam\u00b7seyn", "!"], "token_in