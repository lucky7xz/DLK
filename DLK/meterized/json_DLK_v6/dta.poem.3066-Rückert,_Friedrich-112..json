{"dta.poem.3066": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "112.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nun ward es dir, wonach du Jahrlang dich gegr\u00e4mt;", "tokens": ["Nun", "ward", "es", "dir", ",", "wo\u00b7nach", "du", "Jahr\u00b7lang", "dich", "ge\u00b7gr\u00e4mt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,", "PWAV", "PPER", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ward dir, und du bist, mehr als erfreut, besch\u00e4mt.", "tokens": ["Es", "ward", "dir", ",", "und", "du", "bist", ",", "mehr", "als", "er\u00b7freut", ",", "be\u00b7sch\u00e4mt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "KON", "PPER", "VAFIN", "$,", "PIAT", "KOKOM", "VVFIN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Besch\u00e4mt, zu sehn, wie du so kindisch hast verlangt", "tokens": ["Be\u00b7sch\u00e4mt", ",", "zu", "sehn", ",", "wie", "du", "so", "kin\u00b7disch", "hast", "ver\u00b7langt"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "PTKZU", "VVINF", "$,", "PWAV", "PPER", "ADV", "ADJD", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach Etwas, das nun ist so Nichts, da du's erlangt.", "tokens": ["Nach", "Et\u00b7was", ",", "das", "nun", "ist", "so", "Nichts", ",", "da", "du's", "er\u00b7langt", "."], "token_info": ["word", "word", "punct", "word", "wo