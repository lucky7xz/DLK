{"textgrid.poem.64501": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "125. An den Herrn v. Gerstenberg", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gern las ich einst des braven ", "tokens": ["Gern", "las", "ich", "einst", "des", "bra\u00b7ven"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und kannte dich im ", "tokens": ["Und", "kann\u00b7te", "dich", "im"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Das Mittel sich so zu verstecken", "tokens": ["Das", "Mit\u00b7tel", "sich", "so", "zu", "ver\u00b7ste\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Willst du es ", "tokens": ["Willst", "du", "es"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Er l\u00e4\u00dft von Witz und Kunst sich als Professor h\u00f6ren,", "tokens": ["Er", "l\u00e4\u00dft", "von", "Witz", "und", "Kunst", "sich", "als", "Pro\u00b7fes\u00b7sor", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "PRF", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch merkt man, da\u00df aus ihm der Musketirer spricht.", "tokens": ["Doch", "merkt", "man", ",", "da\u00df", "aus", "ihm", "der", "Mus\u00b7ke\u00b7ti\u00b7rer", "spricht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$,", "KOUS", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}}}}}