{"textgrid.poem.40325": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Venus Religio", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Karfreitagsruhe. F\u00fchlst du's ", "tokens": ["Kar\u00b7frei\u00b7tags\u00b7ru\u00b7he", ".", "F\u00fchlst", "du's"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$.", "NN", "NE"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.2": {"text": "dies bange Gr\u00fcn, und diesen Hauch,", "tokens": ["dies", "ban\u00b7ge", "Gr\u00fcn", ",", "und", "die\u00b7sen", "Hauch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "NN", "$,", "KON", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "der dr\u00fcber tr\u00e4umt?", "tokens": ["der", "dr\u00fc\u00b7ber", "tr\u00e4umt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PAV", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und f\u00fchlst du's, wie der Fliederstrauch", "tokens": ["Und", "f\u00fchlst", "du's", ",", "wie", "der", "Flie\u00b7der\u00b7strauch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "von Knospen perlt und \u00fcbersch\u00e4umt?", "tokens": ["von", "Knos\u00b7pen", "perlt", "und", "\u00fc\u00b7bersc\u00b7h\u00e4umt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und sehnen deine Br\u00fcste sich", "tokens": ["Und", "seh\u00b7nen", "dei\u00b7ne", "Br\u00fcs\u00b7te", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "dem Auferstehungsmorgen zu,", "tokens": ["dem", "Auf\u00b7er\u00b7ste\u00b7hungs\u00b7mor\u00b7gen", "zu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "wie's Magdalenen innerlich", "tokens": ["wie's", "Mag\u00b7da\u00b7le\u00b7nen", "in\u00b7ner\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "nicht lie\u00df in Ruh,", "tokens": ["nicht", "lie\u00df", "in", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "bis sie zum offnen Grabe schlich?", "tokens": ["bis", "sie", "zum", "off\u00b7nen", "Gra\u00b7be", "schlich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn \u00fcbermorgen graut der Tag", "tokens": ["Denn", "\u00fc\u00b7ber\u00b7mor\u00b7gen", "graut", "der", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ins Fr\u00fchlingsfeld,", "tokens": ["ins", "Fr\u00fch\u00b7lings\u00b7feld", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "da unterwarf sich Der die Welt,", "tokens": ["da", "un\u00b7ter\u00b7warf", "sich", "Der", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "den einst dein Volk daf\u00fcr gequ\u00e4lt,", "tokens": ["den", "einst", "dein", "Volk", "da\u00b7f\u00fcr", "ge\u00b7qu\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "da\u00df eine Sehnsucht in ihm lag.", "tokens": ["da\u00df", "ei\u00b7ne", "Sehn\u00b7sucht", "in", "ihm", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Viel Glocken l\u00e4uten zu mir her,", "tokens": ["Viel", "Glo\u00b7cken", "l\u00e4u\u00b7ten", "zu", "mir", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wie Grufthauch schwer, wie Lufthauch leer;", "tokens": ["wie", "Gruft\u00b7hauch", "schwer", ",", "wie", "Luft\u00b7hauch", "leer", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "$,", "PWAV", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "wem l\u00e4uten sie?", "tokens": ["wem", "l\u00e4u\u00b7ten", "sie", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Das waren Deine Glocken nie", "tokens": ["Das", "wa\u00b7ren", "Dei\u00b7ne", "Glo\u00b7cken", "nie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und sind nicht Meine Glocken mehr.", "tokens": ["und", "sind", "nicht", "Mei\u00b7ne", "Glo\u00b7cken", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Im Flieder h\u00e4ngt ein altes Laub;", "tokens": ["Im", "Flie\u00b7der", "h\u00e4ngt", "ein", "al\u00b7tes", "Laub", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "du willst nun mein sein ganz und gar.", "tokens": ["du", "willst", "nun", "mein", "sein", "ganz", "und", "gar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "VAINF", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch liegt der Hain voll Moderstaub;", "tokens": ["Noch", "liegt", "der", "Hain", "voll", "Mo\u00b7der\u00b7staub", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ist dir auch klar,", "tokens": ["ist", "dir", "auch", "klar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "---+", "measure": "unknown.measure.single"}, "line.5": {"text": "da\u00df mir dein Gott nie heilig war?!", "tokens": ["da\u00df", "mir", "dein", "Gott", "nie", "hei\u00b7lig", "war", "?!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "An seinem Grabe d\u00fcrstet mich", "tokens": ["An", "sei\u00b7nem", "Gra\u00b7be", "d\u00fcrs\u00b7tet", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "nach einer neuen Menschheit, Du!", "tokens": ["nach", "ei\u00b7ner", "neu\u00b7en", "Menschheit", ",", "Du", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fchlst du's wie ich?", "tokens": ["F\u00fchlst", "du's", "wie", "ich", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KOKOM", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Sag: sehnen deine Br\u00fcste sich", "tokens": ["Sag", ":", "seh\u00b7nen", "dei\u00b7ne", "Br\u00fcs\u00b7te", "sich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "PPOSAT", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "dieser Auferstehung zu? \u2013", "tokens": ["die\u00b7ser", "Auf\u00b7er\u00b7ste\u00b7hung", "zu", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PDAT", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": ". . . . . . . . . . . . . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$."]}}, "stanza.8": {"line.1": {"text": "Ja, so spielt'ich schier Gottvater,", "tokens": ["Ja", ",", "so", "spielt'\u00b7ich", "schier", "Gott\u00b7va\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "schwebend ob der Flucht der Zeiten.", "tokens": ["schwe\u00b7bend", "ob", "der", "Flucht", "der", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOUS", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Barg mein Allgeist nicht das Riesentheater", "tokens": ["Barg", "mein", "All\u00b7geist", "nicht", "das", "Rie\u00b7sen\u00b7the\u00b7a\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Mit aufbrausendem Gefieder", "tokens": ["Mit", "auf\u00b7brau\u00b7sen\u00b7dem", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "packt mich wie ein flammenbekr\u00e4nzter", "tokens": ["packt", "mich", "wie", "ein", "flam\u00b7men\u00b7be\u00b7kr\u00e4nz\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KOKOM", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ph\u00f6nix dieser Glaube wieder \u2013", "tokens": ["Ph\u00f6\u00b7nix", "die\u00b7ser", "Glau\u00b7be", "wie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "niemals sah ich die Nacht begl\u00e4nzter!", "tokens": ["nie\u00b7mals", "sah", "ich", "die", "Nacht", "be\u00b7gl\u00e4nz\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Barg des Weibes Schoo\u00df nicht Schicksalsspiele,", "tokens": ["Barg", "des", "Wei\u00b7bes", "Schoo\u00df", "nicht", "Schick\u00b7sals\u00b7spie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "PTKNEG", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "mehr als alle Himmelsr\u00e4ume?", "tokens": ["mehr", "als", "al\u00b7le", "Him\u00b7mels\u00b7r\u00e4u\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Herrisch rief ich sie zum Sch\u00f6pferziele,", "tokens": ["Her\u00b7risch", "rief", "ich", "sie", "zum", "Sch\u00f6p\u00b7fer\u00b7zie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "die Erf\u00fcllerin meiner Tr\u00e4ume,", "tokens": ["die", "Er\u00b7f\u00fcl\u00b7le\u00b7rin", "mei\u00b7ner", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}}}}