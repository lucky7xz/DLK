{"textgrid.poem.34349": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach ihr W\u00fcnsche junger Jahre", "tokens": ["Ach", "ihr", "W\u00fcn\u00b7sche", "jun\u00b7ger", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seyd zu gut f\u00fcr diese Welt!", "tokens": ["Seyd", "zu", "gut", "f\u00fcr", "die\u00b7se", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PTKA", "ADJD", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsre sch\u00f6nste Bl\u00fcthe f\u00e4llt,", "tokens": ["Uns\u00b7re", "sch\u00f6ns\u00b7te", "Bl\u00fc\u00b7the", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unser bester Theil gesellt", "tokens": ["Un\u00b7ser", "bes\u00b7ter", "Theil", "ge\u00b7sellt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lange vor uns sich zur Baare.", "tokens": ["Lan\u00b7ge", "vor", "uns", "sich", "zur", "Baa\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ach ihr W\u00fcnsche junger Jahre", "tokens": ["Ach", "ihr", "W\u00fcn\u00b7sche", "jun\u00b7ger", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Seyd zu gut f\u00fcr diese Welt!", "tokens": ["Seyd", "zu", "gut", "f\u00fcr", "die\u00b7se", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PTKA", "ADJD", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Als mich die Menschen noch nicht kannten,", "tokens": ["Als", "mich", "die", "Men\u00b7schen", "noch", "nicht", "kann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Engel nur mich handeln sahn,", "tokens": ["Die", "En\u00b7gel", "nur", "mich", "han\u00b7deln", "sahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn ichs ihnen recht gethan,", "tokens": ["Und", "wenn", "ichs", "ih\u00b7nen", "recht", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich lispelnd ihre Schwester nannten \u2013", "tokens": ["Mich", "lis\u00b7pelnd", "ih\u00b7re", "Schwes\u00b7ter", "nann\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach ihr W\u00fcnsche junger Jahre", "tokens": ["Ach", "ihr", "W\u00fcn\u00b7sche", "jun\u00b7ger", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "t