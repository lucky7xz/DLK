{"textgrid.poem.36735": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Claude Lorrain", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was der Historie Raffael ist, das bist du der Landschaft,", "tokens": ["Was", "der", "His\u00b7to\u00b7rie", "Raf\u00b7fael", "ist", ",", "das", "bist", "du", "der", "Land\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NE", "VAFIN", "$,", "PDS", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "---+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Eine Seele hat euch beiden den Pinsel gef\u00fchrt;", "tokens": ["Ei\u00b7ne", "See\u00b7le", "hat", "euch", "bei\u00b7den", "den", "Pin\u00b7sel", "ge\u00b7f\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PIAT", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Was der eine von lauterem Licht in den Menschen gezaubert,", "tokens": ["Was", "der", "ei\u00b7ne", "von", "lau\u00b7te\u00b7rem", "Licht", "in", "den", "Men\u00b7schen", "ge\u00b7zau\u00b7bert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ART", "APPR", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Hat der beseelten Natur dieser an Sch\u00f6nheit verliehn.", "tokens": ["Hat", "der", "be\u00b7seel\u00b7ten", "Na\u00b7tur", "die\u00b7ser", "an", "Sch\u00f6n\u00b7heit", "ver\u00b7liehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PDAT", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}}}}