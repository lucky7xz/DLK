{"textgrid.poem.52260": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Requiescat!", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer den wucht'gen Hammer schwingt;", "tokens": ["Wer", "den", "wucht'\u00b7gen", "Ham\u00b7mer", "schwingt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer im Felde m\u00e4ht die \u00c4hren;", "tokens": ["Wer", "im", "Fel\u00b7de", "m\u00e4ht", "die", "\u00c4h\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer ins Mark der Erde dringt,", "tokens": ["Wer", "ins", "Mark", "der", "Er\u00b7de", "dringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weib und Kinder zu ern\u00e4hren;", "tokens": ["Weib", "und", "Kin\u00b7der", "zu", "er\u00b7n\u00e4h\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer stroman den Nachen zieht;", "tokens": ["Wer", "stro\u00b7man", "den", "Na\u00b7chen", "zieht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wer bei Woll' und Werg der Flachse", "tokens": ["Wer", "bei", "Woll'", "und", "Werg", "der", "Flach\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "NE", "KON", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Hinterm Webestuhl sich m\u00fcht,", "tokens": ["Hin\u00b7term", "We\u00b7be\u00b7stuhl", "sich", "m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df sein blonder Junge wachse: \u2013", "tokens": ["Da\u00df", "sein", "blon\u00b7der", "Jun\u00b7ge", "wach\u00b7se", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jedem Ehre, jedem Preis!", "tokens": ["Je\u00b7dem", "Eh\u00b7re", ",", "je\u00b7dem", "Preis", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ehre jeder Handvoll Schwielen!", "tokens": ["Eh\u00b7re", "je\u00b7der", "Hand\u00b7voll", "Schwie\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ehre jedem Tropfen Schwei\u00df,", "tokens": ["Eh\u00b7re", "je\u00b7dem", "Trop\u00b7fen", "Schwei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der in H\u00fctten f\u00e4llt und M\u00fchlen!", "tokens": ["Der", "in", "H\u00fct\u00b7ten", "f\u00e4llt", "und", "M\u00fch\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ehre jeder nassen Stirn", "tokens": ["Eh\u00b7re", "je\u00b7der", "nas\u00b7sen", "Stirn"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hinterm Pfluge! \u2013 Doch auch dessen,", "tokens": ["Hin\u00b7term", "Pflu\u00b7ge", "!", "\u2013", "Doch", "auch", "des\u00b7sen", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "$(", "KON", "ADV", "PDS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Der mit Sch\u00e4del und mit Hirn", "tokens": ["Der", "mit", "Sch\u00e4\u00b7del", "und", "mit", "Hirn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NE", "KON", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Hungernd pfl\u00fcgt, sei nicht vergessen!", "tokens": ["Hun\u00b7gernd", "pfl\u00fcgt", ",", "sei", "nicht", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "$,", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ob in enger B\u00fccherei", "tokens": ["Ob", "in", "en\u00b7ger", "B\u00fc\u00b7che\u00b7rei"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Dunst und Moder ihn umst\u00e4ube;", "tokens": ["Dunst", "und", "Mo\u00b7der", "ihn", "um\u00b7st\u00e4u\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob er Sklav der Messe sei,", "tokens": ["Ob", "er", "Sklav", "der", "Mes\u00b7se", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieder oder Dramen schreibe;", "tokens": ["Lie\u00b7der", "o\u00b7der", "Dra\u00b7men", "schrei\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob er um verruchten Lohn", "tokens": ["Ob", "er", "um", "ver\u00b7ruch\u00b7ten", "Lohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Fremden Ungeschmack vertiere;", "tokens": ["Frem\u00b7den", "Un\u00b7ge\u00b7schmack", "ver\u00b7tie\u00b7re", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ob er in gelehrter Fron", "tokens": ["Ob", "er", "in", "ge\u00b7lehr\u00b7ter", "Fron"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Griechisch und Latein doziere: \u2013", "tokens": ["Grie\u00b7chisch", "und", "La\u00b7tein", "do\u00b7zie\u00b7re", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "KON", "NN", "VVFIN", "$.", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Er ist auch ein Proletar!", "tokens": ["Er", "ist", "auch", "ein", "Pro\u00b7le\u00b7tar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ihm auch hei\u00dft es: \u00bbDarbe! borge!\u00ab", "tokens": ["Ihm", "auch", "hei\u00dft", "es", ":", "\u00bb", "Dar\u00b7be", "!", "bor\u00b7ge", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$.", "$(", "NN", "$.", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihm auch bleicht das dunkle Haar,", "tokens": ["Ihm", "auch", "bleicht", "das", "dunk\u00b7le", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihn auch hetzt ins Grab die Sorge!", "tokens": ["Ihn", "auch", "hetzt", "ins", "Grab", "die", "Sor\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit dem Zwange, mit der Not", "tokens": ["Mit", "dem", "Zwan\u00b7ge", ",", "mit", "der", "Not"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie die andern mu\u00df er ringen,", "tokens": ["Wie", "die", "an\u00b7dern", "mu\u00df", "er", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der Kinder Schrei nach Brot", "tokens": ["Und", "der", "Kin\u00b7der", "Schrei", "nach", "Brot"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "APPR", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "L\u00e4hmt auch ihm die freien Schwingen!", "tokens": ["L\u00e4hmt", "auch", "ihm", "die", "frei\u00b7en", "Schwin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Manchen hab' ich so gekannt!", "tokens": ["Man\u00b7chen", "hab'", "ich", "so", "ge\u00b7kannt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach den Wolken flog sein Streben: \u2013", "tokens": ["Nach", "den", "Wol\u00b7ken", "flog", "sein", "Stre\u00b7ben", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tief im Staube von der Hand", "tokens": ["Tief", "im", "Stau\u00b7be", "von", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In den Mund doch mu\u00dft' er leben!", "tokens": ["In", "den", "Mund", "doch", "mu\u00dft'", "er", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Eingepfercht und eingedornt,", "tokens": ["Ein\u00b7ge\u00b7pfercht", "und", "ein\u00b7ge\u00b7dornt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00c4chzt' er zwischen T\u00fcr und Angel;", "tokens": ["\u00c4chzt'", "er", "zwi\u00b7schen", "T\u00fcr", "und", "An\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Der Bedarf hat ihn gespornt,", "tokens": ["Der", "Be\u00b7darf", "hat", "ihn", "ge\u00b7spornt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und gepeitscht hat ihn der Mangel.", "tokens": ["Und", "ge\u00b7peitscht", "hat", "ihn", "der", "Man\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Also schrieb er Blatt auf Blatt,", "tokens": ["Al\u00b7so", "schrieb", "er", "Blatt", "auf", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bleich und mit verh\u00e4rmten Wangen,", "tokens": ["Bleich", "und", "mit", "ver\u00b7h\u00e4rm\u00b7ten", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4hrend drau\u00dfen Blum' und Blatt", "tokens": ["W\u00e4h\u00b7rend", "drau\u00b7\u00dfen", "Blum'", "und", "Blatt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich im Morgenwinde schwangen,", "tokens": ["Sich", "im", "Mor\u00b7gen\u00b7win\u00b7de", "schwan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nachtigall und Drossel schlug,", "tokens": ["Nach\u00b7ti\u00b7gall", "und", "Dros\u00b7sel", "schlug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Lerche sang und Habicht kreiste: \u2013", "tokens": ["Ler\u00b7che", "sang", "und", "Ha\u00b7bicht", "kreis\u00b7te", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Er hing \u00fcber sein Buch,", "tokens": ["Er", "hing", "\u00fc\u00b7ber", "sein", "Buch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+---+", "measure": "dactylic.init"}, "line.8": {"text": "Tagel\u00f6hner mit dem Geiste!", "tokens": ["Ta\u00b7ge\u00b7l\u00f6h\u00b7ner", "mit", "dem", "Geis\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Dennoch ob sein Herz auch schrie,", "tokens": ["Den\u00b7noch", "ob", "sein", "Herz", "auch", "schrie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Blieb tapfer, blieb ergeben:", "tokens": ["Blieb", "tap\u00b7fer", ",", "blieb", "er\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbdieses auch ist Poesie,", "tokens": ["\u00bb", "die\u00b7ses", "auch", "ist", "Poe\u00b7sie", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDAT", "ADV", "VAFIN", "NN", "$,"], "meter": "+--++-", "measure": "dactylic.init"}, "line.4": {"text": "Denn es ist das Menschenleben!\u00ab", "tokens": ["Denn", "es", "ist", "das", "Men\u00b7schen\u00b7le\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wenn gar der Mut ihm sank,", "tokens": ["Und", "wenn", "gar", "der", "Mut", "ihm", "sank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Hielt er fest sich an dem einen:", "tokens": ["Hielt", "er", "fest", "sich", "an", "dem", "ei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "PRF", "APPR", "ART", "ART", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "\u00bbmeine Ehre wahrt' ich blank!", "tokens": ["\u00bb", "mei\u00b7ne", "Eh\u00b7re", "wahrt'", "ich", "blank", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Was ich tu', ist f\u00fcr die Meinen!\u00ab", "tokens": ["Was", "ich", "tu'", ",", "ist", "f\u00fcr", "die", "Mei\u00b7nen", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Endlich lie\u00df ihn doch die Kraft!", "tokens": ["End\u00b7lich", "lie\u00df", "ihn", "doch", "die", "Kraft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus sein Ringen, aus sein Schaffen!", "tokens": ["Aus", "sein", "Rin\u00b7gen", ",", "aus", "sein", "Schaf\u00b7fen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur zuweilen, fieberhaft,", "tokens": ["Nur", "zu\u00b7wei\u00b7len", ",", "fie\u00b7ber\u00b7haft", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Konnt' er noch empor sich raffen!", "tokens": ["Konnt'", "er", "noch", "em\u00b7por", "sich", "raf\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKVZ", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nachts oft von der Muse Ku\u00df", "tokens": ["Nachts", "oft", "von", "der", "Mu\u00b7se", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fchlt' er seine SChl\u00e4fen pochen;", "tokens": ["F\u00fchlt'", "er", "sei\u00b7ne", "Schl\u00e4\u00b7fen", "po\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Frei dann flog der Genius,", "tokens": ["Frei", "dann", "flog", "der", "Ge\u00b7nius", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Den des Tages Drang gebrochen!", "tokens": ["Den", "des", "Ta\u00b7ges", "Drang", "ge\u00b7bro\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Lang jetzt ruht er unterm Rain,", "tokens": ["Lang", "jetzt", "ruht", "er", "un\u00b7term", "Rain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Drauf im Gras die Winde w\u00fchlen;", "tokens": ["Drauf", "im", "Gras", "die", "Win\u00b7de", "w\u00fch\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne Kreuz und ohne Stein", "tokens": ["Oh\u00b7ne", "Kreuz", "und", "oh\u00b7ne", "Stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schl\u00e4ft er aus auf seinen Pf\u00fchlen.", "tokens": ["Schl\u00e4ft", "er", "aus", "auf", "sei\u00b7nen", "Pf\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Rotgeweinten Angesichts", "tokens": ["Rot\u00b7ge\u00b7wein\u00b7ten", "An\u00b7ge\u00b7sichts"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Irrt sein Weib und irrt sein Samen \u2013", "tokens": ["Irrt", "sein", "Weib", "und", "irrt", "sein", "Sa\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bettlerkinder erben nichts", "tokens": ["Bett\u00b7ler\u00b7kin\u00b7der", "er\u00b7ben", "nichts"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "PIS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Als des Vaters reinen Namen!", "tokens": ["Als", "des", "Va\u00b7ters", "rei\u00b7nen", "Na\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Ruhm und Ehre jedem Flei\u00df!", "tokens": ["Ruhm", "und", "Eh\u00b7re", "je\u00b7dem", "Flei\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ehre jeder Handvoll Schwielen!", "tokens": ["Eh\u00b7re", "je\u00b7der", "Hand\u00b7voll", "Schwie\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ehre jedem Tropfen Schwei\u00df,", "tokens": ["Eh\u00b7re", "je\u00b7dem", "Trop\u00b7fen", "Schwei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der in H\u00fctten f\u00e4llt und M\u00fchlen!", "tokens": ["Der", "in", "H\u00fct\u00b7ten", "f\u00e4llt", "und", "M\u00fch\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ehre jeder nassen Stirn", "tokens": ["Eh\u00b7re", "je\u00b7der", "nas\u00b7sen", "Stirn"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": 