{"textgrid.poem.60290": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sch\u00f6ner J\u00fcngling, bist als Held gefallen;", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6ner J\u00fcngling, bist als Held gefallen;", "tokens": ["Sch\u00f6\u00b7ner", "J\u00fcng\u00b7ling", ",", "bist", "als", "Held", "ge\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VAFIN", "KOKOM", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sieg und Ruhm in deiner letzten Stunde", "tokens": ["Sieg", "und", "Ruhm", "in", "dei\u00b7ner", "letz\u00b7ten", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "F\u00e4cheln dir die hei\u00dfe Todeswunde,", "tokens": ["F\u00e4\u00b7cheln", "dir", "die", "hei\u00b7\u00dfe", "To\u00b7des\u00b7wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Draus die Seele mu\u00df von hinnen wallen.", "tokens": ["Draus", "die", "See\u00b7le", "mu\u00df", "von", "hin\u00b7nen", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VMFIN", "APPR", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "An den Schultern narbenvolle Viere", "tokens": ["An", "den", "Schul\u00b7tern", "nar\u00b7ben\u00b7vol\u00b7le", "Vie\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Tragen dich auf deinen Grabeswegen,", "tokens": ["Tra\u00b7gen", "dich", "auf", "dei\u00b7nen", "Gra\u00b7bes\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Zu der Trommel trauerdumpfen Schl\u00e4gen", "tokens": ["Zu", "der", "Trom\u00b7mel", "trau\u00b7er\u00b7dump\u00b7fen", "Schl\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Folgen finster deine Grenadiere.", "tokens": ["Fol\u00b7gen", "fins\u00b7ter", "dei\u00b7ne", "Gre\u00b7na\u00b7die\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6ner J\u00fcngling, dir am Grabe schallen", "tokens": ["Sch\u00f6\u00b7ner", "J\u00fcng\u00b7ling", ",", "dir", "am", "Gra\u00b7be", "schal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ehrend die Kanonen ihr Geschmetter,", "tokens": ["Eh\u00b7rend", "die", "Ka\u00b7no\u00b7nen", "ihr", "Ge\u00b7schmet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wie im Walde sommerschw\u00fcle Wetter", "tokens": ["Wie", "im", "Wal\u00b7de", "som\u00b7mer\u00b7schw\u00fc\u00b7le", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Auf den toten Fr\u00fchling niederhallen!", "tokens": ["Auf", "den", "to\u00b7ten", "Fr\u00fch\u00b7ling", "nie\u00b7der\u00b7hal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}