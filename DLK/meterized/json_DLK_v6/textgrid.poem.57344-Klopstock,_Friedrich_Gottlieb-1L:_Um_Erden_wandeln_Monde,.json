{"textgrid.poem.57344": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Um Erden wandeln Monde,", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um Erden wandeln Monde,", "tokens": ["Um", "Er\u00b7den", "wan\u00b7deln", "Mon\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVFIN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erden um Sonnen,", "tokens": ["Er\u00b7den", "um", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Aller Sonnen Heere wandeln", "tokens": ["Al\u00b7ler", "Son\u00b7nen", "Hee\u00b7re", "wan\u00b7deln"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um eine grosse Sonne:", "tokens": ["Um", "ei\u00b7ne", "gros\u00b7se", "Son\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbvater unser, der du bist im Himmel!\u00ab", "tokens": ["\u00bb", "va\u00b7ter", "un\u00b7ser", ",", "der", "du", "bist", "im", "Him\u00b7mel", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "PPOSAT", "$,", "PRELS", "PPER", "VAFIN", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Auf allen diesen Welten, leuchtenden, und erleuchteten,", "tokens": ["Auf", "al\u00b7len", "die\u00b7sen", "Wel\u00b7ten", ",", "leuch\u00b7ten\u00b7den", ",", "und", "er\u00b7leuch\u00b7te\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PDAT", "NN", "$,", "VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-++-+-+", "measure": "unknown.measure.octa.plus"}, "line.2": {"text": "Wohnen Geister an Kr\u00e4ften ungleich, und an Leibern;", "tokens": ["Woh\u00b7nen", "Geis\u00b7ter", "an", "Kr\u00e4f\u00b7ten", "un\u00b7gleich", ",", "und", "an", "Lei\u00b7bern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "ADJD", "$,", "KON", "APPR", "NN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Aber alle denken Gott, und freuen sich Gottes.", "tokens": ["A\u00b7ber", "al\u00b7le", "den\u00b7ken", "Gott", ",", "und", "freu\u00b7en", "sich", "Got\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "NN", "$,", "KON", "VVFIN", "PRF", "NN", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "\u00bbgeheiliget werde dein Name.\u00ab", "tokens": ["\u00bb", "ge\u00b7hei\u00b7li\u00b7get", "wer\u00b7de", "dein", "Na\u00b7me", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVPP", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Er, der Hocherhabene,", "tokens": ["Er", ",", "der", "Ho\u00b7cher\u00b7ha\u00b7be\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Der allein ganz sich denken,", "tokens": ["Der", "al\u00b7lein", "ganz", "sich", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Seiner ganz sich freuen kann,", "tokens": ["Sei\u00b7ner", "ganz", "sich", "freu\u00b7en", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "PRF", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Machte den tiefen Entwurf", "tokens": ["Mach\u00b7te", "den", "tie\u00b7fen", "Ent\u00b7wurf"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Zur Seligkeit aller seiner Weltbewohner.", "tokens": ["Zur", "Se\u00b7lig\u00b7keit", "al\u00b7ler", "sei\u00b7ner", "Welt\u00b7be\u00b7woh\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "\u00bbzu uns komme dein Reich.\u00ab", "tokens": ["\u00bb", "zu", "uns", "kom\u00b7me", "dein", "Reich", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Wohl ihnen, dass nicht sie, dass er", "tokens": ["Wohl", "ih\u00b7nen", ",", "dass", "nicht", "sie", ",", "dass", "er"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPER", "$,", "KOUS", "PTKNEG", "PPER", "$,", "KOUS", "PPER"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ihr Jetziges, und ihr Zuk\u00fcnftiges ordnete,", "tokens": ["Ihr", "Jet\u00b7zi\u00b7ges", ",", "und", "ihr", "Zu\u00b7k\u00fcnf\u00b7ti\u00b7ges", "ord\u00b7ne\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+---+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wohl ihnen, wohl!", "tokens": ["Wohl", "ih\u00b7nen", ",", "wohl", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPER", "$,", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und wohl auch uns!", "tokens": ["Und", "wohl", "auch", "uns", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "\u00bbdein Wille gescheh;", "tokens": ["\u00bb", "dein", "Wil\u00b7le", "ge\u00b7scheh", ";"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Wie im Himmel, also auch auf Erden.\u00ab", "tokens": ["Wie", "im", "Him\u00b7mel", ",", "al\u00b7so", "auch", "auf", "Er\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "APPRART", "NN", "$,", "ADV", "ADV", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Er hebt mit dem Halme die \u00c4hr' empor;", "tokens": ["Er", "hebt", "mit", "dem", "Hal\u00b7me", "die", "\u00c4hr'", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Reifet den goldnen Apfel, die Purpurtraube;", "tokens": ["Rei\u00b7fet", "den", "gold\u00b7nen", "Ap\u00b7fel", ",", "die", "Pur\u00b7pur\u00b7trau\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Weidet am H\u00fcgel das Lamm, das Reh im Walde:", "tokens": ["Wei\u00b7det", "am", "H\u00fc\u00b7gel", "das", "Lamm", ",", "das", "Reh", "im", "Wal\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Aber sein Donner rollet auch her,", "tokens": ["A\u00b7ber", "sein", "Don\u00b7ner", "rol\u00b7let", "auch", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Und die Schlosse zerschmettert es", "tokens": ["Und", "die", "Schlos\u00b7se", "zer\u00b7schmet\u00b7tert", "es"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Am Halme, am Zweig', an dem H\u00fcgel, und im Walde!", "tokens": ["Am", "Hal\u00b7me", ",", "am", "Zweig'", ",", "an", "dem", "H\u00fc\u00b7gel", ",", "und", "im", "Wal\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,", "APPR", "ART", "NN", "$,", "KON", "APPRART", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "\u00bbunser t\u00e4gliches Brodt gieb uns heute.\u00ab", "tokens": ["\u00bb", "un\u00b7ser", "t\u00e4g\u00b7li\u00b7ches", "Brodt", "gieb", "uns", "heu\u00b7te", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "VVIMP", "PPER", "ADV", "$.", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Ob wohl hoch \u00fcber des Donners Bahn", "tokens": ["Ob", "wohl", "hoch", "\u00fc\u00b7ber", "des", "Don\u00b7ners", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "S\u00fcnder auch, und Sterbliche sind?", "tokens": ["S\u00fcn\u00b7der", "auch", ",", "und", "Sterb\u00b7li\u00b7che", "sind", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KON", "NN", "VAFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Dort auch der Freund zum Feinde wird?", "tokens": ["Dort", "auch", "der", "Freund", "zum", "Fein\u00b7de", "wird", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Freund im Tode sich trennen muss?", "tokens": ["Der", "Freund", "im", "To\u00b7de", "sich", "tren\u00b7nen", "muss", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "\u00bbvergieb uns unsere Schuld,", "tokens": ["\u00bb", "ver\u00b7gieb", "uns", "un\u00b7se\u00b7re", "Schuld", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Wie wir vergeben unseren Schuldigern.\u00ab", "tokens": ["Wie", "wir", "ver\u00b7ge\u00b7ben", "un\u00b7se\u00b7ren", "Schul\u00b7di\u00b7gern", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}}, "stanza.7": {"line.1": {"text": "Gesonderte Pfade gehen zum hohen Ziel,", "tokens": ["Ge\u00b7son\u00b7der\u00b7te", "Pfa\u00b7de", "ge\u00b7hen", "zum", "ho\u00b7hen", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zu der Gl\u00fcckseligkeit!", "tokens": ["Zu", "der", "Gl\u00fcck\u00b7se\u00b7lig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-++-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Einige kr\u00fcmmen sich durch Ein\u00f6den,", "tokens": ["Ei\u00b7ni\u00b7ge", "kr\u00fcm\u00b7men", "sich", "durch", "Ein\u00b7\u00f6\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Doch selbst an diesen sprosst es von Freuden auf,", "tokens": ["Doch", "selbst", "an", "die\u00b7sen", "sprosst", "es", "von", "Freu\u00b7den", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PDAT", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und labet den Durstenden.", "tokens": ["Und", "la\u00b7bet", "den", "Durs\u00b7ten\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "\u00bbf\u00fchr' uns nicht in Versuchung,", "tokens": ["\u00bb", "f\u00fchr'", "uns", "nicht", "in", "Ver\u00b7su\u00b7chung", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.7": {"text": "Sondern erl\u00f6s' uns vom \u00dcbel.\u00ab", "tokens": ["Son\u00b7dern", "er\u00b7l\u00f6s'", "uns", "vom", "\u00dc\u00b7bel", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$.", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.8": {"line.1": {"text": "Anbetung dir, der die grosse Sonne", "tokens": ["An\u00b7be\u00b7tung", "dir", ",", "der", "die", "gros\u00b7se", "Son\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Mit Sonnen, und Erden, und Monden umgab;", "tokens": ["Mit", "Son\u00b7nen", ",", "und", "Er\u00b7den", ",", "und", "Mon\u00b7den", "um\u00b7gab", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "NN", "$,", "KON", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Der Geister erschuf;", "tokens": ["Der", "Geis\u00b7ter", "er\u00b7schuf", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ihre Seligkeit ordnete;", "tokens": ["Ih\u00b7re", "Se\u00b7lig\u00b7keit", "ord\u00b7ne\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Die \u00c4hre hebt;", "tokens": ["Die", "\u00c4h\u00b7re", "hebt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Der dem Tode ruft;", "tokens": ["Der", "dem", "To\u00b7de", "ruft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Zum Ziele durch Ein\u00f6den fuhrt, und den Wanderer labt,", "tokens": ["Zum", "Zie\u00b7le", "durch", "Ein\u00b7\u00f6\u00b7den", "fuhrt", ",", "und", "den", "Wan\u00b7de\u00b7rer", "labt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "VVFIN", "$,", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Anbetung dir!", "tokens": ["An\u00b7be\u00b7tung", "dir", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "\u00bbdenn dein ist das Reich, und die Macht,", "tokens": ["\u00bb", "denn", "dein", "ist", "das", "Reich", ",", "und", "die", "Macht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPOSAT", "VAFIN", "ART", "NN", "$,", "KON", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Und die Herlichkeit. Amen.\u00ab", "tokens": ["Und", "die", "Her\u00b7lich\u00b7keit", ".", "A\u00b7men", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "$.", "NN", "$.", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}}}}}