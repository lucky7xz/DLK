{"textgrid.poem.39694": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "Der Nil", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aber ich st\u00fcrze von Bergen hernieder,", "tokens": ["A\u00b7ber", "ich", "st\u00fcr\u00b7ze", "von", "Ber\u00b7gen", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Wo mich der Regen des Himmels gek\u00fchlt,", "tokens": ["Wo", "mich", "der", "Re\u00b7gen", "des", "Him\u00b7mels", "ge\u00b7k\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Tr\u00e4nke erbarmend die lechzenden Br\u00fcder", "tokens": ["Tr\u00e4n\u00b7ke", "er\u00b7bar\u00b7mend", "die", "lech\u00b7zen\u00b7den", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "ART", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Da\u00df sich ihr brennendes Bette erf\u00fcllt.", "tokens": ["Da\u00df", "sich", "ihr", "bren\u00b7nen\u00b7des", "Bet\u00b7te", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Jauchzend begr\u00fc\u00dfen mich alle die Quellen;", "tokens": ["Jauch\u00b7zend", "be\u00b7gr\u00fc\u00b7\u00dfen", "mich", "al\u00b7le", "die", "Quel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PIS", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "K\u00fchlend umfange ich, Erde, auch dich;", "tokens": ["K\u00fch\u00b7lend", "um\u00b7fan\u00b7ge", "ich", ",", "Er\u00b7de", ",", "auch", "dich", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "NN", "$,", "ADV", "PPER", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Leben erschwellt mir die Tropfen, die Wellen,", "tokens": ["Le\u00b7ben", "er\u00b7schwellt", "mir", "die", "Trop\u00b7fen", ",", "die", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Leben dir spendend umarme ich dich.", "tokens": ["Le\u00b7ben", "dir", "spen\u00b7dend", "um\u00b7ar\u00b7me", "ich", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "VVFIN", "PPER", "PRF", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Theueres Land du! Geb\u00e4hrerin Erde!", "tokens": ["Theu\u00b7e\u00b7res", "Land", "du", "!", "Ge\u00b7b\u00e4h\u00b7re\u00b7rin", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "$.", "NN", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Nimm nun den Sohn auch den liebenden auf,", "tokens": ["Nimm", "nun", "den", "Sohn", "auch", "den", "lie\u00b7ben\u00b7den", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "ADV", "ART", "ADJA", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Du, die in Kl\u00fcften gebahr mich und n\u00e4hrte,", "tokens": ["Du", ",", "die", "in", "Kl\u00fcf\u00b7ten", "ge\u00b7bahr", "mich", "und", "n\u00e4hr\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "NN", "VVFIN", "PPER", "KON", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nimm jetzt, o Mutter! den Sehnenden auf.", "tokens": ["Nimm", "jetzt", ",", "o", "Mut\u00b7ter", "!", "den", "Seh\u00b7nen\u00b7den", "auf", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "FM", "NN", "$.", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}