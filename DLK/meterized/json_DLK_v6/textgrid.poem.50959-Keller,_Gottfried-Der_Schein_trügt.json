{"textgrid.poem.50959": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der Schein tr\u00fcgt", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df ein Haus, das ragt mit stolzen Zinnen,", "tokens": ["Ich", "wei\u00df", "ein", "Haus", ",", "das", "ragt", "mit", "stol\u00b7zen", "Zin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PDS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Frei spielt das Licht in allen seinen S\u00e4len,", "tokens": ["Frei", "spielt", "das", "Licht", "in", "al\u00b7len", "sei\u00b7nen", "S\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sein Giebel schimmert frei von allen Fehlen,", "tokens": ["Sein", "Gie\u00b7bel", "schim\u00b7mert", "frei", "von", "al\u00b7len", "Feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kein Neider schilt's, nicht au\u00dfen und nicht innen.", "tokens": ["Kein", "Nei\u00b7der", "schilt's", ",", "nicht", "au\u00b7\u00dfen", "und", "nicht", "in\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "PTKNEG", "ADV", "KON", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nur wer es wei\u00df mit Klugheit zu beginnen,", "tokens": ["Nur", "wer", "es", "wei\u00df", "mit", "Klug\u00b7heit", "zu", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPER", "VVFIN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In seinen tiefsten Keller sich zu stehlen,", "tokens": ["In", "sei\u00b7nen", "tiefs\u00b7ten", "Kel\u00b7ler", "sich", "zu", "steh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sieht \u00fcppig feuchtes Unkraut dort verhehlen", "tokens": ["Sieht", "\u00fcp\u00b7pig", "feuch\u00b7tes", "Un\u00b7kraut", "dort", "ver\u00b7heh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ADJA", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von dicken Schlangen wahre K\u00f6niginnen.", "tokens": ["Von", "di\u00b7cken", "Schlan\u00b7gen", "wah\u00b7re", "K\u00f6\u00b7ni\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch w\u00fcrde der sich arg betrogen haben,", "tokens": ["Doch", "w\u00fcr\u00b7de", "der", "sich", "arg", "be\u00b7tro\u00b7gen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "PRF", "ADJD", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der rasch empor die Treppen wollte steigen,", "tokens": ["Der", "rasch", "em\u00b7por", "die", "Trep\u00b7pen", "woll\u00b7te", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PTKVZ", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Neider mit der Kunde zu erlaben:", "tokens": ["Die", "Nei\u00b7der", "mit", "der", "Kun\u00b7de", "zu", "er\u00b7la\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Denn tiefer noch, im allertiefsten Schweigen,", "tokens": ["Denn", "tie\u00b7fer", "noch", ",", "im", "al\u00b7ler\u00b7tiefs\u00b7ten", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da liegt ein ungeme\u00dfner Schatz begraben,", "tokens": ["Da", "liegt", "ein", "un\u00b7ge\u00b7me\u00df\u00b7ner", "Schatz", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der niemals wird dem Lichte wohl sich zeigen!", "tokens": ["Der", "nie\u00b7mals", "wird", "dem", "Lich\u00b7te", "wohl", "sich", "zei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "ART", "NN", "ADV", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}