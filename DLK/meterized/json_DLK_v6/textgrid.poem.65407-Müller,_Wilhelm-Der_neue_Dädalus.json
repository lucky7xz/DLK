{"textgrid.poem.65407": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der neue D\u00e4dalus", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dem goldnen Labyrinthe deiner Locken eingefangen,", "tokens": ["In", "dem", "gold\u00b7nen", "La\u00b7by\u00b7rin\u00b7the", "dei\u00b7ner", "Lo\u00b7cken", "ein\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Hab' ich meine m\u00fcde Freiheit in den Schlingen aufgehangen.", "tokens": ["Hab'", "ich", "mei\u00b7ne", "m\u00fc\u00b7de", "Frei\u00b7heit", "in", "den", "Schlin\u00b7gen", "auf\u00b7ge\u00b7han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Denn wie sollt' ich es versuchen, aus den holden Irrgewinden,", "tokens": ["Denn", "wie", "sollt'", "ich", "es", "ver\u00b7su\u00b7chen", ",", "aus", "den", "hol\u00b7den", "Irr\u00b7ge\u00b7win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VMFIN", "PPER", "PPER", "VVINF", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Die sich um mein Herz geringelt, wieder mich heraus zu finden?", "tokens": ["Die", "sich", "um", "mein", "Herz", "ge\u00b7rin\u00b7gelt", ",", "wie\u00b7der", "mich", "he\u00b7raus", "zu", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$,", "ADV", "PRF", "APZR", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "K\u00f6nnt' ich auch aus Wachs mir Fl\u00fcgel, wie ein D\u00e4dalus, bereiten,", "tokens": ["K\u00f6nnt'", "ich", "auch", "aus", "Wachs", "mir", "Fl\u00fc\u00b7gel", ",", "wie", "ein", "D\u00e4\u00b7da\u00b7lus", ",", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "NN", "PPER", "NN", "$,", "PWAV", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Ach, ich w\u00fcrde doch sie beide nur nach deiner Sonne breiten,", "tokens": ["Ach", ",", "ich", "w\u00fcr\u00b7de", "doch", "sie", "bei\u00b7de", "nur", "nach", "dei\u00b7ner", "Son\u00b7ne", "brei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADV", "PPER", "PIS", "ADV", "APPR", "PPOSAT", "NN", "ADJA", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Bis die Federn mir zerschm\u00f6lzen an der Gluth der nahen Strahlen", "tokens": ["Bis", "die", "Fe\u00b7dern", "mir", "zer\u00b7schm\u00f6l\u00b7zen", "an", "der", "Gluth", "der", "na\u00b7hen", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Und ich s\u00e4nk' aus meinem Himmel in das schwarze Meer der Qualen.", "tokens": ["Und", "ich", "s\u00e4nk'", "aus", "mei\u00b7nem", "Him\u00b7mel", "in", "das", "schwar\u00b7ze", "Meer", "der", "Qua\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}