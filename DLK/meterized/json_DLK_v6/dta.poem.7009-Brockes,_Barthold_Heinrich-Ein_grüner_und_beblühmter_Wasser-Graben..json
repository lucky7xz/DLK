{"dta.poem.7009": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein gr\u00fcner und bebl\u00fchmter  \n Wasser-Graben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Gras, durch den so nahen Safft,", "tokens": ["Das", "Gras", ",", "durch", "den", "so", "na\u00b7hen", "Safft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Wassers, so sie tr\u00e4nckt, hat desto gr\u00f6ssre Krafft,", "tokens": ["Des", "Was\u00b7sers", ",", "so", "sie", "tr\u00e4nckt", ",", "hat", "des\u00b7to", "gr\u00f6ss\u00b7re", "Krafft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Steht folglich in gedoppelt sch\u00f6ner Zier,", "tokens": ["Steht", "folg\u00b7lich", "in", "ge\u00b7dop\u00b7pelt", "sch\u00f6\u00b7ner", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ist l\u00e4nger, gr\u00fcner, dicht- und sch\u00f6ner hier,", "tokens": ["Ist", "l\u00e4n\u00b7ger", ",", "gr\u00fc\u00b7ner", ",", "dicht", "und", "sch\u00f6\u00b7ner", "hier", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "ADJA", "$,", "TRUNC", "KON", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Als anderwerts das Gras. Man sieht durch helle H\u00f6h\u2019n,", "tokens": ["Als", "an\u00b7der\u00b7werts", "das", "Gras", ".", "Man", "sieht", "durch", "hel\u00b7le", "H\u00f6h'n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$.", "PIS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und holden dunckel-gr\u00fcnen Tieffen,", "tokens": ["Und", "hol\u00b7den", "dun\u00b7ckel\u00b7gr\u00fc\u00b7nen", "Tief\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Viel nettes Kr\u00e4uter-Werck, voll bunter Bl\u00e4tter, stehen:", "tokens": ["Viel", "net\u00b7tes", "Kr\u00e4u\u00b7ter\u00b7\u00b7Werck", ",", "voll", "bun\u00b7ter", "Bl\u00e4t\u00b7ter", ",", "ste\u00b7hen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "ADJD", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die bald beschattet, bald im Licht,", "tokens": ["Die", "bald", "be\u00b7schat\u00b7tet", ",", "bald", "im", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Nachdem der Sonnen Glantz durch lange Bl\u00fchte bricht,", "tokens": ["Nach\u00b7dem", "der", "Son\u00b7nen", "Glantz", "durch", "lan\u00b7ge", "Bl\u00fch\u00b7te", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da denn das hohe Gras, auf dessen glatte Spitzen", "tokens": ["Da", "denn", "das", "ho\u00b7he", "Gras", ",", "auf", "des\u00b7sen", "glat\u00b7te", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,", "APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Viel kleine Sonnen-Bilder blitzen,", "tokens": ["Viel", "klei\u00b7ne", "Son\u00b7nen\u00b7Bil\u00b7der", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Mit denen es sich lieblich reget,", "tokens": ["Mit", "de\u00b7nen", "es", "sich", "lieb\u00b7lich", "re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Den Schatten auch zugleich beweget,", "tokens": ["Den", "Schat\u00b7ten", "auch", "zu\u00b7gleich", "be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und ein gef\u00e4llig Spiel von D\u00e4mmrung, Licht und Nacht,", "tokens": ["Und", "ein", "ge\u00b7f\u00e4l\u00b7lig", "Spiel", "von", "D\u00e4mm\u00b7rung", ",", "Licht", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die alle lieblich, gr\u00fcn, zur Lust der Augen macht.", "tokens": ["Die", "al\u00b7le", "lieb\u00b7lich", ",", "gr\u00fcn", ",", "zur", "Lust", "der", "Au\u00b7gen", "macht", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "$,", "ADJD", "$,", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Mein GOTT, mit welcher Lust wird Aug\u2019 und Hertz ge-", "tokens": ["Mein", "GoTT", ",", "mit", "wel\u00b7cher", "Lust", "wird", "Aug'", "und", "Hertz", "ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PWAT", "NN", "VAFIN", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "tr\u00e4nckt,", "tokens": ["tr\u00e4nckt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.18": {"text": "Wenn sich der Blick bald auf die Spitzen lenckt;", "tokens": ["Wenn", "sich", "der", "Blick", "bald", "auf", "die", "Spit\u00b7zen", "lenckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Die angenehm, verwirret und verschrenckt,", "tokens": ["Die", "an\u00b7ge\u00b7nehm", ",", "ver\u00b7wir\u00b7ret", "und", "ver\u00b7schrenckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Bald in die dunckle Tieffen senckt,", "tokens": ["Bald", "in", "die", "dunck\u00b7le", "Tief\u00b7fen", "senckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "In welchen tausend Seltenheiten", "tokens": ["In", "wel\u00b7chen", "tau\u00b7send", "Sel\u00b7ten\u00b7hei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "In gr\u00fcner D\u00e4mmerung zu sehen seyn.", "tokens": ["In", "gr\u00fc\u00b7ner", "D\u00e4m\u00b7me\u00b7rung", "zu", "se\u00b7hen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Es mischt sich wunder-sch\u00f6n ein mannigfalt\u2019ger Schein,", "tokens": ["Es", "mischt", "sich", "wun\u00b7der\u00b7sch\u00f6n", "ein", "man\u00b7nig\u00b7falt'\u00b7ger", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Von Farben, Schatten, Licht und Glantz.", "tokens": ["Von", "Far\u00b7ben", ",", "Schat\u00b7ten", ",", "Licht", "und", "Glantz", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Kein mit der gr\u00f6sten Kunst geflochtner Crantz", "tokens": ["Kein", "mit", "der", "gr\u00f6s\u00b7ten", "Kunst", "ge\u00b7flocht\u00b7ner", "Crantz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "APPR", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Sieht lieblicher, sieht bunter aus,", "tokens": ["Sieht", "lieb\u00b7li\u00b7cher", ",", "sieht", "bun\u00b7ter", "aus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Als dieses Grabens Rand. Es schien", "tokens": ["Als", "die\u00b7ses", "Gra\u00b7bens", "Rand", ".", "Es", "schien"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "An manchem Ort das holde Gr\u00fcn", "tokens": ["An", "man\u00b7chem", "Ort", "das", "hol\u00b7de", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "So angenehm, durchflochten und gemischt,", "tokens": ["So", "an\u00b7ge\u00b7nehm", ",", "durch\u00b7floch\u00b7ten", "und", "ge\u00b7mischt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Es schien ein buntes Heer, von Bluhmen eingebunden,", "tokens": ["Es", "schien", "ein", "bun\u00b7tes", "Heer", ",", "von", "Bluh\u00b7men", "ein\u00b7ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Dort, wo bey Tausenden gef\u00e4rbte Bluhmen stunden,", "tokens": ["Dort", ",", "wo", "bey", "Tau\u00b7sen\u00b7den", "ge\u00b7f\u00e4rb\u00b7te", "Bluh\u00b7men", "stun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "APPR", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Schien etwas Kraut mit Flei\u00df mit eingewunden:", "tokens": ["Schien", "et\u00b7was", "Kraut", "mit", "Flei\u00df", "mit", "ein\u00b7ge\u00b7wun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "APPR", "NN", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Ein brennend Gelb, ein Silber-weisser Schein,", "tokens": ["Ein", "bren\u00b7nend", "Gelb", ",", "ein", "Sil\u00b7ber\u00b7weis\u00b7ser", "Schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Ein gl\u00fchend Roth, so hier das Gr\u00fcn bedecket,", "tokens": ["Ein", "gl\u00fc\u00b7hend", "Roth", ",", "so", "hier", "das", "Gr\u00fcn", "be\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Ein lebendiges Gr\u00fcn, das dort", "tokens": ["Ein", "le\u00b7ben\u00b7di\u00b7ges", "Gr\u00fcn", ",", "das", "dort"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV"], "meter": "-----+-+", "measure": "unknown.measure.di"}, "line.36": {"text": "An diesem bald, und bald an jenem Ort,", "tokens": ["An", "die\u00b7sem", "bald", ",", "und", "bald", "an", "je\u00b7nem", "Ort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADV", "$,", "KON", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Der bunten Bluhmen Schmeltz verstecket,", "tokens": ["Der", "bun\u00b7ten", "Bluh\u00b7men", "Sch\u00b7meltz", "ver\u00b7ste\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.38": {"text": "Die theils der Sonnen Glantz, indem er durch sie strahlte,", "tokens": ["Die", "theils", "der", "Son\u00b7nen", "Glantz", ",", "in\u00b7dem", "er", "durch", "sie", "strahl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "NN", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Mit einem Glantz, der g\u00fclden schien, bemahlte,", "tokens": ["Mit", "ei\u00b7nem", "Glantz", ",", "der", "g\u00fcl\u00b7den", "schien", ",", "be\u00b7mahl\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Wann einige der schwanck- und glatten Spitzen", "tokens": ["Wann", "ei\u00b7ni\u00b7ge", "der", "schwan\u00b7ck", "und", "glat\u00b7ten", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ART", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.41": {"text": "In Silber-weissen Schimmer blitzen:", "tokens": ["In", "Sil\u00b7ber\u00b7weis\u00b7sen", "Schim\u00b7mer", "blit\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "War angenehm, ja mehr als wundersch\u00f6n,", "tokens": ["War", "an\u00b7ge\u00b7nehm", ",", "ja", "mehr", "als", "wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "ADV", "PIAT", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Zumahl denjenigen, die solche Lieblichkeit,", "tokens": ["Zu\u00b7mahl", "den\u00b7je\u00b7ni\u00b7gen", ",", "die", "sol\u00b7che", "Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "$,", "PRELS", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Als ein von der Natur ihr selbst-gewircktes Kleid,", "tokens": ["Als", "ein", "von", "der", "Na\u00b7tur", "ihr", "selbst\u00b7ge\u00b7wirck\u00b7tes", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Mit Lust und Ehr-Furcht sehn.", "tokens": ["Mit", "Lust", "und", "Ehr\u00b7Furcht", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.46": {"text": "Seh' ich in stiller Lust die Lieblichkeit und Pracht,", "tokens": ["Seh'", "ich", "in", "stil\u00b7ler", "Lust", "die", "Lieb\u00b7lich\u00b7keit", "und", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Und in derselbigen die Weisheit, Lieb und Macht,", "tokens": ["Und", "in", "der\u00b7sel\u00b7bi\u00b7gen", "die", "Weis\u00b7heit", ",", "Lieb", "und", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDS", "ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Desjenigen, aus welchem alles quillet,", "tokens": ["Des\u00b7je\u00b7ni\u00b7gen", ",", "aus", "wel\u00b7chem", "al\u00b7les", "quil\u00b7let", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "APPR", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.49": {"text": "Der Erde, Lufft und Meer,", "tokens": ["Der", "Er\u00b7de", ",", "Lufft", "und", "Meer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.50": {"text": "Ja aller Himmel Himmel Heer,", "tokens": ["Ja", "al\u00b7ler", "Him\u00b7mel", "Him\u00b7mel", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PIAT", "NN", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Nur durch ein Wort mit Licht und Sch\u00f6nheit f\u00fcllet;", "tokens": ["Nur", "durch", "ein", "Wort", "mit", "Licht", "und", "Sch\u00f6n\u00b7heit", "f\u00fcl\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.52": {"text": "So w\u00fcnsch ich aus dem innersten der Seelen,", "tokens": ["So", "w\u00fcnsch", "ich", "aus", "dem", "in\u00b7ners\u00b7ten", "der", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.53": {"text": "Da\u00df Jhm, so lang ich hier auf Erden walle,", "tokens": ["Da\u00df", "Jhm", ",", "so", "lang", "ich", "hier", "auf", "Er\u00b7den", "wal\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "ADJD", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.54": {"text": "Mein Leben, meine Freud', an seinem Werck gefalle,", "tokens": ["Mein", "Le\u00b7ben", ",", "mei\u00b7ne", "Freud'", ",", "an", "sei\u00b7nem", "Werck", "ge\u00b7fal\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Und ich, in frohe Lust gesetzt zu Seiner Ehr,", "tokens": ["Und", "ich", ",", "in", "fro\u00b7he", "Lust", "ge\u00b7setzt", "zu", "Sei\u00b7ner", "Ehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPR", "ADJA", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Indem ich Jhn in Seinem Werck erkenne;", "tokens": ["In\u00b7dem", "ich", "Jhn", "in", "Sei\u00b7nem", "Werck", "er\u00b7ken\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.57": {"text": "Auch gegen Jhn je mehr und mehr,", "tokens": ["Auch", "ge\u00b7gen", "Jhn", "je", "mehr", "und", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADV", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.58": {"text": "In s\u00fcsser Gegen-Lieb' und Danck, entbrenne!", "tokens"