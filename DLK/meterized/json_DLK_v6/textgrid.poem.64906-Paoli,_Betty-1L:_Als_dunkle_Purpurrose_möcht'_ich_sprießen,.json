{"textgrid.poem.64906": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als dunkle Purpurrose m\u00f6cht' ich sprie\u00dfen,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als dunkle Purpurrose m\u00f6cht' ich sprie\u00dfen,", "tokens": ["Als", "dunk\u00b7le", "Pur\u00b7pur\u00b7ro\u00b7se", "m\u00f6cht'", "ich", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Damascus Kind, an farb'gen Blumenw\u00e4nden;", "tokens": ["Da\u00b7mas\u00b7cus", "Kind", ",", "an", "fa\u00b7rb'\u00b7gen", "Blu\u00b7men\u00b7w\u00e4n\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und meiner D\u00fcfte seelenvolles Gr\u00fc\u00dfen", "tokens": ["Und", "mei\u00b7ner", "D\u00fcf\u00b7te", "see\u00b7len\u00b7vol\u00b7les", "Gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als Opferhauch zu dir, mein Dichter! senden.", "tokens": ["Als", "Op\u00b7fer\u00b7hauch", "zu", "dir", ",", "mein", "Dich\u00b7ter", "!", "sen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPER", "$,", "PPOSAT", "NN", "$.", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Balsamgew\u00fcrzt vom Schlaf auf Lotospf\u00fchlen", "tokens": ["Bal\u00b7sam\u00b7ge\u00b7w\u00fcrzt", "vom", "Schlaf", "auf", "Lo\u00b7tosp\u00b7f\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "APPR", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "M\u00f6cht' ich als Lufthauch z\u00e4rtlich dich umwallen,", "tokens": ["M\u00f6cht'", "ich", "als", "Luft\u00b7hauch", "z\u00e4rt\u00b7lich", "dich", "um\u00b7wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "KOUS", "NN", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Fiebergluten deiner Stirne k\u00fchlen", "tokens": ["Die", "Fie\u00b7berg\u00b7lu\u00b7ten", "dei\u00b7ner", "Stir\u00b7ne", "k\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und deine ew'gen Lieder wiederhallen.", "tokens": ["Und", "dei\u00b7ne", "ew'\u00b7gen", "Lie\u00b7der", "wie\u00b7der\u00b7hal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Als Nachtigall m\u00f6cht' ich vom Baume klagen,", "tokens": ["Als", "Nach\u00b7ti\u00b7gall", "m\u00f6cht'", "ich", "vom", "Bau\u00b7me", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der dich mit seinem Schattenzelt erquicket,", "tokens": ["Der", "dich", "mit", "sei\u00b7nem", "Schat\u00b7ten\u00b7zelt", "er\u00b7quic\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als Welle m\u00f6cht' ich an das Ufer schlagen,", "tokens": ["Als", "Wel\u00b7le", "m\u00f6cht'", "ich", "an", "das", "U\u00b7fer", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem deines Wandelns Spuren eingedr\u00fccket.", "tokens": ["Dem", "dei\u00b7nes", "Wan\u00b7delns", "Spu\u00b7ren", "ein\u00b7ge\u00b7dr\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Als s\u00fc\u00dfes Traumbild m\u00f6cht' ich dich beschleichen,", "tokens": ["Als", "s\u00fc\u00b7\u00dfes", "Traum\u00b7bild", "m\u00f6cht'", "ich", "dich", "be\u00b7schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als Trostoase deinem Blick begegnen,", "tokens": ["Als", "Tros\u00b7toa\u00b7se", "dei\u00b7nem", "Blick", "be\u00b7geg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Als Muse m\u00f6cht' ich dir den Lorber reichen", "tokens": ["Als", "Mu\u00b7se", "m\u00f6cht'", "ich", "dir", "den", "Lor\u00b7ber", "rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VMFIN", "PPER", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dich mit einem Weihekusse segnen! \u2013", "tokens": ["Und", "dich", "mit", "ei\u00b7nem", "Wei\u00b7he\u00b7kus\u00b7se", "seg\u00b7nen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Allein als Tochter dieser dunkeln Erde,", "tokens": ["Al\u00b7lein", "als", "Toch\u00b7ter", "die\u00b7ser", "dun\u00b7keln", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der h\u00f6h're Loose streng versagt geblieben,", "tokens": ["Der", "h\u00f6h'\u00b7re", "Loo\u00b7se", "streng", "ver\u00b7sagt", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kann nichts ich bringen zu dem Opferherde,", "tokens": ["Kann", "nichts", "ich", "brin\u00b7gen", "zu", "dem", "Op\u00b7fer\u00b7her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPER", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als nur mein Herz mit seinem tiefen Lieben.", "tokens": ["Als", "nur", "mein", "Herz", "mit", "sei\u00b7nem", "tie\u00b7fen", "Lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Du aber wirst in dieser reinen Gabe", "tokens": ["Du", "a\u00b7ber", "wirst", "in", "die\u00b7ser", "rei\u00b7nen", "Ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den ernsten Gru\u00df der Musa nicht vermissen;", "tokens": ["Den", "erns\u00b7ten", "Gru\u00df", "der", "Mu\u00b7sa", "nicht", "ver\u00b7mis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du wirst darin der Rosen w\u00fcrz'ge Labe,", "tokens": ["Du", "wirst", "da\u00b7rin", "der", "Ro\u00b7sen", "w\u00fcrz'\u00b7ge", "La\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Traumes milden Trost zu finden wissen.", "tokens": ["Des", "Trau\u00b7mes", "mil\u00b7den", "Trost", "zu", "fin\u00b7den", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und f\u00fchlen wirst du, da\u00df die Freuden alle,", "tokens": ["Und", "f\u00fch\u00b7len", "wirst", "du", ",", "da\u00df", "die", "Freu\u00b7den", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VAFIN", "PPER", "$,", "KOUS", "ART", "NN", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die einzeln unser fl\u00fccht'ges Sein versch\u00f6nen,", "tokens": ["Die", "ein\u00b7zeln", "un\u00b7ser", "fl\u00fccht'\u00b7ges", "Sein", "ver\u00b7sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit gr\u00e4bersprengendem, gewalt'gem Schalle", "tokens": ["Mit", "gr\u00e4\u00b7ber\u00b7spren\u00b7gen\u00b7dem", ",", "ge\u00b7walt'\u00b7gem", "Schal\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In einem Liebeswort zusamment\u00f6nen.", "tokens": ["In", "ei\u00b7nem", "Lie\u00b7bes\u00b7wort", "zu\u00b7sam\u00b7men\u00b7t\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}