{"textgrid.poem.26087": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Die Farben, die der Grauwinter verga\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Farben, die der Grauwinter verga\u00df,", "tokens": ["Die", "Far\u00b7ben", ",", "die", "der", "Grau\u00b7win\u00b7ter", "ver\u00b7ga\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Kommen vom Berg her\u00fcber \u00fcber die Stra\u00df':", "tokens": ["Kom\u00b7men", "vom", "Berg", "her\u00b7\u00fc\u00b7ber", "\u00fc\u00b7ber", "die", "Stra\u00df'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Das Grasgr\u00fcn und das Rot von Ziegeln sommerhei\u00df,", "tokens": ["Das", "Gras\u00b7gr\u00fcn", "und", "das", "Rot", "von", "Zie\u00b7geln", "som\u00b7mer\u00b7hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Himmelblau und gezupfter Wolken Daunenwei\u00df.", "tokens": ["Das", "Him\u00b7mel\u00b7blau", "und", "ge\u00b7zupf\u00b7ter", "Wol\u00b7ken", "Dau\u00b7nen\u00b7wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "L\u00e4ndlich gekleidet, wie aus Bauernschr\u00e4nken und Truhen,", "tokens": ["L\u00e4nd\u00b7lich", "ge\u00b7klei\u00b7det", ",", "wie", "aus", "Bau\u00b7ern\u00b7schr\u00e4n\u00b7ken", "und", "Tru\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "PWAV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Geht der Fr\u00fchlingstag auf frischen staublosen Schuhen,", "tokens": ["Geht", "der", "Fr\u00fch\u00b7lings\u00b7tag", "auf", "fri\u00b7schen", "stau\u00b7blo\u00b7sen", "Schu\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Geht gedankenlos alter Sitte und alten Wegen nach;", "tokens": ["Geht", "ge\u00b7dan\u00b7ken\u00b7los", "al\u00b7ter", "Sit\u00b7te", "und", "al\u00b7ten", "We\u00b7gen", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJA", "NN", "KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+----+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Schnellt die flugfrohen Schwalben wieder \u00fcber das Dach,", "tokens": ["Schnellt", "die", "flug\u00b7fro\u00b7hen", "Schwal\u00b7ben", "wie\u00b7der", "\u00fc\u00b7ber", "das", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "L\u00e4\u00dft kleine fiebernde Lerchen singen und ruft Herzfarben wach.", "tokens": ["L\u00e4\u00dft", "klei\u00b7ne", "fie\u00b7bern\u00b7de", "Ler\u00b7chen", "sin\u00b7gen", "und", "ruft", "Herz\u00b7far\u00b7ben", "wach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "ADJA", "NN", "VVINF", "KON", "VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+--+-+-+", "measure": "iambic.septa.relaxed"}}}}}