{"textgrid.poem.36562": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Lied nach der Schlacht bei Collin", "genre": "verse", "period": "N.A.", "pub_year": 1757, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zur\u00fcck, rief Vater ", "tokens": ["Zu\u00b7r\u00fcck", ",", "rief", "Va\u00b7ter"], "token_info": ["word", "punct", "word", "word"], "pos": ["PTKVZ", "$,", "VVFIN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Zur\u00fcck, rief er, zur\u00fcck!", "tokens": ["Zu\u00b7r\u00fcck", ",", "rief", "er", ",", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PTKVZ", "$,", "VVFIN", "PPER", "$,", "PTKVZ", "$."], "meter": "-++--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nachdenkend dacht' er schon bei sich:", "tokens": ["Nach\u00b7den\u00b7kend", "dacht'", "er", "schon", "bei", "sich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "ADV", "APPR", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gott giebt dem Feinde Gl\u00fcck.", "tokens": ["Gott", "giebt", "dem", "Fein\u00b7de", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir aber st\u00fcrmten noch das Nest,", "tokens": ["Wir", "a\u00b7ber", "st\u00fcrm\u00b7ten", "noch", "das", "Nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir wollten noch hinan!", "tokens": ["Wir", "woll\u00b7ten", "noch", "hi\u00b7nan", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir kletterten, wir hielten fest", "tokens": ["Wir", "klet\u00b7ter\u00b7ten", ",", "wir", "hiel\u00b7ten", "fest"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns an einander an.", "tokens": ["Uns", "an", "ein\u00b7an\u00b7der", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und sagten dem, der oben stand:", "tokens": ["Und", "sag\u00b7ten", "dem", ",", "der", "o\u00b7ben", "stand", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie kommen wir herauf?", "tokens": ["Wie", "kom\u00b7men", "wir", "her\u00b7auf", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schlugen tapfer Hand in Hand,", "tokens": ["Und", "schlu\u00b7gen", "tap\u00b7fer", "Hand", "in", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und halfen uns hinauf.", "tokens": ["Und", "hal\u00b7fen", "uns", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da st\u00fcrzte von Kart\u00e4tschensaat", "tokens": ["Da", "st\u00fcrz\u00b7te", "von", "Kar\u00b7t\u00e4t\u00b7schen\u00b7saat"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Getroffen, eine Schar", "tokens": ["Ge\u00b7trof\u00b7fen", ",", "ei\u00b7ne", "Schar"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVPP", "$,", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Helden, ohne Heldenthat,", "tokens": ["Von", "Hel\u00b7den", ",", "oh\u00b7ne", "Hel\u00b7den\u00b7that", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die halb schon oben war!", "tokens": ["Die", "halb", "schon", "o\u00b7ben", "war", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das sahe ", "tokens": ["Das", "sa\u00b7he"], "token_info": ["word", "word"], "pos": ["PDS", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Wie blutete sein Herz!", "tokens": ["Wie", "blu\u00b7te\u00b7te", "sein", "Herz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie stand, bei mitleidsvollem Ach,", "tokens": ["Wie", "stand", ",", "bei", "mit\u00b7le\u00b7ids\u00b7vol\u00b7lem", "Ach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "APPR", "PIS", "ITJ", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Sein Auge Himmelw\u00e4rts!", "tokens": ["Sein", "Au\u00b7ge", "Him\u00b7mel\u00b7w\u00e4rts", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}}, "stanza.6": {"line.1": {"text": "Was f\u00fcr fanftm\u00fct'ge Blicke gab", "tokens": ["Was", "f\u00fcr", "fanft\u00b7m\u00fct'\u00b7ge", "Bli\u00b7cke", "gab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Heldenangesicht!", "tokens": ["Sein", "Hel\u00b7den\u00b7an\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00dft, rief er, Kinder, la\u00dft doch ab!", "tokens": ["La\u00dft", ",", "rief", "er", ",", "Kin\u00b7der", ",", "la\u00dft", "doch", "ab", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VVFIN", "PPER", "$,", "NN", "$,", "VVIMP", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit uns ist Gott heut' nicht.", "tokens": ["Mit", "uns", "ist", "Gott", "heut'", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da lie\u00dfen wir den bl\u00f6den Feind", "tokens": ["Da", "lie\u00b7\u00dfen", "wir", "den", "bl\u00f6\u00b7den", "Feind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In seinem Felsennest.", "tokens": ["In", "sei\u00b7nem", "Fel\u00b7sen\u00b7nest", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun jubelt er! o Menschenfreund!", "tokens": ["Nun", "ju\u00b7belt", "er", "!", "o", "Men\u00b7schen\u00b7freund", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun hat er Siegesfest.", "tokens": ["Nun", "hat", "er", "Sie\u00b7ges\u00b7fest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wie kann er aber? Br\u00fcder, sagt!", "tokens": ["Wie", "kann", "er", "a\u00b7ber", "?", "Br\u00fc\u00b7der", ",", "sagt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "$.", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er kann ja nicht, f\u00fcrwahr!", "tokens": ["Er", "kann", "ja", "nicht", ",", "f\u00fcr\u00b7wahr", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "$,", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn haben wir ihn nicht gejagt,", "tokens": ["Denn", "ha\u00b7ben", "wir", "ihn", "nicht", "ge\u00b7jagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So weit zu jagen war?", "tokens": ["So", "weit", "zu", "ja\u00b7gen", "war", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wir stritten, nicht mit Ro\u00df und Mann,", "tokens": ["Wir", "strit\u00b7ten", ",", "nicht", "mit", "Ro\u00df", "und", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PTKNEG", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Felsen stritten wir.", "tokens": ["Mit", "Fel\u00b7sen", "strit\u00b7ten", "wir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hier, Heldenbr\u00fcder, bind' er an.", "tokens": ["Hier", ",", "Hel\u00b7den\u00b7br\u00fc\u00b7der", ",", "bind'", "er", "an", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hier, Br\u00fcder, sieg' er! hier!", "tokens": ["Hier", ",", "Br\u00fc\u00b7der", ",", "sieg'", "er", "!", "hier", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVIMP", "PPER", "$.", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Du Feind! herab in gr\u00fcnes Feld,", "tokens": ["Du", "Feind", "!", "her\u00b7ab", "in", "gr\u00fc\u00b7nes", "Feld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und weise freie Brust,", "tokens": ["Und", "wei\u00b7se", "frei\u00b7e", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und streit und sieg und stirb ein Held!", "tokens": ["Und", "streit", "und", "sieg", "und", "stirb", "ein", "Held", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hier ist zu sterben Lust!", "tokens": ["Hier", "ist", "zu", "ster\u00b7ben", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Allein der Bl\u00f6de wagt sich nicht,", "tokens": ["Al\u00b7lein", "der", "Bl\u00f6\u00b7de", "wagt", "sich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PRF", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir m\u00f6gen lange stehn", "tokens": ["Wir", "m\u00f6\u00b7gen", "lan\u00b7ge", "stehn"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und auf ihn warten. ", "tokens": ["Und", "auf", "ihn", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}