{"textgrid.poem.46688": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ich sprach, wann Kind um Kind gelegt]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sprach, wann Kind um Kind gelegt", "tokens": ["Ich", "sprach", ",", "wann", "Kind", "um", "Kind", "ge\u00b7legt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ins Bett war, um zu schlafen:", "tokens": ["Ins", "Bett", "war", ",", "um", "zu", "schla\u00b7fen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "$,", "KOUI", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun hat sich Sturm und Wind gelegt,", "tokens": ["Nun", "hat", "sich", "Sturm", "und", "Wind", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wir sind wie im Hafen.", "tokens": ["Und", "wir", "sind", "wie", "im", "Ha\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "KOKOM", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie \u00fcbel hast du's ausgelegt,", "tokens": ["Wie", "\u00fc\u00b7bel", "hast", "du's", "aus\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Tod, um uns zu strafen,", "tokens": ["O", "Tod", ",", "um", "uns", "zu", "stra\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und hast sie uns hinaus gelegt,", "tokens": ["Und", "hast", "sie", "uns", "hin\u00b7aus", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PRF", "APZR", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo sie im Sturme schlafen.", "tokens": ["Wo", "sie", "im", "Stur\u00b7me", "schla\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Schlaflosigkeit ist mir gelegt", "tokens": ["Schlaf\u00b7lo\u00b7sig\u00b7keit", "ist", "mir", "ge\u00b7legt"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "VVPP"], "meter": "++--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Aufs Auge, seit sie schlafen;", "tokens": ["Aufs", "Au\u00b7ge", ",", "seit", "sie", "schla\u00b7fen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nicht hat der Sturm sich hier gelegt,", "tokens": ["Nicht", "hat", "der", "Sturm", "sich", "hier", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "ART", "NN", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sie ruhn dort im Hafen.", "tokens": ["Und", "sie", "ruhn", "dort", "im", "Ha\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}}}}