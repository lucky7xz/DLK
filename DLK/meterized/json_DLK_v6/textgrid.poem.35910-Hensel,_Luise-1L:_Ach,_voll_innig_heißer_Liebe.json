{"textgrid.poem.35910": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach, voll innig hei\u00dfer Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, voll innig hei\u00dfer Liebe", "tokens": ["Ach", ",", "voll", "in\u00b7nig", "hei\u00b7\u00dfer", "Lie\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADJD", "ADJD", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schl\u00e4gt dies Herz so froh und tr\u00fcbe,", "tokens": ["Schl\u00e4gt", "dies", "Herz", "so", "froh", "und", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "NN", "ADV", "ADJD", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00fc\u00dfes Kind, Dir heute zu.", "tokens": ["S\u00fc\u00b7\u00dfes", "Kind", ",", "Dir", "heu\u00b7te", "zu", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Freude, da\u00df ich Dich geboren,", "tokens": ["Freu\u00b7de", ",", "da\u00df", "ich", "Dich", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kummer, da\u00df ich Dich verloren,", "tokens": ["Kum\u00b7mer", ",", "da\u00df", "ich", "Dich", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "L\u00e4\u00dft mir heute nirgend Ruh.", "tokens": ["L\u00e4\u00dft", "mir", "heu\u00b7te", "nir\u00b7gend", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O, wie war ich doch so selig,", "tokens": ["O", ",", "wie", "war", "ich", "doch", "so", "se\u00b7lig", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als Du noch so frisch und fr\u00f6hlich", "tokens": ["Als", "Du", "noch", "so", "frisch", "und", "fr\u00f6h\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mich an's treue Herz gedr\u00fcckt.", "tokens": ["Mich", "an's", "treu\u00b7e", "Herz", "ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jetzt, ach! flie\u00dfen meine Thr\u00e4nen,", "tokens": ["Jetzt", ",", "ach", "!", "flie\u00b7\u00dfen", "mei\u00b7ne", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ITJ", "$.", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die Brust hebt banges Sehnen,", "tokens": ["Und", "die", "Brust", "hebt", "ban\u00b7ges", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn mein Herz ist mir entr\u00fcckt.", "tokens": ["Denn", "mein", "Herz", "ist", "mir", "ent\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Soll ich nie Dich mehr erblicken?", "tokens": ["Soll", "ich", "nie", "Dich", "mehr", "er\u00b7bli\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach! Dich an die Brust zu dr\u00fccken", "tokens": ["Ach", "!", "Dich", "an", "die", "Brust", "zu", "dr\u00fc\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einmal noch auf dieser Welt,", "tokens": ["Ein\u00b7mal", "noch", "auf", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist mein Wunsch, mein stetes Flehen \u2013", "tokens": ["Ist", "mein", "Wunsch", ",", "mein", "ste\u00b7tes", "Fle\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja, ich mu\u00df Dich wiedersehen,", "tokens": ["Ja", ",", "ich", "mu\u00df", "Dich", "wie\u00b7der\u00b7se\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eh' dies Herz in Staub zerf\u00e4llt.", "tokens": ["Eh'", "dies", "Herz", "in", "Staub", "zer\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch ich wollte ja nicht klagen;", "tokens": ["Doch", "ich", "woll\u00b7te", "ja", "nicht", "kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir der Mutter W\u00fcnsche sagen", "tokens": ["Dir", "der", "Mut\u00b7ter", "W\u00fcn\u00b7sche", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sollte nur allein dies Blatt.", "tokens": ["Soll\u00b7te", "nur", "al\u00b7lein", "dies", "Blatt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "PDS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimm es, Kind, aus meinen H\u00e4nden;", "tokens": ["Nimm", "es", ",", "Kind", ",", "aus", "mei\u00b7nen", "H\u00e4n\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00f6ge Gott Dir Alles spenden,", "tokens": ["M\u00f6\u00b7ge", "Gott", "Dir", "Al\u00b7les", "spen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PPER", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was mein Herz f\u00fcr Dich erbat.", "tokens": ["Was", "mein", "Herz", "f\u00fcr", "Dich", "er\u00b7bat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}