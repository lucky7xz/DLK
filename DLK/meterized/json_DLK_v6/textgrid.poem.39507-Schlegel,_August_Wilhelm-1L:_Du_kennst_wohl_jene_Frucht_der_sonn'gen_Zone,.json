{"textgrid.poem.39507": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du kennst wohl jene Frucht der sonn'gen Zone,", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du kennst wohl jene Frucht der sonn'gen Zone,", "tokens": ["Du", "kennst", "wohl", "je\u00b7ne", "Frucht", "der", "sonn'\u00b7gen", "Zo\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PDAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die aus dem goldnen Schoo\u00dfe gr\u00fcne Spro\u00dfen", "tokens": ["Die", "aus", "dem", "gold\u00b7nen", "Schoo\u00b7\u00dfe", "gr\u00fc\u00b7ne", "Spro\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Empor l\u00e4\u00dft, wie zum Palmenwipfel, scho\u00dfen,", "tokens": ["Em\u00b7por", "l\u00e4\u00dft", ",", "wie", "zum", "Pal\u00b7men\u00b7wip\u00b7fel", ",", "scho\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PWAV", "APPRART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df unter schatt'gem Baldachin sie throne.", "tokens": ["Da\u00df", "un\u00b7ter", "schatt'\u00b7gem", "Bal\u00b7da\u00b7ch\u00b7in", "sie", "thro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch schafft, getrennt von ihrer Frucht, die Krone,", "tokens": ["Doch", "schafft", ",", "ge\u00b7trennt", "von", "ih\u00b7rer", "Frucht", ",", "die", "Kro\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich, wurzelnd, neu den w\u00fcrzigen Geno\u00dfen,", "tokens": ["Sich", ",", "wur\u00b7zelnd", ",", "neu", "den", "w\u00fcr\u00b7zi\u00b7gen", "Ge\u00b7no\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "$,", "VVPP", "$,", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bew\u00e4hrend, da\u00df, gleich durch sie hin ergo\u00dfen,", "tokens": ["Be\u00b7w\u00e4h\u00b7rend", ",", "da\u00df", ",", "gleich", "durch", "sie", "hin", "er\u00b7go\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "$,", "ADV", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Die s\u00fc\u00dfe Kraft im Kern, im Schmucke wohne.", "tokens": ["Die", "s\u00fc\u00b7\u00dfe", "Kraft", "im", "Kern", ",", "im", "Schmu\u00b7cke", "woh\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So, Freund, will deine Dichtung mir gemuten:", "tokens": ["So", ",", "Freund", ",", "will", "dei\u00b7ne", "Dich\u00b7tung", "mir", "ge\u00b7mu\u00b7ten", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VMFIN", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In jugendlicher Fr\u00fchlingspracht verborgen", "tokens": ["In", "ju\u00b7gend\u00b7li\u00b7cher", "Fr\u00fch\u00b7lings\u00b7pracht", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hegt sie des fernen Himmelstrichs Arome.", "tokens": ["Hegt", "sie", "des", "fer\u00b7nen", "Him\u00b7mel\u00b7strichs", "A\u00b7ro\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Hier duft'ges Abendland, dort gl\u00fchnder Morgen;", "tokens": ["Hier", "duft'\u00b7ges", "A\u00b7bend\u00b7land", ",", "dort", "gl\u00fchn\u00b7der", "Mor\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dazwischen hauchen L\u00fcft' und Meere fluten", "tokens": ["Da\u00b7zwi\u00b7schen", "hau\u00b7chen", "L\u00fcft'", "und", "Mee\u00b7re", "flu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADJA", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hin und zur\u00fcck mit linder Sehnsucht Strome.", "tokens": ["Hin", "und", "zu\u00b7r\u00fcck", "mit", "lin\u00b7der", "Sehn\u00b7sucht", "Stro\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PTKVZ", "APPR", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}