{"dta.poem.10945": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Demuth .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Gott! von dem wir, was du nicht, nicht aber was du\nbist, verstehen,", "tokens": ["O", "Gott", "!", "von", "dem", "wir", ",", "was", "du", "nicht", ",", "nicht", "a\u00b7ber", "was", "du", "bist", ",", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$.", "APPR", "PRELS", "PPER", "$,", "PWS", "PPER", "PTKNEG", "$,", "PTKNEG", "ADV", "PWS", "PPER", "VAFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Ach la\u00df uns dich auf eine Weise, die dir gef\u00e4llig ist, er-", "tokens": ["Ach", "la\u00df", "uns", "dich", "auf", "ei\u00b7ne", "Wei\u00b7se", ",", "die", "dir", "ge\u00b7f\u00e4l\u00b7lig", "ist", ",", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ITJ", "VVIMP", "PPER", "PRF", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$,", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Was dir an uns gefallen kann, wird, allem Ansehn nach,", "tokens": ["Was", "dir", "an", "uns", "ge\u00b7fal\u00b7len", "kann", ",", "wird", ",", "al\u00b7lem", "An\u00b7sehn", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "VVINF", "VMFIN", "$,", "VAFIN", "$,", "PIS", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Die, blo\u00df durch deiner Gr\u00f6\u00df Erk\u00e4nntni\u00df, in uns gewirkte", "tokens": ["Die", ",", "blo\u00df", "durch", "dei\u00b7ner", "Gr\u00f6\u00df", "Er\u00b7k\u00e4nnt\u00b7ni\u00df", ",", "in", "uns", "ge\u00b7wirk\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,", "APPR", "PPER", "ADJA"], "meter": "-+-+-+-+-++-+-", "measure": "unknown.measure.septa"}}}}}