{"textgrid.poem.56455": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Abend", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Versunken ist der Tag in Purpurrot,", "tokens": ["Ver\u00b7sun\u00b7ken", "ist", "der", "Tag", "in", "Pur\u00b7pur\u00b7rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Strom schwimmt wei\u00df in ungeheurer Gl\u00e4tte.", "tokens": ["Der", "Strom", "schwimmt", "wei\u00df", "in", "un\u00b7ge\u00b7heu\u00b7rer", "Gl\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Segel kommt. Es hebt sich aus dem Boot", "tokens": ["Ein", "Se\u00b7gel", "kommt", ".", "Es", "hebt", "sich", "aus", "dem", "Boot"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Am Steuer gro\u00df des Schiffers Silhouette.", "tokens": ["Am", "Steu\u00b7er", "gro\u00df", "des", "Schif\u00b7fers", "Sil\u00b7hou\u00b7et\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Auf allen Inseln steigt des Herbstes Wald", "tokens": ["Auf", "al\u00b7len", "In\u00b7seln", "steigt", "des", "Herbs\u00b7tes", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit roten H\u00e4uptern in den Raum, den klaren.", "tokens": ["Mit", "ro\u00b7ten", "H\u00e4up\u00b7tern", "in", "den", "Raum", ",", "den", "kla\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und aus der Schluchten dunkler Tiefe hallt", "tokens": ["Und", "aus", "der", "Schluch\u00b7ten", "dunk\u00b7ler", "Tie\u00b7fe", "hallt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Waldung Ton, wie Rauschen der Kitharen.", "tokens": ["Der", "Wal\u00b7dung", "Ton", ",", "wie", "Rau\u00b7schen", "der", "Ki\u00b7tha\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PWAV", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das Dunkel ist im Osten ausgegossen,", "tokens": ["Das", "Dun\u00b7kel", "ist", "im", "Os\u00b7ten", "aus\u00b7ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie blauer Wein kommt aus gest\u00fcrzter Urne.", "tokens": ["Wie", "blau\u00b7er", "Wein", "kommt", "aus", "ge\u00b7st\u00fcrz\u00b7ter", "Ur\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ferne steht, vom Mantel schwarz umflossen,", "tokens": ["Und", "fer\u00b7ne", "steht", ",", "vom", "Man\u00b7tel", "schwarz", "um\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "APPRART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die hohe Nacht auf schattigem Kothurne.", "tokens": ["Die", "ho\u00b7he", "Nacht", "auf", "schat\u00b7ti\u00b7gem", "Ko\u00b7thur\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}