{"textgrid.poem.56379": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Faust", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sind's keine Tr\u00e4ume, die du hingezeichnet,", "tokens": ["Sin\u00b7d's", "kei\u00b7ne", "Tr\u00e4u\u00b7me", ",", "die", "du", "hin\u00b7ge\u00b7zeich\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "So folg ich, Seher, deiner Riesenspur,", "tokens": ["So", "folg", "ich", ",", "Se\u00b7her", ",", "dei\u00b7ner", "Rie\u00b7sen\u00b7spur", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich schreite deine Bahn und zage nicht.", "tokens": ["Ich", "schrei\u00b7te", "dei\u00b7ne", "Bahn", "und", "za\u00b7ge", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn horchend deinem m\u00e4cht'gen Rufe, Geister,", "tokens": ["Wenn", "hor\u00b7chend", "dei\u00b7nem", "m\u00e4cht'\u00b7gen", "Ru\u00b7fe", ",", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADJD", "PPOSAT", "ADJA", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dir dienend, ihres Reiches Nacht entstiegen;", "tokens": ["Dir", "die\u00b7nend", ",", "ih\u00b7res", "Rei\u00b7ches", "Nacht", "ent\u00b7stie\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wird mir die Geisterwelt sich auch er\u00f6ffnen.", "tokens": ["Wird", "mir", "die", "Geis\u00b7ter\u00b7welt", "sich", "auch", "er\u00b7\u00f6ff\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Belehrung zollen mir die finstern M\u00e4chte.", "tokens": ["Be\u00b7leh\u00b7rung", "zol\u00b7len", "mir", "die", "fins\u00b7tern", "M\u00e4ch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nein! niederknieen nicht vor dir, Verk\u00fcnder", "tokens": ["Nein", "!", "nie\u00b7der\u00b7kni\u00b7e\u00b7en", "nicht", "vor", "dir", ",", "Ver\u00b7k\u00fcn\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PTKANT", "$.", "VVFIN", "PTKNEG", "APPR", "PPER", "$,", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des siebenmal erf\u00fcllten schweren Fluches,", "tokens": ["Des", "sie\u00b7ben\u00b7mal", "er\u00b7f\u00fcll\u00b7ten", "schwe\u00b7ren", "Flu\u00b7ches", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der mir das Haupt umflammt, und nicht vor ihm.", "tokens": ["Der", "mir", "das", "Haupt", "um\u00b7flammt", ",", "und", "nicht", "vor", "ihm", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,", "KON", "PTKNEG", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vernichtung hei\u00dft der Gott, den ich anrufe.", "tokens": ["Ver\u00b7nich\u00b7tung", "hei\u00dft", "der", "Gott", ",", "den", "ich", "an\u00b7ru\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ihr seid unm\u00e4chtig, der Vergangenheit", "tokens": ["Ihr", "seid", "un\u00b7m\u00e4ch\u00b7tig", ",", "der", "Ver\u00b7gan\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ihr leicht erworbnes Eigentum zu rauben.", "tokens": ["Ihr", "leicht", "er\u00b7worb\u00b7nes", "Ei\u00b7gen\u00b7tum", "zu", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O k\u00f6nnt ich wieder fluchen euch! o k\u00f6nnt ich", "tokens": ["O", "k\u00f6nnt", "ich", "wie\u00b7der", "flu\u00b7chen", "euch", "!", "o", "k\u00f6nnt", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "VVFIN", "PPER", "$.", "FM", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Menschenqualen euch verzagen sehn,", "tokens": ["In", "Men\u00b7schen\u00b7qua\u00b7len", "euch", "ver\u00b7za\u00b7gen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In ew'gen Menschenqualen euch verzweifeln,", "tokens": ["In", "ew'\u00b7gen", "Men\u00b7schen\u00b7qua\u00b7len", "euch", "ver\u00b7zwei\u00b7feln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und laut auflachend gr\u00e4\u00dflich euch verh\u00f6hnen!", "tokens": ["Und", "laut", "auf\u00b7la\u00b7chend", "gr\u00e4\u00df\u00b7lich", "euch", "ver\u00b7h\u00f6h\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Fluch selber mir, da\u00df ich ohnm\u00e4chtig bin,", "tokens": ["Fluch", "sel\u00b7ber", "mir", ",", "da\u00df", "ich", "ohn\u00b7m\u00e4ch\u00b7tig", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df nur ein leiser, eitler Laut der Lippe", "tokens": ["Da\u00df", "nur", "ein", "lei\u00b7ser", ",", "eit\u00b7ler", "Laut", "der", "Lip\u00b7pe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "$,", "ADJA", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Entbebet, in dem Winde zu verhallen!", "tokens": ["Ent\u00b7be\u00b7bet", ",", "in", "dem", "Win\u00b7de", "zu", "ver\u00b7hal\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Ersehnte Spornerin der eitlen W\u00fcnsche,", "tokens": ["Er\u00b7sehn\u00b7te", "Spor\u00b7ne\u00b7rin", "der", "eit\u00b7len", "W\u00fcn\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich habe, Wahrheit, deine Dunstgestalt", "tokens": ["Ich", "ha\u00b7be", ",", "Wahr\u00b7heit", ",", "dei\u00b7ne", "Dunst\u00b7ge\u00b7stalt"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verfolgt, und unerme\u00dflich weit verfolgt,", "tokens": ["Ver\u00b7folgt", ",", "und", "un\u00b7er\u00b7me\u00df\u00b7lich", "weit", "ver\u00b7folgt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "ADJD", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ihr geopfert jeden Hoffnungsschimmer;", "tokens": ["Und", "ihr", "ge\u00b7o\u00b7pfert", "je\u00b7den", "Hoff\u00b7nungs\u00b7schim\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gestrandet steh ich nun auf schroffer Klippe,", "tokens": ["Ge\u00b7stran\u00b7det", "steh", "ich", "nun", "auf", "schrof\u00b7fer", "Klip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Rings um mich her die dunkle tiefe Flut,", "tokens": ["Rings", "um", "mich", "her", "die", "dunk\u00b7le", "tie\u00b7fe", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "APZR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und um das Haupt mir donnerschwangre Wolken.", "tokens": ["Und", "um", "das", "Haupt", "mir", "don\u00b7ner\u00b7schwang\u00b7re", "Wol\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich werde nimmer, nimmer sie umfangen,", "tokens": ["Ich", "wer\u00b7de", "nim\u00b7mer", ",", "nim\u00b7mer", "sie", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Um die ich hin den teuren Preis geworfen!", "tokens": ["Um", "die", "ich", "hin", "den", "teu\u00b7ren", "Preis", "ge\u00b7wor\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Verdammnis, ewige, in deinen Scho\u00df! \u2013", "tokens": ["Ver\u00b7damm\u00b7nis", ",", "e\u00b7wi\u00b7ge", ",", "in", "dei\u00b7nen", "Scho\u00df", "!", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "ADJA", "$,", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vielleicht Vernichtung nur, vielleicht Erkenntnis,", "tokens": ["Viel\u00b7leicht", "Ver\u00b7nich\u00b7tung", "nur", ",", "viel\u00b7leicht", "Er\u00b7kennt\u00b7nis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gewi\u00dfheit doch.", "tokens": ["Ge\u00b7wi\u00df\u00b7heit", "doch", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}