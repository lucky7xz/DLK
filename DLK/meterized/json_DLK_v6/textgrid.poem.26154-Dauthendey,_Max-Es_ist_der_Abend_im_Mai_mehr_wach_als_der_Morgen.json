{"textgrid.poem.26154": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Es ist der Abend im Mai mehr wach als der Morgen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ging in der Nacht unter bl\u00fchenden Lauben,", "tokens": ["Ich", "ging", "in", "der", "Nacht", "un\u00b7ter", "bl\u00fc\u00b7hen\u00b7den", "Lau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Deren Ger\u00fcche wie \u00c4ther\u00f6le verstauben.", "tokens": ["De\u00b7ren", "Ge\u00b7r\u00fc\u00b7che", "wie", "\u00c4\u00b7the\u00b7r\u00f6\u00b7le", "ver\u00b7stau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "KOKOM", "NN", "VVINF", "$."], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Auch die Ohren mu\u00dften dem Tauben klingen,", "tokens": ["Auch", "die", "Oh\u00b7ren", "mu\u00df\u00b7ten", "dem", "Tau\u00b7ben", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So \u00fcbten Nachtigallen der Leidenschaft Singen,", "tokens": ["So", "\u00fcb\u00b7ten", "Nach\u00b7ti\u00b7gal\u00b7len", "der", "Lei\u00b7den\u00b7schaft", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Sie l\u00f6sten sich ab in den B\u00e4umen verborgen.", "tokens": ["Sie", "l\u00f6s\u00b7ten", "sich", "ab", "in", "den", "B\u00e4u\u00b7men", "ver\u00b7bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Es ist der Abend im Mai mehr wach als der Morgen.", "tokens": ["Es", "ist", "der", "A\u00b7bend", "im", "Mai", "mehr", "wach", "als", "der", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Sie sangen von ihrem Begehr ohne Zaudern,", "tokens": ["Sie", "san\u00b7gen", "von", "ih\u00b7rem", "Be\u00b7gehr", "oh\u00b7ne", "Zau\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Von Verz\u00fcckung und s\u00fc\u00dfen Schaudern,", "tokens": ["Von", "Ver\u00b7z\u00fc\u00b7ckung", "und", "s\u00fc\u00b7\u00dfen", "Schau\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Und alle, die sie h\u00f6rten, mu\u00dften Feuer fangen.", "tokens": ["Und", "al\u00b7le", ",", "die", "sie", "h\u00f6r\u00b7ten", ",", "mu\u00df\u00b7ten", "Feu\u00b7er", "fan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,", "VMFIN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}