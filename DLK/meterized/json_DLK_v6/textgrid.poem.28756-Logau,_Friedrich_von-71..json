{"textgrid.poem.28756": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "71.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gro\u00dfmut gilt und Hochmut nicht.", "tokens": ["Gro\u00df\u00b7mut", "gilt", "und", "Hoch\u00b7mut", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Jener steht, und dieser bricht;", "tokens": ["Je\u00b7ner", "steht", ",", "und", "die\u00b7ser", "bricht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "VVFIN", "$,", "KON", "PDS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieser pflegt sich selbst zu f\u00e4llen;", "tokens": ["Die\u00b7ser", "pflegt", "sich", "selbst", "zu", "f\u00e4l\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jener pflegt sich hoch zu stellen;", "tokens": ["Je\u00b7ner", "pflegt", "sich", "hoch", "zu", "stel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "VVFIN", "PRF", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jener schaffet, was er wil,", "tokens": ["Je\u00b7ner", "schaf\u00b7fet", ",", "was", "er", "wil", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieser schaffet selten viel.", "tokens": ["Die\u00b7ser", "schaf\u00b7fet", "sel\u00b7ten", "viel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}