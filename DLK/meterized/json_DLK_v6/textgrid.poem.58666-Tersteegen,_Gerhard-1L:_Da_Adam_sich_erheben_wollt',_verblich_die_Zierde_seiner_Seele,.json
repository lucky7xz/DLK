{"textgrid.poem.58666": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da Adam sich erheben wollt', verblich die Zierde seiner Seele,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da Adam sich erheben wollt', verblich die Zierde seiner Seele,", "tokens": ["Da", "A\u00b7dam", "sich", "er\u00b7he\u00b7ben", "wollt'", ",", "ver\u00b7blich", "die", "Zier\u00b7de", "sei\u00b7ner", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "VVINF", "VMFIN", "$,", "ADJD", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Da Christus sich erniedrigte zum Kreuz, zum Grab und zu der H\u00f6lle,", "tokens": ["Da", "Chris\u00b7tus", "sich", "er\u00b7nied\u00b7rig\u00b7te", "zum", "Kreuz", ",", "zum", "Grab", "und", "zu", "der", "H\u00f6l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PRF", "VVFIN", "APPRART", "NN", "$,", "APPRART", "NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.3": {"text": "Da ward die Zierde wiederbracht,", "tokens": ["Da", "ward", "die", "Zier\u00b7de", "wie\u00b7der\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Land des Lebens aufgemacht;", "tokens": ["Das", "Land", "des", "Le\u00b7bens", "auf\u00b7ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer nun die Zierde da will sehn,", "tokens": ["Wer", "nun", "die", "Zier\u00b7de", "da", "will", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mu\u00df auch mit ihm heruntergehn.", "tokens": ["Mu\u00df", "auch", "mit", "ihm", "her\u00b7un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wer etwas Licht und Gnade kriegt, will unvermerkt wie Adam steigen;", "tokens": ["Wer", "et\u00b7was", "Licht", "und", "Gna\u00b7de", "kriegt", ",", "will", "un\u00b7ver\u00b7merkt", "wie", "A\u00b7dam", "stei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "KON", "NN", "VVFIN", "$,", "VMFIN", "ADJD", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.8": {"text": "Mein Gott, wie tief mu\u00df man sich drauf zum Grab in jener W\u00fcste beugen,", "tokens": ["Mein", "Gott", ",", "wie", "tief", "mu\u00df", "man", "sich", "drauf", "zum", "Grab", "in", "je\u00b7ner", "W\u00fcs\u00b7te", "beu\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "ADJD", "VMFIN", "PIS", "PRF", "PAV", "APPRART", "NN", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.9": {"text": "Beraubt von Leben, Trost und Licht,", "tokens": ["Be\u00b7raubt", "von", "Le\u00b7ben", ",", "Trost", "und", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Bis man ist worden gar zunicht'!", "tokens": ["Bis", "man", "ist", "wor\u00b7den", "gar", "zu\u00b7nicht'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "VAPP", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Da wird erst der Lebend'gen Land", "tokens": ["Da", "wird", "erst", "der", "Le\u00b7ben\u00b7d'\u00b7gen", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Und seine Zierd' im Grund bekannt.", "tokens": ["Und", "sei\u00b7ne", "Zierd'", "im", "Grund", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}