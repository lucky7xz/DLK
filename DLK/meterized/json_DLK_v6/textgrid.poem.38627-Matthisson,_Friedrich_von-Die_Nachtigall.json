{"textgrid.poem.38627": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Nachtigall", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unter dem Ahorn, an der Felsenquelle,", "tokens": ["Un\u00b7ter", "dem", "A\u00b7horn", ",", "an", "der", "Fel\u00b7sen\u00b7quel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Horcht' ich sinnend der Nachtigall; elysisch", "tokens": ["Horcht'", "ich", "sin\u00b7nend", "der", "Nach\u00b7ti\u00b7gall", ";", "e\u00b7ly\u00b7sisch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "$.", "ADJD"], "meter": "+-+--+-+---", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Hallten, gleich Harmonikat\u00f6nen, ihre", "tokens": ["Hall\u00b7ten", ",", "gleich", "Har\u00b7mo\u00b7ni\u00b7ka\u00b7t\u00f6\u00b7nen", ",", "ih\u00b7re"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$,", "ADV", "NN", "$,", "PPOSAT"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Silberakkorde.", "tokens": ["Sil\u00b7be\u00b7rak\u00b7kor\u00b7de", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Feyerlich schwiegen die entz\u00fcckten Wipfel;", "tokens": ["Fey\u00b7er\u00b7lich", "schwie\u00b7gen", "die", "ent\u00b7z\u00fcck\u00b7ten", "Wip\u00b7fel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Leiser str\u00f6mte der Born; in Lieb' und Wohlklang", "tokens": ["Lei\u00b7ser", "str\u00f6m\u00b7te", "der", "Born", ";", "in", "Lieb'", "und", "Wohl\u00b7klang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NE", "$.", "APPR", "NN", "KON", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Hinzuschmelzen schien die Natur; Diana", "tokens": ["Hin\u00b7zu\u00b7schmel\u00b7zen", "schien", "die", "Na\u00b7tur", ";", "Di\u00b7a\u00b7na"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "NE"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Senkte den Wagen.", "tokens": ["Senk\u00b7te", "den", "Wa\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "S\u00e4ngerin, fragt' ich, hat der Sohn Cytherens,", "tokens": ["S\u00e4n\u00b7ge\u00b7rin", ",", "fragt'", "ich", ",", "hat", "der", "Sohn", "Cy\u00b7the\u00b7rens", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "VAFIN", "ART", "NN", "NE", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Mit dem Pfeile dir G\u00f6tterspeise reichend,", "tokens": ["Mit", "dem", "Pfei\u00b7le", "dir", "G\u00f6t\u00b7ter\u00b7spei\u00b7se", "rei\u00b7chend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "NN", "VVPP", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "In die s\u00fcsse Kehle dir seines Nektars", "tokens": ["In", "die", "s\u00fcs\u00b7se", "Keh\u00b7le", "dir", "sei\u00b7nes", "Nek\u00b7tars"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Zauber getr\u00e4ufelt?", "tokens": ["Zau\u00b7ber", "ge\u00b7tr\u00e4u\u00b7felt", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Amor erzog mich nicht! im Alpenthale,", "tokens": ["A\u00b7mor", "er\u00b7zog", "mich", "nicht", "!", "im", "Al\u00b7pen\u00b7tha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "$.", "APPRART", "NN", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Nah' dem Baume der meine Wieg' umbl\u00fchte,", "tokens": ["Nah'", "dem", "Bau\u00b7me", "der", "mei\u00b7ne", "Wieg'", "um\u00b7bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Sang ein Hirt, in orphischen T\u00f6nen, Liebe,", "tokens": ["Sang", "ein", "Hirt", ",", "in", "or\u00b7phi\u00b7schen", "T\u00f6\u00b7nen", ",", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Fr\u00fchling und Unschuld.", "tokens": ["Fr\u00fch\u00b7ling", "und", "Un\u00b7schuld", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Sch\u00fcchtern begann ich seine Himmelslaute", "tokens": ["Sch\u00fcch\u00b7tern", "be\u00b7gann", "ich", "sei\u00b7ne", "Him\u00b7mels\u00b7lau\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Nachzufl\u00f6ten; da l\u00e4chelte die Wehmuth;", "tokens": ["Nach\u00b7zu\u00b7fl\u00f6\u00b7ten", ";", "da", "l\u00e4\u00b7chel\u00b7te", "die", "Weh\u00b7muth", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Hofnung hellte rosig des d\u00fcstern Grames", "tokens": ["Hof\u00b7nung", "hell\u00b7te", "ro\u00b7sig", "des", "d\u00fcs\u00b7tern", "Gra\u00b7mes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Fliehende Nebel.", "tokens": ["Flie\u00b7hen\u00b7de", "Ne\u00b7bel", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Also die S\u00e4ngerin; mir flossen Thr\u00e4nen.", "tokens": ["Al\u00b7so", "die", "S\u00e4n\u00b7ge\u00b7rin", ";", "mir", "flos\u00b7sen", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$.", "PPER", "VVFIN", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Salis! rauschten die Wind' im Fr\u00fchlingslaube;", "tokens": ["Sa\u00b7lis", "!", "rauschten", "die", "Wind'", "im", "Fr\u00fch\u00b7lings\u00b7lau\u00b7be", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Salis! seufzte traurig der Wiesenhalme", "tokens": ["Sa\u00b7lis", "!", "seufz\u00b7te", "trau\u00b7rig", "der", "Wie\u00b7sen\u00b7hal\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "VVFIN", "ADJD", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Leises Gefl\u00fcster.", "tokens": ["Lei\u00b7ses", "Ge\u00b7fl\u00fcs\u00b7ter", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}