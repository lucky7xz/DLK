{"textgrid.poem.65757": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "31", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Herzen aller, die ich f\u00fcr begraben,", "tokens": ["Die", "Her\u00b7zen", "al\u00b7ler", ",", "die", "ich", "f\u00fcr", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "$,", "PRELS", "PPER", "APPR", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weil sie mir fehlten, hielt, bereichern deine Brust.", "tokens": ["Weil", "sie", "mir", "fehl\u00b7ten", ",", "hielt", ",", "be\u00b7rei\u00b7chern", "dei\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da wohnen Lieb' und alle Liebesgaben,", "tokens": ["Da", "woh\u00b7nen", "Lieb'", "und", "al\u00b7le", "Lie\u00b7bes\u00b7ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und jeder Freund, den ich mir tot gewu\u00dft.", "tokens": ["Und", "je\u00b7der", "Freund", ",", "den", "ich", "mir", "tot", "ge\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "PPER", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie manche heilige fromme Tr\u00e4ne lernte", "tokens": ["Wie", "man\u00b7che", "hei\u00b7li\u00b7ge", "from\u00b7me", "Tr\u00e4\u00b7ne", "lern\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "ADJA", "ADJA", "NN", "VVFIN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Mein Aug' aus treu and\u00e4cht'ger Liebe weinen", "tokens": ["Mein", "Aug'", "aus", "treu", "an\u00b7d\u00e4cht'\u00b7ger", "Lie\u00b7be", "wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ADJD", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Zum Zoll den Toten, die nun wie entfernte,", "tokens": ["Zum", "Zoll", "den", "To\u00b7ten", ",", "die", "nun", "wie", "ent\u00b7fern\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PRELS", "ADV", "KOKOM", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In dir verborgne Wesen mir erscheinen!", "tokens": ["In", "dir", "ver\u00b7borg\u00b7ne", "We\u00b7sen", "mir", "er\u00b7schei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Lebend'ger Liebe Grab bist du erbaut,", "tokens": ["Le\u00b7ben\u00b7d'\u00b7ger", "Lie\u00b7be", "Grab", "bist", "du", "er\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.10": {"text": "Prangst mit Troph\u00e4en meiner toten Lieben,", "tokens": ["Prangst", "mit", "Trop\u00b7h\u00e4\u00b7en", "mei\u00b7ner", "to\u00b7ten", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPOSAT", "ADJA", "ADJA", "$,"], "meter": "+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.11": {"text": "Die all' ihr Teil an mir dir anvertraut:", "tokens": ["Die", "all'", "ihr", "Teil", "an", "mir", "dir", "an\u00b7ver\u00b7traut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PPOSAT", "NN", "APPR", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Der vielen Gut, dir ist's allein verblieben.", "tokens": ["Der", "vie\u00b7len", "Gut", ",", "dir", "ist's", "al\u00b7lein", "ver\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "PPER", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Die einst geliebten Bilder zeigst du mir,", "tokens": ["Die", "einst", "ge\u00b7lieb\u00b7ten", "Bil\u00b7der", "zeigst", "du", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und du, ihr Inbegriff, mein alles eignet dir.", "tokens": ["Und", "du", ",", "ihr", "In\u00b7be\u00b7griff", ",", "mein", "al\u00b7les", "eig\u00b7net", "dir", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "PIS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}