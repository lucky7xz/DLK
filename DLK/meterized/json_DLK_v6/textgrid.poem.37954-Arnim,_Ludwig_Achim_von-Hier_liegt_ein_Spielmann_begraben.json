{"textgrid.poem.37954": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Hier liegt ein Spielmann begraben", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.85", "no:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbguten Morgen Spielmann,", "tokens": ["\u00bb", "gu\u00b7ten", "Mor\u00b7gen", "Spiel\u00b7mann", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wo bleibst du so lang?\u00ab", "tokens": ["Wo", "bleibst", "du", "so", "lang", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJD", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Da drunten, da droben,", "tokens": ["Da", "drun\u00b7ten", ",", "da", "dro\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "ADV", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Da tanzten die Schwaben,", "tokens": ["Da", "tanz\u00b7ten", "die", "Schwa\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Mit der kleinen Killekeia,", "tokens": ["Mit", "der", "klei\u00b7nen", "Kil\u00b7le\u00b7kei\u00b7a", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit der gro\u00dfen Kum Kum.", "tokens": ["Mit", "der", "gro\u00b7\u00dfen", "Kum", "Kum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Da kamen die Weiber", "tokens": ["Da", "ka\u00b7men", "die", "Wei\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Mit Sichel und Scheiben,", "tokens": ["Mit", "Si\u00b7chel", "und", "Schei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und wollten den Schwaben", "tokens": ["Und", "woll\u00b7ten", "den", "Schwa\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Das Tanzen vertreiben,", "tokens": ["Das", "Tan\u00b7zen", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Mit der kleinen Killekeia,", "tokens": ["Mit", "der", "klei\u00b7nen", "Kil\u00b7le\u00b7kei\u00b7a", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit der gro\u00dfen Kum Kum.", "tokens": ["Mit", "der", "gro\u00b7\u00dfen", "Kum", "Kum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Da laufen die Schwaben", "tokens": ["Da", "lau\u00b7fen", "die", "Schwa\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und fallen in Graben,", "tokens": ["Und", "fal\u00b7len", "in", "Gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Da sprechen die Schwaben:", "tokens": ["Da", "spre\u00b7chen", "die", "Schwa\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Liegt ein Spielmann begraben,", "tokens": ["Liegt", "ein", "Spiel\u00b7mann", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Mit der kleinen Killekeia,", "tokens": ["Mit", "der", "klei\u00b7nen", "Kil\u00b7le\u00b7kei\u00b7a", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit der gro\u00dfen Kum Kum.", "tokens": ["Mit", "der", "gro\u00b7\u00dfen", "Kum", "Kum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Da laufen die Schwaben,", "tokens": ["Da", "lau\u00b7fen", "die", "Schwa\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Weiber nachtraben,", "tokens": ["Die", "Wei\u00b7ber", "nach\u00b7tra\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Bis \u00fcber die Grenze,", "tokens": ["Bis", "\u00fc\u00b7ber", "die", "Gren\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mit Sichel und Sense:", "tokens": ["Mit", "Si\u00b7chel", "und", "Sen\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "\u00bbguten Morgen Spielleut,", "tokens": ["\u00bb", "gu\u00b7ten", "Mor\u00b7gen", "Spiel\u00b7leut", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "NN", "$,"], "meter": "+-+---", "measure": "unknown.measure.di"}, "line.6": {"text": "Nun schneidet das Korn.\u00ab", "tokens": ["Nun", "schnei\u00b7det", "das", "Korn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}