{"textgrid.poem.26558": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gl\u00fccklich seid ihr, ihr Reichen!", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fccklich seid ihr, ihr Reichen!", "tokens": ["Gl\u00fcck\u00b7lich", "seid", "ihr", ",", "ihr", "Rei\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Nicht da\u00df des Armen begn\u00fcgsames Herz", "tokens": ["Nicht", "da\u00df", "des", "Ar\u00b7men", "be\u00b7gn\u00fcg\u00b7sa\u00b7mes", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Nach eu'rem Golde verlangt,", "tokens": ["Nach", "eu'\u00b7rem", "Gol\u00b7de", "ver\u00b7langt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Oder da\u00df er th\u00f6richt w\u00e4hnt,", "tokens": ["O\u00b7der", "da\u00df", "er", "th\u00f6\u00b7richt", "w\u00e4hnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Erlassen sei euch des Leidens Zoll,", "tokens": ["Er\u00b7las\u00b7sen", "sei", "euch", "des", "Lei\u00b7dens", "Zoll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der auferlegt allem Athmenden.", "tokens": ["Der", "auf\u00b7er\u00b7legt", "al\u00b7lem", "Ath\u00b7men\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "NN", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Nein! Er wei\u00df vielmehr,", "tokens": ["Nein", "!", "Er", "wei\u00df", "viel\u00b7mehr", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Da\u00df der Schmerz auch in Pal\u00e4sten wohnt,", "tokens": ["Da\u00df", "der", "Schmerz", "auch", "in", "Pa\u00b7l\u00e4s\u00b7ten", "wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Verzweiflung in stolzen Karossen f\u00e4hrt,", "tokens": ["Ver\u00b7zwei\u00b7flung", "in", "stol\u00b7zen", "Ka\u00b7ros\u00b7sen", "f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Und eu'rer Frau'n Diamantenpracht", "tokens": ["Und", "eu'\u00b7rer", "Frau'n", "Di\u00b7a\u00b7man\u00b7ten\u00b7pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Meist nur an erstarrte Thr\u00e4nen mahnt,", "tokens": ["Meist", "nur", "an", "er\u00b7starr\u00b7te", "Thr\u00e4\u00b7nen", "mahnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Die im Verborg'nen sie weinen \u2013:", "tokens": ["Die", "im", "Ver\u00b7bor\u00b7g'\u00b7nen", "sie", "wei\u00b7nen", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "APPRART", "NN", "PPER", "VVINF", "$(", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Er wei\u00df es und hat f\u00fcr euch", "tokens": ["Er", "wei\u00df", "es", "und", "hat", "f\u00fcr", "euch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VAFIN", "APPR", "PPER"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Weit eher den Seufzer des Mitleids,", "tokens": ["Weit", "e\u00b7her", "den", "Seuf\u00b7zer", "des", "Mit\u00b7leids", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Als das bittere Wort des Neides.", "tokens": ["Als", "das", "bit\u00b7te\u00b7re", "Wort", "des", "Nei\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und danken sollt ihr daf\u00fcr", "tokens": ["Und", "dan\u00b7ken", "sollt", "ihr", "da\u00b7f\u00fcr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PAV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "In sch\u00f6ner Demuth den G\u00f6ttern!", "tokens": ["In", "sch\u00f6\u00b7ner", "De\u00b7muth", "den", "G\u00f6t\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Frei bewahren k\u00f6nnt ihr euch", "tokens": ["Frei", "be\u00b7wah\u00b7ren", "k\u00f6nnt", "ihr", "euch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVINF", "VMFIN", "PPER", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von Allem, was den Menschen entweiht.", "tokens": ["Von", "Al\u00b7lem", ",", "was", "den", "Men\u00b7schen", "ent\u00b7weiht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Denn niemals seid ihr hingestellt", "tokens": ["Denn", "nie\u00b7mals", "seid", "ihr", "hin\u00b7ge\u00b7stellt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf den schmalen Klippenrand der Noth,", "tokens": ["Auf", "den", "schma\u00b7len", "Klip\u00b7pen\u00b7rand", "der", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Der lauterstes Wollen", "tokens": ["Der", "lau\u00b7ters\u00b7tes", "Wol\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Von unw\u00fcrdigem Handeln trennt,", "tokens": ["Von", "un\u00b7w\u00fcr\u00b7di\u00b7gem", "Han\u00b7deln", "trennt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Und jene Sorge kennt ihr nicht,", "tokens": ["Und", "je\u00b7ne", "Sor\u00b7ge", "kennt", "ihr", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die mit heimt\u00fcckischem Rattenzahn", "tokens": ["Die", "mit", "heim\u00b7t\u00fc\u00b7cki\u00b7schem", "Rat\u00b7ten\u00b7zahn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "An der Seele fri\u00dft,", "tokens": ["An", "der", "See\u00b7le", "fri\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "Erhabenen Sinn an Gemeines kettet", "tokens": ["Er\u00b7ha\u00b7be\u00b7nen", "Sinn", "an", "Ge\u00b7mei\u00b7nes", "ket\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "NN", "VVFIN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Und ein gro\u00dfes Herz", "tokens": ["Und", "ein", "gro\u00b7\u00dfes", "Herz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.14": {"text": "Niederzwingt in den Sumpf der Duldung,", "tokens": ["Nie\u00b7der\u00b7zwingt", "in", "den", "Sumpf", "der", "Dul\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.15": {"text": "Bis es nach langem Kampf", "tokens": ["Bis", "es", "nach", "lan\u00b7gem", "Kampf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "An sich selbst verzweifelt,", "tokens": ["An", "sich", "selbst", "ver\u00b7zwei\u00b7felt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.17": {"text": "Schuldig wird \u2013 und versinkt.", "tokens": ["Schul\u00b7dig", "wird", "\u2013", "und", "ver\u00b7sinkt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$(", "KON", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Euch selber treu bleiben k\u00f6nnt ihr,", "tokens": ["Euch", "sel\u00b7ber", "treu", "blei\u00b7ben", "k\u00f6nnt", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VVINF", "VMFIN", "PPER", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Wenn ihr nur ", "tokens": ["Wenn", "ihr", "nur"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und nichts verhindert euch,", "tokens": ["Und", "nichts", "ver\u00b7hin\u00b7dert", "euch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gl\u00fccklich seid ihr, ihr Reichen!", "tokens": ["Gl\u00fcck\u00b7lich", "seid", "ihr", ",", "ihr", "Rei\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}