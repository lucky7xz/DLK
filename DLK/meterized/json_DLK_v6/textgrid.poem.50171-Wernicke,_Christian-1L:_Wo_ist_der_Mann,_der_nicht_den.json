{"textgrid.poem.50171": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo ist der Mann, der nicht den ", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo ist der Mann, der nicht den ", "tokens": ["Wo", "ist", "der", "Mann", ",", "der", "nicht", "den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "PRELS", "PTKNEG", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und der ihn kennt, mit Ruhm nicht dessen ", "tokens": ["Und", "der", "ihn", "kennt", ",", "mit", "Ruhm", "nicht", "des\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "PPER", "VVFIN", "$,", "APPR", "NN", "PTKNEG", "PDS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der allen, wenn er gleich nicht kan, ", "tokens": ["Der", "al\u00b7len", ",", "wenn", "er", "gleich", "nicht", "kan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und keinem, kan er gleich, doch ", "tokens": ["Und", "kei\u00b7nem", ",", "kan", "er", "gleich", ",", "doch"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "PIS", "$,", "VMFIN", "PPER", "ADV", "$,", "ADV"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.5": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Und dessen ", "tokens": ["Und", "des\u00b7sen"], "token_info": ["word", "word"], "pos": ["KON", "PDS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Den sich zwey K\u00f6nige bey folg' an ihre Seit'", "tokens": ["Den", "sich", "zwey", "K\u00f6\u00b7ni\u00b7ge", "bey", "fol\u00b7g'", "an", "ih\u00b7re", "Seit'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "CARD", "NN", "APPR", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+--+--+---+-+", "measure": "dactylic.tri.plus"}, "line.8": {"text": "Aus ", "tokens": ["Aus"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Der auff dem h\u00f6chsten Stuhl itzt nach dem Herrscher sitzt,", "tokens": ["Der", "auff", "dem", "h\u00f6chs\u00b7ten", "Stuhl", "itzt", "nach", "dem", "Herr\u00b7scher", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und wie er ", "tokens": ["Und", "wie", "er"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWAV", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Der, weil er was uns hier beliebt macht in sich schliesst,", "tokens": ["Der", ",", "weil", "er", "was", "uns", "hier", "be\u00b7liebt", "macht", "in", "sich", "schliesst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "PWS", "PPER", "ADV", "ADJD", "VVFIN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}}}}}