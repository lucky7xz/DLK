{"textgrid.poem.50639": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da\u00df meinen Namen du durch b\u00f6sen Leumund", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df meinen Namen du durch b\u00f6sen Leumund", "tokens": ["Da\u00df", "mei\u00b7nen", "Na\u00b7men", "du", "durch", "b\u00f6\u00b7sen", "Leu\u00b7mund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und ich mich nicht an dir auf andre Weise r\u00e4che,", "tokens": ["Und", "ich", "mich", "nicht", "an", "dir", "auf", "and\u00b7re", "Wei\u00b7se", "r\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PRF", "PTKNEG", "APPR", "PPER", "APPR", "ADJA", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als da\u00df ich allezeit von dir viel Gutes spreche,", "tokens": ["Als", "da\u00df", "ich", "al\u00b7le\u00b7zeit", "von", "dir", "viel", "Gu\u00b7tes", "spre\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "APPR", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gereichet mir zum Schimpf mehr, als du wohl gedenkst:", "tokens": ["Ge\u00b7rei\u00b7chet", "mir", "zum", "Schimpf", "mehr", ",", "als", "du", "wohl", "ge\u00b7denkst", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "$,", "KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es l\u00e4\u00dft die Welt sich nicht betr\u00fcgen", "tokens": ["Es", "l\u00e4\u00dft", "die", "Welt", "sich", "nicht", "be\u00b7tr\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}