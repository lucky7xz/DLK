{"textgrid.poem.25824": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[zersticht das Alter dein Gesicht]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zersticht das Alter dein Gesicht,", "tokens": ["Zer\u00b7sticht", "das", "Al\u00b7ter", "dein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und flicht dir Asche in dein Haar,", "tokens": ["Und", "flicht", "dir", "A\u00b7sche", "in", "dein", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dornen in deine Lippen \u2013", "tokens": ["Dor\u00b7nen", "in", "dei\u00b7ne", "Lip\u00b7pen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Jugendklar bleibt dein Auge.", "tokens": ["Ju\u00b7gend\u00b7klar", "bleibt", "dein", "Au\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "In deinen Augen springt heilig ein Quell,", "tokens": ["In", "dei\u00b7nen", "Au\u00b7gen", "springt", "hei\u00b7lig", "ein", "Quell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "An dem die dunkle Nymphe singt;", "tokens": ["An", "dem", "die", "dunk\u00b7le", "Nym\u00b7phe", "singt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Heilig ein Quell,", "tokens": ["Hei\u00b7lig", "ein", "Quell", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Drinnen M\u00e4rchenmonde hell funkeln.", "tokens": ["Drin\u00b7nen", "M\u00e4r\u00b7chen\u00b7mon\u00b7de", "hell", "fun\u00b7keln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Wer einen Blick mit dir getauscht,", "tokens": ["Wer", "ei\u00b7nen", "Blick", "mit", "dir", "ge\u00b7tauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tr\u00e4gt ihn berauscht von Aug zu Auge.", "tokens": ["Tr\u00e4gt", "ihn", "be\u00b7rauscht", "von", "Aug", "zu", "Au\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Augenlicht bricht,", "tokens": ["Dein", "Au\u00b7gen\u00b7licht", "bricht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Wenn auf Erden das letzte Auge versiecht.", "tokens": ["Wenn", "auf", "Er\u00b7den", "das", "letz\u00b7te", "Au\u00b7ge", "ver\u00b7siecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}}}}}