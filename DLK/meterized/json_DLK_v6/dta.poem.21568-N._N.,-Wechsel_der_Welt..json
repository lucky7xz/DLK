{"dta.poem.21568": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Wechsel der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Die Welt ist nimmer geblieben", "tokens": ["Die", "Welt", "ist", "nim\u00b7mer", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die herrliche Welt des Homer,", "tokens": ["Die", "herr\u00b7li\u00b7che", "Welt", "des", "Ho\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NE", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die G\u00f6tter sind l\u00e4ngst vertrieben,", "tokens": ["Die", "G\u00f6t\u00b7ter", "sind", "l\u00e4ngst", "ver\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gest\u00fcrzt sind die Tempel ins Meer.", "tokens": ["Ge\u00b7st\u00fcrzt", "sind", "die", "Tem\u00b7pel", "ins", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Verschollen die heiligen Lieder,", "tokens": ["Ver\u00b7schol\u00b7len", "die", "hei\u00b7li\u00b7gen", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Verwirbelt der Opferrauch,", "tokens": ["Ver\u00b7wir\u00b7belt", "der", "Op\u00b7fer\u00b7rauch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In Hohn und Gesp\u00f6tt darnieder", "tokens": ["In", "Hohn", "und", "Ge\u00b7sp\u00f6tt", "dar\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "PAV"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gebrochen der Priester Brauch.", "tokens": ["Ge\u00b7bro\u00b7chen", "der", "Pries\u00b7ter", "Brauch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Nicht betende V\u00f6lker mehr wallen", "tokens": ["Nicht", "be\u00b7ten\u00b7de", "V\u00f6l\u00b7ker", "mehr", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJA", "NN", "ADV", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Zu schimmernden S\u00e4ulenreihn,", "tokens": ["Zu", "schim\u00b7mern\u00b7den", "S\u00e4u\u00b7len\u00b7reihn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Den hohen Olympiern allen", "tokens": ["Den", "ho\u00b7hen", "O\u00b7lym\u00b7piern", "al\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PIAT"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Geschm\u00fcckte Geschenke zu weihn.", "tokens": ["Ge\u00b7schm\u00fcck\u00b7te", "Ge\u00b7schen\u00b7ke", "zu", "weihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Die Himmlischen mu\u00dften erliegen", "tokens": ["Die", "Himm\u00b7li\u00b7schen", "mu\u00df\u00b7ten", "er\u00b7lie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "In toller Jahrhunderte Kampf,", "tokens": ["In", "tol\u00b7ler", "Jahr\u00b7hun\u00b7der\u00b7te", "Kampf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Es schwelgt in Troph\u00e4en und Siegen,", "tokens": ["Es", "schwelgt", "in", "Trop\u00b7h\u00e4\u00b7en", "und", "Sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es herrscht: der allm\u00e4chtige Dampf.", "tokens": ["Es", "herrscht", ":", "der", "all\u00b7m\u00e4ch\u00b7ti\u00b7ge", "Dampf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Es sausen die H\u00e4mmer und dr\u00f6hnen", "tokens": ["Es", "sau\u00b7sen", "die", "H\u00e4m\u00b7mer", "und", "dr\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Auf Silber, auf Gold und auf Blei,", "tokens": ["Auf", "Sil\u00b7ber", ",", "auf", "Gold", "und", "auf", "Blei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Maschinen rasseln und st\u00f6hnen", "tokens": ["Ma\u00b7schi\u00b7nen", "ras\u00b7seln", "und", "st\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVINF", "KON", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein gellendes Einerlei.             ", "tokens": ["Ein", "gel\u00b7len\u00b7des", "Ei\u00b7ner\u00b7lei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Kaum kann der Donner dringen", "tokens": ["Kaum", "kann", "der", "Don\u00b7ner", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch all der Fabriken Gebraus,", "tokens": ["Durch", "all", "der", "Fab\u00b7ri\u00b7ken", "Ge\u00b7braus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Lieder und Glockenklingen", "tokens": ["Und", "Lie\u00b7der", "und", "Glo\u00b7cken\u00b7klin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Verschwimmen im R\u00e4dergesaus.", "tokens": ["Ver\u00b7schwim\u00b7men", "im", "R\u00e4\u00b7der\u00b7ge\u00b7saus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}