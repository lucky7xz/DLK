{"dta.poem.18943": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "22.  \n  Von Doctor Schlechten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.85", "sv:0.14"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Wjr musten gestern so starck sauffen/", "tokens": ["Wjr", "mus\u00b7ten", "ge\u00b7stern", "so", "starck", "sauf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADV", "ADV", "ADJD", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Da\u00df mein Pferd vnd Ich vbern hauffen", "tokens": ["Da\u00df", "mein", "Pferd", "vnd", "Ich", "vbern", "hauf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "PPER", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gefallen/ sprach der Doctor Schlecht:", "tokens": ["Ge\u00b7fal\u00b7len", "/", "sprach", "der", "Doc\u00b7tor", "Schlecht", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd Ich vermein/ Er redet recht", "tokens": ["Vnd", "Ich", "ver\u00b7mein", "/", "Er", "re\u00b7det", "recht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "PTKVZ", "$(", "PPER", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "(als der die warheit wolt bekennen)", "tokens": ["(", "als", "der", "die", "war\u00b7heit", "wolt", "be\u00b7ken\u00b7nen", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ART", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Esel nach dem Pferd zu nennen.", "tokens": ["Den", "E\u00b7sel", "nach", "dem", "Pferd", "zu", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}}}}}