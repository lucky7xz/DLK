{"textgrid.poem.33568": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sp\u00e4t noch, wenn schon l\u00e4ngst verklungen", "tokens": ["Sp\u00e4t", "noch", ",", "wenn", "schon", "l\u00e4ngst", "ver\u00b7klun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "$,", "KOUS", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle Saiten am Klavier,", "tokens": ["Al\u00b7le", "Sai\u00b7ten", "am", "Kla\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Zittert noch, was du gesungen,", "tokens": ["Zit\u00b7tert", "noch", ",", "was", "du", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die tiefste Seele mir;", "tokens": ["Durch", "die", "tiefs\u00b7te", "See\u00b7le", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fchrt mich \u00fcber Meeresweiten,", "tokens": ["F\u00fchrt", "mich", "\u00fc\u00b7ber", "Mee\u00b7res\u00b7wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "S\u00f6hnt mich aus mit dem Geschick", "tokens": ["S\u00f6hnt", "mich", "aus", "mit", "dem", "Ge\u00b7schick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "APPR", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Und verkn\u00fcpft mir alle Zeiten", "tokens": ["Und", "ver\u00b7kn\u00fcpft", "mir", "al\u00b7le", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit dem sch\u00f6nsten Augenblick.", "tokens": ["Mit", "dem", "sch\u00f6ns\u00b7ten", "Au\u00b7gen\u00b7blick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ja, noch mit der tiefen Wunde,", "tokens": ["Ja", ",", "noch", "mit", "der", "tie\u00b7fen", "Wun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die dein ernstes Wort mir schlug,", "tokens": ["Die", "dein", "erns\u00b7tes", "Wort", "mir", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Preis' ich ewig hoch die Stunde,", "tokens": ["Preis'", "ich", "e\u00b7wig", "hoch", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die dich mir entgegentrug.", "tokens": ["Die", "dich", "mir", "ent\u00b7ge\u00b7gen\u00b7trug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Frevel w\u00e4r' es, mehr zu sagen,", "tokens": ["Fre\u00b7vel", "w\u00e4r'", "es", ",", "mehr", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch es k\u00fchlt die bange Glut,", "tokens": ["Doch", "es", "k\u00fchlt", "die", "ban\u00b7ge", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df wir auch noch im Entsagen", "tokens": ["Da\u00df", "wir", "auch", "noch", "im", "Ent\u00b7sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Uns verstehn \u2013 ach, gar zu gut!", "tokens": ["Uns", "ver\u00b7stehn", "\u2013", "ach", ",", "gar", "zu", "gut", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$(", "XY", "$,", "ADV", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}