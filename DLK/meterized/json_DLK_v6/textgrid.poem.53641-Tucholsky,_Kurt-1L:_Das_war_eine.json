{"textgrid.poem.53641": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das war eine ", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das war eine ", "tokens": ["Das", "war", "ei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VAFIN", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Eine mit Organisation,", "tokens": ["Ei\u00b7ne", "mit", "Or\u00b7ga\u00b7ni\u00b7sa\u00b7ti\u00b7on", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "eine mit Stempeln und Kompetenzen \u2013", "tokens": ["ei\u00b7ne", "mit", "Stem\u00b7peln", "und", "Kom\u00b7pe\u00b7ten\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NN", "$("], "meter": "+--++-++--", "measure": "dactylic.init"}, "line.4": {"text": "beileibe nicht mit wilden T\u00e4nzen", "tokens": ["bei\u00b7lei\u00b7be", "nicht", "mit", "wil\u00b7den", "T\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "um Guillotinen \u2013", "tokens": ["um", "Guil\u00b7lo\u00b7ti\u00b7nen", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Nee, \u00fcber Ihnen", "tokens": ["Nee", ",", "\u00fc\u00b7ber", "Ih\u00b7nen"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "APPR", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "aber auch! Das ist des Landes nicht der Brauch!", "tokens": ["a\u00b7ber", "auch", "!", "Das", "ist", "des", "Lan\u00b7des", "nicht", "der", "Brauch", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "PDS", "VAFIN", "ART", "NN", "PTKNEG", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Denn \u00bbdas Aufstellen solcher Maschinen ist allen Roten", "tokens": ["Denn", "\u00bb", "das", "Auf\u00b7stel\u00b7len", "sol\u00b7cher", "Ma\u00b7schi\u00b7nen", "ist", "al\u00b7len", "Ro\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$(", "ART", "NN", "PIAT", "NN", "VAFIN", "PIAT", "NN"], "meter": "+-+--+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "auf \u00f6ffentlichen Pl\u00e4tzen bei Strafe verboten!", "tokens": ["auf", "\u00f6f\u00b7fent\u00b7li\u00b7chen", "Pl\u00e4t\u00b7zen", "bei", "Stra\u00b7fe", "ver\u00b7bo\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "(gez.) Piesecke. Kommissar.\u00ab", "tokens": ["(", "ge\u00b7z.", ")", "Pie\u00b7sec\u00b7ke", ".", "Kom\u00b7mis\u00b7sar", ".", "\u00ab"], "token_info": ["punct", "abbreviation", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "NE", "$(", "NN", "$.", "NN", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Alles so, wie es einstmals war.", "tokens": ["Al\u00b7les", "so", ",", "wie", "es", "einst\u00b7mals", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "$,", "PWAV", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Eigentlich: noch viel schlimmer.", "tokens": ["Ei\u00b7gent\u00b7lich", ":", "noch", "viel", "schlim\u00b7mer", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Offiziere im alten Waffenschimmer.", "tokens": ["Of\u00b7fi\u00b7zie\u00b7re", "im", "al\u00b7ten", "Waf\u00b7fen\u00b7schim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Jeder: 1 Pferd. Die H\u00f6hern: 2.", "tokens": ["Je\u00b7der", ":", "1", "Pferd", ".", "Die", "H\u00f6\u00b7hern", ":", "2."], "token_info": ["word", "punct", "number", "word", "punct", "word", "word", "punct", "ordinal"], "pos": ["PIS", "$.", "CARD", "NN", "$.", "ART", "NN", "$.", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Die Republik hats ja \u2013 dideldumdei!", "tokens": ["Die", "Re\u00b7pub\u00b7lik", "hats", "ja", "\u2013", "di\u00b7del\u00b7dum\u00b7dei", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$(", "FM", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Alles so, wie es immer gewesen.", "tokens": ["Al\u00b7les", "so", ",", "wie", "es", "im\u00b7mer", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "$,", "PWAV", "PPER", "ADV", "VAPP", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ins Gef\u00e4ngnis ohne viel Federlesen", "tokens": ["Ins", "Ge\u00b7f\u00e4ng\u00b7nis", "oh\u00b7ne", "viel", "Fe\u00b7der\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "PIAT", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "mit all dem Volk, das unbequem.", "tokens": ["mit", "all", "dem", "Volk", ",", "das", "un\u00b7be\u00b7quem", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Staatsgerichtshof? Es ist nicht an dem.", "tokens": ["Staats\u00b7ge\u00b7richts\u00b7hof", "?", "Es", "ist", "nicht", "an", "dem", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "PTKNEG", "APPR", "ART", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Unsere F\u00fchrer ermordet. Schl\u00e4ge . . . Geschrei . . .", "tokens": ["Un\u00b7se\u00b7re", "F\u00fch\u00b7rer", "er\u00b7mor\u00b7det", ".", "Schl\u00e4\u00b7ge", ".", ".", ".", "Ge\u00b7schrei", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "punct", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$.", "NN", "$.", "$.", "$.", "NN", "$.", "$.", "$."], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Helfferich w\u00fcnscht sich den Kaiser herbei.", "tokens": ["Helf\u00b7fe\u00b7rich", "w\u00fcnscht", "sich", "den", "Kai\u00b7ser", "her\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "\u00dcber all dem der Ausrei\u00dfer Ludendorff schwebt.", "tokens": ["\u00dc\u00b7ber", "all", "dem", "der", "Aus\u00b7rei\u00b7\u00dfer", "Lu\u00b7den\u00b7dorff", "schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "ART", "NN", "NE", "VVFIN", "$."], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Liebknecht ist tot.", "tokens": ["Lieb\u00b7knecht", "ist", "tot", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Und sowas lebt.", "tokens": ["Und", "so\u00b7was", "lebt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}