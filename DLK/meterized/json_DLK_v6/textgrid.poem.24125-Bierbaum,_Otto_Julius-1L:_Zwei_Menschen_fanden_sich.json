, "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ich sah mich selber an und erschrak,", "tokens": ["Ich", "sah", "mich", "sel\u00b7ber", "an", "und", "er\u00b7schrak", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Da\u00df ich allein sei.", "tokens": ["Da\u00df", "ich", "al\u00b7lein", "sei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Endlos Leben an Leben um mich,", "tokens": ["End\u00b7los", "Le\u00b7ben", "an", "Le\u00b7ben", "um", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "NN", "APPR", "PPER", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Ich aber allein,", "tokens": ["Ich", "a\u00b7ber", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und nichts \u00fcber mir.", "tokens": ["Und", "nichts", "\u00fc\u00b7ber", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Da b\u00fcckte ich mich in mich selbst", "tokens": ["Da", "b\u00fcck\u00b7te", "ich", "mich", "in", "mich", "selbst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und verbarg mich in mir", "tokens": ["Und", "ver\u00b7barg", "mich", "in", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPER"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Und tr\u00e4umte.", "tokens": ["Und", "tr\u00e4um\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.10": {"line.1": {"text": "Was ich getr\u00e4umt, war wirr und wild,", "tokens": ["Was", "ich", "ge\u00b7tr\u00e4umt", ",", "war", "wirr", "und", "wild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aber als ich erwachte", "tokens": ["A\u00b7ber", "als", "ich", "er\u00b7wach\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "War ich heiter und wu\u00dfte", "tokens": ["War", "ich", "hei\u00b7ter", "und", "wu\u00df\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Den Sinn meines Lebens.", "tokens": ["Den", "Sinn", "mei\u00b7nes", "Le\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Der ist: Still mich treiben lassen von dem,", "tokens": ["Der", "ist", ":", "Still", "mich", "trei\u00b7ben", "las\u00b7sen", "von", "dem", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$.", "ADJD", "PPER", "VVINF", "VVINF", "APPR", "ART", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Das in mir ist und nicht fragen:", "tokens": ["Das", "in", "mir", "ist", "und", "nicht", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPER", "VAFIN", "KON", "PTKNEG", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wohin?", "tokens": ["Wo\u00b7hin", "?"], "token_info": ["word", "punct"], "pos": ["PWAV", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.11": {"line.1": {"text": "Dunkel sind die Ziele,", "tokens": ["Dun\u00b7kel", "sind", "die", "Zie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Dunkel sind die G\u00f6tter,", "tokens": ["Dun\u00b7kel", "sind", "die", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Dunkel ist die Welt.", "tokens": ["Dun\u00b7kel", "ist", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Aber eine warme Flamme leuchtet in mir", "tokens": ["A\u00b7ber", "ei\u00b7ne", "war\u00b7me", "Flam\u00b7me", "leuch\u00b7tet", "in", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPR", "PPER"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.5": {"text": "Und l\u00e4\u00dft mich wachsen.", "tokens": ["Und", "l\u00e4\u00dft", "mich", "wach\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "Weiter wei\u00df ich nichts als diese Flamme,", "tokens": ["Wei\u00b7ter", "wei\u00df", "ich", "nichts", "a