{"textgrid.poem.24125": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zwei Menschen fanden sich", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei Menschen fanden sich", "tokens": ["Zwei", "Men\u00b7schen", "fan\u00b7den", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Im dichten Garten des Lebens,", "tokens": ["Im", "dich\u00b7ten", "Gar\u00b7ten", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie sich zwei Bl\u00e4tter im Wirbelwinde finden;", "tokens": ["Wie", "sich", "zwei", "Bl\u00e4t\u00b7ter", "im", "Wir\u00b7bel\u00b7win\u00b7de", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "CARD", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und sie zeugten mich.", "tokens": ["Und", "sie", "zeug\u00b7ten", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Dann haben sie mich geh\u00fctet und gen\u00e4hrt,", "tokens": ["Dann", "ha\u00b7ben", "sie", "mich", "ge\u00b7h\u00fc\u00b7tet", "und", "ge\u00b7n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVPP", "KON", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Gehalten und gef\u00fchrt,", "tokens": ["Ge\u00b7hal\u00b7ten", "und", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bis ich stark ward, allein zu gehen", "tokens": ["Bis", "ich", "stark", "ward", ",", "al\u00b7lein", "zu", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In den gro\u00dfen, dichten Garten.", "tokens": ["In", "den", "gro\u00b7\u00dfen", ",", "dich\u00b7ten", "Gar\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich bin aufs geradewohl gegangen,", "tokens": ["Ich", "bin", "aufs", "ge\u00b7ra\u00b7de\u00b7wohl", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADV", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dahin, dorthin,", "tokens": ["Da\u00b7hin", ",", "dor\u00b7thin", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["PAV", "$,", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Hatte kein Ziel.", "tokens": ["Hat\u00b7te", "kein", "Ziel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Irgend ein Ding in mir", "tokens": ["Ir\u00b7gend", "ein", "Ding", "in", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "PPER"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Trieb mich,", "tokens": ["Trieb", "mich", ","], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Bald sachte dr\u00e4ngend wie aus dunklen Tiefen,", "tokens": ["Bald", "sach\u00b7te", "dr\u00e4n\u00b7gend", "wie", "aus", "dunk\u00b7len", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "KOKOM", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Bald mit St\u00f6\u00dfen, die waren,", "tokens": ["Bald", "mit", "St\u00f6\u00b7\u00dfen", ",", "die", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "PRELS", "VAFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Als ob sie aus grellen Hellen k\u00e4men.", "tokens": ["Als", "ob", "sie", "aus", "grel\u00b7len", "Hel\u00b7len", "k\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Manchmal stand ich still", "tokens": ["Manch\u00b7mal", "stand", "ich", "still"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Und lauschte:", "tokens": ["Und", "lauschte", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Ob ich nicht einen Ruf vern\u00e4hme, da\u00df ich w\u00fc\u00dfte:", "tokens": ["Ob", "ich", "nicht", "ei\u00b7nen", "Ruf", "ver\u00b7n\u00e4h\u00b7me", ",", "da\u00df", "ich", "w\u00fc\u00df\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wohin?", "tokens": ["Wo\u00b7hin", "?"], "token_info": ["word", "punct"], "pos": ["PWAV", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.4": {"line.1": {"text": "Kein Ruf.", "tokens": ["Kein", "Ruf", "."], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.5": {"line.1": {"text": "Wanderte weiter in die Welt", "tokens": ["Wan\u00b7der\u00b7te", "wei\u00b7ter", "in", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ohn Ahnen, wohin.", "tokens": ["Ohn", "Ah\u00b7nen", ",", "wo\u00b7hin", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Aber das Ding in mir,", "tokens": ["A\u00b7ber", "das", "Ding", "in", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Das wu\u00dfte wohl, wohin", "tokens": ["Das", "wu\u00df\u00b7te", "wohl", ",", "wo\u00b7hin"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PDS", "VVFIN", "ADV", "$,", "PWAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Michs triebe.", "tokens": ["Michs", "trie\u00b7be", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.6": {"line.1": {"text": "Hat mich \u00fcber Berge gef\u00fchrt,", "tokens": ["Hat", "mich", "\u00fc\u00b7ber", "Ber\u00b7ge", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Abgr\u00fcnden vor\u00fcber,", "tokens": ["Ab\u00b7gr\u00fcn\u00b7den", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Hat mich durch schw\u00fcle Ebenen gedr\u00e4ngt,", "tokens": ["Hat", "mich", "durch", "schw\u00fc\u00b7le", "E\u00b7be\u00b7nen", "ge\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.4": {"text": "Mitten durch Fieberd\u00fcnste,", "tokens": ["Mit\u00b7ten", "durch", "Fie\u00b7ber\u00b7d\u00fcns\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Warf mich aufs Meer und lehrte mich schwimmen.", "tokens": ["Warf", "mich", "aufs", "Meer", "und", "lehr\u00b7te", "mich", "schwim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "KON", "VVFIN", "PPER", "VVINF", "$."], "meter": "++-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Manchen Sto\u00df erhielt ich in der Welt,", "tokens": ["Man\u00b7chen", "Sto\u00df", "er\u00b7hielt", "ich", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Wunden empfing ich,", "tokens": ["Wun\u00b7den", "emp\u00b7fing", "ich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Die Narben wurden,", "tokens": ["Die", "Nar\u00b7ben", "wur\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Schmerzen w\u00fchlten sich Wohnungen in mir", "tokens": ["Schmer\u00b7zen", "w\u00fchl\u00b7ten", "sich", "Woh\u00b7nun\u00b7gen", "in", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "NN", "APPR", "PPER"], "meter": "+-+---+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Und kalkten sich ein;", "tokens": ["Und", "kalk\u00b7ten", "sich", "ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Ich m\u00fc\u00dfte mich selber zerrei\u00dfen,", "tokens": ["Ich", "m\u00fc\u00df\u00b7te", "mich", "sel\u00b7ber", "zer\u00b7rei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.12": {"text": "Wollte ich sie aus mir austreiben.", "tokens": ["Woll\u00b7te", "ich", "sie", "aus", "mir", "aus\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "+----+-+-", "measure": "dactylic.init"}, "line.13": {"text": "Ich verga\u00df sie, wenn sie nicht tobten,", "tokens": ["Ich", "ver\u00b7ga\u00df", "sie", ",", "wenn", "sie", "nicht", "tob\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "--+-++-+-", "measure": "anapaest.init"}, "line.14": {"text": "Und, wenn sie tobten, schrie ich mit,", "tokens": ["Und", ",", "wenn", "sie", "tob\u00b7ten", ",", "schrie", "ich", "mit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Bis sie stille waren.", "tokens": ["Bis", "sie", "stil\u00b7le", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "VAFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.16": {"text": "Hetzte auch einen Schmerz auf den andern,", "tokens": ["Hetz\u00b7te", "auch", "ei\u00b7nen", "Schmerz", "auf", "den", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "ART", "ADJA", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.17": {"text": "Da\u00df sie sich fra\u00dfen,", "tokens": ["Da\u00df", "sie", "sich", "fra\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Und ich lachte, wenn ich sah,", "tokens": ["Und", "ich", "lach\u00b7te", ",", "wenn", "ich", "sah", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Wie sie im Uebereinanderherfallen", "tokens": ["Wie", "sie", "im", "Ue\u00b7be\u00b7re\u00b7in\u00b7an\u00b7der\u00b7her\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "St\u00fccke aus mir rissen.", "tokens": ["St\u00fc\u00b7cke", "aus", "mir", "ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Dann kamen weiche H\u00e4nde und streichelten mich;", "tokens": ["Dann", "ka\u00b7men", "wei\u00b7che", "H\u00e4n\u00b7de", "und", "strei\u00b7chel\u00b7ten", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+++-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Wie ein schwarzer Baum, der gr\u00fcne Knospen", "tokens": ["Wie", "ein", "schwar\u00b7zer", "Baum", ",", "der", "gr\u00fc\u00b7ne", "Knos\u00b7pen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Der Sonne aufthut, f\u00fchlte ich die Wollust", "tokens": ["Der", "Son\u00b7ne", "auf\u00b7thut", ",", "f\u00fchl\u00b7te", "ich", "die", "Wol\u00b7lust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Sein zu werden.", "tokens": ["Im", "Sein", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Alles, das war,", "tokens": ["Al\u00b7les", ",", "das", "war", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PIS", "$,", "PDS", "VAFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "War nur f\u00fcr mich,", "tokens": ["War", "nur", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Alle die Welt", "tokens": ["Al\u00b7le", "die", "Welt"], "token_info": ["word", "word", "word"], "pos": ["PIS", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "War mein Geschwister.", "tokens": ["War", "mein", "Ge\u00b7schwis\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Ich wuchs in die Welt, wie in der Blume", "tokens": ["Ich", "wuchs", "in", "die", "Welt", ",", "wie", "in", "der", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Der starke Samenstengel sich hebt,", "tokens": ["Der", "star\u00b7ke", "Sa\u00b7mens\u00b7ten\u00b7gel", "sich", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Und mir war: Ich w\u00e4re der Sinn der Welt.", "tokens": ["Und", "mir", "war", ":", "Ich", "w\u00e4\u00b7re", "der", "Sinn", "der", "Welt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$.", "PPER", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Wunderbar schwoll meine Seele aus,", "tokens": ["Wun\u00b7der\u00b7bar", "schwoll", "mei\u00b7ne", "See\u00b7le", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ueber mich weg in die Ahnungen des Seins;", "tokens": ["Ue\u00b7ber", "mich", "weg", "in", "die", "Ah\u00b7nun\u00b7gen", "des", "Seins", ";"], "token_info": ["word", "word", "word", "word", "word", 