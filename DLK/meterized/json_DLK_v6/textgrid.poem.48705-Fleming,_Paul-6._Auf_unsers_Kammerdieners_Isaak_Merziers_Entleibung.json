{"textgrid.poem.48705": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "6. Auf unsers Kammerdieners Isaak Merziers Entleibung", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, da\u00df du dasmal doch vom Schlafe bist erwacht!", "tokens": ["Ach", ",", "da\u00df", "du", "das\u00b7mal", "doch", "vom", "Schla\u00b7fe", "bist", "er\u00b7wacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADV", "ADV", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gab dir denn deinen Tod kein Traum nicht zu verstehen?", "tokens": ["Gab", "dir", "denn", "dei\u00b7nen", "Tod", "kein", "Traum", "nicht", "zu", "ver\u00b7ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PIAT", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nein, dein Verh\u00e4ngn\u00fc\u00df hie\u00df dich an die M\u00f6rder gehen.", "tokens": ["Nein", ",", "dein", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "hie\u00df", "dich", "an", "die", "M\u00f6r\u00b7der", "ge\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da liegst du nun vor uns. Die ungetreue Nacht,", "tokens": ["Da", "liegst", "du", "nun", "vor", "uns", ".", "Die", "un\u00b7ge\u00b7treu\u00b7e", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "die blind ist und blind macht, die hat dich umgebracht.", "tokens": ["die", "blind", "ist", "und", "blind", "macht", ",", "die", "hat", "dich", "um\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "KON", "ADJD", "VVFIN", "$,", "PRELS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der T\u00e4ter folgt der Flucht der wolgefu\u00dften Rehen.", "tokens": ["Der", "T\u00e4\u00b7ter", "folgt", "der", "Flucht", "der", "wol\u00b7ge\u00b7fu\u00df\u00b7ten", "Re\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir m\u00fcssen dir dein Grab auch auf der Flucht erh\u00f6hen,", "tokens": ["Wir", "m\u00fcs\u00b7sen", "dir", "dein", "Grab", "auch", "auf", "der", "Flucht", "er\u00b7h\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "weil unser langer Weg uns also eilen macht.", "tokens": ["weil", "un\u00b7ser", "lan\u00b7ger", "Weg", "uns", "al\u00b7so", "ei\u00b7len", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PPER", "ADV", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Lieg' hier und ruhe wol in deiner k\u00fchlen Erden!", "tokens": ["Lieg'", "hier", "und", "ru\u00b7he", "wol", "in", "dei\u00b7ner", "k\u00fch\u00b7len", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "NN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gott wei\u00df, wie, wo und wenn wir Andern fallen werden.", "tokens": ["Gott", "wei\u00df", ",", "wie", ",", "wo", "und", "wenn", "wir", "An\u00b7dern", "fal\u00b7len", "wer\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PWAV", "$,", "PWAV", "KON", "KOUS", "PPER", "ADJA", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der geb' uns besser Heil, der geb' uns bessern Mut!", "tokens": ["Der", "geb'", "uns", "bes\u00b7ser", "Heil", ",", "der", "geb'", "uns", "bes\u00b7sern", "Mut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "NN", "$,", "PRELS", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dank hab' auch, liebe Stat, f\u00fcr alle deine Treue!", "tokens": ["Dank", "hab'", "auch", ",", "lie\u00b7be", "Stat", ",", "f\u00fcr", "al\u00b7le", "dei\u00b7ne", "Treu\u00b7e", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "ADV", "$,", "ADJA", "NN", "$,", "APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch da\u00df nach dieser Zeit dich unser nicht gereue", "tokens": ["Doch", "da\u00df", "nach", "die\u00b7ser", "Zeit", "dich", "un\u00b7ser", "nicht", "ge\u00b7reu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "PDAT", "NN", "PPER", "PPOSAT", "PTKNEG", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und du seist recht gerecht, so r\u00e4ch' auch unser Blut!", "tokens": ["und", "du", "seist", "recht", "ge\u00b7recht", ",", "so", "r\u00e4ch'", "auch", "un\u00b7ser", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJD", "$,", "ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}