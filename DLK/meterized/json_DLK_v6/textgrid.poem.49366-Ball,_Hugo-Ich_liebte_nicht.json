{"textgrid.poem.49366": {"metadata": {"author": {"name": "Ball, Hugo", "birth": "N.A.", "death": "N.A."}, "title": "Ich liebte nicht", "genre": "verse", "period": "N.A.", "pub_year": 1915, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich liebte nicht die Totenkopfhusaren", "tokens": ["Ich", "lieb\u00b7te", "nicht", "die", "To\u00b7ten\u00b7kopf\u00b7hu\u00b7sa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und nicht die M\u00f6rser mit den M\u00e4dchennamen", "tokens": ["Und", "nicht", "die", "M\u00f6r\u00b7ser", "mit", "den", "M\u00e4d\u00b7chen\u00b7na\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und als am End die gro\u00dfen Tage kamen,", "tokens": ["Und", "als", "am", "End", "die", "gro\u00b7\u00dfen", "Ta\u00b7ge", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da bin ich unauff\u00e4llig weggefahren.", "tokens": ["Da", "bin", "ich", "un\u00b7auf\u00b7f\u00e4l\u00b7lig", "weg\u00b7ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gott sei's geklagt und ihnen, meine Damen:", "tokens": ["Gott", "sei's", "ge\u00b7klagt", "und", "ih\u00b7nen", ",", "mei\u00b7ne", "Da\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "KON", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gleich Absalom blieb ich an langen Haaren,", "tokens": ["Gleich", "Ab\u00b7sa\u00b7lom", "blieb", "ich", "an", "lan\u00b7gen", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dieweil sie schluchzten \u00fcber Totenbahren", "tokens": ["Die\u00b7weil", "sie", "schluchz\u00b7ten", "\u00fc\u00b7ber", "To\u00b7ten\u00b7bah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Wehbaum h\u00e4ngen aller ihrer Dramen.", "tokens": ["Im", "Weh\u00b7baum", "h\u00e4n\u00b7gen", "al\u00b7ler", "ih\u00b7rer", "Dra\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie werden auch in diesen Versen finden", "tokens": ["Sie", "wer\u00b7den", "auch", "in", "die\u00b7sen", "Ver\u00b7sen", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PDAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Manch Marterspiel und st\u00fcrzend Abenteuer.", "tokens": ["Manch", "Mar\u00b7ter\u00b7spiel", "und", "st\u00fcr\u00b7zend", "A\u00b7bent\u00b7eu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Man stirbt nicht nur durch Minen und durch Flinten.", "tokens": ["Man", "stirbt", "nicht", "nur", "durch", "Mi\u00b7nen", "und", "durch", "Flin\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "ADV", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Man wird nicht von Granaten nur zerrissen.", "tokens": ["Man", "wird", "nicht", "von", "Gra\u00b7na\u00b7ten", "nur", "zer\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PTKNEG", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In meine N\u00e4chte drangen Ungeheuer,", "tokens": ["In", "mei\u00b7ne", "N\u00e4ch\u00b7te", "dran\u00b7gen", "Un\u00b7ge\u00b7heu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die mich die H\u00f6lle wohl empfinden lie\u00dfen.", "tokens": ["Die", "mich", "die", "H\u00f6l\u00b7le", "wohl", "emp\u00b7fin\u00b7den", "lie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}