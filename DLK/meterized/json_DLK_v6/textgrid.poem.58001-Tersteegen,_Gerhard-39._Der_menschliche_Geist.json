{"textgrid.poem.58001": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "39. Der menschliche Geist", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Geist, ein Wunderding, k\u00f6nnt er gleich alles kriegen,", "tokens": ["Ein", "Geist", ",", "ein", "Wun\u00b7der\u00b7ding", ",", "k\u00f6nnt", "er", "gleich", "al\u00b7les", "krie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "VVFIN", "PPER", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was Erd' und Himmel hat, es w\u00fcrd' ihn nicht vergn\u00fcgen;", "tokens": ["Was", "Erd'", "und", "Him\u00b7mel", "hat", ",", "es", "w\u00fcrd'", "ihn", "nicht", "ver\u00b7gn\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VAFIN", "$,", "PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sobald er aber Gott im Grund gefunden hat,", "tokens": ["So\u00b7bald", "er", "a\u00b7ber", "Gott", "im", "Grund", "ge\u00b7fun\u00b7den", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "APPRART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "H\u00e4tt' er auch gar nichts mehr, so spricht er: Ich bin satt.", "tokens": ["H\u00e4tt'", "er", "auch", "gar", "nichts", "mehr", ",", "so", "spricht", "er", ":", "Ich", "bin", "satt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PIS", "ADV", "$,", "ADV", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}