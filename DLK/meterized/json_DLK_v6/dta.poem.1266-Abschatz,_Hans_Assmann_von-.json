{"dta.poem.1266": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Was machstu noch bey mir/ vergebnes Hoffen?", "tokens": ["Was", "machs\u00b7tu", "noch", "bey", "mir", "/", "ver\u00b7geb\u00b7nes", "Hof\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "PPER", "$(", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Du hast getroffen", "tokens": ["Du", "hast", "ge\u00b7trof\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Verst\u00e4hlte Sinnen/", "tokens": ["Ver\u00b7st\u00e4hl\u00b7te", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die zu gewinnen", "tokens": ["Die", "zu", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "PTKZU", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Kein \u00e4chzend Sehnen", "tokens": ["Kein", "\u00e4ch\u00b7zend", "Seh\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJD", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Kein\u2019 heisse Thr\u00e4nen", "tokens": ["Kein'", "heis\u00b7se", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Genung seyn k\u00fcnnen/", "tokens": ["Ge\u00b7nung", "seyn", "k\u00fcn\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Die von den tr\u00fcben Augen rinnen.", "tokens": ["Die", "von", "den", "tr\u00fc\u00b7ben", "Au\u00b7gen", "rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ist denn nun kein Erbarmni\u00df hier/", "tokens": ["Ist", "denn", "nun", "kein", "Er\u00b7barm\u00b7ni\u00df", "hier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PIAT", "NN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Was machst du noch bey mir?", "tokens": ["Was", "machst", "du", "noch", "bey", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}