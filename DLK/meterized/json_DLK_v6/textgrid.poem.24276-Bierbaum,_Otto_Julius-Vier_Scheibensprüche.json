{"textgrid.poem.24276": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Vier Scheibenspr\u00fcche", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hei\u00df: Das Gl\u00fcck. Soll es dir gl\u00fccken,", "tokens": ["Ich", "hei\u00df", ":", "Das", "Gl\u00fcck", ".", "Soll", "es", "dir", "gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$.", "ART", "NN", "$.", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df du mich triffst, mu\u00dft dus verstehn,", "tokens": ["Da\u00df", "du", "mich", "triffst", ",", "mu\u00dft", "dus", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So, wie im Leben: scharf zu sehn", "tokens": ["So", ",", "wie", "im", "Le\u00b7ben", ":", "scharf", "zu", "sehn"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "APPRART", "NN", "$.", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und doch \u2013 ein Auge zuzudr\u00fccken.", "tokens": ["Und", "doch", "\u2013", "ein", "Au\u00b7ge", "zu\u00b7zu\u00b7dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer auf die Mitte h\u00e4lt, h\u00e4lt richtig.", "tokens": ["Wer", "auf", "die", "Mit\u00b7te", "h\u00e4lt", ",", "h\u00e4lt", "rich\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Satz ist auch f\u00fcrs Leben wichtig,", "tokens": ["Der", "Satz", "ist", "auch", "f\u00fcrs", "Le\u00b7ben", "wich\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo mancher schon ins Leere lief,", "tokens": ["Wo", "man\u00b7cher", "schon", "ins", "Lee\u00b7re", "lief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hielt er sich nur ein bi\u00dfchen schief:", "tokens": ["Hielt", "er", "sich", "nur", "ein", "bi\u00df\u00b7chen", "schief", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu rechts, zu links, zu hoch, zu tief.", "tokens": ["Zu", "rechts", ",", "zu", "links", ",", "zu", "hoch", ",", "zu", "tief", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "APPR", "ADV", "$,", "PTKA", "ADJD", "$,", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ruhiges Blut, ruhige Hand,", "tokens": ["Ru\u00b7hi\u00b7ges", "Blut", ",", "ru\u00b7hi\u00b7ge", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ruhiger Blick, gradaus gewandt:", "tokens": ["Ru\u00b7hi\u00b7ger", "Blick", ",", "gra\u00b7daus", "ge\u00b7wandt", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die drei guten Gaben", "tokens": ["Die", "drei", "gu\u00b7ten", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "CARD", "ADJA", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.4": {"text": "M\u00f6ge das deutsche Vaterland", "tokens": ["M\u00f6\u00b7ge", "das", "deut\u00b7sche", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Nicht blo\u00df hier am Sch\u00fctzenstand", "tokens": ["Nicht", "blo\u00df", "hier", "am", "Sch\u00fct\u00b7zen\u00b7stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ADV", "APPRART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Immer in F\u00fclle haben.", "tokens": ["Im\u00b7mer", "in", "F\u00fcl\u00b7le", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VAFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "K\u00e4ms blo\u00df aufs Knallen an,", "tokens": ["K\u00e4ms", "blo\u00df", "aufs", "Knal\u00b7len", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Tr\u00e4f jede B\u00fcchs.", "tokens": ["Tr\u00e4f", "je\u00b7de", "B\u00fcchs", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Viel redet mancher Mann", "tokens": ["Viel", "re\u00b7det", "man\u00b7cher", "Mann"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und sagt doch nix.", "tokens": ["Und", "sagt", "doch", "nix", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}