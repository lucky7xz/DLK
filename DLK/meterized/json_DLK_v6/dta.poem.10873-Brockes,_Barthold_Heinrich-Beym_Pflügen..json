{"dta.poem.10873": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Beym Pfl\u00fcgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seh ich den regen Pflug, auf dunklem Grunde, ziehn:", "tokens": ["Seh", "ich", "den", "re\u00b7gen", "Pflug", ",", "auf", "dunk\u00b7lem", "Grun\u00b7de", ",", "ziehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Scheint mir der Ackersmann, sich gleichsam zu bem\u00fchn,", "tokens": ["Scheint", "mir", "der", "A\u00b7ckers\u00b7mann", ",", "sich", "gleich\u00b7sam", "zu", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der milden Mutter dunkles Kleid,", "tokens": ["Der", "mil\u00b7den", "Mut\u00b7ter", "dunk\u00b7les", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit einer neuen Zierlichkeit,", "tokens": ["Mit", "ei\u00b7ner", "neu\u00b7en", "Zier\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und in der That mit sch\u00f6nen gr\u00fcnen Schn\u00fcren,", "tokens": ["Und", "in", "der", "That", "mit", "sch\u00f6\u00b7nen", "gr\u00fc\u00b7nen", "Schn\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In der geraden Furchen Strichen, durch die bald gr\u00fcne Saat,", "tokens": ["In", "der", "ge\u00b7ra\u00b7den", "Fur\u00b7chen", "Stri\u00b7chen", ",", "durch", "die", "bald", "gr\u00fc\u00b7ne", "Saat", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,", "APPR", "PRELS", "ADV", "ADJA", "NN", "$,"], "meter": "---+-+-+----+-+", "measure": "unknown.measure.penta"}, "line.7": {"text": "Die gr\u00fcnen Schn\u00fcre werden endlich, durch den recht g\u00fcldnen", "tokens": ["Die", "gr\u00fc\u00b7nen", "Schn\u00fc\u00b7re", "wer\u00b7den", "end\u00b7lich", ",", "durch", "den", "recht", "g\u00fcld\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "$,", "APPR", "ART", "ADJD", "ADJA"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Mit Golde gleichsam \u00fcbersponnen,", "tokens": ["Mit", "Gol\u00b7de", "gleich\u00b7sam", "\u00fc\u00b7bers\u00b7pon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wodurch ein g\u00fcldnes Tuch zuletzt die Mutter schm\u00fcckt,", "tokens": ["Wo\u00b7durch", "ein", "g\u00fcld\u00b7nes", "Tuch", "zu\u00b7letzt", "die", "Mut\u00b7ter", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Bis endlich, wenn der Schmuck der Erden,", "tokens": ["Bis", "end\u00b7lich", ",", "wenn", "der", "Schmuck", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "KOUS", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "In unsre Scheunen eingef\u00fchrt,", "tokens": ["In", "uns\u00b7re", "Scheu\u00b7nen", "ein\u00b7ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die K\u00f6rner, die vorhin das Feld geziert,", "tokens": ["Die", "K\u00f6r\u00b7ner", ",", "die", "vor\u00b7hin", "das", "Feld", "ge\u00b7ziert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "F\u00fcr uns zu wirklichem und wahrem Golde werden.", "tokens": ["F\u00fcr", "uns", "zu", "wirk\u00b7li\u00b7chem", "und", "wah\u00b7rem", "Gol\u00b7de", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ADJA", "KON", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}