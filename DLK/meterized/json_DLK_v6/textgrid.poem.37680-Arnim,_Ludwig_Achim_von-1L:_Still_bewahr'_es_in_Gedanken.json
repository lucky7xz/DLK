{"textgrid.poem.37680": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Still bewahr' es in Gedanken", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Still bewahr' es in Gedanken", "tokens": ["Still", "be\u00b7wahr'", "es", "in", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses tief geheime Wort,", "tokens": ["Die\u00b7ses", "tief", "ge\u00b7hei\u00b7me", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur im Herzen ist der Ort,", "tokens": ["Nur", "im", "Her\u00b7zen", "ist", "der", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo der Adel tritt in Schranken,", "tokens": ["Wo", "der", "A\u00b7del", "tritt", "in", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn die Jugend in den N\u00f6then", "tokens": ["Wenn", "die", "Ju\u00b7gend", "in", "den", "N\u00f6\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Helllaut rufet mit Trommeten.", "tokens": ["Hell\u00b7laut", "ru\u00b7fet", "mit", "Trom\u00b7me\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "In den Schranken stehn die Ahnen,", "tokens": ["In", "den", "Schran\u00b7ken", "stehn", "die", "Ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn der Zweifel Kampf beginnt,", "tokens": ["Wenn", "der", "Zwei\u00b7fel", "Kampf", "be\u00b7ginnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie aus Fels die Quelle rinnt,", "tokens": ["Wie", "aus", "Fels", "die", "Quel\u00b7le", "rinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NE", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Frischend ihre Geister mahnen,", "tokens": ["Fri\u00b7schend", "ih\u00b7re", "Geis\u00b7ter", "mah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Geister werden zu Gedanken,", "tokens": ["Geis\u00b7ter", "wer\u00b7den", "zu", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Halten fest wo alle wanken.", "tokens": ["Hal\u00b7ten", "fest", "wo", "al\u00b7le", "wan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "PWAV", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Geister sind in jedem Hause.", "tokens": ["Geis\u00b7ter", "sind", "in", "je\u00b7dem", "Hau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wecken aus dem Schlaf den Muth.", "tokens": ["We\u00b7cken", "aus", "dem", "Schlaf", "den", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Also rinnt das edle Blut,", "tokens": ["Al\u00b7so", "rinnt", "das", "ed\u00b7le", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Geistig wie der Wein beim Schmause,", "tokens": ["Geis\u00b7tig", "wie", "der", "Wein", "beim", "Schmau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df vereinet, die getrennet,", "tokens": ["Da\u00df", "ver\u00b7ei\u00b7net", ",", "die", "ge\u00b7tren\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "$,", "PRELS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine Lieb' in allen brennet.", "tokens": ["Ei\u00b7ne", "Lieb'", "in", "al\u00b7len", "bren\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Immer mit dem gr\u00f6\u00dften Ma\u00dfe", "tokens": ["Im\u00b7mer", "mit", "dem", "gr\u00f6\u00df\u00b7ten", "Ma\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mi\u00dft des Hauses Geist das Kind,", "tokens": ["Mi\u00dft", "des", "Hau\u00b7ses", "Geist", "das", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das Kind sich dehnt geschwind,", "tokens": ["Und", "das", "Kind", "sich", "dehnt", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PRF", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Will sich zeigen von der Rasse,", "tokens": ["Will", "sich", "zei\u00b7gen", "von", "der", "Ras\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was ihm Herrliches bescheeret,", "tokens": ["Was", "ihm", "Herr\u00b7li\u00b7ches", "be\u00b7schee\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zeigt sich h\u00f6her, sicher w\u00e4hret.", "tokens": ["Zeigt", "sich", "h\u00f6\u00b7her", ",", "si\u00b7cher", "w\u00e4h\u00b7ret", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "$,", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nicht die Geister zu vertreiben,", "tokens": ["Nicht", "die", "Geis\u00b7ter", "zu", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht des Volkes Geist jetzt auf,", "tokens": ["Steht", "des", "Vol\u00b7kes", "Geist", "jetzt", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rein, da\u00df jedem freier Lauf,", "tokens": ["Rein", ",", "da\u00df", "je\u00b7dem", "frei\u00b7er", "Lauf", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedem Haus ein Geist soll bleiben:", "tokens": ["Je\u00b7dem", "Haus", "ein", "Geist", "soll", "blei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}