{"textgrid.poem.49771": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie sch\u00f6n, o Mensch, mit deinem Palmenzweige", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sch\u00f6n, o Mensch, mit deinem Palmenzweige", "tokens": ["Wie", "sch\u00f6n", ",", "o", "Mensch", ",", "mit", "dei\u00b7nem", "Pal\u00b7men\u00b7zwei\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "FM", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Stehst du mit gro\u00dfem Stolz und Selbstgen\u00fcgung", "tokens": ["Stehst", "du", "mit", "gro\u00b7\u00dfem", "Stolz", "und", "Selbst\u00b7ge\u00b7n\u00fc\u00b7gung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum zweitenmal an des Jahrhunderts Neige!", "tokens": ["Zum", "zwei\u00b7ten\u00b7mal", "an", "des", "Jahr\u00b7hun\u00b7derts", "Nei\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Im Vorjahr war's nur preu\u00dfische Verf\u00fcgung.", "tokens": ["Im", "Vor\u00b7jahr", "wa\u00b7r's", "nur", "preu\u00b7\u00dfi\u00b7sche", "Ver\u00b7f\u00fc\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Civis Germanus, la\u00df dir heute brauen", "tokens": ["Ci\u00b7vis", "Ger\u00b7ma\u00b7nus", ",", "la\u00df", "dir", "heu\u00b7te", "brau\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "$,", "VVIMP", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die gro\u00dfe Bowle voll des hei\u00dfen Punsches,", "tokens": ["Die", "gro\u00b7\u00dfe", "Bow\u00b7le", "voll", "des", "hei\u00b7\u00dfen", "Pun\u00b7sches", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Z\u00fcnd eine Pfeife an; dann la\u00df uns schauen,", "tokens": ["Z\u00fcnd", "ei\u00b7ne", "Pfei\u00b7fe", "an", ";", "dann", "la\u00df", "uns", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$.", "ADV", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was schon erreicht, und was noch Ziel des Wunsches.", "tokens": ["Was", "schon", "er\u00b7reicht", ",", "und", "was", "noch", "Ziel", "des", "Wun\u00b7sches", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "$,", "KON", "PWS", "ADV", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hast du bedacht und hast du wohl erwogen?", "tokens": ["Hast", "du", "be\u00b7dacht", "und", "hast", "du", "wohl", "er\u00b7wo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "KON", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf deiner Stirn tr\u00e4gst du den Weltmachtsstempel,", "tokens": ["Auf", "dei\u00b7ner", "Stirn", "tr\u00e4gst", "du", "den", "Welt\u00b7machts\u00b7stem\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du hast jetzt Ruhm und Ehre eingesogen", "tokens": ["Du", "hast", "jetzt", "Ruhm", "und", "Eh\u00b7re", "ein\u00b7ge\u00b7so\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie fr\u00fcher Schnaps und Wein und andern Plempel.", "tokens": ["Wie", "fr\u00fc\u00b7her", "Schnaps", "und", "Wein", "und", "an\u00b7dern", "Plem\u00b7pel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "KON", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die deutsche Fahne weht in fernen Zonen;", "tokens": ["Die", "deut\u00b7sche", "Fah\u00b7ne", "weht", "in", "fer\u00b7nen", "Zo\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Graf Waldersee befehligt die Paraden.", "tokens": ["Graf", "Wal\u00b7der\u00b7see", "be\u00b7feh\u00b7ligt", "die", "Pa\u00b7ra\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und den Ministern billige Tiraden.", "tokens": ["Und", "den", "Mi\u00b7nis\u00b7tern", "bil\u00b7li\u00b7ge", "Ti\u00b7ra\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}, "stanza.5": {"line.1": {"text": "Moderner Hunne, la\u00df uns darauf prosten,", "tokens": ["Mo\u00b7der\u00b7ner", "Hun\u00b7ne", ",", "la\u00df", "uns", "da\u00b7rauf", "pros\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVIMP", "PPER", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es lebe Bl\u00fccher, Waldersee und Wrangel!", "tokens": ["Es", "le\u00b7be", "Bl\u00fc\u00b7cher", ",", "Wal\u00b7der\u00b7see", "und", "Wran\u00b7gel", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$,", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du siehst die Taten, die dich soviel kosten,", "tokens": ["Du", "siehst", "die", "Ta\u00b7ten", ",", "die", "dich", "so\u00b7viel", "kos\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "PIAT", "ADJA", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Daf\u00fcr im Biograph, im Tingeltangel.", "tokens": ["Da\u00b7f\u00fcr", "im", "Bio\u00b7graph", ",", "im", "Tin\u00b7gel\u00b7tan\u00b7gel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Du siehst die sch\u00f6nen Bilder in der \u00bbWoche\u00ab,", "tokens": ["Du", "siehst", "die", "sch\u00f6\u00b7nen", "Bil\u00b7der", "in", "der", "\u00bb", "Wo\u00b7che", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "$(", "NN", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kann auch die Zeitung keine Taten melden;", "tokens": ["Kann", "auch", "die", "Zei\u00b7tung", "kei\u00b7ne", "Ta\u00b7ten", "mel\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In dieser gro\u00df gearteten Epoche", "tokens": ["In", "die\u00b7ser", "gro\u00df", "ge\u00b7ar\u00b7te\u00b7ten", "E\u00b7po\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Macht nicht die ", "tokens": ["Macht", "nicht", "die"], "token_info": ["word", "word", "word"], "pos": ["NN", "PTKNEG", "ART"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Ja, deutscher Spie\u00df, du wirst zur Zeit bewundert.", "tokens": ["Ja", ",", "deut\u00b7scher", "Spie\u00df", ",", "du", "wirst", "zur", "Zeit", "be\u00b7wun\u00b7dert", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "NN", "$,", "PPER", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von deinem Mute ist man hingerissen,", "tokens": ["Von", "dei\u00b7nem", "Mu\u00b7te", "ist", "man", "hin\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Man r\u00fchmt es noch im kommenden Jahrhundert,", "tokens": ["Man", "r\u00fchmt", "es", "noch", "im", "kom\u00b7men\u00b7den", "Jahr\u00b7hun\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Wie ihr den alten Kr\u00fcger rausgeschmissen.", "tokens": ["Wie", "ihr", "den", "al\u00b7ten", "Kr\u00fc\u00b7ger", "raus\u00b7ge\u00b7schmis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "La\u00df das erhabne Bild uns noch erg\u00e4nzen,", "tokens": ["La\u00df", "das", "er\u00b7hab\u00b7ne", "Bild", "uns", "noch", "er\u00b7g\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es zeigt dir deutlich, was wir jetzt erleben,", "tokens": ["Es", "zeigt", "dir", "deut\u00b7lich", ",", "was", "wir", "jetzt", "er\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "PRELS", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df wir auch innen, nicht blo\u00df au\u00dfen gl\u00e4nzen.", "tokens": ["Da\u00df", "wir", "auch", "in\u00b7nen", ",", "nicht", "blo\u00df", "au\u00b7\u00dfen", "gl\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "$,", "PTKNEG", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es mu\u00df nicht gleich wo solche Lumpen geben.", "tokens": ["Es", "mu\u00df", "nicht", "gleich", "wo", "sol\u00b7che", "Lum\u00b7pen", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "PWAV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Sto\u00df an mit mir und la\u00df dein Glas erklingen,", "tokens": ["Sto\u00df", "an", "mit", "mir", "und", "la\u00df", "dein", "Glas", "er\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "PPER", "KON", "VVIMP", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Jahr ist um; ein Scheidegru\u00df dem alten!", "tokens": ["Das", "Jahr", "ist", "um", ";", "ein", "Schei\u00b7de\u00b7gru\u00df", "dem", "al\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "$.", "ART", "NN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das neue wird uns viele Reden bringen,", "tokens": ["Das", "neu\u00b7e", "wird", "uns", "vie\u00b7le", "Re\u00b7den", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur du, mein Lieber, hast das Maul zu halten.", "tokens": ["Nur", "du", ",", "mein", "Lie\u00b7ber", ",", "hast", "das", "Maul", "zu", "hal\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PPOSAT", "NN", "$,", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}