{"textgrid.poem.58977": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "196. Ein Christ, ein M\u00e4rtyrer", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Seele, willst du leben fromm,", "tokens": ["O", "See\u00b7le", ",", "willst", "du", "le\u00b7ben", "fromm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VMFIN", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Leben wird ein Martertum;", "tokens": ["Dein", "Le\u00b7ben", "wird", "ein", "Mar\u00b7ter\u00b7tum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erwarte nichts als Kreuz und Sterben!", "tokens": ["Er\u00b7war\u00b7te", "nichts", "als", "Kreuz", "und", "Ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KOKOM", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer hier mitstirbt, wird dort miterben.", "tokens": ["Wer", "hier", "mits\u00b7tirbt", ",", "wird", "dort", "mi\u00b7ter\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}