{"textgrid.poem.48790": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "15. Er redet sich selber an", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist es denn nun mehr, da\u00df du so hungrig bist", "tokens": ["Was", "ist", "es", "denn", "nun", "mehr", ",", "da\u00df", "du", "so", "hung\u00b7rig", "bist"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "ADV", "ADV", "$,", "KOUS", "PPER", "ADV", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "viel L\u00e4nder durch zu sehn bei Regen, Frost und Hitze,", "tokens": ["viel", "L\u00e4n\u00b7der", "durch", "zu", "sehn", "bei", "Re\u00b7gen", ",", "Frost", "und", "Hit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PTKZU", "VVINF", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "durch Wildn\u00fc\u00df und durch See zu kommen an die Spitze,", "tokens": ["durch", "Wild\u00b7n\u00fc\u00df", "und", "durch", "See", "zu", "kom\u00b7men", "an", "die", "Spit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "wo Leute, die man ehrt? Ern\u00e4hrst du, was dich fri\u00dft,", "tokens": ["wo", "Leu\u00b7te", ",", "die", "man", "ehrt", "?", "Er\u00b7n\u00e4hrst", "du", ",", "was", "dich", "fri\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "PIS", "VVFIN", "$.", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "die Faulheit dieser Zeit? Vernimst du noch die List", "tokens": ["die", "Faul\u00b7heit", "die\u00b7ser", "Zeit", "?", "Ver\u00b7nimst", "du", "noch", "die", "List"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN", "$.", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "des leichten Wahnes nicht, wirst willig arm vom Witze", "tokens": ["des", "leich\u00b7ten", "Wah\u00b7nes", "nicht", ",", "wirst", "wil\u00b7lig", "arm", "vom", "Wit\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "$,", "VAFIN", "ADJD", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "an Mangel reich zu sein? Bist keinem Stande n\u00fctze?", "tokens": ["an", "Man\u00b7gel", "reich", "zu", "sein", "?", "Bist", "kei\u00b7nem", "Stan\u00b7de", "n\u00fct\u00b7ze", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "PTKZU", "VAINF", "$.", "VAFIN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bleibst allzeit ungeehrt und ewig ungek\u00fc\u00dft?", "tokens": ["Bleibst", "all\u00b7zeit", "un\u00b7geehrt", "und", "e\u00b7wig", "un\u00b7ge\u00b7k\u00fc\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "KON", "ADJD", "ADJD", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "War dieses nun dein Zweck Sophien so zu hassen,", "tokens": ["War", "die\u00b7ses", "nun", "dein", "Zweck", "So\u00b7phi\u00b7en", "so", "zu", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "ADV", "PPOSAT", "NN", "NE", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Olympen Feind zu sein, Hygeen zu verlassen?", "tokens": ["O\u00b7lym\u00b7pen", "Feind", "zu", "sein", ",", "Hy\u00b7ge\u00b7en", "zu", "ver\u00b7las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKZU", "VAINF", "$,", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Tu Rechnung von dir selbst, von dir und deiner Tat!", "tokens": ["Tu", "Rech\u00b7nung", "von", "dir", "selbst", ",", "von", "dir", "und", "dei\u00b7ner", "Tat", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "PPER", "ADV", "$,", "APPR", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch, du bist wider dich. Die Sehnsucht fremder Sachen,", "tokens": ["Doch", ",", "du", "bist", "wi\u00b7der", "dich", ".", "Die", "Sehn\u00b7sucht", "frem\u00b7der", "Sa\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "VAFIN", "APPR", "PPER", "$.", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "was wird sie dermaleins noch endlich aus dir machen,", "tokens": ["was", "wird", "sie", "der\u00b7ma\u00b7leins", "noch", "end\u00b7lich", "aus", "dir", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "weil auch dein eigner Rat bei dir selbst Stat nicht hat?", "tokens": ["weil", "auch", "dein", "eig\u00b7ner", "Rat", "bei", "dir", "selbst", "Stat", "nicht", "hat", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "ADV", "NN", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}