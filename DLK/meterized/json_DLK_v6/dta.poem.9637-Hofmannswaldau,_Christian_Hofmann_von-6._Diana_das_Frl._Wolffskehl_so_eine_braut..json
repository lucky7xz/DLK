{"dta.poem.9637": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "6.  \n  Diana/ das Frl. Wolffskehl/ so eine braut.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich jage was ich kan/ die helden zu bewirthen/", "tokens": ["Ich", "ja\u00b7ge", "was", "ich", "kan", "/", "die", "hel\u00b7den", "zu", "be\u00b7wirt\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "PPER", "VMFIN", "$(", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und werde/ wie ein wild/ vom liebes-gott erjagt.", "tokens": ["Und", "wer\u00b7de", "/", "wie", "ein", "wild", "/", "vom", "lie\u00b7bes\u00b7gott", "er\u00b7jagt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$(", "KOKOM", "ART", "ADJD", "$(", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer kennt Endymion/ den so geschlancken hirten?", "tokens": ["Wer", "kennt", "En\u00b7dy\u00b7mi\u00b7on", "/", "den", "so", "ge\u00b7schlan\u00b7cken", "hir\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "$(", "ART", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der hat mich \u00fcberrascht; wer h\u00e4tt\u2019 es mir gesagt!", "tokens": ["Der", "hat", "mich", "\u00fc\u00b7berr\u00b7ascht", ";", "wer", "h\u00e4tt'", "es", "mir", "ge\u00b7sagt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVPP", "$.", "PWS", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So geht es/ wenn man sich gar zu vermessen wagt:", "tokens": ["So", "geht", "es", "/", "wenn", "man", "sich", "gar", "zu", "ver\u00b7mes\u00b7sen", "wagt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PIS", "PRF", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Man jagt und wird erjagt;", "tokens": ["Man", "jagt", "und", "wird", "er\u00b7jagt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Jtzt such ich/ f\u00fcr den wald/ der Venus liebes-myrthen.", "tokens": ["Jtzt", "such", "ich", "/", "f\u00fcr", "den", "wald", "/", "der", "Ve\u00b7nus", "lie\u00b7bes\u00b7myr\u00b7then", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "APPR", "ART", "NN", "$(", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}