{"dta.poem.139": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Lob des Frauen-Zimmers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Vortreffliches Geschlecht! ihr Sonnen dieser Erden!", "tokens": ["Vor\u00b7treff\u00b7li\u00b7ches", "Ge\u00b7schlecht", "!", "ihr", "Son\u00b7nen", "die\u00b7ser", "Er\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPOSAT", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Sch\u00f6nheit hat an euch ihr Meister-St\u00fcck gemacht/", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "hat", "an", "euch", "ihr", "Meis\u00b7ter\u00b7St\u00fcck", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhr seyd ein Paradies und Engels an Geb\u00e4hrden", "tokens": ["Ihr", "seyd", "ein", "Pa\u00b7ra\u00b7dies", "und", "En\u00b7gels", "an", "Ge\u00b7b\u00e4hr\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Himmel auf der Welt wo Lust und Freude lacht.", "tokens": ["Ein", "Him\u00b7mel", "auf", "der", "Welt", "wo", "Lust", "und", "Freu\u00b7de", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PWAV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn sich das grosse Licht des Firmaments entfernet/", "tokens": ["Wenn", "sich", "das", "gros\u00b7se", "Licht", "des", "Fir\u00b7ma\u00b7ments", "ent\u00b7fer\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und mit beliebten Glantz die Unter-Welt bestrahlt;", "tokens": ["Und", "mit", "be\u00b7lieb\u00b7ten", "Glantz", "die", "Un\u00b7ter\u00b7Welt", "be\u00b7strahlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Als denn man erst von euch und euren Blicken lernet/", "tokens": ["Als", "denn", "man", "erst", "von", "euch", "und", "eu\u00b7ren", "Bli\u00b7cken", "ler\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KON", "PIS", "ADV", "APPR", "PPER", "KON", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das ein gedoppelt Licht an eurer Stirnen prahlt.", "tokens": ["Das", "ein", "ge\u00b7dop\u00b7pelt", "Licht", "an", "eu\u00b7rer", "Stir\u00b7nen", "prahlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "VVPP", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es mu\u00df die grosse Welt mit einer sich begn\u00fcgen/", "tokens": ["Es", "mu\u00df", "die", "gros\u00b7se", "Welt", "mit", "ei\u00b7ner", "sich", "be\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "APPR", "ART", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Zwo Sonnen zieren euch beliebte kleine Welt.", "tokens": ["Zwo", "Son\u00b7nen", "zie\u00b7ren", "euch", "be\u00b7lieb\u00b7te", "klei\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Jhr k\u00f6nnt des Himmels-Pracht an Sch\u00f6nheit \u00fcberwiegen/", "tokens": ["Ihr", "k\u00f6nnt", "des", "Him\u00b7mels\u00b7Pracht", "an", "Sch\u00f6n\u00b7heit", "\u00fc\u00b7berw\u00b7ie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Eur Glantz den Sternen-Schein mehr als die Wage h\u00e4lt.", "tokens": ["Eur", "Glantz", "den", "Ster\u00b7nen\u00b7Schein", "mehr", "als", "die", "Wa\u00b7ge", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "PIAT", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Sch\u00f6nheit/ die euch ziert/ macht euch zu Pracht-G\u00f6ttinnen/", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "/", "die", "euch", "ziert", "/", "macht", "euch", "zu", "Pracht\u00b7G\u00f6t\u00b7tin\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "VVFIN", "$(", "VVFIN", "PPER", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die G\u00f6tter haben euch zu ihren Gott erkl\u00e4hrt;", "tokens": ["Die", "G\u00f6t\u00b7ter", "ha\u00b7ben", "euch", "zu", "ih\u00b7ren", "Gott", "er\u00b7kl\u00e4hrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Jhr k\u00f6nnt durch Lieblichkeit derselben Hertz gewinnen/", "tokens": ["Ihr", "k\u00f6nnt", "durch", "Lieb\u00b7lich\u00b7keit", "der\u00b7sel\u00b7ben", "Hertz", "ge\u00b7win\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Was ists wenn ", "tokens": ["Was", "ists", "wenn"], "token_info": ["word", "word", "word"], "pos": ["PWS", "VAFIN", "KOUS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Euch ist der beste Schatz der Sch\u00f6nheit beygeleget/", "tokens": ["Euch", "ist", "der", "bes\u00b7te", "Schatz", "der", "Sch\u00f6n\u00b7heit", "bey\u00b7ge\u00b7le\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und dieser machet euch recht unvergleichlich sch\u00f6n.", "tokens": ["Und", "die\u00b7ser", "ma\u00b7chet", "euch", "recht", "un\u00b7ver\u00b7gleich\u00b7lich", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Es ist ein feines Bild in eur Gesicht gepr\u00e4get", "tokens": ["Es", "ist", "ein", "fei\u00b7nes", "Bild", "in", "eur", "Ge\u00b7sicht", "ge\u00b7pr\u00e4\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.21": {"text": "Die sch\u00f6nen Haare sind den Netzen zu vergleichen/", "tokens": ["Die", "sch\u00f6\u00b7nen", "Haa\u00b7re", "sind", "den", "Net\u00b7zen", "zu", "ver\u00b7glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Worin so manches Hertz den freyen Stand verliehrt.", "tokens": ["Wo\u00b7rin", "so", "man\u00b7ches", "Hertz", "den", "frey\u00b7en", "Stand", "ver\u00b7liehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Vor euren Stirnen mu\u00df der glatte ", "tokens": ["Vor", "eu\u00b7ren", "Stir\u00b7nen", "mu\u00df", "der", "glat\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Der Himmel ist nicht so/ als dieser Platz/ geziert.", "tokens": ["Der", "Him\u00b7mel", "ist", "nicht", "so", "/", "als", "die\u00b7ser", "Platz", "/", "ge\u00b7ziert", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "$(", "KOUS", "PDAT", "NN", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die Anmuth hat daselbst ihr wunder-sch\u00f6nes Wesen/", "tokens": ["Die", "An\u00b7muth", "hat", "da\u00b7selbst", "ihr", "wun\u00b7der\u00b7sch\u00f6\u00b7nes", "We\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Jhr ist der sch\u00f6ne Ort auf Erb-Pacht eingethan/", "tokens": ["Ihr", "ist", "der", "sch\u00f6\u00b7ne", "Ort", "auf", "Er\u00b7bPacht", "ein\u00b7ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Sie hat recht mit Bedacht die Stirne auserlesen/", "tokens": ["Sie", "hat", "recht", "mit", "Be\u00b7dacht", "die", "Stir\u00b7ne", "au\u00b7ser\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Weil sie die ", "tokens": ["Weil", "sie", "die"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.29": {"text": "Die sch\u00f6nen Augen sind das Brenne-Glas der Liebe/", "tokens": ["Die", "sch\u00f6\u00b7nen", "Au\u00b7gen", "sind", "das", "Bren\u00b7ne\u00b7Glas", "der", "Lie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ein spiegel s\u00fcsser Lust/ und eure beste Zier/", "tokens": ["Ein", "spie\u00b7gel", "s\u00fcs\u00b7ser", "Lust", "/", "und", "eu\u00b7re", "bes\u00b7te", "Zier", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Das Hertze f\u00e4ngt dadurch das Feur der sch\u00f6nen triebe/", "tokens": ["Das", "Hert\u00b7ze", "f\u00e4ngt", "da\u00b7durch", "das", "Feur", "der", "sch\u00f6\u00b7nen", "trie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "PAV", "ART", "NN", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Zwo Sonnen stellen sie mit ihren Strahlen f\u00fcr.", "tokens": ["Zwo", "Son\u00b7nen", "stel\u00b7len", "sie", "mit", "ih\u00b7ren", "Strah\u00b7len", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die M\u00e4nner haben sie vor einen Gott erkl\u00e4hret/", "tokens": ["Die", "M\u00e4n\u00b7ner", "ha\u00b7ben", "sie", "vor", "ei\u00b7nen", "Gott", "er\u00b7kl\u00e4h\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die Hertzen behten sie mehr als die Lippen an/", "tokens": ["Die", "Hert\u00b7zen", "beh\u00b7ten", "sie", "mehr", "als", "die", "Lip\u00b7pen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIAT", "KOKOM", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Mit Ehr-Furcht werden sie als wie ein Gott verehret", "tokens": ["Mit", "Ehr\u00b7Furcht", "wer\u00b7den", "sie", "als", "wie", "ein", "Gott", "ver\u00b7eh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "KOUS", "KOKOM", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Was ihr ", "tokens": ["Was", "ihr"], "token_info": ["word", "word"], "pos": ["PWS", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.37": {"text": "Jhr holder Anblick kan die Seelen fr\u00f6lich machen/", "tokens": ["Ihr", "hol\u00b7der", "An\u00b7blick", "kan", "die", "See\u00b7len", "fr\u00f6\u00b7lich", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die vor Bek\u00fcmmerni\u00df fast halb erstorben sind;", "tokens": ["Die", "vor", "Be\u00b7k\u00fcm\u00b7mer\u00b7ni\u00df", "fast", "halb", "ers\u00b7tor\u00b7ben", "sind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADV", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wenn sie durch einen Blick halb von der Seiten lachen", "tokens": ["Wenn", "sie", "durch", "ei\u00b7nen", "Blick", "halb", "von", "der", "Sei\u00b7ten", "la\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ADJD", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So sp\u00fchrt man eine Gluht die Hertz und Seel entz\u00fcndt.", "tokens": ["So", "sp\u00fchrt", "man", "ei\u00b7ne", "Gluht", "die", "Hertz", "und", "Seel", "ent\u00b7z\u00fcndt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "ART", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Kein Purpur kan die Pracht der Wangen \u00fcbergehen/", "tokens": ["Kein", "Pur\u00b7pur", "kan", "die", "Pracht", "der", "Wan\u00b7gen", "\u00fc\u00b7ber\u00b7ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Kein ", "tokens": ["Kein"], "token_info": ["word"], "pos": ["PIAT"], "meter": "+", "measure": "single.up"}, "line.43": {"text": "Die Rose mu\u00df sich hier als eine Sclavin sehen", "tokens": ["Die", "Ro\u00b7se", "mu\u00df", "sich", "hier", "als", "ei\u00b7ne", "Scla\u00b7vin", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PRF", "ADV", "KOUS", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Der theure Scarlach wird den Rosen bey gesetzt.", "tokens": ["Der", "theu\u00b7re", "Scar\u00b7lach", "wird", "den", "Ro\u00b7sen", "bey", "ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Zinnober hat den Mund/ den sch\u00f6nen Mund umgeben/", "tok