{"textgrid.poem.47290": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Lohn der Freigebigkeit", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unterm Baume stand der Knabe,", "tokens": ["Un\u00b7term", "Bau\u00b7me", "stand", "der", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reichte nicht bis an den Ast,", "tokens": ["Reich\u00b7te", "nicht", "bis", "an", "den", "Ast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bettelte um eine Gabe", "tokens": ["Bet\u00b7tel\u00b7te", "um", "ei\u00b7ne", "Ga\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von der Zweige reichen Last.", "tokens": ["Von", "der", "Zwei\u00b7ge", "rei\u00b7chen", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und der Baum begann zu regen", "tokens": ["Und", "der", "Baum", "be\u00b7gann", "zu", "re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seinen Wipfel leis' im Wind,", "tokens": ["Sei\u00b7nen", "Wip\u00b7fel", "leis'", "im", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00fcttelt einen Apfelregen", "tokens": ["Sch\u00fct\u00b7telt", "ei\u00b7nen", "Ap\u00b7fel\u00b7re\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nieder dem erstaunten Kind.", "tokens": ["Nie\u00b7der", "dem", "er\u00b7staun\u00b7ten", "Kind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was es essen konnte, a\u00df es,", "tokens": ["Was", "es", "es\u00b7sen", "konn\u00b7te", ",", "a\u00df", "es", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "VMFIN", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles essen konnt' es nicht.", "tokens": ["Al\u00b7les", "es\u00b7sen", "konnt'", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber schon so viel besa\u00df es,", "tokens": ["A\u00b7ber", "schon", "so", "viel", "be\u00b7sa\u00df", "es", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ihm noch viel mehr gebricht.", "tokens": ["Da\u00df", "ihm", "noch", "viel", "mehr", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Einen Apfel wirft zum Spiele", "tokens": ["Ei\u00b7nen", "Ap\u00b7fel", "wirft", "zum", "Spie\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Es dem Geber ins Gesicht,", "tokens": ["Es", "dem", "Ge\u00b7ber", "ins", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freut sich, da\u00df er dort vom Stiele", "tokens": ["Freut", "sich", ",", "da\u00df", "er", "dort", "vom", "Stie\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "$,", "KOUS", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen reifen Bruder bricht.", "tokens": ["Ei\u00b7nen", "rei\u00b7fen", "Bru\u00b7der", "bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und so viel' als niederfallen,", "tokens": ["Und", "so", "viel'", "als", "nie\u00b7der\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "KOKOM", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schleudert er hinauf und treibt", "tokens": ["Schleu\u00b7dert", "er", "hin\u00b7auf", "und", "treibt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Es so lange, bis von allen", "tokens": ["Es", "so", "lan\u00b7ge", ",", "bis", "von", "al\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "$,", "KOUS", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fr\u00fcchten keine droben bleibt.", "tokens": ["Fr\u00fcch\u00b7ten", "kei\u00b7ne", "dro\u00b7ben", "bleibt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Was der kahle Baum nun denket?", "tokens": ["Was", "der", "kah\u00b7le", "Baum", "nun", "den\u00b7ket", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Z\u00fcrnend wieget er das Haupt:", "tokens": ["Z\u00fcr\u00b7nend", "wie\u00b7get", "er", "das", "Haupt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbweil ich dir zu viel geschenket,", "tokens": ["\u00bb", "weil", "ich", "dir", "zu", "viel", "ge\u00b7schen\u00b7ket", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PPER", "APPR", "PIS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hast du alles mir geraubt.\u00ab", "tokens": ["Hast", "du", "al\u00b7les", "mir", "ge\u00b7raubt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "PIS", "PPER", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}