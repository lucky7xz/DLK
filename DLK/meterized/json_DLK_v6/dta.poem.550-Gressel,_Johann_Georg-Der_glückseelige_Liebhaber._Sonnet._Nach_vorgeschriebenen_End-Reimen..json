{"dta.poem.550": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der gl\u00fcckseelige Liebhaber.  \n Sonnet.  \n Nach vorgeschriebenen End-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mit s\u00fcsser Anmuth bricht des Himmels-Strahl - - herein/", "tokens": ["Mit", "s\u00fcs\u00b7ser", "An\u00b7muth", "bricht", "des", "Him\u00b7mels\u00b7Strahl", "her\u00b7ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$(", "$(", "PTKVZ", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Es freun sich meiner Lust die H\u00f6hen und die - - Gr\u00fcffte/", "tokens": ["Es", "freun", "sich", "mei\u00b7ner", "Lust", "die", "H\u00f6\u00b7hen", "und", "die", "Gr\u00fcff\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PPOSAT", "NN", "ART", "NN", "KON", "ART", "$(", "$(", "NN", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Sie reichen Balsam her vor Wolffes-Milch und - - Giffte/", "tokens": ["Sie", "rei\u00b7chen", "Bal\u00b7sam", "her", "vor", "Wolf\u00b7fes\u00b7Milch", "und", "Giff\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APZR", "APPR", "NN", "KON", "$(", "$(", "NN", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Der Liebsten Zucker-Ku\u00df/ vertilgt nun alle - - Pein", "tokens": ["Der", "Liebs\u00b7ten", "Zu\u00b7cke\u00b7rKu\u00df", "/", "ver\u00b7tilgt", "nun", "al\u00b7le", "Pein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "VVFIN", "ADV", "PIS", "$(", "$(", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es streuen Wollust aus die Seegen-schwangre - - L\u00fcffte", "tokens": ["Es", "streu\u00b7en", "Wol\u00b7lust", "aus", "die", "See\u00b7gen\u00b7schwan\u00b7gre", "L\u00fcff\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ART", "NN", "$(", "$(", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und ein beliebter Schoo\u00df er\u00f6ffnet seine - - Kl\u00fcffte", "tokens": ["Und", "ein", "be\u00b7lieb\u00b7ter", "Schoo\u00df", "er\u00b7\u00f6ff\u00b7net", "sei\u00b7ne", "Kl\u00fcff\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "$(", "$(", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Lust-Entz\u00fcckung l\u00e4st den Geist erstorben - - seyn.", "tokens": ["Die", "Lust\u00b7Ent\u00b7z\u00fc\u00b7ckung", "l\u00e4st", "den", "Geist", "ers\u00b7tor\u00b7ben", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVPP", "$(", "$(", "VAINF", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.9": {"text": "Doch es ermuntern sich die halb erstorbnen - - Glieder/", "tokens": ["Doch", "es", "er\u00b7mun\u00b7tern", "sich", "die", "halb", "ers\u00b7torb\u00b7nen", "Glie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ART", "ADJD", "ADJA", "$(", "$(", "NN", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "Und legen Furcht und Angst des s\u00fcssen Todes - - nieder/", "tokens": ["Und", "le\u00b7gen", "Furcht", "und", "Angst", "des", "s\u00fcs\u00b7sen", "To\u00b7des", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "ART", "ADJA", "NN", "$(", "$(", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.11": {"text": "Sie w\u00fcnschen/ da\u00df sie offt der Liebes-Krey\u00df - - umringt/", "tokens": ["Sie", "w\u00fcn\u00b7schen", "/", "da\u00df", "sie", "offt", "der", "Lie\u00b7bes\u00b7Krey\u00df", "um\u00b7ringt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "ADV", "ART", "NN", "$(", "$(", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der in der Liebes-Gluth den Geist zum Sterben - - zwingt.", "tokens": ["Der", "in", "der", "Lie\u00b7bes\u00b7Gluth", "den", "Geist", "zum", "Ster\u00b7ben", "zwingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "APPRART", "NN", "$(", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.13": {"text": "Ich nehre in der Brust mit Lust die Liebes - - Flammen/", "tokens": ["Ich", "neh\u00b7re", "in", "der", "Brust", "mit", "Lust", "die", "Lie\u00b7bes", "Flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "NN", "ART", "NN", "$(", "$(", "NN", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "Und will der Liebe Thun aus Thorheit nicht - - verdammen.", "tokens": ["Und", "will", "der", "Lie\u00b7be", "Thun", "aus", "Thor\u00b7heit", "nicht", "ver\u00b7dam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "NN", "APPR", "NN", "PTKNEG", "$(", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}