{"textgrid.poem.36382": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "11. Christus ein Hirt", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Himmels Bot' von oben", "tokens": ["Des", "Him\u00b7mels", "Bot'", "von", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch Luft und Wolken drang,", "tokens": ["Durch", "Luft", "und", "Wol\u00b7ken", "drang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und freudig, unverschoben,", "tokens": ["Und", "freu\u00b7dig", ",", "un\u00b7ver\u00b7scho\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So zu den Hirten sang:", "tokens": ["So", "zu", "den", "Hir\u00b7ten", "sang", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Auf, auf, nun anzubeten", "tokens": ["Auf", ",", "auf", ",", "nun", "an\u00b7zu\u00b7be\u00b7ten"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "$,", "PTKVZ", "$,", "ADV", "VVIZU"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das golden sch\u00f6ne Kind;", "tokens": ["Das", "gol\u00b7den", "sch\u00f6\u00b7ne", "Kind", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf, auf, zur Hirtenmetten,", "tokens": ["Auf", ",", "auf", ",", "zur", "Hir\u00b7ten\u00b7met\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$,", "PTKVZ", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du frommes Feldgesind.", "tokens": ["Du", "from\u00b7mes", "Feld\u00b7ge\u00b7sind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auf, eilend auf zur Krippen,", "tokens": ["Auf", ",", "ei\u00b7lend", "auf", "zur", "Krip\u00b7pen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "VVPP", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum kleinen Sch\u00e4ferlein,", "tokens": ["Zum", "klei\u00b7nen", "Sch\u00e4\u00b7fer\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "K\u00fc\u00dft ihm die Purpurlippen,", "tokens": ["K\u00fc\u00dft", "ihm", "die", "Pur\u00b7pur\u00b7lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Purpurm\u00fcndelein.", "tokens": ["Das", "Pur\u00b7pur\u00b7m\u00fcn\u00b7del\u00b7ein", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "K\u00fc\u00dft ihm die Rosenwangen,", "tokens": ["K\u00fc\u00dft", "ihm", "die", "Ro\u00b7sen\u00b7wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Winterbl\u00fcmelein,", "tokens": ["Die", "Win\u00b7ter\u00b7bl\u00fc\u00b7me\u00b7lein", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So trotz dem Fr\u00fchling prangen,", "tokens": ["So", "trotz", "dem", "Fr\u00fch\u00b7ling", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mag es gleich Winter sein.", "tokens": ["Mag", "es", "gleich", "Win\u00b7ter", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "NN", "VAINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Das Kindlein halb erfroren,", "tokens": ["Das", "Kin\u00b7dlein", "halb", "er\u00b7fro\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch auch nicht minder brinnt,", "tokens": ["Doch", "auch", "nicht", "min\u00b7der", "brinnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im herben Frost geboren", "tokens": ["Im", "her\u00b7ben", "Frost", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es Feu'r im Busen find't.", "tokens": ["Es", "Feu'r", "im", "Bu\u00b7sen", "find'", "t."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "NN", "APPRART", "NN", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Lind hebet's nur in Armen,", "tokens": ["Lind", "he\u00b7bet's", "nur", "in", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Und preist es mit Verstand:", "tokens": ["Und", "preist", "es", "mit", "Ver\u00b7stand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bald wird es euch erwarmen", "tokens": ["Bald", "wird", "es", "euch", "er\u00b7war\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit s\u00fc\u00dfem Herzensbrand.", "tokens": ["Mit", "s\u00fc\u00b7\u00dfem", "Her\u00b7zens\u00b7brand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es liebet Schaf' und Hirten", "tokens": ["Es", "lie\u00b7bet", "Schaf'", "und", "Hir\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Hirtenkindelein,", "tokens": ["Das", "Hir\u00b7ten\u00b7kin\u00b7de\u00b7lein", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es leitet her von Hirten", "tokens": ["Es", "lei\u00b7tet", "her", "von", "Hir\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Stand und Stammen sein.", "tokens": ["Den", "Stand", "und", "Stam\u00b7men", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ein L\u00e4mmlein auch ohn' Flecken", "tokens": ["Ein", "L\u00e4mm\u00b7lein", "auch", "ohn'", "Fle\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es f\u00fchrt in seinem Schild,", "tokens": ["Es", "f\u00fchrt", "in", "sei\u00b7nem", "Schild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Samt einem Hirtenstecken,", "tokens": ["Samt", "ei\u00b7nem", "Hir\u00b7ten\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gar zierlich abgebild't.", "tokens": ["Gar", "zier\u00b7lich", "ab\u00b7ge\u00b7bild'", "t."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["ADV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O wohl dem sch\u00f6nen Hirten,", "tokens": ["O", "wohl", "dem", "sch\u00f6\u00b7nen", "Hir\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem k\u00fcnft'gen Hirten gut,", "tokens": ["Dem", "k\u00fcnft'\u00b7gen", "Hir\u00b7ten", "gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach, ach, mich in Begierden", "tokens": ["Ach", ",", "ach", ",", "mich", "in", "Be\u00b7gier\u00b7den"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ITJ", "$,", "PRF", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Zeit verlangen tut.", "tokens": ["Der", "Zeit", "ver\u00b7lan\u00b7gen", "tut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Alsdann er wird erwecken", "tokens": ["Als\u00b7dann", "er", "wird", "er\u00b7we\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und treiben auf zu Feld,", "tokens": ["Und", "trei\u00b7ben", "auf", "zu", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit sanftem Hirtenstecken", "tokens": ["Mit", "sanf\u00b7tem", "Hir\u00b7ten\u00b7ste\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die V\u00f6lker aller Welt.", "tokens": ["Die", "V\u00f6l\u00b7ker", "al\u00b7ler", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ja er wird seinen Stecken", "tokens": ["Ja", "er", "wird", "sei\u00b7nen", "Ste\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "PPER", "VAFIN", "PPOSAT", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Den Sonnenstrahlen gleich,", "tokens": ["Den", "Son\u00b7nen\u00b7strah\u00b7len", "gleich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "All \u00fcberall erstrecken", "tokens": ["All", "\u00fc\u00b7be\u00b7rall", "er\u00b7stre\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In alle Land' und Reich.", "tokens": ["In", "al\u00b7le", "Land'", "und", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Alsdann mit sch\u00f6nem Frieden", "tokens": ["Als\u00b7dann", "mit", "sch\u00f6\u00b7nem", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die sch\u00f6ne Welt gekr\u00f6nt,", "tokens": ["Die", "sch\u00f6\u00b7ne", "Welt", "ge\u00b7kr\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wird sehn ununterschieden", "tokens": ["Wird", "sehn", "un\u00b7un\u00b7ter\u00b7schie\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Tiere all' vers\u00f6hnt.", "tokens": ["Die", "Tie\u00b7re", "all'", "ver\u00b7s\u00f6hnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Mit wilden L\u00f6w' und B\u00e4ren", "tokens": ["Mit", "wil\u00b7den", "L\u00f6\u00b7w'", "und", "B\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gleich werden in gemein,", "tokens": ["Gleich", "wer\u00b7den", "in", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus einer Krippe zehren", "tokens": ["Aus", "ei\u00b7ner", "Krip\u00b7pe", "zeh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die zarten L\u00e4mmerlein.", "tokens": ["Die", "zar\u00b7ten", "L\u00e4m\u00b7mer\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Auf einem Grund und Wasen,", "tokens": ["Auf", "ei\u00b7nem", "Grund", "und", "Wa\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zur sch\u00f6nen Sommerbl\u00fch,", "tokens": ["Zur", "sch\u00f6\u00b7nen", "Som\u00b7mer\u00b7bl\u00fch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit W\u00f6lfen werden grasen", "tokens": ["Mit", "W\u00f6l\u00b7fen", "wer\u00b7den", "gra\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Kinder, Schaf' und K\u00fch'.", "tokens": ["Die", "Kin\u00b7der", ",", "Schaf'", "und", "K\u00fch'", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Es wird an Tann' und Linden,", "tokens": ["Es", "wird", "an", "Tann'", "und", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An Buch'- und Eschenlaub,", "tokens": ["An", "Buch'", "und", "E\u00b7schen\u00b7laub", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sich h\u00e4ufig lassen finden", "tokens": ["Sich", "h\u00e4u\u00b7fig", "las\u00b7sen", "fin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "ADJD", "VVINF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohl manche s\u00fc\u00dfe Traub'.", "tokens": ["Wohl", "man\u00b7che", "s\u00fc\u00b7\u00dfe", "Traub'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Es wird von Eichenb\u00e4umen", "tokens": ["Es", "wird", "von", "Ei\u00b7chen\u00b7b\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gepre\u00dft der Honig lind,", "tokens": ["Ge\u00b7pre\u00dft", "der", "Ho\u00b7nig", "lind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wie sich's kaum l\u00e4\u00dft tr\u00e4umen,", "tokens": ["Und", "wie", "sich's", "kaum", "l\u00e4\u00dft", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "ADV", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das \u00d6l vom Felsen rinnt.", "tokens": ["Das", "\u00d6l", "vom", "Fel\u00b7sen", "rinnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Erd', Himmel wird sich wenden,", "tokens": ["Erd'", ",", "Him\u00b7mel", "wird", "sich", "wen\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", 