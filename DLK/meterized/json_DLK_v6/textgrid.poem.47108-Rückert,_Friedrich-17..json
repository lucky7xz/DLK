{"textgrid.poem.47108": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gestern sprach der Mond zu mir,", "tokens": ["Ge\u00b7stern", "sprach", "der", "Mond", "zu", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Als ich von der Liebsten ging,", "tokens": ["Als", "ich", "von", "der", "Liebs\u00b7ten", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Wie er hell in stiller Zier", "tokens": ["Wie", "er", "hell", "in", "stil\u00b7ler", "Zier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcber dunklen Wolken hing:", "tokens": ["\u00dc\u00b7ber", "dunk\u00b7len", "Wol\u00b7ken", "hing", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hat der Freund so manches Mal", "tokens": ["Hat", "der", "Freund", "so", "man\u00b7ches", "Mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADV", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sonst doch nach mir aufgeschaut,", "tokens": ["Sonst", "doch", "nach", "mir", "auf\u00b7ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es hat mein feuchter Strahl", "tokens": ["Und", "es", "hat", "mein", "feuch\u00b7ter", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wehmut ihm ins Herz getaut.", "tokens": ["Weh\u00b7mut", "ihm", "ins", "Herz", "ge\u00b7taut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Bin ich dir nicht mehr vertraut?", "tokens": ["Bin", "ich", "dir", "nicht", "mehr", "ver\u00b7traut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Blickst du nicht nach mir einmal?", "tokens": ["Blickst", "du", "nicht", "nach", "mir", "ein\u00b7mal", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In Gedanken deine Braut,", "tokens": ["In", "Ge\u00b7dan\u00b7ken", "dei\u00b7ne", "Braut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Merkst du gar nicht meinen Strahl.", "tokens": ["Merkst", "du", "gar", "nicht", "mei\u00b7nen", "Strahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Streu' ich doch auf deinen Weg", "tokens": ["Streu'", "ich", "doch", "auf", "dei\u00b7nen", "Weg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine sch\u00f6nsten Schimmer gern;", "tokens": ["Mei\u00b7ne", "sch\u00f6ns\u00b7ten", "Schim\u00b7mer", "gern", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dir zu zeigen Weg und Steg,", "tokens": ["Dir", "zu", "zei\u00b7gen", "Weg", "und", "Steg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eifr' ich mit dem Abendstern.", "tokens": ["Eifr'", "ich", "mit", "dem", "A\u00b7bends\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Himmel schaut in deine Lust,", "tokens": ["Him\u00b7mel", "schaut", "in", "dei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Teilst du gleich sie nicht ihm mit;", "tokens": ["Teilst", "du", "gleich", "sie", "nicht", "ihm", "mit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "PTKNEG", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es lenken unbewu\u00dft", "tokens": ["Und", "es", "len\u00b7ken", "un\u00b7be\u00b7wu\u00dft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVINF", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine Lichter deinen Schritt.", "tokens": ["Sei\u00b7ne", "Lich\u00b7ter", "dei\u00b7nen", "Schritt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "In der Morgensonne Glanz", "tokens": ["In", "der", "Mor\u00b7gen\u00b7son\u00b7ne", "Glanz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gingest heut zu deinem Gl\u00fcck;", "tokens": ["Gin\u00b7gest", "heut", "zu", "dei\u00b7nem", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Nacht im Sternenkranz", "tokens": ["Und", "die", "Nacht", "im", "Ster\u00b7nen\u00b7kranz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "F\u00fchrt im Dunkel dich zur\u00fcck.", "tokens": ["F\u00fchrt", "im", "Dun\u00b7kel", "dich", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Mond und Sonne siehst du nicht,", "tokens": ["Mond", "und", "Son\u00b7ne", "siehst", "du", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch dich sehen Sonn' und Mond", "tokens": ["Doch", "dich", "se\u00b7hen", "Sonn'", "und", "Mond"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "NN", "KON", "NN"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Und erquicken sich am Licht,", "tokens": ["Und", "er\u00b7qui\u00b7cken", "sich", "am", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Das in deinem Herzen wohnt.", "tokens": ["Das", "in", "dei\u00b7nem", "Her\u00b7zen", "wohnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Schau nun doch mich an einmal,", "tokens": ["Schau", "nun", "doch", "mich", "an", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "PRF", "APPR", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Birg es meinen Blicken nicht,", "tokens": ["Birg", "es", "mei\u00b7nen", "Bli\u00b7cken", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie der Liebe Gottesstrahl", "tokens": ["Wie", "der", "Lie\u00b7be", "Got\u00b7tes\u00b7strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kl\u00e4rt ein Menschenangesicht!", "tokens": ["Kl\u00e4rt", "ein", "Men\u00b7schen\u00b7an\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}