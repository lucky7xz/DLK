{"dta.poem.20451": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Gedancken \u00fcber die eitelkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist di\u00df thun/ so dein gem\u00fcthe liebet?", "tokens": ["Was", "ist", "di\u00df", "thun", "/", "so", "dein", "ge\u00b7m\u00fc\u00b7the", "lie\u00b7bet", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "VVINF", "$(", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem sich die lieb\u2019 so leichtlich \u00fcbergiebet?", "tokens": ["Dem", "sich", "die", "lieb'", "so", "leicht\u00b7lich", "\u00fc\u00b7berg\u00b7ie\u00b7bet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ART", "VVFIN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein weisser koth/ der farb und masqve tr\u00e4gt/", "tokens": ["Ein", "weis\u00b7ser", "koth", "/", "der", "farb", "und", "mas\u00b7qve", "tr\u00e4gt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den ieder wind der eitelkeit bewegt.", "tokens": ["Den", "ie\u00b7der", "wind", "der", "ei\u00b7tel\u00b7keit", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den mehr dein gold/ als deine bitte lencket/", "tokens": ["Den", "mehr", "dein", "gold", "/", "als", "dei\u00b7ne", "bit\u00b7te", "len\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "$(", "KOKOM", "PPOSAT", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der sinnen koth um andern koth verschencket.", "tokens": ["Der", "sin\u00b7nen", "koth", "um", "an\u00b7dern", "koth", "ver\u00b7schen\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die stirne/ so dein freuden-spiegel ist/", "tokens": ["Die", "stir\u00b7ne", "/", "so", "dein", "freu\u00b7den\u00b7spie\u00b7gel", "ist", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "ADV", "PPOSAT", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Bleibt schl\u00fcpffrig ei\u00df; und so du das erkiest/", "tokens": ["Bleibt", "schl\u00fcpf\u00b7frig", "ei\u00df", ";", "und", "so", "du", "das", "er\u00b7kiest", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "$.", "KON", "ADV", "PPER", "PDS", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So wird dein fu\u00df mit deiner freyheit fallen.", "tokens": ["So", "wird", "dein", "fu\u00df", "mit", "dei\u00b7ner", "frey\u00b7heit", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "PTKVZ", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die stimme/ so du st\u00fcndlich h\u00f6r\u2019st erschallen/", "tokens": ["Die", "stim\u00b7me", "/", "so", "du", "st\u00fcnd\u00b7lich", "h\u00f6r'st", "er\u00b7schal\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "ADV", "PPER", "ADJD", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ist dieser gleich/ so die Syrene bringt/", "tokens": ["Ist", "die\u00b7ser", "gleich", "/", "so", "die", "Sy\u00b7re\u00b7ne", "bringt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "$(", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Und t\u00f6dten kan/ wie s\u00fcsse sie auch klingt.", "tokens": ["Und", "t\u00f6d\u00b7ten", "kan", "/", "wie", "s\u00fcs\u00b7se", "sie", "auch", "klingt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "$(", "PWAV", "VVFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Die k\u00fcsse/ so du von den lippen stiehlest/", "tokens": ["Die", "k\u00fcs\u00b7se", "/", "so", "du", "von", "den", "lip\u00b7pen", "stieh\u00b7lest", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Die stehlen dich/ wie wenig du es f\u00fchlest;", "tokens": ["Die", "steh\u00b7len", "dich", "/", "wie", "we\u00b7nig", "du", "es", "f\u00fch\u00b7lest", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$(", "PWAV", "PIS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Ja lieb\u2019 ist wohl mehr knecht/ als frey zu seyn.", "tokens": ["Ja", "lieb'", "ist", "wohl", "mehr", "knecht", "/", "als", "frey", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "VAFIN", "ADV", "PIAT", "NN", "$(", "KOUS", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Der wangen pracht/ der klaren augen schein/", "tokens": ["Der", "wan\u00b7gen", "pracht", "/", "der", "kla\u00b7ren", "au\u00b7gen", "schein", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Wird itzt ein plitz/ der dich zu boden schl\u00e4get/", "tokens": ["Wird", "itzt", "ein", "plitz", "/", "der", "dich", "zu", "bo\u00b7den", "schl\u00e4\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$(", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und mehr als lod ins grab der wollust tr\u00e4get.", "tokens": ["Und", "mehr", "als", "lod", "ins", "grab", "der", "wol\u00b7lust", "tr\u00e4\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "VVFIN", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Wie sehr dein mund die stoltze brust verehrt/", "tokens": ["Wie", "sehr", "dein", "mund", "die", "stolt\u00b7ze", "brust", "ver\u00b7ehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Und leichten schein mit leichten worten nehrt/", "tokens": ["Und", "leich\u00b7ten", "schein", "mit", "leich\u00b7ten", "wor\u00b7ten", "nehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "So wird dir doch noch die erfahrung sagen:", "tokens": ["So", "wird", "dir", "doch", "noch", "die", "er\u00b7fah\u00b7rung", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Wie diese brust hat b\u00f6se brunst getragen.", "tokens": ["Wie", "die\u00b7se", "brust", "hat", "b\u00f6\u00b7se", "brunst", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VVFIN", "VAFIN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Und solcher schmuck/ wie sch\u00f6ne er auch steh\u2019t/", "tokens": ["Und", "sol\u00b7cher", "schmuck", "/", "wie", "sch\u00f6\u00b7ne", "er", "auch", "steh't", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$(", "PWAV", "VVFIN", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Mit faulen hin zu ihrem grabe geht.", "tokens": ["Mit", "fau\u00b7len", "hin", "zu", "ih\u00b7rem", "gra\u00b7be", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Drum richte leib und seele wieder auff/", "tokens": ["Drum", "rich\u00b7te", "leib", "und", "see\u00b7le", "wie\u00b7der", "auff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NN", "KON", "VVFIN", "ADV", "APPR", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "La\u00df der vernunfft den vormahls freyen lauff;", "tokens": ["La\u00df", "der", "ver\u00b7nunfft", "den", "vor\u00b7mahls", "frey\u00b7en", "lauff", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Halt an das fleisch/ erl\u00e4utere die sinnen/", "tokens": ["Halt", "an", "das", "fleisch", "/", "er\u00b7l\u00e4u\u00b7te\u00b7re", "die", "sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "NN", "$(", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "So wirst du rein und wei\u00dflich leben k\u00f6nnen.", "tokens": ["So", "wirst", "du", "rein", "und", "wei\u00df\u00b7lich", "le\u00b7ben", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}