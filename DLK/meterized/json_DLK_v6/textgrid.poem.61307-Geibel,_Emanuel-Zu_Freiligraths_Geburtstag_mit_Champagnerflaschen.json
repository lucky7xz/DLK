{"textgrid.poem.61307": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Zu Freiligraths Geburtstag mit Champagnerflaschen", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Frankreichs H\u00f6hn, die sonnenklar", "tokens": ["Von", "Fran\u00b7kreichs", "H\u00f6hn", ",", "die", "son\u00b7nen\u00b7klar"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "NE", "$,", "PRELS", "XY"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von goldnem Segen triefen,", "tokens": ["Von", "gold\u00b7nem", "Se\u00b7gen", "trie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da bringen wir dir Nektar dar", "tokens": ["Da", "brin\u00b7gen", "wir", "dir", "Nek\u00b7tar", "dar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "NN", "PTKVZ"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "F\u00fcr deinen Hippogryphen;", "tokens": ["F\u00fcr", "dei\u00b7nen", "Hip\u00b7po\u00b7gry\u00b7phen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "F\u00fcr ihn, der sich so stolz geb\u00e4umt", "tokens": ["F\u00fcr", "ihn", ",", "der", "sich", "so", "stolz", "ge\u00b7b\u00e4umt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "PRELS", "PRF", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Euphrat und am Nile,", "tokens": ["Am", "Eu\u00b7ph\u00b7rat", "und", "am", "Ni\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "KON", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und den du jetzt auf deutsch gez\u00e4umt", "tokens": ["Und", "den", "du", "jetzt", "auf", "deutsch", "ge\u00b7z\u00e4umt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PPER", "ADV", "APPR", "ADJD", "VVPP"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Zu sch\u00f6nerm Ritterspiele.", "tokens": ["Zu", "sch\u00f6\u00b7nerm", "Rit\u00b7ter\u00b7spie\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hoch auf! Er scharret mit Gewiehr", "tokens": ["Hoch", "auf", "!", "Er", "schar\u00b7ret", "mit", "Ge\u00b7wiehr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "$.", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und knirscht in Kett' und Stange", "tokens": ["Und", "knirscht", "in", "Kett'", "und", "Stan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und stampft, als wollt' er sagen dir:", "tokens": ["Und", "stampft", ",", "als", "wollt'", "er", "sa\u00b7gen", "dir", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "VMFIN", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbwas rastest du so lange?\u00ab", "tokens": ["\u00bb", "was", "ras\u00b7test", "du", "so", "lan\u00b7ge", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein frischer Reiter bist ja du,", "tokens": ["Ein", "fri\u00b7scher", "Rei\u00b7ter", "bist", "ja", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum la\u00df dein Tier nicht warten;", "tokens": ["Drum", "la\u00df", "dein", "Tier", "nicht", "war\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sitz auf und reit dem Meere zu", "tokens": ["Sitz", "auf", "und", "reit", "dem", "Mee\u00b7re", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "KON", "NN", "ART", "NN", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch deines Rheinlands Garten.", "tokens": ["Durch", "dei\u00b7nes", "Rhein\u00b7lands", "Gar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und wenn der Huf vom Fl\u00fcgelhengst", "tokens": ["Und", "wenn", "der", "Huf", "vom", "Fl\u00fc\u00b7gel\u00b7hengst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erklingt im Land der Schleusen,", "tokens": ["Er\u00b7klingt", "im", "Land", "der", "Schleu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann r\u00fchren, die da schliefen l\u00e4ngst,", "tokens": ["Dann", "r\u00fch\u00b7ren", ",", "die", "da", "schlie\u00b7fen", "l\u00e4ngst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "PRELS", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Grabe sich die Geusen.", "tokens": ["Im", "Gra\u00b7be", "sich", "die", "Geu\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie steigen auf, eine wilde Schar,", "tokens": ["Sie", "stei\u00b7gen", "auf", ",", "ei\u00b7ne", "wil\u00b7de", "Schar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Kleid von d\u00fcstrer Farbe,", "tokens": ["Im", "Kleid", "von", "d\u00fcst\u00b7rer", "Far\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit langem Schwert und kurzem Haar", "tokens": ["Mit", "lan\u00b7gem", "Schwert", "und", "kur\u00b7zem", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und auf der Stirn die Narbe.", "tokens": ["Und", "auf", "der", "Stirn", "die", "Nar\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und einer spricht: \u00bbHalt an, Gesell!", "tokens": ["Und", "ei\u00b7ner", "spricht", ":", "\u00bb", "Halt", "an", ",", "Ge\u00b7sell", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$.", "$(", "VVIMP", "PTKVZ", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du riefst, und wir erwachten;", "tokens": ["Du", "riefst", ",", "und", "wir", "er\u00b7wach\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Spiel' auf, spiel' auf! Wir folgen schnell", "tokens": ["Spiel'", "auf", ",", "spiel'", "auf", "!", "Wir", "fol\u00b7gen", "schnell"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Zechgelag und Schlachten.", "tokens": ["Zu", "Zech\u00b7ge\u00b7lag", "und", "Schlach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Hoch flattert unsrer Masten Zier,", "tokens": ["Hoch", "flat\u00b7tert", "uns\u00b7rer", "Mas\u00b7ten", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Banner von Oranien;", "tokens": ["Das", "Ban\u00b7ner", "von", "O\u00b7ra\u00b7ni\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie gerne trutzen wir mit dir", "tokens": ["Wie", "ger\u00b7ne", "trut\u00b7zen", "wir", "mit", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem finstern Mann in Spanien.", "tokens": ["Dem", "fins\u00b7tern", "Mann", "in", "Spa\u00b7ni\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wie gerne stehn wir Glied an Glied", "tokens": ["Wie", "ger\u00b7ne", "stehn", "wir", "Glied", "an", "Glied"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit dir zum andern Male,", "tokens": ["Mit", "dir", "zum", "an\u00b7dern", "Ma\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df unser Sieg in einem Lied", "tokens": ["Da\u00df", "un\u00b7ser", "Sieg", "in", "ei\u00b7nem", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aufs neu' verherrlicht strahle.", "tokens": ["Aufs", "neu'", "ver\u00b7herr\u00b7licht", "strah\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Frisch! Weck' die Saiten aus der Ruh'!", "tokens": ["Frisch", "!", "Weck'", "die", "Sai\u00b7ten", "aus", "der", "Ruh'", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Greif ein mit keckem Finger!", "tokens": ["Greif", "ein", "mit", "ke\u00b7ckem", "Fin\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Wir hoffen Gro\u00dfes. \u2013 L\u00e4ssest du", "tokens": ["Wir", "hof\u00b7fen", "Gro\u00b7\u00dfes", ".", "\u2013", "L\u00e4s\u00b7sest", "du"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$.", "$(", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns harren, k\u00fchner Singer?\u00ab", "tokens": ["Uns", "har\u00b7ren", ",", "k\u00fch\u00b7ner", "Sin\u00b7ger", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVINF", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Doch willst du nicht ins Niederland,", "tokens": ["Doch", "willst", "du", "nicht", "ins", "Nie\u00b7der\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So reit ins Land Westfalen;", "tokens": ["So", "reit", "ins", "Land", "West\u00b7fa\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von alters her ist's dir bekannt,", "tokens": ["Von", "al\u00b7ters", "her", "ist's", "dir", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APZR", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du magst es pr\u00e4chtig malen.", "tokens": ["Du", "magst", "es", "pr\u00e4ch\u00b7tig", "ma\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Die Heide braun, den Eichengrund,", "tokens": ["Die", "Hei\u00b7de", "braun", ",", "den", "Ei\u00b7chen\u00b7grund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den stillen Hof dazwischen,", "tokens": ["Den", "stil\u00b7len", "Hof", "da\u00b7zwi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Weidgeselln mit Horn und Hund,", "tokens": ["Den", "Weid\u00b7ge\u00b7selln", "mit", "Horn", "und", "Hund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Damhirsch in den B\u00fcschen.", "tokens": ["Den", "Dam\u00b7hirsch", "in", "den", "B\u00fc\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Den gr\u00fcnsten Waldplatz such' dir dort,", "tokens": ["Den", "gr\u00fcns\u00b7ten", "Wald\u00b7platz", "such'", "dir", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um auszuruhn vom Ritte;", "tokens": ["Um", "aus\u00b7zu\u00b7ruhn", "vom", "Rit\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "VVIZU", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bemooste Stein' umstehn den Ort,", "tokens": ["Be\u00b7moos\u00b7te", "Stein'", "um\u00b7stehn", "den", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fern lugt die K\u00f6hlerh\u00fctte.", "tokens": ["Fern", "lugt", "die", "K\u00f6h\u00b7ler\u00b7h\u00fct\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Der Meiler gl\u00fcht. Es ballt der Rauch", "tokens": ["Der", "Mei\u00b7ler", "gl\u00fcht", ".", "Es", "ballt", "der", "Rauch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich m\u00e4hlich zu Gestalten;", "tokens": ["Sich", "m\u00e4h\u00b7lich", "zu", "Ge\u00b7stal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "D\u00fcster wehen im Windeshauch", "tokens": ["D\u00fcs\u00b7ter", "we\u00b7hen", "im", "Win\u00b7des\u00b7hauch"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Der langen Gew\u00e4nder Falten.", "tokens": ["Der", "lan\u00b7gen", "Ge\u00b7w\u00e4n\u00b7der", "Fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.15": {"line.1": {"text": "Sie schweben zum Freigrafenstein,", "tokens": ["Sie", "schwe\u00b7ben", "zum", "Frei\u00b7gra\u00b7fen\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": 