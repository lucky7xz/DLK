{"textgrid.poem.41007": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "An eine zur Weihnachtszeit geborene Freundin", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eh' der Heiland uns erschienen,", "tokens": ["Eh'", "der", "Hei\u00b7land", "uns", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwebten Engel erdenw\u00e4rts,", "tokens": ["Schweb\u00b7ten", "En\u00b7gel", "er\u00b7den\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "An der Krippe ihm zu dienen,", "tokens": ["An", "der", "Krip\u00b7pe", "ihm", "zu", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lindernd ihm der Erde Schmerz.", "tokens": ["Lin\u00b7dernd", "ihm", "der", "Er\u00b7de", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Weil auch ", "tokens": ["Weil", "auch"], "token_info": ["word", "word"], "pos": ["KOUS", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Kurz vor jenem Christtag bist,", "tokens": ["Kurz", "vor", "je\u00b7nem", "Christ\u00b7tag", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PDAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sagt' ich oft schon: \u00bbZu so frommen", "tokens": ["Sagt'", "ich", "oft", "schon", ":", "\u00bb", "Zu", "so", "from\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$.", "$(", "APPR", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Engeln sie zu z\u00e4hlen ist!\u00ab", "tokens": ["En\u00b7geln", "sie", "zu", "z\u00e4h\u00b7len", "ist", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "PTKZU", "VVINF", "VAFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber da\u00df ich konnte nennen", "tokens": ["A\u00b7ber", "da\u00df", "ich", "konn\u00b7te", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbkind\u00ab dich schon in Red' und Lied,", "tokens": ["\u00bb", "kind", "\u00ab", "dich", "schon", "in", "Red'", "und", "Lied", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$(", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kann wohl der nur nicht mi\u00dfkennen,", "tokens": ["Kann", "wohl", "der", "nur", "nicht", "mi\u00df\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der oft Engelsbilder sieht.", "tokens": ["Der", "oft", "En\u00b7gels\u00b7bil\u00b7der", "sieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Engel tragen Kindermienen,", "tokens": ["En\u00b7gel", "tra\u00b7gen", "Kin\u00b7der\u00b7mie\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So nur es gekommen ist,", "tokens": ["So", "nur", "es", "ge\u00b7kom\u00b7men", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df du mir ein Kind geschienen,", "tokens": ["Da\u00df", "du", "mir", "ein", "Kind", "ge\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da du doch ein Engel bist.", "tokens": ["Da", "du", "doch", "ein", "En\u00b7gel", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}