{"dta.poem.12585": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Erster Theil.  \n Von den Tisch-Liedern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Reichthum! du bringst alles Weh;", "tokens": ["Reicht\u00b7hum", "!", "du", "bringst", "al\u00b7les", "Weh", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Nicht die Erde, nicht die See", "tokens": ["Nicht", "die", "Er\u00b7de", ",", "nicht", "die", "See"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "$,", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trage deine Thronen!", "tokens": ["Tra\u00b7ge", "dei\u00b7ne", "Thro\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Geh zum schwarzen H\u00f6llen-Flu\u00df!", "tokens": ["Geh", "zum", "schwar\u00b7zen", "H\u00f6l\u00b7len\u00b7Flu\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Geh zum finstern Tartarus!", "tokens": ["Geh", "zum", "fins\u00b7tern", "Tar\u00b7ta\u00b7rus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Da nur must du wohnen.", "tokens": ["Da", "nur", "must", "du", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "--+-+-", "measure": "anapaest.init"}}}}}