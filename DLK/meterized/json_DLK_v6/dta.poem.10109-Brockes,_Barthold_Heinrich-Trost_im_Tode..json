{"dta.poem.10109": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trost im Tode.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da, was von unsrer Zeit vergangen,", "tokens": ["Da", ",", "was", "von", "uns\u00b7rer", "Zeit", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dahin, was k\u00fcnftig, noch nicht ist,", "tokens": ["Da\u00b7hin", ",", "was", "k\u00fcnf\u00b7tig", ",", "noch", "nicht", "ist", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PRELS", "ADJD", "$,", "ADV", "PTKNEG", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Indem es noch zu seyn nicht angefangen;", "tokens": ["In\u00b7dem", "es", "noch", "zu", "seyn", "nicht", "an\u00b7ge\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKZU", "VAINF", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sich demnach, wenn man es recht ermisst,", "tokens": ["Und", "sich", "dem\u00b7nach", ",", "wenn", "man", "es", "recht", "er\u00b7misst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PAV", "$,", "KOUS", "PIS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das k\u00fcnftge so mit dem vergangnen bindet,", "tokens": ["Das", "k\u00fcnft\u00b7ge", "so", "mit", "dem", "ver\u00b7gang\u00b7nen", "bin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df man die Gegenwart kaum kaum dazwischen findet;", "tokens": ["Da\u00df", "man", "die", "Ge\u00b7gen\u00b7wart", "kaum", "kaum", "da\u00b7zwi\u00b7schen", "fin\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "ADV", "ADV", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So giebt uns dieses eine Lehre,", "tokens": ["So", "giebt", "uns", "die\u00b7ses", "ei\u00b7ne", "Leh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die billig offt zu \u00fcberlegen w\u00e4re:", "tokens": ["Die", "bil\u00b7lig", "offt", "zu", "\u00fc\u00b7berl\u00b7e\u00b7gen", "w\u00e4\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da unser kurtzes JETZT all' Augenblick verschwin-\ndet,", "tokens": ["Da", "un\u00b7ser", "kurt\u00b7zes", "JeTZT", "all'", "Au\u00b7gen\u00b7blick", "ver\u00b7schwin", "det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NE", "PIAT", "NN", "TRUNC", "PDS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und man doch den Verlust nicht mercket noch empfindet;", "tokens": ["Und", "man", "doch", "den", "Ver\u00b7lust", "nicht", "mer\u00b7cket", "noch", "emp\u00b7fin\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "ART", "NN", "PTKNEG", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So wird auch, wann der Tod uns raubt des Tages\nSchein,", "tokens": ["So", "wird", "auch", ",", "wann", "der", "Tod", "uns", "raubt", "des", "Ta\u00b7ges", "Schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Lebens schwindend JETZT uns auch unf\u00fchlbar\nseyn.", "tokens": ["Des", "Le\u00b7bens", "schwin\u00b7dend", "JeTZT", "uns", "auch", "un\u00b7f\u00fchl\u00b7bar", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NE", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}