{"dta.poem.1201": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "AnEMONS  \n und  \n  ADONIS  \n Blumen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Geh hin/ begl\u00fcckter Ring/ die Finger zu umschl\u00fcssen/", "tokens": ["Geh", "hin", "/", "be\u00b7gl\u00fcck\u00b7ter", "Ring", "/", "die", "Fin\u00b7ger", "zu", "um\u00b7schl\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$(", "ADJA", "NN", "$(", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die edler als dein Gold/ und werther als dein Stein.", "tokens": ["Die", "ed\u00b7ler", "als", "dein", "Gold", "/", "und", "wert\u00b7her", "als", "dein", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KOKOM", "PPOSAT", "NN", "$(", "KON", "ADJD", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "K\u00f6nt ich auff eine Zeit an deiner Stelle seyn/", "tokens": ["K\u00f6nt", "ich", "auff", "ei\u00b7ne", "Zeit", "an", "dei\u00b7ner", "Stel\u00b7le", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie solte dieser Tausch das Leben mir vers\u00fcssen!", "tokens": ["Wie", "sol\u00b7te", "die\u00b7ser", "Tausch", "das", "Le\u00b7ben", "mir", "ver\u00b7s\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PDAT", "NN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du kanst/ ohn alle Scheu/ die zarten Glieder k\u00fcssen/", "tokens": ["Du", "kanst", "/", "ohn", "al\u00b7le", "Scheu", "/", "die", "zar\u00b7ten", "Glie\u00b7der", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "APPR", "PIAT", "NN", "$(", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dir stralet Tag und Nacht der hellen Augen Schein.", "tokens": ["Dir", "stra\u00b7let", "Tag", "und", "Nacht", "der", "hel\u00b7len", "Au\u00b7gen", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was sonst der sch\u00e4le Neyd der Kleider birget ein/", "tokens": ["Was", "sonst", "der", "sch\u00e4\u00b7le", "Neyd", "der", "Klei\u00b7der", "bir\u00b7get", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Kanst du/ von ihrer Hand gef\u00fchret/ frey begr\u00fcssen.", "tokens": ["Kanst", "du", "/", "von", "ih\u00b7rer", "Hand", "ge\u00b7f\u00fch\u00b7ret", "/", "frey", "be\u00b7gr\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$(", "APPR", "PPOSAT", "NN", "VVPP", "$(", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich gebe dich an die/ der ich ergeben bin/", "tokens": ["Ich", "ge\u00b7be", "dich", "an", "die", "/", "der", "ich", "er\u00b7ge\u00b7ben", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "$(", "PRELS", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Du bleibest stets um sie/ ich mu\u00df zur\u00fccke bleiben/", "tokens": ["Du", "blei\u00b7best", "stets", "um", "sie", "/", "ich", "mu\u00df", "zu\u00b7r\u00fc\u00b7cke", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "$(", "PPER", "VMFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Darff/ wo du \u00f6ffters bist/ nicht sicher dencken hin.", "tokens": ["Darff", "/", "wo", "du", "\u00f6ff\u00b7ters", "bist", "/", "nicht", "si\u00b7cher", "den\u00b7cken", "hin", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$(", "PWAV", "PPER", "ADV", "VAFIN", "$(", "PTKNEG", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wie sucht das Gl\u00fccke so sein Spiel mit mir zu treiben!", "tokens": ["Wie", "sucht", "das", "Gl\u00fc\u00b7cke", "so", "sein", "Spiel", "mit", "mir", "zu", "trei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "PPOSAT", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich bringe dir zu weg und thue mehr f\u00fcr dich/", "tokens": ["Ich", "brin\u00b7ge", "dir", "zu", "weg", "und", "thue", "mehr", "f\u00fcr", "dich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "PTKVZ", "KON", "VVFIN", "ADV", "APPR", "PPER", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.14": {"text": "Als mir nicht selbsten wird erlaubt zu thun f\u00fcr mich.", "tokens": ["Als", "mir", "nicht", "selbs\u00b7ten", "wird", "er\u00b7laubt", "zu", "thun", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VAFIN", "VVPP", "PTKZU", "VVINF", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}