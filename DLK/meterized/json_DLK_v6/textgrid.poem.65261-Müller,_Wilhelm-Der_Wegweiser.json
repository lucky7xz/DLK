{"textgrid.poem.65261": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Wegweiser", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was vermeid' ich denn die Wege,", "tokens": ["Was", "ver\u00b7meid'", "ich", "denn", "die", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die andren Wandrer gehn,", "tokens": ["Wo", "die", "an\u00b7dren", "Wand\u00b7rer", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Suche mir versteckte Stege", "tokens": ["Su\u00b7che", "mir", "ver\u00b7steck\u00b7te", "Ste\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch verschneite Felsenh\u00f6hn?", "tokens": ["Durch", "ver\u00b7schnei\u00b7te", "Fel\u00b7sen\u00b7h\u00f6hn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Habe ja doch nichts begangen,", "tokens": ["Ha\u00b7be", "ja", "doch", "nichts", "be\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "PIS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich Menschen sollte scheun \u2013", "tokens": ["Da\u00df", "ich", "Men\u00b7schen", "soll\u00b7te", "scheun", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welch ein th\u00f6richtes Verlangen", "tokens": ["Welch", "ein", "th\u00f6\u00b7rich\u00b7tes", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Treibt mich in die W\u00fcstenein?", "tokens": ["Treibt", "mich", "in", "die", "W\u00fcs\u00b7ten\u00b7ein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Weiser stehen auf den Stra\u00dfen,", "tokens": ["Wei\u00b7ser", "ste\u00b7hen", "auf", "den", "Stra\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weisen auf die St\u00e4dte zu,", "tokens": ["Wei\u00b7sen", "auf", "die", "St\u00e4d\u00b7te", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich wandre sonder Ma\u00dfen,", "tokens": ["Und", "ich", "wand\u00b7re", "son\u00b7der", "Ma\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne Ruh', und suche Ruh'.", "tokens": ["Oh\u00b7ne", "Ruh'", ",", "und", "su\u00b7che", "Ruh'", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Einen Weiser seh' ich stehen", "tokens": ["Ei\u00b7nen", "Wei\u00b7ser", "seh'", "ich", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unverr\u00fcckt vor meinem Blick;", "tokens": ["Un\u00b7ver\u00b7r\u00fcckt", "vor", "mei\u00b7nem", "Blick", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine Stra\u00dfe mu\u00df ich gehen,", "tokens": ["Ei\u00b7ne", "Stra\u00b7\u00dfe", "mu\u00df", "ich", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die noch Keiner ging zur\u00fcck.", "tokens": ["Die", "noch", "Kei\u00b7ner", "ging", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}