{"dta.poem.71": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "Die k\u00fcnftige Geliebte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1771", "urn": "urn:nbn:de:kobv:b4-200905199486", "language": ["de:0.99"], "booktitle": "[Klopstock, Friedrich Gottlieb]: Oden. Hamburg, 1771."}, "poem": {"stanza.1": {"line.1": {"text": "Dir nur, liebendes Herz, euch, meine vertraulich-", "tokens": ["Dir", "nur", ",", "lie\u00b7ben\u00b7des", "Herz", ",", "euch", ",", "mei\u00b7ne", "ver\u00b7trau\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "$,", "ADJA", "NN", "$,", "PPER", "$,", "PPOSAT", "TRUNC"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sing ich traurig allein die\u00df wehm\u00fcthige Lied.", "tokens": ["Sing", "ich", "trau\u00b7rig", "al\u00b7lein", "die\u00df", "weh\u00b7m\u00fct\u00b7hi\u00b7ge", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "PDS", "ADJA", "NN", "$."], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Nur mein Auge soll\u2019s mit schmachtendem Feuer dur", "tokens": ["Nur", "mein", "Au\u00b7ge", "soll's", "mit", "schmach\u00b7ten\u00b7dem", "Feu\u00b7er", "dur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "APPR", "ADJA", "NN", "APPRART"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Und, an Klagen verw\u00f6hnt, h\u00f6r es mein leiseres Ohr:", "tokens": ["Und", ",", "an", "Kla\u00b7gen", "ver\u00b7w\u00f6hnt", ",", "h\u00f6r", "es", "mein", "lei\u00b7se\u00b7res", "Ohr", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "NN", "VVPP", "$,", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Ach! warum, o Natur, warum, unz\u00e4rtliche Mutter,", "tokens": ["Ach", "!", "wa\u00b7rum", ",", "o", "Na\u00b7tur", ",", "wa\u00b7rum", ",", "un\u00b7z\u00e4rt\u00b7li\u00b7che", "Mut\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "$,", "FM", "NN", "$,", "PWAV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Gabst du zu dem Gef\u00fchl mir ein zu biegsames Herz?", "tokens": ["Gabst", "du", "zu", "dem", "Ge\u00b7f\u00fchl", "mir", "ein", "zu", "bieg\u00b7sa\u00b7mes", "Herz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PPER", "ART", "PTKZU", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Und ins biegsame Herz die unbezwingliche Liebe,", "tokens": ["Und", "ins", "bieg\u00b7sa\u00b7me", "Herz", "die", "un\u00b7be\u00b7zwing\u00b7li\u00b7che", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Daurend Verlangen, und ach keine Geliebte dazu?", "tokens": ["Dau\u00b7rend", "Ver\u00b7lan\u00b7gen", ",", "und", "ach", "kei\u00b7ne", "Ge\u00b7lieb\u00b7te", "da\u00b7zu", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KON", "ADV", "PIAT", "NN", "PTKVZ", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Die du k\u00fcnftig mich liebst, (wenn anders zu meinen", "tokens": ["Die", "du", "k\u00fcnf\u00b7tig", "mich", "liebst", ",", "(", "wenn", "an\u00b7ders", "zu", "mei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "PPER", "VVFIN", "$,", "$(", "KOUS", "ADV", "APPR", "PPOSAT"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Einst das Schicksal erweicht eine Geliebte mir giebt!)", "tokens": ["Einst", "das", "Schick\u00b7sal", "er\u00b7weicht", "ei\u00b7ne", "Ge\u00b7lieb\u00b7te", "mir", "giebt", "!", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Die du k\u00fcnftig mich liebst, o du aus allen erkohren,", "tokens": ["Die", "du", "k\u00fcnf\u00b7tig", "mich", "liebst", ",", "o", "du", "aus", "al\u00b7len", "er\u00b7koh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "PPER", "VVFIN", "$,", "FM", "PPER", "APPR", "PIS", "VVINF", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Sag, wo dein fliehender Fu\u00df ohne mich einsam", "tokens": ["Sag", ",", "wo", "dein", "flie\u00b7hen\u00b7der", "Fu\u00df", "oh\u00b7ne", "mich", "ein\u00b7sam"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "ADJD"], "meter": "+--+--++--+-", "measure": "dactylic.di.plus"}, "line.13": {"text": "Nur mit Einem verrathenden Laute, mit Einem der T\u00f6ne,", "tokens": ["Nur", "mit", "Ei\u00b7nem", "ver\u00b7ra\u00b7then\u00b7den", "Lau\u00b7te", ",", "mit", "Ei\u00b7nem", "der", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "PIS", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Die der Frohen entfliehn, sag es, einst Gl\u00fcckliche,", "tokens": ["Die", "der", "Fro\u00b7hen", "ent\u00b7fliehn", ",", "sag", "es", ",", "einst", "Gl\u00fcck\u00b7li\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "$,", "VVFIN", "PPER", "$,", "ADV", "NN", "$,"], "meter": "--+--+-+-+--", "measure": "anapaest.di.plus"}, "line.15": {"text": "F\u00fchlst du, wie ich, der Liebe Gewalt, verlangst du nach", "tokens": ["F\u00fchlst", "du", ",", "wie", "ich", ",", "der", "Lie\u00b7be", "Ge\u00b7walt", ",", "ver\u00b7langst", "du", "nach"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "$,", "ART", "NN", "NN", "$,", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Ohne da\u00df du mich kennst; o so verheele mirs nicht!", "tokens": ["Oh\u00b7ne", "da\u00df", "du", "mich", "kennst", ";", "o", "so", "ver\u00b7hee\u00b7le", "mirs", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PRF", "VVFIN", "$.", "FM", "ADV", "VVFIN", "NE", "PTKNEG", "$."], "meter": "+-++-+-+-+--+", "measure": "iambic.septa.chol"}, "line.17": {"text": "Sag es mit einem durchdringendem Ach, das meinem", "tokens": ["Sag", "es", "mit", "ei\u00b7nem", "durch\u00b7drin\u00b7gen\u00b7dem", "Ach", ",", "das", "mei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.18": {"text": "Das aus innerster Brust Klage seufzet, und stirbt.", "tokens": ["Das", "aus", "in\u00b7ners\u00b7ter", "Brust", "Kla\u00b7ge", "seuf\u00b7zet", ",", "und", "stirbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADJA", "NN", "NN", "VVFIN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Oft um Mitternacht wehklagt die bebende Lippe,", "tokens": ["Oft", "um", "Mit\u00b7ter\u00b7nacht", "weh\u00b7klagt", "die", "be\u00b7ben\u00b7de", "Lip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Da\u00df, die ich liebe, du mir immer unsichtbar noch bist!", "tokens": ["Da\u00df", ",", "die", "ich", "lie\u00b7be", ",", "du", "mir", "im\u00b7mer", "un\u00b7sicht\u00b7bar", "noch", "bist", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "PPER", "VVFIN", "$,", "PPER", "PPER", "ADV", "ADJD", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.21": {"text": "Oft um Mitternacht streckt sich mein zitternder Arm aus,", "tokens": ["Oft", "um", "Mit\u00b7ter\u00b7nacht", "streckt", "sich", "mein", "zit\u00b7tern\u00b7der", "Arm", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "PRF", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "--+-++--+--+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Und umfasset ein Bild, ach das deine vielleicht!", "tokens": ["Und", "um\u00b7fas\u00b7set", "ein", "Bild", ",", "ach", "das", "dei\u00b7ne", "viel\u00b7leicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "XY", "ART", "PPOSAT", "ADV", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.23": {"text": "Wo, wo such ich dich? wo werd ich endlich dich finden?", "tokens": ["Wo", ",", "wo", "such", "ich", "dich", "?", "wo", "werd", "ich", "end\u00b7lich", "dich", "fin\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "VVFIN", "PPER", "PRF", "$.", "PWAV", "VAFIN", "PPER", "ADV", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.24": {"text": "Du, die meine Begier stark und unsterblich verlangt!", "tokens": ["Du", ",", "die", "mei\u00b7ne", "Be\u00b7gier", "stark", "und", "uns\u00b7terb\u00b7lich", "ver\u00b7langt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.25": {"text": "Jener Ort, der dich h\u00e4lt, wo ist er? wo fliesset der Himmel,", "tokens": ["Je\u00b7ner", "Ort", ",", "der", "dich", "h\u00e4lt", ",", "wo", "ist", "er", "?", "wo", "flies\u00b7set", "der", "Him\u00b7mel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PWAV", "VAFIN", "PPER", "$.", "PWAV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+-++--+-", "measure": "trochaic.septa.relaxed"}, "line.26": {"text": "Welcher dein Aug umw\u00f6lbt, heiter und l\u00e4chelnd", "tokens": ["Wel\u00b7cher", "dein", "Aug", "um\u00b7w\u00f6lbt", ",", "hei\u00b7ter", "und", "l\u00e4\u00b7chelnd"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAT", "PPOSAT", "NN", "VVPP", "$,", "ADJD", "KON", "ADJD"], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}, "line.27": {"text": "Werd ich mein Auge zu dir einst, segnender Himmel,", "tokens": ["Werd", "ich", "mein", "Au\u00b7ge", "zu", "dir", "einst", ",", "seg\u00b7nen\u00b7der", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "APPR", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.28": {"text": "Und umarmet die sehn, die aufbl\u00fchen du sahst?", "tokens": ["Und", "um\u00b7ar\u00b7met", "die", "sehn", ",", "die", "auf\u00b7bl\u00fc\u00b7hen", "du", "sahst", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "VVINF", "$,", "PRELS", "VVFIN", "PPER", "VVFIN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.29": {"text": "Aber ich kenne dich nicht! Es ging die fernere Sonne", "tokens": ["A\u00b7ber", "ich", "ken\u00b7ne", "dich", "nicht", "!", "Es", "ging", "die", "fer\u00b7ne\u00b7re", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.30": {"text": "Meinen Thr\u00e4nen daselbst niemals nicht unter und", "tokens": ["Mei\u00b7nen", "Thr\u00e4\u00b7nen", "da\u00b7selbst", "nie\u00b7mals", "nicht", "un\u00b7ter", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PAV", "ADV", "PTKNEG", "APPR", "KON"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.31": {"text": "Soll ich jene Gefilde nicht sehn? F\u00fchrt nie dort im", "tokens": ["Soll", "ich", "je\u00b7ne", "Ge\u00b7fil\u00b7de", "nicht", "sehn", "?", "F\u00fchrt", "nie", "dort", "im"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PDAT", "NN", "PTKNEG", "VVINF", "$.", "VVFIN", "ADV", "ADV", "APPRART"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.32": {"text": "Meine zitternde Hand sie durch ein bl\u00fchendes Thal?", "tokens": ["Mei\u00b7ne", "zit\u00b7tern\u00b7de", "Hand", "sie", "durch", "ein", "bl\u00fc\u00b7hen\u00b7des", "Thal", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.33": {"text": "Sinkt sie, von s\u00fcsser Gewalt der m\u00e4chtigen Liebe be-", "tokens": ["Sinkt", "sie", ",", "von", "s\u00fcs\u00b7ser", "Ge\u00b7walt", "der", "m\u00e4ch\u00b7ti\u00b7gen", "Lie\u00b7be", "be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "TRUNC"], "meter": "+--+--+-+--+--", "measure": "dactylic.di.plus"}, "line.34": {"text": "Nie mit der D\u00e4mmerung Stern mir an die beben-", "tokens": ["Nie", "mit", "der", "D\u00e4m\u00b7me\u00b7rung", "Stern", "mir", "an", "die", "be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "PPER", "APPR", "ART", "TRUNC"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.35": {"text": "Ach, wie schl\u00e4gt mir mein Herz! Wie zittern durch meine", "tokens": ["Ach", ",", "wie", "schl\u00e4gt", "mir", "mein", "Herz", "!", "Wie", "zit\u00b7tern", "durch", "mei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "PWAV", "VVFIN", "APPR", "PPOSAT"], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.36": {"text": "Freud und Hofnung, dem Schmerz un\u00fcberwindlich", "tokens": ["Freud", "und", "Hof\u00b7nung", ",", "dem", "Schmerz", "un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "ART", "NN", "ADJD"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.37": {"text": "Unbesingbare Lust, ein s\u00fcsser begeisternder Schauer,", "tokens": ["Un\u00b7be\u00b7sing\u00b7ba\u00b7re", "Lust", ",", "ein", "s\u00fcs\u00b7ser", "be\u00b7geis\u00b7tern\u00b7der", "Schau\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.38": {"text": "Eine Thr\u00e4ne, die mir still von den Wangen entfiel;", "tokens": ["Ei\u00b7ne", "Thr\u00e4\u00b7ne", ",", "die", "mir", "still", "von", "den", "Wan\u00b7gen", "ent\u00b7fiel", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.39": {"text": "Und, o ich sehe sie! mitweinende, weibliche Z\u00e4hren", "tokens": ["Und", ",", "o", "ich", "se\u00b7he", "sie", "!", "mit\u00b7wei\u00b7nen\u00b7de", ",", "weib\u00b7li\u00b7che", "Z\u00e4h\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "$,", "FM", "PPER", "VVFIN", "PPER", "$.", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.40": {"text": "Ein mir lispelnder Hauch, und ein ersch\u00fctterndes Ach;", "tokens": ["Ein", "mir", "lis\u00b7peln\u00b7der", "Hauch", ",", "und", "ein", "er\u00b7sch\u00fct\u00b7tern\u00b7des", "Ach", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "$,", "KON", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.41": {"text": "Ein zusegnender Laut, der mir rief, wie ein Schatten", "tokens": ["Ein", "zu\u00b7seg\u00b7nen\u00b7der", "Laut", ",", "der", "mir", "rief", ",", "wie", "ein", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PWAV", "ART", "NN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.42": {"text": "Liebend ruft, weissagt, dich, die mich h\u00f6rete, mir.", "tokens": ["Lie\u00b7bend", "ruft", ",", "weis\u00b7sagt", ",", "dich", ",", "die", "mich", "h\u00f6\u00b7re\u00b7te", ",", "mir", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "VVFIN", "$,", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,", "PPER", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.43": {"text": "O. du, die du Sie mir und meiner Liebe gebahrest,", "tokens": ["O.", "du", ",", "die", "du", "Sie", "mir", "und", "mei\u00b7ner", "Lie\u00b7be", "ge\u00b7bah\u00b7rest", ","], "token_info": ["abbreviation", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "PPER", "PPER", "PPER", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.44": {"text": "H\u00e4ltst du Sie, Mutter, umarmt; dreymal geseg-", "tokens": ["H\u00e4ltst", "du", "Sie", ",", "Mut\u00b7ter", ",", "um\u00b7armt", ";", "drey\u00b7mal", "ge\u00b7seg"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "$,", "NN", "$,", "VVPP", "$.", "ADV", "TRUNC"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.45": {"text": "Dreymal gesegnet sey mir dein gleich empfindendes Herze,", "tokens": ["Drey\u00b7mal", "ge\u00b7seg\u00b7net", "sey", "mir", "dein", "gleich", "emp\u00b7fin\u00b7den\u00b7des", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "PPER", "PPOSAT", "ADV", "ADJA", "VVFIN", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.46": {"text": "Das der Tochter zuerst weibliche Z\u00e4rtlichkeit gab!", "tokens": ["Das", "der", "Toch\u00b7ter", "zu\u00b7erst", "weib\u00b7li\u00b7che", "Z\u00e4rt\u00b7lich\u00b7keit", "gab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.47": {"text": "Aber la\u00df sie itzt frey! Sie eilt zu den Blumen, und will da", "tokens": ["A\u00b7ber", "la\u00df", "sie", "itzt", "frey", "!", "Sie", "eilt", "zu", "den", "Blu\u00b7men", ",", "und", "will", "da"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "ADV", "ADJD", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "VMFIN", "ADV"], "meter": "--+--+-+--+-+--", "measure": "anapaest.di.plus"}, "line.48": {"text": "Nicht von Zeugen behorcht, will gesehen nicht seyn.", "tokens": ["Nicht", "von", "Zeu\u00b7gen", "be\u00b7horcht", ",", "will", "ge\u00b7se\u00b7hen", "nicht", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VVFIN", "$,", "VMFIN", "VVPP", "PTKNEG", "VAINF", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.49": {"text": "Eile nicht so! Doch mit welchem Namen soll ich dich", "tokens": ["Ei\u00b7le", "nicht", "so", "!", "Doch", "mit", "wel\u00b7chem", "Na\u00b7men", "soll", "ich", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ADV", "$.", "KON", "APPR", "PWAT", "NN", "VMFIN", "PPER", "PRF"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.50": {"text": "Du, die unaussprechlich meinem Verlangen gef\u00e4llt?", "tokens": ["Du", ",", "die", "un\u00b7aus\u00b7sprech\u00b7lich", "mei\u00b7nem", "Ver\u00b7lan\u00b7gen", "ge\u00b7f\u00e4llt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADJD", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.51": {"text": "Heissest du Laura? Laura besang Petracha in Liedern,", "tokens": ["Heis\u00b7sest", "du", "Lau\u00b7ra", "?", "Lau\u00b7ra", "be\u00b7sang", "Pet\u00b7ra\u00b7cha", "in", "Lie\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$.", "NE", "VVFIN", "NE", "APPR", "NN", "$,"], "meter": "+--+-+--++-+-+-", "measure": "iambic.septa.invert"}, "line.52": {"text": "Zwar dem Bewunderer sch\u00f6n, aber dem Liebenden", "tokens": ["Zwar", "dem", "Be\u00b7wun\u00b7de\u00b7rer", "sch\u00f6n", ",", "a\u00b7ber", "dem", "Lie\u00b7ben\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJD", "$,", "ADV", "ART", "NN"], "meter": "+--+---+--+--", "measure": "dactylic.di.plus"}, "line.53": {"text": "Wirst du Fanny genannt? Ist Cidli dein feyerlicher Name?", "tokens": ["Wirst", "du", "Fan\u00b7ny", "ge\u00b7nannt", "?", "Ist", "Cid\u00b7li", "dein", "fey\u00b7er\u00b7li\u00