{"textgrid.poem.35970": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Bl\u00fcmlein all' im Tale,", "tokens": ["Ihr", "Bl\u00fcm\u00b7lein", "all'", "im", "Ta\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ja bl\u00fcht nun mir zur Lust,", "tokens": ["Ja", "bl\u00fcht", "nun", "mir", "zur", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "ADV", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich m\u00f6cht' euch pfl\u00fccken alle", "tokens": ["Ich", "m\u00f6cht'", "euch", "pfl\u00fc\u00b7cken", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr ", "tokens": ["F\u00fcr"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Ihr Bl\u00fcmlein hier wie ferne,", "tokens": ["Ihr", "Bl\u00fcm\u00b7lein", "hier", "wie", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "KOKOM", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie traurig m\u00fc\u00dft ihr bl\u00fchn,", "tokens": ["Wie", "trau\u00b7rig", "m\u00fc\u00dft", "ihr", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich tr\u00fcg' euch alle gerne", "tokens": ["Ich", "tr\u00fcg'", "euch", "al\u00b7le", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu ihren F\u00fc\u00dfen hin.", "tokens": ["Zu", "ih\u00b7ren", "F\u00fc\u00b7\u00dfen", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du Sonn' am Himmel oben,", "tokens": ["Du", "Sonn'", "am", "Him\u00b7mel", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du scheinst so hell ins Tal,", "tokens": ["Du", "scheinst", "so", "hell", "ins", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du Mond bist hoch zu loben,", "tokens": ["Du", "Mond", "bist", "hoch", "zu", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Sternlein allzumal.", "tokens": ["Ihr", "Stern\u00b7lein", "all\u00b7zu\u00b7mal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich wollt', ich k\u00f6nnt' euch ziehen", "tokens": ["Ich", "wollt'", ",", "ich", "k\u00f6nnt'", "euch", "zie\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zur Erd' herab, zu mir.", "tokens": ["Zur", "Erd'", "her\u00b7ab", ",", "zu", "mir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr solltet alle bl\u00fchen", "tokens": ["Ihr", "soll\u00b7tet", "al\u00b7le", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIS", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor ihrer H\u00fcttent\u00fcr.", "tokens": ["Vor", "ih\u00b7rer", "H\u00fct\u00b7ten\u00b7t\u00fcr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich pflanzt' um ihre H\u00fctte", "tokens": ["Ich", "pflanzt'", "um", "ih\u00b7re", "H\u00fct\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Viel bunte Blumen hin,", "tokens": ["Viel", "bun\u00b7te", "Blu\u00b7men", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So wohnt die Liebe mitten", "tokens": ["So", "wohnt", "die", "Lie\u00b7be", "mit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In meiner Liebe drin.", "tokens": ["In", "mei\u00b7ner", "Lie\u00b7be", "drin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Mit Blumen streu' ich immer", "tokens": ["Mit", "Blu\u00b7men", "streu'", "ich", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Den Weg aus ihrem Haus,", "tokens": ["Den", "Weg", "aus", "ih\u00b7rem", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie geht im Morgenschimmer", "tokens": ["Sie", "geht", "im", "Mor\u00b7gen\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So lieblich dort heraus.", "tokens": ["So", "lieb\u00b7lich", "dort", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ihr Blumen all im Tale,", "tokens": ["Ihr", "Blu\u00b7men", "all", "im", "Ta\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PIAT", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Blumen, meine Lust,", "tokens": ["Ihr", "Blu\u00b7men", ",", "mei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich will euch pfl\u00fccken alle", "tokens": ["Ich", "will", "euch", "pfl\u00fc\u00b7cken", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr ", "tokens": ["F\u00fcr"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}}}}}