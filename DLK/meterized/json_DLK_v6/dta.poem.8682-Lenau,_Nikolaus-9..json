{"dta.poem.8682": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Greis trat l\u00e4chelnd mir entgegen,", "tokens": ["Ein", "Greis", "trat", "l\u00e4\u00b7chelnd", "mir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bot mir die Hand gedankenvoll,", "tokens": ["Bot", "mir", "die", "Hand", "ge\u00b7dan\u00b7ken\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und hob sie dann empor zum Segen,", "tokens": ["Und", "hob", "sie", "dann", "em\u00b7por", "zum", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der sanft vom Himmel niederquoll;", "tokens": ["Der", "sanft", "vom", "Him\u00b7mel", "nie\u00b7der\u00b7quoll", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und ich empfand es tief im Herzen,", "tokens": ["Und", "ich", "emp\u00b7fand", "es", "tief", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Zorn der Donner Gottes nicht;", "tokens": ["Da\u00df", "Zorn", "der", "Don\u00b7ner", "Got\u00b7tes", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df aus der Weste leichten Scherzen,", "tokens": ["Da\u00df", "aus", "der", "Wes\u00b7te", "leich\u00b7ten", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie aus Gewittern Liebe spricht.", "tokens": ["Wie", "aus", "Ge\u00b7wit\u00b7tern", "Lie\u00b7be", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und einen Labebecher trank ich,", "tokens": ["Und", "ei\u00b7nen", "La\u00b7be\u00b7be\u00b7cher", "trank", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und schlich, wohin die Ruh' mich rief,", "tokens": ["Und", "schlich", ",", "wo\u00b7hin", "die", "Ruh'", "mich", "rief", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hinaus zur Scheune, m\u00fcde sank ich", "tokens": ["Hin\u00b7aus", "zur", "Scheu\u00b7ne", ",", "m\u00fc\u00b7de", "sank", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hier in des Heues Duft \u2014 und schlief.", "tokens": ["Hier", "in", "des", "Heu\u00b7es", "Duft", "und", "schlief", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$(", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Was mich erfreut auf meinen Wegen,", "tokens": ["Was", "mich", "er\u00b7freut", "auf", "mei\u00b7nen", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das tr\u00e4umt' ich nun im Schlafe nach,", "tokens": ["Das", "tr\u00e4umt'", "ich", "nun", "im", "Schla\u00b7fe", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und tr\u00e4umend h\u00f6rt' ich, wie der Regen", "tokens": ["Und", "tr\u00e4u\u00b7mend", "h\u00f6rt'", "ich", ",", "wie", "der", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sanft niedertr\u00e4ufelt' auf das Dach.", "tokens": ["Sanft", "nie\u00b7der\u00b7tr\u00e4u\u00b7felt'", "auf", "das", "Dach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "S\u00fc\u00df tr\u00e4umt es sich in einer Scheune,", "tokens": ["S\u00fc\u00df", "tr\u00e4umt", "es", "sich", "in", "ei\u00b7ner", "Scheu\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn drauf der Regen leise klopft;", "tokens": ["Wenn", "drauf", "der", "Re\u00b7gen", "lei\u00b7se", "klopft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PAV", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So mag sich's ruh'n im Todtenschreine,", "tokens": ["So", "mag", "sich's", "ruh'n", "im", "Tod\u00b7ten\u00b7schrei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf den die Freundesz\u00e4hre tropft.", "tokens": ["Auf", "den", "die", "Freun\u00b7des\u00b7z\u00e4h\u00b7re", "tropft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}