{"dta.poem.5857": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als G\u00f6tter ehrten sie die Lufft, die Fluht,", "tokens": ["Als", "G\u00f6t\u00b7ter", "ehr\u00b7ten", "sie", "die", "Lufft", ",", "die", "Fluht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Licht und auch des Feuers Gluht;", "tokens": ["Das", "Licht", "und", "auch", "des", "Feu\u00b7ers", "Gluht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es haben Sonn\u2019 und Mond (ob sie gleich sonder Leben,", "tokens": ["Es", "ha\u00b7ben", "Sonn'", "und", "Mond", "(", "ob", "sie", "gleich", "son\u00b7der", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "$(", "KOUS", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Elementen gleich, aus welchen sie formiret;)", "tokens": ["Den", "E\u00b7le\u00b7men\u00b7ten", "gleich", ",", "aus", "wel\u00b7chen", "sie", "for\u00b7mi\u00b7ret", ";)"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "emoticon"], "pos": ["ART", "NN", "ADV", "$,", "APPR", "PWAT", "PPER", "VVFIN", "$."], "meter": "+-+--+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Als G\u00f6tter, wie es hie\u00df, der Welt das Licht gegeben.", "tokens": ["Als", "G\u00f6t\u00b7ter", ",", "wie", "es", "hie\u00df", ",", "der", "Welt", "das", "Licht", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}