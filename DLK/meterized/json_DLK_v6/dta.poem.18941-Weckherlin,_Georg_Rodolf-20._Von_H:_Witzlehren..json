{"dta.poem.18941": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "20.  \n  Von H: Witzlehren.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist kein M\u00e4dlin in der stat", "tokens": ["Es", "ist", "kein", "M\u00e4d\u00b7lin", "in", "der", "stat"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Das Witzlehr nicht wehrt vnd lieb hat/", "tokens": ["Das", "Witz\u00b7lehr", "nicht", "wehrt", "vnd", "lieb", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVFIN", "KON", "ADJD", "VAFIN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Spricht Er/ ob wahr/ kan ich nicht wissen:", "tokens": ["Spricht", "Er", "/", "ob", "wahr", "/", "kan", "ich", "nicht", "wis\u00b7sen", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "KOUS", "ADJD", "$(", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das aber kan ich wol vergwissen/", "tokens": ["Das", "a\u00b7ber", "kan", "ich", "wol", "ver\u00b7gwis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VMFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df keine Jungfraw in der stat", "tokens": ["Da\u00df", "kei\u00b7ne", "Jung\u00b7fraw", "in", "der", "stat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Lieb vnd wehrt den Witzlehren hat.", "tokens": ["Lieb", "vnd", "wehrt", "den", "Witz\u00b7leh\u00b7ren", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ART", "NN", "VAFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}