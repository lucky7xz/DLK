{"textgrid.poem.66836": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Lied, das rollt wie Sonnengold,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Lied, das rollt wie Sonnengold,", "tokens": ["Mein", "Lied", ",", "das", "rollt", "wie", "Son\u00b7nen\u00b7gold", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Purpurstrom des Daseins hold.", "tokens": ["Dem", "Pur\u00b7pur\u00b7strom", "des", "Da\u00b7seins", "hold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn violett erbl\u00fcht die Nacht,", "tokens": ["Wenn", "vi\u00b7o\u00b7lett", "er\u00b7bl\u00fcht", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fl\u00f6t' ich zur weiten Sternenwacht.", "tokens": ["Fl\u00f6t'", "ich", "zur", "wei\u00b7ten", "Ster\u00b7nen\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ged\u00e4mpften Echos meld' ich Streit", "tokens": ["Ge\u00b7d\u00e4mpf\u00b7ten", "E\u00b7chos", "meld'", "ich", "Streit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Menschenleid.", "tokens": ["Und", "Men\u00b7schen\u00b7leid", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wo scharfes Elend Lust zerst\u00f6rt,", "tokens": ["Wo", "schar\u00b7fes", "E\u00b7lend", "Lust", "zer\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schmettr' ich und schluchz' ich qualemp\u00f6rt.", "tokens": ["Schmett\u00b7r'", "ich", "und", "schluch\u00b7z'", "ich", "qua\u00b7lem\u00b7p\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Weh, wenn mein Auge Not erblickt!", "tokens": ["Weh", ",", "wenn", "mein", "Au\u00b7ge", "Not", "er\u00b7blickt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPOSAT", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich schlage, da\u00df der Busch erschrickt.", "tokens": ["Ich", "schla\u00b7ge", ",", "da\u00df", "der", "Busch", "er\u00b7schrickt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Sch\u00f6nheit schwillt mein Klang zu Schutz,", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "schwillt", "mein", "Klang", "zu", "Schutz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu Schutz und Trutz.", "tokens": ["Zu", "Schutz", "und", "Trutz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Wo einer wund von Kampf und Pein,", "tokens": ["Wo", "ei\u00b7ner", "wund", "von", "Kampf", "und", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Trostnachtigall, da tr\u00f6ste fein!", "tokens": ["Trost\u00b7nach\u00b7ti\u00b7gall", ",", "da", "tr\u00f6s\u00b7te", "fein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Frisch wie der Tau gen Morgen quillt,", "tokens": ["Frisch", "wie", "der", "Tau", "gen", "Mor\u00b7gen", "quillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gib Kraft und Wohllaut stark und mild!", "tokens": ["Gib", "Kraft", "und", "Wohl\u00b7laut", "stark", "und", "mild", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wirf Wonnen in der Lauscher Scho\u00df,", "tokens": ["Wirf", "Won\u00b7nen", "in", "der", "Lau\u00b7scher", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schlag schmelzend los!", "tokens": ["Schlag", "schmel\u00b7zend", "los", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}