{"dta.poem.12593": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Erster Theil.  \n Von den Tisch-Liedern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.42", "id:0.42", "fr:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sohn Telamons, tapfrer Ajax, man sagt, da\u00df du nach dem", "tokens": ["Sohn", "Te\u00b7la\u00b7mons", ",", "tapf\u00b7rer", "A\u00b7jax", ",", "man", "sagt", ",", "da\u00df", "du", "nach", "dem"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "$,", "ADJA", "NE", "$,", "PIS", "VVFIN", "$,", "KOUS", "PPER", "APPR", "ART"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Achilles der beste von allen denen Griechen gewesen, die Troja", "tokens": ["A\u00b7chil\u00b7les", "der", "bes\u00b7te", "von", "al\u00b7len", "de\u00b7nen", "Grie\u00b7chen", "ge\u00b7we\u00b7sen", ",", "die", "Tro\u00b7ja"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ART", "ADJA", "APPR", "PIAT", "PDS", "NN", "VAPP", "$,", "PRELS", "NE"], "meter": "-+--+--+-+-+--+--+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "belagerten. Telamon sey zuerst hingezogen, und Ajax, der an-", "tokens": ["be\u00b7la\u00b7ger\u00b7ten", ".", "Te\u00b7la\u00b7mon", "sey", "zu\u00b7erst", "hin\u00b7ge\u00b7zo\u00b7gen", ",", "und", "A\u00b7jax", ",", "der", "an"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$.", "NN", "VAFIN", "ADV", "VVPP", "$,", "KON", "NE", "$,", "ART", "TRUNC"], "meter": "-+--+--+-+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "dere unter den Griechen, n\u00e4chst dem Achilles, sey ihm gefolget.", "tokens": ["de\u00b7re", "un\u00b7ter", "den", "Grie\u00b7chen", ",", "n\u00e4chst", "dem", "A\u00b7chil\u00b7les", ",", "sey", "ihm", "ge\u00b7fol\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "$,", "APPR", "ART", "NE", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "--+--+-+-+--+--+-", "measure": "anapaest.di.plus"}}}}}