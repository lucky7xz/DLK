{"textgrid.poem.50199": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Geschlagen und verwundt, und ", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geschlagen und verwundt, und ", "tokens": ["Ge\u00b7schla\u00b7gen", "und", "ver\u00b7wundt", ",", "und"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NN", "KON", "VVPP", "$,", "KON"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er lag biss zw\u00f6lff im Bett', und ", "tokens": ["Er", "lag", "biss", "zw\u00f6lff", "im", "Bett'", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADV", "CARD", "APPRART", "NN", "$,", "KON"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er wusste nichts, und ", "tokens": ["Er", "wuss\u00b7te", "nichts", ",", "und"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PIS", "$,", "KON"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Er war schon alt, und ", "tokens": ["Er", "war", "schon", "alt", ",", "und"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "KON"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Schwinds\u00fcchtig, ", "tokens": ["Schwind\u00b7s\u00fcch\u00b7tig", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Er war kein Dichterling, und ", "tokens": ["Er", "war", "kein", "Dich\u00b7ter\u00b7ling", ",", "und"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "KON"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Noch weit kein Kammer-Raht, und ", "tokens": ["Noch", "weit", "kein", "Kam\u00b7mer\u00b7Raht", ",", "und"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADJD", "PIAT", "NN", "$,", "KON"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Er blieb zu Hauss, und ", "tokens": ["Er", "blieb", "zu", "Hauss", ",", "und"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "KON"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "War hungrig, aber ", "tokens": ["War", "hung\u00b7rig", ",", "a\u00b7ber"], "token_info": ["word", "word", "punct", "word"], "pos": ["VAFIN", "ADJD", "$,", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Fast ging er nackt und bloss, und ", "tokens": ["Fast", "ging", "er", "nackt", "und", "bloss", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJA", "$,", "KON"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}