{"textgrid.poem.58526": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "564. Leidentliche Reinigung des Geistes", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sobald des Feuers Glut ein gr\u00fcnes H\u00f6lzchen findet,", "tokens": ["So\u00b7bald", "des", "Feu\u00b7ers", "Glut", "ein", "gr\u00fc\u00b7nes", "H\u00f6lz\u00b7chen", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Saugt sie den Saft heraus und alle Kraft vertreibt,", "tokens": ["Saugt", "sie", "den", "Saft", "he\u00b7raus", "und", "al\u00b7le", "Kraft", "ver\u00b7treibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie macht die Rinde schwarz, die Flamme sich entz\u00fcndet", "tokens": ["Sie", "macht", "die", "Rin\u00b7de", "schwarz", ",", "die", "Flam\u00b7me", "sich", "ent\u00b7z\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "$,", "ART", "NN", "PRF", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und brennet durch und durch, bis nichts vom Holze bleibt;", "tokens": ["Und", "bren\u00b7net", "durch", "und", "durch", ",", "bis", "nichts", "vom", "Hol\u00b7ze", "bleibt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "KON", "APPR", "$,", "KOUS", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dann gl\u00fcht es sch\u00f6n und still. So geht es auch im Herzen,", "tokens": ["Dann", "gl\u00fcht", "es", "sch\u00f6n", "und", "still", ".", "So", "geht", "es", "auch", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$.", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn man sich unbedingt der Liebe Zucht ergibt,", "tokens": ["Wenn", "man", "sich", "un\u00b7be\u00b7dingt", "der", "Lie\u00b7be", "Zucht", "er\u00b7gibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dies Feuer l\u00e4utert uns durch wunderliche Schmerzen", "tokens": ["Dies", "Feu\u00b7er", "l\u00e4u\u00b7tert", "uns", "durch", "wun\u00b7der\u00b7li\u00b7che", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und \u00f6fters um und um bis in den Tod betr\u00fcbt.", "tokens": ["Und", "\u00f6f\u00b7ters", "um", "und", "um", "bis", "in", "den", "Tod", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "KON", "APPR", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch ist es Liebe nur, so ", "tokens": ["Doch", "ist", "es", "Lie\u00b7be", "nur", ",", "so"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VAFIN", "PPER", "NN", "ADV", "$,", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Und brennen weg, was ihr im Grunde widerstrebt;", "tokens": ["Und", "bren\u00b7nen", "weg", ",", "was", "ihr", "im", "Grun\u00b7de", "wi\u00b7der\u00b7strebt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "PTKVZ", "$,", "PWS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Gib ihrer Flamme Raum, so wird sie dich verwandeln,", "tokens": ["Gib", "ih\u00b7rer", "Flam\u00b7me", "Raum", ",", "so", "wird", "sie", "dich", "ver\u00b7wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "NN", "$,", "ADV", "VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bis du ganz eins mit Gott, und Gott in dir nur lebt!", "tokens": ["Bis", "du", "ganz", "eins", "mit", "Gott", ",", "und", "Gott", "in", "dir", "nur", "lebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "APPR", "NN", "$,", "KON", "NN", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}