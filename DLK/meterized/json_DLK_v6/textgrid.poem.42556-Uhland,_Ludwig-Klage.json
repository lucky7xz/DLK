{"textgrid.poem.42556": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Klage", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lebendig sein begraben,", "tokens": ["Le\u00b7ben\u00b7dig", "sein", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "VVPP", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Es ist ein schlimmer Stern;", "tokens": ["Es", "ist", "ein", "schlim\u00b7mer", "Stern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch kann man Ungl\u00fcck haben,", "tokens": ["Doch", "kann", "man", "Un\u00b7gl\u00fcck", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "NN", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das jenem nicht zu fern:", "tokens": ["Das", "je\u00b7nem", "nicht", "zu", "fern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn man, bei hei\u00dfem Herzen", "tokens": ["Wenn", "man", ",", "bei", "hei\u00b7\u00dfem", "Her\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und innern Lebens voll,", "tokens": ["Und", "in\u00b7nern", "Le\u00b7bens", "voll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vor K\u00fcmmernis und Schmerzen", "tokens": ["Vor", "K\u00fcm\u00b7mer\u00b7nis", "und", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Fr\u00fchzeitig altern soll.", "tokens": ["Fr\u00fch\u00b7zei\u00b7tig", "al\u00b7tern", "soll", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VMFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}