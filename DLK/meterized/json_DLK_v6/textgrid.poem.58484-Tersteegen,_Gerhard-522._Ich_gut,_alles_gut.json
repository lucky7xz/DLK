{"textgrid.poem.58484": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "522. Ich gut, alles gut", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leg nie die Schuld auf das, was ohne dich geschicht,", "tokens": ["Leg", "nie", "die", "Schuld", "auf", "das", ",", "was", "oh\u00b7ne", "dich", "ge\u00b7schicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "APPR", "ART", "$,", "PRELS", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Klag \u00fcber b\u00f6se Zeit, Welt, Teufel, Menschen nicht!", "tokens": ["Klag", "\u00fc\u00b7ber", "b\u00f6\u00b7se", "Zeit", ",", "Welt", ",", "Teu\u00b7fel", ",", "Men\u00b7schen", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein eignes ungebeugt von Gott entferntes Herze", "tokens": ["Dein", "eig\u00b7nes", "un\u00b7ge\u00b7beugt", "von", "Gott", "ent\u00b7fern\u00b7tes", "Her\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJD", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist selbst die bittre Quell' von Unruh, Plag' und Schmerze;", "tokens": ["Ist", "selbst", "die", "bitt\u00b7re", "Quell'", "von", "Un\u00b7ruh", ",", "Plag'", "und", "Schmer\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dies Herz mu\u00df nur durch Gott grundgut und heilig werden,", "tokens": ["Dies", "Herz", "mu\u00df", "nur", "durch", "Gott", "grund\u00b7gut", "und", "hei\u00b7lig", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VMFIN", "ADV", "APPR", "NN", "ADJD", "KON", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dann findst du auch beim Kreuz den Himmel schon auf Erden.", "tokens": ["Dann", "findst", "du", "auch", "beim", "Kreuz", "den", "Him\u00b7mel", "schon", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}