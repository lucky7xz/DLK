{"textgrid.poem.67335": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Andenken an meinen ersten Todten,", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fch ich einst den Bruder sah", "tokens": ["Fr\u00fch", "ich", "einst", "den", "Bru\u00b7der", "sah"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PPER", "ADV", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem Tod umfangen!", "tokens": ["Mit", "dem", "Tod", "um\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Augen brechend lag er da,", "tokens": ["Au\u00b7gen", "bre\u00b7chend", "lag", "er", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine Rosenwangen", "tokens": ["Sei\u00b7ne", "Ro\u00b7sen\u00b7wan\u00b7gen"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Schon Ebenbild des Todes!", "tokens": ["Schon", "E\u00b7ben\u00b7bild", "des", "To\u00b7des", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im kalten Schwei\u00df, mit kalter Hand,", "tokens": ["Im", "kal\u00b7ten", "Schwei\u00df", ",", "mit", "kal\u00b7ter", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da schon alle Welt ihm schwand,", "tokens": ["Da", "schon", "al\u00b7le", "Welt", "ihm", "schwand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Da sucht', da nannt' er mich!", "tokens": ["Da", "sucht'", ",", "da", "nannt'", "er", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Hob Aengste l\u00e4chelnd sich:", "tokens": ["Hob", "A\u00b7engs\u00b7te", "l\u00e4\u00b7chelnd", "sich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJD", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "\u00bbdu auch, Bruder, Du willst mich verlassen?\u00ab", "tokens": ["\u00bb", "du", "auch", ",", "Bru\u00b7der", ",", "Du", "willst", "mich", "ver\u00b7las\u00b7sen", "?", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "ADV", "$,", "NN", "$,", "PPER", "VMFIN", "PRF", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Ach, starrte mich an,", "tokens": ["Ach", ",", "starr\u00b7te", "mich", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Sank mir in die Arme.", "tokens": ["Sank", "mir", "in", "die", "Ar\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Bruderarmen kam der Tod,", "tokens": ["Bru\u00b7der\u00b7ar\u00b7men", "kam", "der", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Ihn wegzuholen.", "tokens": ["Ihn", "weg\u00b7zu\u00b7ho\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Wo, o s\u00fc\u00dfer Knabe, wo", "tokens": ["Wo", ",", "o", "s\u00fc\u00b7\u00dfer", "Kna\u00b7be", ",", "wo"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "$,", "FM", "ADJA", "NN", "$,", "PWAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Bist Du hingeschieden?", "tokens": ["Bist", "Du", "hin\u00b7ge\u00b7schie\u00b7den", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.17": {"text": "Bl\u00fchtest, Rosenknospe, mir \u2013", "tokens": ["Bl\u00fch\u00b7test", ",", "Ro\u00b7sen\u00b7knos\u00b7pe", ",", "mir", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Nun verwelkt hienieden.", "tokens": ["Nun", "ver\u00b7welkt", "hien\u00b7ie\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "Hienieden nur erschienen", "tokens": ["Hien\u00b7ie\u00b7den", "nur", "er\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Wie Sonnenblick, wie Morgenstrahl", "tokens": ["Wie", "Son\u00b7nen\u00b7blick", ",", "wie", "Mor\u00b7gen\u00b7strahl"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAV", "NN", "$,", "PWAV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "In des Wandrers dunkelm Thal.", "tokens": ["In", "des", "Wand\u00b7rers", "dun\u00b7kelm", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Dein Geist, das Morgenroth!", "tokens": ["Dein", "Geist", ",", "das", "Mor\u00b7gen\u00b7roth", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Dein sch\u00f6nes Herz! \u2013 der Tod", "tokens": ["Dein", "sch\u00f6\u00b7nes", "Herz", "!", "\u2013", "der", "Tod"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "$(", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Hat den Rosenknaben mir zerst\u00f6ret!", "tokens": ["Hat", "den", "Ro\u00b7sen\u00b7kna\u00b7ben", "mir", "zer\u00b7st\u00f6\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "Bist kalt wie das Grab,", "tokens": ["Bist", "kalt", "wie", "das", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.26": {"text": "Todebleicher Erdklo\u00df.", "tokens": ["To\u00b7de\u00b7blei\u00b7cher", "Erd\u00b7klo\u00df", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.27": {"text": "Ach, des Lebens sanfter Strom", "tokens": ["Ach", ",", "des", "Le\u00b7bens", "sanf\u00b7ter", "Strom"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Ist starr erloschen.", "tokens": ["Ist", "starr", "er\u00b7lo\u00b7schen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.29": {"text": "Kalter Knabe! Bruder, nicht", "tokens": ["Kal\u00b7ter", "Kna\u00b7be", "!", "Bru\u00b7der", ",", "nicht"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ADJA", "NN", "$.", "NN", "$,", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Ewig mir verloren!", "tokens": ["E\u00b7wig", "mir", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.31": {"text": "Holdes Wahnbild! ach, wozu,", "tokens": ["Hol\u00b7des", "Wahn\u00b7bild", "!", "ach", ",", "wo\u00b7zu", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$.", "XY", "$,", "PWAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Wozu schmerzgeboren", "tokens": ["Wo\u00b7zu", "schmerz\u00b7ge\u00b7bo\u00b7ren"], "token_info": ["word", "word"], "pos": ["PWAV", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.33": {"text": "Hier auf die Schattenerde?", "tokens": ["Hier", "auf", "die", "Schat\u00b7te\u00b7ner\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Auf meinen Knieen flossen Dir", "tokens": ["Auf", "mei\u00b7nen", "Kni\u00b7e\u00b7en", "flos\u00b7sen", "Dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.35": {"text": "Fr\u00fche zarte Thr\u00e4nen hier!", "tokens": ["Fr\u00fc\u00b7he", "zar\u00b7te", "Thr\u00e4\u00b7nen", "hier", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Wozu sind sie verweint?", "tokens": ["Wo\u00b7zu", "sind", "sie", "ver\u00b7weint", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "Du Traumbild! Schattenfreund!", "tokens": ["Du", "Traum\u00b7bild", "!", "Schat\u00b7ten\u00b7freund", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "$.", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.38": {"text": "Schattenrose, mir nur vorgespiegelt!", "tokens": ["Schat\u00b7ten\u00b7ro\u00b7se", ",", "mir", "nur", "vor\u00b7ge\u00b7spie\u00b7gelt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.39": {"text": "Wirst Erde bald sein \u2013", "tokens": ["Wirst", "Er\u00b7de", "bald", "sein", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "VAINF", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.40": {"text": "Handvoll stumme Erde!", "tokens": ["Hand\u00b7voll", "stum\u00b7me", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.41": {"text": "Gott, o Gott, wie tr\u00fcgst Du uns", "tokens": ["Gott", ",", "o", "Gott", ",", "wie", "tr\u00fcgst", "Du", "uns"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "FM", "NN", "$,", "PWAV", "VVFIN", "PPER", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.42": {"text": "Mit Wonn' im Leben.", "tokens": ["Mit", "Wonn'", "im", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.43": {"text": "Lebenswonn' und alle Lust,", "tokens": ["Le\u00b7bens\u00b7wonn'", "und", "al\u00b7le", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KON", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.44": {"text": "Nichts ist selbst das Leben!", "tokens": ["Nichts", "ist", "selbst", "das", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.45": {"text": "Schatten auf den Wogen her", "tokens": ["Schat\u00b7ten", "auf", "den", "Wo\u00b7gen", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "APZR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.46": {"text": "Kommen wir und schweben \u2013", "tokens": ["Kom\u00b7men", "wir", "und", "schwe\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.47": {"text": "Wohin? \u2013 Ach, holder Knabe!", "tokens": ["Wo\u00b7hin", "?", "\u2013", "Ach", ",", "hol\u00b7der", "Kna\u00b7be", "!"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$.", "$(", "ITJ", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.48": {"text": "Sie sangen Dir in Todespein,", "tokens": ["Sie", "san\u00b7gen", "Dir", "in", "To\u00b7de\u00b7spein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Sangen Labungston Dir ein:", "tokens": ["San\u00b7gen", "La\u00b7bungs\u00b7ton", "Dir", "ein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.50": {"text": "\u00bbzu Christ, dem Bruder mein,", "tokens": ["\u00bb", "zu", "Christ", ",", "dem", "Bru\u00b7der", "mein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "$,", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.51": {"text": "Zum Himmel schlaf' ich ein!\u00ab", "tokens": ["Zum", "Him\u00b7mel", "schlaf'", "ich", "ein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.52": {"text": "Da ri\u00df sich sein letzter Blick gen Himmel.", "tokens": ["Da", "ri\u00df", "sich", "sein", "letz\u00b7ter", "Blick", "gen", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.53": {"text": "Wo wandelst Du nun?", "tokens": ["Wo", "wan\u00b7delst", "Du", "nun", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.54": {"text": "Selige Erscheinung,", "tokens": ["Se\u00b7li\u00b7ge", "Er\u00b7schei\u00b7nung", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.55": {"text": "Kommst Du, wenn mein Blick einst bricht,", "tokens": ["Kommst", "Du", ",", "wenn", "mein", "Blick", "einst", "bricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.56": {"text": "Mich heimzuholen?", "tokens": ["Mich", "heim\u00b7zu\u00b7ho\u00b7len", "?"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}