{"textgrid.poem.57958": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Die \u00dcberschule", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist die neue T\u00f6chterschule,", "tokens": ["Das", "ist", "die", "neu\u00b7e", "T\u00f6ch\u00b7ter\u00b7schu\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie kostet nur 'ne Million,", "tokens": ["Sie", "kos\u00b7tet", "nur", "'ne", "Mil\u00b7li\u00b7on", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Anbetracht des hohen Zweckes", "tokens": ["In", "An\u00b7be\u00b7tracht", "des", "ho\u00b7hen", "Zwe\u00b7ckes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist das ein wahrer S\u00fcndenlohn.", "tokens": ["Ist", "das", "ein", "wah\u00b7rer", "S\u00fcn\u00b7den\u00b7lohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn was da lernt, ist hoher Adel,", "tokens": ["Denn", "was", "da", "lernt", ",", "ist", "ho\u00b7her", "A\u00b7del", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVFIN", "$,", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn nicht F.-K.-Kartell S.-C.,", "tokens": ["Wenn", "nicht", "F.", "K.", "S.", "C.", ","], "token_info": ["word", "word", "abbreviation", "word", "abbreviation", "word", "abbreviation", "word", "abbreviation", "punct"], "pos": ["KOUS", "PTKNEG", "NE", "$(", "NE", "NE", "NN", "$(", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und wenn auch das nicht, im Besitze", "tokens": ["Und", "wenn", "auch", "das", "nicht", ",", "im", "Be\u00b7sit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "ADV", "PDS", "PTKNEG", "$,", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Von einem \u00dcberportemonnaie.", "tokens": ["Von", "ei\u00b7nem", "\u00dc\u00b7ber\u00b7por\u00b7te\u00b7mon\u00b7nai\u00b7e", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das ist die Creme, ist die Elite,", "tokens": ["Das", "ist", "die", "Cre\u00b7me", ",", "ist", "die", "E\u00b7li\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Flott von junger Weiblichkeit,", "tokens": ["Der", "Flott", "von", "jun\u00b7ger", "Weib\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und viel zu einfach ist die Schule,", "tokens": ["Und", "viel", "zu", "ein\u00b7fach", "ist", "die", "Schu\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKA", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die solchem hohen Zweck geweiht.", "tokens": ["Die", "sol\u00b7chem", "ho\u00b7hen", "Zweck", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Zehn Millionen w\u00e4ren n\u00f6tig", "tokens": ["Zehn", "Mil\u00b7lion\u00b7en", "w\u00e4\u00b7ren", "n\u00f6\u00b7tig"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "VAFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gewesen f\u00fcr ein solches Haus.", "tokens": ["Ge\u00b7we\u00b7sen", "f\u00fcr", "ein", "sol\u00b7ches", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn f\u00fcr die h\u00f6chsten h\u00f6h'ren T\u00f6chter", "tokens": ["Denn", "f\u00fcr", "die", "h\u00f6chs\u00b7ten", "h\u00f6h'\u00b7ren", "T\u00f6ch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Reicht nur 'ne hohe Summe aus.", "tokens": ["Reicht", "nur", "'ne", "ho\u00b7he", "Sum\u00b7me", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}