{"textgrid.poem.25620": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "Als eine Buhlerin die Rolle der Aemilia Galotti spielte", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie t\u00e4uscht ihr k\u00fcnstlich Spiel! Glaubt doch fast jedermann,", "tokens": ["Wie", "t\u00e4uscht", "ihr", "k\u00fcnst\u00b7lich", "Spiel", "!", "Glaubt", "doch", "fast", "je\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "NN", "$.", "VVFIN", "ADV", "ADV", "PIS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df alle Dinge wirklich so geschehen.", "tokens": ["Da\u00df", "al\u00b7le", "Din\u00b7ge", "wirk\u00b7lich", "so", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch b\u00f6t' ihr in der That ein Prinz so etwas an:", "tokens": ["Doch", "b\u00f6t'", "ihr", "in", "der", "That", "ein", "Prinz", "so", "et\u00b7was", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "ADV", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ei! ei! da w\u00fcrden wir ein artig Schauspiel sehen.", "tokens": ["Ei", "!", "ei", "!", "da", "w\u00fcr\u00b7den", "wir", "ein", "ar\u00b7tig", "Schau\u00b7spiel", "se\u00b7hen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN",