{"dta.poem.19907": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Des  \n  Sch\u00e4fers Liebeswerbung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Kom, bis mein Liebchen, bis mein Weib!               ", "tokens": ["Kom", ",", "bis", "mein", "Lieb\u00b7chen", ",", "bis", "mein", "Weib", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und fodre Lust und Zeitvertreib,", "tokens": ["Und", "fod\u00b7re", "Lust", "und", "Zeit\u00b7ver\u00b7treib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So oft und viel dein Herz begehrt,", "tokens": ["So", "oft", "und", "viel", "dein", "Herz", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Garten, Flur, und Hain gew\u00e4rt.", "tokens": ["Und", "Gar\u00b7ten", ",", "Flur", ",", "und", "Hain", "ge\u00b7w\u00e4rt", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "$,", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Bald wollen wir von freien H\u00f6hn", "tokens": ["Bald", "wol\u00b7len", "wir", "von", "frei\u00b7en", "H\u00f6hn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Rund um die Heerden weiden sehn,", "tokens": ["Rund", "um", "die", "Heer\u00b7den", "wei\u00b7den", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "VVINF", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und sehn der L\u00e4mmer Fr\u00f6lichkeit,", "tokens": ["Und", "sehn", "der", "L\u00e4m\u00b7mer", "Fr\u00f6\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und junger Stiere H\u00f6rnerstreit;", "tokens": ["Und", "jun\u00b7ger", "Stie\u00b7re", "H\u00f6r\u00b7ner\u00b7streit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}