{"dta.poem.17967": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "210.  Was man innwendig ist sucht man  \n nicht au\u00dfwendig.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Mann/ wer in Tugenden von innen Reich und sch\u00f6n/", "tokens": ["Mann", "/", "wer", "in", "Tu\u00b7gen\u00b7den", "von", "in\u00b7nen", "Reich", "und", "sch\u00f6n", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PWS", "APPR", "NN", "APPR", "ADJA", "NN", "KON", "ADJD", "$("], "meter": "+--+---+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Der wird von aussen nicht nach Schmuck und Reichthum", "tokens": ["Der", "wird", "von", "aus\u00b7sen", "nicht", "nach", "Schmuck", "und", "Reicht\u00b7hum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "NE", "PTKNEG", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "stehn.", "tokens": ["stehn", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}}}}}