{"textgrid.poem.26667": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Zuversicht der Unsterblichkeit", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum neuen Leben ist der Tote hier erstanden,", "tokens": ["Zum", "neu\u00b7en", "Le\u00b7ben", "ist", "der", "To\u00b7te", "hier", "er\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das wei\u00df und glaub ich festiglich.", "tokens": ["Das", "wei\u00df", "und", "glaub", "ich", "fes\u00b7tig\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich lehrens schon die ", "tokens": ["Mich", "leh\u00b7rens", "schon", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "ART"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}}}}