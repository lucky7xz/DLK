{"textgrid.poem.53281": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lasset vns embsig Gott den Herren bitten", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lasset vns embsig Gott den Herren bitten", "tokens": ["Las\u00b7set", "vns", "emb\u00b7sig", "Gott", "den", "Her\u00b7ren", "bit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df wir bey zeiten diese Leibes-H\u00fctten", "tokens": ["Da\u00df", "wir", "bey", "zei\u00b7ten", "die\u00b7se", "Lei\u00b7bes\u00b7H\u00fct\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "M\u00f6gen ablegen, vnd aus diesem Leyden", "tokens": ["M\u00f6\u00b7gen", "ab\u00b7le\u00b7gen", ",", "vnd", "aus", "die\u00b7sem", "Ley\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "$,", "KON", "APPR", "PDAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Selig abscheiden,", "tokens": ["Se\u00b7lig", "ab\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Da\u00df wir gelangen in die Zahl der Frommen,", "tokens": ["Da\u00df", "wir", "ge\u00b7lan\u00b7gen", "in", "die", "Zahl", "der", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vnd da die wahre Lebens-Krafft bekommen,", "tokens": ["Vnd", "da", "die", "wah\u00b7re", "Le\u00b7bens\u00b7Krafft", "be\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Welche mit Kranckheit vns nicht mehr beleget", "tokens": ["Wel\u00b7che", "mit", "Kran\u00b7ck\u00b7heit", "vns", "nicht", "mehr", "be\u00b7le\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAT", "APPR", "NN", "PPER", "PTKNEG", "ADV", "VVFIN"], "meter": "+--+--+-+-++", "measure": "dactylic.di.plus"}, "line.4": {"text": "Noch Jammer heget.", "tokens": ["Noch", "Jam\u00b7mer", "he\u00b7get", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Da wir zugleich so wie die Engel singen,", "tokens": ["Da", "wir", "zu\u00b7gleich", "so", "wie", "die", "En\u00b7gel", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da so viel Seiten ohn Auffh\u00f6ren klingen,", "tokens": ["Da", "so", "viel", "Sei\u00b7ten", "ohn", "Auff\u00b7h\u00f6\u00b7ren", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da vns nur Reichthumb, Lust vnd fr\u00f6lich Leben", "tokens": ["Da", "vns", "nur", "Reicht\u00b7humb", ",", "Lust", "vnd", "fr\u00f6\u00b7lich", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "NN", "$,", "NN", "KON", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "M\u00fcssen vmbgeben.", "tokens": ["M\u00fcs\u00b7sen", "vmb\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "David ertichtet noch da sch\u00f6ne Lieder,", "tokens": ["Da\u00b7vid", "er\u00b7tich\u00b7tet", "noch", "da", "sch\u00f6\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Singt wie vns Christus, seine Freund vnd Br\u00fcder,", "tokens": ["Singt", "wie", "vns", "Chris\u00b7tus", ",", "sei\u00b7ne", "Freund", "vnd", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "PPER", "NE", "$,", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch sein Verdienst von S\u00fcnd vnd allem B\u00f6sen", "tokens": ["Durch", "sein", "Ver\u00b7dienst", "von", "S\u00fcnd", "vnd", "al\u00b7lem", "B\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "KON", "PIS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wollen erl\u00f6sen.", "tokens": ["Wol\u00b7len", "er\u00b7l\u00f6\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Bleibt so der Herr der S\u00e4nger vnd Poeten,", "tokens": ["Bleibt", "so", "der", "Herr", "der", "S\u00e4n\u00b7ger", "vnd", "Po\u00b7et\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihm folgen nach die S\u00e4nger vnd Propheten,", "tokens": ["Ihm", "fol\u00b7gen", "nach", "die", "S\u00e4n\u00b7ger", "vnd", "Pro\u00b7phe\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Saal des Himmels mu\u00df von solchem allen", "tokens": ["Der", "Saal", "des", "Him\u00b7mels", "mu\u00df", "von", "sol\u00b7chem", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VMFIN", "APPR", "PIAT", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Starck widerschallen.", "tokens": ["Starck", "wi\u00b7der\u00b7schal\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Die Schaar der Seraphin vnd Cherubinen", "tokens": ["Die", "Schaar", "der", "Se\u00b7ra\u00b7phin", "vnd", "Che\u00b7ru\u00b7bi\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "M\u00fcssen Gott auch mit flug vnd Stimme dienen,", "tokens": ["M\u00fcs\u00b7sen", "Gott", "auch", "mit", "flug", "vnd", "Stim\u00b7me", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Aber wenn sie den Trometen Thon erheben", "tokens": ["A\u00b7ber", "wenn", "sie", "den", "Tro\u00b7me\u00b7ten", "Thon", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ART", "ADJA", "NN", "VVINF"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Mu\u00df alles beben.", "tokens": ["Mu\u00df", "al\u00b7les", "be\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Die Schwell vnd Balcken m\u00fcssen sich erschittern,", "tokens": ["Die", "Schwell", "vnd", "Bal\u00b7cken", "m\u00fcs\u00b7sen", "sich", "er\u00b7schit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die W\u00e4nd' vnd Pfeiler allerseit erzittern,", "tokens": ["Die", "W\u00e4nd'", "vnd", "Pfei\u00b7ler", "al\u00b7ler\u00b7seit", "er\u00b7zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Rauch, Dampff vnd Nebel mu\u00df das Hau\u00df erf\u00fcllen", "tokens": ["Rauch", ",", "Dampff", "vnd", "Ne\u00b7bel", "mu\u00df", "das", "Hau\u00df", "er\u00b7f\u00fcl\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gott zu verh\u00fcllen.", "tokens": ["Gott", "zu", "ver\u00b7h\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "So herrlich geht es zu vor Gottes Throne,", "tokens": ["So", "herr\u00b7lich", "geht", "es", "zu", "vor", "Got\u00b7tes", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PTKZU", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hie bleibt man jmmer bey dem Jammer-Thone,", "tokens": ["Hie", "bleibt", "man", "jm\u00b7mer", "bey", "dem", "Jam\u00b7mer\u00b7Tho\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df vns, Herr, zeitig von der Welt entwehnen,", "tokens": ["La\u00df", "vns", ",", "Herr", ",", "zei\u00b7tig", "von", "der", "Welt", "ent\u00b7weh\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vnd dort hin sehnen.", "tokens": ["Vnd", "dort", "hin", "seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Endlich f\u00fchr vns auch zu den Frommen Schaaren,", "tokens": ["End\u00b7lich", "f\u00fchr", "vns", "auch", "zu", "den", "From\u00b7men", "Schaa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "La\u00df vns mit Glauben wol versehn hinfahren,", "tokens": ["La\u00df", "vns", "mit", "Glau\u00b7ben", "wol", "ver\u00b7sehn", "hin\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Damit auch wir dich in dem Himmel droben", "tokens": ["Da\u00b7mit", "auch", "wir", "dich", "in", "dem", "Him\u00b7mel", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "PPER", "PRF", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ewiglich loben.", "tokens": ["E\u00b7wig\u00b7lich", "lo\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}