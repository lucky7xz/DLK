{"dta.poem.3313": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die Trinker.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Mit Narren sollt ich mich erfreun?", "tokens": ["Mit", "Nar\u00b7ren", "sollt", "ich", "mich", "er\u00b7freun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr Wein schmeckt ekelhaft gemein,", "tokens": ["Ihr", "Wein", "schmeckt", "e\u00b7kel\u00b7haft", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Wasser, das die Musen scheuchet;", "tokens": ["Wie", "Was\u00b7ser", ",", "das", "die", "Mu\u00b7sen", "scheu\u00b7chet", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und w\u00e4r es auch der beste Wein,", "tokens": ["Und", "w\u00e4r", "es", "auch", "der", "bes\u00b7te", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der an der Mosel bleichet.", "tokens": ["Der", "an", "der", "Mo\u00b7sel", "blei\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Kann ich mit Klugen mich erfreun;", "tokens": ["Kann", "ich", "mit", "Klu\u00b7gen", "mich", "er\u00b7freun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So schmeckt auch Wasser ungemein", "tokens": ["So", "schmeckt", "auch", "Was\u00b7ser", "un\u00b7ge\u00b7mein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und gleich burgundischem Ly\u00e4en.", "tokens": ["Und", "gleich", "bur\u00b7gun\u00b7di\u00b7schem", "Ly\u00b7\u00e4\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch, Freunde! seht, wir haben Wein!", "tokens": ["Doch", ",", "Freun\u00b7de", "!", "seht", ",", "wir", "ha\u00b7ben", "Wein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$.", "VVFIN", "$,", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer wollte Wein verschm\u00e4hen?", "tokens": ["Wer", "woll\u00b7te", "Wein", "ver\u00b7schm\u00e4\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es m\u00fcsse k\u00fchne V\u00f6llerey", "tokens": ["Es", "m\u00fcs\u00b7se", "k\u00fch\u00b7ne", "V\u00f6l\u00b7le\u00b7rey"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht, unter b\u00e4urischem Geschrey,", "tokens": ["Nicht", ",", "un\u00b7ter", "b\u00e4u\u00b7ri\u00b7schem", "Ge\u00b7schrey", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit ihrem Thyrsus hier gebiethen!", "tokens": ["Mit", "ih\u00b7rem", "Thyr\u00b7sus", "hier", "ge\u00b7bie\u00b7then", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O Bacchus! gehe still vorbey,", "tokens": ["O", "Bac\u00b7chus", "!", "ge\u00b7he", "still", "vor\u00b7bey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und rase bey den Scythen!", "tokens": ["Und", "ra\u00b7se", "bey", "den", "Scyt\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie f\u00fcrcht' ich deinen trunknen Blick!", "tokens": ["Wie", "f\u00fcrcht'", "ich", "dei\u00b7nen", "trunk\u00b7nen", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie droht manch fliegend Felsenst\u00fcck!", "tokens": ["Wie", "droht", "manch", "flie\u00b7gend", "Fel\u00b7sen\u00b7st\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seh ich die w\u00fcthende M\u00e4nade?", "tokens": ["Seh", "ich", "die", "w\u00fct\u00b7hen\u00b7de", "M\u00e4\u00b7na\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+---+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Welch rauher Jubel br\u00fcllt zur\u00fcck", "tokens": ["Welch", "rau\u00b7her", "Ju\u00b7bel", "br\u00fcllt", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vom Thrazischen Gestade!", "tokens": ["Vom", "Thra\u00b7zi\u00b7schen", "Ge\u00b7sta\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Trinkt nicht, von wilder Lust entbrannt,", "tokens": ["Trinkt", "nicht", ",", "von", "wil\u00b7der", "Lust", "ent\u00b7brannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bis an des Rausches welker Hand", "tokens": ["Bis", "an", "des", "Rau\u00b7sches", "wel\u00b7ker", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "PWAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der blinde Bacchus taumlend schleichet!", "tokens": ["Der", "blin\u00b7de", "Bac\u00b7chus", "taum\u00b7lend", "schlei\u00b7chet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sonst flieh ich schneller, als der Sand", "tokens": ["Sonst", "flieh", "ich", "schnel\u00b7ler", ",", "als", "der", "Sand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vom Wirbelwind entweichet.", "tokens": ["Vom", "Wir\u00b7bel\u00b7wind", "ent\u00b7wei\u00b7chet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}