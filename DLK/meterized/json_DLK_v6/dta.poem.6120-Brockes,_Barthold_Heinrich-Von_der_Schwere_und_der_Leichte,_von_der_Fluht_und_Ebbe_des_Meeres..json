{"dta.poem.6120": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Schwere und der Leichte,  \n von der Fluht und Ebbe  \n des Meeres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allein, durch welche Macht wird doch das Meer beweget,", "tokens": ["Al\u00b7lein", ",", "durch", "wel\u00b7che", "Macht", "wird", "doch", "das", "Meer", "be\u00b7we\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PWAT", "NN", "VAFIN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann wir es seinen Strand verschlingen sehn,", "tokens": ["Wann", "wir", "es", "sei\u00b7nen", "Strand", "ver\u00b7schlin\u00b7gen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "PPOSAT", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und da\u00df, wie schwer es gleich, durch neuen Druck erreget,", "tokens": ["Und", "da\u00df", ",", "wie", "schwer", "es", "gleich", ",", "durch", "neu\u00b7en", "Druck", "er\u00b7re\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "PWAV", "ADJD", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die saltzen Fluhten sich erh\u00f6hn,", "tokens": ["Die", "salt\u00b7zen", "Fluh\u00b7ten", "sich", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So da\u00df es scheint, als ob aus seinem Bette", "tokens": ["So", "da\u00df", "es", "scheint", ",", "als", "ob", "aus", "sei\u00b7nem", "Bet\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "VVFIN", "$,", "KOKOM", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Man es geworffen h\u00e4tte?", "tokens": ["Man", "es", "ge\u00b7worf\u00b7fen", "h\u00e4t\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}