{"textgrid.poem.46668": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ueber alle Gr\u00e4ber w\u00e4chst zuletzt das Gras]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ueber alle Gr\u00e4ber w\u00e4chst zuletzt das Gras,", "tokens": ["Ue\u00b7ber", "al\u00b7le", "Gr\u00e4\u00b7ber", "w\u00e4chst", "zu\u00b7letzt", "das", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Alle Wunden heilt die Zeit, ein Trost ist das,", "tokens": ["Al\u00b7le", "Wun\u00b7den", "heilt", "die", "Zeit", ",", "ein", "Trost", "ist", "das", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VAFIN", "PDS", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Wohl der schlechteste, den man dir kann ertheilen;", "tokens": ["Wohl", "der", "schlech\u00b7tes\u00b7te", ",", "den", "man", "dir", "kann", "er\u00b7thei\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "$,", "PRELS", "PIS", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Armes Herz, du willst nicht, da\u00df die Wunden heilen.", "tokens": ["Ar\u00b7mes", "Herz", ",", "du", "willst", "nicht", ",", "da\u00df", "die", "Wun\u00b7den", "hei\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VMFIN", "PTKNEG", "$,", "KOUS", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Etwas hast du noch, solang es schmerzlich brennt;", "tokens": ["Et\u00b7was", "hast", "du", "noch", ",", "so\u00b7lang", "es", "schmerz\u00b7lich", "brennt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Das Verschmerzte nur ist todt und abgetrennt.", "tokens": ["Das", "Ver\u00b7schmerz\u00b7te", "nur", "ist", "todt", "und", "ab\u00b7ge\u00b7trennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD", "KON", "VVPP", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}}}}}