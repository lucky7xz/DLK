{"dta.poem.20751": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Halberstadt, den 18. Februar 1762.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Geist und mein Gef\u00fchl sind die beflammte Ode,                 ", "tokens": ["Mein", "Geist", "und", "mein", "Ge\u00b7f\u00fchl", "sind", "die", "be\u00b7flamm\u00b7te", "O\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ausser mir sich schwingt,", "tokens": ["Die", "aus\u00b7ser", "mir", "sich", "schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Freundschaft, Z\u00e4rtlichkeit, von Krieg und Helden,", "tokens": ["Von", "Freund\u00b7schaft", ",", "Z\u00e4rt\u00b7lich\u00b7keit", ",", "von", "Krieg", "und", "Hel\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Tode", "tokens": ["To\u00b7de"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Und von dem Himmel singt.", "tokens": ["Und", "von", "dem", "Him\u00b7mel", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wenn ich als S\u00e4ngerin ein Monument mir stifte,", "tokens": ["Wenn", "ich", "als", "S\u00e4n\u00b7ge\u00b7rin", "ein", "Mo\u00b7nu\u00b7ment", "mir", "stif\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So scheu ich nicht die letzte Bahn,", "tokens": ["So", "scheu", "ich", "nicht", "die", "letz\u00b7te", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Welt bleibt unter mir, ich flieg in hohe L\u00fcfte,", "tokens": ["Die", "Welt", "bleibt", "un\u00b7ter", "mir", ",", "ich", "flieg", "in", "ho\u00b7he", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und singe Thyrsis, dir, als Schwan.", "tokens": ["Und", "sin\u00b7ge", "Thyr\u00b7sis", ",", "dir", ",", "als", "Schwan", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "$,", "PPER", "$,", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}