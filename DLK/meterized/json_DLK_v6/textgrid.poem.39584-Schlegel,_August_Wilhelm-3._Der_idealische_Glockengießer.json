{"textgrid.poem.39584": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "3. Der idealische Glockengie\u00dfer", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht Zinn und Kupfer, nach gemeiner Weise,", "tokens": ["Nicht", "Zinn", "und", "Kup\u00b7fer", ",", "nach", "ge\u00b7mei\u00b7ner", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nein, Wortgepr\u00e4ng' und Reim, m\u00fchsam in eins verschmelzt,", "tokens": ["Nein", ",", "Wort\u00b7ge\u00b7pr\u00e4ng'", "und", "Reim", ",", "m\u00fch\u00b7sam", "in", "eins", "ver\u00b7schmelzt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "KON", "NN", "$,", "ADJD", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bis sich die z\u00e4he Mass' in Strophen weiter w\u00e4lzt:", "tokens": ["Bis", "sich", "die", "z\u00e4\u00b7he", "Mass'", "in", "Stro\u00b7phen", "wei\u00b7ter", "w\u00e4lzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "ADJA", "NN", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das ist im Glockenlied die edle Glockenspeise.", "tokens": ["Das", "ist", "im", "Glo\u00b7cken\u00b7lied", "die", "ed\u00b7le", "Glo\u00b7cken\u00b7spei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}