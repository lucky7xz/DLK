{"dta.poem.11848": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wollte, w\u00e4r' ich reich, viel lieber als den Streit", "tokens": ["Ich", "woll\u00b7te", ",", "w\u00e4r'", "ich", "reich", ",", "viel", "lie\u00b7ber", "als", "den", "Streit"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "VAFIN", "PPER", "ADJD", "$,", "ADV", "ADV", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um nichts wan nicht'ges Gut zu h\u00f6ren weit und breit,", "tokens": ["Um", "nichts", "wan", "nicht'\u00b7ges", "Gut", "zu", "h\u00f6\u00b7ren", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIS", "PWAV", "PIS", "ADJD", "PTKZU", "VVINF", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mein eignes geben hin und sagen: Nehmt und theilt!", "tokens": ["Mein", "eig\u00b7nes", "ge\u00b7ben", "hin", "und", "sa\u00b7gen", ":", "Nehmt", "und", "theilt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "PTKVZ", "KON", "VVINF", "$.", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch w\u00fcrde so der Streit gemehrt nur, nicht geheilt.", "tokens": ["Doch", "w\u00fcr\u00b7de", "so", "der", "Streit", "ge\u00b7mehrt", "nur", ",", "nicht", "ge\u00b7heilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "