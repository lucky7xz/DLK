{"textgrid.poem.32396": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "74. Auf den Fabull", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fabull verschlie\u00dfet alle Kisten", "tokens": ["Fa\u00b7bull", "ver\u00b7schlie\u00b7\u00dfet", "al\u00b7le", "Kis\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PIAT", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Vor Freunden, Dienern, Weib und Kind,", "tokens": ["Vor", "Freun\u00b7den", ",", "Die\u00b7nern", ",", "Weib", "und", "Kind", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Damit sich niemand l\u00e4\u00dft gel\u00fcsten", "tokens": ["Da\u00b7mit", "sich", "nie\u00b7mand", "l\u00e4\u00dft", "ge\u00b7l\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PIS", "VVFIN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu sehen, da\u00df sie ledig sind.", "tokens": ["Zu", "se\u00b7hen", ",", "da\u00df", "sie", "le\u00b7dig", "sind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}