{"dta.poem.11793": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "79.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das wei\u00dfe Licht ist leicht, das dunkle Schwarz ist schwer;", "tokens": ["Das", "wei\u00b7\u00dfe", "Licht", "ist", "leicht", ",", "das", "dunk\u00b7le", "Schwarz", "ist", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,", "ART", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Schwer' und Leichte wiegt sich alles Wesenheer;", "tokens": ["In", "Schwer'", "und", "Leich\u00b7te", "wiegt", "sich", "al\u00b7les", "We\u00b7sen\u00b7heer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VVFIN", "PRF", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie zwischen Wei\u00df und Schwarz schwankt alle Schaar der Farben,", "tokens": ["Wie", "zwi\u00b7schen", "Wei\u00df", "und", "Schwarz", "schwankt", "al\u00b7le", "Schaar", "der", "Far\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "KON", "NE", "VVFIN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die so Geburt als Tod von beiden stets erwarben.", "tokens": ["Die", "so", "Ge\u00b7burt", "als", "Tod", "von", "bei\u00b7den", "stets", "er\u00b7war\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "KOUS", "NN", "APPR", "PIAT", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das Licht ist Leben nicht allein, auch Todeshauch,", "tokens": ["Das", "Licht", "ist", "Le\u00b7ben", "nicht", "al\u00b7lein", ",", "auch", "To\u00b7des\u00b7hauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "PTKNEG", "ADV", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Nacht nicht Tod allein, ist Lebensmutter auch.", "tokens": ["Die", "Nacht", "nicht", "Tod", "al\u00b7lein", ",", "ist", "Le\u00b7bens\u00b7mut\u00b7ter", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "NN", "ADV", "$,", "VAFIN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Vater ist das Licht, der stets erzeugt die Farben,", "tokens": ["Der", "Va\u00b7ter", "ist", "das", "Licht", ",", "der", "stets", "er\u00b7zeugt", "die", "Far\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Todesengel dann, von dessen Ku\u00df sie starben.", "tokens": ["Der", "To\u00b7de\u00b7sen\u00b7gel", "dann", ",", "von", "des\u00b7sen", "Ku\u00df", "sie", "star\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "APPR", "PRELAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die Mutter ist die Nacht, die stets gebirt die Farben,", "tokens": ["Die", "Mut\u00b7ter", "ist", "die", "Nacht", ",", "die", "stets", "ge\u00b7birt", "die", "Far\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dann ist sie das Grab, in der sie Ruh erwarben.", "tokens": ["Und", "dann", "ist", "sie", "das", "Grab", ",", "in", "der", "sie", "Ruh", "er\u00b7war\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Was von der Mutter kam, kehrt in der Mutter Scho\u00df,", "tokens": ["Was", "von", "der", "Mut\u00b7ter", "kam", ",", "kehrt", "in", "der", "Mut\u00b7ter", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Weil, was den Ursprung nahm vom Vater, zu ihm flo\u00df.", "tokens": ["Weil", ",", "was", "den", "Ur\u00b7sprung", "nahm", "vom", "Va\u00b7ter", ",", "zu", "ihm", "flo\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "ART", "NN", "VVFIN", "APPRART", "NN", "$,", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}