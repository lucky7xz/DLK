{"textgrid.poem.60755": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es lag ein W\u00fcterich auf goldnem Kissen,", "genre": "verse", "period": "N.A.", "pub_year": 1832, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es lag ein W\u00fcterich auf goldnem Kissen,", "tokens": ["Es", "lag", "ein", "W\u00fc\u00b7te\u00b7rich", "auf", "gold\u00b7nem", "Kis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und schlief; da kamen f\u00fcrchterliche Tr\u00e4ume", "tokens": ["Und", "schlief", ";", "da", "ka\u00b7men", "f\u00fcrch\u00b7ter\u00b7li\u00b7che", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihm ins Gem\u00fct, Gleich wilden Schlangenbissen:", "tokens": ["Ihm", "ins", "Ge\u00b7m\u00fct", ",", "Gleich", "wil\u00b7den", "Schlan\u00b7gen\u00b7bis\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie f\u00fchrten ihn in au\u00dferirdische R\u00e4ume,", "tokens": ["Sie", "f\u00fchr\u00b7ten", "ihn", "in", "au\u00b7\u00dfer\u00b7ir\u00b7di\u00b7sche", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Vom Reich der Geister f\u00fchlt er sich umfangen,", "tokens": ["Vom", "Reich", "der", "Geis\u00b7ter", "f\u00fchlt", "er", "sich", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das ewig klar und ohne Wolkens\u00e4ume:", "tokens": ["Das", "e\u00b7wig", "klar", "und", "oh\u00b7ne", "Wol\u00b7ken\u00b7s\u00e4u\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJD", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Entsetzlich war ihm, was die Geister sangen,", "tokens": ["Ent\u00b7setz\u00b7lich", "war", "ihm", ",", "was", "die", "Geis\u00b7ter", "san\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie einst Tarquin vom Brutus ward vertrieben,", "tokens": ["Wie", "einst", "Tar\u00b7quin", "vom", "Bru\u00b7tus", "ward", "ver\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NE", "APPRART", "NE", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und wie Hipparchos nicht dem Tod entgangen.", "tokens": ["Und", "wie", "Hip\u00b7par\u00b7chos", "nicht", "dem", "Tod", "ent\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NE", "PTKNEG", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und solche Frevler wagt man hier zu lieben,", "tokens": ["Und", "sol\u00b7che", "Frev\u00b7ler", "wagt", "man", "hier", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PIS", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "So denkt er bei sich selbst, wo ist die Achtung", "tokens": ["So", "denkt", "er", "bei", "sich", "selbst", ",", "wo", "ist", "die", "Ach\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PRF", "ADV", "$,", "PWAV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "F\u00fcr jeden Machtspruch, den ich ausgeschrieben?", "tokens": ["F\u00fcr", "je\u00b7den", "Machts\u00b7pruch", ",", "den", "ich", "aus\u00b7ge\u00b7schrie\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Was will die Sonne hier, da l\u00e4ngst Umnachtung", "tokens": ["Was", "will", "die", "Son\u00b7ne", "hier", ",", "da", "l\u00e4ngst", "Um\u00b7nach\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ART", "NN", "ADV", "$,", "KOUS", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Ich \u00fcbern Horizont der Welt verbreitet,", "tokens": ["Ich", "\u00fc\u00b7bern", "Ho\u00b7ri\u00b7zont", "der", "Welt", "ver\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Wo Jeder kniet vor mir in Selbstverachtung?", "tokens": ["Wo", "Je\u00b7der", "kniet", "vor", "mir", "in", "Selbst\u00b7ver\u00b7ach\u00b7tung", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "APPR", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und sieh, ein Mann mit hoher Stirne schreitet", "tokens": ["Und", "sieh", ",", "ein", "Mann", "mit", "ho\u00b7her", "Stir\u00b7ne", "schrei\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Auf ihn heran und ruft: Bejammernswerter,", "tokens": ["Auf", "ihn", "he\u00b7ran", "und", "ruft", ":", "Be\u00b7jam\u00b7merns\u00b7wer\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "KON", "VVFIN", "$.", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Welch Schreckenschicksal ist dir hier bereitet!", "tokens": ["Welch", "Schre\u00b7cken\u00b7schick\u00b7sal", "ist", "dir", "hier", "be\u00b7rei\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Hier herrscht die Freiheit stets in unbeschwerter", "tokens": ["Hier", "herrscht", "die", "Frei\u00b7heit", "stets", "in", "un\u00b7be\u00b7schwer\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Gedankenruh, du kannst sie nicht verjagen,", "tokens": ["Ge\u00b7dan\u00b7ken\u00b7ruh", ",", "du", "kannst", "sie", "nicht", "ver\u00b7ja\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Ohnm\u00e4chtig sind hier alle deine Schwerter!", "tokens": ["Ohn\u00b7m\u00e4ch\u00b7tig", "sind", "hier", "al\u00b7le", "dei\u00b7ne", "Schwer\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Doch will zuerst ich, wer ich sei, dir sagen:", "tokens": ["Doch", "will", "zu\u00b7erst", "ich", ",", "wer", "ich", "sei", ",", "dir", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PPER", "$,", "PWS", "PPER", "VAFIN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Ich bin der gro\u00dfe florentinische Dichter,", "tokens": ["Ich", "bin", "der", "gro\u00b7\u00dfe", "flo\u00b7ren\u00b7ti\u00b7ni\u00b7sche", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-++-+-", "measure": "unknown.measure.hexa"}, "line.24": {"text": "Nach dessen Staub du magst Ravenna fragen:", "tokens": ["Nach", "des\u00b7sen", "Staub", "du", "magst", "Ra\u00b7ven\u00b7na", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "VMFIN", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Ich war den S\u00fcndern meiner Zeit ein Richter;", "tokens": ["Ich", "war", "den", "S\u00fcn\u00b7dern", "mei\u00b7ner", "Zeit", "ein", "Rich\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Doch unter Allen, welche schon verwesen,", "tokens": ["Doch", "un\u00b7ter", "Al\u00b7len", ",", "wel\u00b7che", "schon", "ver\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Erreichte keiner dich und dein Gelichter!", "tokens": ["Er\u00b7reich\u00b7te", "kei\u00b7ner", "dich", "und", "dein", "Ge\u00b7lich\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Was wird man einst auf deinem Grabe lesen,", "tokens": ["Was", "wird", "man", "einst", "auf", "dei\u00b7nem", "Gra\u00b7be", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PIS", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Der du zugleich Herodes gegen Kinder,", "tokens": ["Der", "du", "zu\u00b7gleich", "He\u00b7ro\u00b7des", "ge\u00b7gen", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Und gegen M\u00e4nner Ezzelin gewesen!", "tokens": ["Und", "ge\u00b7gen", "M\u00e4n\u00b7ner", "Ez\u00b7ze\u00b7lin", "ge\u00b7we\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "NE", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Ein Unterdr\u00fccker, nicht ein \u00dcberwinder;", "tokens": ["Ein", "Un\u00b7ter\u00b7dr\u00fc\u00b7cker", ",", "nicht", "ein", "\u00dc\u00b7berw\u00b7in\u00b7der", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Gezeugt von einer schauderbarn Lemure,", "tokens": ["Ge\u00b7zeugt", "von", "ei\u00b7ner", "schau\u00b7der\u00b7barn", "Le\u00b7mu\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Und dann gepfropft noch auf den Stamm der Schinder!", "tokens": ["Und", "dann", "ge\u00b7pfropft", "noch", "auf", "den", "Stamm", "der", "Schin\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Sohn eines Bankerts, Enkel einer Hure,", "tokens": ["Sohn", "ei\u00b7nes", "Ban\u00b7kerts", ",", "En\u00b7kel", "ei\u00b7ner", "Hu\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.35": {"text": "Vernimmst du nicht, da\u00df Alle dich begr\u00fc\u00dfen:", "tokens": ["Ver\u00b7nimmst", "du", "nicht", ",", "da\u00df", "Al\u00b7le", "dich", "be\u00b7gr\u00fc\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PIS", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Rehabeam, wie steht's mit deinem Schwure?", "tokens": ["Re\u00b7ha\u00b7beam", ",", "wie", "steht's", "mit", "dei\u00b7nem", "Schwu\u00b7re", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.37": {"text": "Hier hast du nun die grause Schuld zu b\u00fc\u00dfen:", "tokens": ["Hier", "hast", "du", "nun", "die", "grau\u00b7se", "Schuld", "zu", "b\u00fc\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Die Letzten selbst im Reich der Geister grollen", "tokens": ["Die", "Letz\u00b7ten", "selbst", "im", "Reich", "der", "Geis\u00b7ter", "grol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Dir ins Gesicht und treten dich mit F\u00fc\u00dfen!", "tokens": ["Dir", "ins", "Ge\u00b7sicht", "und", "tre\u00b7ten", "dich", "mit", "F\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "KON", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.40": {"text": "Gehorsam wu\u00dfte dir die Welt zu zollen:", "tokens": ["Ge\u00b7hor\u00b7sam", "wu\u00df\u00b7te", "dir", "die", "Welt", "zu", "zol\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Dort nannten Schurken dich sogar den Frommen,", "tokens": ["Dort", "nann\u00b7ten", "Schur\u00b7ken", "dich", "so\u00b7gar", "den", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Hier w\u00e4r's Verbrechen, dir gehorchen wollen!", "tokens": ["Hier", "w\u00e4r's", "Ver\u00b7bre\u00b7chen", ",", "dir", "ge\u00b7hor\u00b7chen", "wol\u00b7len", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,", "PPER", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Wo sind die Sklaven alle hingekommen,", "tokens": ["Wo", "sind", "die", "Skla\u00b7ven", "al\u00b7le", "hin\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Die unterw\u00fcrfig ihrem Herrn und Meister", "tokens": ["Die", "un\u00b7ter\u00b7w\u00fcr\u00b7fig", "ih\u00b7rem", "Herrn", "und", "Meis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Jedweden blutigen Frevel \u00fcbernommen?", "tokens": ["Jed\u00b7we\u00b7den", "blu\u00b7ti\u00b7gen", "Fre\u00b7vel", "\u00fc\u00b7ber\u00b7nom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.46": {"text": "Hier gilt Gesetz, hier \u00e4u\u00dfert sich in freister", "tokens": ["Hier", "gilt", "Ge\u00b7setz", ",", "hier", "\u00e4u\u00b7\u00dfert", "sich", "in", "freis\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "$,", "ADV", "VVFIN", "PRF", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "Tatkraft die Tugend, die du hast gelogen:", "tokens": ["Tat\u00b7kraft", "die", "Tu\u00b7gend", ",", "die", "du", "hast", "ge\u00b7lo\u00b7gen", 