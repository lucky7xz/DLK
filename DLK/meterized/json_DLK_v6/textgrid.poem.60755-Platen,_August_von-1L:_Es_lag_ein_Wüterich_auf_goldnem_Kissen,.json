 "measure": "iambic.penta"}, "line.58": {"text": "Bald stehn sie Alle gegen dich, die Kl\u00e4ger,", "tokens": ["Bald", "stehn", "sie", "Al\u00b7le", "ge\u00b7gen", "dich", ",", "die", "Kl\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "APPR", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.59": {"text": "Wann ihre Z\u00e4hren sich zum Strom verm\u00e4hlen,", "tokens": ["Wann", "ih\u00b7re", "Z\u00e4h\u00b7ren", "sich", "zum", "Strom", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.60": {"text": "Aus dem du sch\u00f6pfen sollst als Wassertr\u00e4ger!", "tokens": ["Aus", "dem", "du", "sch\u00f6p\u00b7fen", "sollst", "als", "Was\u00b7ser\u00b7tr\u00e4\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVINF", "VMFIN", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.61": {"text": "Vom K\u00f6nig Kodrus will ich dir erz\u00e4hlen,", "tokens": ["Vom", "K\u00f6\u00b7nig", "Ko\u00b7drus", "will", "ich", "dir", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.62": {"text": "Der in den Tod ging, um sein Volk zu retten:", "tokens": ["Der", "in", "den", "Tod", "ging", ",", "um", "sein", "Volk", "zu", "ret\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,", "KOUI", "PPOSAT", "NN", "PTKZU", "VVINF"