{"dta.poem.20820": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Drauf satzt er sich/ es ruhte Geist und Hertz/", "tokens": ["Drauf", "satzt", "er", "sich", "/", "es", "ruh\u00b7te", "Geist", "und", "Hertz", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "$(", "PPER", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sein gleicher Muth/ sein Reden-voller Schertz/", "tokens": ["Sein", "glei\u00b7cher", "Muth", "/", "sein", "Re\u00b7den\u00b7vol\u00b7ler", "Schertz", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die wolten uns fast eine Lust erregen", "tokens": ["Die", "wol\u00b7ten", "uns", "fast", "ei\u00b7ne", "Lust", "er\u00b7re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit ihm zu gehn auf seinen bleichen Wegen.", "tokens": ["Mit", "ihm", "zu", "gehn", "auf", "sei\u00b7nen", "blei\u00b7chen", "We\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}