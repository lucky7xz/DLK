{"textgrid.poem.56230": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Was soll ich sagen?", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Aug ist tr\u00fcb, mein Mund ist stumm,", "tokens": ["Mein", "Aug", "ist", "tr\u00fcb", ",", "mein", "Mund", "ist", "stumm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du hei\u00dfest mich reden, es sei darum.", "tokens": ["Du", "hei\u00b7\u00dfest", "mich", "re\u00b7den", ",", "es", "sei", "da\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,", "PPER", "VAFIN", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dein Aug ist klar, dein Mund ist rot,", "tokens": ["Dein", "Aug", "ist", "klar", ",", "dein", "Mund", "ist", "rot", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und was du nur w\u00fcnschest, das ist ein Gebot.", "tokens": ["Und", "was", "du", "nur", "w\u00fcn\u00b7schest", ",", "das", "ist", "ein", "Ge\u00b7bot", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "$,", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "----+-+-+-+", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "Mein Haar ist grau, mein Herz ist wund,", "tokens": ["Mein", "Haar", "ist", "grau", ",", "mein", "Herz", "ist", "wund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist so jung, und bist so gesund.", "tokens": ["Du", "bist", "so", "jung", ",", "und", "bist", "so", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "KON", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Du hei\u00dfest mich reden, und machst mir's so schwer,", "tokens": ["Du", "hei\u00b7\u00dfest", "mich", "re\u00b7den", ",", "und", "machst", "mir's", "so", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,", "KON", "VVFIN", "NE", "ADV", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ich seh dich so an, und zittre so sehr.", "tokens": ["Ich", "seh", "dich", "so", "an", ",", "und", "zitt\u00b7re", "so", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "KON", "VVFIN", "ADV", "ADV", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}}}}