{"textgrid.poem.33870": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Bettelm\u00e4dchen", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Bettelm\u00e4dchen lauscht am Thor,", "tokens": ["Das", "Bet\u00b7tel\u00b7m\u00e4d\u00b7chen", "lauscht", "am", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es friert sie gar zu sehr;", "tokens": ["Es", "friert", "sie", "gar", "zu", "sehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKA", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der junge Ritter tritt hervor,", "tokens": ["Der", "jun\u00b7ge", "Rit\u00b7ter", "tritt", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er wirft ihr hin den Mantel", "tokens": ["Er", "wirft", "ihr", "hin", "den", "Man\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und spricht: was willst du mehr?", "tokens": ["Und", "spricht", ":", "was", "willst", "du", "mehr", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das M\u00e4dchen sagt kein einzig Wort,", "tokens": ["Das", "M\u00e4d\u00b7chen", "sagt", "kein", "ein\u00b7zig", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es friert sie gar zu sehr;", "tokens": ["Es", "friert", "sie", "gar", "zu", "sehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKA", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann geht sie stolz und gl\u00fchend fort,", "tokens": ["Dann", "geht", "sie", "stolz", "und", "gl\u00fc\u00b7hend", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und l\u00e4\u00dft den Mantel liegen", "tokens": ["Und", "l\u00e4\u00dft", "den", "Man\u00b7tel", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und spricht: ich will Nichts mehr!", "tokens": ["Und", "spricht", ":", "ich", "will", "Nichts", "mehr", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VMFIN", "PIS", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}