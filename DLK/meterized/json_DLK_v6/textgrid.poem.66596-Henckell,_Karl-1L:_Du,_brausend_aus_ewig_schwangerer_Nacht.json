{"textgrid.poem.66596": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du, brausend aus ewig schwangerer Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, brausend aus ewig schwangerer Nacht", "tokens": ["Du", ",", "brau\u00b7send", "aus", "e\u00b7wig", "schwan\u00b7ge\u00b7rer", "Nacht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ADJD", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und ewig zeugendem Lichte,", "tokens": ["Und", "e\u00b7wig", "zeu\u00b7gen\u00b7dem", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Aus feuchtem Brodem und Glut entfacht,", "tokens": ["Aus", "feuch\u00b7tem", "Bro\u00b7dem", "und", "Glut", "ent\u00b7facht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Verwegenstes der Gedichte:", "tokens": ["Ver\u00b7we\u00b7gens\u00b7tes", "der", "Ge\u00b7dich\u00b7te", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Getr\u00e4umt von Gott, dem urspr\u00fcnglichen Geist,", "tokens": ["Ge\u00b7tr\u00e4umt", "von", "Gott", ",", "dem", "ur\u00b7spr\u00fcng\u00b7li\u00b7chen", "Geist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.6": {"text": "Dem Grund des Abgrunds entquollen,", "tokens": ["Dem", "Grund", "des", "Ab\u00b7grunds", "ent\u00b7quol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Du, das da sch\u00e4umt und zittert und kreist \u2013", "tokens": ["Du", ",", "das", "da", "sch\u00e4umt", "und", "zit\u00b7tert", "und", "kreist", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Wie rollen", "tokens": ["Wie", "rol\u00b7len"], "token_info": ["word", "word"], "pos": ["PWAV", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Geheimnisvoll die Rhythmen des Alls", "tokens": ["Ge\u00b7heim\u00b7nis\u00b7voll", "die", "Rhyth\u00b7men", "des", "Alls"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Durch deine d\u00e4monischen Fluten,", "tokens": ["Durch", "dei\u00b7ne", "d\u00e4\u00b7mo\u00b7ni\u00b7schen", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Im Wirbel der Wollust, im Schrei des Metalls,", "tokens": ["Im", "Wir\u00b7bel", "der", "Wol\u00b7lust", ",", "im", "Schrei", "des", "Me\u00b7talls", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "In gewitterflammenden Ruten!", "tokens": ["In", "ge\u00b7wit\u00b7ter\u00b7flam\u00b7men\u00b7den", "Ru\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Im adlerschwebenden Gletschersang", "tokens": ["Im", "ad\u00b7ler\u00b7schwe\u00b7ben\u00b7den", "Glet\u00b7scher\u00b7sang"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der unbesieglichen Seelen,", "tokens": ["Der", "un\u00b7be\u00b7sieg\u00b7li\u00b7chen", "See\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Im schattend\u00e4mmernden Untergang \u2013", "tokens": ["Im", "schat\u00b7ten\u00b7d\u00e4m\u00b7mern\u00b7den", "Un\u00b7ter\u00b7gang", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "In H\u00f6hlen", "tokens": ["In", "H\u00f6h\u00b7len"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "Der schwelenden Wut und des heimlichen Leids,", "tokens": ["Der", "schwe\u00b7len\u00b7den", "Wut", "und", "des", "heim\u00b7li\u00b7chen", "Leids", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Im Feuer der stolzen Emp\u00f6rung,", "tokens": ["Im", "Feu\u00b7er", "der", "stol\u00b7zen", "Em\u00b7p\u00f6\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "In bl\u00fchender Rosen ber\u00fcckendem Reiz,", "tokens": ["In", "bl\u00fc\u00b7hen\u00b7der", "Ro\u00b7sen", "be\u00b7r\u00fc\u00b7cken\u00b7dem", "Reiz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "In seliger Sehnsucht Erh\u00f6rung.", "tokens": ["In", "se\u00b7li\u00b7ger", "Sehn\u00b7sucht", "Er\u00b7h\u00f6\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "In lachender Laune weltheiterem Laut,", "tokens": ["In", "la\u00b7chen\u00b7der", "Lau\u00b7ne", "welt\u00b7hei\u00b7te\u00b7rem", "Laut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PWAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "In Genien, der Urkraft ergeben,", "tokens": ["In", "Ge\u00b7ni\u00b7en", ",", "der", "Ur\u00b7kraft", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ART", "NN", "VVPP", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Was da atmet und schwingt, was da leuchtet und taut:", "tokens": ["Was", "da", "at\u00b7met", "und", "schwingt", ",", "was", "da", "leuch\u00b7tet", "und", "taut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "KON", "VVFIN", "$,", "PRELS", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.6": {"line.1": {"text": "Du Leben!", "tokens": ["Du", "Le\u00b7ben", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}