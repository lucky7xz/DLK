{"textgrid.poem.45700": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Politisch", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit wem soll sich verb\u00fcnden der Hase!", "tokens": ["Mit", "wem", "soll", "sich", "ver\u00b7b\u00fcn\u00b7den", "der", "Ha\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "VMFIN", "PRF", "VVFIN", "ART", "NN", "$."], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Fuchs schleicht ihm nach im Grase,", "tokens": ["Der", "Fuchs", "schleicht", "ihm", "nach", "im", "Gra\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PPER", "APPR", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Von oben rauschen des Geiers Schwingen,", "tokens": ["Von", "o\u00b7ben", "rau\u00b7schen", "des", "Gei\u00b7ers", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Bauer im Kohlfeld legt ihm Schlingen,", "tokens": ["Der", "Bau\u00b7er", "im", "Kohl\u00b7feld", "legt", "ihm", "Schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und macht er sich endlich auf die F\u00fc\u00dfe,", "tokens": ["Und", "macht", "er", "sich", "end\u00b7lich", "auf", "die", "F\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Treffen ihn des J\u00e4gers Sch\u00fcsse.", "tokens": ["Tref\u00b7fen", "ihn", "des", "J\u00e4\u00b7gers", "Sch\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}