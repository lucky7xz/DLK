{"dta.poem.268": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Sie versichert ihm ihrer Gegen-Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Deine Best\u00e4ndigkeit hat nun gesiegt!", "tokens": ["Dei\u00b7ne", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "hat", "nun", "ge\u00b7siegt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Alle die Plagen", "tokens": ["Al\u00b7le", "die", "Pla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PIS", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sind schon verschlagen/", "tokens": ["Sind", "schon", "ver\u00b7schla\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Da\u00df sie nicht k\u00f6nnen dein Hertze mehr nagen/", "tokens": ["Da\u00df", "sie", "nicht", "k\u00f6n\u00b7nen", "dein", "Hert\u00b7ze", "mehr", "na\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VMFIN", "PPOSAT", "VVFIN", "ADV", "VVFIN", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Weil es das Gl\u00fccke so artig f\u00fcgt.", "tokens": ["Weil", "es", "das", "Gl\u00fc\u00b7cke", "so", "ar\u00b7tig", "f\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Deine Best\u00e4ndigkeit hat nun gesiegt.", "tokens": ["Dei\u00b7ne", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "hat", "nun", "ge\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Deine Best\u00e4ndigkeit hat nun gesiegt:", "tokens": ["Dei\u00b7ne", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "hat", "nun", "ge\u00b7siegt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Was dich gedr\u00fccket/", "tokens": ["Was", "dich", "ge\u00b7dr\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Jetzt dich erquicket", "tokens": ["Jetzt", "dich", "er\u00b7quic\u00b7ket"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wenn dich der ", "tokens": ["Wenn", "dich", "der"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Und die Vergn\u00fcgung die Geister einwiegt.", "tokens": ["Und", "die", "Ver\u00b7gn\u00fc\u00b7gung", "die", "Geis\u00b7ter", "ein\u00b7wiegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Deine Best\u00e4ndigkeit hat nun gesiegt.", "tokens": ["Dei\u00b7ne", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "hat", "nun", "ge\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Deine Best\u00e4ndigkeit hat nun gesiegt:", "tokens": ["Dei\u00b7ne", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "hat", "nun", "ge\u00b7siegt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Weil deine Flammen", "tokens": ["Weil", "dei\u00b7ne", "Flam\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Vom Himmel stammen/", "tokens": ["Vom", "Him\u00b7mel", "stam\u00b7men", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Kan sie mein Hertze nicht f\u00fcglich verdammen/", "tokens": ["Kan", "sie", "mein", "Hert\u00b7ze", "nicht", "f\u00fcg\u00b7lich", "ver\u00b7dam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "VVFIN", "PTKNEG", "ADJD", "VVINF", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Sondern sie machen mich h\u00f6chlich vergn\u00fcgt.", "tokens": ["Son\u00b7dern", "sie", "ma\u00b7chen", "mich", "h\u00f6ch\u00b7lich", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADJD", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Deine Best\u00e4ndigkeit hat nun gesiegt.", "tokens": ["Dei\u00b7ne", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "hat", "nun", "ge\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Deine Best\u00e4ndigkeit hat nun gesiegt:", "tokens": ["Dei\u00b7ne", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "hat", "nun", "ge\u00b7siegt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Weil deine Blicke", "tokens": ["Weil", "dei\u00b7ne", "Bli\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ohn falsche T\u00fccke", "tokens": ["Ohn", "fal\u00b7sche", "T\u00fc\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Auch sind best\u00e4ndig im widrigen Gl\u00fccke", "tokens": ["Auch", "sind", "be\u00b7st\u00e4n\u00b7dig", "im", "wid\u00b7ri\u00b7gen", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Da\u00df mich kein Nebel der Falschheit betriegt", "tokens": ["Da\u00df", "mich", "kein", "Ne\u00b7bel", "der", "Falschheit", "be\u00b7triegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Deine Best\u00e4ndigkeit hat nun gesiegt.", "tokens": ["Dei\u00b7ne", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "hat", "nun", "ge\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Deine Best\u00e4ndigkeit hat nun gesiegt:", "tokens": ["Dei\u00b7ne", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "hat", "nun", "ge\u00b7sie