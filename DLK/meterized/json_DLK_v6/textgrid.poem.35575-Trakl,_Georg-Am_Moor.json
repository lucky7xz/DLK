{"textgrid.poem.35575": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Am Moor", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wanderer im schwarzen Wind; leise fl\u00fcstert das d\u00fcrre Rohr", "tokens": ["Wan\u00b7de\u00b7rer", "im", "schwar\u00b7zen", "Wind", ";", "lei\u00b7se", "fl\u00fcs\u00b7tert", "das", "d\u00fcr\u00b7re", "Rohr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN", "$.", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+--+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "In der Stille des Moors. Am grauen Himmel", "tokens": ["In", "der", "Stil\u00b7le", "des", "Mo\u00b7ors", ".", "Am", "grau\u00b7en", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$.", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Ein Zug von wilden V\u00f6geln folgt;", "tokens": ["Ein", "Zug", "von", "wil\u00b7den", "V\u00f6\u00b7geln", "folgt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Quere \u00fcber finsteren Wassern.", "tokens": ["Que\u00b7re", "\u00fc\u00b7ber", "fins\u00b7te\u00b7ren", "Was\u00b7sern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Aufruhr. In verfallener H\u00fctte", "tokens": ["Auf\u00b7ruhr", ".", "In", "ver\u00b7fal\u00b7le\u00b7ner", "H\u00fct\u00b7te"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "ADJA", "NN"], "meter": "----+--+-", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Aufflattert mit schwarzen Fl\u00fcgeln die F\u00e4ulnis;", "tokens": ["Auf\u00b7flat\u00b7tert", "mit", "schwar\u00b7zen", "Fl\u00fc\u00b7geln", "die", "F\u00e4ul\u00b7nis", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Verkr\u00fcppelte Birken seufzen im Wind.", "tokens": ["Ver\u00b7kr\u00fcp\u00b7pel\u00b7te", "Bir\u00b7ken", "seuf\u00b7zen", "im", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Abend in verlassener Schenke. Den Heimweg umwittert", "tokens": ["A\u00b7bend", "in", "ver\u00b7las\u00b7se\u00b7ner", "Schen\u00b7ke", ".", "Den", "Heim\u00b7weg", "um\u00b7wit\u00b7tert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Die sanfte Schwermut grasender Herden,", "tokens": ["Die", "sanf\u00b7te", "Schwer\u00b7mut", "gra\u00b7sen\u00b7der", "Her\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Erscheinung der Nacht: Kr\u00f6ten tauchen aus silbernen Wassern.", "tokens": ["Er\u00b7schei\u00b7nung", "der", "Nacht", ":", "Kr\u00f6\u00b7ten", "tau\u00b7chen", "aus", "sil\u00b7ber\u00b7nen", "Was\u00b7sern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+---+-+--+--+-", "measure": "iambic.penta.relaxed"}}}}}