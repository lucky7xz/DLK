{"textgrid.poem.62431": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Willkommen hier im Waffenfeld,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willkommen hier im Waffenfeld,", "tokens": ["Will\u00b7kom\u00b7men", "hier", "im", "Waf\u00b7fen\u00b7feld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Willkommen lieber Herr und Held,", "tokens": ["Will\u00b7kom\u00b7men", "lie\u00b7ber", "Herr", "und", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo deine Mannen", "tokens": ["Wo", "dei\u00b7ne", "Man\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Den Sieg gewannen.", "tokens": ["Den", "Sieg", "ge\u00b7wan\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wie herrlich strahlt dein Angesicht", "tokens": ["Wie", "herr\u00b7lich", "strahlt", "dein", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Sonnenlicht, im Freudenlicht,", "tokens": ["Im", "Son\u00b7nen\u00b7licht", ",", "im", "Freu\u00b7den\u00b7licht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Siegesglanze,", "tokens": ["Im", "Sie\u00b7ges\u00b7glan\u00b7ze", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Du K\u00f6nigslanze.", "tokens": ["Du", "K\u00f6\u00b7nigs\u00b7lan\u00b7ze", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Nicht sch\u00f6ner hat Sie dich geschaut", "tokens": ["Nicht", "sch\u00f6\u00b7ner", "hat", "Sie", "dich", "ge\u00b7schaut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "VAFIN", "PPER", "PRF", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am ersten Tag die junge Braut,", "tokens": ["Am", "ers\u00b7ten", "Tag", "die", "jun\u00b7ge", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die dort nun betet,", "tokens": ["Die", "dort", "nun", "be\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wo's Blau sich r\u00f6thet.", "tokens": ["Wo's", "Blau", "sich", "r\u00f6\u00b7thet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PRF", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Nicht l\u00e4nger stehn die Feinde mehr,", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "stehn", "die", "Fein\u00b7de", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Leuchten wandelt vor dir her,", "tokens": ["Das", "Leuch\u00b7ten", "wan\u00b7delt", "vor", "dir", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die alte Flamme", "tokens": ["Die", "al\u00b7te", "Flam\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Vom Heldenstamme.", "tokens": ["Vom", "Hel\u00b7den\u00b7stam\u00b7me", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Sie werfen Raub und Waffen fort,", "tokens": ["Sie", "wer\u00b7fen", "Raub", "und", "Waf\u00b7fen", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem R\u00e4cher weihn wir diesen Ort,", "tokens": ["Dem", "R\u00e4\u00b7cher", "weihn", "wir", "die\u00b7sen", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus mildem Bache", "tokens": ["Aus", "mil\u00b7dem", "Ba\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wird Strom der Rache.", "tokens": ["Wird", "Strom", "der", "Ra\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Der stolze Frevler wird ein Spott;", "tokens": ["Der", "stol\u00b7ze", "Frev\u00b7ler", "wird", "ein", "Spott", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit uns ist Gott, mit uns ist Gott,", "tokens": ["Mit", "uns", "ist", "Gott", ",", "mit", "uns", "ist", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NN", "$,", "APPR", "PPER", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und unsrer Fahnen", "tokens": ["Und", "uns\u00b7rer", "Fah\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Freu'n sich die Ahnen.", "tokens": ["Freu'n", "sich", "die", "Ah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Du gr\u00fc\u00dfest uns, wir gr\u00fc\u00dfen dich,", "tokens": ["Du", "gr\u00fc\u00b7\u00dfest", "uns", ",", "wir", "gr\u00fc\u00b7\u00dfen", "dich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es freuen Mensch und Engel sich,", "tokens": ["Es", "freu\u00b7en", "Mensch", "und", "En\u00b7gel", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn Recht verschaffen", "tokens": ["Wenn", "Recht", "ver\u00b7schaf\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Gott und die Waffen.", "tokens": ["Gott", "und", "die", "Waf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Willkommen drum im Waffenfeld,", "tokens": ["Will\u00b7kom\u00b7men", "drum", "im", "Waf\u00b7fen\u00b7feld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Willkommen frommer, deutscher Held,", "tokens": ["Will\u00b7kom\u00b7men", "from\u00b7mer", ",", "deut\u00b7scher", "Held", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo deine Mannen", "tokens": ["Wo", "dei\u00b7ne", "Man\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Den Sieg gewannen.", "tokens": ["Den", "Sieg", "ge\u00b7wan\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}