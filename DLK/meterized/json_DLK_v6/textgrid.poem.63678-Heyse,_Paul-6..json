{"textgrid.poem.63678": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seit dies an mir geschah, sind vier Jahrzehnte", "tokens": ["Seit", "dies", "an", "mir", "ge\u00b7schah", ",", "sind", "vier", "Jahr\u00b7zehn\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDS", "APPR", "PPER", "VVFIN", "$,", "VAFIN", "CARD", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Mir \u00fcber Haupt und Herzen hingezogen;", "tokens": ["Mir", "\u00fc\u00b7ber", "Haupt", "und", "Her\u00b7zen", "hin\u00b7ge\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das holde Wunder hat mich nicht betrogen", "tokens": ["Das", "hol\u00b7de", "Wun\u00b7der", "hat", "mich", "nicht", "be\u00b7tro\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "PTKNEG", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und gab in F\u00fclle mir, was ich ersehnte.", "tokens": ["Und", "gab", "in", "F\u00fcl\u00b7le", "mir", ",", "was", "ich", "er\u00b7sehn\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein Leben, das ich schon im Welken w\u00e4hnte,", "tokens": ["Mein", "Le\u00b7ben", ",", "das", "ich", "schon", "im", "Wel\u00b7ken", "w\u00e4hn\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verj\u00fcngt noch einmal ging's in hohen Wogen,", "tokens": ["Ver\u00b7j\u00fcngt", "noch", "ein\u00b7mal", "ging's", "in", "ho\u00b7hen", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und diese Jugend ist nicht ganz verflogen,", "tokens": ["Und", "die\u00b7se", "Ju\u00b7gend", "ist", "nicht", "ganz", "ver\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So altersm\u00fcd auch jetzt die Beichte t\u00f6nte.", "tokens": ["So", "al\u00b7ters\u00b7m\u00fcd", "auch", "jetzt", "die", "Beich\u00b7te", "t\u00f6n\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Noch heute, wenn mir Kraft und Mut entwich", "tokens": ["Noch", "heu\u00b7te", ",", "wenn", "mir", "Kraft", "und", "Mut", "ent\u00b7wich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "NN", "KON", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und meine Klagen str\u00f6mten ungez\u00fcgelt \u2013", "tokens": ["Und", "mei\u00b7ne", "Kla\u00b7gen", "str\u00f6m\u00b7ten", "un\u00b7ge\u00b7z\u00fc\u00b7gelt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein L\u00e4cheln nur von meinem andern Ich", "tokens": ["Ein", "L\u00e4\u00b7cheln", "nur", "von", "mei\u00b7nem", "an\u00b7dern", "Ich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "ADJA", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Gen\u00fcgt, da\u00df neu die Seele sich befl\u00fcgelt,", "tokens": ["Ge\u00b7n\u00fcgt", ",", "da\u00df", "neu", "die", "See\u00b7le", "sich", "be\u00b7fl\u00fc\u00b7gelt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "ADJD", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Blick in dieses Aug, drin tr\u00f6stend sich", "tokens": ["Ein", "Blick", "in", "die\u00b7ses", "Aug", ",", "drin", "tr\u00f6s\u00b7tend", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "$,", "ADV", "ADJD", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Solch eine Welt von Lieb und G\u00fcte spiegelt.", "tokens": ["Solch", "ei\u00b7ne", "Welt", "von", "Lieb", "und", "G\u00fc\u00b7te", "spie\u00b7gelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}