{"dta.poem.14286": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Nachkl\u00e4nge .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df nicht, was ich so th\u00f6richt bin!", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "ich", "so", "th\u00f6\u00b7richt", "bin", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Wolken im Herbstwind jagen,", "tokens": ["Wol\u00b7ken", "im", "Herbst\u00b7wind", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Die V\u00f6gel zieh'n \u00fcber die W\u00e4lder hin,", "tokens": ["Die", "V\u00f6\u00b7gel", "zieh'n", "\u00fc\u00b7ber", "die", "W\u00e4l\u00b7der", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Das klang wie in Fr\u00fchlingstagen.", "tokens": ["Das", "klang", "wie", "in", "Fr\u00fch\u00b7lings\u00b7ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}