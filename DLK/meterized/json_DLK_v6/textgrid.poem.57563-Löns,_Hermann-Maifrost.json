{"textgrid.poem.57563": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Maifrost", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft leuchtet im Fr\u00fchling die Sonne so warm,", "tokens": ["Oft", "leuch\u00b7tet", "im", "Fr\u00fch\u00b7ling", "die", "Son\u00b7ne", "so", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Doch rauh pfeift der Nachtwind von Norden,", "tokens": ["Doch", "rauh", "pfeift", "der", "Nacht\u00b7wind", "von", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "So ist mir auch gestern in deinem Arm", "tokens": ["So", "ist", "mir", "auch", "ge\u00b7stern", "in", "dei\u00b7nem", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Eisig zu Mute geworden.", "tokens": ["Ei\u00b7sig", "zu", "Mu\u00b7te", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VAPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Dein Mund war so s\u00fc\u00df, dem Busen so weich,", "tokens": ["Dein", "Mund", "war", "so", "s\u00fc\u00df", ",", "dem", "Bu\u00b7sen", "so", "weich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So warm deines Herzens Pochen \u2013", "tokens": ["So", "warm", "dei\u00b7nes", "Her\u00b7zens", "Po\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da kam aus dem kalten Gedankenreich", "tokens": ["Da", "kam", "aus", "dem", "kal\u00b7ten", "Ge\u00b7dan\u00b7ken\u00b7reich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ein kalter Gedanke gekrochen.", "tokens": ["Ein", "kal\u00b7ter", "Ge\u00b7dan\u00b7ke", "ge\u00b7kro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Du sprachest so sch\u00f6n und platonisch zu mir", "tokens": ["Du", "spra\u00b7chest", "so", "sch\u00f6n", "und", "pla\u00b7to\u00b7nisch", "zu", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "APPR", "PPER"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von Liebe gleichf\u00fchlender Seelen \u2013", "tokens": ["Von", "Lie\u00b7be", "gleich\u00b7f\u00fch\u00b7len\u00b7der", "See\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Doch mir schien alles das nur die Gier", "tokens": ["Doch", "mir", "schien", "al\u00b7les", "das", "nur", "die", "Gier"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PIS", "ART", "ADV", "ART", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Der Leiber, sich zu verm\u00e4hlen.", "tokens": ["Der", "Lei\u00b7ber", ",", "sich", "zu", "ver\u00b7m\u00e4h\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Das alles ist L\u00fcge und Trug der Natur,", "tokens": ["Das", "al\u00b7les", "ist", "L\u00fc\u00b7ge", "und", "Trug", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Schlaflieder, uns einzuwiegen,", "tokens": ["Schlaf\u00b7lie\u00b7der", ",", "uns", "ein\u00b7zu\u00b7wie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVPP", "$,"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Sie schmeichelt dem stolzen Geiste nur,", "tokens": ["Sie", "schmei\u00b7chelt", "dem", "stol\u00b7zen", "Geis\u00b7te", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da\u00df die Leiber sich williger f\u00fcgen.", "tokens": ["Da\u00df", "die", "Lei\u00b7ber", "sich", "wil\u00b7li\u00b7ger", "f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "ADJD", "VVINF", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.5": {"line.1": {"text": "Und dieser Gedanke, mein liebes Kind,", "tokens": ["Und", "die\u00b7ser", "Ge\u00b7dan\u00b7ke", ",", "mein", "lie\u00b7bes", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mu\u00df die innigste Liebe ermorden \u2013", "tokens": ["Mu\u00df", "die", "in\u00b7nigs\u00b7te", "Lie\u00b7be", "er\u00b7mor\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie schade, da\u00df wir keine Tiere mehr sind", "tokens": ["Wie", "scha\u00b7de", ",", "da\u00df", "wir", "kei\u00b7ne", "Tie\u00b7re", "mehr", "sind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "KOUS", "PPER", "PIAT", "NN", "ADV", "VAFIN"], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Oder noch keine Engel geworden!", "tokens": ["O\u00b7der", "noch", "kei\u00b7ne", "En\u00b7gel", "ge\u00b7wor\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "VAPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}