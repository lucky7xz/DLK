{"dta.poem.8873": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Vi.  \n  An Schelling.   In ein Exemplar der Gaselen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Wer immer K\u00f6nig ist im Reich des Wahren?", "tokens": ["Wer", "im\u00b7mer", "K\u00f6\u00b7nig", "ist", "im", "Reich", "des", "Wah\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "VAFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du siehst sie beyde sich im H\u00f6chsten paaren,", "tokens": ["Du", "siehst", "sie", "bey\u00b7de", "sich", "im", "H\u00f6chs\u00b7ten", "paa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gleich in einander wie verlornen T\u00f6nen.", "tokens": ["Gleich", "in", "ein\u00b7an\u00b7der", "wie", "ver\u00b7lor\u00b7nen", "T\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du wirst die kleine Gabe nicht verh\u00f6hnen,", "tokens": ["Du", "wirst", "die", "klei\u00b7ne", "Ga\u00b7be", "nicht", "ver\u00b7h\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wirst diese morgenl\u00e4ndisch bunten Schaaren", "tokens": ["Wirst", "die\u00b7se", "mor\u00b7gen\u00b7l\u00e4n\u00b7disch", "bun\u00b7ten", "Schaa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PDS", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In ihrer Bilderf\u00fclle gern gewahren,", "tokens": ["In", "ih\u00b7rer", "Bil\u00b7der\u00b7f\u00fcl\u00b7le", "gern", "ge\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und gerne dich an ihren Klang gew\u00f6hnen.", "tokens": ["Und", "ger\u00b7ne", "dich", "an", "ih\u00b7ren", "Klang", "ge\u00b7w\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zwar auf den Bl\u00fcthen eines fernen Landes", "tokens": ["Zwar", "auf", "den", "Bl\u00fc\u00b7then", "ei\u00b7nes", "fer\u00b7nen", "Lan\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schweb' ich nur fl\u00fcchtig, gleich dem Schmetterlinge,", "tokens": ["Schweb'", "ich", "nur", "fl\u00fcch\u00b7tig", ",", "gleich", "dem", "Schmet\u00b7ter\u00b7lin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vielleicht genie\u00dfend eines eitlen Tandes.", "tokens": ["Viel\u00b7leicht", "ge\u00b7nie\u00b7\u00dfend", "ei\u00b7nes", "eit\u00b7len", "Tan\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du aber tauchst die heil'ge Bienenschwinge", "tokens": ["Du", "a\u00b7ber", "tauchst", "die", "heil'\u00b7ge", "Bie\u00b7nen\u00b7schwin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Herab vom Saum des Weltenblumenrandes", "tokens": ["Her\u00b7ab", "vom", "Saum", "des", "Wel\u00b7ten\u00b7blu\u00b7men\u00b7ran\u00b7des"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In das geheimni\u00dfvolle Wie der Dinge.", "tokens": ["In", "das", "ge\u00b7heim\u00b7ni\u00df\u00b7vol\u00b7le", "Wie", "der", "Din\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}