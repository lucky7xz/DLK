{"textgrid.poem.56832": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: An seinem Grabe rief des Priesters Mund:", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An seinem Grabe rief des Priesters Mund:", "tokens": ["An", "sei\u00b7nem", "Gra\u00b7be", "rief", "des", "Pries\u00b7ters", "Mund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbob unbewu\u00dft, er war ", "tokens": ["\u00bb", "ob", "un\u00b7be\u00b7wu\u00dft", ",", "er", "war"], "token_info": ["punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "KOUS", "ADJD", "$,", "PPER", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O glaubt es, des Allm\u00e4chtigen Bildnis ist", "tokens": ["O", "glaubt", "es", ",", "des", "All\u00b7m\u00e4ch\u00b7ti\u00b7gen", "Bild\u00b7nis", "ist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "verschwunden nie aus seiner Seele Grund!\u00ab", "tokens": ["ver\u00b7schwun\u00b7den", "nie", "aus", "sei\u00b7ner", "See\u00b7le", "Grund", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wohl mancher bi\u00df sich da die Lippe wund,", "tokens": ["Wohl", "man\u00b7cher", "bi\u00df", "sich", "da", "die", "Lip\u00b7pe", "wund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPR", "PRF", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ersah er, wie voll heuchlerischer List", "tokens": ["er\u00b7sah", "er", ",", "wie", "voll", "heuch\u00b7le\u00b7ri\u00b7scher", "List"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "der Moloch Kirche noch die Toten fri\u00dft", "tokens": ["der", "Mo\u00b7loch", "Kir\u00b7che", "noch", "die", "To\u00b7ten", "fri\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "in seinen gierigen, eifers\u00fcchtigen Schlund.", "tokens": ["in", "sei\u00b7nen", "gie\u00b7ri\u00b7gen", ",", "ei\u00b7fer\u00b7s\u00fcch\u00b7ti\u00b7gen", "Schlund", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Und ob ein Held auch alle Kerker brach,", "tokens": ["Und", "ob", "ein", "Held", "auch", "al\u00b7le", "Ker\u00b7ker", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "die je ihn diesem Unget\u00fcm versklavt,", "tokens": ["die", "je", "ihn", "die\u00b7sem", "Un\u00b7ge\u00b7t\u00fcm", "ver\u00b7sklavt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "im Tode schleicht ihm seine \u00bbLiebe\u00ab nach", "tokens": ["im", "To\u00b7de", "schleicht", "ihm", "sei\u00b7ne", "\u00bb", "Lie\u00b7be", "\u00ab", "nach"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPOSAT", "$(", "NN", "$(", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "und spricht: \u00bbDie \u00e4ndern ruhn in meinem Bauch,", "tokens": ["und", "spricht", ":", "\u00bb", "Die", "\u00e4n\u00b7dern", "ruhn", "in", "mei\u00b7nem", "Bauch", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "wie sollt ich Dich als frei und ungestraft", "tokens": ["wie", "sollt", "ich", "Dich", "als", "frei", "und", "un\u00b7ge\u00b7straft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "KOUS", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "verschonen?! Sei getrost, ich fre\u00df' dich auch.\u00ab", "tokens": ["ver\u00b7scho\u00b7nen", "?!", "Sei", "ge\u00b7trost", ",", "ich", "fre\u00df'", "dich", "auch", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVINF", "$.", "VAFIN", "VVPP", "$,", "PPER", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}