{"textgrid.poem.52723": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Romanze", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Darf ich einmal dein genie\u00dfen", "tokens": ["Darf", "ich", "ein\u00b7mal", "dein", "ge\u00b7nie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne Trennungsschmerz?", "tokens": ["Oh\u00b7ne", "Tren\u00b7nungs\u00b7schmerz", "?"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Werd ich dich, Geliebte, schlie\u00dfen", "tokens": ["Werd", "ich", "dich", ",", "Ge\u00b7lieb\u00b7te", ",", "schlie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "PRF", "$,", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Endlich an dies Herz?", "tokens": ["End\u00b7lich", "an", "dies", "Herz", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDS", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Kaum da\u00df ein gestohlner Ku\u00df", "tokens": ["Kaum", "da\u00df", "ein", "ge\u00b7stohl\u00b7ner", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Noch uns Gl\u00fccklichen verg\u00f6nnet,", "tokens": ["Noch", "uns", "Gl\u00fcck\u00b7li\u00b7chen", "ver\u00b7g\u00f6n\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da nach ewigem Genu\u00df", "tokens": ["Da", "nach", "e\u00b7wi\u00b7gem", "Ge\u00b7nu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Unsre Seele brennet.", "tokens": ["Uns\u00b7re", "See\u00b7le", "bren\u00b7net", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Aber du, Gepr\u00fcfte, Theure,", "tokens": ["A\u00b7ber", "du", ",", "Ge\u00b7pr\u00fcf\u00b7te", ",", "Theu\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "$,", "NN", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bleibest treu gesellt,", "tokens": ["Blei\u00b7best", "treu", "ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn ich durch die Klippen steure", "tokens": ["Wenn", "ich", "durch", "die", "Klip\u00b7pen", "steu\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Kahn zerschellt \u2013", "tokens": ["Und", "der", "Kahn", "zer\u00b7schellt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$("], "meter": "--+-+", "measure": "anapaest.init"}, "line.5": {"text": "Stehst du rettend auf dem Stein,", "tokens": ["Stehst", "du", "ret\u00b7tend", "auf", "dem", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "H\u00e4ndereichend, denn ich z\u00e4hle", "tokens": ["H\u00e4n\u00b7de\u00b7rei\u00b7chend", ",", "denn", "ich", "z\u00e4h\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "KON", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Auf dein k\u00fchnes Herz allein,", "tokens": ["Auf", "dein", "k\u00fch\u00b7nes", "Herz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Deine gro\u00dfe Seele.", "tokens": ["Dei\u00b7ne", "gro\u00b7\u00dfe", "See\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Stille, M\u00e4dchen, deine Z\u00e4hren,", "tokens": ["Stil\u00b7le", ",", "M\u00e4d\u00b7chen", ",", "dei\u00b7ne", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gro\u00dfe Lieb ist k\u00fchn!", "tokens": ["Gro\u00b7\u00dfe", "Lieb", "ist", "k\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Hero und Leander w\u00e4ren", "tokens": ["He\u00b7ro", "und", "Le\u00b7an\u00b7der", "w\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KON", "NN", "VAFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Blo\u00dfe Phantasien?", "tokens": ["Blo\u00b7\u00dfe", "Phan\u00b7ta\u00b7si\u00b7en", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Hero und Leander war!", "tokens": ["He\u00b7ro", "und", "Le\u00b7an\u00b7der", "war", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und gewi\u00df, auch heutgen Tages", "tokens": ["Und", "ge\u00b7wi\u00df", ",", "auch", "heut\u00b7gen", "Ta\u00b7ges"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Gibt es Liebe und Gefahr,", "tokens": ["Gibt", "es", "Lie\u00b7be", "und", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Herzen ihres Schlages.", "tokens": ["Her\u00b7zen", "ih\u00b7res", "Schla\u00b7ges", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Wer ist gegen uns verschworen?", "tokens": ["Wer", "ist", "ge\u00b7gen", "uns", "ver\u00b7schwo\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Kleinmuth fahre hin!", "tokens": ["Klein\u00b7muth", "fah\u00b7re", "hin", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wer nicht wagte war verloren", "tokens": ["Wer", "nicht", "wag\u00b7te", "war", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PTKNEG", "VVFIN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stets von Anbeginn.", "tokens": ["Stets", "von", "An\u00b7be\u00b7ginn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "An der Liebe nur, getrost,", "tokens": ["An", "der", "Lie\u00b7be", "nur", ",", "ge\u00b7trost", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$,", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Halte fest mit deinem Herzen,", "tokens": ["Hal\u00b7te", "fest", "mit", "dei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und sein Fr\u00fchlicht haucht der Ost", "tokens": ["Und", "sein", "Fr\u00fch\u00b7licht", "haucht", "der", "Ost"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "In die Nacht der Schmerzen!", "tokens": ["In", "die", "Nacht", "der", "Schmer\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}