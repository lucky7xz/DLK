{"textgrid.poem.44476": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Dichternot", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein deutscher Dichter ist \u00fcbel dran,", "tokens": ["Ein", "deut\u00b7scher", "Dich\u00b7ter", "ist", "\u00fc\u00b7bel", "dran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und doch auch wieder gut:", "tokens": ["Und", "doch", "auch", "wie\u00b7der", "gut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was plagt sich nicht der arme Mann,", "tokens": ["Was", "plagt", "sich", "nicht", "der", "ar\u00b7me", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er wei\u00df kaum, wie sichs ruht.", "tokens": ["Er", "wei\u00df", "kaum", ",", "wie", "sichs", "ruht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Heut ist man objektiv gesinnt,", "tokens": ["Heut", "ist", "man", "ob\u00b7jek\u00b7tiv", "ge\u00b7sinnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er ist denn objektiv;", "tokens": ["Er", "ist", "denn", "ob\u00b7jek\u00b7tiv", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch morgen ahnt die Welt und minnt,", "tokens": ["Doch", "mor\u00b7gen", "ahnt", "die", "Welt", "und", "minnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da seufzt er brunnentief.", "tokens": ["Da", "seufzt", "er", "brun\u00b7nen\u00b7tief", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Heut leugnet man den Herrn des All,", "tokens": ["Heut", "leug\u00b7net", "man", "den", "Herrn", "des", "All", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er leugnet, was er kann;", "tokens": ["Er", "leug\u00b7net", ",", "was", "er", "kann", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Horch! Naht dort nicht ein Beterschwall?", "tokens": ["Horch", "!", "Naht", "dort", "nicht", "ein", "Be\u00b7ter\u00b7schwall", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "ADV", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er schlie\u00dft sich singend an.", "tokens": ["Er", "schlie\u00dft", "sich", "sin\u00b7gend", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Heut treibt man Spanisch, morgen W\u00e4lsch,", "tokens": ["Heut", "treibt", "man", "Spa\u00b7nisch", ",", "mor\u00b7gen", "W\u00e4lsch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jetzt Griechisch, dann Sanskrit;", "tokens": ["Jetzt", "Grie\u00b7chisch", ",", "dann", "Sans\u00b7krit", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis auf sein l\u00e4ngst gelerntes Deutsch", "tokens": ["Bis", "auf", "sein", "l\u00e4ngst", "ge\u00b7lern\u00b7tes", "Deutsch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "PPOSAT", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lernt er die Sprachen mit.", "tokens": ["Lernt", "er", "die", "Spra\u00b7chen", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nun wird man radikal. Drauf hin!", "tokens": ["Nun", "wird", "man", "ra\u00b7di\u00b7kal", ".", "Drauf", "hin", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "$.", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein \u00e7a ira zur Hand!", "tokens": ["Ein", "\u00e7a", "i\u00b7ra", "zur", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die deutschen Frauen ehren ihn,", "tokens": ["Die", "deut\u00b7schen", "Frau\u00b7en", "eh\u00b7ren", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie einst den selgen ", "tokens": ["Wie", "einst", "den", "sel\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Doch kommt ein hoher Namenstag,", "tokens": ["Doch", "kommt", "ein", "ho\u00b7her", "Na\u00b7mens\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fchlt alle Welt sich weich;", "tokens": ["F\u00fchlt", "al\u00b7le", "Welt", "sich", "weich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PRF", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er eilet, was er eilen mag,", "tokens": ["Er", "ei\u00b7let", ",", "was", "er", "ei\u00b7len", "mag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schreibt ein Carmen gleich.", "tokens": ["Und", "schreibt", "ein", "Car\u00b7men", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und treibt er sich nicht rastlos um,", "tokens": ["Und", "treibt", "er", "sich", "nicht", "rast\u00b7los", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "PTKNEG", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "W\u00e4rs gar die h\u00f6chste Not;", "tokens": ["W\u00e4rs", "gar", "die", "h\u00f6chs\u00b7te", "Not", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00e4nd erst ein \u00dcbergang ihn stumm,", "tokens": ["F\u00e4nd", "erst", "ein", "\u00dc\u00b7ber\u00b7gang", "ihn", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er g\u00e4lte gleich f\u00fcr tot.", "tokens": ["Er", "g\u00e4l\u00b7te", "gleich", "f\u00fcr", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Soweit nun hats der Dichter schlecht,", "tokens": ["So\u00b7weit", "nun", "hats", "der", "Dich\u00b7ter", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch gut auch insoweit:", "tokens": ["Doch", "gut", "auch", "in\u00b7so\u00b7weit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil, wenn das ", "tokens": ["Weil", ",", "wenn", "das"], "token_info": ["word", "punct", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Er gern das ", "tokens": ["Er", "gern", "das"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}}}}