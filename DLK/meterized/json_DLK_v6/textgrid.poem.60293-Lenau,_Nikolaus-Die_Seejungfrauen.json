{"textgrid.poem.60293": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Die Seejungfrauen", "genre": "verse", "period": "N.A.", "pub_year": 1832, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freundlich wehn die Abendwinde,", "tokens": ["Freund\u00b7lich", "wehn", "die", "Ab\u00b7end\u00b7win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schimmern Mond und Sterne;", "tokens": ["Schim\u00b7mern", "Mond", "und", "Ster\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und das Schiff, so leicht und linde,", "tokens": ["Und", "das", "Schiff", ",", "so", "leicht", "und", "lin\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADV", "ADJD", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00e4gt mich nach der Ferne.", "tokens": ["Tr\u00e4gt", "mich", "nach", "der", "Fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Fried und Liebe, hold verbunden,", "tokens": ["Fried", "und", "Lie\u00b7be", ",", "hold", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schweben auf der Tiefe,", "tokens": ["Schwe\u00b7ben", "auf", "der", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ob der Tod mit seinen Wunden", "tokens": ["Ob", "der", "Tod", "mit", "sei\u00b7nen", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun auf immer schliefe.", "tokens": ["Nun", "auf", "im\u00b7mer", "schlie\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Sinnend starr ich nach dem hellen,", "tokens": ["Sin\u00b7nend", "starr", "ich", "nach", "dem", "hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Grenzenlosen Meere,", "tokens": ["Gren\u00b7zen\u00b7lo\u00b7sen", "Mee\u00b7re", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nach des Mondes und der Wellen", "tokens": ["Nach", "des", "Mon\u00b7des", "und", "der", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heimlichem Verkehre;", "tokens": ["Heim\u00b7li\u00b7chem", "Ver\u00b7keh\u00b7re", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+---+-", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Pl\u00f6tzlich seh ich rasche Wogen", "tokens": ["Pl\u00f6tz\u00b7lich", "seh", "ich", "ra\u00b7sche", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der Tiefe springen,", "tokens": ["Aus", "der", "Tie\u00b7fe", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Die da kommen hergezogen,", "tokens": ["Die", "da", "kom\u00b7men", "her\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen Gru\u00df zu bringen.", "tokens": ["Ei\u00b7nen", "Gru\u00df", "zu", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Ists ein Gru\u00df von Tiefverbannten", "tokens": ["Ists", "ein", "Gru\u00df", "von", "Tief\u00b7ver\u00b7bann\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An die Sternenlichter?", "tokens": ["An", "die", "Ster\u00b7nen\u00b7lich\u00b7ter", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Gilt das Gr\u00fc\u00dfen dem verwandten", "tokens": ["Gilt", "das", "Gr\u00fc\u00b7\u00dfen", "dem", "ver\u00b7wand\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ahnungsvollen Dichter?", "tokens": ["Ah\u00b7nungs\u00b7vol\u00b7len", "Dich\u00b7ter", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Tiefew\u00e4rts mit s\u00fc\u00dfem Zwange", "tokens": ["Tie\u00b7fe\u00b7w\u00e4rts", "mit", "s\u00fc\u00b7\u00dfem", "Zwan\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zieht es mich zu schauen,", "tokens": ["Zieht", "es", "mich", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mit geheimnisvollem Drange", "tokens": ["Mit", "ge\u00b7heim\u00b7nis\u00b7vol\u00b7lem", "Dran\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu den Seejungfrauen.", "tokens": ["Zu", "den", "See\u00b7jung\u00b7frau\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Ja, von euch, ihr R\u00e4tselhaften,", "tokens": ["Ja", ",", "von", "euch", ",", "ihr", "R\u00e4t\u00b7sel\u00b7haf\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kam dies volle Rauschen,", "tokens": ["Kam", "dies", "vol\u00b7le", "Rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Dran die Seele sehnend haften", "tokens": ["Dran", "die", "See\u00b7le", "seh\u00b7nend", "haf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df und niederlauschen.", "tokens": ["Mu\u00df", "und", "nie\u00b7der\u00b7lau\u00b7schen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "KON", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Ward euch ahnend eine Kunde", "tokens": ["Ward", "euch", "ah\u00b7nend", "ei\u00b7ne", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Im Korallenhage,", "tokens": ["Im", "Ko\u00b7ral\u00b7len\u00b7ha\u00b7ge", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df ein warmes Herz zur Stunde", "tokens": ["Da\u00df", "ein", "war\u00b7mes", "Herz", "zur", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Euch vor\u00fcberschlage?", "tokens": ["Euch", "vor\u00b7\u00fc\u00b7bersc\u00b7hla\u00b7ge", "?"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Gl\u00fccklich die Piloten waren,", "tokens": ["Gl\u00fcck\u00b7lich", "die", "Pi\u00b7lo\u00b7ten", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denen ihr erschienen", "tokens": ["De\u00b7nen", "ihr", "er\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PDS", "PPOSAT", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mit den sch\u00f6nen, wunderbaren,", "tokens": ["Mit", "den", "sch\u00f6\u00b7nen", ",", "wun\u00b7der\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieblich fremden Mienen!", "tokens": ["Lieb\u00b7lich", "frem\u00b7den", "Mie\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "K\u00f6nnt ich tauchen nieder, nieder", "tokens": ["K\u00f6nnt", "ich", "tau\u00b7chen", "nie\u00b7der", ",", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "VVFIN", "PTKVZ", "$,", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis in eure N\u00e4hen!", "tokens": ["Bis", "in", "eu\u00b7re", "N\u00e4\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "K\u00f6nnt ich eurer schlanken Glieder", "tokens": ["K\u00f6nnt", "ich", "eu\u00b7rer", "schlan\u00b7ken", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leisen Wandel sehen!", "tokens": ["Lei\u00b7sen", "Wan\u00b7del", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "Sehen euch den Reigen \u00fcben,", "tokens": ["Se\u00b7hen", "euch", "den", "Rei\u00b7gen", "\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwesterlich verschlungen,", "tokens": ["Schwes\u00b7ter\u00b7lich", "ver\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schweigend in den ewig tr\u00fcben", "tokens": ["Schwei\u00b7gend", "in", "den", "e\u00b7wig", "tr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meeresd\u00e4mmerungen!", "tokens": ["Mee\u00b7res\u00b7d\u00e4m\u00b7me\u00b7run\u00b7gen", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}