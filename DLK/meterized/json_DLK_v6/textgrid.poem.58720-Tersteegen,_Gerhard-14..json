{"textgrid.poem.58720": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wem ist's je gelungen, der sich wider ihn gelegt hat? Hiob 9, 4", "tokens": ["Wem", "ist's", "je", "ge\u00b7lun\u00b7gen", ",", "der", "sich", "wi\u00b7der", "ihn", "ge\u00b7legt", "hat", "?", "Hiob", "9", ",", "4"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "number", "punct", "number"], "pos": ["PWS", "VAFIN", "ADV", "VVPP", "$,", "PRELS", "PRF", "APPR", "PPER", "VVPP", "VAFIN", "$.", "NE", "CARD", "$,", "CARD"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Viel lieber geb' ich mich gefangen deiner Hand,", "tokens": ["Viel", "lie\u00b7ber", "geb'", "ich", "mich", "ge\u00b7fan\u00b7gen", "dei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als da\u00df ich mit dir wollte streiten;", "tokens": ["Als", "da\u00df", "ich", "mit", "dir", "woll\u00b7te", "strei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Ruhm von solchen Tapferkeiten", "tokens": ["Der", "Ruhm", "von", "sol\u00b7chen", "Tap\u00b7fer\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Reimt sich wohl schlecht auf meinen Stand.", "tokens": ["Reimt", "sich", "wohl", "schlecht", "auf", "mei\u00b7nen", "Stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich mu\u00df, mein Br\u00e4utigam, nur fechten wider mich,", "tokens": ["Ich", "mu\u00df", ",", "mein", "Br\u00e4u\u00b7ti\u00b7gam", ",", "nur", "fech\u00b7ten", "wi\u00b7der", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPOSAT", "NE", "$,", "ADV", "VVINF", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich unterwerfen deinen Schl\u00e4gen,", "tokens": ["Mich", "un\u00b7ter\u00b7wer\u00b7fen", "dei\u00b7nen", "Schl\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sonst w\u00e4r' ich \u00fcberaus verwegen,", "tokens": ["Sonst", "w\u00e4r'", "ich", "\u00fc\u00b7be\u00b7raus", "ver\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Recht dein Zorn erregte sich.", "tokens": ["Mit", "Recht", "dein", "Zorn", "er\u00b7reg\u00b7te", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach Herr, der Sieg ist dein, nimm meine Waffen an!", "tokens": ["Ach", "Herr", ",", "der", "Sieg", "ist", "dein", ",", "nimm", "mei\u00b7ne", "Waf\u00b7fen", "an", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "ART", "NN", "VAFIN", "PPOSAT", "$,", "VVIMP", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich will nur deinen Feind bestreiten,", "tokens": ["Ich", "will", "nur", "dei\u00b7nen", "Feind", "be\u00b7strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht deine tausend Freundlichkeiten,", "tokens": ["Nicht", "dei\u00b7ne", "tau\u00b7send", "Freund\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dran Herz und alles untertan.", "tokens": ["Dran", "Herz", "und", "al\u00b7les", "un\u00b7ter\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "KON", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du gro\u00dfer Gott, ich will denn dein Gefangner sein,", "tokens": ["Du", "gro\u00b7\u00dfer", "Gott", ",", "ich", "will", "denn", "dein", "Ge\u00b7fang\u00b7ner", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "VMFIN", "ADV", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O ja, ich will von ganzem Herzen", "tokens": ["O", "ja", ",", "ich", "will", "von", "gan\u00b7zem", "Her\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ITJ", "$,", "PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dies gro\u00dfe Gl\u00fcck auch nie verscherzen,", "tokens": ["Dies", "gro\u00b7\u00dfe", "Gl\u00fcck", "auch", "nie", "ver\u00b7scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich litte lieber alle Pein.", "tokens": ["Ich", "lit\u00b7te", "lie\u00b7ber", "al\u00b7le", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}