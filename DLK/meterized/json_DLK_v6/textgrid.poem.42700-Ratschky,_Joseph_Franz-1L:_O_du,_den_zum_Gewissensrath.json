{"textgrid.poem.42700": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: O du, den zum Gewissensrath", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du, den zum Gewissensrath", "tokens": ["O", "du", ",", "den", "zum", "Ge\u00b7wis\u00b7sens\u00b7rath"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich Theodor erkoren,", "tokens": ["Sich", "Theo\u00b7dor", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "NE", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "O lass, ehrw\u00fcrdigster Pr\u00e4lat!", "tokens": ["O", "lass", ",", "ehr\u00b7w\u00fcr\u00b7digs\u00b7ter", "Pr\u00e4\u00b7lat", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "In deinen hohen Ohren,", "tokens": ["In", "dei\u00b7nen", "ho\u00b7hen", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die zwar durchlauchtigen Vergehn", "tokens": ["Die", "zwar", "durch\u00b7lauch\u00b7ti\u00b7gen", "Ver\u00b7gehn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sonst bloss allein zu Diensten stehn,", "tokens": ["Sonst", "bloss", "al\u00b7lein", "zu", "Diens\u00b7ten", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nun auch gemeine S\u00fcnden", "tokens": ["Nun", "auch", "ge\u00b7mei\u00b7ne", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein Zufluchtspl\u00e4tzchen finden!", "tokens": ["Ein", "Zu\u00b7fluchts\u00b7pl\u00e4tz\u00b7chen", "fin\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich klag', o hochgeweihter Mann!", "tokens": ["Ich", "klag'", ",", "o", "hoch\u00b7ge\u00b7weih\u00b7ter", "Mann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor deinem Richterstuhle", "tokens": ["Vor", "dei\u00b7nem", "Rich\u00b7ter\u00b7stuh\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mich als ein lockres Herrchen an,", "tokens": ["Mich", "als", "ein", "lock\u00b7res", "Herr\u00b7chen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das, von der dreisten Schule", "tokens": ["Das", ",", "von", "der", "dreis\u00b7ten", "Schu\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der neuern Philosophenzunft", "tokens": ["Der", "neu\u00b7ern", "Phi\u00b7lo\u00b7so\u00b7phen\u00b7zunft"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verf\u00fchrt, der menschlichen Vernunft", "tokens": ["Ver\u00b7f\u00fchrt", ",", "der", "menschli\u00b7chen", "Ver\u00b7nunft"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Oft mehr, als manchem Haupte", "tokens": ["Oft", "mehr", ",", "als", "man\u00b7chem", "Haup\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Mutter Kirche, glaubte.", "tokens": ["Der", "Mut\u00b7ter", "Kir\u00b7che", ",", "glaub\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich w\u00e4hnte, dass die Maurerey", "tokens": ["Ich", "w\u00e4hn\u00b7te", ",", "dass", "die", "Mau\u00b7re\u00b7rey"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein ehrenvoller Orden,", "tokens": ["Ein", "eh\u00b7ren\u00b7vol\u00b7ler", "Or\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und (Herr, verzeih mir's) besser sey,", "tokens": ["Und", "(", "Herr", ",", "ver\u00b7zeih", "mir's", ")", "bes\u00b7ser", "sey", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$(", "NN", "$,", "VVIMP", "NE", "$(", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als alle tr\u00e4gen Horden", "tokens": ["Als", "al\u00b7le", "tr\u00e4\u00b7gen", "Hor\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Derwische, die zum Ruin", "tokens": ["Der", "Der\u00b7wi\u00b7sche", ",", "die", "zum", "Ru\u00b7in"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Des Landvolks rings durch Bayern ziehn,", "tokens": ["Des", "Land\u00b7volks", "rings", "durch", "Bay\u00b7ern", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NE", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Um frommen Christenkindern", "tokens": ["Um", "from\u00b7men", "Chris\u00b7ten\u00b7kin\u00b7dern"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die H\u00e4user auszupl\u00fcndern.", "tokens": ["Die", "H\u00e4u\u00b7ser", "aus\u00b7zu\u00b7pl\u00fcn\u00b7dern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich opferte gewissenlos", "tokens": ["Ich", "op\u00b7fer\u00b7te", "ge\u00b7wis\u00b7sen\u00b7los"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bey Baylen und Volt\u00e4ren", "tokens": ["Bey", "Bay\u00b7len", "und", "Vol\u00b7t\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Manch St\u00fcndchen auf, und hielt Rousseau's", "tokens": ["Manch", "St\u00fcnd\u00b7chen", "auf", ",", "und", "hielt", "Rous\u00b7seau's"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "PTKVZ", "$,", "KON", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verblendung hoch in Ehren.", "tokens": ["Ver\u00b7blen\u00b7dung", "hoch", "in", "Eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich pflegte ", "tokens": ["Ich", "pfleg\u00b7te"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Herrn Zabuesnigs ", "tokens": ["Herrn", "Za\u00b7bu\u00b7es\u00b7nigs"], "token_info": ["word", "word"], "pos": ["NN", "NE"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Oft frevelhaft beym Schmauchen", "tokens": ["Oft", "fre\u00b7vel\u00b7haft", "beym", "Schmau\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Als Fidibus zu brauchen.", "tokens": ["Als", "Fi\u00b7di\u00b7bus", "zu", "brau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich glaubte, dass zur Professur", "tokens": ["Ich", "glaub\u00b7te", ",", "dass", "zur", "Pro\u00b7fes\u00b7sur"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Kirchenrechts nur Layen,", "tokens": ["Des", "Kir\u00b7chen\u00b7rechts", "nur", "La\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nicht M\u00f6nche, die dem Staat ein Schwur", "tokens": ["Nicht", "M\u00f6n\u00b7che", ",", "die", "dem", "Staat", "ein", "Schwur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "NN", "$,", "PRELS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entwandte, tauglich seyen,", "tokens": ["Ent\u00b7wand\u00b7te", ",", "taug\u00b7lich", "se\u00b7yen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVINF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Und dass bey einem solchen Amt", "tokens": ["Und", "dass", "bey", "ei\u00b7nem", "sol\u00b7chen", "Amt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ART", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Kuttentr\u00e4gern insgesamt", "tokens": ["Den", "Kut\u00b7ten\u00b7tr\u00e4\u00b7gern", "ins\u00b7ge\u00b7samt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht mehr zu trauen w\u00e4re,", "tokens": ["Nicht", "mehr", "zu", "trau\u00b7en", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Als Katzen bey dem Schmeere.", "tokens": ["Als", "Kat\u00b7zen", "bey", "dem", "Schmee\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich h\u00e4tte gerne f\u00fcr ein Paar", "tokens": ["Ich", "h\u00e4t\u00b7te", "ger\u00b7ne", "f\u00fcr", "ein", "Paar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Zaupsers lahmen Fingern", "tokens": ["Von", "Zaup\u00b7sers", "lah\u00b7men", "Fin\u00b7gern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die ganze wohlbeleibte Schaar", "tokens": ["Die", "gan\u00b7ze", "wohl\u00b7be\u00b7leib\u00b7te", "Schaar"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Bayerns Ordensj\u00fcngern", "tokens": ["Von", "Bay\u00b7erns", "Or\u00b7dens\u00b7j\u00fcn\u00b7gern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit Haar und Bart und Kopf und Fuss", "tokens": ["Mit", "Haar", "und", "Bart", "und", "Kopf", "und", "Fuss"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ja (weil ich's doch gestehen muss)", "tokens": ["Ja", "(", "weil", "ich's", "doch", "ge\u00b7ste\u00b7hen", "muss", ")"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "KOUS", "PIS", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O Frank, bey meinem Leben!", "tokens": ["O", "Frank", ",", "bey", "mei\u00b7nem", "Le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dich selber hingegeben.", "tokens": ["Dich", "sel\u00b7ber", "hin\u00b7ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich sah (bis du des Bessern mich", "tokens": ["Ich", "sah", "(", "bis", "du", "des", "Bes\u00b7sern", "mich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "ART", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Predigtstuhl belehrtest,", "tokens": ["Vom", "Pre\u00b7digt\u00b7stuhl", "be\u00b7lehr\u00b7test", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und, dass dir's Ernst war, feyerlich", "tokens": ["Und", ",", "dass", "dir's", "Ernst", "war", ",", "fey\u00b7er\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "$,", "KOUS", "NE", "NE", "VAFIN", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am Maurervolk bew\u00e4hrtest)", "tokens": ["Am", "Mau\u00b7rer\u00b7volk", "be\u00b7w\u00e4hr\u00b7test", ")"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Menschen st\u00e4ts f\u00fcr Br\u00fcder an,", "tokens": ["Die", "Men\u00b7schen", "st\u00e4ts", "f\u00fcr", "Br\u00fc\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und w\u00e4hnt', es w\u00e4re wohlgethan,", "tokens": ["Und", "w\u00e4hnt'", ",", "es", "w\u00e4\u00b7re", "wohl\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn man die Menschenliebe", "tokens": ["Wenn", "man", "die", "Men\u00b7schen\u00b7lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So weit, als m\u00f6glich, triebe.", "tokens": ["So", "weit", ",", "als", "m\u00f6g\u00b7lich", ",", "trie\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich weiss, dass dieser S\u00fcndenschwall", "tokens": ["Ich", "weiss", ",", "dass", "die\u00b7ser", "S\u00fcn\u00b7den\u00b7schwall"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich nach dem Kirchenrechte", "tokens": ["Mich", "nach", "dem", "Kir\u00b7chen\u00b7rech\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der frommen Bayern Knall und Fall", "tokens": ["Der", "from\u00b7men", "Bay\u00b7ern", "Knall", "und", "Fall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun an den Bratspiess br\u00e4chte:", "tokens": ["Nun", "an", "den", "Brat\u00b7spiess", "br\u00e4ch\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch, Vater Frank, verzeih! ... Wo nicht,", "tokens": ["Doch", ",", "Va\u00b7ter", "Frank", ",", "ver\u00b7zeih", "!", "...", "Wo", "nicht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "NE", "$,", "VVIMP", "$.", "$(", "PWAV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": 