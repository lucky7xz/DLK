{"textgrid.poem.57634": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Mond und Sonne", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein leer Gesicht, wie ausgebrannt,", "tokens": ["Ein", "leer", "Ge\u00b7sicht", ",", "wie", "aus\u00b7ge\u00b7brannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein kalter, m\u00fcder Schein,", "tokens": ["Ein", "kal\u00b7ter", ",", "m\u00fc\u00b7der", "Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht Flammentraum, nicht Glutidee", "tokens": ["Nicht", "Flam\u00b7men\u00b7traum", ",", "nicht", "Glu\u00b7ti\u00b7dee"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "NN", "$,", "PTKNEG", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bereitet ihm mehr Pein.", "tokens": ["Be\u00b7rei\u00b7tet", "ihm", "mehr", "Pein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Kalt, tot und still und ach so alt,", "tokens": ["Kalt", ",", "tot", "und", "still", "und", "ach", "so", "alt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "KON", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ohne Lust und Leid \u2013", "tokens": ["Und", "oh\u00b7ne", "Lust", "und", "Leid", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es liegt die Jugend hinter ihm", "tokens": ["Es", "liegt", "die", "Ju\u00b7gend", "hin\u00b7ter", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Millionen Jahre weit.", "tokens": ["Mil\u00b7lion\u00b7en", "Jah\u00b7re", "weit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gl\u00fchende, kochende Lodergedanken,", "tokens": ["Gl\u00fc\u00b7hen\u00b7de", ",", "ko\u00b7chen\u00b7de", "Lo\u00b7der\u00b7ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Funkelnde Blitze im brennenden Blick,", "tokens": ["Fun\u00b7keln\u00b7de", "Blit\u00b7ze", "im", "bren\u00b7nen\u00b7den", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Aber tieftraurige, nachtschwarze Flecken", "tokens": ["A\u00b7ber", "tief\u00b7trau\u00b7ri\u00b7ge", ",", "nacht\u00b7schwar\u00b7ze", "Fle\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "ADJA", "$,", "ADJA", "NN"], "meter": "+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "K\u00fcnden den fressenden Mangel an Gl\u00fcck.", "tokens": ["K\u00fcn\u00b7den", "den", "fres\u00b7sen\u00b7den", "Man\u00b7gel", "an", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Kampf ist das Leben und Not ist das Denken,", "tokens": ["Kampf", "ist", "das", "Le\u00b7ben", "und", "Not", "ist", "das", "Den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "KON", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Schmerzende Angst in dem Glutherzen thront \u2013", "tokens": ["Schmer\u00b7zen\u00b7de", "Angst", "in", "dem", "Glut\u00b7her\u00b7zen", "thront", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "+--+--++-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Warte nur, gl\u00fchende, lodernde Sonne,", "tokens": ["War\u00b7te", "nur", ",", "gl\u00fc\u00b7hen\u00b7de", ",", "lo\u00b7dern\u00b7de", "Son\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Du auch bist einst noch so still wie der Mond.", "tokens": ["Du", "auch", "bist", "einst", "noch", "so", "still", "wie", "der", "Mond", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "ADV", "ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}}}}}