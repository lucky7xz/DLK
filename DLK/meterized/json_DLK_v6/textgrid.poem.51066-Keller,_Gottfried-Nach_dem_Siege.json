{"textgrid.poem.51066": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Nach dem Siege", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft rot vor Scham ergl\u00fchen eure Wangen,", "tokens": ["La\u00dft", "rot", "vor", "Scham", "er\u00b7gl\u00fc\u00b7hen", "eu\u00b7re", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "APPR", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ihr mit eurer Reime leerem Beten", "tokens": ["Die", "ihr", "mit", "eu\u00b7rer", "Rei\u00b7me", "lee\u00b7rem", "Be\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Euch anschickt, vor ein tapfres Volk zu treten,", "tokens": ["Euch", "an\u00b7schickt", ",", "vor", "ein", "tapf\u00b7res", "Volk", "zu", "tre\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das eben kommt von Tat und Sieg gegangen!", "tokens": ["Das", "e\u00b7ben", "kommt", "von", "Tat", "und", "Sieg", "ge\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Des Trommlers Schlegel, die im Wirbel sprangen,", "tokens": ["Des", "Tromm\u00b7lers", "Schle\u00b7gel", ",", "die", "im", "Wir\u00b7bel", "spran\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der rauhste Tagruf gellender Trompeten,", "tokens": ["Der", "rauhs\u00b7te", "Tag\u00b7ruf", "gel\u00b7len\u00b7der", "Trom\u00b7pe\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie gelten jetzo mehr, ihr Nach-Propheten,", "tokens": ["Sie", "gel\u00b7ten", "jet\u00b7zo", "mehr", ",", "ihr", "Nach\u00b7Pro\u00b7phe\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als all eu'r unn\u00fctz eitles Versefangen!", "tokens": ["Als", "all", "eu'r", "un\u00b7n\u00fctz", "eit\u00b7les", "Ver\u00b7se\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "PPOSAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der letzte schlichte W\u00e4chter vor dem Heere,", "tokens": ["Der", "letz\u00b7te", "schlich\u00b7te", "W\u00e4ch\u00b7ter", "vor", "dem", "Hee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der, Treu und Pflicht im Herzen, hat getragen", "tokens": ["Der", ",", "Treu", "und", "Pflicht", "im", "Her\u00b7zen", ",", "hat", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "$,", "NN", "KON", "NN", "APPRART", "NN", "$,", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In kalter Sternennacht die blanke Wehre,", "tokens": ["In", "kal\u00b7ter", "Ster\u00b7nen\u00b7nacht", "die", "blan\u00b7ke", "Weh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und jeder, der nur ", "tokens": ["Und", "je\u00b7der", ",", "der", "nur"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "PIS", "$,", "PRELS", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Ist nun ein K\u00f6nig von lebend'ger Ehre \u2013", "tokens": ["Ist", "nun", "ein", "K\u00f6\u00b7nig", "von", "le\u00b7ben\u00b7d'\u00b7ger", "Eh\u00b7re", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Was soll ihm unser Singen noch und Sagen?", "tokens": ["Was", "soll", "ihm", "un\u00b7ser", "Sin\u00b7gen", "noch", "und", "Sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PPOSAT", "NN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}