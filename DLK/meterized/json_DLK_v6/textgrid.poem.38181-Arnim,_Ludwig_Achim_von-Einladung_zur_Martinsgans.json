{"textgrid.poem.38181": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Einladung zur Martinsgans", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann der heilge Sankt Martin", "tokens": ["Wann", "der", "heil\u00b7ge", "Sankt", "Mar\u00b7tin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "VVFIN", "NE"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Will der Bischofsehr entfliehn,", "tokens": ["Will", "der", "Bi\u00b7schof\u00b7sehr", "ent\u00b7fliehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sitzt er in dem G\u00e4nsestall", "tokens": ["Sitzt", "er", "in", "dem", "G\u00e4n\u00b7se\u00b7stall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Niemand findt ihn \u00fcberall,", "tokens": ["Nie\u00b7mand", "findt", "ihn", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis der G\u00e4nse gro\u00df Geschrey", "tokens": ["Bis", "der", "G\u00e4n\u00b7se", "gro\u00df", "Ge\u00b7schrey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Seine Sucher ruft herbey.", "tokens": ["Sei\u00b7ne", "Su\u00b7cher", "ruft", "her\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun dieweil das Gickgackslied", "tokens": ["Nun", "die\u00b7weil", "das", "Gick\u00b7gacks\u00b7lied"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Diesen heilgen Mann verrieth,", "tokens": ["Die\u00b7sen", "heil\u00b7gen", "Mann", "ver\u00b7rieth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Daf\u00fcr thut am Martinstag", "tokens": ["Da\u00b7f\u00fcr", "thut", "am", "Mar\u00b7tins\u00b7tag"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPRART", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Man den G\u00e4nsen diese Plag,", "tokens": ["Man", "den", "G\u00e4n\u00b7sen", "die\u00b7se", "Plag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ein strenges Todesrecht", "tokens": ["Da\u00df", "ein", "stren\u00b7ges", "To\u00b7des\u00b7recht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gehn mu\u00df \u00fcber ihr Geschlecht.", "tokens": ["Gehn", "mu\u00df", "\u00fc\u00b7ber", "ihr", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Drum wir billig halten auch", "tokens": ["Drum", "wir", "bil\u00b7lig", "hal\u00b7ten", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "ADJD", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Diesen alten Martinsbrauch,", "tokens": ["Die\u00b7sen", "al\u00b7ten", "Mar\u00b7tins\u00b7brauch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Laden fein zu diesem Fest", "tokens": ["La\u00b7den", "fein", "zu", "die\u00b7sem", "Fest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsre allerliebste G\u00e4st", "tokens": ["Uns\u00b7re", "al\u00b7ler\u00b7liebs\u00b7te", "G\u00e4st"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf die Martinsg\u00e4nslein ein,", "tokens": ["Auf", "die", "Mar\u00b7tins\u00b7g\u00e4ns\u00b7lein", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bey Musik und k\u00fchlem Wein.", "tokens": ["Bey", "Mu\u00b7sik", "und", "k\u00fch\u00b7lem", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}