{"textgrid.poem.42064": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Robinson sucht Gott", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ists darum, da\u00df du schweigst, Gott, weil du haust", "tokens": ["Ists", "da\u00b7rum", ",", "da\u00df", "du", "schweigst", ",", "Gott", ",", "weil", "du", "haust"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PAV", "$,", "KOUS", "PPER", "VVFIN", "$,", "NN", "$,", "KOUS", "PPER", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In dem Verstummenden? Bist du der leere", "tokens": ["In", "dem", "Ver\u00b7stum\u00b7men\u00b7den", "?", "Bist", "du", "der", "lee\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "VAFIN", "PPER", "ART", "ADJA"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Schallose Raum, in dem es schweigt, und baust", "tokens": ["Schal\u00b7lo\u00b7se", "Raum", ",", "in", "dem", "es", "schweigt", ",", "und", "baust"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,", "KON", "VVFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Dich rund um uns? Bist, den ich kniend ehre,", "tokens": ["Dich", "rund", "um", "uns", "?", "Bist", ",", "den", "ich", "kni\u00b7end", "eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PPER", "$.", "VAFIN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Der Angespiene Eines, und es gleichen", "tokens": ["Der", "An\u00b7ge\u00b7spi\u00b7e\u00b7ne", "Ei\u00b7nes", ",", "und", "es", "glei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PIS", "$,", "KON", "PPER", "VVINF"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Gebet und Fluch dir Ohrenlosem l\u00e4ngst", "tokens": ["Ge\u00b7bet", "und", "Fluch", "dir", "Oh\u00b7ren\u00b7lo\u00b7sem", "l\u00e4ngst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "PPER", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sich aus, eh sie ert\u00f6nten? Und es streichen", "tokens": ["Sich", "aus", ",", "eh", "sie", "er\u00b7t\u00f6n\u00b7ten", "?", "Und", "es", "strei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "PTKVZ", "$,", "KOUS", "PPER", "VVFIN", "$.", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sich Schmerz und Freude, da\u00df nichts bleibt? Du h\u00e4ngst", "tokens": ["Sich", "Schmerz", "und", "Freu\u00b7de", ",", "da\u00df", "nichts", "bleibt", "?", "Du", "h\u00e4ngst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PRF", "NN", "KON", "NN", "$,", "KOUS", "PIS", "VVFIN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ein gro\u00dfer Ausgleich, bist wie das Bemessen", "tokens": ["Ein", "gro\u00b7\u00dfer", "Aus\u00b7gleich", ",", "bist", "wie", "das", "Be\u00b7mes\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VAFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der Wage, die im Gleichgewicht besteht?", "tokens": ["Der", "Wa\u00b7ge", ",", "die", "im", "Gleich\u00b7ge\u00b7wicht", "be\u00b7steht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Alles geht auf? Und da\u00df mir hier, vergessen", "tokens": ["Al\u00b7les", "geht", "auf", "?", "Und", "da\u00df", "mir", "hier", ",", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PIS", "VVFIN", "PTKVZ", "$.", "KON", "KOUS", "PPER", "ADV", "$,", "VVPP"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.12": {"text": "Fast von mir selbst, die Einsamkeit vergeht", "tokens": ["Fast", "von", "mir", "selbst", ",", "die", "Ein\u00b7sam\u00b7keit", "ver\u00b7geht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "ADV", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und sich erneut, unendlich, trifft dich nicht?!", "tokens": ["Und", "sich", "er\u00b7neut", ",", "un\u00b7end\u00b7lich", ",", "trifft", "dich", "nicht", "?!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADJD", "$,", "ADJD", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Du wei\u00dft sie dr\u00fcben gehn in bunten Scharen,", "tokens": ["Du", "wei\u00dft", "sie", "dr\u00fc\u00b7ben", "gehn", "in", "bun\u00b7ten", "Scha\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Sie staunen vor den rasch versch\u00e4umten Jahren,", "tokens": ["Sie", "stau\u00b7nen", "vor", "den", "rasch", "ver\u00b7sch\u00e4um\u00b7ten", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und warfst mich hin: ihr einiges Gewicht.", "tokens": ["Und", "warfst", "mich", "hin", ":", "ihr", "ei\u00b7ni\u00b7ges", "Ge\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKVZ", "$.", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}