{"textgrid.poem.67948": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "16. Billiges Ungl\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wem Gott das seltne Gl\u00fcck verlieh,", "tokens": ["Wem", "Gott", "das", "selt\u00b7ne", "Gl\u00fcck", "ver\u00b7lieh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich selbst sein eigner Herr zu seyn;", "tokens": ["Sich", "selbst", "sein", "eig\u00b7ner", "Herr", "zu", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PPOSAT", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und freut sich dieses Gl\u00fcckes nie,", "tokens": ["Und", "freut", "sich", "die\u00b7ses", "Gl\u00fc\u00b7ckes", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PDAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und will nur in dem falschen Schein", "tokens": ["Und", "will", "nur", "in", "dem", "fal\u00b7schen", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erhabner Grossen sich erfreun:", "tokens": ["Er\u00b7hab\u00b7ner", "Gros\u00b7sen", "sich", "er\u00b7freun", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der ist es werth, ihr Knecht zu seyn.", "tokens": ["Der", "ist", "es", "werth", ",", "ihr", "Knecht", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "$,", "PPOSAT", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer still und gl\u00fccklich leben kann,", "tokens": ["Wer", "still", "und", "gl\u00fcck\u00b7lich", "le\u00b7ben", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn er ein armes M\u00e4dchen freyt;", "tokens": ["Wenn", "er", "ein", "ar\u00b7mes", "M\u00e4d\u00b7chen", "freyt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und geht des reichen Teufels Bahn", "tokens": ["Und", "geht", "des", "rei\u00b7chen", "Teu\u00b7fels", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am Weibe, die mit Zank und Streit", "tokens": ["Am", "Wei\u00b7be", ",", "die", "mit", "Zank", "und", "Streit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihm t\u00e4glich Sonn' und Mond verleid't:", "tokens": ["Ihm", "t\u00e4g\u00b7lich", "Sonn'", "und", "Mond", "ver\u00b7leid't", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ists werth, da\u00df ihn es ewig reut.", "tokens": ["Ists", "werth", ",", "da\u00df", "ihn", "es", "e\u00b7wig", "reut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wen die Natur zur Freud und Lust,", "tokens": ["Wen", "die", "Na\u00b7tur", "zur", "Freud", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPRART", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und zarten Liebe bildete;", "tokens": ["Und", "zar\u00b7ten", "Lie\u00b7be", "bil\u00b7de\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und h\u00e4ngt sich an der Wollust Brust,", "tokens": ["Und", "h\u00e4ngt", "sich", "an", "der", "Wol\u00b7lust", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sauget Schwachheit, Gram und Weh,", "tokens": ["Und", "sau\u00b7get", "Schwach\u00b7heit", ",", "Gram", "und", "Weh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und alt nun noch heirathete,", "tokens": ["Und", "alt", "nun", "noch", "hei\u00b7ra\u00b7the\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ein junges Weib \u2013 o weh! o weh!", "tokens": ["Ein", "jun\u00b7ges", "Weib", "\u2013", "o", "weh", "!", "o", "weh", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "FM", "PTKVZ", "$.", "FM", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wem die Natur gesunden Leib", "tokens": ["Wem", "die", "Na\u00b7tur", "ge\u00b7sun\u00b7den", "Leib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und festen Arm dazu verlieh;", "tokens": ["Und", "fes\u00b7ten", "Arm", "da\u00b7zu", "ver\u00b7lieh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und w\u00e4hlt sich nun zum Zeitvertreib", "tokens": ["Und", "w\u00e4hlt", "sich", "nun", "zum", "Zeit\u00b7ver\u00b7treib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der hochgelahrten Doctors M\u00fch,", "tokens": ["Der", "hoch\u00b7ge\u00b7lahr\u00b7ten", "Doc\u00b7tors", "M\u00fch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und consultirt sie sp\u00e4t und fr\u00fch \u2013", "tokens": ["Und", "con\u00b7sul\u00b7tirt", "sie", "sp\u00e4t", "und", "fr\u00fch", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ins Grab hin consultir' er sie.", "tokens": ["Ins", "Grab", "hin", "con\u00b7sul\u00b7tir'", "er", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So wem Gott guten Sinn verlieh,", "tokens": ["So", "wem", "Gott", "gu\u00b7ten", "Sinn", "ver\u00b7lieh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ihn verlieh ihm gar umsunst;", "tokens": ["Und", "ihn", "ver\u00b7lieh", "ihm", "gar", "um\u00b7sunst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er h\u00e4ngt sich an der Thorheit M\u00fch,", "tokens": ["Er", "h\u00e4ngt", "sich", "an", "der", "Thor\u00b7heit", "M\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kr\u00fcppelt um der Narren Kunst,", "tokens": ["Und", "kr\u00fcp\u00b7pelt", "um", "der", "Nar\u00b7ren", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein grosser Mann zu seyn einmal \u2013", "tokens": ["Ein", "gros\u00b7ser", "Mann", "zu", "seyn", "ein\u00b7mal", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VAINF", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Seys \u2013 im gelehrten Hospital.", "tokens": ["Seys", "\u2013", "im", "ge\u00b7lehr\u00b7ten", "Hos\u00b7pi\u00b7tal", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}