{"textgrid.poem.40864": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Stimmen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Gent auf dem Markte wehten erbeutete Fahnen zur Schau,", "tokens": ["Zu", "Gent", "auf", "dem", "Mark\u00b7te", "weh\u00b7ten", "er\u00b7beu\u00b7te\u00b7te", "Fah\u00b7nen", "zur", "Schau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVFIN", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und Siegesbogen erh\u00f6hten sich rings in stolzem Bau,", "tokens": ["Und", "Sie\u00b7ges\u00b7bo\u00b7gen", "er\u00b7h\u00f6h\u00b7ten", "sich", "rings", "in", "stol\u00b7zem", "Bau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PRF", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Vor dem Palast der F\u00fcrstin da hielt der Siegeszug,", "tokens": ["Vor", "dem", "Pa\u00b7last", "der", "F\u00fcrs\u00b7tin", "da", "hielt", "der", "Sie\u00b7ges\u00b7zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Inmitten ein schmucker Krieger, der l\u00e4chelnd ein Kn\u00e4blein trug.", "tokens": ["In\u00b7mit\u00b7ten", "ein", "schmu\u00b7cker", "Krie\u00b7ger", ",", "der", "l\u00e4\u00b7chelnd", "ein", "Kn\u00e4\u00b7blein", "trug", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Es winken seine Blumen dem G\u00e4rtner so freundlich nicht,", "tokens": ["Es", "win\u00b7ken", "sei\u00b7ne", "Blu\u00b7men", "dem", "G\u00e4rt\u00b7ner", "so", "freund\u00b7lich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN", "ADV", "ADJD", "PTKNEG", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wie dem begl\u00fcckten Vater des Kindes Augenlicht;", "tokens": ["Wie", "dem", "be\u00b7gl\u00fcck\u00b7ten", "Va\u00b7ter", "des", "Kin\u00b7des", "Au\u00b7gen\u00b7licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der Jungfrau Bildni\u00df spiegelt ein klarer Quell zur\u00fcck,", "tokens": ["Der", "Jung\u00b7frau", "Bild\u00b7ni\u00df", "spie\u00b7gelt", "ein", "kla\u00b7rer", "Quell", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die Mutter sucht's und findet's begl\u00fcckt in des S\u00e4uglings Blick.", "tokens": ["Die", "Mut\u00b7ter", "sucht's", "und", "fin\u00b7det's", "be\u00b7gl\u00fcckt", "in", "des", "S\u00e4ug\u00b7lings", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "NE", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "O Max, wie schien dir so herrlich des Gl\u00fcckes Sonnenglanz!", "tokens": ["O", "Max", ",", "wie", "schien", "dir", "so", "herr\u00b7lich", "des", "Gl\u00fc\u00b7ckes", "Son\u00b7nen\u00b7glanz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWAV", "VVFIN", "PPER", "ADV", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dein Kind h\u00e4ngt dir am Munde,", "tokens": ["Dein", "Kind", "h\u00e4ngt", "dir", "am", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In deinem Arm die Geliebte, manch treuer Freund dir nah,", "tokens": ["In", "dei\u00b7nem", "Arm", "die", "Ge\u00b7lieb\u00b7te", ",", "manch", "treu\u00b7er", "Freund", "dir", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$,", "PIAT", "ADJA", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wo ist ein h\u00f6her Begl\u00fcckter, so weit die Sonne sah!", "tokens": ["Wo", "ist", "ein", "h\u00f6\u00b7her", "Be\u00b7gl\u00fcck\u00b7ter", ",", "so", "weit", "die", "Son\u00b7ne", "sah", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "$,", "ADV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Und Siegesfest und Jubel durchziehn das ganze Land,", "tokens": ["Und", "Sie\u00b7ges\u00b7fest", "und", "Ju\u00b7bel", "durch\u00b7ziehn", "das", "gan\u00b7ze", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und widerhallend jauchzt es bis an der Marken Rand,", "tokens": ["Und", "wi\u00b7der\u00b7hal\u00b7lend", "jauchzt", "es", "bis", "an", "der", "Mar\u00b7ken", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "In Burgen und in St\u00e4dten, in Henn'gau und Burgund,", "tokens": ["In", "Bur\u00b7gen", "und", "in", "St\u00e4d\u00b7ten", ",", "in", "Henn'\u00b7gau", "und", "Bur\u00b7gund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da sind die Lilien zertreten, da flattert der Aar zur Stund'.", "tokens": ["Da", "sind", "die", "Li\u00b7li\u00b7en", "zer\u00b7tre\u00b7ten", ",", "da", "flat\u00b7tert", "der", "Aar", "zur", "Stund'", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+--+--+-+", "measure": "iambic.septa.relaxed"}}, "stanza.5": {"line.1": {"text": "Da murmelt Frankreichs Ludwig halb l\u00e4chelnd in den Bart:", "tokens": ["Da", "mur\u00b7melt", "Fran\u00b7kreichs", "Lud\u00b7wig", "halb", "l\u00e4\u00b7chelnd", "in", "den", "Bart", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "NE", "ADJD", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbder Aar ist Zugvogel worden, doch ganz besondrer Art,", "tokens": ["\u00bb", "der", "Aar", "ist", "Zug\u00b7vo\u00b7gel", "wor\u00b7den", ",", "doch", "ganz", "be\u00b7sond\u00b7rer", "Art", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "NN", "VAPP", "$,", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+---+-+-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Die Schwalben und St\u00f6rche kehren im Fr\u00fchling wieder nach Haus;", "tokens": ["Die", "Schwal\u00b7ben", "und", "St\u00f6r\u00b7che", "keh\u00b7ren", "im", "Fr\u00fch\u00b7ling", "wie\u00b7der", "nach", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "APPRART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+--+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Doch seltsam ist's, der Adler blieb bis zum Herbste aus.\u00ab", "tokens": ["Doch", "selt\u00b7sam", "ist's", ",", "der", "Ad\u00b7ler", "blieb", "bis", "zum", "Herbs\u00b7te", "aus", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Doch Max zu Gent, der scherzet bei frohem Siegesmahl:", "tokens": ["Doch", "Max", "zu", "Gent", ",", "der", "scher\u00b7zet", "bei", "fro\u00b7hem", "Sie\u00b7ges\u00b7mahl", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPR", "NN", "$,", "PRELS", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbwas Wunder, da\u00df im Herbste, bei matterm Sonnenstrahl,", "tokens": ["\u00bb", "was", "Wun\u00b7der", ",", "da\u00df", "im", "Herbs\u00b7te", ",", "bei", "mat\u00b7term", "Son\u00b7nen\u00b7strahl", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "NN", "$,", "KOUS", "APPRART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Nun Rosen, Nelk' und Tulpen und alle Blumen vergl\u00fchn?", "tokens": ["Nun", "Ro\u00b7sen", ",", "Nel\u00b7k'", "und", "Tul\u00b7pen", "und", "al\u00b7le", "Blu\u00b7men", "ver\u00b7gl\u00fchn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$,", "NE", "KON", "NN", "KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Drum d\u00e4ucht mir's auch nat\u00fcrlich, da\u00df nimmer die Lilien bl\u00fchn.\u00ab", "tokens": ["Drum", "d\u00e4ucht", "mir's", "auch", "na\u00b7t\u00fcr\u00b7lich", ",", "da\u00df", "nim\u00b7mer", "die", "Li\u00b7li\u00b7en", "bl\u00fchn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "VVFIN", "NE", "ADV", "ADV", "$,", "KOUS", "ADV", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}}}}}