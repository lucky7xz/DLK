{"textgrid.poem.59822": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Weise guter Zecher ist", "tokens": ["Die", "Wei\u00b7se", "gu\u00b7ter", "Ze\u00b7cher", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In fr\u00fch und sp\u00e4ter Stunde,", "tokens": ["In", "fr\u00fch", "und", "sp\u00e4\u00b7ter", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df alter Wein im Becher ist", "tokens": ["Da\u00df", "al\u00b7ter", "Wein", "im", "Be\u00b7cher", "ist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "APPRART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und neuer Witz im Munde \u2013", "tokens": ["Und", "neu\u00b7er", "Witz", "im", "Mun\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Denn wo man eins davon entbehrt,", "tokens": ["Denn", "wo", "man", "eins", "da\u00b7von", "ent\u00b7behrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "PIS", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da ist das andre auch nichts wert \u2013", "tokens": ["Da", "ist", "das", "and\u00b7re", "auch", "nichts", "wert", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "PIS", "ADV", "PIS", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das eine steht zum andern.", "tokens": ["Das", "ei\u00b7ne", "steht", "zum", "an\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Je mehr wir uns vertieft im Wein,", "tokens": ["Je", "mehr", "wir", "uns", "ver\u00b7tieft", "im", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Je h\u00f6her steigt der Geist uns \u2013", "tokens": ["Je", "h\u00f6\u00b7her", "steigt", "der", "Geist", "uns", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "PPER", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Bart der Weisheit trieft von Wein,", "tokens": ["Der", "Bart", "der", "Weis\u00b7heit", "trieft", "von", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die ganze Welt umkreist uns,", "tokens": ["Die", "gan\u00b7ze", "Welt", "um\u00b7kreist", "uns", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Versunken ganz in Trunkenheit", "tokens": ["Ver\u00b7sun\u00b7ken", "ganz", "in", "Trun\u00b7ken\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und trunken in Versunkenheit,", "tokens": ["Und", "trun\u00b7ken", "in", "Ver\u00b7sun\u00b7ken\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In Wein, Gesang und Liebe!", "tokens": ["In", "Wein", ",", "Ge\u00b7sang", "und", "Lie\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Weisen beim Pokale stehn", "tokens": ["Die", "Wei\u00b7sen", "beim", "Po\u00b7ka\u00b7le", "stehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hoch \u00fcber der Gemeinheit,", "tokens": ["Hoch", "\u00fc\u00b7ber", "der", "Ge\u00b7mein\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie Berge \u00fcberm Tale stehn", "tokens": ["Wie", "Ber\u00b7ge", "\u00fc\u00b7berm", "Ta\u00b7le", "stehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In himmelhoher Reinheit \u2013", "tokens": ["In", "him\u00b7mel\u00b7ho\u00b7her", "Rein\u00b7heit", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Berge f\u00e4rbt des Himmels Licht,", "tokens": ["Die", "Ber\u00b7ge", "f\u00e4rbt", "des", "Him\u00b7mels", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Uns widerstrahlt das Angesicht", "tokens": ["Uns", "wi\u00b7der\u00b7strahlt", "das", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im Glanz der vollen Becher!", "tokens": ["Im", "Glanz", "der", "vol\u00b7len", "Be\u00b7cher", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sagt, was die Welt im Tausch uns gibt", "tokens": ["Sagt", ",", "was", "die", "Welt", "im", "Tausch", "uns", "gibt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PRELS", "ART", "NN", "APPRART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr unser lustig Leben!", "tokens": ["F\u00fcr", "un\u00b7ser", "lus\u00b7tig", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Wonne, die ein Rausch uns gibt,", "tokens": ["Die", "Won\u00b7ne", ",", "die", "ein", "Rausch", "uns", "gibt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer mag uns Bess'res geben?", "tokens": ["Wer", "mag", "uns", "Bess'\u00b7res", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nur eins kenn' ich, das sch\u00f6ner ist:", "tokens": ["Nur", "eins", "kenn'", "ich", ",", "das", "sch\u00f6\u00b7ner", "ist", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "$,", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn du, Hafisa, bei mir bist", "tokens": ["Wenn", "du", ",", "Ha\u00b7fi\u00b7sa", ",", "bei", "mir", "bist"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "NE", "$,", "APPR", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit K\u00fcssen und mit Scherzen!", "tokens": ["Mit", "K\u00fcs\u00b7sen", "und", "mit", "Scher\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und weil so kurz das Leben ist,", "tokens": ["Und", "weil", "so", "kurz", "das", "Le\u00b7ben", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ADJD", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00df stets der Weisen Ziel sein:", "tokens": ["Mu\u00df", "stets", "der", "Wei\u00b7sen", "Ziel", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des Gl\u00fccks, das uns gegeben ist,", "tokens": ["Des", "Gl\u00fccks", ",", "das", "uns", "ge\u00b7ge\u00b7ben", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kann nimmermehr zuviel sein!", "tokens": ["Kann", "nim\u00b7mer\u00b7mehr", "zu\u00b7viel", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIS", "VAINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Drum Kind, la\u00df alle Skrupel sein", "tokens": ["Drum", "Kind", ",", "la\u00df", "al\u00b7le", "Skru\u00b7pel", "sein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "NN", "$,", "VVIMP", "PIAT", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und steig herab in unsre Reihn,", "tokens": ["Und", "steig", "her\u00b7ab", "in", "uns\u00b7re", "Reihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie ins Gebirg' die Sonne!", "tokens": ["Wie", "ins", "Ge\u00b7bir\u00b7g'", "die", "Son\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}