{"textgrid.poem.54474": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "63.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Printz der Ewigkeit/ der Richter aller Sachen/", "tokens": ["Der", "Printz", "der", "E\u00b7wig\u00b7keit", "/", "der", "Rich\u00b7ter", "al\u00b7ler", "Sa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "ART", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Heegt Vrtheil; rei\u00df entzwey/ Fels/ Erden/ Berg vnd Meer", "tokens": ["Heegt", "Vrtheil", ";", "rei\u00df", "ent\u00b7zwey", "/", "Fels", "/", "Er\u00b7den", "/", "Berg", "vnd", "Meer"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NN", "$.", "VVFIN", "PTKVZ", "$(", "NE", "$(", "NN", "$(", "NN", "KON", "NN"], "meter": "-+--++--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ihr Kl\u00fcffte/ Tahl vnd See/ gebt eure Todten her/", "tokens": ["Ihr", "Kl\u00fcff\u00b7te", "/", "Tahl", "vnd", "See", "/", "gebt", "eu\u00b7re", "Tod\u00b7ten", "her", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "NN", "KON", "NN", "$(", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihr Todten komt ans Licht/ komt au\u00df der Hellen Rachen.", "tokens": ["Ihr", "Tod\u00b7ten", "komt", "ans", "Licht", "/", "komt", "au\u00df", "der", "Hel\u00b7len", "Ra\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$(", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ihr Feinde Gottes komt/ komt vor ihr alten Trachen/", "tokens": ["Ihr", "Fein\u00b7de", "Got\u00b7tes", "komt", "/", "komt", "vor", "ihr", "al\u00b7ten", "Tra\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$(", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vnd h\u00f6rt den letzten Spruch/ der euch so herb vnd schwer/", "tokens": ["Vnd", "h\u00f6rt", "den", "letz\u00b7ten", "Spruch", "/", "der", "euch", "so", "herb", "vnd", "schwer", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$(", "PRELS", "PPER", "ADV", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den Frommen lieblich f\u00e4llt/ O Ewigkeit verzehr", "tokens": ["Den", "From\u00b7men", "lieb\u00b7lich", "f\u00e4llt", "/", "O", "E\u00b7wig\u00b7keit", "ver\u00b7zehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$(", "NE", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die/ die die Donner-Sti i de\u00df H\u00f6chsten an wird krachen!", "tokens": ["Die", "/", "die", "die", "Don\u00b7ner\u00b7S\u00b7ti", "i", "de\u00df", "H\u00f6chs\u00b7ten", "an", "wird", "kra\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PRELS", "ART", "NE", "NE", "ART", "NN", "APPR", "VAFIN", "VVINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Der Hauffen/ theilet sich/ der grosse Tag entdeckt", "tokens": ["Der", "Hauf\u00b7fen", "/", "thei\u00b7let", "sich", "/", "der", "gros\u00b7se", "Tag", "ent\u00b7deckt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "VVFIN", "PRF", "$(", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Was vns verborgen vor: was man so tieff versteckt.", "tokens": ["Was", "vns", "ver\u00b7bor\u00b7gen", "vor", ":", "was", "man", "so", "tieff", "ver\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "PTKVZ", "$.", "PWS", "PIS", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich seh' hier als verdamt/ die ich vor heilig sch\u00e4tzte:", "tokens": ["Ich", "seh'", "hier", "als", "ver\u00b7damt", "/", "die", "ich", "vor", "hei\u00b7lig", "sch\u00e4tz\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KOKOM", "VVPP", "$(", "PRELS", "PPER", "APPR", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Hier scheidet Freund vnd Freund auff ewig/ Weib vnd Mann:", "tokens": ["Hier", "schei\u00b7det", "Freund", "vnd", "Freund", "auff", "e\u00b7wig", "/", "Weib", "vnd", "Mann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "APPR", "ADJD", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Den st\u00f6st der Richter hin vnd jenen nimt er an.", "tokens": ["Den", "st\u00f6st", "der", "Rich\u00b7ter", "hin", "vnd", "je\u00b7nen", "nimt", "er", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKVZ", "KON", "PDS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wer vor verschmachtet/ lacht; der weint/ wer sich ergetzte.", "tokens": ["Wer", "vor", "ver\u00b7schmach\u00b7tet", "/", "lacht", ";", "der", "weint", "/", "wer", "sich", "er\u00b7getz\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "VVPP", "$(", "VVFIN", "$.", "ART", "VVFIN", "$(", "PWS", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}