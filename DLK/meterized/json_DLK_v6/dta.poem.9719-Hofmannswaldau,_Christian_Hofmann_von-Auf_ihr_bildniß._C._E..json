{"dta.poem.9719": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf ihr bildni\u00df.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weil unser auge nicht den glantz der engel leidet/", "tokens": ["Weil", "un\u00b7ser", "au\u00b7ge", "nicht", "den", "glantz", "der", "en\u00b7gel", "lei\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein engel aber gern beyn menschen wolte seyn;", "tokens": ["Ein", "en\u00b7gel", "a\u00b7ber", "gern", "beyn", "men\u00b7schen", "wol\u00b7te", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADJD", "VVINF", "VMFIN", "VAINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So hat er endlich sich und seinen wunder-schein", "tokens": ["So", "hat", "er", "end\u00b7lich", "sich", "und", "sei\u00b7nen", "wun\u00b7der\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PRF", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hier in ein sterblich bild zu unsrer lust verkleidet.", "tokens": ["Hier", "in", "ein", "sterb\u00b7lich", "bild", "zu", "uns\u00b7rer", "lust", "ver\u00b7klei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJD", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}