{"dta.poem.913": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "DiE RATTEN", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "In Hof scheint wei\u00df der herbstliche Mond.", "tokens": ["In", "Hof", "scheint", "wei\u00df", "der", "herbst\u00b7li\u00b7che", "Mond", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Vom Dachrand fallen phantastische Schatten.", "tokens": ["Vom", "Dach\u00b7rand", "fal\u00b7len", "phan\u00b7tas\u00b7ti\u00b7sche", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein Schweigen in leeren Fenstern wohnt;", "tokens": ["Ein", "Schwei\u00b7gen", "in", "lee\u00b7ren", "Fens\u00b7tern", "wohnt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da tauchen leise herauf die Ratten", "tokens": ["Da", "tau\u00b7chen", "lei\u00b7se", "her\u00b7auf", "die", "Rat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "ADV", "ART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und huschen pfeifend hier und dort", "tokens": ["Und", "hu\u00b7schen", "pfei\u00b7fend", "hier", "und", "dort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ein gr\u00e4ulicher Dunsthauch wittert", "tokens": ["Und", "ein", "gr\u00e4u\u00b7li\u00b7cher", "Dunst\u00b7hauch", "wit\u00b7tert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ihnen nach aus dem Abort,", "tokens": ["Ih\u00b7nen", "nach", "aus", "dem", "Ab\u00b7ort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Den geisterhaft der Mondschein durchzittert", "tokens": ["Den", "geis\u00b7ter\u00b7haft", "der", "Mond\u00b7schein", "durch\u00b7zit\u00b7tert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Und sie keifen vor Gier wie toll", "tokens": ["Und", "sie", "kei\u00b7fen", "vor", "Gier", "wie", "toll"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "KOKOM", "ADJD"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und erf\u00fcllen Haus und Scheunen,", "tokens": ["Und", "er\u00b7f\u00fcl\u00b7len", "Haus", "und", "Scheu\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die von Korn und Fr\u00fcchten voll.", "tokens": ["Die", "von", "Korn", "und", "Fr\u00fcch\u00b7ten", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eisige Winde im Dunkel greinen.", "tokens": ["Ei\u00b7si\u00b7ge", "Win\u00b7de", "im", "Dun\u00b7kel", "grei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}