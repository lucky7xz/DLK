{"textgrid.poem.56659": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Abend", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wieder ein Abend; ein Tag in das Nichts,", "tokens": ["Und", "wie\u00b7der", "ein", "A\u00b7bend", ";", "ein", "Tag", "in", "das", "Nichts", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$.", "ART", "NN", "APPR", "ART", "PIS", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "das grenzenlose Nichts gerollt \u2013", "tokens": ["das", "gren\u00b7zen\u00b7lo\u00b7se", "Nichts", "ge\u00b7rollt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "in den bleiernen Wolken ein Sterben des Lichts", "tokens": ["in", "den", "blei\u00b7er\u00b7nen", "Wol\u00b7ken", "ein", "Ster\u00b7ben", "des", "Lichts"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "ART", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "und \u00fcber den W\u00e4ldern der Mond, gelb wie altfr\u00e4nkisches Gold.", "tokens": ["und", "\u00fc\u00b7ber", "den", "W\u00e4l\u00b7dern", "der", "Mond", ",", "gelb", "wie", "alt\u00b7fr\u00e4n\u00b7ki\u00b7sches", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$,", "ADJD", "KOKOM", "ADJA", "NN", "$."], "meter": "-+--+--++-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Nun dunkelt es schnell; ein Wind macht sich auf", "tokens": ["Nun", "dun\u00b7kelt", "es", "schnell", ";", "ein", "Wind", "macht", "sich", "auf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$.", "ART", "NN", "VVFIN", "PRF", "APPR"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und rasselt im Schieferged\u00e4che am Turm \u2013", "tokens": ["und", "ras\u00b7selt", "im", "Schie\u00b7fer\u00b7ge\u00b7d\u00e4\u00b7che", "am", "Turm", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPRART", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "kopf\u00fcber purzelt der Tage Lauf", "tokens": ["kopf\u00b7\u00fc\u00b7ber", "pur\u00b7zelt", "der", "Ta\u00b7ge", "Lauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "vor der Ewigkeit drohendem Sturm.", "tokens": ["vor", "der", "E\u00b7wig\u00b7keit", "dro\u00b7hen\u00b7dem", "Sturm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}}}}