{"textgrid.poem.64361": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "83. Lied f\u00fcr Agnes, ihren Kleinen in Schlaf zu singen", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlafe, s\u00fc\u00dfer Knabe,", "tokens": ["Schla\u00b7fe", ",", "s\u00fc\u00b7\u00dfer", "Kna\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Mir am Busen ein.", "tokens": ["Mir", "am", "Bu\u00b7sen", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wohl mir, da\u00df ich habe", "tokens": ["Wohl", "mir", ",", "da\u00df", "ich", "ha\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "KOUS", "PPER", "VAFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Dich, mein B\u00fcbelein!", "tokens": ["Dich", ",", "mein", "B\u00fc\u00b7be\u00b7lein", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Unter diesem Herzen,", "tokens": ["Un\u00b7ter", "die\u00b7sem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "B\u00fcbchen, trug ich dich;", "tokens": ["B\u00fcb\u00b7chen", ",", "trug", "ich", "dich", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Hier an diesem Herzen,", "tokens": ["Hier", "an", "die\u00b7sem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "B\u00fcbchen, s\u00e4ugt' ich dich.", "tokens": ["B\u00fcb\u00b7chen", ",", "s\u00e4ugt'", "ich", "dich", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "K\u00fchle Weste dringen", "tokens": ["K\u00fch\u00b7le", "Wes\u00b7te", "drin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "In den Busch hinein,", "tokens": ["In", "den", "Busch", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Kleine V\u00f6gel singen", "tokens": ["Klei\u00b7ne", "V\u00f6\u00b7gel", "sin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ihre Jungen ein.", "tokens": ["Ih\u00b7re", "Jun\u00b7gen", "ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "B\u00fcbchen, es erschallet", "tokens": ["B\u00fcb\u00b7chen", ",", "es", "er\u00b7schal\u00b7let"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Mein Gesang f\u00fcr dich;", "tokens": ["Mein", "Ge\u00b7sang", "f\u00fcr", "dich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "B\u00fcbchen, es umwallet", "tokens": ["B\u00fcb\u00b7chen", ",", "es", "um\u00b7wal\u00b7let"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Meine Locke dich.", "tokens": ["Mei\u00b7ne", "Lo\u00b7cke", "dich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "In dem warmen Neste", "tokens": ["In", "dem", "war\u00b7men", "Nes\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Liegt das V\u00f6glein weich,", "tokens": ["Liegt", "das", "V\u00f6\u00b7glein", "weich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "In dem Schirm der \u00c4ste,", "tokens": ["In", "dem", "Schirm", "der", "\u00c4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Unterm Bl\u00fctenzweig.", "tokens": ["Un\u00b7term", "Bl\u00fc\u00b7ten\u00b7zweig", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Sanfter Schlummer labe", "tokens": ["Sanf\u00b7ter", "Schlum\u00b7mer", "la\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Dich in meinem Arm;", "tokens": ["Dich", "in", "mei\u00b7nem", "Arm", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Ruhe, s\u00fc\u00dfer Knabe,", "tokens": ["Ru\u00b7he", ",", "s\u00fc\u00b7\u00dfer", "Kna\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Ruhe weich und warm.", "tokens": ["Ru\u00b7he", "weich", "und", "warm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}