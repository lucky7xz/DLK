{"textgrid.poem.44902": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[in Politik zwei wichtge kleine Dinger]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Politik zwei wichtge kleine Dinger", "tokens": ["In", "Po\u00b7li\u00b7tik", "zwei", "wicht\u00b7ge", "klei\u00b7ne", "Din\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "CARD", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind Daumen eben und Zeigefinger,", "tokens": ["Sind", "Dau\u00b7men", "e\u00b7ben", "und", "Zei\u00b7ge\u00b7fin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "KON", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie halten die Feder,", "tokens": ["Sie", "hal\u00b7ten", "die", "Fe\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Das wei\u00df ein jeder.", "tokens": ["Das", "wei\u00df", "ein", "je\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "PIS", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Doch Wichtgres noch wird oft durch sie betrieben,", "tokens": ["Doch", "Wicht\u00b7gres", "noch", "wird", "oft", "durch", "sie", "be\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn sie sich \u00fcbereinander schieben.", "tokens": ["Wenn", "sie", "sich", "\u00fc\u00b7be\u00b7re\u00b7in\u00b7an\u00b7der", "schie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}