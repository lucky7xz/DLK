{"dta.poem.8798": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XiX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Doch auch ein selbstisches Gen\u00fcgen, Freund!", "tokens": ["Doch", "auch", "ein", "selbs\u00b7ti\u00b7sches", "Ge\u00b7n\u00fc\u00b7gen", ",", "Freund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie kleiden sich in sichre Harmonie,", "tokens": ["Sie", "klei\u00b7den", "sich", "in", "sich\u00b7re", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Uns um so sichrer zu betr\u00fcgen, Freund!", "tokens": ["Uns", "um", "so", "sich\u00b7rer", "zu", "be\u00b7tr\u00fc\u00b7gen", ",", "Freund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "APPR", "ADV", "ADJD", "PTKZU", "VVINF", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch suchen mehr wir, als die glatte Stirn,", "tokens": ["Doch", "su\u00b7chen", "mehr", "wir", ",", "als", "die", "glat\u00b7te", "Stirn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die keine Runzel wagt zu pfl\u00fcgen, Freund!", "tokens": ["Die", "kei\u00b7ne", "Run\u00b7zel", "wagt", "zu", "pfl\u00fc\u00b7gen", ",", "Freund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "PTKZU", "VVINF", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Was in den Adern uns lebendig rollt,", "tokens": ["Was", "in", "den", "A\u00b7dern", "uns", "le\u00b7ben\u00b7dig", "rollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es sey kein Leben, das wir l\u00fcgen, Freund!", "tokens": ["Es", "sey", "kein", "Le\u00b7ben", ",", "das", "wir", "l\u00fc\u00b7gen", ",", "Freund", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Kein F\u00e4cher sey der sch\u00f6ne Fittig dir,", "tokens": ["Kein", "F\u00e4\u00b7cher", "sey", "der", "sch\u00f6\u00b7ne", "Fit\u00b7tig", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ART", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Er trage dich zu hohen Fl\u00fcgen, Freund!", "tokens": ["Er", "tra\u00b7ge", "dich", "zu", "ho\u00b7hen", "Fl\u00fc\u00b7gen", ",", "Freund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}