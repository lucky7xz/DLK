{"textgrid.poem.46107": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Ueber die heimf\u00fchrung frauen Barbara, marggr\u00e4fin zu Baden, geborner herzogin zu Wirtemberg", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als marggraf Friderich", "tokens": ["Als", "marg\u00b7graf", "Fri\u00b7de\u00b7rich"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NE", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "kont billich triumfieren,", "tokens": ["kont", "bil\u00b7lich", "tri\u00b7um\u00b7fie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "weil man ihm pr\u00e4chtiglich", "tokens": ["weil", "man", "ihm", "pr\u00e4ch\u00b7tig\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "ADJD"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "wolt sein gemahl heimf\u00fchren,", "tokens": ["wolt", "sein", "ge\u00b7mahl", "heim\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da offenbarten bald mit klang", "tokens": ["Da", "of\u00b7fen\u00b7bar\u00b7ten", "bald", "mit", "klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "die Nymfelein ihr wolgefallen", "tokens": ["die", "Nym\u00b7fel\u00b7ein", "ihr", "wol\u00b7ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "und machten durch ihr lobgesang", "tokens": ["und", "mach\u00b7ten", "durch", "ihr", "lob\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "berg, thal, feld und wald widerhallen.", "tokens": ["berg", ",", "thal", ",", "feld", "und", "wald", "wi\u00b7der\u00b7hal\u00b7len", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}