{"dta.poem.19209": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "56.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df dir in der Natur am Was, Wozu und Wie", "tokens": ["La\u00df", "dir", "in", "der", "Na\u00b7tur", "am", "Was", ",", "Wo\u00b7zu", "und", "Wie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "APPRART", "NN", "$,", "PWAV", "KON", "PWAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gen\u00fcgen! das Warum begreifest du doch nie.", "tokens": ["Ge\u00b7n\u00fc\u00b7gen", "!", "das", "Wa\u00b7rum", "be\u00b7grei\u00b7fest", "du", "doch", "nie", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "PWAV", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was wirkt, und wie es wirkt, wozu du brauchen kannst", "tokens": ["Was", "wirkt", ",", "und", "wie", "es", "wirkt", ",", "wo\u00b7zu", "du", "brau\u00b7chen", "kannst"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "KON", "PWAV", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Wirkung, ohne da\u00df du ihren Grund erkannst.", "tokens": ["Die", "Wir\u00b7kung", ",", "oh\u00b7ne", "da\u00df", "du", "ih\u00b7ren", "Grund", "er\u00b7kannst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "F\u00fchrt sicher \u00fcbers Meer zum Ziel doch der Magnet", "tokens": ["F\u00fchrt", "si\u00b7cher", "\u00fc\u00b7bers", "Meer", "zum", "Ziel", "doch", "der", "Mag\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "APPRART", "NN", "ADV", "ART", "NN"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Den, der nicht fragt warum, nur sieht wie er sich dreht.", "tokens": ["Den", ",", "der", "nicht", "fragt", "wa\u00b7rum", ",", "nur", "sieht", "wie", "er", "sich", "dreht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PTKNEG", "VVFIN", "PWAV", "$,", "ADV", "VVFIN", "KOKOM", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}