{"textgrid.poem.42587": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "11. Den Landst\u00e4nden zum Christophstag", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wieder schwankt die ernste Waage,", "tokens": ["Und", "wie\u00b7der", "schwankt", "die", "erns\u00b7te", "Waa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der alte Kampf belebt sich neu;", "tokens": ["Der", "al\u00b7te", "Kampf", "be\u00b7lebt", "sich", "neu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jetzt kommen erst die rechten Tage,", "tokens": ["Jetzt", "kom\u00b7men", "erst", "die", "rech\u00b7ten", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo Korn sich sondern wird von Spreu,", "tokens": ["Wo", "Korn", "sich", "son\u00b7dern", "wird", "von", "Spreu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "ADV", "VAFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo man den Falschen von dem Treuen", "tokens": ["Wo", "man", "den", "Fal\u00b7schen", "von", "dem", "Treu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Geh\u00f6rig unterscheiden kann,", "tokens": ["Ge\u00b7h\u00f6\u00b7rig", "un\u00b7ter\u00b7schei\u00b7den", "kann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den Unerschrocknen von dem Scheuen,", "tokens": ["Den", "Un\u00b7er\u00b7schrock\u00b7nen", "von", "dem", "Scheu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den halben von dem ganzen Mann.", "tokens": ["Den", "hal\u00b7ben", "von", "dem", "gan\u00b7zen", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Den wird man f\u00fcr ", "tokens": ["Den", "wird", "man", "f\u00fcr"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PIS", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der von dem Recht erleuchtet ist,", "tokens": ["Der", "von", "dem", "Recht", "er\u00b7leuch\u00b7tet", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den wird man einen ", "tokens": ["Den", "wird", "man", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PIS", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Der nie sein Ritterwort vergi\u00dft,", "tokens": ["Der", "nie", "sein", "Rit\u00b7ter\u00b7wort", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "In dem sich regt der freie Geist,", "tokens": ["In", "dem", "sich", "regt", "der", "frei\u00b7e", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der wird als ", "tokens": ["Der", "wird", "als"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VAFIN", "KOKOM"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Der seine Burg zu schirmen wei\u00dft.", "tokens": ["Der", "sei\u00b7ne", "Burg", "zu", "schir\u00b7men", "wei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Jetzt wahret, ", "tokens": ["Jetzt", "wah\u00b7ret", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Steht auf zu m\u00e4nnlichem Entscheid!", "tokens": ["Steht", "auf", "zu", "m\u00e4nn\u00b7li\u00b7chem", "Ent\u00b7scheid", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Damit ihr nicht dem Land zur B\u00fcrde,", "tokens": ["Da\u00b7mit", "ihr", "nicht", "dem", "Land", "zur", "B\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Ausland zum Gel\u00e4chter seid.", "tokens": ["Dem", "Aus\u00b7land", "zum", "Ge\u00b7l\u00e4ch\u00b7ter", "seid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es ist so viel schon unterhandelt,", "tokens": ["Es", "ist", "so", "viel", "schon", "un\u00b7ter\u00b7han\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es ist gesprochen fort und fort,", "tokens": ["Es", "ist", "ge\u00b7spro\u00b7chen", "fort", "und", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es ist geschrieben und gesandelt \u2013", "tokens": ["Es", "ist", "ge\u00b7schrie\u00b7ben", "und", "ge\u00b7san\u00b7delt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So sprecht nun euer ", "tokens": ["So", "sprecht", "nun", "eu\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Und kann es nicht sein Ziel erstreben,", "tokens": ["Und", "kann", "es", "nicht", "sein", "Ziel", "er\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So tretet in das Volk zur\u00fcck!", "tokens": ["So", "tre\u00b7tet", "in", "das", "Volk", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ihr vom Rechte nichts vergeben,", "tokens": ["Da\u00df", "ihr", "vom", "Rech\u00b7te", "nichts", "ver\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sei euch ein lohnend stolzes Gl\u00fcck!", "tokens": ["Sei", "euch", "ein", "loh\u00b7nend", "stol\u00b7zes", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erharret ruhig und bedenket:", "tokens": ["Er\u00b7har\u00b7ret", "ru\u00b7hig", "und", "be\u00b7den\u00b7ket", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Freiheit Morgen steigt herauf,", "tokens": ["Der", "Frei\u00b7heit", "Mor\u00b7gen", "steigt", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Gott ist's, der die Sonne lenket,", "tokens": ["Ein", "Gott", "ist's", ",", "der", "die", "Son\u00b7ne", "len\u00b7ket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und unaufhaltsam ist ihr Lauf!", "tokens": ["Und", "un\u00b7auf\u00b7halt\u00b7sam", "ist", "ihr", "Lauf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}