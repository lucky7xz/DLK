{"textgrid.poem.58163": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Frag nicht, warum du unbest\u00e4ndig!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frag nicht, warum du unbest\u00e4ndig!", "tokens": ["Frag", "nicht", ",", "wa\u00b7rum", "du", "un\u00b7be\u00b7st\u00e4n\u00b7dig", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PWAV", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du bleibst nicht stets bei Gott inwendig.", "tokens": ["Du", "bleibst", "nicht", "stets", "bei", "Gott", "in\u00b7wen\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Frag nicht, warum ein Kindlein f\u00e4llt,", "tokens": ["Frag", "nicht", ",", "wa\u00b7rum", "ein", "Kin\u00b7dlein", "f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn es sich nicht zur Mutter h\u00e4lt!", "tokens": ["Wenn", "es", "sich", "nicht", "zur", "Mut\u00b7ter", "h\u00e4lt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}