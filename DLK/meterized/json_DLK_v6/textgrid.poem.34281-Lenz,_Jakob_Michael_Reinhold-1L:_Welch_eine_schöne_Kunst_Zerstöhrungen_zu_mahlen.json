{"textgrid.poem.34281": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Welch eine sch\u00f6ne Kunst Zerst\u00f6hrungen zu mahlen", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch eine sch\u00f6ne Kunst Zerst\u00f6hrungen zu mahlen", "tokens": ["Welch", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Kunst", "Zer\u00b7st\u00f6h\u00b7run\u00b7gen", "zu", "mah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "ADJA", "NN", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und das wie Herostrat von Idealen?", "tokens": ["Und", "das", "wie", "He\u00b7rost\u00b7rat", "von", "I\u00b7dea\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "KOKOM", "NN", "APPR", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Welch ein Triumph, dem Todten, welcher ringt", "tokens": ["Welch", "ein", "Tri\u00b7umph", ",", "dem", "Tod\u00b7ten", ",", "wel\u00b7cher", "ringt"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "VVFIN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Ein edler Mensch zu seyn, zu weisen \u2013 wie er stinkt", "tokens": ["Ein", "ed\u00b7ler", "Mensch", "zu", "seyn", ",", "zu", "wei\u00b7sen", "\u2013", "wie", "er", "stinkt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VAINF", "$,", "PTKZU", "VVINF", "$(", "PWAV", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie er vom Berg an den er Steine welzet", "tokens": ["Wie", "er", "vom", "Berg", "an", "den", "er", "Stei\u00b7ne", "wel\u00b7zet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN", "APPR", "PRELS", "PPER", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Herab Kopf\u00fcber purzelt und zu Staub zerschmelzet.", "tokens": ["Her\u00b7ab", "Kopf\u00b7\u00fc\u00b7ber", "pur\u00b7zelt", "und", "zu", "Staub", "zer\u00b7schmel\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "KON", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein heldenhaft Bem\u00fchn! o Lorbeerwerth", "tokens": ["Ein", "hel\u00b7den\u00b7haft", "Be\u00b7m\u00fchn", "!", "o", "Lor\u00b7beer\u00b7werth"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVINF", "$.", "FM", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So lang ein Armer noch die Zahl vermehrt", "tokens": ["So", "lang", "ein", "Ar\u00b7mer", "noch", "die", "Zahl", "ver\u00b7mehrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "ADV", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Zur Ewigkeit der Ewigkeit hin\u00fcber.", "tokens": ["Zur", "E\u00b7wig\u00b7keit", "der", "E\u00b7wig\u00b7keit", "hin\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wer s\u00e4he nicht die ", "tokens": ["Wer", "s\u00e4\u00b7he", "nicht", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Als tausend sch\u00f6ne L\u00fcgen, die der Geist", "tokens": ["Als", "tau\u00b7send", "sch\u00f6\u00b7ne", "L\u00fc\u00b7gen", ",", "die", "der", "Geist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "CARD", "ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Allein geheckt und auch allein geneu\u00dft?", "tokens": ["Al\u00b7lein", "ge\u00b7heckt", "und", "auch", "al\u00b7lein", "ge\u00b7neu\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KON", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Die ihm so oft die Wirklichkeit geraubet,", "tokens": ["Die", "ihm", "so", "oft", "die", "Wirk\u00b7lich\u00b7keit", "ge\u00b7rau\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Die nur erfreun, so lang er an sie glaubet,", "tokens": ["Die", "nur", "er\u00b7freun", ",", "so", "lang", "er", "an", "sie", "glau\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "$,", "ADV", "ADJD", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Von denen er, wenn er befangen wird,", "tokens": ["Von", "de\u00b7nen", "er", ",", "wenn", "er", "be\u00b7fan\u00b7gen", "wird", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "So oft hinab zur Sinnlichkeit verirrt.", "tokens": ["So", "oft", "hin\u00b7ab", "zur", "Sinn\u00b7lich\u00b7keit", "ver\u00b7irrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Kurzum die nichts sind. Wollt ihr euch bekehren", "tokens": ["Kur\u00b7zum", "die", "nichts", "sind", ".", "Wollt", "ihr", "euch", "be\u00b7keh\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "PIS", "VAFIN", "$.", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Kommt nur zu mir, ich will euch Wahrheit lehren,", "tokens": ["Kommt", "nur", "zu", "mir", ",", "ich", "will", "euch", "Wahr\u00b7heit", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "PPER", "$,", "PPER", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Im sinnlichen Genu\u00df ganz aufgel\u00f6st \u2013", "tokens": ["Im", "sinn\u00b7li\u00b7chen", "Ge\u00b7nu\u00df", "ganz", "auf\u00b7ge\u00b7l\u00f6st", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Da f\u00fchlt ihr sie, da seht ihr sie entbl\u00f6st;", "tokens": ["Da", "f\u00fchlt", "ihr", "sie", ",", "da", "seht", "ihr", "sie", "ent\u00b7bl\u00f6st", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "ADV", "VVFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Das andre all ist nichts, ist Dunst, sind Tr\u00e4ume", "tokens": ["Das", "and\u00b7re", "all", "ist", "nichts", ",", "ist", "Dunst", ",", "sind", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "PIAT", "VAFIN", "PIS", "$,", "VAFIN", "NN", "$,", "VAFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Und steht nur dazu da, da\u00df es sich reime.", "tokens": ["Und", "steht", "nur", "da\u00b7zu", "da", ",", "da\u00df", "es", "sich", "rei\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PAV", "PTKVZ", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "O Wahnwitz der die Dichterwelt regiert", "tokens": ["O", "Wahn\u00b7witz", "der", "die", "Dich\u00b7ter\u00b7welt", "re\u00b7giert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "ART", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Und manches sch\u00f6ne Kind durch sie verf\u00fchrt!", "tokens": ["Und", "man\u00b7ches", "sch\u00f6\u00b7ne", "Kind", "durch", "sie", "ver\u00b7f\u00fchrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "O Raserey! die ihr Gehirn verstimmet,", "tokens": ["O", "Ra\u00b7se\u00b7rey", "!", "die", "ihr", "Ge\u00b7hirn", "ver\u00b7stim\u00b7met", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Da\u00df oft ihr blitzend Aug in s\u00fcssen Tr\u00e4umen schwimmet,", "tokens": ["Da\u00df", "oft", "ihr", "blit\u00b7zend", "Aug", "in", "s\u00fcs\u00b7sen", "Tr\u00e4u\u00b7men", "schwim\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJD", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Da\u00df halbge\u00f6fnet ihr entz\u00fcckter Mund", "tokens": ["Da\u00df", "halb\u00b7ge\u00b7\u00f6f\u00b7net", "ihr", "ent\u00b7z\u00fcck\u00b7ter", "Mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Dem unsichtbaren Liebsten laut ihr Herz gestund,", "tokens": ["Dem", "un\u00b7sicht\u00b7ba\u00b7ren", "Liebs\u00b7ten", "laut", "ihr", "Herz", "ge\u00b7stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Da\u00df ihre Wangen gl\u00fchn, die sch\u00f6nen Wangen", "tokens": ["Da\u00df", "ih\u00b7re", "Wan\u00b7gen", "gl\u00fchn", ",", "die", "sch\u00f6\u00b7nen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVINF", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "An denen Amors halb verzweifelt hangen", "tokens": ["An", "de\u00b7nen", "A\u00b7mors", "halb", "ver\u00b7zwei\u00b7felt", "han\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "NE", "ADJD", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Und ihre H\u00e4nde ringen, da\u00df sie nun", "tokens": ["Und", "ih\u00b7re", "H\u00e4n\u00b7de", "rin\u00b7gen", ",", "da\u00df", "sie", "nun"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "In Tr\u00e4nen baden, nicht mehr n\u00e4rrisch thun.", "tokens": ["In", "Tr\u00e4\u00b7nen", "ba\u00b7den", ",", "nicht", "mehr", "n\u00e4r\u00b7risch", "thun", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,", "PTKNEG", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "O Sch\u00f6nen h\u00f6rt mir zu, la\u00dft euch entf\u00e4rben,", "tokens": ["O", "Sch\u00f6\u00b7nen", "h\u00f6rt", "mir", "zu", ",", "la\u00dft", "euch", "ent\u00b7f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.34": {"text": "Ihr seyd betrogen, Liebe ist Verderben.", "tokens": ["Ihr", "seyd", "be\u00b7tro\u00b7gen", ",", "Lie\u00b7be", "ist", "Ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Sie t\u00f6dtet eure Freuden, sie ist Dunst.", "tokens": ["Sie", "t\u00f6d\u00b7tet", "eu\u00b7re", "Freu\u00b7den", ",", "sie", "ist", "Dunst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Willfahret jedem, g\u00f6nnet eure Gunst", "tokens": ["Will\u00b7fah\u00b7ret", "je\u00b7dem", ",", "g\u00f6n\u00b7net", "eu\u00b7re", "Gunst"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Dem ersten besten Haushahn auf zwey Beinen \u2013", "tokens": ["Dem", "ers\u00b7ten", "bes\u00b7ten", "Haus\u00b7hahn", "auf", "zwey", "Bei\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "APPR", "CARD", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "So seyd ihr gl\u00fccklich, braucht nicht mehr zu weinen;", "tokens": ["So", "seyd", "ihr", "gl\u00fcck\u00b7lich", ",", "braucht", "nicht", "mehr", "zu", "wei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "VVFIN", "PTKNEG", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "F\u00fchlt immer Herzweh, werdt des Lebens froh,", "tokens": ["F\u00fchlt", "im\u00b7mer", "Herz\u00b7weh", ",", "werdt", "des", "Le\u00b7bens", "froh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "$,", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Brennt unaufh\u00f6rlich wie ein B\u00fcndel Stroh,", "tokens": ["Brennt", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "wie", "ein", "B\u00fcn\u00b7del", "Stroh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Bis ihr zerflattert, bis ", "tokens": ["Bis", "ihr", "zer\u00b7flat\u00b7tert", ",", "bis"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "PPER", "VVPP", "$,", "KOUS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.42": {"text": "Ihr sanft entschlummert: dann verwest im Friede.", "tokens": ["Ihr", "sanft", "ent\u00b7schlum\u00b7mert", ":", "dann", "ver\u00b7west", "im", "Frie\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$.", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}