{"textgrid.poem.60767": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "62.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sehnt sich ewig dieser Geist ins Weite,", "tokens": ["Es", "sehnt", "sich", "e\u00b7wig", "die\u00b7ser", "Geist", "ins", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "PDAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und m\u00f6chte f\u00fcrder, immer f\u00fcrder streben:", "tokens": ["Und", "m\u00f6ch\u00b7te", "f\u00fcr\u00b7der", ",", "im\u00b7mer", "f\u00fcr\u00b7der", "stre\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJD", "$,", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nie k\u00f6nnt ich lang an einer Scholle kleben,", "tokens": ["Nie", "k\u00f6nnt", "ich", "lang", "an", "ei\u00b7ner", "Schol\u00b7le", "kle\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und h\u00e4tt ein Eden ich an jeder Seite.", "tokens": ["Und", "h\u00e4tt", "ein", "E\u00b7den", "ich", "an", "je\u00b7der", "Sei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein Geist, bewegt von innerlichem Streite,", "tokens": ["Mein", "Geist", ",", "be\u00b7wegt", "von", "in\u00b7ner\u00b7li\u00b7chem", "Strei\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Empfand so sehr in diesem kurzen Leben,", "tokens": ["Emp\u00b7fand", "so", "sehr", "in", "die\u00b7sem", "kur\u00b7zen", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie leicht es ist, die Heimat aufzugeben,", "tokens": ["Wie", "leicht", "es", "ist", ",", "die", "Hei\u00b7mat", "auf\u00b7zu\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VAFIN", "$,", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Allein wie schwer, zu finden eine zweite.", "tokens": ["Al\u00b7lein", "wie", "schwer", ",", "zu", "fin\u00b7den", "ei\u00b7ne", "zwei\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADJD", "$,", "PTKZU", "VVINF", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch wer aus voller Seele ha\u00dft das Schlechte,", "tokens": ["Doch", "wer", "aus", "vol\u00b7ler", "See\u00b7le", "ha\u00dft", "das", "Schlech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auch aus der Heimat wird es ihn verjagen,", "tokens": ["Auch", "aus", "der", "Hei\u00b7mat", "wird", "es", "ihn", "ver\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn dort verehrt es wird vom Volk der Knechte.", "tokens": ["Wenn", "dort", "ver\u00b7ehrt", "es", "wird", "vom", "Volk", "der", "Knech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "PPER", "VAFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Weit kl\u00fcger ist's, dem Vaterland entsagen,", "tokens": ["Weit", "kl\u00fc\u00b7ger", "ist's", ",", "dem", "Va\u00b7ter\u00b7land", "ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VAFIN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als unter einem kindischen Geschlechte", "tokens": ["Als", "un\u00b7ter", "ei\u00b7nem", "kin\u00b7di\u00b7schen", "Ge\u00b7schlech\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Joch des blinden P\u00f6belhasses tragen.", "tokens": ["Das", "Joch", "des", "blin\u00b7den", "P\u00f6\u00b7bel\u00b7has\u00b7ses", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}