{"dta.poem.522": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Vier M\u00e4dgens lieben ihn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Bellis' und Clelia, wie auch Bellandrens Schein/", "tokens": ["Bel\u00b7lis'", "und", "Cle\u00b7lia", ",", "wie", "auch", "Bel\u00b7land\u00b7rens", "Schein", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$,", "PWAV", "ADV", "NE", "NN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Die m\u00fchen sich allzeit/ zu Dienste mir zu seyn/", "tokens": ["Die", "m\u00fc\u00b7hen", "sich", "all\u00b7zeit", "/", "zu", "Diens\u00b7te", "mir", "zu", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PRF", "ADV", "$(", "APPR", "NN", "PPER", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich nehm es auch verlieb/ wenn ", "tokens": ["Ich", "nehm", "es", "auch", "ver\u00b7lieb", "/", "wenn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVFIN", "$(", "KOUS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht nur Vergn\u00fcgung kriegt/ sonst stehen sie dahin.", "tokens": ["Nicht", "nur", "Ver\u00b7gn\u00fc\u00b7gung", "kriegt", "/", "sonst", "ste\u00b7hen", "sie", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "NN", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "PAV", "$."], "meter": "-+-+-+-+-+-+"