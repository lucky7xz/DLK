{"textgrid.poem.56324": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Ungewitter", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf hohen Burgeszinnen", "tokens": ["Auf", "ho\u00b7hen", "Bur\u00b7ges\u00b7zin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der alte K\u00f6nig stand,", "tokens": ["Der", "al\u00b7te", "K\u00f6\u00b7nig", "stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und \u00fcberschaute d\u00fcster", "tokens": ["Und", "\u00fc\u00b7bersc\u00b7hau\u00b7te", "d\u00fcs\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das d\u00fcster umw\u00f6lkte Land.", "tokens": ["Das", "d\u00fcs\u00b7ter", "um\u00b7w\u00f6lk\u00b7te", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Es zog das Ungewitter", "tokens": ["Es", "zog", "das", "Un\u00b7ge\u00b7wit\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Sturmesgewalt herauf,", "tokens": ["Mit", "Stur\u00b7mes\u00b7ge\u00b7walt", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er st\u00fctzte seine Rechte", "tokens": ["Er", "st\u00fctz\u00b7te", "sei\u00b7ne", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf seines Schwertes Knauf.", "tokens": ["Auf", "sei\u00b7nes", "Schwer\u00b7tes", "Knauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Linke, der entsunken", "tokens": ["Die", "Lin\u00b7ke", ",", "der", "ent\u00b7sun\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das goldene Zepter schon,", "tokens": ["Das", "gol\u00b7de\u00b7ne", "Zep\u00b7ter", "schon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hielt noch auf der finstern Stirne", "tokens": ["Hielt", "noch", "auf", "der", "fins\u00b7tern", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die schwere goldene Kron.", "tokens": ["Die", "schwe\u00b7re", "gol\u00b7de\u00b7ne", "Kron", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Da zog ihn seine Buhle", "tokens": ["Da", "zog", "ihn", "sei\u00b7ne", "Buh\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Leis an des Mantels Saum:", "tokens": ["Leis", "an", "des", "Man\u00b7tels", "Saum", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbdu hast mich einst geliebet,", "tokens": ["\u00bb", "du", "hast", "mich", "einst", "ge\u00b7lie\u00b7bet", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du liebst mich wohl noch kaum?\u00ab", "tokens": ["Du", "liebst", "mich", "wohl", "noch", "kaum", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbwas Lieb und Lust und Minne?", "tokens": ["\u00bb", "was", "Lieb", "und", "Lust", "und", "Min\u00b7ne", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "La\u00df ab, du s\u00fc\u00dfe Gestalt!", "tokens": ["La\u00df", "ab", ",", "du", "s\u00fc\u00b7\u00dfe", "Ge\u00b7stalt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Das Ungewitter ziehet", "tokens": ["Das", "Un\u00b7ge\u00b7wit\u00b7ter", "zie\u00b7het"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Herauf mit Sturmesgewalt.", "tokens": ["Her\u00b7auf", "mit", "Stur\u00b7mes\u00b7ge\u00b7walt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Ich bin auf Burgeszinnen", "tokens": ["Ich", "bin", "auf", "Bur\u00b7ges\u00b7zin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht K\u00f6nig mit Schwert und Kron,", "tokens": ["Nicht", "K\u00f6\u00b7nig", "mit", "Schwert", "und", "Kron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich bin der emp\u00f6rten Zeiten", "tokens": ["Ich", "bin", "der", "em\u00b7p\u00f6r\u00b7ten", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Unm\u00e4chtiger, bangender Sohn.", "tokens": ["Un\u00b7m\u00e4ch\u00b7ti\u00b7ger", ",", "ban\u00b7gen\u00b7der", "Sohn", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Was Lieb und Lust und Minne?", "tokens": ["Was", "Lieb", "und", "Lust", "und", "Min\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "La\u00df ab, du s\u00fc\u00dfe Gestalt!", "tokens": ["La\u00df", "ab", ",", "du", "s\u00fc\u00b7\u00dfe", "Ge\u00b7stalt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Das Ungewitter ziehet", "tokens": ["Das", "Un\u00b7ge\u00b7wit\u00b7ter", "zie\u00b7het"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Herauf mit Sturmesgewalt.\u00ab", "tokens": ["Her\u00b7auf", "mit", "Stur\u00b7mes\u00b7ge\u00b7walt", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "NN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}