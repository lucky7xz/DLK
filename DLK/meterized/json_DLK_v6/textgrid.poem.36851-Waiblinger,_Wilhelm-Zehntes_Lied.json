{"textgrid.poem.36851": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Zehntes Lied", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Endlich wundert ihr euch, ihr begreifet nicht, wie der S\u00e4nger", "tokens": ["End\u00b7lich", "wun\u00b7dert", "ihr", "euch", ",", "ihr", "be\u00b7grei\u00b7fet", "nicht", ",", "wie", "der", "S\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "So Verha\u00dftes, wie er euch im Gedichte bedenkt.", "tokens": ["So", "Ver\u00b7ha\u00df\u00b7tes", ",", "wie", "er", "euch", "im", "Ge\u00b7dich\u00b7te", "be\u00b7denkt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PWAV", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Denn unw\u00fcrdig, ihr f\u00fchlet es selbst, unw\u00fcrdig der Muse", "tokens": ["Denn", "un\u00b7w\u00fcr\u00b7dig", ",", "ihr", "f\u00fch\u00b7let", "es", "selbst", ",", "un\u00b7w\u00fcr\u00b7dig", "der", "Mu\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,", "ADJD", "ART", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Seid ihr ja ganz und verdient selber die gei\u00dfelnde nicht.", "tokens": ["Seid", "ihr", "ja", "ganz", "und", "ver\u00b7dient", "sel\u00b7ber", "die", "gei\u00b7\u00df\u00b7eln\u00b7de", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ADV", "ADV", "KON", "VVFIN", "ADV", "ART", "ADJA", "PTKNEG", "$."], "meter": "-+-+--+---+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Aber weil ihr von Tugend mir prahlt, von Bibel und Sitte,", "tokens": ["A\u00b7ber", "weil", "ihr", "von", "Tu\u00b7gend", "mir", "prahlt", ",", "von", "Bi\u00b7bel", "und", "Sit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "NN", "PPER", "VVFIN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Weil euch Lust und Genu\u00df stets nur ein Aergerni\u00df ist,", "tokens": ["Weil", "euch", "Lust", "und", "Ge\u00b7nu\u00df", "stets", "nur", "ein", "A\u00b7er\u00b7ger\u00b7ni\u00df", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "ADV", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Weil ihr mich t\u00e4glich verdammt und dem gl\u00fccklichen Sp\u00f6tter den Bannstrahl,", "tokens": ["Weil", "ihr", "mich", "t\u00e4g\u00b7lich", "ver\u00b7dammt", "und", "dem", "gl\u00fcck\u00b7li\u00b7chen", "Sp\u00f6t\u00b7ter", "den", "Bann\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVPP", "KON", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Den zerst\u00f6renden, mir t\u00e4glich nach S\u00fcden verschickt:", "tokens": ["Den", "zer\u00b7st\u00f6\u00b7ren\u00b7den", ",", "mir", "t\u00e4g\u00b7lich", "nach", "S\u00fc\u00b7den", "ver\u00b7schickt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PPER", "ADJD", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "So erfreut mir's das Herz, euch t\u00e4glich zu \u00e4rgern und euch nur", "tokens": ["So", "er\u00b7freut", "mir's", "das", "Herz", ",", "euch", "t\u00e4g\u00b7lich", "zu", "\u00e4r\u00b7gern", "und", "euch", "nur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "ART", "NN", "$,", "PPER", "ADJD", "PTKZU", "VVINF", "KON", "PPER", "ADV"], "meter": "--+--+-+--+-+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Will ich erz\u00e4hlen wie mir Freuden an Freuden erbl\u00fchn.", "tokens": ["Will", "ich", "er\u00b7z\u00e4h\u00b7len", "wie", "mir", "Freu\u00b7den", "an", "Freu\u00b7den", "er\u00b7bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "PWAV", "PPER", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Bachus, ihr kennet ihn nicht, ist stets mein Gef\u00e4hrte geblieben,", "tokens": ["Ba\u00b7chus", ",", "ihr", "ken\u00b7net", "ihn", "nicht", ",", "ist", "stets", "mein", "Ge\u00b7f\u00e4hr\u00b7te", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+--+-+---+-+--+-", "measure": "iambic.hexa.invert"}, "line.12": {"text": "Aber als Gott mir gezeigt hat ihn Sicilien erst.", "tokens": ["A\u00b7ber", "als", "Gott", "mir", "ge\u00b7zeigt", "hat", "ihn", "Si\u00b7ci\u00b7li\u00b7en", "erst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "PPER", "VVPP", "VAFIN", "PPER", "NE", "ADV", "$."], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.13": {"text": "Zweifel plagen auch euch, so erlaubt dem S\u00e4nger den Kampf auch,", "tokens": ["Zwei\u00b7fel", "pla\u00b7gen", "auch", "euch", ",", "so", "er\u00b7laubt", "dem", "S\u00e4n\u00b7ger", "den", "Kampf", "auch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PPER", "$,", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Welchem trinakrischen Wein werde der k\u00f6stlichste Preis.", "tokens": ["Wel\u00b7chem", "tri\u00b7nak\u00b7ri\u00b7schen", "Wein", "wer\u00b7de", "der", "k\u00f6st\u00b7lichs\u00b7te", "Preis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+---+--+", "measure": "iambic.hexa.chol"}, "line.15": {"text": "Syrakus, es bietet mir hier auf goldener Schaale", "tokens": ["Sy\u00b7ra\u00b7kus", ",", "es", "bie\u00b7tet", "mir", "hier", "auf", "gol\u00b7de\u00b7ner", "Schaa\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Schon den s\u00fc\u00dfen Muskat, schon Amarina zum Trank.", "tokens": ["Schon", "den", "s\u00fc\u00b7\u00dfen", "Mus\u00b7kat", ",", "schon", "A\u00b7ma\u00b7ri\u00b7na", "zum", "Trank", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "ADV", "NE", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.17": {"text": "Nah an den Tr\u00fcmmern auch der palmenreichen Selinus", "tokens": ["Nah", "an", "den", "Tr\u00fcm\u00b7mern", "auch", "der", "pal\u00b7men\u00b7rei\u00b7chen", "Se\u00b7li\u00b7nus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.18": {"text": "Hat mich das purpurne Blut n\u00e4her den G\u00f6ttern ger\u00fcckt.", "tokens": ["Hat", "mich", "das", "pur\u00b7pur\u00b7ne", "Blut", "n\u00e4\u00b7her", "den", "G\u00f6t\u00b7tern", "ge\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Wo ert\u00f6nt nicht dein Ruhm, Marsala? Dir g\u00e4b' ich die Krone,", "tokens": ["Wo", "er\u00b7t\u00f6nt", "nicht", "dein", "Ruhm", ",", "Mar\u00b7sa\u00b7la", "?", "Dir", "g\u00e4b'", "ich", "die", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,", "NE", "$.", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "--+--++-+-+--+-", "measure": "anapaest.di.plus"}, "line.20": {"text": "Reichte mir Alcamo schon, reichte Palermo mir nicht", "tokens": ["Reich\u00b7te", "mir", "Al\u00b7ca\u00b7mo", "schon", ",", "reich\u00b7te", "Pa\u00b7ler\u00b7mo", "mir", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NE", "ADV", "$,", "VVFIN", "NE", "PPER", "PTKNEG"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.21": {"text": "Andern Nektars Entz\u00fcckungen schon im uranischen Kelche,", "tokens": ["An\u00b7dern", "Nek\u00b7tars", "Ent\u00b7z\u00fc\u00b7ckun\u00b7gen", "schon", "im", "u\u00b7ra\u00b7ni\u00b7schen", "Kel\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+-+-+-", "measure": "iambic.septa.invert"}, "line.22": {"text": "Nicht im Kelche, den mir Amor, der lust'ge, kredenzt.", "tokens": ["Nicht", "im", "Kel\u00b7che", ",", "den", "mir", "A\u00b7mor", ",", "der", "lust'\u00b7ge", ",", "kre\u00b7denzt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "$,", "PRELS", "PPER", "NE", "$,", "ART", "ADJA", "$,", "VVFIN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "Aber wenn auch der Freund, der treffliche, nimmer vergessne,", "tokens": ["A\u00b7ber", "wenn", "auch", "der", "Freund", ",", "der", "treff\u00b7li\u00b7che", ",", "nim\u00b7mer", "ver\u00b7gess\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADV", "VVFIN", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.24": {"text": "Deutschen Herzens, ja werth mehr als ein Deutscher zu sein,", "tokens": ["Deut\u00b7schen", "Her\u00b7zens", ",", "ja", "werth", "mehr", "als", "ein", "Deut\u00b7scher", "zu", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "ADJD", "PIAT", "KOKOM", "ART", "NN", "PTKZU", "VAINF", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.25": {"text": "Wenn unerm\u00fcdlicher Gastfreundschaft der sch\u00f6nen Messina", "tokens": ["Wenn", "un\u00b7er\u00b7m\u00fcd\u00b7li\u00b7cher", "Gast\u00b7freund\u00b7schaft", "der", "sch\u00f6\u00b7nen", "Mes\u00b7si\u00b7na"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "ART", "ADJA", "NE"], "meter": "-+-+--++--+-+-+", "measure": "iambic.septa.relaxed"}, "line.26": {"text": "Gellias mich an die Gluth g\u00f6ttlichen Nektars gew\u00f6hnt;", "tokens": ["Gel\u00b7li\u00b7as", "mich", "an", "die", "Gluth", "g\u00f6tt\u00b7li\u00b7chen", "Nek\u00b7tars", "ge\u00b7w\u00f6hnt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-++-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.27": {"text": "Dennoch sei mir vor allen gelobt, o Traube des Aetna,", "tokens": ["Den\u00b7noch", "sei", "mir", "vor", "al\u00b7len", "ge\u00b7lobt", ",", "o", "Trau\u00b7be", "des", "A\u00b7et\u00b7na", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PIS", "VVPP", "$,", "FM", "NN", "ART", "NE", "$,"], "meter": "+-+--+--+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.28": {"text": "Der, wie des donnernden Bergs Lava dem Krater entstr\u00f6mt,", "tokens": ["Der", ",", "wie", "des", "don\u00b7nern\u00b7den", "Bergs", "La\u00b7va", "dem", "Kra\u00b7ter", "ent\u00b7str\u00f6mt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ART", "ADJA", "NN", "NE", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "Goldene Str\u00f6m' entquellen, begeisternde, s\u00e4mmtlicher Wunder,", "tokens": ["Gol\u00b7de\u00b7ne", "Str\u00f6m'", "ent\u00b7quel\u00b7len", ",", "be\u00b7geis\u00b7tern\u00b7de", ",", "s\u00e4mmt\u00b7li\u00b7cher", "Wun\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.30": {"text": "Die der Aetna gebiert, gr\u00f6\u00dftes und seligstes du.", "tokens": ["Die", "der", "A\u00b7et\u00b7na", "ge\u00b7biert", ",", "gr\u00f6\u00df\u00b7tes", "und", "se\u00b7ligs\u00b7tes", "du", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NE", "VVPP", "$,", "ADJA", "KON", "ADJA", "PPER", "$."], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Kein Element versagt dir den Kranz; dich k\u00fchlet die Meerfluth,", "tokens": ["Kein", "E\u00b7le\u00b7ment", "ver\u00b7sagt", "dir", "den", "Kranz", ";", "dich", "k\u00fch\u00b7let", "die", "Meer\u00b7fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.32": {"text": "Dich uml\u00e4chelt des Lichts heiterste, mildeste Kraft;", "tokens": ["Dich", "um\u00b7l\u00e4\u00b7chelt", "des", "Lichts", "hei\u00b7ters\u00b7te", ",", "mil\u00b7des\u00b7te", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.33": {"text": "Dich durchbrennt die Flamme des Bergs, und die Erde, die tausend", "tokens": ["Dich", "durch\u00b7brennt", "die", "Flam\u00b7me", "des", "Bergs", ",", "und", "die", "Er\u00b7de", ",", "die", "tau\u00b7send"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,", "KON", "ART", "NN", "$,", "PRELS", "CARD"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.34": {"text": "Bl\u00fcthen und Fr\u00fcchte bei dir Fr\u00fchling und Winter vereint.", "tokens": ["Bl\u00fc\u00b7then", "und", "Fr\u00fcch\u00b7te", "bei", "dir", "Fr\u00fch\u00b7ling", "und", "Win\u00b7ter", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PPER", "NN", "KON", "NN", "VVPP", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.35": {"text": "Gl\u00e4nze, lieblichstes Gold; es kr\u00e4nzt dich die Myrthe, der Lorbeer;", "tokens": ["Gl\u00e4n\u00b7ze", ",", "lieb\u00b7lichs\u00b7tes", "Gold", ";", "es", "kr\u00e4nzt", "dich", "die", "Myr\u00b7the", ",", "der", "Lor\u00b7beer", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.36": {"text": "Der ich dich schl\u00fcrfe, mir ist Lorbeer und Myrthe gewi\u00df.", "tokens": ["Der", "ich", "dich", "schl\u00fcr\u00b7fe", ",", "mir", "ist", "Lor\u00b7beer", "und", "Myr\u00b7the", "ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "VVFIN", "$,", "PPER", "VAFIN", "NE", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}}}}