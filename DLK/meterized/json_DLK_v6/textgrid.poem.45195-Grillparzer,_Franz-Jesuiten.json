{"textgrid.poem.45195": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Jesuiten", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Schweizer worfeln t\u00fcchtig drauf,", "tokens": ["Die", "Schwei\u00b7zer", "wor\u00b7feln", "t\u00fcch\u00b7tig", "drauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Frucht f\u00e4llt dicht dabei,", "tokens": ["Die", "Frucht", "f\u00e4llt", "dicht", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch \u00d6streich h\u00e4lt nach oben auf", "tokens": ["Doch", "\u00d6s\u00b7treich", "h\u00e4lt", "nach", "o\u00b7ben", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ADV", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sammelt sich \u2013 die Spreu.", "tokens": ["Und", "sam\u00b7melt", "sich", "\u2013", "die", "Spreu", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$(", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}