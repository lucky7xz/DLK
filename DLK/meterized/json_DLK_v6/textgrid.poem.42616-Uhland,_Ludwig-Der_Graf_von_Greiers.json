{"textgrid.poem.42616": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Graf von Greiers", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der junge Graf von Greiers, er steht vor seinem Haus,", "tokens": ["Der", "jun\u00b7ge", "Graf", "von", "Grei\u00b7ers", ",", "er", "steht", "vor", "sei\u00b7nem", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Er sieht am sch\u00f6nen Morgen weit ins Gebirg hinaus,", "tokens": ["Er", "sieht", "am", "sch\u00f6\u00b7nen", "Mor\u00b7gen", "weit", "ins", "Ge\u00b7birg", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "ADJD", "APPRART", "NN", "APZR", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Er sieht die Felsenh\u00f6rner verkl\u00e4rt im goldnen Strahl", "tokens": ["Er", "sieht", "die", "Fel\u00b7sen\u00b7h\u00f6r\u00b7ner", "ver\u00b7kl\u00e4rt", "im", "gold\u00b7nen", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und d\u00e4mmernd mitten inne das gr\u00fcnste Alpental.", "tokens": ["Und", "d\u00e4m\u00b7mernd", "mit\u00b7ten", "in\u00b7ne", "das", "gr\u00fcns\u00b7te", "Al\u00b7pen\u00b7tal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbo Alpe, gr\u00fcne Alpe! wie zieht's nach dir mich hin!", "tokens": ["\u00bb", "o", "Al\u00b7pe", ",", "gr\u00fc\u00b7ne", "Al\u00b7pe", "!", "wie", "zieht's", "nach", "dir", "mich", "hin", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NE", "$,", "ADJA", "NN", "$.", "PWAV", "VVFIN", "APPR", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Begl\u00fcckt, die dich befahren, Berghirt und Sennerin!", "tokens": ["Be\u00b7gl\u00fcckt", ",", "die", "dich", "be\u00b7fah\u00b7ren", ",", "Berg\u00b7hirt", "und", "Sen\u00b7ne\u00b7rin", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRELS", "PRF", "VVINF", "$,", "VVPP", "KON", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Oft sah ich sonst hin\u00fcber, empfand nicht Leid noch Lust,", "tokens": ["Oft", "sah", "ich", "sonst", "hin\u00b7\u00fc\u00b7ber", ",", "emp\u00b7fand", "nicht", "Leid", "noch", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$,", "VVFIN", "PTKNEG", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Doch heute dringt ein Sehnen mir in die tiefste Brust.\u00ab", "tokens": ["Doch", "heu\u00b7te", "dringt", "ein", "Seh\u00b7nen", "mir", "in", "die", "tiefs\u00b7te", "Brust", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PPER", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Und nah und n\u00e4her klingen Schalmeien an sein Ohr,", "tokens": ["Und", "nah", "und", "n\u00e4\u00b7her", "klin\u00b7gen", "Schal\u00b7mei\u00b7en", "an", "sein", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die Hirtinnen und Hirten, sie ziehn zur Burg empor,", "tokens": ["Die", "Hir\u00b7tin\u00b7nen", "und", "Hir\u00b7ten", ",", "sie", "ziehn", "zur", "Burg", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und auf des Schlosses Rasen hebt an der Ringeltanz,", "tokens": ["Und", "auf", "des", "Schlos\u00b7ses", "Ra\u00b7sen", "hebt", "an", "der", "Rin\u00b7gel\u00b7tanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die wei\u00dfen \u00c4rmel schimmern, bunt flattern Band und Kranz.", "tokens": ["Die", "wei\u00b7\u00dfen", "\u00c4r\u00b7mel", "schim\u00b7mern", ",", "bunt", "flat\u00b7tern", "Band", "und", "Kranz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ADJD", "VVINF", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Der Sennerinnen j\u00fcngste, schlank wie ein Maienreis,", "tokens": ["Der", "Sen\u00b7ne\u00b7rin\u00b7nen", "j\u00fcngs\u00b7te", ",", "schlank", "wie", "ein", "Mai\u00b7en\u00b7reis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "$,", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Erfa\u00dft die Hand des Grafen, da mu\u00df er in den Kreis.", "tokens": ["Er\u00b7fa\u00dft", "die", "Hand", "des", "Gra\u00b7fen", ",", "da", "mu\u00df", "er", "in", "den", "Kreis", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,", "ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Es schlinget ihn der Reigen in seine Wirbel ein:", "tokens": ["Es", "schlin\u00b7get", "ihn", "der", "Rei\u00b7gen", "in", "sei\u00b7ne", "Wir\u00b7bel", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "\u00bbhei! junger Graf von Greiers, gefangen mu\u00dft du sein!\u00ab", "tokens": ["\u00bb", "hei", "!", "jun\u00b7ger", "Graf", "von", "Grei\u00b7ers", ",", "ge\u00b7fan\u00b7gen", "mu\u00dft", "du", "sein", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$.", "ADJA", "NN", "APPR", "NE", "$,", "ADJD", "VMFIN", "PPER", "VAINF", "$.", "$("], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.5": {"line.1": {"text": "Sie raffen ihn von hinnen mit Sprung und Reigenlied,", "tokens": ["Sie", "raf\u00b7fen", "ihn", "von", "hin\u00b7nen", "mit", "Sprung", "und", "Rei\u00b7gen\u00b7lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sie tanzen durch die D\u00f6rfer, wo Glied sich reiht an Glied,", "tokens": ["Sie", "tan\u00b7zen", "durch", "die", "D\u00f6r\u00b7fer", ",", "wo", "Glied", "sich", "reiht", "an", "Glied", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PWAV", "NN", "PRF", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sie tanzen \u00fcber Matten, sie tanzen durch den Wald,", "tokens": ["Sie", "tan\u00b7zen", "\u00fc\u00b7ber", "Mat\u00b7ten", ",", "sie", "tan\u00b7zen", "durch", "den", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Bis fernhin auf den Alpen der helle Klang verhallt.", "tokens": ["Bis", "fern\u00b7hin", "auf", "den", "Al\u00b7pen", "der", "hel\u00b7le", "Klang", "ver\u00b7hallt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Schon steigt der zweite Morgen, der dritte wird schon klar \u2013", "tokens": ["Schon", "steigt", "der", "zwei\u00b7te", "Mor\u00b7gen", ",", "der", "drit\u00b7te", "wird", "schon", "klar", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "VAFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wo bleibt der Graf von Greiers? ist er verschollen gar?", "tokens": ["Wo", "bleibt", "der", "Graf", "von", "Grei\u00b7ers", "?", "ist", "er", "ver\u00b7schol\u00b7len", "gar", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "NE", "$.", "VAFIN", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und wieder sinkt zum Abend der schw\u00fclen Sonne Lauf;", "tokens": ["Und", "wie\u00b7der", "sinkt", "zum", "A\u00b7bend", "der", "schw\u00fc\u00b7len", "Son\u00b7ne", "Lauf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da donnert's im Gebirge, da ziehn die Wetter auf.", "tokens": ["Da", "don\u00b7nert's", "im", "Ge\u00b7bir\u00b7ge", ",", "da", "ziehn", "die", "Wet\u00b7ter", "auf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPRART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Geborsten ist die Wolke, der Bach zum Strom geschwellt,", "tokens": ["Ge\u00b7bors\u00b7ten", "ist", "die", "Wol\u00b7ke", ",", "der", "Bach", "zum", "Strom", "ge\u00b7schwellt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und als mit j\u00e4hem Strahle der Blitz die Nacht erhellt,", "tokens": ["Und", "als", "mit", "j\u00e4\u00b7hem", "Strah\u00b7le", "der", "Blitz", "die", "Nacht", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "ADJA", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da zeigt sich in den Strudeln ein Mann, der wogt und ringt,", "tokens": ["Da", "zeigt", "sich", "in", "den", "Stru\u00b7deln", "ein", "Mann", ",", "der", "wogt", "und", "ringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Bis er den Ast ergriffen und sich ans Ufer schwingt.", "tokens": ["Bis", "er", "den", "Ast", "er\u00b7grif\u00b7fen", "und", "sich", "ans", "U\u00b7fer", "schwingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "KON", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "\u00bbda bin ich! weggerissen aus eurer Berge Scho\u00df,", "tokens": ["\u00bb", "da", "bin", "ich", "!", "weg\u00b7ge\u00b7ris\u00b7sen", "aus", "eu\u00b7rer", "Ber\u00b7ge", "Scho\u00df", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "$.", "VVPP", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Im Tanzen und im Schwingen ergriff mich Sturmgetos;", "tokens": ["Im", "Tan\u00b7zen", "und", "im", "Schwin\u00b7gen", "er\u00b7griff", "mich", "Sturm\u00b7ge\u00b7tos", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ihr alle seid geborgen in H\u00fctt und Felsenspalt,", "tokens": ["Ihr", "al\u00b7le", "seid", "ge\u00b7bor\u00b7gen", "in", "H\u00fctt", "und", "Fel\u00b7sen\u00b7spalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VAFIN", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Nur mich hat fortgeschwemmet des Wolkenbruchs Gewalt.", "tokens": ["Nur", "mich", "hat", "fort\u00b7ge\u00b7schwem\u00b7met", "des", "Wol\u00b7ken\u00b7bruchs", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Leb wohl, du gr\u00fcne Alpe, mit deiner frohen Schar!", "tokens": ["Leb", "wohl", ",", "du", "gr\u00fc\u00b7ne", "Al\u00b7pe", ",", "mit", "dei\u00b7ner", "fro\u00b7hen", "Schar", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "ADJA", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Lebt wohl, drei sel'ge Tage, da ich ein Hirte war!", "tokens": ["Lebt", "wohl", ",", "drei", "sel'\u00b7ge", "Ta\u00b7ge", ",", "da", "ich", "ein", "Hir\u00b7te", "war", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "CARD", "ADJA", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "O! nicht bin ich geboren zu solchem Paradies,", "tokens": ["O", "!", "nicht", "bin", "ich", "ge\u00b7bo\u00b7ren", "zu", "sol\u00b7chem", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PTKNEG", "VAFIN", "PPER", "VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Aus dem mit Blitzesflamme des Himmels Zorn mich wies.", "tokens": ["Aus", "dem", "mit", "Blit\u00b7zes\u00b7flam\u00b7me", "des", "Him\u00b7mels", "Zorn", "mich", "wies", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "NN", "ART", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Du frische Alpenrose, r\u00fchr nimmer meine Hand!", "tokens": ["Du", "fri\u00b7sche", "Al\u00b7pen\u00b7ro\u00b7se", ",", "r\u00fchr", "nim\u00b7mer", "mei\u00b7ne", "Hand", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ich f\u00fchl's, die kalte Woge, sie l\u00f6scht nicht diesen Brand.", "tokens": ["Ich", "f\u00fchl's", ",", "die", "kal\u00b7te", "Wo\u00b7ge", ",", "sie", "l\u00f6scht", "nicht", "die\u00b7sen", "Brand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "PDAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Du zauberischer Reigen, lock nimmer mich hinaus!", 