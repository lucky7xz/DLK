{"textgrid.poem.33884": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5. Der Tod", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Glocken hast du noch gepfl\u00fcckt,", "tokens": ["Die", "Glo\u00b7cken", "hast", "du", "noch", "ge\u00b7pfl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die uns den Lenz verk\u00fcnden,", "tokens": ["Die", "uns", "den", "Lenz", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch nicht, vom schweren Schnee' gedr\u00fcckt,", "tokens": ["Doch", "nicht", ",", "vom", "schwe\u00b7ren", "Schnee'", "ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "$,", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Farben sich entz\u00fcnden.", "tokens": ["In", "Far\u00b7ben", "sich", "ent\u00b7z\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Auch hast du dir zum Sonntagsstrau\u00df", "tokens": ["Auch", "hast", "du", "dir", "zum", "Sonn\u00b7tags\u00b7strau\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Veilchen noch gewunden", "tokens": ["Die", "Veil\u00b7chen", "noch", "ge\u00b7wun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ihren Duft im Gotteshaus", "tokens": ["Und", "ih\u00b7ren", "Duft", "im", "Got\u00b7tes\u00b7haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So s\u00fc\u00df, wie nie, gefunden.", "tokens": ["So", "s\u00fc\u00df", ",", "wie", "nie", ",", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "ADV", "$,", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein frischer Maienblumenkranz", "tokens": ["Ein", "fri\u00b7scher", "Mai\u00b7en\u00b7blu\u00b7men\u00b7kranz"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "War dir in's Haar geflochten,", "tokens": ["War", "dir", "in's", "Haar", "ge\u00b7floch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als dir in deinem letzten Tanz", "tokens": ["Als", "dir", "in", "dei\u00b7nem", "letz\u00b7ten", "Tanz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die zarten Schl\u00e4fe pochten.", "tokens": ["Die", "zar\u00b7ten", "Schl\u00e4\u00b7fe", "poch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Rosen treffen dich schon bleich", "tokens": ["Die", "Ro\u00b7sen", "tref\u00b7fen", "dich", "schon", "bleich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Kreise deiner Schwestern:", "tokens": ["Im", "Krei\u00b7se", "dei\u00b7ner", "Schwes\u00b7tern", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der wei\u00dfen bist du heute gleich,", "tokens": ["Der", "wei\u00b7\u00dfen", "bist", "du", "heu\u00b7te", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der rothen glichst du gestern.", "tokens": ["Der", "ro\u00b7then", "glichst", "du", "ge\u00b7stern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Doch kommen sie zur rechten Frist,", "tokens": ["Doch", "kom\u00b7men", "sie", "zur", "rech\u00b7ten", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um deinen Sarg zu decken,", "tokens": ["Um", "dei\u00b7nen", "Sarg", "zu", "de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und was du warst und was du bist,", "tokens": ["Und", "was", "du", "warst", "und", "was", "du", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VAFIN", "KON", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch einmal zu erwecken!", "tokens": ["Noch", "ein\u00b7mal", "zu", "er\u00b7we\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Nelken bl\u00fchen mir allein", "tokens": ["Die", "Nel\u00b7ken", "bl\u00fc\u00b7hen", "mir", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und k\u00f6nnen mich nur freuen,", "tokens": ["Und", "k\u00f6n\u00b7nen", "mich", "nur", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Um sie bei hellem Mondenschein", "tokens": ["Um", "sie", "bei", "hel\u00b7lem", "Mon\u00b7den\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dir auf das Grab zu streuen.", "tokens": ["Dir", "auf", "das", "Grab", "zu", "streu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}