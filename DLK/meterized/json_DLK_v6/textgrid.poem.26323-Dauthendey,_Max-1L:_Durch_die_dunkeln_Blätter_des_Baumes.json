{"textgrid.poem.26323": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Durch die dunkeln Bl\u00e4tter des Baumes", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch die dunkeln Bl\u00e4tter des Baumes", "tokens": ["Durch", "die", "dun\u00b7keln", "Bl\u00e4t\u00b7ter", "des", "Bau\u00b7mes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Sieht mich eine gelbe Abendwolke an.", "tokens": ["Sieht", "mich", "ei\u00b7ne", "gel\u00b7be", "Ab\u00b7end\u00b7wol\u00b7ke", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Die leichten Bl\u00e4tter winken im Wind,", "tokens": ["Die", "leich\u00b7ten", "Bl\u00e4t\u00b7ter", "win\u00b7ken", "im", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sie, die des Baumes gl\u00fcckliche Familie sind.", "tokens": ["Sie", ",", "die", "des", "Bau\u00b7mes", "gl\u00fcck\u00b7li\u00b7che", "Fa\u00b7mi\u00b7lie", "sind", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wann kommt die goldne Wolke zu mir heran?", "tokens": ["Wann", "kommt", "die", "gold\u00b7ne", "Wol\u00b7ke", "zu", "mir", "he\u00b7ran", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nicht mal wie ein Baum ich froh sein kann.", "tokens": ["Nicht", "mal", "wie", "ein", "Baum", "ich", "froh", "sein", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KOKOM", "ART", "NN", "PPER", "ADJD", "VAINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Die Tage sind ein wirrer Wahn,", "tokens": ["Die", "Ta\u00b7ge", "sind", "ein", "wir\u00b7rer", "Wahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Wirr, ohne die Gnade des Traumes.", "tokens": ["Wirr", ",", "oh\u00b7ne", "die", "Gna\u00b7de", "des", "Trau\u00b7mes", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}