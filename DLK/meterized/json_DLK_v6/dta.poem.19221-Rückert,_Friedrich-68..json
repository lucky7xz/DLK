{"dta.poem.19221": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "68.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Wandersmann, der aus der weiten W\u00fcste kam,", "tokens": ["Ein", "Wan\u00b7ders\u00b7mann", ",", "der", "aus", "der", "wei\u00b7ten", "W\u00fcs\u00b7te", "kam", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo er nicht Menschenwort noch Menschenblick vernahm,", "tokens": ["Wo", "er", "nicht", "Men\u00b7schen\u00b7wort", "noch", "Men\u00b7schen\u00b7blick", "ver\u00b7nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Tritt in ein Felsenthal, von B\u00e4umen k\u00fchl beschattet,", "tokens": ["Tritt", "in", "ein", "Fel\u00b7sen\u00b7thal", ",", "von", "B\u00e4u\u00b7men", "k\u00fchl", "be\u00b7schat\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo eine Quelle rauscht, da setzt er sich ermattet.", "tokens": ["Wo", "ei\u00b7ne", "Quel\u00b7le", "rauscht", ",", "da", "setzt", "er", "sich", "er\u00b7mat\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nun schaut er in den Quell, und sieht sich selb darinn,", "tokens": ["Nun", "schaut", "er", "in", "den", "Quell", ",", "und", "sieht", "sich", "selb", "da\u00b7rinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "PRF", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wei\u00df nicht da\u00df ers ist, und schwankt in seinem Sinn.", "tokens": ["Und", "wei\u00df", "nicht", "da\u00df", "ers", "ist", ",", "und", "schwankt", "in", "sei\u00b7nem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "KOUS", "PIS", "VAFIN", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Er schwankt, alswie er sieht sein Bild im Quelle schwanken,", "tokens": ["Er", "schwankt", ",", "als\u00b7wie", "er", "sieht", "sein", "Bild", "im", "Quel\u00b7le", "schwan\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sinkt, wie in die Flut, in wogende Gedanken.", "tokens": ["Und", "sinkt", ",", "wie", "in", "die", "Flut", ",", "in", "wo\u00b7gen\u00b7de", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "APPR", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}}, "stanza.5": {"line.1": {"text": "Dann ruft er staunend aus: Wer bist du? und mit Staunen", "tokens": ["Dann", "ruft", "er", "stau\u00b7nend", "aus", ":", "Wer", "bist", "du", "?", "und", "mit", "Stau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "PWS", "VAFIN", "PPER", "$.", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "H\u00f6rt er der Felsen Mund Wer bist du? gegenraunen.", "tokens": ["H\u00f6rt", "er", "der", "Fel\u00b7sen", "Mund", "Wer", "bist", "du", "?", "ge\u00b7gen\u00b7rau\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "PWS", "VAFIN", "PPER", "$.", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Durch Gegenfrage wird die Frage nicht beschwichtet,", "tokens": ["Durch", "Ge\u00b7gen\u00b7fra\u00b7ge", "wird", "die", "Fra\u00b7ge", "nicht", "be\u00b7schwich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch hat die Einsamkeit nichts andres ihm berichtet.", "tokens": ["Doch", "hat", "die", "Ein\u00b7sam\u00b7keit", "nichts", "and\u00b7res", "ihm", "be\u00b7rich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PIS", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Sich selb nur sieht der Mensch im Spiegel der Natur,", "tokens": ["Sich", "selb", "nur", "sieht", "der", "Mensch", "im", "Spie\u00b7gel", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was er sie befragt, das widerholt sie nur.", "tokens": ["Und", "was", "er", "sie", "be\u00b7fragt", ",", "das", "wi\u00b7der\u00b7holt", "sie", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PPER", "VVPP", "$,", "PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}