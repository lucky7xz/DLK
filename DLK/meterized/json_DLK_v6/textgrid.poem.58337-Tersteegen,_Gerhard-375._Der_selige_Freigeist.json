{"textgrid.poem.58337": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "375. Der selige Freigeist", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein freier Geist in Gott erhaben stehet,", "tokens": ["Ein", "frei\u00b7er", "Geist", "in", "Gott", "er\u00b7ha\u00b7ben", "ste\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nichts bindet ihn als Gottes Liebe nur,", "tokens": ["Nichts", "bin\u00b7det", "ihn", "als", "Got\u00b7tes", "Lie\u00b7be", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "KOUS", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nichts r\u00fchret ihn, weil er, im Geist erh\u00f6het,", "tokens": ["Nichts", "r\u00fch\u00b7ret", "ihn", ",", "weil", "er", ",", "im", "Geist", "er\u00b7h\u00f6\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "L\u00e4\u00dft unter sich Welt, Sinne und Natur.", "tokens": ["L\u00e4\u00dft", "un\u00b7ter", "sich", "Welt", ",", "Sin\u00b7ne", "und", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}