{"textgrid.poem.60940": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Ehedem", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nnt ich so sch\u00f6n, wie du warst, o Venedig, und w\u00e4r's nur f\u00fcr Einen", "tokens": ["K\u00f6nnt", "ich", "so", "sch\u00f6n", ",", "wie", "du", "warst", ",", "o", "Ve\u00b7ne\u00b7dig", ",", "und", "w\u00e4r's", "nur", "f\u00fcr", "Ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "$,", "PWAV", "PPER", "VAFIN", "$,", "FM", "NE", "$,", "KON", "VAFIN", "ADV", "APPR", "ART"], "meter": "+--+--+-+-+-+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Einzigen Tag, dich schaun, Eine verg\u00e4ngliche Nacht!", "tokens": ["Ein\u00b7zi\u00b7gen", "Tag", ",", "dich", "schaun", ",", "Ei\u00b7ne", "ver\u00b7g\u00e4ng\u00b7li\u00b7che", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRF", "VVINF", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Wieder von Gondeln belebt, von unz\u00e4hligen, diese Kan\u00e4le", "tokens": ["Wie\u00b7der", "von", "Gon\u00b7deln", "be\u00b7lebt", ",", "von", "un\u00b7z\u00e4h\u00b7li\u00b7gen", ",", "die\u00b7se", "Ka\u00b7n\u00e4\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "NN", "VVPP", "$,", "APPR", "ADJA", "$,", "PDAT", "NN"], "meter": "+--+--+-+---+--+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Schaun und des Reichtums Pomp neben des Handels Erwerb!", "tokens": ["Schaun", "und", "des", "Reich\u00b7tums", "Pomp", "ne\u00b7ben", "des", "Han\u00b7dels", "Er\u00b7werb", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ART", "NN", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Diese Pal\u00e4ste, ver\u00f6det und leer und mit Brettern verschlossen,", "tokens": ["Die\u00b7se", "Pa\u00b7l\u00e4s\u00b7te", ",", "ver\u00b7\u00f6\u00b7det", "und", "leer", "und", "mit", "Bret\u00b7tern", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "VVPP", "KON", "ADJD", "KON", "APPR", "NN", "VVPP", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Deren Balkone sich einst f\u00fcllten mit herrlichen Fraun,", "tokens": ["De\u00b7ren", "Bal\u00b7ko\u00b7ne", "sich", "einst", "f\u00fcll\u00b7ten", "mit", "herr\u00b7li\u00b7chen", "Fraun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "W\u00e4ren sie wieder beseelt von Gitarren und fr\u00f6hlichem Echo,", "tokens": ["W\u00e4\u00b7ren", "sie", "wie\u00b7der", "be\u00b7seelt", "von", "Gi\u00b7tar\u00b7ren", "und", "fr\u00f6h\u00b7li\u00b7chem", "E\u00b7cho", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Oder von Siegsbotschaft, oder von Liebe zumal!", "tokens": ["O\u00b7der", "von", "Siegs\u00b7bot\u00b7schaft", ",", "o\u00b7der", "von", "Lie\u00b7be", "zu\u00b7mal", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "KON", "APPR", "NN", "ADV", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.9": {"text": "Still, wie das Grab, nun spiegelt und schwermutsvoll in der Flut sich", "tokens": ["Still", ",", "wie", "das", "Grab", ",", "nun", "spie\u00b7gelt", "und", "schwer\u00b7muts\u00b7voll", "in", "der", "Flut", "sich"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "$,", "ADV", "VVFIN", "KON", "ADJD", "APPR", "ART", "NN", "PRF"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Gotischen Fenstergeb\u00e4lks schlanker und zierlicher Bau.", "tokens": ["Go\u00b7ti\u00b7schen", "Fens\u00b7ter\u00b7ge\u00b7b\u00e4lks", "schlan\u00b7ker", "und", "zier\u00b7li\u00b7cher", "Bau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}}}}}