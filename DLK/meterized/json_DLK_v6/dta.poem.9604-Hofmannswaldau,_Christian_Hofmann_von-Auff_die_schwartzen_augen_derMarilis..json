{"dta.poem.9604": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auff die schwartzen augen derMarilis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist das schwartze doch/ mein kind/", "tokens": ["Was", "ist", "das", "schwart\u00b7ze", "doch", "/", "mein", "kind", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "VVFIN", "ADV", "$(", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das sich in deinen augen findt?", "tokens": ["Das", "sich", "in", "dei\u00b7nen", "au\u00b7gen", "findt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sag/ ob ich irre/ wann mich d\u00fcncket/", "tokens": ["Sag", "/", "ob", "ich", "ir\u00b7re", "/", "wann", "mich", "d\u00fcn\u00b7cket", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "VVFIN", "$(", "PWAV", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df dir das ferne Morenland", "tokens": ["Da\u00df", "dir", "das", "fer\u00b7ne", "Mo\u00b7ren\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So schwartze farben zugesandt/", "tokens": ["So", "schwart\u00b7ze", "far\u00b7ben", "zu\u00b7ge\u00b7sandt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit welchen du dein aug geschmincket.", "tokens": ["Mit", "wel\u00b7chen", "du", "dein", "aug", "ge\u00b7schmin\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der sch\u00f6nen augen schwartze pracht", "tokens": ["Der", "sch\u00f6\u00b7nen", "au\u00b7gen", "schwart\u00b7ze", "pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entwirfft das bild der mitternacht/", "tokens": ["Ent\u00b7wirfft", "das", "bild", "der", "mit\u00b7ter\u00b7nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die solch unsichtbar eisen nehret;", "tokens": ["Die", "solch", "un\u00b7sicht\u00b7bar", "ei\u00b7sen", "neh\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJD", "VVINF", "VVFIN", "$."], "meter": "+----+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Zu dem sich stetig der magnet", "tokens": ["Zu", "dem", "sich", "ste\u00b7tig", "der", "mag\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PRF", "ADJD", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Verliebter welt und seelen dreht/", "tokens": ["Ver\u00b7lieb\u00b7ter", "welt", "und", "see\u00b7len", "dreht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und gleichsam wie gezwungen kehret.", "tokens": ["Und", "gleich\u00b7sam", "wie", "ge\u00b7zwun\u00b7gen", "keh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie aber wird mir umb das hertz?", "tokens": ["Wie", "a\u00b7ber", "wird", "mir", "umb", "das", "hertz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bey diesen augen ist kein schertz/", "tokens": ["Bey", "die\u00b7sen", "au\u00b7gen", "ist", "kein", "schertz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sehe dunckle wolcken blitzen/", "tokens": ["Ich", "se\u00b7he", "dunck\u00b7le", "wol\u00b7cken", "blit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich sehe sonnen in der nacht", "tokens": ["Ich", "se\u00b7he", "son\u00b7nen", "in", "der", "nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sp\u00fcre/ da\u00df mit aller macht", "tokens": ["Und", "sp\u00fc\u00b7re", "/", "da\u00df", "mit", "al\u00b7ler", "macht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$(", "KOUS", "APPR", "PIAT", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auch ausgel\u00f6schte kohlen hitzen.", "tokens": ["Auch", "aus\u00b7ge\u00b7l\u00f6schte", "koh\u00b7len", "hit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVINF", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "O schwartzes aug/ so alles brennt!", "tokens": ["O", "schwart\u00b7zes", "aug", "/", "so", "al\u00b7les", "brennt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$(", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O nacht/ die selbst die sonne blendt!", "tokens": ["O", "nacht", "/", "die", "selbst", "die", "son\u00b7ne", "blendt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "ART", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O finsterni\u00df bey stetem lichte!", "tokens": ["O", "fins\u00b7ter\u00b7ni\u00df", "bey", "ste\u00b7tem", "lich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O licht bey dicker finsterni\u00df!", "tokens": ["O", "licht", "bey", "di\u00b7cker", "fins\u00b7ter\u00b7ni\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo bleib ich/ meine Marilis/", "tokens": ["Wo", "bleib", "ich", "/", "mei\u00b7ne", "Ma\u00b7ri\u00b7lis", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$(", "PPOSAT", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Forthin vor deinem angesichte?", "tokens": ["For\u00b7thin", "vor", "dei\u00b7nem", "an\u00b7ge\u00b7sich\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}