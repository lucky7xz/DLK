{"dta.poem.23670": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Mein Leser,", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lebt, \u00fcberlebt die Splitterrichter,", "tokens": ["Lebt", ",", "\u00fc\u00b7ber\u00b7lebt", "die", "Split\u00b7ter\u00b7rich\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Freunde, die ihr weislich lacht,", "tokens": ["Ihr", "Freun\u00b7de", ",", "die", "ihr", "weis\u00b7lich", "lacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und einem aufgewekkten Dichter", "tokens": ["Und", "ei\u00b7nem", "auf\u00b7ge\u00b7wekk\u00b7ten", "Dich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht ieden Scherz zum Frevel macht!", "tokens": ["Nicht", "ie\u00b7den", "Scherz", "zum", "Fre\u00b7vel", "macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}