{"textgrid.poem.25080": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Giebt es noch neuen Gram f\u00fcr mich,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Giebt es noch neuen Gram f\u00fcr mich,", "tokens": ["Giebt", "es", "noch", "neu\u00b7en", "Gram", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Seitdem ich sie verloren habe?", "tokens": ["Seit\u00b7dem", "ich", "sie", "ver\u00b7lo\u00b7ren", "ha\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl manches Mal noch \u00fcberschleicht", "tokens": ["Wohl", "man\u00b7ches", "Mal", "noch", "\u00fc\u00b7bersc\u00b7hleicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich ein Gef\u00fchl, das neuem Kummer gleicht;", "tokens": ["Mich", "ein", "Ge\u00b7f\u00fchl", ",", "das", "neu\u00b7em", "Kum\u00b7mer", "gleicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dann will ich zu ihr eilen,", "tokens": ["Dann", "will", "ich", "zu", "ihr", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bei ihr die Wunde auszuheilen;", "tokens": ["Bei", "ihr", "die", "Wun\u00b7de", "aus\u00b7zu\u00b7hei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Doch pl\u00f6tzlich sag' ich mir: sie liegt im Grabe,", "tokens": ["Doch", "pl\u00f6tz\u00b7lich", "sag'", "ich", "mir", ":", "sie", "liegt", "im", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPER", "$.", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und in dem alten stirbt der neue Schmerz.", "tokens": ["Und", "in", "dem", "al\u00b7ten", "stirbt", "der", "neu\u00b7e", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ist eine Freude noch f\u00fcr mich,", "tokens": ["Ist", "ei\u00b7ne", "Freu\u00b7de", "noch", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seitdem ich sie verloren habe?", "tokens": ["Seit\u00b7dem", "ich", "sie", "ver\u00b7lo\u00b7ren", "ha\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl hie und da noch, halb mir unbewu\u00dft,", "tokens": ["Wohl", "hie", "und", "da", "noch", ",", "halb", "mir", "un\u00b7be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "ADV", "$,", "ADJD", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Regt sich ein froh Gef\u00fchl in meiner Brust;", "tokens": ["Regt", "sich", "ein", "froh", "Ge\u00b7f\u00fchl", "in", "mei\u00b7ner", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJD", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Dann will ich zu ihr eilen,", "tokens": ["Dann", "will", "ich", "zu", "ihr", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie soll mit mir die Freude teilen;", "tokens": ["Sie", "soll", "mit", "mir", "die", "Freu\u00b7de", "tei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Doch pl\u00f6tzlich sag' ich mir: sie liegt im Grabe,", "tokens": ["Doch", "pl\u00f6tz\u00b7lich", "sag'", "ich", "mir", ":", "sie", "liegt", "im", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPER", "$.", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und f\u00fchle gr\u00f6\u00dfer noch den alten Schmerz.", "tokens": ["Und", "f\u00fch\u00b7le", "gr\u00f6\u00b7\u00dfer", "noch", "den", "al\u00b7ten", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}