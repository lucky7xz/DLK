{"textgrid.poem.43643": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Des Sch\u00e4fers Wunsch", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hans Peter zog am Morgen", "tokens": ["Hans", "Pe\u00b7ter", "zog", "am", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ganz fr\u00fche fort von Haus,", "tokens": ["Ganz", "fr\u00fc\u00b7he", "fort", "von", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "PTKVZ", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er trieb die Schaf' und L\u00e4mmer", "tokens": ["Er", "trieb", "die", "Schaf'", "und", "L\u00e4m\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ins Freie 'naus.", "tokens": ["Ins", "Frei\u00b7e", "'naus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Das that er alle Morgen", "tokens": ["Das", "that", "er", "al\u00b7le", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und that es wohlgemuth,", "tokens": ["Und", "that", "es", "wohl\u00b7ge\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihm waren seine Schafe,", "tokens": ["Ihm", "wa\u00b7ren", "sei\u00b7ne", "Scha\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er ihnen gut.", "tokens": ["Er", "ih\u00b7nen", "gut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Mal that er's nicht so gerne,", "tokens": ["Mal", "that", "er's", "nicht", "so", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "PTKNEG", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es schien das Herz ihm schwer:", "tokens": ["Es", "schien", "das", "Herz", "ihm", "schwer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach! wenn ich doch ein Kaiser", "tokens": ["Ach", "!", "wenn", "ich", "doch", "ein", "Kai\u00b7ser"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "KOUS", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und K\u00f6nig w\u00e4r'!", "tokens": ["Und", "K\u00f6\u00b7nig", "w\u00e4r'", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Hans Peter! sagt der Vater,", "tokens": ["Hans", "Pe\u00b7ter", "!", "sagt", "der", "Va\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was, Junge, f\u00e4llt dir ein?", "tokens": ["Was", ",", "Jun\u00b7ge", ",", "f\u00e4llt", "dir", "ein", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Narr wirst du noch eher", "tokens": ["Ein", "Narr", "wirst", "du", "noch", "e\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ADV"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Als Kaiser sein.", "tokens": ["Als", "Kai\u00b7ser", "sein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Ach! w\u00e4r' ich auch kein Kaiser,", "tokens": ["Ach", "!", "w\u00e4r'", "ich", "auch", "kein", "Kai\u00b7ser", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VAFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Graf nur m\u00f6cht' ich sein,", "tokens": ["Ein", "Graf", "nur", "m\u00f6cht'", "ich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann h\u00fctet' ich zu Pferde", "tokens": ["Dann", "h\u00fc\u00b7tet'", "ich", "zu", "Pfer\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Schafe mein.", "tokens": ["Die", "Scha\u00b7fe", "mein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}