{"textgrid.poem.67293": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Venus-Urania auch hat ihren G\u00fcrtel, gewebet", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Venus-Urania auch hat ihren G\u00fcrtel, gewebet", "tokens": ["Ve\u00b7nus\u00b7U\u00b7ra\u00b7nia", "auch", "hat", "ih\u00b7ren", "G\u00fcr\u00b7tel", ",", "ge\u00b7we\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "ADV", "VAFIN", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Strahlend von Grazienhand, F\u00fclle des Zaubers in ihm,", "tokens": ["Strah\u00b7lend", "von", "Gra\u00b7zi\u00b7en\u00b7hand", ",", "F\u00fcl\u00b7le", "des", "Zau\u00b7bers", "in", "ihm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "NN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Edleren, h\u00f6heren Zaubers, der jenes beth\u00f6rende Listwerk,", "tokens": ["Ed\u00b7le\u00b7ren", ",", "h\u00f6\u00b7he\u00b7ren", "Zau\u00b7bers", ",", "der", "je\u00b7nes", "be\u00b7th\u00f6\u00b7ren\u00b7de", "List\u00b7werk", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "PRELS", "PDAT", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Das selbst G\u00f6tter umgarnt, himmelan strebend, verschm\u00e4ht.", "tokens": ["Das", "selbst", "G\u00f6t\u00b7ter", "um\u00b7garnt", ",", "him\u00b7me\u00b7lan", "stre\u00b7bend", ",", "ver\u00b7schm\u00e4ht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "ADV", "NN", "VVPP", "$,", "ADV", "ADJD", "$,", "VVPP", "$."], "meter": "--+-+-+-+---+", "measure": "anapaest.init"}, "line.5": {"text": "Diesen \u2013 und lieblicher gl\u00e4nzte die Sternenkrone der G\u00f6ttinn \u2013", "tokens": ["Die\u00b7sen", "\u2013", "und", "lieb\u00b7li\u00b7cher", "gl\u00e4nz\u00b7te", "die", "Ster\u00b7nen\u00b7kro\u00b7ne", "der", "G\u00f6t\u00b7tinn", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "$(", "KON", "ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$("], "meter": "+--+--+--+-+-+-+", "measure": "dactylic.tri.plus"}, "line.6": {"text": "Schenkte der J\u00fcngerinn Sie, freundlich zum Weihegeschenk.", "tokens": ["Schenk\u00b7te", "der", "J\u00fcn\u00b7ge\u00b7rinn", "Sie", ",", "freund\u00b7lich", "zum", "Wei\u00b7he\u00b7ge\u00b7schenk", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "O wie umschmiegte vertraulich das Himmelskleinod des Weibes", "tokens": ["O", "wie", "um\u00b7schmieg\u00b7te", "ver\u00b7trau\u00b7lich", "das", "Him\u00b7mels\u00b7klei\u00b7nod", "des", "Wei\u00b7bes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VVFIN", "ADJD", "ART", "NN", "ART", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Busen, wie f\u00fchlt es bei ihr sich so behaglich daheim!", "tokens": ["Bu\u00b7sen", ",", "wie", "f\u00fchlt", "es", "bei", "ihr", "sich", "so", "be\u00b7hag\u00b7lich", "da\u00b7heim", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PPER", "APPR", "PPER", "PRF", "ADV", "ADJD", "PTKVZ", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Solches zeigte die Muse dem Seher, dem in des Bildes", "tokens": ["Sol\u00b7ches", "zeig\u00b7te", "die", "Mu\u00b7se", "dem", "Se\u00b7her", ",", "dem", "in", "des", "Bil\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Z\u00fcgen die liebe Gestalt seiner Elisa erschien.", "tokens": ["Z\u00fc\u00b7gen", "die", "lie\u00b7be", "Ge\u00b7stalt", "sei\u00b7ner", "E\u00b7li\u00b7sa", "er\u00b7schien", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PPOSAT", "NE", "VVFIN", "$."], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}}}}}