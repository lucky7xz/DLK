{"textgrid.poem.34939": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Flogest aus nach Sonn' und Gl\u00fcck,", "tokens": ["Flo\u00b7gest", "aus", "nach", "Sonn'", "und", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nackt und schlecht kommst du zur\u00fcck.", "tokens": ["Nackt", "und", "schlecht", "kommst", "du", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deutsche Treue, deutsche Hemde,", "tokens": ["Deut\u00b7sche", "Treu\u00b7e", ",", "deut\u00b7sche", "Hem\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die verschlei\u00dft man in der Fremde.", "tokens": ["Die", "ver\u00b7schlei\u00dft", "man", "in", "der", "Frem\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Siehst sehr sterbebl\u00e4\u00dflich aus,", "tokens": ["Siehst", "sehr", "ster\u00b7be\u00b7bl\u00e4\u00df\u00b7lich", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch getrost, du bist zu Haus.", "tokens": ["Doch", "ge\u00b7trost", ",", "du", "bist", "zu", "Haus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "$,", "PPER", "VAFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Warm wie an dem Flackerherde", "tokens": ["Warm", "wie", "an", "dem", "Fla\u00b7cker\u00b7her\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liegt man in der deutschen Erde.", "tokens": ["Liegt", "man", "in", "der", "deut\u00b7schen", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mancher leider wurde lahm", "tokens": ["Man\u00b7cher", "lei\u00b7der", "wur\u00b7de", "lahm"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "ADV", "VAFIN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und nicht mehr nach Hause kam \u2013", "tokens": ["Und", "nicht", "mehr", "nach", "Hau\u00b7se", "kam", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Streckt verlangend aus die Arme,", "tokens": ["Streckt", "ver\u00b7lan\u00b7gend", "aus", "die", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df der Herr sich sein erbarme!", "tokens": ["Da\u00df", "der", "Herr", "sich", "sein", "er\u00b7bar\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "PPOSAT", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}