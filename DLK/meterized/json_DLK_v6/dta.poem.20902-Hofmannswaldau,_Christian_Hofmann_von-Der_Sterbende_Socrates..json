{"dta.poem.20902": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df wann des Todes Grimm uns trennet/", "tokens": ["Da\u00df", "wann", "des", "To\u00b7des", "Grimm", "uns", "tren\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "ART", "NN", "NE", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So greifft er alles dieses an/", "tokens": ["So", "greifft", "er", "al\u00b7les", "die\u00b7ses", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "PDAT", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was er an uns bestreiten kan/", "tokens": ["Was", "er", "an", "uns", "be\u00b7strei\u00b7ten", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "VVINF", "VMFIN", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Weil ers vor seinem Raub erkennet.", "tokens": ["Weil", "ers", "vor", "sei\u00b7nem", "Raub", "er\u00b7ken\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch kommet unser rechtes Hab/", "tokens": ["Doch", "kom\u00b7met", "un\u00b7ser", "rech\u00b7tes", "Hab", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was nicht zuf\u00e4lt und feste stehet/", "tokens": ["Was", "nicht", "zu\u00b7f\u00e4lt", "und", "fes\u00b7te", "ste\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "VVFIN", "KON", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das nimmer stirbt und nicht vergehet/", "tokens": ["Das", "nim\u00b7mer", "stirbt", "und", "nicht", "ver\u00b7ge\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "KON", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit nichten in das kalte Grab/", "tokens": ["Mit", "nich\u00b7ten", "in", "das", "kal\u00b7te", "Grab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und unser Geist dem Leib entnommen", "tokens": ["Und", "un\u00b7ser", "Geist", "dem", "Leib", "ent\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wird zu den Himmelsgeistern kommen.", "tokens": ["Wird", "zu", "den", "Him\u00b7mels\u00b7geis\u00b7tern", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}