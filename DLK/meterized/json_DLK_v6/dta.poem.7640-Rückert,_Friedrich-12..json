{"dta.poem.7640": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du f\u00fchlst dich \u00fcberall im Mittelpunkt der Welt,", "tokens": ["Du", "f\u00fchlst", "dich", "\u00fc\u00b7be\u00b7rall", "im", "Mit\u00b7tel\u00b7punkt", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo in dein Auge Sonn- und Mond- und Sternlicht f\u00e4llt.", "tokens": ["Wo", "in", "dein", "Au\u00b7ge", "Sonn", "und", "Mon\u00b7d", "und", "Stern\u00b7licht", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPOSAT", "NN", "TRUNC", "KON", "TRUNC", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Kein Unterschied ist, ob du h\u00f6her oder tiefer", "tokens": ["Kein", "Un\u00b7ter\u00b7schied", "ist", ",", "ob", "du", "h\u00f6\u00b7her", "o\u00b7der", "tie\u00b7fer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "$,", "KOUS", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gen\u00fcber ihnen stehst, gerader oder schiefer.", "tokens": ["Ge\u00b7n\u00fc\u00b7ber", "ih\u00b7nen", "stehst", ",", "ge\u00b7ra\u00b7der", "o\u00b7der", "schie\u00b7fer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie Standes Abst\u00e4nd' auch hier auseinander wichen,", "tokens": ["Wie", "Stan\u00b7des", "Ab\u00b7st\u00e4nd'", "auch", "hier", "aus\u00b7ein\u00b7an\u00b7der", "wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor der Unendlichkeit dort sind sie ausgeglichen.", "tokens": ["Vor", "der", "Un\u00b7end\u00b7lich\u00b7keit", "dort", "sind", "sie", "aus\u00b7ge\u00b7gli\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VAFIN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}