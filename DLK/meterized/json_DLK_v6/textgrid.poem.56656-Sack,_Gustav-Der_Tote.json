{"textgrid.poem.56656": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der Tote", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da, hinten, in der Heide, wo der Westwind st\u00f6\u00dft,", "tokens": ["Da", ",", "hin\u00b7ten", ",", "in", "der", "Hei\u00b7de", ",", "wo", "der", "West\u00b7wind", "st\u00f6\u00dft", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "APPR", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "hat seine Stunde geschlagen;", "tokens": ["hat", "sei\u00b7ne", "Stun\u00b7de", "ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "da hat sich der Narre die Adern gel\u00f6st", "tokens": ["da", "hat", "sich", "der", "Nar\u00b7re", "die", "A\u00b7dern", "ge\u00b7l\u00f6st"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PRF", "ART", "NN", "ART", "NN", "VVPP"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "und sich zu Grabe getragen.", "tokens": ["und", "sich", "zu", "Gra\u00b7be", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Bald kreisten die Raben rabenschwarz und dicht", "tokens": ["Bald", "kreis\u00b7ten", "die", "Ra\u00b7ben", "ra\u00b7ben\u00b7schwarz", "und", "dicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "\u00fcber dem armen Kadaver,", "tokens": ["\u00fc\u00b7ber", "dem", "ar\u00b7men", "Ka\u00b7da\u00b7ver", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "auf seine hungernden D\u00e4rme erpicht", "tokens": ["auf", "sei\u00b7ne", "hun\u00b7gern\u00b7den", "D\u00e4r\u00b7me", "er\u00b7picht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "hielten sie laut ihr kr\u00e4chzend Palaver.", "tokens": ["hiel\u00b7ten", "sie", "laut", "ihr", "kr\u00e4ch\u00b7zend", "Pa\u00b7la\u00b7ver", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "ADJD", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Dann nagte der Fuchs in windiger Nacht", "tokens": ["Dann", "nag\u00b7te", "der", "Fuchs", "in", "win\u00b7di\u00b7ger", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NE", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "seine steifgefrorenen Glieder,", "tokens": ["sei\u00b7ne", "steif\u00b7ge\u00b7fro\u00b7re\u00b7nen", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "und als der ", "tokens": ["und", "als", "der"], "token_info": ["word", "word", "word"], "pos": ["KON", "KOUS", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "tanzten die Schmei\u00dfen nieder.", "tokens": ["tanz\u00b7ten", "die", "Schmei\u00b7\u00dfen", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Im Herbste aber gl\u00e4nzten blank", "tokens": ["Im", "Herbs\u00b7te", "a\u00b7ber", "gl\u00e4nz\u00b7ten", "blank"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "seine Knochen wie Kreide und Seide,", "tokens": ["sei\u00b7ne", "Kno\u00b7chen", "wie", "Krei\u00b7de", "und", "Sei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KOKOM", "NN", "KON", "NE", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "und klagend stie\u00df seinen Regengesang", "tokens": ["und", "kla\u00b7gend", "stie\u00df", "sei\u00b7nen", "Re\u00b7gen\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "der Westwind \u00fcber die Heide.", "tokens": ["der", "West\u00b7wind", "\u00fc\u00b7ber", "die", "Hei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}