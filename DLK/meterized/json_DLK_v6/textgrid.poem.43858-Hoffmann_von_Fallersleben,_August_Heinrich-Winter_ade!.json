{"textgrid.poem.43858": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Winter ade!", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So h\u00f6rt doch, was die Lerche singt!", "tokens": ["So", "h\u00f6rt", "doch", ",", "was", "die", "Ler\u00b7che", "singt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00f6rt, wie sie frohe Botschaft bringt!", "tokens": ["H\u00f6rt", ",", "wie", "sie", "fro\u00b7he", "Bot\u00b7schaft", "bringt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es kommt auf goldnem Sonnenstrahl", "tokens": ["Es", "kommt", "auf", "gold\u00b7nem", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Fr\u00fchling heim in unser Thal,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "heim", "in", "un\u00b7ser", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er streuet bunte Blumen aus", "tokens": ["Er", "streu\u00b7et", "bun\u00b7te", "Blu\u00b7men", "aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und bringet Freud' in jedes Haus.", "tokens": ["Und", "brin\u00b7get", "Freud'", "in", "je\u00b7des", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Winter, ade!", "tokens": ["Win\u00b7ter", ",", "a\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADV", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Fr\u00fchling, juchhe!", "tokens": ["Fr\u00fch\u00b7ling", ",", "juch\u00b7he", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Was uns die liebe Lerche singt,", "tokens": ["Was", "uns", "die", "lie\u00b7be", "Ler\u00b7che", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In unsern Herzen wiederklingt.", "tokens": ["In", "un\u00b7sern", "Her\u00b7zen", "wie\u00b7der\u00b7klingt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Winter sagt ade! ade!", "tokens": ["Der", "Win\u00b7ter", "sagt", "a\u00b7de", "!", "a\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$.", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und hin ist K\u00e4lte, Reif und Schnee", "tokens": ["Und", "hin", "ist", "K\u00e4l\u00b7te", ",", "Reif", "und", "Schnee"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Nebel hin und Dunkelheit \u2013", "tokens": ["Und", "Ne\u00b7bel", "hin", "und", "Dun\u00b7kel\u00b7heit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKVZ", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Willkommen, s\u00fc\u00dfe Fr\u00fchlingszeit!", "tokens": ["Will\u00b7kom\u00b7men", ",", "s\u00fc\u00b7\u00dfe", "Fr\u00fch\u00b7lings\u00b7zeit", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Winter, ade!", "tokens": ["Win\u00b7ter", ",", "a\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADV", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Fr\u00fchling, juchhe!", "tokens": ["Fr\u00fch\u00b7ling", ",", "juch\u00b7he", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}