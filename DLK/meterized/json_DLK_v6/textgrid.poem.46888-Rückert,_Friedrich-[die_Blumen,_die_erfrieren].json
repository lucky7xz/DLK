{"textgrid.poem.46888": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[die Blumen, die erfrieren]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Blumen, die erfrieren,", "tokens": ["Die", "Blu\u00b7men", ",", "die", "er\u00b7frie\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erneuern sch\u00f6ner sich.", "tokens": ["Er\u00b7neu\u00b7ern", "sch\u00f6\u00b7ner", "sich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So mu\u00dft' ich dich verlieren,", "tokens": ["So", "mu\u00dft'", "ich", "dich", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Um zu gewinnen dich.", "tokens": ["Um", "zu", "ge\u00b7win\u00b7nen", "dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKZU", "VVINF", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die du, wie sch\u00f6n du warest,", "tokens": ["Die", "du", ",", "wie", "sch\u00f6n", "du", "wa\u00b7rest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun erst mir offenbarest,", "tokens": ["Nun", "erst", "mir", "of\u00b7fen\u00b7ba\u00b7rest", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Herl\u00e4chelnd \u00fcber mich", "tokens": ["Her\u00b7l\u00e4\u00b7chelnd", "\u00fc\u00b7ber", "mich"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Aus sch\u00f6neren Revieren!", "tokens": ["Aus", "sch\u00f6\u00b7ne\u00b7ren", "Re\u00b7vie\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So mu\u00dft' ich dich verlieren,", "tokens": ["So", "mu\u00dft'", "ich", "dich", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Um zu gewinnen dich.", "tokens": ["Um", "zu", "ge\u00b7win\u00b7nen", "dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKZU", "VVINF", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nie sahen so im Wachen", "tokens": ["Nie", "sa\u00b7hen", "so", "im", "Wa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Augen dich, die schwachen,", "tokens": ["Die", "Au\u00b7gen", "dich", ",", "die", "schwa\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie du nun minniglich", "tokens": ["Wie", "du", "nun", "min\u00b7nig\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Kommst meinen Traum zu zieren.", "tokens": ["Kommst", "mei\u00b7nen", "Traum", "zu", "zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So mu\u00dft' ich dich verlieren,", "tokens": ["So", "mu\u00dft'", "ich", "dich", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Um zu gewinnen dich.", "tokens": ["Um", "zu", "ge\u00b7win\u00b7nen", "dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKZU", "VVINF", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Oft hat von mir hienieden", "tokens": ["Oft", "hat", "von", "mir", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dich eine Wand geschieden;", "tokens": ["Dich", "ei\u00b7ne", "Wand", "ge\u00b7schie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun scheiden dich und mich", "tokens": ["Nun", "schei\u00b7den", "dich", "und", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Durchsichtige Saphiren;", "tokens": ["Durch\u00b7sich\u00b7ti\u00b7ge", "Sa\u00b7phi\u00b7ren", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So mu\u00dft' ich dich verlieren,", "tokens": ["So", "mu\u00dft'", "ich", "dich", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Um zu gewinnen dich.", "tokens": ["Um", "zu", "ge\u00b7win\u00b7nen", "dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKZU", "VVINF", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich h\u00f6r' in einer Stunde", "tokens": ["Ich", "h\u00f6r'", "in", "ei\u00b7ner", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus deinem s\u00fc\u00dfen Munde", "tokens": ["Aus", "dei\u00b7nem", "s\u00fc\u00b7\u00dfen", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mehr Worte nun, als ich", "tokens": ["Mehr", "Wor\u00b7te", "nun", ",", "als", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "ADV", "$,", "KOUS", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In Jahren h\u00f6rt', in vieren.", "tokens": ["In", "Jah\u00b7ren", "h\u00f6rt'", ",", "in", "vie\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "APPR", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So mu\u00dft' ich dich verlieren,", "tokens": ["So", "mu\u00dft'", "ich", "dich", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Um zu gewinnen dich.", "tokens": ["Um", "zu", "ge\u00b7win\u00b7nen", "dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKZU", "VVINF", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Erfrorne Blum' im Garten,", "tokens": ["Er\u00b7fror\u00b7ne", "Blum'", "im", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "La\u00df dich im Herzen warten,", "tokens": ["La\u00df", "dich", "im", "Her\u00b7zen", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und bl\u00fch' hier ewiglich,", "tokens": ["Und", "bl\u00fch'", "hier", "e\u00b7wig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Um nie mehr zu erfrieren!", "tokens": ["Um", "nie", "mehr", "zu", "er\u00b7frie\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So mu\u00dft' ich dich verlieren,", "tokens": ["So", "mu\u00dft'", "ich", "dich", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Um zu gewinnen dich.", "tokens": ["Um", "zu", "ge\u00b7win\u00b7nen", "dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKZU", "VVINF", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}