{"textgrid.poem.58077": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "115. Man soll noch den Sabbat feiern", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Sabbatruh, bei M\u00fc\u00dfiggehn,", "tokens": ["In", "Sab\u00b7ba\u00b7truh", ",", "bei", "M\u00fc\u00b7\u00dfig\u00b7gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wirst du gar viel gewinnen;", "tokens": ["Wirst", "du", "gar", "viel", "ge\u00b7win\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df eignes Wirken stille stehn", "tokens": ["La\u00df", "eig\u00b7nes", "Wir\u00b7ken", "stil\u00b7le", "stehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADJA", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und leide Gott von innen!", "tokens": ["Und", "lei\u00b7de", "Gott", "von", "in\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Still mit Gedanken, Willen, Lust,", "tokens": ["Still", "mit", "Ge\u00b7dan\u00b7ken", ",", "Wil\u00b7len", ",", "Lust", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Wirk auch nicht viel mit Kopf und Brust,", "tokens": ["Wirk", "auch", "nicht", "viel", "mit", "Kopf", "und", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKNEG", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "La\u00df Zeit und Ort und alles dar", "tokens": ["La\u00df", "Zeit", "und", "Ort", "und", "al\u00b7les", "dar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "NN", "KON", "NN", "KON", "PIS", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und auch dich selbst in Wahrheit,", "tokens": ["Und", "auch", "dich", "selbst", "in", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Verlier dich still ins Dunkle gar,", "tokens": ["Ver\u00b7lier", "dich", "still", "ins", "Dunk\u00b7le", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Da wohnet Gottes Klarheit!", "tokens": ["Da", "woh\u00b7net", "Got\u00b7tes", "Klar\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Kannst du ", "tokens": ["Kannst", "du"], "token_info": ["word", "word"], "pos": ["VMFIN", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.12": {"text": "Was will der H\u00f6lle Macht dir tun?", "tokens": ["Was", "will", "der", "H\u00f6l\u00b7le", "Macht", "dir", "tun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}