{"dta.poem.3952": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "XiI.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Hochwerthester Patron, ein Blat von Demuths-", "tokens": ["Hoch\u00b7wert\u00b7hes\u00b7ter", "Pat\u00b7ron", ",", "ein", "Blat", "von", "De\u00b7muths"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "APPR", "TRUNC"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Fr\u00fcchten,", "tokens": ["Fr\u00fcch\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Die\u00df suchet dich voritzt, in Ehrfurcht zu berichten,", "tokens": ["Die\u00df", "su\u00b7chet", "dich", "vo\u00b7ritzt", ",", "in", "Ehr\u00b7furcht", "zu", "be\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVPP", "$,", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df ich noch ohne Dienst und zwar h\u00f6chst ", "tokens": ["Da\u00df", "ich", "noch", "oh\u00b7ne", "Dienst", "und", "zwar", "h\u00f6chst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "KON", "ADV", "ADV"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.5": {"text": "bin,", "tokens": ["bin", ","], "token_info": ["word", "punct"], "pos": ["VAFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Dich fleht die\u00df schlechte Blat aus Wehmuths-vol-", "tokens": ["Dich", "fleht", "die\u00df", "schlech\u00b7te", "Blat", "aus", "Weh\u00b7muths\u00b7vol"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDS", "ADJA", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "len Sinn,", "tokens": ["len", "Sinn", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Lie\u00df solchen meinen Brief, wirst du das Siegel", "tokens": ["Lie\u00df", "sol\u00b7chen", "mei\u00b7nen", "Brief", ",", "wirst", "du", "das", "Sie\u00b7gel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "PPOSAT", "NN", "$,", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "brechen,", "tokens": ["bre\u00b7chen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "So wird dein Mitleid mir Gunst und Erh\u00f6rung", "tokens": ["So", "wird", "dein", "Mit\u00b7leid", "mir", "Gunst", "und", "Er\u00b7h\u00f6\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PPER", "NN", "KON", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "sprechen.", "tokens": ["spre\u00b7chen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}