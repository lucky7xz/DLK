{"textgrid.poem.54938": {"metadata": {"author": {"name": "Scheerbart, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach ja! Jetzt wei\u00df ich's ganz genau!", "genre": "verse", "period": "N.A.", "pub_year": 1889, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach ja! Jetzt wei\u00df ich's ganz genau!", "tokens": ["Ach", "ja", "!", "Jetzt", "wei\u00df", "ich's", "ganz", "ge\u00b7nau", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "ADV", "VVFIN", "PIS", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Max und Moritz kam ich her!", "tokens": ["Von", "Max", "und", "Mo\u00b7ritz", "kam", "ich", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NE", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die lagen in einem Syrupmeer", "tokens": ["Die", "la\u00b7gen", "in", "ei\u00b7nem", "Sy\u00b7ru\u00b7pmeer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und waren bl\u00f6de wie der gro\u00dfe Stier.", "tokens": ["Und", "wa\u00b7ren", "bl\u00f6\u00b7de", "wie", "der", "gro\u00b7\u00dfe", "Stier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJA", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es kam ein Strahl durch das Revier", "tokens": ["Es", "kam", "ein", "Strahl", "durch", "das", "Re\u00b7vier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und h\u00fcpfte mit uns Dreien.", "tokens": ["Und", "h\u00fcpf\u00b7te", "mit", "uns", "Drei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "CARD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das sollte uns bald entzweien.", "tokens": ["Das", "soll\u00b7te", "uns", "bald", "ent\u00b7zwei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Nach jenem Trubel durft ich endlich", "tokens": ["Nach", "je\u00b7nem", "Tru\u00b7bel", "durft", "ich", "end\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "So selig ruhen auf dem Zuckersterne,", "tokens": ["So", "se\u00b7lig", "ru\u00b7hen", "auf", "dem", "Zu\u00b7ckers\u00b7ter\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der mir aus allen seinen Kratern", "tokens": ["Der", "mir", "aus", "al\u00b7len", "sei\u00b7nen", "Kra\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ein gl\u00fcckliches Vergessen dampfte.", "tokens": ["Ein", "gl\u00fcck\u00b7li\u00b7ches", "Ver\u00b7ges\u00b7sen", "dampf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}