{"textgrid.poem.25132": {"metadata": {"author": {"name": "Keats, John", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du Zeit der Feuchte und der Fruchtbarkeit,", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Zeit der Feuchte und der Fruchtbarkeit,", "tokens": ["Du", "Zeit", "der", "Feuch\u00b7te", "und", "der", "Frucht\u00b7bar\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Freundin des Sonnengotts, der Reife sendet,", "tokens": ["Freun\u00b7din", "des", "Son\u00b7nen\u00b7gotts", ",", "der", "Rei\u00b7fe", "sen\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Mit ihm vereinigt, da\u00df zur S\u00fc\u00dfigkeit", "tokens": ["Mit", "ihm", "ver\u00b7ei\u00b7nigt", ",", "da\u00df", "zur", "S\u00fc\u00b7\u00dfig\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "VVPP", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Rankenweins betaute Traube endet,", "tokens": ["Des", "Ran\u00b7ken\u00b7weins", "be\u00b7tau\u00b7te", "Trau\u00b7be", "en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df Apfellast die moosigen B\u00e4ume biegt,", "tokens": ["Da\u00df", "Ap\u00b7fel\u00b7last", "die", "moo\u00b7si\u00b7gen", "B\u00e4u\u00b7me", "biegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Da\u00df aller Fr\u00fcchte Herz von Saft durchquollen,", "tokens": ["Da\u00df", "al\u00b7ler", "Fr\u00fcch\u00b7te", "Herz", "von", "Saft", "durch\u00b7quol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df K\u00fcrbis schwillt und jede Nu\u00df sich f\u00fcllt", "tokens": ["Da\u00df", "K\u00fcr\u00b7bis", "schwillt", "und", "je\u00b7de", "Nu\u00df", "sich", "f\u00fcllt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVFIN", "KON", "PIAT", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mit w\u00fcrzigem Kern, und weicher gelber Pollen", "tokens": ["Mit", "w\u00fcr\u00b7zi\u00b7gem", "Kern", ",", "und", "wei\u00b7cher", "gel\u00b7ber", "Pol\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "ADJA", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "In vielen sp\u00e4ten Blumen wartend liegt,", "tokens": ["In", "vie\u00b7len", "sp\u00e4\u00b7ten", "Blu\u00b7men", "war\u00b7tend", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und jede Biene schwer zur Zelle fliegt,", "tokens": ["Und", "je\u00b7de", "Bie\u00b7ne", "schwer", "zur", "Zel\u00b7le", "fliegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Draus Sommers Segen sch\u00e4umend \u00fcberquillt.", "tokens": ["Draus", "Som\u00b7mers", "Se\u00b7gen", "sch\u00e4u\u00b7mend", "\u00fc\u00b7berq\u00b7uillt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wer sah nicht oft in deiner Pracht dich stehn?", "tokens": ["Wer", "sah", "nicht", "oft", "in", "dei\u00b7ner", "Pracht", "dich", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sucht einer drau\u00dfen, mag er wohl dich finden", "tokens": ["Sucht", "ei\u00b7ner", "drau\u00b7\u00dfen", ",", "mag", "er", "wohl", "dich", "fin\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADV", "$,", "VMFIN", "PPER", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit L\u00e4cheln \u00fcber weite Speicher gehn,", "tokens": ["Mit", "L\u00e4\u00b7cheln", "\u00fc\u00b7ber", "wei\u00b7te", "Spei\u00b7cher", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Haare sanft bewegt von F\u00e4chelwinden,", "tokens": ["Die", "Haa\u00b7re", "sanft", "be\u00b7wegt", "von", "F\u00e4\u00b7chel\u00b7win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Oder auf halbgem\u00e4htem Ackerreich.", "tokens": ["O\u00b7der", "auf", "halb\u00b7ge\u00b7m\u00e4h\u00b7tem", "A\u00b7cker\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Im Mohnduft schlafen: vor den n\u00e4chsten Schwaden", "tokens": ["Im", "Mohn\u00b7duft", "schla\u00b7fen", ":", "vor", "den", "n\u00e4chs\u00b7ten", "Schwa\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVINF", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Voll Blumen h\u00e4lt die Sense noch zur\u00fcck.", "tokens": ["Voll", "Blu\u00b7men", "h\u00e4lt", "die", "Sen\u00b7se", "noch", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und manchmal gehst du, \u00c4hrenlesern gleich,", "tokens": ["Und", "manch\u00b7mal", "gehst", "du", ",", "\u00c4h\u00b7ren\u00b7le\u00b7sern", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Quer \u00fcbern Bach, den hohen Kopf beladen,", "tokens": ["Quer", "\u00fc\u00b7bern", "Bach", ",", "den", "ho\u00b7hen", "Kopf", "be\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Oder du l\u00e4\u00dft den ernsten H\u00fcterblick", "tokens": ["O\u00b7der", "du", "l\u00e4\u00dft", "den", "erns\u00b7ten", "H\u00fc\u00b7ter\u00b7blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Im gelben Flu\u00df der Obstweinkelter baden.", "tokens": ["Im", "gel\u00b7ben", "Flu\u00df", "der", "Obst\u00b7wein\u00b7kel\u00b7ter", "ba\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wo ist, ach wo, des Fr\u00fchlings Finkenschlag?", "tokens": ["Wo", "ist", ",", "ach", "wo", ",", "des", "Fr\u00fch\u00b7lings", "Fin\u00b7ken\u00b7schlag", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "$,", "XY", "PWAV", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O still! Musik \u2013 auch dir ist sie verliehen \u2013", "tokens": ["O", "still", "!", "Mu\u00b7sik", "\u2013", "auch", "dir", "ist", "sie", "ver\u00b7lie\u00b7hen", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "NN", "$(", "ADV", "PPER", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn wolkenbunt verbl\u00fcht der sanfte Tag", "tokens": ["Wenn", "wol\u00b7ken\u00b7bunt", "ver\u00b7bl\u00fcht", "der", "sanf\u00b7te", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Rosenschatten \u00fcber Stoppeln ziehen:", "tokens": ["Und", "Ro\u00b7sen\u00b7schat\u00b7ten", "\u00fc\u00b7ber", "Stop\u00b7peln", "zie\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dann klagt in Uferweiden das Gewimmel", "tokens": ["Dann", "klagt", "in", "U\u00b7fer\u00b7wei\u00b7den", "das", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der winzigen M\u00fccken \u2013 lebt der Wind empor,", "tokens": ["Der", "win\u00b7zi\u00b7gen", "M\u00fc\u00b7cken", "\u2013", "lebt", "der", "Wind", "em\u00b7por", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Hebt sich der Schleier, stirbt er, sinkt der Flor \u2013", "tokens": ["Hebt", "sich", "der", "Schlei\u00b7er", ",", "stirbt", "er", ",", "sinkt", "der", "Flor", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Erwachsne L\u00e4mmer bl\u00f6ken laut am Bach,", "tokens": ["Er\u00b7wachs\u00b7ne", "L\u00e4m\u00b7mer", "bl\u00f6\u00b7ken", "laut", "am", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und Grillen zirpen; nun entz\u00fcckt das Ohr,", "tokens": ["Und", "Gril\u00b7len", "zir\u00b7pen", ";", "nun", "ent\u00b7z\u00fcckt", "das", "Ohr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$.", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Rotbr\u00fcstchens Fl\u00f6tensang vom Laubendach,", "tokens": ["Rot\u00b7br\u00fcst\u00b7chens", "Fl\u00f6\u00b7ten\u00b7sang", "vom", "Lau\u00b7ben\u00b7dach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.11": {"text": "Und Schwalben sammeln zwitschernd sich im Himmel.", "tokens": ["Und", "Schwal\u00b7ben", "sam\u00b7meln", "zwit\u00b7schernd", "sich", "im", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}