{"textgrid.poem.52722": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Neuner", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohlan, wohlauf du Sch\u00fctze,", "tokens": ["Wo\u00b7hlan", ",", "wohl\u00b7auf", "du", "Sch\u00fct\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Komm mit uns auf den Wall,", "tokens": ["Komm", "mit", "uns", "auf", "den", "Wall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "ART", "NE", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "La\u00df leuchten deine Blitze,", "tokens": ["La\u00df", "leuch\u00b7ten", "dei\u00b7ne", "Blit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "La\u00df klingen B\u00fcchsenknall!", "tokens": ["La\u00df", "klin\u00b7gen", "B\u00fcch\u00b7sen\u00b7knall", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Verschm\u00e4he nicht die St\u00fctze,", "tokens": ["Ver\u00b7schm\u00e4\u00b7he", "nicht", "die", "St\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Denn sichrer trifft der Ball.", "tokens": ["Denn", "sich\u00b7rer", "trifft", "der", "Ball", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Neun Jahre nun gesessen,", "tokens": ["Neun", "Jah\u00b7re", "nun", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bist du im Thorthurm hier;", "tokens": ["Bist", "du", "im", "Thor\u00b7thurm", "hier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie du dich hast vermessen,", "tokens": ["Wie", "du", "dich", "hast", "ver\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erschie\u00dfe Freiheit dir!", "tokens": ["Er\u00b7schie\u00b7\u00dfe", "Frei\u00b7heit", "dir", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Frevelthat vergessen,", "tokens": ["Die", "Fre\u00b7vel\u00b7that", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verzeihen wollen wir.", "tokens": ["Ver\u00b7zei\u00b7hen", "wol\u00b7len", "wir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wir wollen dich erproben,", "tokens": ["Wir", "wol\u00b7len", "dich", "er\u00b7pro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verwegner, blick empor!", "tokens": ["Ver\u00b7weg\u00b7ner", ",", "blick", "em\u00b7por", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Wetterfahne droben", "tokens": ["Die", "Wet\u00b7ter\u00b7fah\u00b7ne", "dro\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf deinem finstern Thor,", "tokens": ["Auf", "dei\u00b7nem", "fins\u00b7tern", "Thor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie sei zum Ziel erhoben", "tokens": ["Sie", "sei", "zum", "Ziel", "er\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fcr dein gepr\u00fcftes Rohr!", "tokens": ["F\u00fcr", "dein", "ge\u00b7pr\u00fcf\u00b7tes", "Rohr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Verewigen sollst du drinnen", "tokens": ["Ve\u00b7re\u00b7wi\u00b7gen", "sollst", "du", "drin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Jahrzahl deiner Haft;", "tokens": ["Die", "Jahr\u00b7zahl", "dei\u00b7ner", "Haft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch magst du dich besinnen,", "tokens": ["Doch", "magst", "du", "dich", "be\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Versaget dir die Kraft,", "tokens": ["Ver\u00b7sa\u00b7get", "dir", "die", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wird dieses dein Beginnen", "tokens": ["Wird", "die\u00b7ses", "dein", "Be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PDAT", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit Handabhaun bestraft!", "tokens": ["Mit", "Hand\u00b7ab\u00b7haun", "be\u00b7straft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Zehn Sch\u00fcsse hast du, Einer", "tokens": ["Zehn", "Sch\u00fcs\u00b7se", "hast", "du", ",", "Ei\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["CARD", "NN", "VAFIN", "PPER", "$,", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nur darf ins Blaue gehn,", "tokens": ["Nur", "darf", "ins", "Blau\u00b7e", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir wollen einen Neuner", "tokens": ["Wir", "wol\u00b7len", "ei\u00b7nen", "Neu\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und auch neun L\u00f6cher sehn.", "tokens": ["Und", "auch", "neun", "L\u00f6\u00b7cher", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nimm dich in Acht, Zigeuner,", "tokens": ["Nimm", "dich", "in", "Acht", ",", "Zi\u00b7geu\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "CARD", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Leicht kann der Wind sich drehn!", "tokens": ["Leicht", "kann", "der", "Wind", "sich", "drehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "ART", "NN", "PRF", "VVINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Lang konnt er nicht erblicken", "tokens": ["Lang", "konnt", "er", "nicht", "er\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der B\u00fcchse trauten Glanz,", "tokens": ["Der", "B\u00fcch\u00b7se", "trau\u00b7ten", "Glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum ist er von Entz\u00fccken", "tokens": ["Drum", "ist", "er", "von", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Freud durchdrungen ganz.", "tokens": ["Und", "Freud", "durch\u00b7drun\u00b7gen", "ganz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbihr Herrn, soll es mi\u00dfgl\u00fccken,", "tokens": ["\u00bb", "ihr", "Herrn", ",", "soll", "es", "mi\u00df\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mein Leben in die Schanz!\u00ab", "tokens": ["Mein", "Le\u00b7ben", "in", "die", "Schanz", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Mit unbeholfnem Knixe", "tokens": ["Mit", "un\u00b7be\u00b7holf\u00b7nem", "Kni\u00b7xe"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verhehlt er seine Wuth,", "tokens": ["Ver\u00b7hehlt", "er", "sei\u00b7ne", "Wuth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann murmelt er: die F\u00fcchse", "tokens": ["Dann", "mur\u00b7melt", "er", ":", "die", "F\u00fcch\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sind stets auf ihrer Hut.", "tokens": ["Sind", "stets", "auf", "ih\u00b7rer", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nimmt seine Doppelb\u00fcchse,", "tokens": ["Nimmt", "sei\u00b7ne", "Dop\u00b7pel\u00b7b\u00fcch\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und zielet fest und gut.", "tokens": ["Und", "zie\u00b7let", "fest", "und", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wie jauchzt der Menge Schreien!", "tokens": ["Wie", "jauchzt", "der", "Men\u00b7ge", "Schrei\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wer h\u00e4tte das gedacht!", "tokens": ["Wer", "h\u00e4t\u00b7te", "das", "ge\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der neunte Schu\u00df des treuen", "tokens": ["Der", "neun\u00b7te", "Schu\u00df", "des", "treu\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gewehres hats vollbracht.", "tokens": ["Ge\u00b7weh\u00b7res", "hats", "voll\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Man dr\u00fcckt die Hand dem Freien,", "tokens": ["Man", "dr\u00fcckt", "die", "Hand", "dem", "Frei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der steigt herab und lacht.", "tokens": ["Der", "steigt", "her\u00b7ab", "und", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbihr Herrn, Hochehrenwerthe,", "tokens": ["\u00bb", "ihr", "Herrn", ",", "Ho\u00b7ch\u00b7eh\u00b7ren\u00b7wert\u00b7he", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was kam euch in den Sinn?", "tokens": ["Was", "kam", "euch", "in", "den", "Sinn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie wu\u00dftet Ihr, Verehrte,", "tokens": ["Wie", "wu\u00df\u00b7tet", "Ihr", ",", "Ver\u00b7ehr\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ich so ehrlich bin?", "tokens": ["Da\u00df", "ich", "so", "ehr\u00b7lich", "bin", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df sich mein Gl\u00fcck bew\u00e4hrte,", "tokens": ["Da\u00df", "sich", "mein", "Gl\u00fcck", "be\u00b7w\u00e4hr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fcrwahr, ist euch Gewinn!", "tokens": ["F\u00fcr\u00b7wahr", ",", "ist", "euch", "Ge\u00b7winn", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Seht, noch ein Schu\u00df ist drinnen!", "tokens": ["Seht", ",", "noch", "ein", "Schu\u00df", "ist", "drin\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nein, werdet mir nicht bla\u00df!", "tokens": ["Nein", ",", "wer\u00b7det", "mir", "nicht", "bla\u00df", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VAFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie sollt ich mich besinnen?", "tokens": ["Wie", "sollt", "ich", "mich", "be\u00b7sin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u2013 Ich rede jetzt im Spa\u00df \u2013", "tokens": ["\u2013", "Ich", "re\u00b7de", "jetzt", "im", "Spa\u00df", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Seht, dort auch, zum Entrinnen,", "tokens": ["Seht", ",", "dort", "auch", ",", "zum", "Ent\u00b7rin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "ADV", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Klepper auf der Ga\u00df!", "tokens": ["Die", "Klep\u00b7per", "auf", "der", "Ga\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Auf Euern Schinderrossen \u2013", "tokens": ["Auf", "Eu\u00b7ern", "Schin\u00b7der\u00b7ros\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was meint Ihr zu dem Spa\u00df \u2013", "tokens": ["Was", "meint", "Ihr", "zu", "dem", "Spa\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "War ich bereits entschlossen,", "tokens": ["War", "ich", "be\u00b7reits", "ent\u00b7schlos\u00b7sen", ","], 