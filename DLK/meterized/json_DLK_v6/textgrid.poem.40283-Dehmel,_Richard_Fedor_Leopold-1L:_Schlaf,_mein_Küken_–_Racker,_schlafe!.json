{"textgrid.poem.40283": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schlaf, mein K\u00fcken \u2013 Racker, schlafe!", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlaf, mein K\u00fcken \u2013 Racker, schlafe!", "tokens": ["Schlaf", ",", "mein", "K\u00fc\u00b7ken", "\u2013", "Ra\u00b7cker", ",", "schla\u00b7fe", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$(", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kuck: im Spiegel stehn zwei Schafe,", "tokens": ["Kuck", ":", "im", "Spie\u00b7gel", "stehn", "zwei", "Scha\u00b7fe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPRART", "NN", "VVFIN", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "bl\u00e4kt ein gro\u00dfes, m\u00e4kt ein kleines,", "tokens": ["bl\u00e4kt", "ein", "gro\u00b7\u00dfes", ",", "m\u00e4kt", "ein", "klei\u00b7nes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,", "VVFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und das kleine, das ist meines!", "tokens": ["und", "das", "klei\u00b7ne", ",", "das", "ist", "mei\u00b7nes", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "PDS", "VAFIN", "PPOSAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bengel, Bengel, br\u00fclle nicht,", "tokens": ["Ben\u00b7gel", ",", "Ben\u00b7gel", ",", "br\u00fcl\u00b7le", "nicht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "du verdammter Strampelwicht.", "tokens": ["du", "ver\u00b7damm\u00b7ter", "Stram\u00b7pel\u00b7wicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Still, mein s\u00fc\u00dfes Engelsf\u00fcllen:", "tokens": ["Still", ",", "mein", "s\u00fc\u00b7\u00dfes", "En\u00b7gels\u00b7f\u00fcl\u00b7len", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "morgen schneet es Zuckerpillen,", "tokens": ["mor\u00b7gen", "schneet", "es", "Zu\u00b7cker\u00b7pil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00fcbermorgen blanke Dreier,", "tokens": ["\u00fc\u00b7ber\u00b7mor\u00b7gen", "blan\u00b7ke", "Drei\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "n\u00e4chste Woche goldne Eier,", "tokens": ["n\u00e4chs\u00b7te", "Wo\u00b7che", "gold\u00b7ne", "Ei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "und der liebe Gott, der lacht,", "tokens": ["und", "der", "lie\u00b7be", "Gott", ",", "der", "lacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "da\u00df der ganze Himmel kracht.", "tokens": ["da\u00df", "der", "gan\u00b7ze", "Him\u00b7mel", "kracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und du kommst und nimmst die Spenden,", "tokens": ["Und", "du", "kommst", "und", "nimmst", "die", "Spen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "s\u00e4st sie aus mit Sonntagsh\u00e4nden,", "tokens": ["s\u00e4st", "sie", "aus", "mit", "Sonn\u00b7tags\u00b7h\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und die Erde bl\u00fcht von Farben,", "tokens": ["und", "die", "Er\u00b7de", "bl\u00fcht", "von", "Far\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und die Menschen thun's in Garben", "tokens": ["und", "die", "Men\u00b7schen", "thun's", "in", "Gar\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Herrr, den Bengel k\u00fcmmert nischt,", "tokens": ["Herrr", ",", "den", "Ben\u00b7gel", "k\u00fcm\u00b7mert", "nischt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "was man auch f\u00fcr L\u00fcgen drischt!", "tokens": ["was", "man", "auch", "f\u00fcr", "L\u00fc\u00b7gen", "drischt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Warte nur, du Satansrachen:", "tokens": ["War\u00b7te", "nur", ",", "du", "Sa\u00b7tans\u00b7ra\u00b7chen", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "heute Nacht, du kleiner Drachen,", "tokens": ["heu\u00b7te", "Nacht", ",", "du", "klei\u00b7ner", "Dra\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "durch den roten H\u00f6llenbogen", "tokens": ["durch", "den", "ro\u00b7ten", "H\u00f6l\u00b7len\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "kommt ein Schmetterling geflogen,", "tokens": ["kommt", "ein", "Schmet\u00b7ter\u00b7ling", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "huscht dir auf die Nase, hu,", "tokens": ["huscht", "dir", "auf", "die", "Na\u00b7se", ",", "hu", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,", "XY", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "deckt dir beide Augen zu;", "tokens": ["deckt", "dir", "bei\u00b7de", "Au\u00b7gen", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "deckt die Fl\u00fcgel sacht zusammen,", "tokens": ["deckt", "die", "Fl\u00fc\u00b7gel", "sacht", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df du tr\u00e4umst von stillen Flammen,", "tokens": ["da\u00df", "du", "tr\u00e4umst", "von", "stil\u00b7len", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "von zwei Flammen, die sich fanden,", "tokens": ["von", "zwei", "Flam\u00b7men", ",", "die", "sich", "fan\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,", "PRELS", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6lle Himmel still verbanden \u2013 \u2013", "tokens": ["H\u00f6l\u00b7le", "Him\u00b7mel", "still", "ver\u00b7ban\u00b7den", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NN", "ADJD", "VVPP", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "so, nu schl\u00e4ft er; es gelang;", "tokens": ["so", ",", "nu", "schl\u00e4ft", "er", ";", "es", "ge\u00b7lang", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Himmel H\u00f6lle, Gott sei Dank!", "tokens": ["Him\u00b7mel", "H\u00f6l\u00b7le", ",", "Gott", "sei", "Dank", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}