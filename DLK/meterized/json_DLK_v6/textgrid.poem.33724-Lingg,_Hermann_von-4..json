{"textgrid.poem.33724": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie klar sich auch im See die Sterne spiegeln,", "tokens": ["Wie", "klar", "sich", "auch", "im", "See", "die", "Ster\u00b7ne", "spie\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PRF", "ADV", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du kannst doch nicht in seine Tiefe schauen.", "tokens": ["Du", "kannst", "doch", "nicht", "in", "sei\u00b7ne", "Tie\u00b7fe", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So l\u00e4chelt mancher Blick und heischt Vertrauen", "tokens": ["So", "l\u00e4\u00b7chelt", "man\u00b7cher", "Blick", "und", "heischt", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "KON", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und birgt doch nur ein Buch mit sieben Siegeln.", "tokens": ["Und", "birgt", "doch", "nur", "ein", "Buch", "mit", "sie\u00b7ben", "Sie\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ART", "NN", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Kerkerschlo\u00df ist leichter aufzuriegeln,", "tokens": ["Ein", "Ker\u00b7ker\u00b7schlo\u00df", "ist", "leich\u00b7ter", "auf\u00b7zu\u00b7rie\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als eine Seele, die, gest\u00e4hlt von rauhen", "tokens": ["Als", "ei\u00b7ne", "See\u00b7le", ",", "die", ",", "ge\u00b7st\u00e4hlt", "von", "rau\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "$,", "VVPP", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erfahrungen, nur strebt, an sich zu bauen,", "tokens": ["Er\u00b7fah\u00b7run\u00b7gen", ",", "nur", "strebt", ",", "an", "sich", "zu", "bau\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "$,", "APPR", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+---++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sich l\u00e4uternd wie das Erz in Feuertiegeln.", "tokens": ["Sich", "l\u00e4u\u00b7ternd", "wie", "das", "Erz", "in", "Feu\u00b7er\u00b7tie\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "KOKOM", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Auch ich r\u00fchm' mich, ich lernt' den Wert erkennen", "tokens": ["Auch", "ich", "r\u00fchm'", "mich", ",", "ich", "lernt'", "den", "Wert", "er\u00b7ken\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von jedem L\u00e4cheln, das wir abgewinnen", "tokens": ["Von", "je\u00b7dem", "L\u00e4\u00b7cheln", ",", "das", "wir", "ab\u00b7ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem Ernst der Dinge, die wir \u00bbDasein\u00ab nennen.", "tokens": ["Dem", "Ernst", "der", "Din\u00b7ge", ",", "die", "wir", "\u00bb", "Da\u00b7sein", "\u00ab", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "$(", "ART", "$(", "VVINF", "$."], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Verzeih! D\u00fcnkt dir vielleicht zu tr\u00fcb mein Sinnen?", "tokens": ["Ver\u00b7zeih", "!", "D\u00fcnkt", "dir", "viel\u00b7leicht", "zu", "tr\u00fcb", "mein", "Sin\u00b7nen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Blume, wenn zu hei\u00df die Strahlen brennen,", "tokens": ["Die", "Blu\u00b7me", ",", "wenn", "zu", "hei\u00df", "die", "Strah\u00b7len", "bren\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PTKA", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schlie\u00dft ihre Bl\u00e4tter gerne dann nach innen.", "tokens": ["Schlie\u00dft", "ih\u00b7re", "Bl\u00e4t\u00b7ter", "ger\u00b7ne", "dann", "nach", "in\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADV", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}