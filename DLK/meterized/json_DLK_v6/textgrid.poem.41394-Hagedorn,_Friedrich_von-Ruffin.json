{"textgrid.poem.41394": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Ruffin", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein sch\u00f6ner Herr, der Pflastertreter Krone,", "tokens": ["Ein", "sch\u00f6\u00b7ner", "Herr", ",", "der", "Pflas\u00b7ter\u00b7tre\u00b7ter", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schon um f\u00fcnf Uhr der Oper edle Zier,", "tokens": ["Schon", "um", "f\u00fcnf", "Uhr", "der", "O\u00b7per", "ed\u00b7le", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "CARD", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit einem Wort: Ruffin, das Wunderthier,", "tokens": ["Mit", "ei\u00b7nem", "Wort", ":", "Ruf\u00b7fin", ",", "das", "Wun\u00b7der\u00b7thier", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Glaubt, da\u00df in ihm die Weisheit sichtbar wohne.", "tokens": ["Glaubt", ",", "da\u00df", "in", "ihm", "die", "Weis\u00b7heit", "sicht\u00b7bar", "woh\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "APPR", "PPER", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was macht ihn stolz? Der Thoren Alles: Geld.", "tokens": ["Was", "macht", "ihn", "stolz", "?", "Der", "Tho\u00b7ren", "Al\u00b7les", ":", "Geld", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "$.", "ART", "NN", "PIS", "$.", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein frommer Greis, den schon, seit vielen Jahren,", "tokens": ["Ein", "from\u00b7mer", "Greis", ",", "den", "schon", ",", "seit", "vie\u00b7len", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Flei\u00df und Verdienst und M\u00e4\u00dfigkeit erh\u00e4lt,", "tokens": ["Flei\u00df", "und", "Ver\u00b7dienst", "und", "M\u00e4\u00b7\u00dfig\u00b7keit", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.8": {"text": "Ward j\u00fcngst von ihm sehr h\u00f6hnisch angefahren.", "tokens": ["Ward", "j\u00fcngst", "von", "ihm", "sehr", "h\u00f6h\u00b7nisch", "an\u00b7ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Alte sprach: Du machst mir nicht Verdru\u00df:", "tokens": ["Der", "Al\u00b7te", "sprach", ":", "Du", "machst", "mir", "nicht", "Ver\u00b7dru\u00df", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du bist nur reich, und trotzest mich vergebens:", "tokens": ["Du", "bist", "nur", "reich", ",", "und", "trot\u00b7zest", "mich", "ver\u00b7ge\u00b7bens", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dir fr\u00f6hnet nur ein eitler Ueberflu\u00df,", "tokens": ["Dir", "fr\u00f6h\u00b7net", "nur", "ein", "eit\u00b7ler", "Ue\u00b7berf\u00b7lu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Freund, doch nein! der Erbfeind deines Lebens.", "tokens": ["Der", "Freund", ",", "doch", "nein", "!", "der", "Erb\u00b7feind", "dei\u00b7nes", "Le\u00b7bens", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PTKANT", "$.", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es ist dein Haus ein f\u00fcrstlicher Palast:", "tokens": ["Es", "ist", "dein", "Haus", "ein", "f\u00fcrst\u00b7li\u00b7cher", "Pa\u00b7last", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Man sorgt, da\u00df dir kein Leckerbissen fehle;", "tokens": ["Man", "sorgt", ",", "da\u00df", "dir", "kein", "Le\u00b7cker\u00b7bis\u00b7sen", "feh\u00b7le", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Du opferst oft so manches deiner Kehle,", "tokens": ["Du", "op\u00b7ferst", "oft", "so", "man\u00b7ches", "dei\u00b7ner", "Keh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df kaum dein Tisch der Sch\u00fcsseln Menge fa\u00dft.", "tokens": ["Da\u00df", "kaum", "dein", "Tisch", "der", "Sch\u00fcs\u00b7seln", "Men\u00b7ge", "fa\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mir aber ist ein andres Loos verliehen:", "tokens": ["Mir", "a\u00b7ber", "ist", "ein", "and\u00b7res", "Loos", "ver\u00b7lie\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wann kehrt bei mir der Schmeichler l\u00e4chelnd ein?", "tokens": ["Wann", "kehrt", "bei", "mir", "der", "Schmeich\u00b7ler", "l\u00e4\u00b7chelnd", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPR", "PPER", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wann darf der Durst auf gar zu vielen Wein", "tokens": ["Wann", "darf", "der", "Durst", "auf", "gar", "zu", "vie\u00b7len", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ART", "NN", "APPR", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den Morgenschlaf zu zeitig mir entziehen?", "tokens": ["Den", "Mor\u00b7gen\u00b7schlaf", "zu", "zei\u00b7tig", "mir", "ent\u00b7zie\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKA", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich lebe nur in stiller Niedrigkeit.", "tokens": ["Ich", "le\u00b7be", "nur", "in", "stil\u00b7ler", "Nied\u00b7rig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Es wagt sich nichts zu meinen sichern H\u00fctten,", "tokens": ["Es", "wagt", "sich", "nichts", "zu", "mei\u00b7nen", "si\u00b7chern", "H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIS", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Als Wahrheit, Recht, Unstr\u00e4flichkeit der Sitten,", "tokens": ["Als", "Wahr\u00b7heit", ",", "Recht", ",", "U\u00b7nstr\u00e4f\u00b7lich\u00b7keit", "der", "Sit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Gesunder Witz und Selbstzufriedenheit.", "tokens": ["Ge\u00b7sun\u00b7der", "Witz", "und", "Selbst\u00b7zu\u00b7frie\u00b7den\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie th\u00f6richt ist dein Hochmuth in Geberden?", "tokens": ["Wie", "th\u00f6\u00b7richt", "ist", "dein", "Hoch\u00b7muth", "in", "Ge\u00b7ber\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-++++-+--", "measure": "unknown.measure.hexa"}, "line.2": {"text": "O J\u00fcngling, J\u00fcngling, stell' ihn ein:", "tokens": ["O", "J\u00fcng\u00b7ling", ",", "J\u00fcng\u00b7ling", ",", "stell'", "ihn", "ein", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was ich bin, kannst du nimmer sein;", "tokens": ["Was", "ich", "bin", ",", "kannst", "du", "nim\u00b7mer", "sein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "$,", "VMFIN", "PPER", "ADV", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was du bist, kann ein jeder werden.", "tokens": ["Was", "du", "bist", ",", "kann", "ein", "je\u00b7der", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "$,", "VMFIN", "ART", "PIS", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}