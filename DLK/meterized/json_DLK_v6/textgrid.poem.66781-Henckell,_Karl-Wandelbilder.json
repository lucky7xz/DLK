{"textgrid.poem.66781": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Wandelbilder", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe manchen Fant gekannt,", "tokens": ["Ich", "ha\u00b7be", "man\u00b7chen", "Fant", "ge\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der zum Charakter ward ernannt.", "tokens": ["Der", "zum", "Cha\u00b7rak\u00b7ter", "ward", "er\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich sa\u00df bei Zechgelag und Mahl,", "tokens": ["Ich", "sa\u00df", "bei", "Zech\u00b7ge\u00b7lag", "und", "Mahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jungdeutschland l\u00e4rmte satt und schal.", "tokens": ["Jung\u00b7deutschland", "l\u00e4rm\u00b7te", "satt", "und", "schal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Im Parlamente war ich auch,", "tokens": ["Im", "Par\u00b7la\u00b7men\u00b7te", "war", "ich", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da sah und h\u00f6rt' ich Schall und Rauch.", "tokens": ["Da", "sah", "und", "h\u00f6rt'", "ich", "Schall", "und", "Rauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Oft vor der B\u00f6rse stockt' mein Fu\u00df,", "tokens": ["Oft", "vor", "der", "B\u00f6r\u00b7se", "stockt'", "mein", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Lustschrei klang's, wie H\u00f6llengru\u00df.", "tokens": ["Wie", "Lust\u00b7schrei", "klang's", ",", "wie", "H\u00f6l\u00b7len\u00b7gru\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "$,", "PWAV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Aus Essen sah ich Feuer lohn,", "tokens": ["Aus", "Es\u00b7sen", "sah", "ich", "Feu\u00b7er", "lohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Rauch geschw\u00e4rzte F\u00e4uste drohn.", "tokens": ["Im", "Rauch", "ge\u00b7schw\u00e4rz\u00b7te", "F\u00e4us\u00b7te", "drohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Mit Trommeln und Trompetenklang", "tokens": ["Mit", "Trom\u00b7meln", "und", "Trom\u00b7pe\u00b7ten\u00b7klang"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Marschiert' ich fr\u00fch die Stadt entlang.", "tokens": ["Mar\u00b7schiert'", "ich", "fr\u00fch", "die", "Stadt", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die M\u00e4dchen sp\u00e4hten aus der T\u00fcr \u2013", "tokens": ["Die", "M\u00e4d\u00b7chen", "sp\u00e4h\u00b7ten", "aus", "der", "T\u00fcr", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hei F\u00fcsilier, hei Musketier!", "tokens": ["Hei", "F\u00fc\u00b7si\u00b7lier", ",", "hei", "Mus\u00b7ke\u00b7tier", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Dem Landmann hab ich zugeschaut,", "tokens": ["Dem", "Land\u00b7mann", "hab", "ich", "zu\u00b7ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie er den Acker still bebaut.", "tokens": ["Wie", "er", "den", "A\u00b7cker", "still", "be\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Der Pflug ging hin und kam zur\u00fcck,", "tokens": ["Der", "Pflug", "ging", "hin", "und", "kam", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Bauer s\u00e4t und m\u00e4ht sein Gl\u00fcck.", "tokens": ["Der", "Bau\u00b7er", "s\u00e4t", "und", "m\u00e4ht", "sein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Bald einen schlauen Juden fand", "tokens": ["Bald", "ei\u00b7nen", "schlau\u00b7en", "Ju\u00b7den", "fand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich witternd schleichen auf dem Land.", "tokens": ["Ich", "wit\u00b7ternd", "schlei\u00b7chen", "auf", "dem", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Die B\u00e4urin rang die H\u00e4nde schwer \u2013", "tokens": ["Die", "B\u00e4u\u00b7rin", "rang", "die", "H\u00e4n\u00b7de", "schwer", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Gl\u00fcck zieht \u00fcbers gro\u00dfe Meer.", "tokens": ["Das", "Gl\u00fcck", "zieht", "\u00fc\u00b7bers", "gro\u00b7\u00dfe", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "In kahler Kammer, kampfeswund,", "tokens": ["In", "kah\u00b7ler", "Kam\u00b7mer", ",", "kamp\u00b7fes\u00b7wund", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein deutscher K\u00fcnstler ging zu Grund.", "tokens": ["Ein", "deut\u00b7scher", "K\u00fcnst\u00b7ler", "ging", "zu", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Grub seinem Volk der Sch\u00f6nheit Gold,", "tokens": ["Grub", "sei\u00b7nem", "Volk", "der", "Sch\u00f6n\u00b7heit", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum stand er in des Mangels Sold.", "tokens": ["Drum", "stand", "er", "in", "des", "Man\u00b7gels", "Sold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Die Sch\u00f6nheit und die Wahrheit stehn", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "und", "die", "Wahr\u00b7heit", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Schatten, bis sie untergehn ...", "tokens": ["Im", "Schat\u00b7ten", ",", "bis", "sie", "un\u00b7ter\u00b7gehn", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}