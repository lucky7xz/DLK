{"textgrid.poem.54572": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "61.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich suche/ werther Freund/ ich such und kan nicht finden/", "tokens": ["Ich", "su\u00b7che", "/", "wert\u00b7her", "Freund", "/", "ich", "such", "und", "kan", "nicht", "fin\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADJA", "NN", "$(", "PPER", "VVFIN", "KON", "VMFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Indem eur Nahmens-Tag euch fr\u00f6lich wieder sieht/", "tokens": ["In\u00b7dem", "eur", "Nah\u00b7mens\u00b7Tag", "euch", "fr\u00f6\u00b7lich", "wie\u00b7der", "sieht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADJD", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und euer neues Gl\u00fcck beym neuen Jahr auffbl\u00fcht/", "tokens": ["Und", "eu\u00b7er", "neu\u00b7es", "Gl\u00fcck", "beym", "neu\u00b7en", "Jahr", "auff\u00b7bl\u00fcht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Band/ ein festes Band/ auf ewig euch zu binden.", "tokens": ["Ein", "Band", "/", "ein", "fes\u00b7tes", "Band", "/", "auf", "e\u00b7wig", "euch", "zu", "bin\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "NN", "$(", "APPR", "ADJD", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein hoher Geist l\u00e4st sich mit keinem Gold umwinden/", "tokens": ["Ein", "ho\u00b7her", "Geist", "l\u00e4st", "sich", "mit", "kei\u00b7nem", "Gold", "um\u00b7win\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wer Diamanten sucht ist sonder Nutz bem\u00fcht/", "tokens": ["Wer", "Di\u00b7a\u00b7man\u00b7ten", "sucht", "ist", "son\u00b7der", "Nutz", "be\u00b7m\u00fcht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "VAFIN", "ADV", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was \u00fcber Sonnen strahlt/ was durch die N\u00e4chte gl\u00fct/", "tokens": ["Was", "\u00fc\u00b7ber", "Son\u00b7nen", "strahlt", "/", "was", "durch", "die", "N\u00e4ch\u00b7te", "gl\u00fct", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VVFIN", "$(", "PWS", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mu\u00df vor dem edlen Glantz der freyen Seele schwinden;", "tokens": ["Mu\u00df", "vor", "dem", "ed\u00b7len", "Glantz", "der", "frey\u00b7en", "See\u00b7le", "schwin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch Liebe zwingt was Welt und Himmel zwingen kan/", "tokens": ["Doch", "Lie\u00b7be", "zwingt", "was", "Welt", "und", "Him\u00b7mel", "zwin\u00b7gen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PIS", "NN", "KON", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sie band den H\u00f6chsten selbst an harte H\u00f6ltzer an!", "tokens": ["Sie", "band", "den", "H\u00f6chs\u00b7ten", "selbst", "an", "har\u00b7te", "H\u00f6lt\u00b7zer", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die bitt ich/ da\u00df sie mir woll ihre Ketten leihen.", "tokens": ["Die", "bitt", "ich", "/", "da\u00df", "sie", "mir", "woll", "ih\u00b7re", "Ket\u00b7ten", "lei\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$(", "KOUS", "PPER", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Was sag ich? nein nicht mir/ nur einer frischen Braut/", "tokens": ["Was", "sag", "ich", "?", "nein", "nicht", "mir", "/", "nur", "ei\u00b7ner", "fri\u00b7schen", "Braut", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "PTKANT", "PTKNEG", "PPER", "$(", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die eh di\u00df Jahr hinweg auf ewig euch vertraut/", "tokens": ["Die", "eh", "di\u00df", "Jahr", "hin\u00b7weg", "auf", "e\u00b7wig", "euch", "ver\u00b7traut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "PDS", "NN", "ADV", "APPR", "ADJD", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So binde/ da\u00df euch nicht m\u00f6g eine Macht befreyen.", "tokens": ["So", "bin\u00b7de", "/", "da\u00df", "euch", "nicht", "m\u00f6g", "ei\u00b7ne", "Macht", "be\u00b7fre\u00b7yen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "PPER", "PTKNEG", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}}}}}