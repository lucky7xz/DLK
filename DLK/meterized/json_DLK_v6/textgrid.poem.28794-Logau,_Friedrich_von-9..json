{"textgrid.poem.28794": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stintia wehrt ihrer Ehren; wer ihr was wil muthen an,", "tokens": ["Stin\u00b7tia", "wehrt", "ih\u00b7rer", "Eh\u00b7ren", ";", "wer", "ihr", "was", "wil", "mu\u00b7then", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$.", "PWS", "PPER", "PIS", "VMFIN", "VVINF", "PTKVZ", "$,"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Ey, der mu\u00df es schwer entgelten, sie erzeigt sich als ein Mann;", "tokens": ["Ey", ",", "der", "mu\u00df", "es", "schwer", "ent\u00b7gel\u00b7ten", ",", "sie", "er\u00b7zeigt", "sich", "als", "ein", "Mann", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "VMFIN", "PPER", "ADJD", "VVINF", "$,", "PPER", "VVFIN", "PRF", "KOUS", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Dann sie greifft bald zum Gewehre, wer entwerden kan, ist froh;", "tokens": ["Dann", "sie", "greifft", "bald", "zum", "Ge\u00b7weh\u00b7re", ",", "wer", "ent\u00b7wer\u00b7den", "kan", ",", "ist", "froh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$,", "PWS", "VVINF", "VMFIN", "$,", "VAFIN", "ADJD", "$."], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Doch wer etwas mehr ist witzig, stehet still; es ist nur Stroh.", "tokens": ["Doch", "wer", "et\u00b7was", "mehr", "ist", "wit\u00b7zig", ",", "ste\u00b7het", "still", ";", "es", "ist", "nur", "Stroh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIS", "ADV", "VAFIN", "ADJD", "$,", "VVFIN", "PTKVZ", "$.", "PPER", "VAFIN", "ADV", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}