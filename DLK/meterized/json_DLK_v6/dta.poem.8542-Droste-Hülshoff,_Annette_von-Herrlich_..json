{"dta.poem.8542": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Herrlich .", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und wenn er aus der Pforte tritt", "tokens": ["Und", "wenn", "er", "aus", "der", "Pfor\u00b7te", "tritt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und weht sein Mantel \u00fcber die Gasse,", "tokens": ["Und", "weht", "sein", "Man\u00b7tel", "\u00fc\u00b7ber", "die", "Gas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dann steh\u2019n die M\u00e4nner, das Haupt geneigt,", "tokens": ["Dann", "steh'n", "die", "M\u00e4n\u00b7ner", ",", "das", "Haupt", "ge\u00b7neigt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sprechend: wo sind deine Vasallen? \u2014", "tokens": ["Spre\u00b7chend", ":", "wo", "sind", "dei\u00b7ne", "Va\u00b7sal\u00b7len", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "$.", "PWAV", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Und die Wittwen und Waisen knieend schrei\u2019n:", "tokens": ["Und", "die", "Witt\u00b7wen", "und", "Wai\u00b7sen", "kni\u00b7e\u00b7end", "schrei'n", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "NN", "ADJD", "VVINF", "$."], "meter": "--+--+-+---", "measure": "anapaest.di.plus"}, "line.6": {"text": "Hilf uns, du m\u00e4cht\u2019ger Gebieter.", "tokens": ["Hilf", "uns", ",", "du", "m\u00e4cht'\u00b7ger", "Ge\u00b7bie\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}