{"textgrid.poem.31469": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach der \u00fcbermilde Himmel / schenkt auf Leiden Freuden ein /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach der \u00fcbermilde Himmel / schenkt auf Leiden Freuden ein /", "tokens": ["Ach", "der", "\u00fc\u00b7ber\u00b7mil\u00b7de", "Him\u00b7mel", "/", "schenkt", "auf", "Lei\u00b7den", "Freu\u00b7den", "ein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "ADJA", "NN", "$(", "VVFIN", "APPR", "ADJA", "NN", "ART", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "senkt die Frommen wol in Noht / l\u00e4sst sie aber nicht versinken.", "tokens": ["senkt", "die", "From\u00b7men", "wol", "in", "Noht", "/", "l\u00e4sst", "sie", "a\u00b7ber", "nicht", "ver\u00b7sin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPR", "NN", "$(", "VVFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Weil sie ringen mit der Angst / pfleget Er der Freud zuwinken.", "tokens": ["Weil", "sie", "rin\u00b7gen", "mit", "der", "Angst", "/", "pfle\u00b7get", "Er", "der", "Freud", "zu\u00b7win\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$(", "VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Er verkl\u00e4ret ihre Tr\u00fcbsal / mit dem gnaden wunderschein /", "tokens": ["Er", "ver\u00b7kl\u00e4\u00b7ret", "ih\u00b7re", "Tr\u00fcb\u00b7sal", "/", "mit", "dem", "gna\u00b7den", "wun\u00b7der\u00b7schein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "schicket ganze Labungs-Str\u00f6me / f\u00fcr ein kleines tr\u00f6pflein Pein.", "tokens": ["schi\u00b7cket", "gan\u00b7ze", "La\u00b7bungs\u00b7Str\u00f6\u00b7me", "/", "f\u00fcr", "ein", "klei\u00b7nes", "tr\u00f6pf\u00b7lein", "Pein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$(", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "ja in w\u00e4hrend-gr\u00f6ster Qual l\u00e4st er Liebes-flammen blinken /", "tokens": ["ja", "in", "w\u00e4h\u00b7ren\u00b7dgr\u00f6s\u00b7ter", "Qual", "l\u00e4st", "er", "Lie\u00b7bes\u00b7flam\u00b7men", "blin\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "PPER", "NN", "VVINF", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "und das Noht-verschm\u00e4hte Herz aus dem Gnaden Abgrund trinken:", "tokens": ["und", "das", "Noht\u00b7ver\u00b7schm\u00e4h\u00b7te", "Herz", "aus", "dem", "Gna\u00b7den", "Ab\u00b7grund", "trin\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Er ergetzt es und versetzt es in seim Erzerbarmung seyn.", "tokens": ["Er", "er\u00b7getzt", "es", "und", "ver\u00b7setzt", "es", "in", "seim", "Er\u00b7zer\u00b7bar\u00b7mung", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Da verschickt es wunderlich eine Eusserkeit der andern:", "tokens": ["Da", "ver\u00b7schickt", "es", "wun\u00b7der\u00b7lich", "ei\u00b7ne", "Eus\u00b7ser\u00b7keit", "der", "an\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "ART", "ADJA", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "aus Erz-Angst in Haupt-Vergn\u00fcgung / aus der Qual in Jubel-Thron", "tokens": ["aus", "Er\u00b7zAngst", "in", "Haup\u00b7tVer\u00b7gn\u00fc\u00b7gung", "/", "aus", "der", "Qual", "in", "Ju\u00b7bel\u00b7Thron"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "$(", "APPR", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "mu\u00df es durch des H\u00f6chsten Krafft / klar doch unverm\u00e4rket wandern /", "tokens": ["mu\u00df", "es", "durch", "des", "H\u00f6chs\u00b7ten", "Krafft", "/", "klar", "doch", "un\u00b7ver\u00b7m\u00e4r\u00b7ket", "wan\u00b7dern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$(", "ADJD", "ADV", "ADJD", "VVFIN", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "da\u00df der Geist / vor Last und Luste / seuffzet ob dem Creutz und Kron.", "tokens": ["da\u00df", "der", "Geist", "/", "vor", "Last", "und", "Lus\u00b7te", "/", "seuff\u00b7zet", "ob", "dem", "Creutz", "und", "Kron", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$(", "APPR", "NN", "KON", "NN", "$(", "VVFIN", "KOUS", "ART", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Wol / ja wol und \u00fcberwol / dem / der fest auf Gott vertrauet!", "tokens": ["Wol", "/", "ja", "wol", "und", "\u00fc\u00b7ber\u00b7wol", "/", "dem", "/", "der", "fest", "auf", "Gott", "ver\u00b7trau\u00b7et", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "ADV", "ADV", "KON", "ADV", "$(", "ART", "$(", "ART", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "weil er nicht allein sein H\u00fclff / ja selbst seine selbstheit / schauet.", "tokens": ["weil", "er", "nicht", "al\u00b7lein", "sein", "H\u00fclff", "/", "ja", "selbst", "sei\u00b7ne", "selbst\u00b7heit", "/", "schau\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "PPOSAT", "NN", "$(", "ADV", "ADV", "PPOSAT", "NN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}