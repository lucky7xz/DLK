"word", "word", "word"], "pos": ["ADJD", "APPR", "PPER", "APZR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hascht das Schicksal \u2013 seht! \u2013 und lustig", "tokens": ["Hascht", "das", "Schick\u00b7sal", "\u2013", "seht", "!", "\u2013", "und", "lus\u00b7tig"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$(", "VVFIN", "$.", "$(", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rei\u00dfts die Menschlein kreuz und quer.", "tokens": ["Rei\u00dfts", "die", "Mensc\u00b7hlein", "kreuz", "und", "quer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}