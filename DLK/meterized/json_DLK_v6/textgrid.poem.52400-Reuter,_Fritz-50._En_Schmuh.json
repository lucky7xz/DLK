{"textgrid.poem.52400": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "50. En Schmuh", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Prenzlau wahnt en Jud', de hadd", "tokens": ["In", "Prenz\u00b7lau", "wahnt", "en", "Jud'", ",", "de", "hadd"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "NE", "NE", "$,", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Giz den wohren Deuwel seihn;", "tokens": ["Mit", "Giz", "den", "woh\u00b7ren", "Deu\u00b7wel", "seihn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hei h\u00f6ll dat Sinig so tau Rad',", "tokens": ["Hei", "h\u00f6ll", "dat", "Si\u00b7nig", "so", "tau", "Rad'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "ADV", "CARD", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Dat ein em bi de beiden Bein", "tokens": ["Dat", "ein", "em", "bi", "de", "bei\u00b7den", "Bein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Uph\u00e4ngen k\u00fcnn un k\u00fcnn em st\u00e4kern,", "tokens": ["Up\u00b7h\u00e4n\u00b7gen", "k\u00fcnn", "un", "k\u00fcnn", "em", "st\u00e4\u00b7kern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Hei r\u00fcckt nicks rut un d\u00e4d vers\u00e4kern,", "tokens": ["Hei", "r\u00fcckt", "nicks", "rut", "un", "d\u00e4d", "ver\u00b7s\u00e4\u00b7kern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Hei wir bi Gott en armen Mann,", "tokens": ["Hei", "wir", "bi", "Gott", "en", "ar\u00b7men", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "NN", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Un stellt sick gottserb\u00e4rmlich an.", "tokens": ["Un", "stellt", "sick", "gotts\u00b7er\u00b7b\u00e4rm\u00b7lich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "De L\u00fcd vertellten, dat hei jede Nacht", "tokens": ["De", "L\u00fcd", "ver\u00b7tell\u00b7ten", ",", "dat", "hei", "je\u00b7de", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "$,", "PRELS", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "In sinen Hus' un Hof un Goren", "tokens": ["In", "si\u00b7nen", "Hus'", "un", "Hof", "un", "Go\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "FM", "FM", "FM", "FM", "FM", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Mit Hun'ngeblaff h\u00f6ll s\u00fclwsten Wacht,", "tokens": ["Mit", "Hun'\u00b7nge\u00b7blaff", "h\u00f6ll", "s\u00fclws\u00b7ten", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "\u00dcm't Fauder f\u00f6r en Hund tau sporen.", "tokens": ["\u00dcm't", "Fau\u00b7der", "f\u00f6r", "en", "Hund", "tau", "spo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Dat weit ick nich un heww dat ok nich h\u00fcrt,", "tokens": ["Dat", "weit", "ick", "nich", "un", "heww", "dat", "ok", "nich", "h\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Behaupt dat dr\u00fcm ok nich, denn Unrecht wir't,", "tokens": ["Be\u00b7haupt", "dat", "dr\u00fcm", "ok", "nich", ",", "denn", "Un\u00b7recht", "wir't", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.nl", "FM.nl", "FM.nl", "FM.nl", "PTKNEG", "$,", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Wull ick wat seggen, wat nich is;", "tokens": ["Wull", "ick", "wat", "seg\u00b7gen", ",", "wat", "nich", "is", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "PRELS", "PTKNEG", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Doch dat, dat weit ick ganz gewi\u00df,", "tokens": ["Doch", "dat", ",", "dat", "weit", "ick", "ganz", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "$,", "PRELS", "ADJD", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Dat hei des Morgens, wenn de Scheper drew,", "tokens": ["Dat", "hei", "des", "Mor\u00b7gens", ",", "wenn", "de", "Sche\u00b7per", "drew", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "ADV", "$,", "KOUS", "NE", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Sick irst nah sine Schap begew,", "tokens": ["Sick", "irst", "nah", "si\u00b7ne", "Schap", "be\u00b7gew", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADJD", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.19": {"text": "De Stalld\u00f6r fast heranner tog", "tokens": ["De", "Stall\u00b7d\u00f6r", "fast", "her\u00b7an\u00b7ner", "tog"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Un up de Schap her\u00fcmmer slog,", "tokens": ["Un", "up", "de", "Schap", "he\u00b7r\u00fcm\u00b7mer", "slog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Bet s' allen Anstand in ehr Angst vergeten", "tokens": ["Bet", "s'", "al\u00b7len", "An\u00b7stand", "in", "ehr", "Angst", "ver\u00b7ge\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "PIAT", "NN", "APPR", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.22": {"text": "Un em irst ehr Rosinen leten.", "tokens": ["Un", "em", "irst", "ehr", "Ro\u00b7si\u00b7nen", "le\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.23": {"text": "Un hadd'n s' em de Rosinen laten,", "tokens": ["Un", "had\u00b7d'n", "s'", "em", "de", "Ro\u00b7si\u00b7nen", "la\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.24": {"text": "Denn makt oll Mauscher d'rut Dukaten.", "tokens": ["Denn", "makt", "oll", "Mau\u00b7scher", "d'\u00b7rut", "Du\u00b7ka\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Oll Mauscher, de w\u00fcrd rik, sihr rik,", "tokens": ["Oll", "Mau\u00b7scher", ",", "de", "w\u00fcrd", "rik", ",", "sihr", "rik", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NE", "VAFIN", "NE", "$,", "PPER", "VVFIN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sin Giz, de blew sick \u00fcmmer glik,", "tokens": ["Sin", "Giz", ",", "de", "blew", "sick", "\u00fcm\u00b7mer", "glik", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "FM", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Un ebenso was dat nich minner", "tokens": ["Un", "e\u00b7ben\u00b7so", "was", "dat", "nich", "min\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "PWS", "ART", "PTKNEG", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit sine S\u00e4hns un Swigerkinner.", "tokens": ["Mit", "si\u00b7ne", "S\u00e4hns", "un", "Swi\u00b7ger\u00b7kin\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "FM", "FM", "FM", "NE", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Doch wat de Oll mit Schap bedrewen,", "tokens": ["Doch", "wat", "de", "Oll", "mit", "Schap", "be\u00b7dre\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "NE", "NE", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Bedrewen sei mit arme S\u00fcnner,", "tokens": ["Be\u00b7dre\u00b7wen", "sei", "mit", "ar\u00b7me", "S\u00fcn\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "De in Verlegenheiten seten;", "tokens": ["De", "in", "Ver\u00b7le\u00b7gen\u00b7hei\u00b7ten", "se\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Kredit w\u00fcrd ehr nich ihre gewen,", "tokens": ["Kre\u00b7dit", "w\u00fcrd", "ehr", "nich", "ih\u00b7re", "ge\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Bet s' ok irst ehr Rosinen leten.", "tokens": ["Bet", "s'", "ok", "irst", "ehr", "Ro\u00b7si\u00b7nen", "le\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Un handelt Mauscher blot mit Posen", "tokens": ["Un", "han\u00b7delt", "Mau\u00b7scher", "blot", "mit", "Po\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NN", "NE", "APPR", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Un h\u00f6chstens mit geflickte Hosen,", "tokens": ["Un", "h\u00f6chs\u00b7tens", "mit", "ge\u00b7flick\u00b7te", "Ho\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Denn m\u00fc\u00dft dat nu bi Itzig un bi Benjamin", "tokens": ["Denn", "m\u00fc\u00dft", "dat", "nu", "bi", "It\u00b7zig", "un", "bi", "Ben\u00b7ja\u00b7min"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "ADV", "FM", "FM", "FM", "FM", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mit Wull, Perdukten, Staatspoppieren sin.", "tokens": ["Mit", "Wull", ",", "Per\u00b7duk\u00b7ten", ",", "Staat\u00b7spop\u00b7pie\u00b7ren", "sin", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "$,", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Von Mauschern sin Nahkamenschaft", "tokens": ["Von", "Mau\u00b7schern", "sin", "Nah\u00b7ka\u00b7men\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "W\u00fcrd an de B\u00f6rs' tau Hamborg spraken.", "tokens": ["W\u00fcrd", "an", "de", "B\u00f6r\u00b7s'", "tau", "Ham\u00b7borg", "spra\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NE", "NE", "NE", "NE", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.16": {"text": "Stamm Mauscher gr\u00e4unt in vuller Kraft,", "tokens": ["Stamm", "Mau\u00b7scher", "gr\u00e4unt", "in", "vul\u00b7ler", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Un an em s\u00fcnd Papiercher un Dukaten", "tokens": ["Un", "an", "em", "s\u00fcnd", "Pa\u00b7pier\u00b7cher", "un", "Du\u00b7ka\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Linglang as Bladd an Bladd upbraken.", "tokens": ["Ling\u00b7lang", "as", "Bladd", "an", "Bladd", "up\u00b7bra\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Un \u00e4wer sine S\u00e4hns un D\u00f6chter", "tokens": ["Un", "\u00e4\u00b7wer", "si\u00b7ne", "S\u00e4hns", "un", "D\u00f6ch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.20": {"text": "Br\u00f6k ok de Bildung pl\u00f6tzlich rin,", "tokens": ["Br\u00f6k", "ok", "de", "Bil\u00b7dung", "pl\u00f6tz\u00b7lich", "rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "As wenn s' mit ein'n N\u00fcrnbarger Trechter", "tokens": ["As", "wenn", "s'", "mit", "ein'n", "N\u00fcrn\u00b7bar\u00b7ger", "Trech\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "NE", "APPR", "ART", "ADJA", "NN"], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}, "line.22": {"text": "Ehr in den D\u00e4ts w\u00fcrd trechtert sin.", "tokens": ["Ehr", "in", "den", "D\u00e4ts", "w\u00fcrd", "trech\u00b7tert", "sin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VAF