{"textgrid.poem.41554": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am f\u00fcnften Sonntage nach Pfingsten", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Abgrund hat sich aufgetan", "tokens": ["Ein", "Ab\u00b7grund", "hat", "sich", "auf\u00b7ge\u00b7tan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PRF", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Auge meiner Seele;", "tokens": ["Dem", "Au\u00b7ge", "mei\u00b7ner", "See\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verdorrt steht meines Lebens Bahn,", "tokens": ["Ver\u00b7dorrt", "steht", "mei\u00b7nes", "Le\u00b7bens", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ich es mir verhehle.", "tokens": ["Wie", "ich", "es", "mir", "ver\u00b7heh\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch Wahrheit alle Schleier bricht,", "tokens": ["Doch", "Wahr\u00b7heit", "al\u00b7le", "Schlei\u00b7er", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weh mir, die Liebe hab' ich nicht!", "tokens": ["Weh", "mir", ",", "die", "Lie\u00b7be", "hab'", "ich", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ART", "NN", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hat sich mein Herz so manches Mal", "tokens": ["Hat", "sich", "mein", "Herz", "so", "man\u00b7ches", "Mal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PRF", "PPOSAT", "NN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verzweifelnd dran gehangen,", "tokens": ["Ver\u00b7zwei\u00b7felnd", "dran", "ge\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "PAV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn meine S\u00fcnden ohne Zahl", "tokens": ["Wenn", "mei\u00b7ne", "S\u00fcn\u00b7den", "oh\u00b7ne", "Zahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gespenstig auf mich drangen:", "tokens": ["Ge\u00b7spens\u00b7tig", "auf", "mich", "dran\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es ist doch wahr, und ist kein Traum,", "tokens": ["Es", "ist", "doch", "wahr", ",", "und", "ist", "kein", "Traum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "KON", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Lieben ist nur Dunst und Schaum.", "tokens": ["Mein", "Lie\u00b7ben", "ist", "nur", "Dunst", "und", "Schaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja! soll noch Rettung dir geschehn,", "tokens": ["Ja", "!", "soll", "noch", "Ret\u00b7tung", "dir", "ge\u00b7schehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "VMFIN", "ADV", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du mein unsterblich Wesen:", "tokens": ["Du", "mein", "uns\u00b7terb\u00b7lich", "We\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mu\u00dft fest du in den Spiegel sehn,", "tokens": ["Mu\u00dft", "fest", "du", "in", "den", "Spie\u00b7gel", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00dft ohne Zucken lesen", "tokens": ["Mu\u00dft", "oh\u00b7ne", "Zu\u00b7cken", "le\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In deiner Brust die dunkle Schrift.", "tokens": ["In", "dei\u00b7ner", "Brust", "die", "dunk\u00b7le", "Schrift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Viel besser Dolch als schleichend Gift!", "tokens": ["Viel", "bes\u00b7ser", "Dolch", "als", "schlei\u00b7chend", "Gift", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "KOKOM", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wem bist du reich? ist es nicht nur", "tokens": ["Wem", "bist", "du", "reich", "?", "ist", "es", "nicht", "nur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "ADJD", "$.", "VAFIN", "PPER", "PTKNEG", "ADV"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Der Arme, so sich beuget?", "tokens": ["Der", "Ar\u00b7me", ",", "so", "sich", "beu\u00b7get", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hast jemals freudiger Natur", "tokens": ["Hast", "je\u00b7mals", "freu\u00b7di\u00b7ger", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du milde dich geneiget?", "tokens": ["Du", "mil\u00b7de", "dich", "ge\u00b7nei\u00b7get", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dem\u00fctig nur und kummervoll", "tokens": ["De\u00b7m\u00fc\u00b7tig", "nur", "und", "kum\u00b7mer\u00b7voll"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADV", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erpre\u00dft man dir den schn\u00f6den Zoll.", "tokens": ["Er\u00b7pre\u00dft", "man", "dir", "den", "schn\u00f6\u00b7den", "Zoll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Kalt wie der Tod kannst, wehe dir,", "tokens": ["Kalt", "wie", "der", "Tod", "kannst", ",", "we\u00b7he", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "VMFIN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die H\u00fclfe du versagen,", "tokens": ["Die", "H\u00fcl\u00b7fe", "du", "ver\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo nur ein \u00fcppig Zweiglein dir", "tokens": ["Wo", "nur", "ein", "\u00fcp\u00b7pig", "Zwei\u00b7glein", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "ADJD", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Scheint freudig aufzuragen;", "tokens": ["Scheint", "freu\u00b7dig", "auf\u00b7zu\u00b7ra\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du, den der fremde Splitter sticht,", "tokens": ["Du", ",", "den", "der", "frem\u00b7de", "Split\u00b7ter", "sticht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und siehst den eignen Balken nicht!", "tokens": ["Und", "siehst", "den", "eig\u00b7nen", "Bal\u00b7ken", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Freiwillig kam es dir nicht ein,", "tokens": ["Frei\u00b7wil\u00b7lig", "kam", "es", "dir", "nicht", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df, ob die Lippe schweiget,", "tokens": ["Da\u00df", ",", "ob", "die", "Lip\u00b7pe", "schwei\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ob unter zarter Demut Schein", "tokens": ["Ob", "un\u00b7ter", "zar\u00b7ter", "De\u00b7mut", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich mild die Rechte zeiget,", "tokens": ["Sich", "mild", "die", "Rech\u00b7te", "zei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es gibt kein s\u00fc\u00dfer Hochmutsspiel", "tokens": ["Es", "gibt", "kein", "s\u00fc\u00b7\u00dfer", "Hoch\u00b7muts\u00b7spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als eigner G\u00fcte Selbstgef\u00fchl!", "tokens": ["Als", "eig\u00b7ner", "G\u00fc\u00b7te", "Selbst\u00b7ge\u00b7f\u00fchl", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Freiwillig hast du nicht gef\u00fchlt,", "tokens": ["Frei\u00b7wil\u00b7lig", "hast", "du", "nicht", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie dich die Nerven zwangen,", "tokens": ["Wie", "dich", "die", "Ner\u00b7ven", "zwan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn, wie elektrisch Feuer spielt,", "tokens": ["Wenn", ",", "wie", "e\u00b7lekt\u00b7risch", "Feu\u00b7er", "spielt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die fremden Schmerzen drangen", "tokens": ["Die", "frem\u00b7den", "Schmer\u00b7zen", "dran\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In deines K\u00f6rpers schwachen Bau,", "tokens": ["In", "dei\u00b7nes", "K\u00f6r\u00b7pers", "schwa\u00b7chen", "Bau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu schn\u00f6der ird'scher Tr\u00e4nen Tau.", "tokens": ["Zu", "schn\u00f6\u00b7der", "ird'\u00b7scher", "Tr\u00e4\u00b7nen", "Tau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Greif an, es ist die h\u00f6chste Zeit,", "tokens": ["Greif", "an", ",", "es", "ist", "die", "h\u00f6chs\u00b7te", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Greif an mit mut'gen H\u00e4nden;", "tokens": ["Greif", "an", "mit", "mut'\u00b7gen", "H\u00e4n\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des Richters Waage liegt bereit,", "tokens": ["Des", "Rich\u00b7ters", "Waa\u00b7ge", "liegt", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Lauf wird schleunig enden!", "tokens": ["Dein", "Lauf", "wird", "schleu\u00b7nig", "en\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zeigt jeder Atemzug nicht an,", "tokens": ["Zeigt", "je\u00b7der", "A\u00b7tem\u00b7zug", "nicht", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie kurz gemessen deine Bahn?", "tokens": ["Wie", "kurz", "ge\u00b7mes\u00b7sen", "dei\u00b7ne", "Bahn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wie elend ich nur bin und schwach,", "tokens": ["Wie", "e\u00b7lend", "ich", "nur", "bin", "und", "schwach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "VAFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nie hab' ich es empfunden,", "tokens": ["Nie", "hab'", "ich", "es", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als da die letzte St\u00fctze brach", "tokens": ["Als", "da", "die", "letz\u00b7te", "St\u00fct\u00b7ze", "brach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In diesen schweren Stunden.", "tokens": ["In", "die\u00b7sen", "schwe\u00b7ren", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch einen gibt es, einen doch,", "tokens": ["Doch", "ei\u00b7nen", "gibt", "es", ",", "ei\u00b7nen", "doch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "$,", "ART", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der eine kann mich retten noch.", "tokens": ["Der", "ei\u00b7ne", "kann", "mich", "ret\u00b7ten", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "So la\u00df, du aller S\u00fcnden Damm,", "tokens": ["So", "la\u00df", ",", "du", "al\u00b7ler", "S\u00fcn\u00b7den", "Damm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "PPER", "PIAT", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du treuster Freund von allen,", "tokens": ["Du", "treus\u00b7ter", "Freund", "von", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "PIAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mich nicht als modermorschen Stamm", "tokens": ["Mich", "nicht", "als", "mo\u00b7der\u00b7mor\u00b7schen", "Stamm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PTKNEG", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So unversehends fallen!", "tokens": ["So", "un\u00b7ver\u00b7se\u00b7hends", "fal\u00b7len", 