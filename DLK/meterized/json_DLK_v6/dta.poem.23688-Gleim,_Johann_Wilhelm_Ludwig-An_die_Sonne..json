{"dta.poem.23688": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An die Sonne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sonne, brich doch durch die Wolken!", "tokens": ["Son\u00b7ne", ",", "brich", "doch", "durch", "die", "Wol\u00b7ken", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df uns doch den hellen Himmel", "tokens": ["La\u00df", "uns", "doch", "den", "hel\u00b7len", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df uns deine Stralen sehen!", "tokens": ["La\u00df", "uns", "dei\u00b7ne", "Stra\u00b7len", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Haben denn die dikken D\u00fcnste", "tokens": ["Ha\u00b7ben", "denn", "die", "dik\u00b7ken", "D\u00fcns\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dich nicht lang genug verdunkelt?", "tokens": ["Dich", "nicht", "lang", "ge\u00b7nug", "ver\u00b7dun\u00b7kelt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADJD", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hat es nicht genug geregnet?", "tokens": ["Hat", "es", "nicht", "ge\u00b7nug", "ge\u00b7reg\u00b7net", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sieh, die Fluren stehn voll Wasser,", "tokens": ["Sieh", ",", "die", "Flu\u00b7ren", "stehn", "voll", "Was\u00b7ser", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und es ist f\u00fcr deine Stralen,", "tokens": ["Und", "es", "ist", "f\u00fcr", "dei\u00b7ne", "Stra\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Viel zu trokknen, viel zu trinken!", "tokens": ["Viel", "zu", "trokk\u00b7nen", ",", "viel", "zu", "trin\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sonne, la\u00df die d\u00fcstern Wolken", "tokens": ["Son\u00b7ne", ",", "la\u00df", "die", "d\u00fcs\u00b7tern", "Wol\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVIMP", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Schnell vor deinen Stralen fliehen;", "tokens": ["Schnell", "vor", "dei\u00b7nen", "Stra\u00b7len", "flie\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Aber, wenn sie, statt des Wassers,", "tokens": ["A\u00b7ber", ",", "wenn", "sie", ",", "statt", "des", "Was\u00b7sers", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "$,", "KOUI", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Wein herunter sch\u00fctten wollen,", "tokens": ["Wein", "her\u00b7un\u00b7ter", "sch\u00fct\u00b7ten", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APZR", "VVFIN", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Solchen Wein, wie ich itzt trinke, ", "tokens": ["Sol\u00b7chen", "Wein", ",", "wie", "ich", "itzt", "trin\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "O so la\u00df die Wolken regnen!", "tokens": ["O", "so", "la\u00df", "die", "Wol\u00b7ken", "reg\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVIMP", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}