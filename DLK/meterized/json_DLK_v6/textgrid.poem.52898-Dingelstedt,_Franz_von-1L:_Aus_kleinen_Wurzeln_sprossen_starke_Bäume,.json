{"textgrid.poem.52898": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus kleinen Wurzeln sprossen starke B\u00e4ume,", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus kleinen Wurzeln sprossen starke B\u00e4ume,", "tokens": ["Aus", "klei\u00b7nen", "Wur\u00b7zeln", "spros\u00b7sen", "star\u00b7ke", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein m\u00e4cht'ger Strom entspringt aus dunklem Quell:", "tokens": ["Ein", "m\u00e4cht'\u00b7ger", "Strom", "ent\u00b7springt", "aus", "dunk\u00b7lem", "Quell", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dran mahnen diese unscheinbaren R\u00e4ume,", "tokens": ["Dran", "mah\u00b7nen", "die\u00b7se", "un\u00b7schein\u00b7ba\u00b7ren", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ehmals dein Zelt, erw\u00e4hltes Israel!", "tokens": ["Eh\u00b7mals", "dein", "Zelt", ",", "er\u00b7w\u00e4hl\u00b7tes", "Is\u00b7rael", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "ADJA", "NE", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Die Sonne dringt, des Mondes Leuchten nimmer", "tokens": ["Die", "Son\u00b7ne", "dringt", ",", "des", "Mon\u00b7des", "Leuch\u00b7ten", "nim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In jene H\u00fctten voller Rauch und Schmutz,", "tokens": ["In", "je\u00b7ne", "H\u00fct\u00b7ten", "vol\u00b7ler", "Rauch", "und", "Schmutz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und nur der Sabbatslampe seltner Schimmer", "tokens": ["Und", "nur", "der", "Sab\u00b7bats\u00b7lam\u00b7pe", "selt\u00b7ner", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bestrahlt den innen streng versteckten Putz.", "tokens": ["Be\u00b7strahlt", "den", "in\u00b7nen", "streng", "ver\u00b7steck\u00b7ten", "Putz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie dr\u00e4uend-schwer die Giebel \u00fcberh\u00e4ngen,", "tokens": ["Wie", "dr\u00e4u\u00b7en\u00b7dschwer", "die", "Gie\u00b7bel", "\u00fc\u00b7ber\u00b7h\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Dampf geschw\u00e4rzt, von Alters Wucht gebeugt!", "tokens": ["Von", "Dampf", "ge\u00b7schw\u00e4rzt", ",", "von", "Al\u00b7ters", "Wucht", "ge\u00b7beugt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "APPR", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie sie zu Schutz und Trutz zusammendr\u00e4ngen,", "tokens": ["Wie", "sie", "zu", "Schutz", "und", "Trutz", "zu\u00b7sam\u00b7men\u00b7dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als h\u00e4tte die Gewalt sie hergescheucht!", "tokens": ["Als", "h\u00e4t\u00b7te", "die", "Ge\u00b7walt", "sie", "her\u00b7ge\u00b7scheucht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Aus niedren Pforten, wie aus M\u00f6rdergruben,", "tokens": ["Aus", "nied\u00b7ren", "Pfor\u00b7ten", ",", "wie", "aus", "M\u00f6r\u00b7der\u00b7gru\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "G\u00e4hnt ew'ges Dunkel r\u00e4tselhaft dich an,", "tokens": ["G\u00e4hnt", "ew'\u00b7ges", "Dun\u00b7kel", "r\u00e4t\u00b7sel\u00b7haft", "dich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und schmale Stiegen klimmen auf in Stuben,", "tokens": ["Und", "schma\u00b7le", "Stie\u00b7gen", "klim\u00b7men", "auf", "in", "Stu\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch deren Fenster nie ein Lichtstrahl rann.", "tokens": ["Durch", "de\u00b7ren", "Fens\u00b7ter", "nie", "ein", "Licht\u00b7strahl", "rann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und stete N\u00e4sse in der engen Gasse,", "tokens": ["Und", "ste\u00b7te", "N\u00e4s\u00b7se", "in", "der", "en\u00b7gen", "Gas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die krumm und winklicht ihres Weges schleicht,", "tokens": ["Die", "krumm", "und", "wink\u00b7licht", "ih\u00b7res", "We\u00b7ges", "schleicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "VVFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und vor den T\u00fcren hagre, scharfe, blasse", "tokens": ["Und", "vor", "den", "T\u00fc\u00b7ren", "hag\u00b7re", ",", "schar\u00b7fe", ",", "blas\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,", "VVFIN", "$,", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gesichter, von der Leidenschaft gebleicht.", "tokens": ["Ge\u00b7sich\u00b7ter", ",", "von", "der", "Lei\u00b7den\u00b7schaft", "ge\u00b7bleicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Das Judenviertel! \u2013 O Barbaren-Zeiten,", "tokens": ["Das", "Ju\u00b7den\u00b7vier\u00b7tel", "!", "\u2013", "O", "Ba\u00b7rba\u00b7ren\u00b7Zei\u00b7ten", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da man ein Volk hier sklavisch eingezw\u00e4ngt,", "tokens": ["Da", "man", "ein", "Volk", "hier", "skla\u00b7visch", "ein\u00b7ge\u00b7zw\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und da des Nachts am Tor, zu beiden Seiten,", "tokens": ["Und", "da", "des", "Nachts", "am", "Tor", ",", "zu", "bei\u00b7den", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADV", "APPRART", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein unerbittlich-ehern Schlo\u00df geh\u00e4ngt;", "tokens": ["Ein", "un\u00b7er\u00b7bitt\u00b7lich\u00b7e\u00b7hern", "Schlo\u00df", "ge\u00b7h\u00e4ngt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Da jeder von des Reiches Kammerknechten", "tokens": ["Da", "je\u00b7der", "von", "des", "Rei\u00b7ches", "Kam\u00b7mer\u00b7knech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein Judenzeichen samt der Kalle trug,", "tokens": ["Sein", "Ju\u00b7den\u00b7zei\u00b7chen", "samt", "der", "Kal\u00b7le", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und da der Junkherr mit der kecken Rechten", "tokens": ["Und", "da", "der", "Junk\u00b7herr", "mit", "der", "ke\u00b7cken", "Rech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Straflos in des Ebr\u00e4ers Antlitz schlug!", "tokens": ["Straf\u00b7los", "in", "des", "Eb\u00b7r\u00e4\u00b7ers", "Ant\u00b7litz", "schlug", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Sie sind dahin, die vielgeschm\u00e4lten Tage,", "tokens": ["Sie", "sind", "da\u00b7hin", ",", "die", "viel\u00b7ge\u00b7schm\u00e4l\u00b7ten", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Bl\u00e4ttlein hat schon leise sich gewandt,", "tokens": ["Das", "Bl\u00e4tt\u00b7lein", "hat", "schon", "lei\u00b7se", "sich", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Juda ringt uns unter ew'ger Klage", "tokens": ["Und", "Ju\u00b7da", "ringt", "uns", "un\u00b7ter", "ew'\u00b7ger", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Listig das Heft aus ungeschickter Hand.", "tokens": ["Lis\u00b7tig", "das", "Heft", "aus", "un\u00b7ge\u00b7schick\u00b7ter", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.9": {"line.1": {"text": "Emanzipiert, wie Ihr es einst verrammelt,", "tokens": ["E\u00b7man\u00b7zi\u00b7piert", ",", "wie", "Ihr", "es", "einst", "ver\u00b7ram\u00b7melt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dies z\u00e4he Volk, die Mode wechselt ja;", "tokens": ["Dies", "z\u00e4\u00b7he", "Volk", ",", "die", "Mo\u00b7de", "wech\u00b7selt", "ja", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "$,", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es hat schon l\u00e4ngst zu Haufen sich gesammelt", "tokens": ["Es", "hat", "schon", "l\u00e4ngst", "zu", "Hau\u00b7fen", "sich", "ge\u00b7sam\u00b7melt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "NN", "PRF", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und steht als Macht, Euch gegen\u00fcber, da.", "tokens": ["Und", "steht", "als", "Macht", ",", "Euch", "ge\u00b7gen\u00b7\u00fc\u00b7ber", ",", "da", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "NN", "$,", "PPER", "PTKVZ", "$,", "KOUS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Den Landmann dr\u00e4ngt es hart aus seinem Sitze,", "tokens": ["Den", "Land\u00b7mann", "dr\u00e4ngt", "es", "hart", "aus", "sei\u00b7nem", "Sit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Kr\u00e4mer scheucht es von dem Markte fort,", "tokens": ["Den", "Kr\u00e4\u00b7mer", "scheucht", "es", "von", "dem", "Mark\u00b7te", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und halb um Gold, und halb mit Sklavenwitze", "tokens": ["Und", "halb", "um", "Gold", ",", "und", "halb", "mit", "Skla\u00b7ven\u00b7wit\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NN", "$,", "KON", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kauft es dem Zeitgeist ab sein Losungswort.", "tokens": ["Kauft", "es", "dem", "Zeit\u00b7geist", "ab", "sein", "Lo\u00b7sungs\u00b7wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Wi\u00dft Ihr, wie tief sein Zauber schon gedrungen?", "tokens": ["Wi\u00dft", "Ihr", ",", "wie", "tief", "sein", "Zau\u00b7ber", "schon", "ge\u00b7drun\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADJD", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schaut um, die ihr von Menschenrechten tr\u00e4umt;", "tokens": ["Schaut", "um", ",", "die", "ihr", "von", "Men\u00b7schen\u00b7rech\u00b7ten", "tr\u00e4umt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie reden drein mit den metallnen Zungen,", "tokens": ["Sie", "re\u00b7den", "drein", "mit", "den", "me\u00b7tall\u00b7nen", "Zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo scheu der Christ verstummt und zagt und s\u00e4umt.", "tokens": ["Wo", "scheu", "der", "Christ", "ver\u00b7stummt", "und", "zagt", "und", "s\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVPP", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Was kann dem Stamm Emanzipieren frommen,", "tokens": ["Was", "kann", "dem", "Stamm", "E\u00b7man\u00b7zi\u00b7pie\u00b7ren", "from\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "NE", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der nie vom Schacher sich emanzipiert?", "tokens": ["Der", "nie", "vom", "Scha\u00b7cher", "sich", "e\u00b7man\u00b7zi\u00b7piert", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Was Ihr ihm schenken wollt hat er genommen,", "tokens": ["Was", "Ihr", "ihm", "schen\u00b7ken", "wollt", "hat", "er", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVINF", "VMFIN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dieweil Ihr u