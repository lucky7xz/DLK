{"textgrid.poem.52941": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber fremde Gr\u00e4ber und Leichensteine", "tokens": ["\u00dc\u00b7ber", "frem\u00b7de", "Gr\u00e4\u00b7ber", "und", "Lei\u00b7chen\u00b7stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Schreit ich allein im Abendscheine.", "tokens": ["Schreit", "ich", "al\u00b7lein", "im", "A\u00b7bend\u00b7schei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hab ich die Schl\u00e4fer drunten gest\u00f6rt?", "tokens": ["Hab", "ich", "die", "Schl\u00e4\u00b7fer", "drun\u00b7ten", "ge\u00b7st\u00f6rt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Haben sie mein fragend' Wort geh\u00f6rt?", "tokens": ["Ha\u00b7ben", "sie", "mein", "fra\u00b7gend'", "Wort", "ge\u00b7h\u00f6rt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Mir ist, als k\u00f6nnt ich in s\u00fc\u00dfem Grauen", "tokens": ["Mir", "ist", ",", "als", "k\u00f6nnt", "ich", "in", "s\u00fc\u00b7\u00dfem", "Grau\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Durch Schollen und S\u00e4rge hinunterschauen", "tokens": ["Durch", "Schol\u00b7len", "und", "S\u00e4r\u00b7ge", "hin\u00b7un\u00b7ter\u00b7schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVINF"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Mitten hinein in die stille Stadt,", "tokens": ["Mit\u00b7ten", "hin\u00b7ein", "in", "die", "stil\u00b7le", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Wo alles Reisen sein Ende hat.", "tokens": ["Wo", "al\u00b7les", "Rei\u00b7sen", "sein", "En\u00b7de", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie vieles Leid, wie viele Trauer", "tokens": ["Wie", "vie\u00b7les", "Leid", ",", "wie", "vie\u00b7le", "Trau\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PIS", "NN", "$,", "PWAV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Innerhalb jener engen Mauer!", "tokens": ["In\u00b7ner\u00b7halb", "je\u00b7ner", "en\u00b7gen", "Mau\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Hinter der eisernen Gittert\u00fcr", "tokens": ["Hin\u00b7ter", "der", "ei\u00b7ser\u00b7nen", "Git\u00b7ter\u00b7t\u00fcr"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Wie manche Gebete, Gel\u00fcbd' und Schw\u00fcr!", "tokens": ["Wie", "man\u00b7che", "Ge\u00b7be\u00b7te", ",", "Ge\u00b7l\u00fcbd'", "und", "Schw\u00fcr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Ach! der menschlichen Liebe ist nirgends so viele,", "tokens": ["Ach", "!", "der", "menschli\u00b7chen", "Lie\u00b7be", "ist", "nir\u00b7gends", "so", "vie\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "ADJA", "NN", "VAFIN", "ADV", "ADV", "PIS", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Als hier am letzten Wanderziele;", "tokens": ["Als", "hier", "am", "letz\u00b7ten", "Wan\u00b7der\u00b7zie\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihre Rosen und Dornen streuet sie mild", "tokens": ["Ih\u00b7re", "Ro\u00b7sen", "und", "Dor\u00b7nen", "streu\u00b7et", "sie", "mild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN", "PPER", "ADJD"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "\u00dcber das tr\u00e4nenreiche Gefild.", "tokens": ["\u00dc\u00b7ber", "das", "tr\u00e4\u00b7nen\u00b7rei\u00b7che", "Ge\u00b7fild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Nur nicht ohne Liebe allein verderben,", "tokens": ["Nur", "nicht", "oh\u00b7ne", "Lie\u00b7be", "al\u00b7lein", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Nur nicht in der Fremde siechen und sterben,", "tokens": ["Nur", "nicht", "in", "der", "Frem\u00b7de", "sie\u00b7chen", "und", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Von Mietlingshand gehegt und gepflegt,", "tokens": ["Von", "Miet\u00b7lings\u00b7hand", "ge\u00b7hegt", "und", "ge\u00b7pflegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mit offenem Aug in den Sarg gelegt!", "tokens": ["Mit", "of\u00b7fe\u00b7nem", "Aug", "in", "den", "Sarg", "ge\u00b7legt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Und sollt ich sie lebend nicht wiedersehen,", "tokens": ["Und", "sollt", "ich", "sie", "le\u00b7bend", "nicht", "wie\u00b7der\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "ADJD", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Heimat, so m\u00f6cht ich drin sterben gehen", "tokens": ["Die", "Hei\u00b7mat", ",", "so", "m\u00f6cht", "ich", "drin", "ster\u00b7ben", "ge\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "VVINF"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und ruhen bei meinem M\u00fctterlein, \u2013", "tokens": ["Und", "ru\u00b7hen", "bei", "mei\u00b7nem", "M\u00fct\u00b7ter\u00b7lein", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nur nicht in der Fremde, nur nicht allein!", "tokens": ["Nur", "nicht", "in", "der", "Frem\u00b7de", ",", "nur", "nicht", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "ART", "NN", "$,", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}