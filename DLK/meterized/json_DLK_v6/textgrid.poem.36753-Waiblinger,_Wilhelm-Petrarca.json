{"textgrid.poem.36753": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Petrarca", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist dir die Liebe der Faden, woraus das Weltall gesponnen,", "tokens": ["Ist", "dir", "die", "Lie\u00b7be", "der", "Fa\u00b7den", ",", "wo\u00b7raus", "das", "Wel\u00b7tall", "ge\u00b7spon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der alles Wesen und selbst Gr\u00e4schen und Sterne verkn\u00fcpft,", "tokens": ["Der", "al\u00b7les", "We\u00b7sen", "und", "selbst", "Gr\u00e4sc\u00b7hen", "und", "Ster\u00b7ne", "ver\u00b7kn\u00fcpft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "KON", "ADV", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dann in Francesco vielleicht hast du den Dichter gefunden,", "tokens": ["Dann", "in", "Fran\u00b7ce\u00b7sco", "viel\u00b7leicht", "hast", "du", "den", "Dich\u00b7ter", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "--+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der das geheime Gespinnst bis an das Ende verfolgt,", "tokens": ["Der", "das", "ge\u00b7hei\u00b7me", "Ge\u00b7spinnst", "bis", "an", "das", "En\u00b7de", "ver\u00b7folgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "APPR", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Aber ich sorg', es hanget die Welt noch an anderen Dingen,", "tokens": ["A\u00b7ber", "ich", "sor\u00b7g'", ",", "es", "han\u00b7get", "die", "Welt", "noch", "an", "an\u00b7de\u00b7ren", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Und die Lieb' nicht allein gab ihr Bewegung und Sein.", "tokens": ["Und", "die", "Lieb'", "nicht", "al\u00b7lein", "gab", "ihr", "Be\u00b7we\u00b7gung", "und", "Sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "ADV", "VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Darum mag's dich verdrie\u00dfen, wenn Laura nur wie zum Halsschmuck", "tokens": ["Da\u00b7rum", "mag's", "dich", "ver\u00b7drie\u00b7\u00dfen", ",", "wenn", "Lau\u00b7ra", "nur", "wie", "zum", "Hals\u00b7schmuck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PRF", "VVINF", "$,", "KOUS", "NE", "ADV", "KOKOM", "APPRART", "NN"], "meter": "--+--+--+--+-+-", "measure": "anapaest.tetra.plus"}, "line.8": {"text": "Himmel und Erde Petrarc's schw\u00e4rmender Sinnlichkeit tr\u00e4gt;", "tokens": ["Him\u00b7mel", "und", "Er\u00b7de", "Pe\u00b7tra\u00b7rc's", "schw\u00e4r\u00b7men\u00b7der", "Sinn\u00b7lich\u00b7keit", "tr\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "NE", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+--+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Oder es langweilt dich: er leiht dir die magische Brille,", "tokens": ["O\u00b7der", "es", "lang\u00b7weilt", "dich", ":", "er", "leiht", "dir", "die", "ma\u00b7gi\u00b7sche", "Bril\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "Aber dein k\u00e4lterer Sinn siehet zuletzt nur ein Weib.", "tokens": ["A\u00b7ber", "dein", "k\u00e4l\u00b7te\u00b7rer", "Sinn", "sie\u00b7het", "zu\u00b7letzt", "nur", "ein", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}}}}}