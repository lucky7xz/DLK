{"textgrid.poem.24905": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Serenade", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leise, um dich nicht zu wecken,", "tokens": ["Lei\u00b7se", ",", "um", "dich", "nicht", "zu", "we\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "PRF", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rauscht der Nachtwind, teure Frau;", "tokens": ["Rauscht", "der", "Nacht\u00b7wind", ",", "teu\u00b7re", "Frau", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Leise in das Marmorbecken", "tokens": ["Lei\u00b7se", "in", "das", "Mar\u00b7mor\u00b7be\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gie\u00dft der Brunnen seinen Tau.", "tokens": ["Gie\u00dft", "der", "Brun\u00b7nen", "sei\u00b7nen", "Tau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie das Wasser, niedertropfend,", "tokens": ["Wie", "das", "Was\u00b7ser", ",", "nie\u00b7der\u00b7trop\u00b7fend", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kreise neben Kreisen zieht,", "tokens": ["Krei\u00b7se", "ne\u00b7ben", "Krei\u00b7sen", "zieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Also zittert, leise klopfend,", "tokens": ["Al\u00b7so", "zit\u00b7tert", ",", "lei\u00b7se", "klop\u00b7fend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir das Herz bei diesem Lied.", "tokens": ["Mir", "das", "Herz", "bei", "die\u00b7sem", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schwingt euch, T\u00f6ne meiner Zither,", "tokens": ["Schwingt", "euch", ",", "T\u00f6\u00b7ne", "mei\u00b7ner", "Zi\u00b7ther", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwingt euch aufw\u00e4rts, fl\u00fcgelleicht;", "tokens": ["Schwingt", "euch", "auf\u00b7w\u00e4rts", ",", "fl\u00fc\u00b7gel\u00b7leicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch das rebumkr\u00e4nzte Gitter", "tokens": ["Durch", "das", "re\u00b7bum\u00b7kr\u00e4nz\u00b7te", "Git\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Sch\u00f6nen Kammer schleicht!", "tokens": ["In", "der", "Sch\u00f6\u00b7nen", "Kam\u00b7mer", "schleicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbist denn, liebliche Dolores,\u00ab \u2013", "tokens": ["\u00bb", "ist", "denn", ",", "lieb\u00b7li\u00b7che", "Do\u00b7lo\u00b7res", ",", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "VAFIN", "ADV", "$,", "ADJA", "NN", "$,", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Also singt in ihren Traum \u2013", "tokens": ["Al\u00b7so", "singt", "in", "ih\u00b7ren", "Traum", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbin der Muschel deines Ohres", "tokens": ["\u00bb", "in", "der", "Mu\u00b7schel", "dei\u00b7nes", "Oh\u00b7res"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr kein Perlenw\u00f6rtchen Raum?", "tokens": ["F\u00fcr", "kein", "Per\u00b7len\u00b7w\u00f6rt\u00b7chen", "Raum", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Denk der Laube, dicht vergittert,", "tokens": ["Denk", "der", "Lau\u00b7be", ",", "dicht", "ver\u00b7git\u00b7tert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo, umrankt von Duftgestr\u00e4uch,", "tokens": ["Wo", ",", "um\u00b7rankt", "von", "Duft\u00b7ge\u00b7str\u00e4uch", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihr in Seligkeit gezittert,", "tokens": ["Ihr", "in", "Se\u00b7lig\u00b7keit", "ge\u00b7zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie die Bl\u00e4tter \u00fcber euch!", "tokens": ["Wie", "die", "Bl\u00e4t\u00b7ter", "\u00fc\u00b7ber", "euch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "War der Platz doch still und sicher,", "tokens": ["War", "der", "Platz", "doch", "still", "und", "si\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und kein Zeuge hat gelauscht;", "tokens": ["Und", "kein", "Zeu\u00b7ge", "hat", "ge\u00b7lauscht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Selten, da\u00df ein abendlicher", "tokens": ["Sel\u00b7ten", ",", "da\u00df", "ein", "a\u00b7bend\u00b7li\u00b7cher"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vogel durch das Laub gerauscht.", "tokens": ["Vo\u00b7gel", "durch", "das", "Laub", "ge\u00b7rauscht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "O dem Freund noch eine Stunde,", "tokens": ["O", "dem", "Freund", "noch", "ei\u00b7ne", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo dein Arm ihn so umschlingt", "tokens": ["Wo", "dein", "Arm", "ihn", "so", "um\u00b7schlingt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Ku\u00df von deinem Munde", "tokens": ["Und", "der", "Ku\u00df", "von", "dei\u00b7nem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Feurig bis ans Herz ihm dringt!", "tokens": ["Feu\u00b7rig", "bis", "ans", "Herz", "ihm", "dringt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Hast du ihn so ganz vergessen?", "tokens": ["Hast", "du", "ihn", "so", "ganz", "ver\u00b7ges\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einsam harrt er am Balkon;", "tokens": ["Ein\u00b7sam", "harrt", "er", "am", "Bal\u00b7kon", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Ueberm Wipfel der Cypressen", "tokens": ["Ue\u00b7berm", "Wip\u00b7fel", "der", "Cyp\u00b7res\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Bleicht des Mondes Sichel schon.", "tokens": ["Bleicht", "des", "Mon\u00b7des", "Si\u00b7chel", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NE", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wie das Wasser, niedertropfend,", "tokens": ["Wie", "das", "Was\u00b7ser", ",", "nie\u00b7der\u00b7trop\u00b7fend", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kreise neben Kreisen zieht,", "tokens": ["Krei\u00b7se", "ne\u00b7ben", "Krei\u00b7sen", "zieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Also zittert, leise klopfend,", "tokens": ["Al\u00b7so", "zit\u00b7tert", ",", "lei\u00b7se", "klop\u00b7fend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihm das Herz bei diesem Lied.\u00ab", "tokens": ["Ihm", "das", "Herz", "bei", "die\u00b7sem", "Lied", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "PDAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}