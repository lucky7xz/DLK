{"textgrid.poem.25555": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie sehnt' ich mich, in deinem Arm' zu schlafen!", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sehnt' ich mich, in deinem Arm' zu schlafen!", "tokens": ["Wie", "sehnt'", "ich", "mich", ",", "in", "dei\u00b7nem", "Arm'", "zu", "schla\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$,", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Froh sah ich dir, wie ", "tokens": ["Froh", "sah", "ich", "dir", ",", "wie"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "$,", "PWAV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Denn alle deine Pfeile, trafen", "tokens": ["Denn", "al\u00b7le", "dei\u00b7ne", "Pfei\u00b7le", ",", "tra\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PIS", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich, Gramvollender! nicht.", "tokens": ["Mich", ",", "Gram\u00b7voll\u00b7en\u00b7der", "!", "nicht", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "NN", "$.", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O! m\u00f6chte mich die Ruhe noch umschweben,", "tokens": ["O", "!", "m\u00f6ch\u00b7te", "mich", "die", "Ru\u00b7he", "noch", "um\u00b7schwe\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VMFIN", "PRF", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So h\u00e4tt' ich nun vielleicht die Todesnacht", "tokens": ["So", "h\u00e4tt'", "ich", "nun", "viel\u00b7leicht", "die", "To\u00b7des\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Verschlummert, w\u00e4r' ins bessre Leben", "tokens": ["Ver\u00b7schlum\u00b7mert", ",", "w\u00e4r'", "ins", "bess\u00b7re", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Schon wieder aufgewacht.", "tokens": ["Schon", "wie\u00b7der", "auf\u00b7ge\u00b7wacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Nun mu\u00df ich noch sie schlummern? mu\u00df die Erde", "tokens": ["Nun", "mu\u00df", "ich", "noch", "sie", "schlum\u00b7mern", "?", "mu\u00df", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$.", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Nun noch einmal im Fr\u00fchlingsglanze sehn?", "tokens": ["Nun", "noch", "ein\u00b7mal", "im", "Fr\u00fch\u00b7lings\u00b7glan\u00b7ze", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und f\u00fchlen, da\u00df ich Armer werde", "tokens": ["Und", "f\u00fch\u00b7len", ",", "da\u00df", "ich", "Ar\u00b7mer", "wer\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "NN", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Bald hin ins Dunkle gehn?", "tokens": ["Bald", "hin", "ins", "Dunk\u00b7le", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Weit warst du noch von meinem Rosengange,", "tokens": ["Weit", "warst", "du", "noch", "von", "mei\u00b7nem", "Ro\u00b7sen\u00b7gan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Beschneytes Ziel, wo krumm das Alter ruht;", "tokens": ["Be\u00b7schney\u00b7tes", "Ziel", ",", "wo", "krumm", "das", "Al\u00b7ter", "ruht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Schon aber saugt die Todesschlange", "tokens": ["Schon", "a\u00b7ber", "saugt", "die", "To\u00b7des\u00b7schlan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Mir aus das Bischen Blut.", "tokens": ["Mir", "aus", "das", "Bi\u00b7schen", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Wo bleiben nun die guten Thaten alle,", "tokens": ["Wo", "blei\u00b7ben", "nun", "die", "gu\u00b7ten", "Tha\u00b7ten", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "ADJA", "NN", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Worauf hinaus die Augen weinend sahn?", "tokens": ["Wo\u00b7rauf", "hin\u00b7aus", "die", "Au\u00b7gen", "wei\u00b7nend", "sahn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APZR", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Ich h\u00e4tte sie gethan, und falle,", "tokens": ["Ich", "h\u00e4t\u00b7te", "sie", "ge\u00b7than", ",", "und", "fal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Und habe nichts gethan.", "tokens": ["Und", "ha\u00b7be", "nichts", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Wird nun mein Haupt den Kranz von Myrthen tragen,", "tokens": ["Wird", "nun", "mein", "Haupt", "den", "Kranz", "von", "Myr\u00b7then", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Den mir im Traum' die Hand der Ehre gab?", "tokens": ["Den", "mir", "im", "Traum'", "die", "Hand", "der", "Eh\u00b7re", "gab", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "O! Niemand wird mit Thr\u00e4nen fragen:", "tokens": ["O", "!", "Nie\u00b7mand", "wird", "mit", "Thr\u00e4\u00b7nen", "fra\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PIS", "VAFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Wo ist des Mannes Grab?", "tokens": ["Wo", "ist", "des", "Man\u00b7nes", "Grab", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Zehn tausendmal k\u00f6nnt' ich spatzieren gehen", "tokens": ["Zehn", "tau\u00b7send\u00b7mal", "k\u00f6nnt'", "ich", "spat\u00b7zie\u00b7ren", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "ADV", "VMFIN", "PPER", "VVINF", "VVINF"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "In deinem Garten, Weisheit! k\u00f6nnte da", "tokens": ["In", "dei\u00b7nem", "Gar\u00b7ten", ",", "Weis\u00b7heit", "!", "k\u00f6nn\u00b7te", "da"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "$.", "VMFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Mehr volle Blumenbeete sehen,", "tokens": ["Mehr", "vol\u00b7le", "Blu\u00b7men\u00b7bee\u00b7te", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Als ", "tokens": ["Als"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.29": {"text": "Ich k\u00f6nnte noch \u2013 und bin nun schon am Ende! \u2013", "tokens": ["Ich", "k\u00f6nn\u00b7te", "noch", "\u2013", "und", "bin", "nun", "schon", "am", "En\u00b7de", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$(", "KON", "VAFIN", "ADV", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Natur, in deiner Bildergallerie", "tokens": ["Na\u00b7tur", ",", "in", "dei\u00b7ner", "Bil\u00b7der\u00b7gal\u00b7le\u00b7rie"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Nach Wundern suchen, ach! und f\u00e4nde", "tokens": ["Nach", "Wun\u00b7dern", "su\u00b7chen", ",", "ach", "!", "und", "f\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVINF", "$,", "ITJ", "$.", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Das letzte Wunder nie.", "tokens": ["Das", "letz\u00b7te", "Wun\u00b7der", "nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Bald werd' ich nun von Philomelens T\u00f6nen", "tokens": ["Bald", "werd'", "ich", "nun", "von", "Phi\u00b7lo\u00b7me\u00b7lens", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Nicht mehr geweckt; um meinen Gartensaal", "tokens": ["Nicht", "mehr", "ge\u00b7weckt", ";", "um", "mei\u00b7nen", "Gar\u00b7ten\u00b7saal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VVPP", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Verbl\u00fchen schon die Tausendsch\u00f6nen", "tokens": ["Ver\u00b7bl\u00fc\u00b7hen", "schon", "die", "Tau\u00b7send\u00b7sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "F\u00fcr mich zum letzten mal.", "tokens": ["F\u00fcr", "mich", "zum", "letz\u00b7ten", "mal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "ADJA", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "Bald ruft dein Mund in Trillern an dem Fl\u00fcgel,", "tokens": ["Bald", "ruft", "dein", "Mund", "in", "Tril\u00b7lern", "an", "dem", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Amalia! den Kenner zum Geh\u00f6r',", "tokens": ["A\u00b7ma\u00b7lia", "!", "den", "Ken\u00b7ner", "zum", "Ge\u00b7h\u00f6r'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.39": {"text": "Nur ich lieg' unter meinem H\u00fcgel", "tokens": ["Nur", "ich", "lieg'", "un\u00b7ter", "mei\u00b7nem", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Und h\u00f6re dich nicht mehr.", "tokens": ["Und", "h\u00f6\u00b7re", "dich", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.41": {"text": "Und alles das um eines M\u00e4dchens willen?", "tokens": ["Und", "al\u00b7les", "das", "um", "ei\u00b7nes", "M\u00e4d\u00b7chens", "wil\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "O welchen Plan vernichtet sie mit mir!", "tokens": ["O", "wel\u00b7chen", "Plan", "ver\u00b7nich\u00b7tet", "sie", "mit", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAT", "NN", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Was konnte meine Fl\u00fcche stillen?", "tokens": ["Was", "konn\u00b7te", "mei\u00b7ne", "Fl\u00fc\u00b7che", "stil\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Warum vergab ich ihr?", "tokens": ["Wa\u00b7rum", "ver\u00b7gab", "ich", "ihr", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.45": {"text": "Kann sie mich itzt aus meinem Kerker retten?", "tokens": ["Kann", "sie", "mich", "itzt", "aus", "mei\u00b7nem", "Ker\u00b7ker", "ret\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Erst mache sie geschehnes, ungeschehn,", "tokens": ["Erst", "ma\u00b7che", "sie", "ge\u00b7scheh\u00b7nes", ",", "un\u00b7ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Sonst mu\u00df sie mich in ihren Ketten", "tokens": ["Sonst", "mu\u00df", "sie", "mich", "in", "ih\u00b7ren", "Ket\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Zum Tode schleppen sehn.", "tokens": ["Zum", "To\u00b7de", "schlep\u00b7pen", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.49": {"text": "Mu\u00df ich denn fort? Jenseit des Grabes lieget", "tokens": ["Mu\u00df", "ich", "denn", "fort", "?", "Jen\u00b7seit", "des", "Gra\u00b7bes", "lie\u00b7get"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "PTKVZ", "$.", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.50": {"text": "Noch eine Welt, allein wer w\u00fcnscht sie sich?", "tokens": ["Noch", "ei\u00b7ne", "Welt", ",", "al\u00b7lein", "wer", "w\u00fcnscht", "sie", "sich", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "PWS", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.51": {"text": "Fest, wie an eine Braut geschmieget,", "tokens": ["Fest", ",", "wie", "an", "ei\u00b7ne", "Braut", "ge\u00b7schmie\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Schmieg' ich an diese mich.", "tokens": ["Schmieg'", "ich", "an", "die\u00b7se", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PDAT", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.53": {"text": "Doch ach! umsonst l\u00e4\u00dft mich der Tod noch weinen;", "tokens": ["Doch", "ach", "!", "um\u00b7sonst", "l\u00e4\u00dft", "mich", "der", "Tod", "noch", "wei\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "ADV", "VVFIN", "PRF", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.54": {"text": "Du aber, die du mich itzt nicht mehr kennst,", "tokens": ["Du", "a\u00b7ber", ",", "die", "du", "mich", "itzt", "nicht", "mehr", "kennst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "PPER", "PRF", "ADV", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.55": {"text": "Frohlocke nicht; bald werd' ich dir erscheinen", "tokens": ["Froh\u00b7lo\u00b7cke", "nicht", ";", "bald", "werd'", "ich", "dir", "er\u00b7schei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$.", "ADV", "VAFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.56": {"text": "Als schreckendes Gespenst!", "tokens": ["Als", "schre\u00b7cken\u00b7des", "Ge\u00b7spenst", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}