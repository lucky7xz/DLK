{"textgrid.poem.55426": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "6", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Osten will das holde Licht", "tokens": ["Von", "Os\u00b7ten", "will", "das", "hol\u00b7de", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun gl\u00e4nzend uns vereinen,", "tokens": ["Nun", "gl\u00e4n\u00b7zend", "uns", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sch\u00f6nre Stunden f\u00e4nd es nicht,", "tokens": ["Und", "sch\u00f6n\u00b7re", "Stun\u00b7den", "f\u00e4nd", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als diesem Tag zu scheinen.", "tokens": ["Als", "die\u00b7sem", "Tag", "zu", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}