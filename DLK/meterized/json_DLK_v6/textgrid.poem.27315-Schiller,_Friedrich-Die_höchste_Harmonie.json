{"textgrid.poem.27315": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die h\u00f6chste Harmonie", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00d6dipus rei\u00dft die Augen sich aus, Jokasta erhenkt sich,", "tokens": ["\u00d6\u00b7di\u00b7pus", "rei\u00dft", "die", "Au\u00b7gen", "sich", "aus", ",", "Jo\u00b7kas\u00b7ta", "er\u00b7henkt", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$,", "NE", "VVFIN", "PRF", "$,"], "meter": "+--+-+--+-++-+-", "measure": "iambic.septa.invert"}, "line.2": {"text": "Beide schuldlos; das St\u00fcck hat sich harmonisch gel\u00f6st.", "tokens": ["Bei\u00b7de", "schuld\u00b7los", ";", "das", "St\u00fcck", "hat", "sich", "har\u00b7mo\u00b7nisch", "ge\u00b7l\u00f6st", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "$.", "ART", "NN", "VAFIN", "PRF", "ADJD", "VVPP", "$."], "meter": "---+-+--+-+-+", "measure": "iambic.penta.relaxed"}}}}}