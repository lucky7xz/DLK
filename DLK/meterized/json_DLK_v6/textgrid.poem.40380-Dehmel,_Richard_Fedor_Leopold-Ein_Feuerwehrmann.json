{"textgrid.poem.40380": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Ein Feuerwehrmann", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann Feuerwehrmann werden;", "tokens": ["Ich", "kann", "Feu\u00b7er\u00b7wehr\u00b7mann", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "kaum da\u00df die Brandflamme prasselt,", "tokens": ["kaum", "da\u00df", "die", "Brand\u00b7flam\u00b7me", "pras\u00b7selt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.3": {"text": "kommt schon galopp mit Fackelgespr\u00fch", "tokens": ["kommt", "schon", "ga\u00b7lopp", "mit", "Fa\u00b7ckel\u00b7ge\u00b7spr\u00fch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJD", "APPR", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "unser Wagen angerasselt,", "tokens": ["un\u00b7ser", "Wa\u00b7gen", "an\u00b7ge\u00b7ras\u00b7selt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "alle Mann wie auf Sprungfedern stehend.", "tokens": ["al\u00b7le", "Mann", "wie", "auf", "Sprung\u00b7fe\u00b7dern", "ste\u00b7hend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KOKOM", "APPR", "NN", "VVPP", "$."], "meter": "+-+-++--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie mit Donner und Blitz um die Wette:", "tokens": ["Wie", "mit", "Don\u00b7ner", "und", "Blitz", "um", "die", "Wet\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "unsre Fackeln sind Rettungszeichen!", "tokens": ["uns\u00b7re", "Fa\u00b7ckeln", "sind", "Ret\u00b7tungs\u00b7zei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Meine Pfeife gellt: beiseit, beiseit!", "tokens": ["Mei\u00b7ne", "Pfei\u00b7fe", "gellt", ":", "bei\u00b7seit", ",", "bei\u00b7seit", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$.", "ADV", "$,", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "und alle Menschen weichen", "tokens": ["und", "al\u00b7le", "Men\u00b7schen", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "uns voll Ehrfurcht aus.", "tokens": ["uns", "voll", "Ehr\u00b7furcht", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Denn dort die gl\u00fchenden Fenster \u2013", "tokens": ["Denn", "dort", "die", "gl\u00fc\u00b7hen\u00b7den", "Fens\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "horch: durch den Qualm ein Schrei!", "tokens": ["horch", ":", "durch", "den", "Qualm", "ein", "Schrei", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da wird nicht lange gefackelt mehr:", "tokens": ["Da", "wird", "nicht", "lan\u00b7ge", "ge\u00b7fa\u00b7ckelt", "mehr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "ADV", "VVPP", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "rasch den Rauchhelm auf, die Spritzen in Reih,", "tokens": ["rasch", "den", "Rauch\u00b7helm", "auf", ",", "die", "Sprit\u00b7zen", "in", "Reih", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PTKVZ", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "und mit Beil und Seil wird gerettet!", "tokens": ["und", "mit", "Beil", "und", "Seil", "wird", "ge\u00b7ret\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "VAFIN", "VVPP", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Vielleicht ein sch\u00f6nes M\u00e4dchen;", "tokens": ["Viel\u00b7leicht", "ein", "sch\u00f6\u00b7nes", "M\u00e4d\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "das wird dann meine Braut.", "tokens": ["das", "wird", "dann", "mei\u00b7ne", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Oder ein kleiner Betteljunge;", "tokens": ["O\u00b7der", "ein", "klei\u00b7ner", "Bet\u00b7tel\u00b7jun\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "der schie\u00dft dann wie ich ins Kraut", "tokens": ["der", "schie\u00dft", "dann", "wie", "ich", "ins", "Kraut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "VVFIN", "ADV", "KOKOM", "PPER", "APPRART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "und wird ein kleiner Held.", "tokens": ["und", "wird", "ein", "klei\u00b7ner", "Held", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}