{"dta.poem.13830": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Soldatenlied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Die jungen J\u00e4ger sich zeigen", "tokens": ["Die", "jun\u00b7gen", "J\u00e4\u00b7ger", "sich", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PRF", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dort dr\u00fcben im gr\u00fcnen Wald", "tokens": ["Dort", "dr\u00fc\u00b7ben", "im", "gr\u00fc\u00b7nen", "Wald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bald schimmernd zwischen den Zweigen,", "tokens": ["Bald", "schim\u00b7mernd", "zwi\u00b7schen", "den", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bald lauernd im Hinterhalt.", "tokens": ["Bald", "lau\u00b7ernd", "im", "Hin\u00b7ter\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wohl sinkt da in ewiges Schweigen", "tokens": ["Wohl", "sinkt", "da", "in", "e\u00b7wi\u00b7ges", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Manch' schlanke Rittergestalt,", "tokens": ["Man\u00b7ch'", "schlan\u00b7ke", "Rit\u00b7ter\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Die anderen \u00fcber ihn steigen,", "tokens": ["Die", "an\u00b7de\u00b7ren", "\u00fc\u00b7ber", "ihn", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Hurrah! in dem sch\u00f6nen Wald,", "tokens": ["Hur\u00b7rah", "!", "in", "dem", "sch\u00f6\u00b7nen", "Wald", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.9": {"text": "\u201ees funkelt das Blau durch die B\u00e4ume \u2014", "tokens": ["\u201e", "es", "fun\u00b7kelt", "das", "Blau", "durch", "die", "B\u00e4u\u00b7me"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Ach, Vater, ich komme bald!\u201c", "tokens": ["Ach", ",", "Va\u00b7ter", ",", "ich", "kom\u00b7me", "bald", "!", "\u201c"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "PPER", "VVFIN", "ADV", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}