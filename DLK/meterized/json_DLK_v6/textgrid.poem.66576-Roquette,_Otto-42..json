{"textgrid.poem.66576": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Widerspr\u00fcche, wohl ist's wahr,", "tokens": ["Wi\u00b7der\u00b7spr\u00fc\u00b7che", ",", "wohl", "ist's", "wahr", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Stehn hier viel in Reih und Gliedern.", "tokens": ["Stehn", "hier", "viel", "in", "Reih", "und", "Glie\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Scheltet drum nicht wandelbar", "tokens": ["Schel\u00b7tet", "drum", "nicht", "wan\u00b7del\u00b7bar"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PAV", "PTKNEG", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sinn und Geist in diesen Liedern!", "tokens": ["Sinn", "und", "Geist", "in", "die\u00b7sen", "Lie\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Widersprechend auch bewegt", "tokens": ["Wi\u00b7der\u00b7spre\u00b7chend", "auch", "be\u00b7wegt"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich die Welt im Wirbeltanze,", "tokens": ["Sich", "die", "Welt", "im", "Wir\u00b7bel\u00b7tan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und so spiegelt, und so regt", "tokens": ["Und", "so", "spie\u00b7gelt", ",", "und", "so", "regt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "$,", "KON", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem Kleinsten sich das Ganze.", "tokens": ["In", "dem", "Kleins\u00b7ten", "sich", "das", "Gan\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}