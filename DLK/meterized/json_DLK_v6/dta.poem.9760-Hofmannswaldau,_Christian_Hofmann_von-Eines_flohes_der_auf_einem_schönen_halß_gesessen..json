{"dta.poem.9760": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines flohes/ der auf einem sch\u00f6nen hal\u00df  \n gesessen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weil ich zu geitzig war zu kosten von der speise/", "tokens": ["Weil", "ich", "zu", "geit\u00b7zig", "war", "zu", "kos\u00b7ten", "von", "der", "spei\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "ADJD", "VAFIN", "APPR", "ADJA", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die den verliebten offt so grossen hunger macht;", "tokens": ["Die", "den", "ver\u00b7lieb\u00b7ten", "offt", "so", "gros\u00b7sen", "hun\u00b7ger", "macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "ADV", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So hat die s\u00fcsse kost mich um das leben bracht:", "tokens": ["So", "hat", "die", "s\u00fcs\u00b7se", "kost", "mich", "um", "das", "le\u00b7ben", "bracht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "VVFIN", "PRF", "APPR", "PDS", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Drum wer da kosten will/ lern erst die rechte weise.", "tokens": ["Drum", "wer", "da", "kos\u00b7ten", "will", "/", "lern", "erst", "die", "rech\u00b7te", "wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PWS", "ADV", "VVINF", "VMFIN", "$(", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}