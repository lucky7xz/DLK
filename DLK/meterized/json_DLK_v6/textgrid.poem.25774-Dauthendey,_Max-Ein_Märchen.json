{"textgrid.poem.25774": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Ein M\u00e4rchen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wollt ihr ein M\u00e4rchen erlauschen?", "tokens": ["Wollt", "ihr", "ein", "M\u00e4r\u00b7chen", "er\u00b7lau\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein M\u00e4rchen? \u2013 Ich wei\u00df eines.", "tokens": ["Ein", "M\u00e4r\u00b7chen", "?", "\u2013", "Ich", "wei\u00df", "ei\u00b7nes", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "PPER", "VVFIN", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O so wunderbar fein, so zart.", "tokens": ["O", "so", "wun\u00b7der\u00b7bar", "fein", ",", "so", "zart", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Wollt' ich's in Laute, in T\u00f6ne gestalten,", "tokens": ["Wollt'", "ich's", "in", "Lau\u00b7te", ",", "in", "T\u00f6\u00b7ne", "ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "W\u00e4re jeder Laut, jeder Ton zu lautt\u00f6nend.", "tokens": ["W\u00e4\u00b7re", "je\u00b7der", "Laut", ",", "je\u00b7der", "Ton", "zu", "laut\u00b7t\u00f6\u00b7nend", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$,", "PIAT", "NN", "PTKA", "ADJD", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Vorsicht! Behutsam!", "tokens": ["Vor\u00b7sicht", "!", "Be\u00b7hut\u00b7sam", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "ADJD", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Denkt leise!", "tokens": ["Denkt", "lei\u00b7se", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Sonnenfunken \u2013 goldner Hauch \u2013", "tokens": ["Son\u00b7nen\u00b7fun\u00b7ken", "\u2013", "gold\u00b7ner", "Hauch", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00f6scht ihn nicht \u2013 leise! leise!", "tokens": ["L\u00f6scht", "ihn", "nicht", "\u2013", "lei\u00b7se", "!", "lei\u00b7se", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$(", "ADJD", "$.", "ADJD", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}}, "stanza.5": {"line.1": {"text": "Ein Garten, eine Gestalt \u2013 ein M\u00e4dchen.", "tokens": ["Ein", "Gar\u00b7ten", ",", "ei\u00b7ne", "Ge\u00b7stalt", "\u2013", "ein", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$(", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Rings auf zitternden Schwingen Farben und D\u00fcfte,", "tokens": ["Rings", "auf", "zit\u00b7tern\u00b7den", "Schwin\u00b7gen", "Far\u00b7ben", "und", "D\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und mein M\u00e4dchen mitten in Farbe und Duft.", "tokens": ["Und", "mein", "M\u00e4d\u00b7chen", "mit\u00b7ten", "in", "Far\u00b7be", "und", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schwarzgr\u00fcne B\u00fcsche stumm, atemstockend,", "tokens": ["Schwarz\u00b7gr\u00fc\u00b7ne", "B\u00fc\u00b7sche", "stumm", ",", "a\u00b7tems\u00b7to\u00b7ckend", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$,", "VVPP", "$,"], "meter": "++-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Und darunter Bl\u00fctenherzen,", "tokens": ["Und", "da\u00b7run\u00b7ter", "Bl\u00fc\u00b7ten\u00b7her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PAV", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Wildrote pochende Herzen,", "tokens": ["Wild\u00b7ro\u00b7te", "po\u00b7chen\u00b7de", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Pochend in hast'gem Genie\u00dfen.", "tokens": ["Po\u00b7chend", "in", "hast'\u00b7gem", "Ge\u00b7nie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Sie singt.", "tokens": ["Sie", "singt", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Ihre Tr\u00e4ume sind ihre Lieder.", "tokens": ["Ih\u00b7re", "Tr\u00e4u\u00b7me", "sind", "ih\u00b7re", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Wei\u00dfe Astern,", "tokens": ["Wei\u00b7\u00dfe", "As\u00b7tern", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Blendende Astern,", "tokens": ["Blen\u00b7den\u00b7de", "As\u00b7tern", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wie sie sich wiegen.", "tokens": ["Wie", "sie", "sich", "wie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und der Garten singt", "tokens": ["Und", "der", "Gar\u00b7ten", "singt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "--+-+", "measure": "anapaest.init"}, "line.5": {"text": "Und die B\u00fcsche,", "tokens": ["Und", "die", "B\u00fc\u00b7sche", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Alles, alles singt in Farben und D\u00fcften.", "tokens": ["Al\u00b7les", ",", "al\u00b7les", "singt", "in", "Far\u00b7ben", "und", "D\u00fcf\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Starrst du auf Rosen,", "tokens": ["Starrst", "du", "auf", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Nimm dich in acht.", "tokens": ["Nimm", "dich", "in", "acht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "CARD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Rosen sengen, brennen,", "tokens": ["Ro\u00b7sen", "sen\u00b7gen", ",", "bren\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "VVINF", "$,", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wei\u00dft du das!", "tokens": ["Wei\u00dft", "du", "das", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Sie wei\u00df nichts.", "tokens": ["Sie", "wei\u00df", "nichts", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$."], "meter": "++-", "measure": "unknown.measure.di"}, "line.6": {"text": "Ahnte sie nur die Glut,", "tokens": ["Ahn\u00b7te", "sie", "nur", "die", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "M\u00fc\u00dfte sie zitternd ergl\u00fchn.", "tokens": ["M\u00fc\u00df\u00b7te", "sie", "zit\u00b7ternd", "er\u00b7gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.9": {"line.1": {"text": "Aber Flammen w\u00e4rmen,", "tokens": ["A\u00b7ber", "Flam\u00b7men", "w\u00e4r\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und W\u00e4rme weckt Flammen.", "tokens": ["Und", "W\u00e4r\u00b7me", "weckt", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "O ber\u00fchre nicht! \u2013 Fort! \u2013 Flieh!", "tokens": ["O", "be\u00b7r\u00fch\u00b7re", "nicht", "!", "\u2013", "Fort", "!", "\u2013", "Flieh", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$.", "$(", "NN", "$.", "$(", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "O ber\u00fchre sie nicht!", "tokens": ["O", "be\u00b7r\u00fch\u00b7re", "sie", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "Zu sp\u00e4t!", "tokens": ["Zu", "sp\u00e4t", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKA", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Erschrick nicht, rette,", "tokens": ["Er\u00b7schrick", "nicht", ",", "ret\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "$,", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Rette aus Flammen den Duft.", "tokens": ["Ret\u00b7te", "aus", "Flam\u00b7men", "den", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.11": {"line.1": {"text": "Angstfahle Bl\u00e4sse knirscht,", "tokens": ["Angst\u00b7fah\u00b7le", "Bl\u00e4s\u00b7se", "knirscht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.2": {"text": "Aber Reue zermalmt nicht.", "tokens": ["A\u00b7ber", "Reu\u00b7e", "zer\u00b7malmt", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKNEG", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Auf wei\u00dfen Astern schwarze Erde.", "tokens": ["Auf", "wei\u00b7\u00dfen", "As\u00b7tern", "schwar\u00b7ze", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Warum schwarze Erde?", "tokens": ["Wa\u00b7rum", "schwar\u00b7ze", "Er\u00b7de", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Warum nicht der Tod?", "tokens": ["Wa\u00b7rum", "nicht", "der", "Tod", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "Erde ist Leben.", "tokens": ["Er\u00b7de", "ist", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.13": {"line.1": {"text": "Auf wei\u00dfen Astern schwarze Erde. \u2013", "tokens": ["Auf", "wei\u00b7\u00dfen", "As\u00b7tern", "schwar\u00b7ze", "Er\u00b7de", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist mein M\u00e4rchen.", "tokens": ["Das", "ist", "mein", "M\u00e4r\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}