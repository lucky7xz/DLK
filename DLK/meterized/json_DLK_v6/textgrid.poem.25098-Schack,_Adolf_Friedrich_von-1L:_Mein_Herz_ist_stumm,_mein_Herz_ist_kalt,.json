{"textgrid.poem.25098": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Herz ist stumm, mein Herz ist kalt,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Herz ist stumm, mein Herz ist kalt,", "tokens": ["Mein", "Herz", "ist", "stumm", ",", "mein", "Herz", "ist", "kalt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erstarrt in des Winters Eise;", "tokens": ["Er\u00b7starrt", "in", "des", "Win\u00b7ters", "Ei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bisweilen in seiner Tiefe nur wallt", "tokens": ["Bis\u00b7wei\u00b7len", "in", "sei\u00b7ner", "Tie\u00b7fe", "nur", "wallt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "ADV", "VVFIN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und zittert und regt sich's leise.", "tokens": ["Und", "zit\u00b7tert", "und", "regt", "sich's", "lei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Dann ist's, als ob ein mildes Taun", "tokens": ["Dann", "ist's", ",", "als", "ob", "ein", "mil\u00b7des", "Taun"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "KOKOM", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Decke des Frostes breche;", "tokens": ["Die", "De\u00b7cke", "des", "Fros\u00b7tes", "bre\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Durch gr\u00fcnende W\u00e4lder, bl\u00fchende Aun", "tokens": ["Durch", "gr\u00fc\u00b7nen\u00b7de", "W\u00e4l\u00b7der", ",", "bl\u00fc\u00b7hen\u00b7de", "Aun"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Murmeln von neuem die B\u00e4che.", "tokens": ["Mur\u00b7meln", "von", "neu\u00b7em", "die", "B\u00e4\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Und H\u00f6rnerklang, von Blatt zu Blatt", "tokens": ["Und", "H\u00f6r\u00b7ner\u00b7klang", ",", "von", "Blatt", "zu", "Blatt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Fr\u00fchlingswinde getragen,", "tokens": ["Im", "Fr\u00fch\u00b7lings\u00b7win\u00b7de", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dringt aus den Schluchten ans Ohr mir matt,", "tokens": ["Dringt", "aus", "den", "Schluch\u00b7ten", "ans", "Ohr", "mir", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "PPER", "PTKVZ", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Wie ein Ruf aus seligen Tagen.", "tokens": ["Wie", "ein", "Ruf", "aus", "se\u00b7li\u00b7gen", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch das alternde Herz wird jung nicht mehr,", "tokens": ["Doch", "das", "al\u00b7tern\u00b7de", "Herz", "wird", "jung", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "ADJD", "PTKNEG", "ADV", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Das Echo sterbenden Schalles", "tokens": ["Das", "E\u00b7cho", "ster\u00b7ben\u00b7den", "Schal\u00b7les"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "T\u00f6nt ferner, immer ferner her,", "tokens": ["T\u00f6nt", "fer\u00b7ner", ",", "im\u00b7mer", "fer\u00b7ner", "her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wieder erstarrt liegt alles.", "tokens": ["Und", "wie\u00b7der", "er\u00b7starrt", "liegt", "al\u00b7les", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VVFIN", "PIS", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}