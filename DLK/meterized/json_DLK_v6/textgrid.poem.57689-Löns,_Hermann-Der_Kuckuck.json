{"textgrid.poem.57689": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Der Kuckuck", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Wald ist still, der Wald ist stumm,", "tokens": ["Der", "Wald", "ist", "still", ",", "der", "Wald", "ist", "stumm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es bebt kein Blatt, es nickt kein Zweig,", "tokens": ["Es", "bebt", "kein", "Blatt", ",", "es", "nickt", "kein", "Zweig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Vogelruf von ferne schallt,", "tokens": ["Ein", "Vo\u00b7gel\u00b7ruf", "von", "fer\u00b7ne", "schallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So voll und rund, so warm und weich.", "tokens": ["So", "voll", "und", "rund", ",", "so", "warm", "und", "weich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das ist der Kuckuck, der da ruft,", "tokens": ["Das", "ist", "der", "Ku\u00b7ckuck", ",", "der", "da", "ruft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "So laut, so laut im tiefen Wald,", "tokens": ["So", "laut", ",", "so", "laut", "im", "tie\u00b7fen", "Wald", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An meine Schulter dr\u00e4ngst du dich,", "tokens": ["An", "mei\u00b7ne", "Schul\u00b7ter", "dr\u00e4ngst", "du", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und deine Hand sucht bei mir Halt.", "tokens": ["Und", "dei\u00b7ne", "Hand", "sucht", "bei", "mir", "Halt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du bist so still, du bist so stumm,", "tokens": ["Du", "bist", "so", "still", ",", "du", "bist", "so", "stumm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich h\u00f6re deines Herzens Schlag,", "tokens": ["Ich", "h\u00f6\u00b7re", "dei\u00b7nes", "Her\u00b7zens", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du h\u00e4ltst den Atem an und z\u00e4hlst,", "tokens": ["Du", "h\u00e4ltst", "den", "A\u00b7tem", "an", "und", "z\u00e4hlst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie oft der Kuckuck rufen mag.", "tokens": ["Wie", "oft", "der", "Ku\u00b7ckuck", "ru\u00b7fen", "mag."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich l\u00e4chle deiner Kinderangst,", "tokens": ["Ich", "l\u00e4ch\u00b7le", "dei\u00b7ner", "Kin\u00b7de\u00b7rangst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du meine s\u00fc\u00dfe Wonne du,", "tokens": ["Du", "mei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Won\u00b7ne", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es bl\u00fcht uns noch so mancher Mai,", "tokens": ["Es", "bl\u00fcht", "uns", "noch", "so", "man\u00b7cher", "Mai", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Kuckuck ruft ja immerzu.", "tokens": ["Der", "Ku\u00b7ckuck", "ruft", "ja", "im\u00b7mer\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}