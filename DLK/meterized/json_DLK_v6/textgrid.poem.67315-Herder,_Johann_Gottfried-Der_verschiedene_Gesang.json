{"textgrid.poem.67315": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der verschiedene Gesang", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst schlug mit wunders\u00fc\u00dfem Schall", "tokens": ["Einst", "schlug", "mit", "wun\u00b7der\u00b7s\u00fc\u00b7\u00dfem", "Schall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die klagenreiche Nachtigall.", "tokens": ["Die", "kla\u00b7gen\u00b7rei\u00b7che", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein muntrer Sperling h\u00f6rte zu:", "tokens": ["Ein", "mun\u00b7trer", "Sper\u00b7ling", "h\u00f6r\u00b7te", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbo s\u00e4ng' ich, Nachtigall, wie Du!", "tokens": ["\u00bb", "o", "s\u00e4ng'", "ich", ",", "Nach\u00b7ti\u00b7gall", ",", "wie", "Du", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "VVFIN", "PPER", "$,", "NN", "$,", "PWAV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch warum soll mir's nicht gelingen?", "tokens": ["Doch", "wa\u00b7rum", "soll", "mir's", "nicht", "ge\u00b7lin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VMFIN", "NE", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich will auch lernen also singen.\u00ab", "tokens": ["Ich", "will", "auch", "ler\u00b7nen", "al\u00b7so", "sin\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVFIN", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Nachtigall spricht: \u00bbNun, wolan!", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "spricht", ":", "\u00bb", "Nun", ",", "wo\u00b7lan", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "ADV", "$,", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es singe, wer da singen kann!", "tokens": ["Es", "sin\u00b7ge", ",", "wer", "da", "sin\u00b7gen", "kann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn nie war ich um Kunst bem\u00fcht,", "tokens": ["Denn", "nie", "war", "ich", "um", "Kunst", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur aus dem Herzen quillt mein Lied.", "tokens": ["Nur", "aus", "dem", "Her\u00b7zen", "quillt", "mein", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Nur meiner Liebe zarte Klagen", "tokens": ["Nur", "mei\u00b7ner", "Lie\u00b7be", "zar\u00b7te", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und tiefe Seufzer will ich sagen.\u00ab", "tokens": ["Und", "tie\u00b7fe", "Seuf\u00b7zer", "will", "ich", "sa\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbwenn Liebe den Gesang Dir giebt,", "tokens": ["\u00bb", "wenn", "Lie\u00b7be", "den", "Ge\u00b7sang", "Dir", "giebt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer ist mehr als der Spatz verliebt?", "tokens": ["Wer", "ist", "mehr", "als", "der", "Spatz", "ver\u00b7liebt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PIS", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch klagen kann ich.\u00ab Was geschieht?", "tokens": ["Auch", "kla\u00b7gen", "kann", "ich", ".", "\u00ab", "Was", "ge\u00b7schieht", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "PPER", "$.", "$(", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Sperling zirpt ein Klagelied,", "tokens": ["Der", "Sper\u00b7ling", "zirpt", "ein", "Kla\u00b7ge\u00b7lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und seine Buhle war zufrieden;", "tokens": ["Und", "sei\u00b7ne", "Buh\u00b7le", "war", "zu\u00b7frie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr war ein Sperlingsohr beschieden.", "tokens": ["Ihr", "war", "ein", "Sper\u00b7lin\u00b7gsohr", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nicht also war's die Nachtigall:", "tokens": ["Nicht", "al\u00b7so", "wa\u00b7r's", "die", "Nach\u00b7ti\u00b7gall", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbwas qu\u00e4lest Du den Widerhall?\u00ab", "tokens": ["\u00bb", "was", "qu\u00e4\u00b7lest", "Du", "den", "Wi\u00b7der\u00b7hall", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sprach sie; \u00bbo bleib in Deiner Art,", "tokens": ["Sprach", "sie", ";", "\u00bb", "o", "bleib", "in", "Dei\u00b7ner", "Art", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "$(", "FM", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die meine la\u00df mir aufgespart!", "tokens": ["Die", "mei\u00b7ne", "la\u00df", "mir", "auf\u00b7ge\u00b7spart", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "VVIMP", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du t\u00e4ndelst froh, ich singe Schmerz;", "tokens": ["Du", "t\u00e4n\u00b7delst", "froh", ",", "ich", "sin\u00b7ge", "Schmerz", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie der Gesang, so ist das Herz.\u00ab", "tokens": ["Wie", "der", "Ge\u00b7sang", ",", "so", "ist", "das", "Herz", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Ihr der Sappho T\u00f6ne wagt,", "tokens": ["Die", "Ihr", "der", "Sap\u00b7pho", "T\u00f6\u00b7ne", "wagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00f6rt, was die Nachtigall Euch sagt!", "tokens": ["H\u00f6rt", ",", "was", "die", "Nach\u00b7ti\u00b7gall", "Euch", "sagt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein muntrer Spatz, der seufzen will, \u2013", "tokens": ["Ein", "mun\u00b7trer", "Spatz", ",", "der", "seuf\u00b7zen", "will", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VVINF", "VMFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O, schwieg' er mit den Seufzern still!", "tokens": ["O", ",", "schwieg'", "er", "mit", "den", "Seuf\u00b7zern", "still", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Lied voll Philomelens Schmerz", "tokens": ["Ein", "Lied", "voll", "Phi\u00b7lo\u00b7me\u00b7lens", "Schmerz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.6": {"text": "Erfordert Philomelens Herz.", "tokens": ["Er\u00b7for\u00b7dert", "Phi\u00b7lo\u00b7me\u00b7lens", "Herz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}}}}