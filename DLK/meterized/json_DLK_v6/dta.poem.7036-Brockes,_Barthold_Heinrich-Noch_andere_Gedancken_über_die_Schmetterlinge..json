{"dta.poem.7036": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Noch andere Gedancken \u00fcber  \n die Schmetterlinge.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die sonderbare Eigenschafft", "tokens": ["Die", "son\u00b7der\u00b7ba\u00b7re", "Ei\u00b7gen\u00b7schafft"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Fibern, die nicht nur zum regen,", "tokens": ["Der", "Fi\u00b7bern", ",", "die", "nicht", "nur", "zum", "re\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "ADV", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zum f\u00fchlen, auch so gar zum schmecken,", "tokens": ["Zum", "f\u00fch\u00b7len", ",", "auch", "so", "gar", "zum", "schme\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVINF", "$,", "ADV", "ADV", "ADV", "APPRART", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und was ihm dienlich, zu entdecken,", "tokens": ["Und", "was", "ihm", "dien\u00b7lich", ",", "zu", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Recht unbegreiflich f\u00e4hig seyn,", "tokens": ["Recht", "un\u00b7be\u00b7greif\u00b7lich", "f\u00e4\u00b7hig", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nahm Aug\u2019 und Hertz bewundern ein;", "tokens": ["Nahm", "Aug'", "und", "Hertz", "be\u00b7wun\u00b7dern", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zumal, als bald darauf mein forschender Verstand,", "tokens": ["Zu\u00b7mal", ",", "als", "bald", "da\u00b7rauf", "mein", "for\u00b7schen\u00b7der", "Ver\u00b7stand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ADV", "PAV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Ursach dieser L\u00e4nge fand.", "tokens": ["Die", "Ur\u00b7sach", "die\u00b7ser", "L\u00e4n\u00b7ge", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Indem der Kopff am Rumpff sich sonder Nacken f\u00fcget,", "tokens": ["In\u00b7dem", "der", "Kopff", "am", "Rumpff", "sich", "son\u00b7der", "Na\u00b7cken", "f\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Derselbe folglich sich nicht bieget,", "tokens": ["Der\u00b7sel\u00b7be", "folg\u00b7lich", "sich", "nicht", "bie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADV", "PRF", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die Augen auch so zart, da\u00df sie sie sonder Schader,", "tokens": ["Die", "Au\u00b7gen", "auch", "so", "zart", ",", "da\u00df", "sie", "sie", "son\u00b7der", "Scha\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADJD", "$,", "KOUS", "PPER", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Um ihre Nahrung zu entdecken,", "tokens": ["Um", "ih\u00b7re", "Nah\u00b7rung", "zu", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Nicht k\u00f6nnen zwischen Bl\u00e4tter stecken;", "tokens": ["Nicht", "k\u00f6n\u00b7nen", "zwi\u00b7schen", "Bl\u00e4t\u00b7ter", "ste\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "So hat sie die Natur", "tokens": ["So", "hat", "sie", "die", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Mit solchen R\u00fcsselchen versehn,", "tokens": ["Mit", "sol\u00b7chen", "R\u00fcs\u00b7sel\u00b7chen", "ver\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.16": {"text": "Da\u00df sie auch in den tieffsten Gr\u00fcnden", "tokens": ["Da\u00df", "sie", "auch", "in", "den", "tieffs\u00b7ten", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.17": {"text": "Der Bluhmen, das, was ihnen dienlich finden.", "tokens": ["Der", "Bluh\u00b7men", ",", "das", ",", "was", "ih\u00b7nen", "dien\u00b7lich", "fin\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "$,", "PWS", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Wie kommt es, da\u00df ein Menschliches Gem\u00fcht,", "tokens": ["Wie", "kommt", "es", ",", "da\u00df", "ein", "Menschli\u00b7ches", "Ge\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.19": {"text": "Solch Wunder offt f\u00fcr Augen sieht;", "tokens": ["Solch", "Wun\u00b7der", "offt", "f\u00fcr", "Au\u00b7gen", "sieht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und da\u00df es ihn dennoch nicht r\u00fchrt,", "tokens": ["Und", "da\u00df", "es", "ihn", "den\u00b7noch", "nicht", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Da\u00df er nicht in den Creaturen,", "tokens": ["Da\u00df", "er", "nicht", "in", "den", "Crea\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Von Weisheit und von Macht so offenbahre Spuren,", "tokens": ["Von", "Weis\u00b7heit", "und", "von", "Macht", "so", "of\u00b7fen\u00b7bah\u00b7re", "Spu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Des Allmachtsvollen Sch\u00f6pfers sp\u00fcrt?", "tokens": ["Des", "All\u00b7machts\u00b7vol\u00b7len", "Sch\u00f6p\u00b7fers", "sp\u00fcrt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}