{"textgrid.poem.63513": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Schlaf nur ein", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, was bin ich aufgewacht?", "tokens": ["Ach", ",", "was", "bin", "ich", "auf\u00b7ge\u00b7wacht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob am Haus die Liebste klopft?", "tokens": ["Ob", "am", "Haus", "die", "Liebs\u00b7te", "klopft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Leise t\u00f6nt es durch die Nacht. \u2013", "tokens": ["Lei\u00b7se", "t\u00f6nt", "es", "durch", "die", "Nacht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbschlaf nur ein,", "tokens": ["\u00bb", "schlaf", "nur", "ein", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Schlaf nur ein!", "tokens": ["Schlaf", "nur", "ein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Regen an die Scheiben tropft.\u00ab", "tokens": ["Re\u00b7gen", "an", "die", "Schei\u00b7ben", "tropft", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Warum klingt mir doch das Ohr?", "tokens": ["Wa\u00b7rum", "klingt", "mir", "doch", "das", "Ohr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Spricht von mir das falsche Kind,", "tokens": ["Spricht", "von", "mir", "das", "fal\u00b7sche", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das mich aus dem Sinn verlor? \u2013", "tokens": ["Das", "mich", "aus", "dem", "Sinn", "ver\u00b7lor", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "PRF", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbschlaf nur ein,", "tokens": ["\u00bb", "schlaf", "nur", "ein", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Schlaf nur ein!", "tokens": ["Schlaf", "nur", "ein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Herdenglocken r\u00fchrt der Wind.\u00ab", "tokens": ["Her\u00b7den\u00b7glo\u00b7cken", "r\u00fchrt", "der", "Wind", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und sie sah im Traum mich an,", "tokens": ["Und", "sie", "sah", "im", "Traum", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sie sprach: Du glaubst es kaum,", "tokens": ["Und", "sie", "sprach", ":", "Du", "glaubst", "es", "kaum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ich leide, s\u00fc\u00dfer Mann! \u2013", "tokens": ["Was", "ich", "lei\u00b7de", ",", "s\u00fc\u00b7\u00dfer", "Mann", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbschlaf nur ein,", "tokens": ["\u00bb", "schlaf", "nur", "ein", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Schlaf nur ein!", "tokens": ["Schlaf", "nur", "ein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Schlaf ihn aus, den falschen Traum!\u00ab", "tokens": ["Schlaf", "ihn", "aus", ",", "den", "fal\u00b7schen", "Traum", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}