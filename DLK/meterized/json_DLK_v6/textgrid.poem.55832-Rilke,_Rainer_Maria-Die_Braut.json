{"textgrid.poem.55832": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Die Braut", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ruf mich, Geliebter, ruf mich laut!", "tokens": ["Ruf", "mich", ",", "Ge\u00b7lieb\u00b7ter", ",", "ruf", "mich", "laut", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "La\u00df deine Braut nicht so lange am Fenster stehn.", "tokens": ["La\u00df", "dei\u00b7ne", "Braut", "nicht", "so", "lan\u00b7ge", "am", "Fens\u00b7ter", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PTKNEG", "ADV", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In den alten Platanenalleen", "tokens": ["In", "den", "al\u00b7ten", "Pla\u00b7ta\u00b7nen\u00b7al\u00b7le\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "wacht der Abend nicht mehr:", "tokens": ["wacht", "der", "A\u00b7bend", "nicht", "mehr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "sie sind leer.", "tokens": ["sie", "sind", "leer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Und kommst du mich nicht in das n\u00e4chtliche Haus", "tokens": ["Und", "kommst", "du", "mich", "nicht", "in", "das", "n\u00e4cht\u00b7li\u00b7che", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PRF", "PTKNEG", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "mit deiner Stimme verschlie\u00dfen,", "tokens": ["mit", "dei\u00b7ner", "Stim\u00b7me", "ver\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "so mu\u00df ich mich aus meinen H\u00e4nden hinaus", "tokens": ["so", "mu\u00df", "ich", "mich", "aus", "mei\u00b7nen", "H\u00e4n\u00b7den", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "APZR"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "in die G\u00e4rten des Dunkelblaus", "tokens": ["in", "die", "G\u00e4r\u00b7ten", "des", "Dun\u00b7kel\u00b7blaus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "ergie\u00dfen...", "tokens": ["er\u00b7gie\u00b7\u00dfen", "..."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-+-", "measure": "amphibrach.single"}}}}}