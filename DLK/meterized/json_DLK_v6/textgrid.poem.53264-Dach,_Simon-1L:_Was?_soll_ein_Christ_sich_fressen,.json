{"textgrid.poem.53264": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was? soll ein Christ sich fressen,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was? soll ein Christ sich fressen,", "tokens": ["Was", "?", "soll", "ein", "Christ", "sich", "fres\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$.", "VMFIN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vnd nur sein Leid ermessen,", "tokens": ["Vnd", "nur", "sein", "Leid", "er\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nicht auff den Herren sehn?", "tokens": ["Nicht", "auff", "den", "Her\u00b7ren", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Den aus dem Creutze schliessen,", "tokens": ["Den", "aus", "dem", "Creut\u00b7ze", "schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Ohn welchen, wie wir wissen,", "tokens": ["Ohn", "wel\u00b7chen", ",", "wie", "wir", "wis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Kein Vngl\u00fcck kan geschehn?", "tokens": ["Kein", "Vn\u00b7gl\u00fcck", "kan", "ge\u00b7schehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ohn Gott vermag vns allen", "tokens": ["Ohn", "Gott", "ver\u00b7mag", "vns", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kein H\u00e4rchen zu entfallen,", "tokens": ["Kein", "H\u00e4r\u00b7chen", "zu", "ent\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kein Finger weh zu thun,", "tokens": ["Kein", "Fin\u00b7ger", "weh", "zu", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er kan nicht mehr als wachen", "tokens": ["Er", "kan", "nicht", "mehr", "als", "wa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "KOKOM", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fcr seiner Herde Sachen", "tokens": ["F\u00fcr", "sei\u00b7ner", "Her\u00b7de", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie vormals so auch nun.", "tokens": ["Wie", "vor\u00b7mals", "so", "auch", "nun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Solt Er es anders meynen", "tokens": ["Solt", "Er", "es", "an\u00b7ders", "mey\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als gut mit vns den seinen?", "tokens": ["Als", "gut", "mit", "vns", "den", "sei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPR", "PPER", "ART", "PPOSAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das gl\u00e4ub ich ewig nicht:", "tokens": ["Das", "gl\u00e4ub", "ich", "e\u00b7wig", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In Tr\u00fcbn\u00fcs vns verlassen", "tokens": ["In", "Tr\u00fcb\u00b7n\u00fcs", "vns", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vnd vnbarmhertzig hassen,", "tokens": ["Vnd", "vn\u00b7barm\u00b7hert\u00b7zig", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist wider seine Pflicht.", "tokens": ["Ist", "wi\u00b7der", "sei\u00b7ne", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er wei\u00df sich anzustellen,", "tokens": ["Er", "wei\u00df", "sich", "an\u00b7zu\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als st\u00fcrtz Er vns zur Hellen,", "tokens": ["Als", "st\u00fcrtz", "Er", "vns", "zur", "Hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vnd wer vns spinnen-feind,", "tokens": ["Vnd", "wer", "vns", "spin\u00b7nen\u00b7feind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bleibt doch in allen N\u00f6hten,", "tokens": ["Bleibt", "doch", "in", "al\u00b7len", "N\u00f6h\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ja m\u00f6cht Er vns auch t\u00f6dten,", "tokens": ["Ja", "m\u00f6cht", "Er", "vns", "auch", "t\u00f6d\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der allerbeste Freund.", "tokens": ["Der", "al\u00b7ler\u00b7bes\u00b7te", "Freund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er kan mit tausent Leiden", "tokens": ["Er", "kan", "mit", "tau\u00b7sent", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich so vnd so verkleiden", "tokens": ["Sich", "so", "vnd", "so", "ver\u00b7klei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "KON", "ADV", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "In wilder L\u00f6wen Haut,", "tokens": ["In", "wil\u00b7der", "L\u00f6\u00b7wen", "Haut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ist aber trew an Sinnen,", "tokens": ["Ist", "a\u00b7ber", "trew", "an", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vnd wird bey jhm von innen", "tokens": ["Vnd", "wird", "bey", "jhm", "von", "in\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PPER", "APPR", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein Vater-hertz geschawt.", "tokens": ["Ein", "Va\u00b7ter\u00b7hertz", "ge\u00b7schawt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Mit vnbekandten wegen", "tokens": ["Mit", "vn\u00b7be\u00b7kand\u00b7ten", "we\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist Er vns \u00fcberlegen,", "tokens": ["Ist", "Er", "vns", "\u00fc\u00b7berl\u00b7e\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Seyn Rath k\u00f6mpt vns nicht bey,", "tokens": ["Seyn", "Rath", "k\u00f6mpt", "vns", "nicht", "bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch bleibt sein weiser Handel", "tokens": ["Doch", "bleibt", "sein", "wei\u00b7ser", "Han\u00b7del"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vnd vnerforschter Wandel", "tokens": ["Vnd", "vner\u00b7forschter", "Wan\u00b7del"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Von allem Tadel frey.", "tokens": ["Von", "al\u00b7lem", "Ta\u00b7del", "frey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Er pflegt nur seinen Frommen", "tokens": ["Er", "pflegt", "nur", "sei\u00b7nen", "From\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So grawsam vorzukommen,", "tokens": ["So", "graw\u00b7sam", "vor\u00b7zu\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer wei\u00df es was Er sucht?", "tokens": ["Wer", "wei\u00df", "es", "was", "Er", "sucht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er leitet vns zum guten,", "tokens": ["Er", "lei\u00b7tet", "vns", "zum", "gu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vnd helt durch scharffe Ruten", "tokens": ["Vnd", "helt", "durch", "scharf\u00b7fe", "Ru\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vns in der Kinderzucht.", "tokens": ["Vns", "in", "der", "Kin\u00b7der\u00b7zucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "Drumb, o betr\u00fcbte Seelen,", "tokens": ["Drumb", ",", "o", "be\u00b7tr\u00fcb\u00b7te", "See\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schawt aus den Trawer-H\u00f6len", "tokens": ["Schawt", "aus", "den", "Tra\u00b7wer\u00b7H\u00f6len"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auff seines Trostes Liecht,", "tokens": ["Auff", "sei\u00b7nes", "Tros\u00b7tes", "Liecht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dem, der euch hat gequ\u00e4hlet", "tokens": ["Dem", ",", "der", "euch", "hat", "ge\u00b7qu\u00e4h\u00b7let"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "$,", "PRELS", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vnd wund geschlagen, f\u00e4hlet", "tokens": ["Vnd", "wund", "ge\u00b7schla\u00b7gen", ",", "f\u00e4h\u00b7let"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "ADJD", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es auch an H\u00fclffe nicht.", "tokens": ["Es", "auch", "an", "H\u00fclf\u00b7fe", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Bedenckt was dort geschrieben,", "tokens": ["Be\u00b7denckt", "was", "dort", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vns, die wir Gott recht lieben,", "tokens": ["Vns", ",", "die", "wir", "Gott", "recht", "lie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nutzt alles Creutz und Pein,", "tokens": ["Nutzt", "al\u00b7les", "Creutz", "und", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das Leid mu\u00df vnsre Wonne,", "tokens": ["Das", "Leid", "mu\u00df", "vns\u00b7re", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Regen vnsre Sonne,", "tokens": ["Der", "Re\u00b7gen", "vns\u00b7re", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Todt das Leben seyn.", "tokens": ["Der", "Todt", "das", "Le\u00b7ben", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}