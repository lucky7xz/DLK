{"textgrid.poem.42368": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dies ist der Kampf der ungebundnen Seele,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies ist der Kampf der ungebundnen Seele,", "tokens": ["Dies", "ist", "der", "Kampf", "der", "un\u00b7ge\u00b7bund\u00b7nen", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die mit den M\u00e4chten um ihr Vorrecht rechtet,", "tokens": ["Die", "mit", "den", "M\u00e4ch\u00b7ten", "um", "ihr", "Vor\u00b7recht", "rech\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und des gequ\u00e4lten Leibes, der geknechtet", "tokens": ["Und", "des", "ge\u00b7qu\u00e4l\u00b7ten", "Lei\u00b7bes", ",", "der", "ge\u00b7knech\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PRELS", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nun will, dass sie der Knechtschaft sich verm\u00e4hle.", "tokens": ["Nun", "will", ",", "dass", "sie", "der", "Knecht\u00b7schaft", "sich", "ver\u00b7m\u00e4h\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "KOUS", "PPER", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Erschreckt, ge\u00e4ngstet um ihr Heil, beendigt", "tokens": ["Er\u00b7schreckt", ",", "ge\u00b7\u00e4ngs\u00b7tet", "um", "ihr", "Heil", ",", "be\u00b7en\u00b7digt"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VVPP", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie nie den Streit, dass sie nicht j\u00e4h erliege,", "tokens": ["Sie", "nie", "den", "Streit", ",", "dass", "sie", "nicht", "j\u00e4h", "er\u00b7lie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht gar zuschanden werde vor dem Siege \u2013", "tokens": ["Nicht", "gar", "zu\u00b7schan\u00b7den", "wer\u00b7de", "vor", "dem", "Sie\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "VAFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Tod und Untergang noch ungeb\u00e4ndigt.", "tokens": ["In", "Tod", "und", "Un\u00b7ter\u00b7gang", "noch", "un\u00b7ge\u00b7b\u00e4n\u00b7digt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Fliehenden \u2013 daheim an keiner Stelle \u2013", "tokens": ["Der", "Flie\u00b7hen\u00b7den", "\u2013", "da\u00b7heim", "an", "kei\u00b7ner", "Stel\u00b7le", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zerrt Wind und Wetterhand an ihren Str\u00e4ngen,", "tokens": ["Zerrt", "Wind", "und", "Wet\u00b7ter\u00b7hand", "an", "ih\u00b7ren", "Str\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und halb herabgezogen bleibt sie h\u00e4ngen", "tokens": ["Und", "halb", "her\u00b7ab\u00b7ge\u00b7zo\u00b7gen", "bleibt", "sie", "h\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In einem Abgrund zwischen Nacht und Helle.", "tokens": ["In", "ei\u00b7nem", "Ab\u00b7grund", "zwi\u00b7schen", "Nacht", "und", "Hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}