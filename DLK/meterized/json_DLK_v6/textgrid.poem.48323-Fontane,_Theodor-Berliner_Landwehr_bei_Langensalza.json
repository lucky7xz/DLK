{"textgrid.poem.48323": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Berliner Landwehr bei Langensalza", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Berliner Landwehr, Gewehr in Hand,", "tokens": ["Ber\u00b7li\u00b7ner", "Land\u00b7wehr", ",", "Ge\u00b7wehr", "in", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Steht bei Langensalza im Sonnenbrand,", "tokens": ["Steht", "bei", "Lan\u00b7gen\u00b7sal\u00b7za", "im", "Son\u00b7nen\u00b7brand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "APPRART", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ein Staub, eine Hitze, es perlt der Schwei\u00df,", "tokens": ["Ein", "Staub", ",", "ei\u00b7ne", "Hit\u00b7ze", ",", "es", "perlt", "der", "Schwei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Berliner Landwehr, wird dir's zu hei\u00df?", "tokens": ["Ber\u00b7li\u00b7ner", "Land\u00b7wehr", ",", "wird", "dir's", "zu", "hei\u00df", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VAFIN", "PIS", "PTKA", "ADJD", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "\u00bbis nich!\u00ab", "tokens": ["\u00bb", "is", "nich", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "PTKNEG", "$.", "$("], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Die Hannoveraner sprengen heran,", "tokens": ["Die", "Han\u00b7no\u00b7ve\u00b7ra\u00b7ner", "spren\u00b7gen", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zweitausend gegen achthundert Mann,", "tokens": ["Zweit\u00b7au\u00b7send", "ge\u00b7gen", "acht\u00b7hun\u00b7dert", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "APPR", "CARD", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Zweitausend Reiter sprengen her:", "tokens": ["Zweit\u00b7au\u00b7send", "Rei\u00b7ter", "spren\u00b7gen", "her", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ergib dich, Landwehr, streck das Gewehr!", "tokens": ["Er\u00b7gib", "dich", ",", "Land\u00b7wehr", ",", "streck", "das", "Ge\u00b7wehr", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "\u00bbis nich!\u00ab", "tokens": ["\u00bb", "is", "nich", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "PTKNEG", "$.", "$("], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Zweitausend Reiter haben gesiegt,", "tokens": ["Zweit\u00b7au\u00b7send", "Rei\u00b7ter", "ha\u00b7ben", "ge\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Was hilft's, Hannover unterliegt.", "tokens": ["Was", "hilft's", ",", "Han\u00b7no\u00b7ver", "un\u00b7ter\u00b7liegt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "NE", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbtrink mit, Kamerad, aus meinem Glas!\u00ab", "tokens": ["\u00bb", "trink", "mit", ",", "Ka\u00b7me\u00b7rad", ",", "aus", "mei\u00b7nem", "Glas", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "\u00bbwir dachten, ihr tr\u00fcgt uns einen Ha\u00df!\u00ab", "tokens": ["\u00bb", "wir", "dach\u00b7ten", ",", "ihr", "tr\u00fcgt", "uns", "ei\u00b7nen", "Ha\u00df", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "\u00bbis nich!\u00ab", "tokens": ["\u00bb", "is", "nich", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "PTKNEG", "$.", "$("], "meter": "-+", "measure": "iambic.single"}}}}}