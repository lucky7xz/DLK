{"dta.poem.15903": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "108.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Worin besteht die Lust, die eigne Lust, auf Fluren", "tokens": ["Wo\u00b7rin", "be\u00b7steht", "die", "Lust", ",", "die", "eig\u00b7ne", "Lust", ",", "auf", "Flu\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Alterthumes nachzugehn versunknen Spuren?", "tokens": ["Des", "Al\u00b7ter\u00b7thu\u00b7mes", "nach\u00b7zu\u00b7gehn", "ver\u00b7sun\u00b7knen", "Spu\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hier aufzufrischen, was der Hauch der Zeit verwischt,", "tokens": ["Hier", "auf\u00b7zu\u00b7fri\u00b7schen", ",", "was", "der", "Hauch", "der", "Zeit", "ver\u00b7wischt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIZU", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dort wegzuwischen, was sich Falsches dreingemischt?", "tokens": ["Dort", "weg\u00b7zu\u00b7wi\u00b7schen", ",", "was", "sich", "Fal\u00b7sches", "drein\u00b7ge\u00b7mischt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "PRF", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mit schwachem Nachtlicht Nacht der R\u00e4thsel zu begl\u00e4nzen,", "tokens": ["Mit", "schwa\u00b7chem", "Nacht\u00b7licht", "Nacht", "der", "R\u00e4th\u00b7sel", "zu", "be\u00b7gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mit Vermuthungen die Tr\u00fcmmer zu erg\u00e4nzen?", "tokens": ["Und", "mit", "Ver\u00b7mu\u00b7thun\u00b7gen", "die", "Tr\u00fcm\u00b7mer", "zu", "er\u00b7g\u00e4n\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Menschheit ists in dir, die sich an sich ergetzt,", "tokens": ["Die", "Menschheit", "ists", "in", "dir", ",", "die", "sich", "an", "sich", "er\u00b7getzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "$,", "PRELS", "PRF", "APPR", "PRF", "VVPP", "$,"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Gern aus dem Alter in die Jugend sich versetzt;", "tokens": ["Gern", "aus", "dem", "Al\u00b7ter", "in", "die", "Ju\u00b7gend", "sich", "ver\u00b7setzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wie du im engern Kreis dich deines Bildes freuest,", "tokens": ["Wie", "du", "im", "en\u00b7gern", "Kreis", "dich", "dei\u00b7nes", "Bil\u00b7des", "freu\u00b7est", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "ADJA", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du des Tagebuchs erloschne Schrift erneuest,", "tokens": ["Wenn", "du", "des", "Ta\u00b7ge\u00b7buchs", "er\u00b7lo\u00b7schne", "Schrift", "er\u00b7neu\u00b7est", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wenn dein r\u00fcckahnender Gedanke gl\u00fccklich kn\u00fcpft", "tokens": ["Wenn", "dein", "r\u00fc\u00b7ckah\u00b7nen\u00b7der", "Ge\u00b7dan\u00b7ke", "gl\u00fcck\u00b7lich", "kn\u00fcpft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Faden, wo er dir vor Jahren ist entschl\u00fcpft.", "tokens": ["Den", "Fa\u00b7den", ",", "wo", "er", "dir", "vor", "Jah\u00b7ren", "ist", "ent\u00b7schl\u00fcpft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "PPER", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}