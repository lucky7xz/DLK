{"textgrid.poem.29777": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "192.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "St\u00e4dte sind die Beutel-M\u00fchlen, und das Land ist M\u00fcller-Gast;", "tokens": ["St\u00e4d\u00b7te", "sind", "die", "Beu\u00b7tel\u00b7M\u00fch\u00b7len", ",", "und", "das", "Land", "ist", "M\u00fcl\u00b7ler\u00b7Gast", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "KON", "ART", "NN", "VAFIN", "NE", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Iedem wird daselbst zu Staube, was sein Beutel in sich fast.", "tokens": ["Ie\u00b7dem", "wird", "da\u00b7selbst", "zu", "Stau\u00b7be", ",", "was", "sein", "Beu\u00b7tel", "in", "sich", "fast", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PAV", "APPR", "NN", "$,", "PRELS", "PPOSAT", "NN", "APPR", "PRF", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}