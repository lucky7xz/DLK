{"textgrid.poem.57871": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Aus \u00dcberzeugung", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sitzen Schorsch und Christian", "tokens": ["Es", "sit\u00b7zen", "Schorsch", "und", "Chris\u00b7ti\u00b7an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei ihrer kleinen Lage,", "tokens": ["Bei", "ih\u00b7rer", "klei\u00b7nen", "La\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Besprechend eifrig lang und breit", "tokens": ["Be\u00b7spre\u00b7chend", "eif\u00b7rig", "lang", "und", "breit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Kandidatenfrage.", "tokens": ["Die", "Kan\u00b7di\u00b7da\u00b7ten\u00b7fra\u00b7ge", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbwei\u00dft du schon, wen du w\u00e4hlen wirst?\u00ab", "tokens": ["\u00bb", "wei\u00dft", "du", "schon", ",", "wen", "du", "w\u00e4h\u00b7len", "wirst", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "$,", "PWS", "PPER", "VVINF", "VAFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fragt Schorsch. \u00bbIch w\u00e4hle den Meyer;", "tokens": ["Fragt", "Schorsch", ".", "\u00bb", "Ich", "w\u00e4h\u00b7le", "den", "Me\u00b7yer", ";"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$.", "$(", "PPER", "VVFIN", "ART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er kauft bei mich schon manches Jahr", "tokens": ["Er", "kauft", "bei", "mich", "schon", "man\u00b7ches", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und is f\u00fcr keine Steuer.\u00ab", "tokens": ["Und", "is", "f\u00fcr", "kei\u00b7ne", "Steu\u00b7er", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "FM", "APPR", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbw\u00e4hl' du den Meyer,\u00ab sagt Christian,", "tokens": ["\u00bb", "w\u00e4hl'", "du", "den", "Me\u00b7yer", ",", "\u00ab", "sagt", "Chris\u00b7ti\u00b7an", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "ART", "NE", "$,", "$(", "VVFIN", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "\u00bbich habe dazu keine Neigung,", "tokens": ["\u00bb", "ich", "ha\u00b7be", "da\u00b7zu", "kei\u00b7ne", "Nei\u00b7gung", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PAV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich w\u00e4hle nicht aus Eigennutz,", "tokens": ["Ich", "w\u00e4h\u00b7le", "nicht", "aus", "Ei\u00b7gen\u00b7nutz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich w\u00e4hle aus \u00dcberzeugung.", "tokens": ["Ich", "w\u00e4h\u00b7le", "aus", "\u00dc\u00b7berz\u00b7eu\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch ehe ich w\u00e4hl', da mu\u00df er sich", "tokens": ["Doch", "e\u00b7he", "ich", "w\u00e4hl'", ",", "da", "mu\u00df", "er", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "ADV", "VMFIN", "PPER", "PRF"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf Ehrenwort verpflichten,", "tokens": ["Auf", "Eh\u00b7ren\u00b7wort", "ver\u00b7pflich\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vor Weihnachten noch 'ne Bogenlamp'", "tokens": ["Vor", "Weih\u00b7nach\u00b7ten", "noch", "'", "ne", "Bo\u00b7gen\u00b7lamp", "'"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "$(", "NE", "NE", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Vor meinem Haus zu errichten!\u00ab", "tokens": ["Vor", "mei\u00b7nem", "Haus", "zu", "er\u00b7rich\u00b7ten", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}