{"textgrid.poem.46023": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[jesukind, du Licht der Blinden]", "genre": "verse", "period": "N.A.", "pub_year": 1825, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jesukind, du Licht der Blinden", "tokens": ["Je\u00b7su\u00b7kind", ",", "du", "Licht", "der", "Blin\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mache mich doch einmal blind", "tokens": ["Ma\u00b7che", "mich", "doch", "ein\u00b7mal", "blind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich dir, wie mir dies Kind,", "tokens": ["Da\u00df", "ich", "dir", ",", "wie", "mir", "dies", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "PWAV", "PPER", "PDS", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf dem Pfad mich m\u00f6g' verbinden", "tokens": ["Auf", "dem", "Pfad", "mich", "m\u00f6g'", "ver\u00b7bin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo du mich auch hin willst f\u00fchren", "tokens": ["Wo", "du", "mich", "auch", "hin", "willst", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "ADV", "ADV", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn mein armes eignes Licht", "tokens": ["Denn", "mein", "ar\u00b7mes", "eig\u00b7nes", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Kann auch selbst beim Licht noch nicht", "tokens": ["Kann", "auch", "selbst", "beim", "Licht", "noch", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "APPRART", "NN", "ADV", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Dich das wahre Licht recht sp\u00fcren", "tokens": ["Dich", "das", "wah\u00b7re", "Licht", "recht", "sp\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "ADJA", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "O wie t\u00f6richt, t\u00f6richt ist", "tokens": ["O", "wie", "t\u00f6\u00b7richt", ",", "t\u00f6\u00b7richt", "ist"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "KOKOM", "ADJD", "$,", "ADJD", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Der nicht in allem ohne Ausnahm'", "tokens": ["Der", "nicht", "in", "al\u00b7lem", "oh\u00b7ne", "Aus\u00b7nahm'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "APPR", "PIS", "APPR", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Folget dir zu jeder Frist.", "tokens": ["Fol\u00b7get", "dir", "zu", "je\u00b7der", "Frist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}