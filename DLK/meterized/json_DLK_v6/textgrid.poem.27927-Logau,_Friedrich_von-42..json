{"textgrid.poem.27927": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hochzeit haben, lange leben", "tokens": ["Hoch\u00b7zeit", "ha\u00b7ben", ",", "lan\u00b7ge", "le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "$,", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00fcnscht ihm ieder sein gegeben.", "tokens": ["W\u00fcnscht", "ihm", "ie\u00b7der", "sein", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "PPOSAT", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Viel gelebt, Hochzeit gehabt,", "tokens": ["Viel", "ge\u00b7lebt", ",", "Hoch\u00b7zeit", "ge\u00b7habt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "NN", "VAPP", "$,"], "meter": "+-++--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Kr\u00e4nckt weit mehr offt, als es labt.", "tokens": ["Kr\u00e4nckt", "weit", "mehr", "offt", ",", "als", "es", "labt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}