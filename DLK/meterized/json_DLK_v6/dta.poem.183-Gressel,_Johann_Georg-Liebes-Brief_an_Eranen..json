{"dta.poem.183": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Liebes-Brief an  Eranen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Licht! sie nehme an mit wohl-gesinntem Hertzen/", "tokens": ["Mein", "Licht", "!", "sie", "neh\u00b7me", "an", "mit", "wohl\u00b7ge\u00b7sinn\u00b7tem", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VVFIN", "APPR", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ein getreuer Geist aus Liebe ", "tokens": ["Was", "ein", "ge\u00b7treu\u00b7er", "Geist", "aus", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es hat ihr holder Strahl als Feuer-reiche Kertzen", "tokens": ["Es", "hat", "ihr", "hol\u00b7der", "Strahl", "als", "Feu\u00b7er\u00b7rei\u00b7che", "Kert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den gantzen Hertzens-Bau in lichten Brand gef\u00fchrt.", "tokens": ["Den", "gant\u00b7zen", "Hert\u00b7zens\u00b7Bau", "in", "lich\u00b7ten", "Brand", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein Leben lebt in ihr/ mein Hertze ist ihr eigen/", "tokens": ["Mein", "Le\u00b7ben", "lebt", "in", "ihr", "/", "mein", "Hert\u00b7ze", "ist", "ihr", "ei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "$(", "PPOSAT", "VVFIN", "VAFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es steht in ihrer Macht ob ich soll gl\u00fccklich seyn.", "tokens": ["Es", "steht", "in", "ih\u00b7rer", "Macht", "ob", "ich", "soll", "gl\u00fcck\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "KOUS", "PPER", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wird ihrer Augen-Paar die Strahlen auf mich neigen/", "tokens": ["Wird", "ih\u00b7rer", "Au\u00b7gen\u00b7Paar", "die", "Strah\u00b7len", "auf", "mich", "nei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "NN", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So nimmt mich ", "tokens": ["So", "nimmt", "mich"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Wo ", "tokens": ["Wo"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Des edlen Balsams-Strauch wird bl\u00fchen in den Auen/", "tokens": ["Des", "ed\u00b7len", "Bal\u00b7sams\u00b7Strauch", "wird", "bl\u00fc\u00b7hen", "in", "den", "Au\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Woselbst die Lieblichkeit mit vollen Str\u00f6hmen flie\u00dft.", "tokens": ["Wo\u00b7selbst", "die", "Lieb\u00b7lich\u00b7keit", "mit", "vol\u00b7len", "Str\u00f6h\u00b7men", "flie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Winter mu\u00df bey mir zu einen Sommer werden/", "tokens": ["Der", "Win\u00b7ter", "mu\u00df", "bey", "mir", "zu", "ei\u00b7nen", "Som\u00b7mer", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "PPER", "APPR", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "In meinem Hertzen z\u00fcndt sich eine Sonne an/", "tokens": ["In", "mei\u00b7nem", "Hert\u00b7zen", "z\u00fcndt", "sich", "ei\u00b7ne", "Son\u00b7ne", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der Augen s\u00fcsser Strahl/ die freundlichen Geberden", "tokens": ["Der", "Au\u00b7gen", "s\u00fcs\u00b7ser", "Strahl", "/", "die", "freund\u00b7li\u00b7chen", "Ge\u00b7ber\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Erquicken mehr den Geist als ", "tokens": ["Er\u00b7qui\u00b7cken", "mehr", "den", "Geist", "als"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "KOKOM"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Mein Licht/ sie z\u00fcrne nicht wenn ihr die Schrifft zu wider", "tokens": ["Mein", "Licht", "/", "sie", "z\u00fcr\u00b7ne", "nicht", "wenn", "ihr", "die", "Schrifft", "zu", "wi\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PPER", "VVFIN", "PTKNEG", "KOUS", "PPER", "ART", "NN", "PTKZU", "PTKVZ"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und so mein frecher Geist nach feinen Willen schreibt;", "tokens": ["Und", "so", "mein", "fre\u00b7cher", "Geist", "nach", "fei\u00b7nen", "Wil\u00b7len", "schreibt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Kein Manns-Bild bleibet hart bey ihren Pracht der Glieder/", "tokens": ["Kein", "Manns\u00b7Bild", "blei\u00b7bet", "hart", "bey", "ih\u00b7ren", "Pracht", "der", "Glie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Das nicht der Augen-Blitz in Sclaven-Ketten treibt?", "tokens": ["Das", "nicht", "der", "Au\u00b7gen\u00b7Blitz", "in", "Scla\u00b7ven\u00b7Ket\u00b7ten", "treibt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Jhr Antlitz ist der Thron wo Zucht und Tugend strahlen/", "tokens": ["Ihr", "Ant\u00b7litz", "ist", "der", "Thron", "wo", "Zucht", "und", "Tu\u00b7gend", "strah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "PWAV", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die Sch\u00f6nheit hat dabey ihr ihren Sitz gericht.", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "hat", "da\u00b7bey", "ihr", "ih\u00b7ren", "Sitz", "ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wie sch\u00f6n auch die Natur die Kirschen kan bemahlen/", "tokens": ["Wie", "sch\u00f6n", "auch", "die", "Na\u00b7tur", "die", "Kir\u00b7schen", "kan", "be\u00b7mah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So gleicht ihr Purpur-Schein doch ihren Lippen nicht.", "tokens": ["So", "gleicht", "ihr", "Pur\u00b7pur\u00b7Schein", "doch", "ih\u00b7ren", "Lip\u00b7pen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Reimt ihre Trefflichkeit und ich zwar nicht zusammen/", "tokens": ["Reimt", "ih\u00b7re", "Treff\u00b7lich\u00b7keit", "und", "ich", "zwar", "nicht", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "PPER", "ADV", "PTKNEG", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Legt man den Demant auch zu keinem Kieselstein", "tokens": ["Legt", "man", "den", "De\u00b7mant", "auch", "zu", "kei\u00b7nem", "Kie\u00b7sel\u00b7stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ART", "NN", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Noch ein geringes Spreu zu Zimmetrinde-Flammen", "tokens": ["Noch", "ein", "ge\u00b7rin\u00b7ges", "Spreu", "zu", "Zim\u00b7me\u00b7trin\u00b7de\u00b7Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So mu\u00df die Zwiebel doch bey sch\u00f6nen Rosen seyn. ", "tokens": ["So", "mu\u00df", "die", "Zwie\u00b7bel", "doch", "bey", "sch\u00f6\u00b7nen", "Ro\u00b7sen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Auf einen wilden Baum pfrofft man offt s\u00fcsse Fr\u00fcchte", "tokens": ["Auf", "ei\u00b7nen", "wil\u00b7den", "Baum", "pfrofft", "man", "offt", "s\u00fcs\u00b7se", "Fr\u00fcch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PIS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Man h\u00fcllet Perl und Gold in schlechte Wolle ein/", "tokens": ["Man", "h\u00fcl\u00b7let", "Perl", "und", "Gold", "in", "schlech\u00b7te", "Wol\u00b7le", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NE", "KON", "NN", "APPR", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die Larve decket offt ein sch\u00f6nes Angesichte", "tokens": ["Die", "Lar\u00b7ve", "de\u00b7cket", "offt", "ein", "sch\u00f6\u00b7nes", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und eine schwartze Wolck steht bey dem Sonnenschein.", "tokens": ["Und", "ei\u00b7ne", "schwart\u00b7ze", "Wolck", "steht", "bey", "dem", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Soll nun mein frecher Geist durch ihren Grimm verderben/", "tokens": ["Soll", "nun", "mein", "fre\u00b7cher", "Geist", "durch", "ih\u00b7ren", "Grimm", "ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und reist die zarte Hand den Hoffnungs-Ancker ab?", "tokens": ["Und", "reist", "die", "zar\u00b7te", "Hand", "den", "Hoff\u00b7nungs\u00b7An\u00b7cker", "ab", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ach Nein! Erbarmni\u00df l\u00e4st mich nicht so H\u00fclff-lo\u00df sterben/", "tokens": ["Ach", "Nein", "!", "Er\u00b7barm\u00b7ni\u00df", "l\u00e4st", "mich", "nicht", "so", "H\u00fclff\u00b7lo\u00df", "ster\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PTKANT", "$.", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und ihre Freundlichkeit die st\u00fcrtzet nicht ins Grab.", "tokens": ["Und", "ih\u00b7re", "Freund\u00b7lich\u00b7keit", "die", "st\u00fcrt\u00b7zet", "nicht", "ins", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "VVFIN", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ich sehe/ da\u00df ein Licht mir in der Nacht auffgehet/", "tokens": ["Ich", "se\u00b7he", "/", "da\u00df", "ein", "Licht", "mir", "in", "der", "Nacht", "auff\u00b7ge\u00b7het", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.39": {"text": "Der Himmel des Gesichts in hellen Strahlen stehet/", "tokens": ["Der", "Him\u00b7mel", "des", "Ge\u00b7sichts", "in", "hel\u00b7len", "Strah\u00b7len", "ste\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Die sch\u00f6nen Augen sind nicht Blitz und Donners voll.", "tokens": ["Die", "sch\u00f6\u00b7nen", "Au\u00b7gen", "sind", "nicht", "Blitz", "und", "Don\u00b7ners", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKNEG", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Es l\u00e4st ihr Freundlich-seyn mich stets das beste hoffen/", "tokens": ["Es", "l\u00e4st", "ihr", "Freund\u00b7lich\u00b7seyn", "mich", "stets", "das", "bes\u00b7te", "hof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PPER", "ADV", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und ihr Erquickungs-Strahl reist Furcht und Schrecken ein/", "tokens": ["Und", "ihr", "Er\u00b7qui\u00b7ckungs\u00b7Strahl", "reist", "Furcht", "und", "Schre\u00b7cken", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "NN", "KON", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Ich sehe/ da\u00df mein Geist im Haven eingeloffen/", "tokens": ["Ich", "se\u00b7he", "/", "da\u00df", "mein", "Geist", "im", "Ha\u00b7ven", "ein\u00b7ge\u00b7lof\u00b7fen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Weil keine Klippen ihm daselbst im Wege seyn.", "tokens": ["Weil", "kei\u00b7ne", "Klip\u00b7pen", "ihm", "da\u00b7selbst", "im", "We\u00b7ge", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "P