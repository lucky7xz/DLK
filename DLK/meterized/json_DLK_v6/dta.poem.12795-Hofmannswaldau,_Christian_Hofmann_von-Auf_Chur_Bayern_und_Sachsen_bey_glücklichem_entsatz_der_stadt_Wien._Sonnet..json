{"dta.poem.12795": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf Chur Bayern und Sachsen  \n bey gl\u00fccklichem entsatz der stadt Wien.  \n Sonnet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Komm aus des grabes nacht, komm, Maximilian!", "tokens": ["Komm", "aus", "des", "gra\u00b7bes", "nacht", ",", "komm", ",", "Ma\u00b7xi\u00b7mi\u00b7li\u00b7an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "$,", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und du, o Moritz! la\u00df die sterbe-kleider liegen!", "tokens": ["Und", "du", ",", "o", "Mo\u00b7ritz", "!", "la\u00df", "die", "ster\u00b7be\u00b7klei\u00b7der", "lie\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "NE", "$.", "VVIMP", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Schaut euren Enckeln zu, wie sie so herrlich siegen!", "tokens": ["Schaut", "eu\u00b7ren", "En\u00b7ckeln", "zu", ",", "wie", "sie", "so", "herr\u00b7lich", "sie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "PWAV", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und was sie vor das heyl der Christenheit gethan.", "tokens": ["Und", "was", "sie", "vor", "das", "heyl", "der", "Chris\u00b7ten\u00b7heit", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Hund fiel Oesterreich mit h\u00f6chstem w\u00fcten an:", "tokens": ["Der", "Hund", "fiel", "O\u00b7es\u00b7ter\u00b7reich", "mit", "h\u00f6chs\u00b7tem", "w\u00fc\u00b7ten", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "APPR", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Es schien, als w\u00e4r die angst aufs \u00e4userste gestiegen;", "tokens": ["Es", "schien", ",", "als", "w\u00e4r", "die", "angst", "aufs", "\u00e4u\u00b7sers\u00b7te", "ge\u00b7stie\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "VAFIN", "ART", "NN", "APPRART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Indem so kamen sie und zeigten durch ihr kriegen,", "tokens": ["In\u00b7dem", "so", "ka\u00b7men", "sie", "und", "zeig\u00b7ten", "durch", "ihr", "krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "PPER", "KON", "VVFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df Deutschland in der noth noch Helden stellen kan.", "tokens": ["Da\u00df", "Deutschland", "in", "der", "noth", "noch", "Hel\u00b7den", "stel\u00b7len", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "ART", "NN", "ADV", "NN", "VVINF", "VMFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Hier hat kein eigen-nutz, kein sch\u00e4ndlicher gewinn,", "tokens": ["Hier", "hat", "kein", "ei\u00b7gen\u00b7nutz", ",", "kein", "sch\u00e4nd\u00b7li\u00b7cher", "ge\u00b7winn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$,", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Um den die meisten sonst anitzt zu felde ziehn,", "tokens": ["Um", "den", "die", "meis\u00b7ten", "sonst", "a\u00b7nitzt", "zu", "fel\u00b7de", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ART", "VVFIN", "ADV", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Den sch\u00f6nen lorbeer-crantz, der beyde ziert, geflochten.", "tokens": ["Den", "sch\u00f6\u00b7nen", "lor\u00b7beer\u00b7crantz", ",", "der", "bey\u00b7de", "ziert", ",", "ge\u00b7floch\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Euch, Helden! wird mit recht der lob-spruch zuerkannt:", "tokens": ["Euch", ",", "Hel\u00b7den", "!", "wird", "mit", "recht", "der", "lob\u00b7spruch", "zu\u00b7er\u00b7kannt", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$.", "VAFIN", "APPR", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da\u00df ihr vor GOttes ehr und vor das vaterland", "tokens": ["Da\u00df", "ihr", "vor", "Got\u00b7tes", "ehr", "und", "vor", "das", "va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "NN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Aus bloser redligkeit und deutscher treu gefochten.", "tokens": ["Aus", "blo\u00b7ser", "red\u00b7lig\u00b7keit", "und", "deut\u00b7scher", "treu", "ge\u00b7foch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}