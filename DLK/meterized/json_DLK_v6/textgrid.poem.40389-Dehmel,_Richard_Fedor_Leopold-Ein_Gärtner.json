{"textgrid.poem.40389": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Ein G\u00e4rtner", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann ein G\u00e4rtnersmann werden,", "tokens": ["Ich", "kann", "ein", "G\u00e4rt\u00b7ners\u00b7mann", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "mit allen Pflanzen vertraut.", "tokens": ["mit", "al\u00b7len", "Pflan\u00b7zen", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Mir schadet keine Treibhausluft", "tokens": ["Mir", "scha\u00b7det", "kei\u00b7ne", "Treib\u00b7haus\u00b7luft"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und auch kein giftiges Kraut;", "tokens": ["und", "auch", "kein", "gif\u00b7ti\u00b7ges", "Kraut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "ich bin so z\u00e4h wie ein Buxbaum.", "tokens": ["ich", "bin", "so", "z\u00e4h", "wie", "ein", "Bux\u00b7baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich nutze die giftigen Kr\u00e4uter,", "tokens": ["Ich", "nut\u00b7ze", "die", "gif\u00b7ti\u00b7gen", "Kr\u00e4u\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "ich z\u00fcchte Heilkr\u00e4uter draus,", "tokens": ["ich", "z\u00fcch\u00b7te", "Heil\u00b7kr\u00e4u\u00b7ter", "draus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PAV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "mitunter auch K\u00fcchenkr\u00e4uter;", "tokens": ["mi\u00b7tun\u00b7ter", "auch", "K\u00fc\u00b7chen\u00b7kr\u00e4u\u00b7ter", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "nur die Unkr\u00e4uter rei\u00df ich aus", "tokens": ["nur", "die", "Un\u00b7kr\u00e4u\u00b7ter", "rei\u00df", "ich", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "oder veredle sie.", "tokens": ["o\u00b7der", "ve\u00b7red\u00b7le", "sie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Und meine Baumschule, Leute,", "tokens": ["Und", "mei\u00b7ne", "Baum\u00b7schu\u00b7le", ",", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "schm\u00fcckt alle Landstra\u00dfen, seht!", "tokens": ["schm\u00fcckt", "al\u00b7le", "Land\u00b7stra\u00b7\u00dfen", ",", "seht", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Jawohl, Herr Nachbar, es lohnt sich,", "tokens": ["Ja\u00b7wohl", ",", "Herr", "Nach\u00b7bar", ",", "es", "lohnt", "sich", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "NN", "$,", "PPER", "VVFIN", "PRF", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "wenn man noch mehr versteht", "tokens": ["wenn", "man", "noch", "mehr", "ver\u00b7steht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "als sch\u00f6ne Str\u00e4u\u00dfe zu binden!", "tokens": ["als", "sch\u00f6\u00b7ne", "Str\u00e4u\u00b7\u00dfe", "zu", "bin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Mein Garten wird nicht verschmachten,", "tokens": ["Mein", "Gar\u00b7ten", "wird", "nicht", "ver\u00b7schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "gef\u00e4llt er manchem schlecht.", "tokens": ["ge\u00b7f\u00e4llt", "er", "man\u00b7chem", "schlecht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er kann euern Beifall verachten,", "tokens": ["Er", "kann", "eu\u00b7ern", "Bei\u00b7fall", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "und euer Schimpfen erst recht;", "tokens": ["und", "eu\u00b7er", "Schimp\u00b7fen", "erst", "recht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "ihn pflegt ein kleiner Held.", "tokens": ["ihn", "pflegt", "ein", "klei\u00b7ner", "Held", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}