{"textgrid.poem.45961": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Rhein\u00fcbergang", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf, ihr starken Siegesbr\u00fcder,", "tokens": ["Auf", ",", "ihr", "star\u00b7ken", "Sie\u00b7ges\u00b7br\u00fc\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Brecht mit Sang und Klang die Nacht,", "tokens": ["Brecht", "mit", "Sang", "und", "Klang", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Singt den Schicksalssternen Lieder,", "tokens": ["Singt", "den", "Schick\u00b7salss\u00b7ter\u00b7nen", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Bis der Tag uns jenseits lacht.", "tokens": ["Bis", "der", "Tag", "uns", "jen\u00b7seits", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}