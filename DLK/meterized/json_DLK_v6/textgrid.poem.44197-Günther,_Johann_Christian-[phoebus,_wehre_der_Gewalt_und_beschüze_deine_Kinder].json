{"textgrid.poem.44197": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[phoebus, wehre der Gewalt und besch\u00fcze deine Kinder]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Phoebus, wehre der Gewalt und besch\u00fcze deine Kinder", "tokens": ["Phoe\u00b7bus", ",", "weh\u00b7re", "der", "Ge\u00b7walt", "und", "be\u00b7sch\u00fc\u00b7ze", "dei\u00b7ne", "Kin\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "ART", "NN", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Oder nenne dich nicht mehr der gesunden Kunst Erfinder.", "tokens": ["O\u00b7der", "nen\u00b7ne", "dich", "nicht", "mehr", "der", "ge\u00b7sun\u00b7den", "Kunst", "Er\u00b7fin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Hastu weder Kraut noch Pflaster? Warum legt dir das Geschrey", "tokens": ["Has\u00b7tu", "we\u00b7der", "Kraut", "noch", "Pflas\u00b7ter", "?", "Wa\u00b7rum", "legt", "dir", "das", "Ge\u00b7schrey"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "NN", "ADV", "NN", "$.", "PWAV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Der vor diesem gro\u00dfen Dichter einen falschen Titul bey?", "tokens": ["Der", "vor", "die\u00b7sem", "gro\u00b7\u00dfen", "Dich\u00b7ter", "ei\u00b7nen", "fal\u00b7schen", "Ti\u00b7tul", "bey", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PDAT", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Jezt beweise, wer du seyst. Siehestu nicht auf den Bergen", "tokens": ["Jezt", "be\u00b7wei\u00b7se", ",", "wer", "du", "seyst", ".", "Sie\u00b7hes\u00b7tu", "nicht", "auf", "den", "Ber\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "PWS", "PPER", "VAFIN", "$.", "PPER", "PTKNEG", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Einen Wald voll Ungemach, ein Gedr\u00e4nge von den S\u00e4rgen?", "tokens": ["Ei\u00b7nen", "Wald", "voll", "Un\u00b7ge\u00b7mach", ",", "ein", "Ge\u00b7dr\u00e4n\u00b7ge", "von", "den", "S\u00e4r\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "L\u00e4stu denn den frechen Parzen noch dergleichen Unfug zu,", "tokens": ["L\u00e4s\u00b7tu", "denn", "den", "fre\u00b7chen", "Par\u00b7zen", "noch", "derg\u00b7lei\u00b7chen", "Un\u00b7fug", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "ADJA", "NN", "ADV", "PIS", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Da\u00df ihr Rocken deinem Scepter so verwegen Eingrif thu?", "tokens": ["Da\u00df", "ihr", "Ro\u00b7cken", "dei\u00b7nem", "Scep\u00b7ter", "so", "ver\u00b7we\u00b7gen", "Ein\u00b7grif", "thu", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "H\u00e4nge doch dein Harfenspiel an die halb verwaisten Linden,", "tokens": ["H\u00e4n\u00b7ge", "doch", "dein", "Har\u00b7fen\u00b7spiel", "an", "die", "halb", "ver\u00b7wais\u00b7ten", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "APPR", "ART", "ADJD", "ADJA", "NE", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Die es warlich nicht verdient, da\u00df sie Brand und Gift empfinden.", "tokens": ["Die", "es", "war\u00b7lich", "nicht", "ver\u00b7dient", ",", "da\u00df", "sie", "Brand", "und", "Gift", "emp\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PTKNEG", "VVPP", "$,", "KOUS", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "H\u00e4ng es mit verkehrten Saythen und betr\u00fcbten Fingern auf", "tokens": ["H\u00e4ng", "es", "mit", "ver\u00b7kehr\u00b7ten", "Say\u00b7then", "und", "be\u00b7tr\u00fcb\u00b7ten", "Fin\u00b7gern", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "APPR"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Und verl\u00e4ngre deinen S\u00f6hnen den verk\u00fcrzten Lebenslauf.", "tokens": ["Und", "ver\u00b7l\u00e4ng\u00b7re", "dei\u00b7nen", "S\u00f6h\u00b7nen", "den", "ver\u00b7k\u00fcrz\u00b7ten", "Le\u00b7bens\u00b7lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Dir zu Schimpfe, gieb nur Acht, sieht man einen nach dem andern", "tokens": ["Dir", "zu", "Schimp\u00b7fe", ",", "gieb", "nur", "Acht", ",", "sieht", "man", "ei\u00b7nen", "nach", "dem", "an\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "NN", "$,", "VVIMP", "ADV", "CARD", "$,", "VVFIN", "PIS", "ART", "APPR", "ART", "ADJA"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Von dem vollen Helicon in das Thal des Todes wandern.", "tokens": ["Von", "dem", "vol\u00b7len", "He\u00b7li\u00b7con", "in", "das", "Thal", "des", "To\u00b7des", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.15": {"text": "S\u00e4umstu dich noch etwas l\u00e4nger, ey, so wird dein sch\u00f6ner Hayn", "tokens": ["S\u00e4ums\u00b7tu", "dich", "noch", "et\u00b7was", "l\u00e4n\u00b7ger", ",", "ey", ",", "so", "wird", "dein", "sch\u00f6\u00b7ner", "Hayn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADJD", "$,", "NE", "$,", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Auf den w\u00fcsten Musenh\u00fcgeln deiner Schande Schauplaz seyn.", "tokens": ["Auf", "den", "w\u00fcs\u00b7ten", "Mu\u00b7sen\u00b7h\u00fc\u00b7geln", "dei\u00b7ner", "Schan\u00b7de", "Schau\u00b7plaz", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "NN", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Nimm doch nur das Elend wahr, wie die Pierinnen schmachten,", "tokens": ["Nimm", "doch", "nur", "das", "E\u00b7lend", "wahr", ",", "wie", "die", "Pie\u00b7rin\u00b7nen", "schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ART", "NN", "PTKVZ", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.18": {"text": "Wie das Fieber und der Schlag Mittel, H\u00fclf und Rath verachten,", "tokens": ["Wie", "das", "Fie\u00b7ber", "und", "der", "Schlag", "Mit\u00b7tel", ",", "H\u00fclf", "und", "Rath", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "KON", "ART", "NN", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.19": {"text": "Wie die bleich- und blauen Lippen von dem bla\u00dfen Schaume blehn", "tokens": ["Wie", "die", "bleich", "und", "blau\u00b7en", "Lip\u00b7pen", "von", "dem", "bla\u00b7\u00dfen", "Schau\u00b7me", "blehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "TRUNC", "KON", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Und die eingefallnen Augen schon in jenes Leben sehn.", "tokens": ["Und", "die", "ein\u00b7ge\u00b7fall\u00b7nen", "Au\u00b7gen", "schon", "in", "je\u00b7nes", "Le\u00b7ben", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.21": {"text": "Unruh, Feuer, K\u00e4lt und Schwei\u00df foltern die gel\u00e4hmten Glieder,", "tokens": ["Un\u00b7ruh", ",", "Feu\u00b7er", ",", "K\u00e4lt", "und", "Schwei\u00df", "fol\u00b7tern", "die", "ge\u00b7l\u00e4hm\u00b7ten", "Glie\u00b7der", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "KON", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.22": {"text": "Die Verwirrung hebt das Haupt und die Ohnmacht schl\u00e4gt es nieder.", "tokens": ["Die", "Ver\u00b7wir\u00b7rung", "hebt", "das", "Haupt", "und", "die", "Ohn\u00b7macht", "schl\u00e4gt", "es", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "KON", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Jener sagt mehr, als er denckt, dieser f\u00fchlt nicht, was er f\u00fchlt,", "tokens": ["Je\u00b7ner", "sagt", "mehr", ",", "als", "er", "denckt", ",", "die\u00b7ser", "f\u00fchlt", "nicht", ",", "was", "er", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.24": {"text": "Da der dritte mit den Fingern und den feuchten Federn spielt.", "tokens": ["Da", "der", "drit\u00b7te", "mit", "den", "Fin\u00b7gern", "und", "den", "feuch\u00b7ten", "Fe\u00b7dern", "spielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "APPR", "ART", "NN", "KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.25": {"text": "Dieser wehret sich umsonst in des Todes kalten Armen;", "tokens": ["Die\u00b7ser", "weh\u00b7ret", "sich", "um\u00b7sonst", "in", "des", "To\u00b7des", "kal\u00b7ten", "Ar\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+---+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.26": {"text": "Jener liegt im Qualme still und vermehret das Erbarmen.", "tokens": ["Je\u00b7ner", "liegt", "im", "Qual\u00b7me", "still", "und", "ver\u00b7meh\u00b7ret", "das", "Er\u00b7bar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "VVFIN", "APPRART", "NN", "PTKVZ", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.27": {"text": "Hier beklagt sich eine Seele, da\u00df sie nicht entfliehen darf;", "tokens": ["Hier", "be\u00b7klagt", "sich", "ei\u00b7ne", "See\u00b7le", ",", "da\u00df", "sie", "nicht", "ent\u00b7flie\u00b7hen", "darf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,", "KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.28": {"text": "Denn das Band des z\u00e4hen Fleisches h\u00e4lt und hemmt sie gar zu scharf.", "tokens": ["Denn", "das", "Band", "des", "z\u00e4\u00b7hen", "Flei\u00b7sches", "h\u00e4lt", "und", "hemmt", "sie", "gar", "zu", "scharf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.29": {"text": "\u00c4rger sieht es nirgends aus, und es herrscht kein solches Grauen", "tokens": ["\u00c4r\u00b7ger", "sieht", "es", "nir\u00b7gends", "aus", ",", "und", "es", "herrscht", "kein", "sol\u00b7ches", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "KON", "PPER", "VVFIN", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.30": {"text": "Um die erste Fr\u00fchlingszeit auf den blumenreichen Auen,", "tokens": ["Um", "die", "ers\u00b7te", "Fr\u00fch\u00b7lings\u00b7zeit", "auf", "den", "blu\u00b7men\u00b7rei\u00b7chen", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.31": {"text": "Wenn ein starckes Nachtgewitter die noch gestern frische Pracht,", "tokens": ["Wenn", "ein", "star\u00b7ckes", "Nacht\u00b7ge\u00b7wit\u00b7ter", "die", "noch", "ge\u00b7stern", "fri\u00b7sche", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "--+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Die so bunt gemischte Farben unverhoft zu Schanden macht.", "tokens": ["Die", "so", "bunt", "ge\u00b7mischte", "Far\u00b7ben", "un\u00b7ver\u00b7hoft", "zu", "Schan\u00b7den", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADJA", "NN", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.33": {"text": "Denn da liegt der bunte Klee auf den abgekehrten Rasen,", "tokens": ["Denn", "da", "liegt", "der", "bun\u00b7te", "Klee", "auf", "den", "ab\u00b7ge\u00b7kehr\u00b7ten", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "Bl\u00e4tter, Bl\u00fcthe, Lust und Gra\u00df scheinen gleichsam weggeblasen,", "tokens": ["Bl\u00e4t\u00b7ter", ",", "Bl\u00fc\u00b7the", ",", "Lust", "und", "Gra\u00df", "schei\u00b7nen", "gleich\u00b7sam", "weg\u00b7ge\u00b7bla\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.35": {"text": "Und die \u00fcbergo\u00dfnen Triften weisen durch des Himmels Neid", "tokens": ["Und", "die", "\u00fc\u00b7ber\u00b7go\u00df\u00b7nen", "Trif\u00b7ten", "wei\u00b7sen", "durch", "des", "Him\u00b7mels", "Neid"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.36": {"text": "Auf den halb zerknickten Stengeln Bilder der Verg\u00e4ngligkeit.", "tokens": ["Auf", "den", "halb", "zer\u00b7knick\u00b7ten", "Sten\u00b7geln", "Bil\u00b7der", "der", "Ver\u00b7g\u00e4n\u00b7glig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.37": {"text": "Welchen Fremdling dauret nicht, liebt er anders Kunst und Tugend,", "tokens": ["Wel\u00b7chen", "Fremd\u00b7