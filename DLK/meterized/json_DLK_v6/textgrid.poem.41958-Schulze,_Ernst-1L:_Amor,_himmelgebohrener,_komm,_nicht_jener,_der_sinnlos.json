{"textgrid.poem.41958": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Amor, himmelgebohrener, komm, nicht jener, der sinnlos", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Amor, himmelgebohrener, komm, nicht jener, der sinnlos", "tokens": ["A\u00b7mor", ",", "him\u00b7mel\u00b7ge\u00b7boh\u00b7re\u00b7ner", ",", "komm", ",", "nicht", "je\u00b7ner", ",", "der", "sinn\u00b7los"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ADJA", "$,", "VVFIN", "$,", "PTKNEG", "PDS", "$,", "PRELS", "ADJD"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "In's wildwogende Meer frevelnder L\u00fcste sich senkt,", "tokens": ["In's", "wild\u00b7wo\u00b7gen\u00b7de", "Meer", "fre\u00b7veln\u00b7der", "L\u00fcs\u00b7te", "sich", "senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Nicht du verderblicher Gott, der tief in die Herzen den Pfeil uns", "tokens": ["Nicht", "du", "ver\u00b7derb\u00b7li\u00b7cher", "Gott", ",", "der", "tief", "in", "die", "Her\u00b7zen", "den", "Pfeil", "uns"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PPER", "ADJA", "NN", "$,", "PRELS", "ADJD", "APPR", "ART", "NN", "ART", "NN", "PPER"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Schleudert und hoffnungslos ewige Gluthen erweckt:", "tokens": ["Schleu\u00b7dert", "und", "hoff\u00b7nungs\u00b7los", "e\u00b7wi\u00b7ge", "Glu\u00b7then", "er\u00b7weckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.5": {"text": "Nein, du reizendes Kind, du fl\u00fcchtiges, welches die G\u00f6tter", "tokens": ["Nein", ",", "du", "rei\u00b7zen\u00b7des", "Kind", ",", "du", "fl\u00fcch\u00b7ti\u00b7ges", ",", "wel\u00b7ches", "die", "G\u00f6t\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "ADJA", "NN", "$,", "PPER", "ADJA", "$,", "PRELS", "ART", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Mit \u00e4therischem Band lieblich und lose verkn\u00fcpft,", "tokens": ["Mit", "\u00e4t\u00b7he\u00b7ri\u00b7schem", "Band", "lieb\u00b7lich", "und", "lo\u00b7se", "ver\u00b7kn\u00fcpft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Komm, du romantischer Knabe, der Abenteuer Besch\u00fctzer,", "tokens": ["Komm", ",", "du", "ro\u00b7man\u00b7ti\u00b7scher", "Kna\u00b7be", ",", "der", "A\u00b7bent\u00b7eu\u00b7er", "Be\u00b7sch\u00fct\u00b7zer", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Zarten Gefl\u00fcsters Freund, Freund der verschwiegenen Lust,", "tokens": ["Zar\u00b7ten", "Ge\u00b7fl\u00fcs\u00b7ters", "Freund", ",", "Freund", "der", "ver\u00b7schwie\u00b7ge\u00b7nen", "Lust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.9": {"text": "Der du keusch und \u00fcppig zugleich und fl\u00fcchtig und treu bist,", "tokens": ["Der", "du", "keusch", "und", "\u00fcp\u00b7pig", "zu\u00b7gleich", "und", "fl\u00fcch\u00b7tig", "und", "treu", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "KON", "ADJD", "ADV", "KON", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Feind der Fesseln und doch immer in Fesseln geschmiegt,", "tokens": ["Feind", "der", "Fes\u00b7seln", "und", "doch", "im\u00b7mer", "in", "Fes\u00b7seln", "ge\u00b7schmiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ADV", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Du, der Schmerz und Freude gew\u00e4hrt, doch nimmer in Tr\u00fcbsinn", "tokens": ["Du", ",", "der", "Schmerz", "und", "Freu\u00b7de", "ge\u00b7w\u00e4hrt", ",", "doch", "nim\u00b7mer", "in", "Tr\u00fcb\u00b7sinn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "NN", "KON", "NN", "VVPP", "$,", "ADV", "ADV", "APPR", "NN"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Unsere Schmerzen und nie wandelt in Ekel die Lust.", "tokens": ["Un\u00b7se\u00b7re", "Schmer\u00b7zen", "und", "nie", "wan\u00b7delt", "in", "E\u00b7kel", "die", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ADV", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.13": {"text": "Komm vom Himmel herab, und bring mir die reizenden M\u00e4dchen,", "tokens": ["Komm", "vom", "Him\u00b7mel", "her\u00b7ab", ",", "und", "bring", "mir", "die", "rei\u00b7zen\u00b7den", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADV", "$,", "KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Welche dich immer umbl\u00fchn, bring mir die Grazien mit.", "tokens": ["Wel\u00b7che", "dich", "im\u00b7mer", "um\u00b7bl\u00fchn", ",", "bring", "mir", "die", "Gra\u00b7zi\u00b7en", "mit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVINF", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Sieh, schon nahte die Stunde, worin dein Schwesterchen ehmals", "tokens": ["Sieh", ",", "schon", "nah\u00b7te", "die", "Stun\u00b7de", ",", "wo\u00b7rin", "dein", "Schwes\u00b7ter\u00b7chen", "eh\u00b7mals"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "NN", "$,", "PWAV", "PPOSAT", "NN", "ADV"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.16": {"text": "Unserem Lichte zuerst heiter entgegengelacht.", "tokens": ["Un\u00b7se\u00b7rem", "Lich\u00b7te", "zu\u00b7erst", "hei\u00b7ter", "ent\u00b7ge\u00b7gen\u00b7ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.17": {"text": "Damals war dir der Tag ein Fest; siegk\u00fcndende Lieder", "tokens": ["Da\u00b7mals", "war", "dir", "der", "Tag", "ein", "Fest", ";", "sieg\u00b7k\u00fcn\u00b7den\u00b7de", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "$.", "ADJA", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Wehten den fr\u00fchesten Schlaf auf die Gebohrene hin.", "tokens": ["Weh\u00b7ten", "den", "fr\u00fc\u00b7hes\u00b7ten", "Schlaf", "auf", "die", "Ge\u00b7boh\u00b7re\u00b7ne", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.19": {"text": "Schalkheit hauchte dein Ku\u00df ihr in's Herz, fantastischen Leichtsinn,", "tokens": ["Schalk\u00b7heit", "hauch\u00b7te", "dein", "Ku\u00df", "ihr", "in's", "Herz", ",", "fan\u00b7tas\u00b7ti\u00b7schen", "Leicht\u00b7sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "PPOSAT", "APPRART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.20": {"text": "T\u00e4ndelnden Witz und der stets wechselnden Laune Begier.", "tokens": ["T\u00e4n\u00b7deln\u00b7den", "Witz", "und", "der", "stets", "wech\u00b7seln\u00b7den", "Lau\u00b7ne", "Be\u00b7gier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ART", "ADV", "ADJA", "NN", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.21": {"text": "Doch was du muthwillig ihr gabst, das schm\u00fcckte die Charis,", "tokens": ["Doch", "was", "du", "mut\u00b7hwil\u00b7lig", "ihr", "gabst", ",", "das", "schm\u00fcck\u00b7te", "die", "Cha\u00b7ris", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.22": {"text": "Und um's Dornengeb\u00fcsch webte sie Rosen umher.", "tokens": ["Und", "um's", "Dor\u00b7nen\u00b7ge\u00b7b\u00fcsch", "web\u00b7te", "sie", "Ro\u00b7sen", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.23": {"text": "Feyre den Tag auch jetzt; denn sie ward nun gr\u00f6\u00dfer und holder;", "tokens": ["Fey\u00b7re", "den", "Tag", "auch", "jetzt", ";", "denn", "sie", "ward", "nun", "gr\u00f6\u00b7\u00dfer", "und", "hol\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ADV", "$.", "KON", "PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.24": {"text": "In der Gestalt und im Geist gleicht sie, du Schelmischer, dir.", "tokens": ["In", "der", "Ge\u00b7stalt", "und", "im", "Geist", "gleicht", "sie", ",", "du", "Schel\u00b7mi\u00b7scher", ",", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "APPRART", "NN", "VVFIN", "PPER", "$,", "PPER", "NN", "$,", "PPER", "$."], "meter": "+--+--++-+-+-+", "measure": "dactylic.di.plus"}, "line.25": {"text": "Schweb' um's seidene Bett mit der lieblichen Schaar; noch ruht sie,", "tokens": ["Schweb'", "um's", "sei\u00b7de\u00b7ne", "Bett", "mit", "der", "lieb\u00b7li\u00b7chen", "Schaar", ";", "noch", "ruht", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+--+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.26": {"text": "Sanft um den rosigen Mund weht das Gelispel des Schlafs.", "tokens": ["Sanft", "um", "den", "ro\u00b7si\u00b7gen", "Mund", "weht", "das", "Ge\u00b7lis\u00b7pel", "des", "Schlafs", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "---+--++--+--+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Wehre mit sch\u00fctzender Schwinge den b\u00f6sen Tr\u00e4umen, den Ungl\u00fcck", "tokens": ["Weh\u00b7re", "mit", "sch\u00fct\u00b7zen\u00b7der", "Schwin\u00b7ge", "den", "b\u00f6\u00b7sen", "Tr\u00e4u\u00b7men", ",", "den", "Un\u00b7gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.28": {"text": "K\u00fcndenden, welche den Schoo\u00df f\u00fcllen der br\u00fctenden Nacht;", "tokens": ["K\u00fcn\u00b7den\u00b7den", ",", "wel\u00b7che", "den", "Schoo\u00df", "f\u00fcl\u00b7len", "der", "br\u00fc\u00b7ten\u00b7den", "Nacht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.29": {"text": "La\u00df nur die freundlichen leise sich nahn, die Kinder Auroras,", "tokens": ["La\u00df", "nur", "die", "freund\u00b7li\u00b7chen", "lei\u00b7se", "sich", "nahn", ",", "die", "Kin\u00b7der", "Au\u00b7ro\u00b7ras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN", "PRF", "ADJA", "$,", "ART", "NN", "NE", "$,"], "meter": "+--+--+--+-+-+-+", "measure": "dactylic.tri.plus"}, "line.30": {"text": "Da\u00df sie mit rosigem Tanz schm\u00fccken das stille Gemach.", "tokens": ["Da\u00df", "sie", "mit", "ro\u00b7si\u00b7gem", "Tanz", "schm\u00fc\u00b7cken", "das", "stil\u00b7le", "Ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Mal' auf die Fl\u00fcgel des gaukelnden Schwarms kunstvoll das Verlangen,", "tokens": ["Mal'", "auf", "die", "Fl\u00fc\u00b7gel", "des", "gau\u00b7keln\u00b7den", "Schwarms", "kunst\u00b7voll", "das", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VMFIN", "ART", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.32": {"text": "Welches der Reizenden Blick rings in den Herzen erregt;", "tokens": ["Wel\u00b7ches", "der", "Rei\u00b7zen\u00b7den", "Blick", "rings", "in", "den", "Her\u00b7zen", "er\u00b7regt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.33": {"text": "Malet, ihr Huldg\u00f6ttinnen, der unausprechlichen Anmuth", "tokens": ["Ma\u00b7let", ",", "ihr", "Huld\u00b7g\u00f6t\u00b7tin\u00b7nen", ",", "der", "un\u00b7aus\u00b7prech\u00b7li\u00b7chen", "An\u00b7muth"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.34": {"text": "Zaubergewalt und den Scherz, welcher sie ewig umbuhlt:", "tokens": ["Zau\u00b7ber\u00b7ge\u00b7walt", "und", "den", "Scherz", ",", "wel\u00b7cher", "sie", "e\u00b7wig", "um\u00b7buhlt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.35": {"text": "Da\u00df sie sich selber erblick' in des Traums irrsamer Gestaltung;", "tokens": ["Da\u00df", "sie", "sich", "sel\u00b7ber", "er\u00b7blick'", "in", "des", "Traums", "irr\u00b7sa\u00b7mer", "Ge\u00b7stal\u00b7tung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+--+-++-+-", "measure": "iambic.septa.relaxed"}, "line.36": {"text": "Ach, kein sch\u00f6neres Bild zeigt ihr der s\u00fc\u00dfeste Traum!", "tokens": ["Ach", ",", "kein", "sch\u00f6\u00b7ne\u00b7res", "Bild", "zeigt", "ihr", "der", "s\u00fc\u00b7\u00dfes\u00b7te", "Traum", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIAT", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.37": {"text": "L\u00e4cheln wird sie im Schlaf, ihr Gl\u00fccklichen! F\u00fclle den K\u00f6cher,", "tokens": ["L\u00e4\u00b7cheln", "wird", "sie", "im", "Schlaf", ",", "ihr", "Gl\u00fcck\u00b7li\u00b7chen", "!", "F\u00fcl\u00b7le", "den", "K\u00f6\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPRART", "NN", "$,", "PPOSAT", "NN", "$.", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.38": {"text": "Amor, damit! O ahmt, Grazien, ahmet es nach!", "tokens": ["A\u00b7mor", ",", "da\u00b7mit", "!", "O", "ahmt", ",", "Gra\u00b7zi\u00b7en", ",", "