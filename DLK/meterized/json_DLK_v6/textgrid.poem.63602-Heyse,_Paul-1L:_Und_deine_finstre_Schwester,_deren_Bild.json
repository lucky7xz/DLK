{"textgrid.poem.63602": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und deine finstre Schwester, deren Bild", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und deine finstre Schwester, deren Bild", "tokens": ["Und", "dei\u00b7ne", "finst\u00b7re", "Schwes\u00b7ter", ",", "de\u00b7ren", "Bild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Graun den Toren, die sich sicher w\u00e4hnen,", "tokens": ["Ein", "Graun", "den", "To\u00b7ren", ",", "die", "sich", "si\u00b7cher", "w\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auch sie hat mir das Herz mit Trost gestillt.", "tokens": ["Auch", "sie", "hat", "mir", "das", "Herz", "mit", "Trost", "ge\u00b7stillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PPER", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Ihr gro\u00dfes Aug ist d\u00fcrr und leer von Tr\u00e4nen.", "tokens": ["Ihr", "gro\u00b7\u00dfes", "Aug", "ist", "d\u00fcrr", "und", "leer", "von", "Tr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "KON", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein seltsam L\u00e4cheln irrt um ihren Mund;", "tokens": ["Ein", "selt\u00b7sam", "L\u00e4\u00b7cheln", "irrt", "um", "ih\u00b7ren", "Mund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die fahle Lippe bebt an bleichen Z\u00e4hnen.", "tokens": ["Die", "fah\u00b7le", "Lip\u00b7pe", "bebt", "an", "blei\u00b7chen", "Z\u00e4h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sie rauft ihr Haar nicht, schl\u00e4gt die Brust nicht wun", "tokens": ["Sie", "rauft", "ihr", "Haar", "nicht", ",", "schl\u00e4gt", "die", "Brust", "nicht", "wun"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$,", "VVFIN", "ART", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sie gleicht dem Bruder; aber kalt und fremd", "tokens": ["Sie", "gleicht", "dem", "Bru\u00b7der", ";", "a\u00b7ber", "kalt", "und", "fremd"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Bohrt sich ihr Blick bis in der Seele Grund.", "tokens": ["Bohrt", "sich", "ihr", "Blick", "bis", "in", "der", "See\u00b7le", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "APPR", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die Linke steinern in das Kinn gestemmt,", "tokens": ["Die", "Lin\u00b7ke", "stei\u00b7nern", "in", "das", "Kinn", "ge\u00b7stemmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die Rechte starr am Dolch, sitzt sie und br\u00fctet", "tokens": ["Die", "Rech\u00b7te", "starr", "am", "Dolch", ",", "sitzt", "sie", "und", "br\u00fc\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "$,", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und harrt der Zeit, da nichts den Willen hemmt.", "tokens": ["Und", "harrt", "der", "Zeit", ",", "da", "nichts", "den", "Wil\u00b7len", "hemmt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KOUS", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Sie stiert so scheu, wie wer Vergrabnes h\u00fctet.", "tokens": ["Sie", "stiert", "so", "scheu", ",", "wie", "wer", "Ver\u00b7grab\u00b7nes", "h\u00fc\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PWAV", "PWS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Sie wei\u00df, man w\u00fcrde sie an Ketten legen,", "tokens": ["Sie", "wei\u00df", ",", "man", "w\u00fcr\u00b7de", "sie", "an", "Ket\u00b7ten", "le\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PIS", "VAFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Erriete man, was heimlich in ihr w\u00fctet.", "tokens": ["Er\u00b7rie\u00b7te", "man", ",", "was", "heim\u00b7lich", "in", "ihr", "w\u00fc\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Doch was sie immer sinnt, es ist zum Segen.", "tokens": ["Doch", "was", "sie", "im\u00b7mer", "sinnt", ",", "es", "ist", "zum", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "$,", "PPER", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Mag sie die Torenwelt \u00bbVerzweiflung\u00ab taufen", "tokens": ["Mag", "sie", "die", "To\u00b7ren\u00b7welt", "\u00bb", "Ver\u00b7zwei\u00b7flung", "\u00ab", "tau\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "$(", "NN", "$(", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und f\u00fcrchten, wie wir Nachtunholde pflegen:", "tokens": ["Und", "f\u00fcrch\u00b7ten", ",", "wie", "wir", "Nach\u00b7tun\u00b7hol\u00b7de", "pfle\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Sie trachtet nur, Gefangne loszukaufen", "tokens": ["Sie", "trach\u00b7tet", "nur", ",", "Ge\u00b7fang\u00b7ne", "los\u00b7zu\u00b7kau\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Aus Lebenshaft und Seelen zu befrein,", "tokens": ["Aus", "Le\u00b7bens\u00b7haft", "und", "See\u00b7len", "zu", "be\u00b7fr\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Die voll von Jammer sind zum \u00dcberlaufen.", "tokens": ["Die", "voll", "von", "Jam\u00b7mer", "sind", "zum", "\u00dc\u00b7berl\u00b7au\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Drum sollte \u00bbletzter Trost\u00ab ihr Name sein,", "tokens": ["Drum", "soll\u00b7te", "\u00bb", "letz\u00b7ter", "Trost", "\u00ab", "ihr", "Na\u00b7me", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "$(", "ADJA", "NN", "$(", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Denn wo mit Schmach und heillos bittrer Not", "tokens": ["Denn", "wo", "mit", "Schmach", "und", "heil\u00b7los", "bit\u00b7trer", "Not"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "NN", "KON", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Geschlagen wird ein zitterndes Gebein,", "tokens": ["Ge\u00b7schla\u00b7gen", "wird", "ein", "zit\u00b7tern\u00b7des", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Und weder Gott noch Teufel Hilfe bot,", "tokens": ["Und", "we\u00b7der", "Gott", "noch", "Teu\u00b7fel", "Hil\u00b7fe", "bot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KON", "NN", "ADV", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Da tritt sie zu dem hoffnungslos Gequ\u00e4lten", "tokens": ["Da", "tritt", "sie", "zu", "dem", "hoff\u00b7nungs\u00b7los", "Ge\u00b7qu\u00e4l\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Und raunt ihm zu: Erw\u00e4hle selbst den Tod!", "tokens": ["Und", "raunt", "ihm", "zu", ":", "Er\u00b7w\u00e4h\u00b7le", "selbst", "den", "Tod", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$.", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Ich hauchte nur dich an, und dich Gest\u00e4hlten", "tokens": ["Ich", "hauch\u00b7te", "nur", "dich", "an", ",", "und", "dich", "Ge\u00b7st\u00e4hl\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "PTKVZ", "$,", "KON", "PPER", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Durchschauert Kraft, die schwere Kerkert\u00fcr", "tokens": ["Durch\u00b7schau\u00b7ert", "Kraft", ",", "die", "schwe\u00b7re", "Ker\u00b7ker\u00b7t\u00fcr"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Zu sprengen, eh' die Henker dich entseelten.", "tokens": ["Zu", "spren\u00b7gen", ",", "eh'", "die", "Hen\u00b7ker", "dich", "ent\u00b7seel\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Willst du dich beugen jeder Ungeb\u00fchr,", "tokens": ["Willst", "du", "dich", "beu\u00b7gen", "je\u00b7der", "Un\u00b7ge\u00b7b\u00fchr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Auch noch die rechte Wange leihn dem Streiche,", "tokens": ["Auch", "noch", "die", "rech\u00b7te", "Wan\u00b7ge", "leihn", "dem", "Strei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Ein B\u00fc\u00dfender und wei\u00dft doch nicht wof\u00fcr? \u2013", "tokens": ["Ein", "B\u00fc\u00b7\u00dfen\u00b7der", "und", "wei\u00dft", "doch", "nicht", "wo\u00b7f\u00fcr", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "ADV", "PTKNEG", "PWAV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "So stillt den \u00c4chzenden die stille Bleiche,", "tokens": ["So", "stillt", "den", "\u00c4ch\u00b7zen\u00b7den", "die", "stil\u00b7le", "Blei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Und wenn ihm fremd ward jeder Rat der Welt,", "tokens": ["Und", "wenn", "ihm", "fremd", "ward", "je\u00b7der", "Rat", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VAFIN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "O sie ber\u00e4t ihn gut, die Listenreiche.", "tokens": ["O", "sie", "be\u00b7r\u00e4t", "ihn", "gut", ",", "die", "Lis\u00b7ten\u00b7rei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "PPER", "ADJD", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Auch mir, du Mitleidvolle, bleib gesellt,", "tokens": ["Auch", "mir", ",", "du", "Mit\u00b7leid\u00b7vol\u00b7le", ",", "bleib", "ge\u00b7sellt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PPER", "NN", "$,", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Ich flehe dich, wenn mir ein Los beschieden,", "tokens": ["Ich", "fle\u00b7he", "dich", ",", "wenn", "mir", "ein", "Los", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Das jeden Lebensbecher mir verg\u00e4llt.", "tokens": ["Das", "je\u00b7den", "Le\u00b7bens\u00b7be\u00b7cher", "mir", "ver\u00b7g\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Die Bande, die uns an das Dasein schmieden,", "tokens": ["Die", "Ban\u00b7de", ",", "die", "uns", "an", "das", "Da\u00b7sein", "schmie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Hilf mir sie sprengen, Freundin, f\u00fchre du", "tokens": ["Hilf", "mir", "sie", "spren\u00b7gen", ",", "Freun\u00b7din", ",", "f\u00fch\u00b7re", "du"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "PPER", "PPER", "VVFIN", "$,", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Die stolze Seele, die das Gl\u00fcck gemieden,", "tokens": ["Die", "stol\u00b7ze", "See\u00b7le", ",", "die", "das", "Gl\u00fcck", "ge\u00b7mie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Den sanften Sternen ew'ger N\u00e4chte zu!", "tokens": ["Den", "sanf\u00b7ten", "Ster\u00b7nen", "ew'\u00b7ger", "N\u00e4ch\u00b7te", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}