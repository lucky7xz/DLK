{"dta.poem.21552": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Verruchtes Epigonenthum,", "tokens": ["Ver\u00b7ruch\u00b7tes", "E\u00b7pi\u00b7go\u00b7nen\u00b7thum", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Egypter- und Teutonenthum,", "tokens": ["E\u00b7gyp\u00b7ter", "und", "Teu\u00b7to\u00b7nen\u00b7thum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df dich der Teufel brate!", "tokens": ["Da\u00df", "dich", "der", "Teu\u00b7fel", "bra\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schon l\u00e4ngst sind wir fascikelsatt,", "tokens": ["Schon", "l\u00e4ngst", "sind", "wir", "fas\u00b7ci\u00b7kel\u00b7satt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Grinst doch durch jedes Titelblatt", "tokens": ["Grinst", "doch", "durch", "je\u00b7des", "Ti\u00b7tel\u00b7blatt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Dante\u2019sche ", "tokens": ["Das", "Dan\u00b7te'\u00b7sche"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}}}}}