{"textgrid.poem.66488": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Morgendufte kommt vom Waldesrande", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Morgendufte kommt vom Waldesrande", "tokens": ["Im", "Mor\u00b7gen\u00b7duf\u00b7te", "kommt", "vom", "Wal\u00b7des\u00b7ran\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das junge Reh zum Wiesengrund geschritten,", "tokens": ["Das", "jun\u00b7ge", "Reh", "zum", "Wie\u00b7sen\u00b7grund", "ge\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da noch die D\u00e4mmrung h\u00fcllt die tiefren Lande.", "tokens": ["Da", "noch", "die", "D\u00e4mm\u00b7rung", "h\u00fcllt", "die", "tief\u00b7ren", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es pr\u00fcft den Thau, und blickt in Thalesmitten", "tokens": ["Es", "pr\u00fcft", "den", "Thau", ",", "und", "blickt", "in", "Tha\u00b7les\u00b7mit\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit klugem Aug' empor zum H\u00f6henkranze,", "tokens": ["Mit", "klu\u00b7gem", "Aug'", "em\u00b7por", "zum", "H\u00f6\u00b7hen\u00b7kran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu dem die ersten Sonnenlichter glitten.", "tokens": ["Zu", "dem", "die", "ers\u00b7ten", "Son\u00b7nen\u00b7lich\u00b7ter", "glit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Noch scheucht es nicht des Tages Strahlenlanze", "tokens": ["Noch", "scheucht", "es", "nicht", "des", "Ta\u00b7ges", "Strah\u00b7len\u00b7lan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur\u00fcck zum Wald, doch stutzt es bei dem Rufen", "tokens": ["Zu\u00b7r\u00fcck", "zum", "Wald", ",", "doch", "stutzt", "es", "bei", "dem", "Ru\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Adlers, der sich wiegt im Aetherglanze.", "tokens": ["Des", "Ad\u00b7lers", ",", "der", "sich", "wiegt", "im", "A\u00b7e\u00b7ther\u00b7glan\u00b7ze", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Schon sichrer blickt es, denn auf leichten Hufen", "tokens": ["Schon", "sich\u00b7rer", "blickt", "es", ",", "denn", "auf", "leich\u00b7ten", "Hu\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erscheint ihm der Genossen muntre Heerde,", "tokens": ["Er\u00b7scheint", "ihm", "der", "Ge\u00b7nos\u00b7sen", "mun\u00b7tre", "Heer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum fr\u00fchen Mahl, herab die Rasenstufen.", "tokens": ["Zum", "fr\u00fc\u00b7hen", "Mahl", ",", "her\u00b7ab", "die", "Ra\u00b7sen\u00b7stu\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und w\u00e4hrend Stille deckt den Fleck der Erde,", "tokens": ["Und", "w\u00e4h\u00b7rend", "Stil\u00b7le", "deckt", "den", "Fleck", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der sie geboren, harrt die Schaar, vertrauend,", "tokens": ["Der", "sie", "ge\u00b7bo\u00b7ren", ",", "harrt", "die", "Schaar", ",", "ver\u00b7trau\u00b7end", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "VVFIN", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df Raum und Stunde sie besch\u00fctzen werde.", "tokens": ["Da\u00df", "Raum", "und", "Stun\u00b7de", "sie", "be\u00b7sch\u00fct\u00b7zen", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Doch \u00fcber Buchenwipfel, die noch thauend", "tokens": ["Doch", "\u00fc\u00b7ber", "Bu\u00b7chen\u00b7wip\u00b7fel", ",", "die", "noch", "thau\u00b7end"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "$,", "PRELS", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Nachtgew\u00f6lke, dringt des Tages Sendung", "tokens": ["Vom", "Nacht\u00b7ge\u00b7w\u00f6l\u00b7ke", ",", "dringt", "des", "Ta\u00b7ges", "Sen\u00b7dung"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Lichtern schon lebend'ger niederschauend;", "tokens": ["In", "Lich\u00b7tern", "schon", "le\u00b7ben\u00b7d'\u00b7ger", "nie\u00b7der\u00b7schau\u00b7end", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Und pl\u00f6tzlich durch der H\u00fcgel offne Wendung", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "durch", "der", "H\u00fc\u00b7gel", "off\u00b7ne", "Wen\u00b7dung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gie\u00dft sich die Strahlenfluth in vollem Gusse,", "tokens": ["Gie\u00dft", "sich", "die", "Strah\u00b7len\u00b7fluth", "in", "vol\u00b7lem", "Gus\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Thal erf\u00fcllend mit des Lichts Verschwendung.", "tokens": ["Das", "Thal", "er\u00b7f\u00fcl\u00b7lend", "mit", "des", "Lichts", "Ver\u00b7schwen\u00b7dung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "O welch ein Bild, geweckt vom Morgenkusse", "tokens": ["O", "welch", "ein", "Bild", ",", "ge\u00b7weckt", "vom", "Mor\u00b7gen\u00b7kus\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PIAT", "ART", "NN", "$,", "VVPP", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Sonne! Langsam sich zur Tiefe neigend", "tokens": ["Der", "Son\u00b7ne", "!", "Lang\u00b7sam", "sich", "zur", "Tie\u00b7fe", "nei\u00b7gend"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ADJD", "PRF", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Matten duftig Gr\u00fcn, vom Rieselflusse.", "tokens": ["Der", "Mat\u00b7ten", "duf\u00b7tig", "Gr\u00fcn", ",", "vom", "Rie\u00b7sel\u00b7flus\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Der Ohm durchschl\u00e4ngelt; W\u00e4lder, aufw\u00e4rts steigend", "tokens": ["Der", "Ohm", "durch\u00b7schl\u00e4n\u00b7gelt", ";", "W\u00e4l\u00b7der", ",", "auf\u00b7w\u00e4rts", "stei\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVPP", "$.", "NN", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu H\u00fcgelreihen, erst nur leicht gehoben,", "tokens": ["Zu", "H\u00fc\u00b7gel\u00b7rei\u00b7hen", ",", "erst", "nur", "leicht", "ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann gr\u00f6\u00dfren Schwungs gedehnte Formen zeigend;", "tokens": ["Dann", "gr\u00f6\u00df\u00b7ren", "Schwungs", "ge\u00b7dehn\u00b7te", "For\u00b7men", "zei\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Sich \u00fcberbauend, bis die H\u00e4upter droben", "tokens": ["Sich", "\u00fc\u00b7berb\u00b7au\u00b7end", ",", "bis", "die", "H\u00e4up\u00b7ter", "dro\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "VVPP", "$,", "KOUS", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In's Thal her\u00fcber schau'n, das fern im Weiten", "tokens": ["In's", "Thal", "her\u00b7\u00fc\u00b7ber", "schau'n", ",", "das", "fern", "im", "Wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVINF", "$,", "PRELS", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich schlie\u00dft, in zarten Farbenduft verwoben.", "tokens": ["Sich", "schlie\u00dft", ",", "in", "zar\u00b7ten", "Far\u00b7ben\u00b7duft", "ver\u00b7wo\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Wer sich zuerst in diesen Waldgebreiten", "tokens": ["Wer", "sich", "zu\u00b7erst", "in", "die\u00b7sen", "Wald\u00b7ge\u00b7brei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PRF", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die St\u00e4tte gr\u00fcndete, verstand zu bauen", "tokens": ["Die", "St\u00e4t\u00b7te", "gr\u00fcn\u00b7de\u00b7te", ",", "ver\u00b7stand", "zu", "bau\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Wohnung, sicher vor der Tage Streiten.", "tokens": ["Die", "Woh\u00b7nung", ",", "si\u00b7cher", "vor", "der", "Ta\u00b7ge", "Strei\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Dort unter Buchen ist das Haus zu schauen,", "tokens": ["Dort", "un\u00b7ter", "Bu\u00b7chen", "ist", "das", "Haus", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gesch\u00fctzt vom Berg, die Stirn zur Ferne richtend,", "tokens": ["Ge\u00b7sch\u00fctzt", "vom", "Berg", ",", "die", "Stirn", "zur", "Fer\u00b7ne", "rich\u00b7tend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$,", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu F\u00fc\u00dfen allen Glanz der Wiesenauen.", "tokens": ["Zu", "F\u00fc\u00b7\u00dfen", "al\u00b7len", "Glanz", "der", "Wie\u00b7se\u00b7nau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Nicht ist's ein Felsenbau, von welchem dichtend", "tokens": ["Nicht", "ist's", "ein", "Fel\u00b7sen\u00b7bau", ",", "von", "wel\u00b7chem", "dich\u00b7tend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "VAFIN", "ART", "NN", "$,", "APPR", "PWAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Sage spr\u00e4ch' in grauer Tr\u00fcmmersprache,", "tokens": ["Die", "Sa\u00b7ge", "spr\u00e4ch'", "in", "grau\u00b7er", "Tr\u00fcm\u00b7mer\u00b7spra\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nein, frei im Gr\u00fcn, auf Thurm und Wehr verzichtend;", "tokens": ["Nein", ",", "frei", "im", "Gr\u00fcn", ",", "auf", "Thurm", "und", "Wehr", "ver\u00b7zich\u00b7tend", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "APPRART", "NN", "$,", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Und doch mit seinem starken Holzgefache", "tokens": ["Und", "doch", "mit", "sei\u00b7nem", "star\u00b7ken", "Holz\u00b7ge\u00b7fa\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jahrhunderten vertraut, seit Fr\u00fchlingswehen", "tokens": ["Jahr\u00b7hun\u00b7der\u00b7ten", "ver\u00b7traut", ",", "seit", "Fr\u00fch\u00b7lings\u00b7we\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "ADJD", "$,", "APPR", "NN"], "meter": "+----+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Zuerst gerauscht zu seinem Schieferdache.", "tokens": ["Zu\u00b7erst", "ge\u00b7rauscht", "zu", "sei\u00b7nem", "Schie\u00b7fer\u00b7da\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Betrachtend aber wirst du lange stehen,", "tokens": ["Be\u00b7trach\u00b7tend", "a\u00b7ber", "wirst", "du", "lan\u00b7ge", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn sich des Eingangs Halle dir erschlossen,", "tokens": ["Wenn", "sich", "des", "Ein\u00b7gangs", "Hal\u00b7le", "dir", "er\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "NE", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geschm\u00fcckt mit Bildern, Waffen, Jagdtroph\u00e4en.", "tokens": ["Ge\u00b7schm\u00fcckt", "mit", "Bil\u00b7dern", ",", "Waf\u00b7fen", ",", "Jagd\u00b7tro\u00b7ph\u00e4\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "Doch da\u00df nicht nur bewohnt von Waidgenossen", "tokens": ["Doch", "da\u00df", "nicht", "nur", "be\u00b7wohnt", "von", "Waid\u00b7ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PTKNEG", "ADV", "VVPP", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Hall' und die Gem\u00e4cher, sagt und deutet", "