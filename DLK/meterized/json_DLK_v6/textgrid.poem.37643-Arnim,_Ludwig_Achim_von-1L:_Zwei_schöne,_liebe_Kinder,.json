{"textgrid.poem.37643": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zwei sch\u00f6ne, liebe Kinder,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei sch\u00f6ne, liebe Kinder,", "tokens": ["Zwei", "sch\u00f6\u00b7ne", ",", "lie\u00b7be", "Kin\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die hatten sich so lieb,", "tokens": ["Die", "hat\u00b7ten", "sich", "so", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df eines dem andern im Winter", "tokens": ["Da\u00df", "ei\u00b7nes", "dem", "an\u00b7dern", "im", "Win\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "ADJA", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mit Singen die Zeit vertrieb,", "tokens": ["Mit", "Sin\u00b7gen", "die", "Zeit", "ver\u00b7trieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Diesseit und jenseit am Wasserfall", "tokens": ["Dies\u00b7seit", "und", "jen\u00b7seit", "am", "Was\u00b7ser\u00b7fall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "APPR", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "H\u00f6ret ihr immer den Doppelschall.", "tokens": ["H\u00f6\u00b7ret", "ihr", "im\u00b7mer", "den", "Dop\u00b7pel\u00b7schall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Der Winter bauet Br\u00fccken,", "tokens": ["Der", "Win\u00b7ter", "bau\u00b7et", "Br\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie beide hat vereint,", "tokens": ["Sie", "bei\u00b7de", "hat", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und jedes mit frohem Entz\u00fccken", "tokens": ["Und", "je\u00b7des", "mit", "fro\u00b7hem", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Br\u00fccke nun ewig meint;", "tokens": ["Die", "Br\u00fc\u00b7cke", "nun", "e\u00b7wig", "meint", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Diesseit und jenseit am Wasserfall", "tokens": ["Dies\u00b7seit", "und", "jen\u00b7seit", "am", "Was\u00b7ser\u00b7fall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "APPR", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wohnten die \u00c4ltern getrennt im Thal.", "tokens": ["Wohn\u00b7ten", "die", "\u00c4l\u00b7tern", "ge\u00b7trennt", "im", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "APPRART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Der Fr\u00fchling ist gekommen,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "ist", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Eis will nun aufgehn,", "tokens": ["Das", "Eis", "will", "nun", "auf\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da werden sie beide beklommen,", "tokens": ["Da", "wer\u00b7den", "sie", "bei\u00b7de", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die laulichen Winde wehn;", "tokens": ["Die", "lau\u00b7li\u00b7chen", "Win\u00b7de", "wehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Diesseit und jenseit am Wasserfall", "tokens": ["Dies\u00b7seit", "und", "jen\u00b7seit", "am", "Was\u00b7ser\u00b7fall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "APPR", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "St\u00fcrzen die B\u00e4che mit wildem Schall.", "tokens": ["St\u00fcr\u00b7zen", "die", "B\u00e4\u00b7che", "mit", "wil\u00b7dem", "Schall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Was hilft der helle Bogen,", "tokens": ["Was", "hilft", "der", "hel\u00b7le", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Womit der Fall entz\u00fcckt,", "tokens": ["Wo\u00b7mit", "der", "Fall", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Von ihnen so liebreich erzogen,", "tokens": ["Von", "ih\u00b7nen", "so", "lieb\u00b7reich", "er\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Zum erstenmal bunt geschm\u00fcckt;", "tokens": ["Zum", "ers\u00b7ten\u00b7mal", "bunt", "ge\u00b7schm\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "ADJD", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Diesseit und jenseit am Wasserfall", "tokens": ["Dies\u00b7seit", "und", "jen\u00b7seit", "am", "Was\u00b7ser\u00b7fall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "APPR", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "H\u00f6ret sie klagen getrennt im Thal.", "tokens": ["H\u00f6\u00b7ret", "sie", "kla\u00b7gen", "ge\u00b7trennt", "im", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Die V\u00f6gel \u00fcber fliegen,", "tokens": ["Die", "V\u00f6\u00b7gel", "\u00fc\u00b7ber", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Kinder traurig stehn,", "tokens": ["Die", "Kin\u00b7der", "trau\u00b7rig", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und m\u00fcssen sich einsam begn\u00fcgen", "tokens": ["Und", "m\u00fcs\u00b7sen", "sich", "ein\u00b7sam", "be\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PRF", "ADJD", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Einander von fern zu sehn;", "tokens": ["Ein\u00b7an\u00b7der", "von", "fern", "zu", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Diesseit und jenseit am Wasserfall", "tokens": ["Dies\u00b7seit", "und", "jen\u00b7seit", "am", "Was\u00b7ser\u00b7fall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "APPR", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Kreuzen die Schwalben mit lautem Schall.", "tokens": ["Kreu\u00b7zen", "die", "Schwal\u00b7ben", "mit", "lau\u00b7tem", "Schall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Sie m\u00f6chten zusammen mit Singen,", "tokens": ["Sie", "m\u00f6ch\u00b7ten", "zu\u00b7sam\u00b7men", "mit", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "So wie der V\u00f6gel Brut,", "tokens": ["So", "wie", "der", "V\u00f6\u00b7gel", "Brut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den himmlischen Fr\u00fchling verbringen,", "tokens": ["Den", "himm\u00b7li\u00b7schen", "Fr\u00fch\u00b7ling", "ver\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Das Scheiden so wehe thut;", "tokens": ["Das", "Schei\u00b7den", "so", "we\u00b7he", "thut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Diesseit und jenseit am Wasserfall", "tokens": ["Dies\u00b7seit", "und", "jen\u00b7seit", "am", "Was\u00b7ser\u00b7fall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "APPR", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sehn sie sich endlich zum letztenmal.", "tokens": ["Sehn", "sie", "sich", "end\u00b7lich", "zum", "letz\u00b7ten\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPRART", "ADV", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Der Knabe kriegt zur Freude", "tokens": ["Der", "Kna\u00b7be", "kriegt", "zur", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein R\u00f6ckchen wie ein Mann,", "tokens": ["Ein", "R\u00f6ck\u00b7chen", "wie", "ein", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das M\u00e4dchen ein Kleidchen von Seide,", "tokens": ["Das", "M\u00e4d\u00b7chen", "ein", "Kleid\u00b7chen", "von", "Sei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Nun gehet die Schule an;", "tokens": ["Nun", "ge\u00b7het", "die", "Schu\u00b7le", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Diesseit und jenseit am Wasserfall", "tokens": ["Dies\u00b7seit", "und", "jen\u00b7seit", "am", "Was\u00b7ser\u00b7fall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "APPR", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Gehn sie zum Kloster bei Glockenschall.", "tokens": ["Gehn", "sie", "zum", "Klos\u00b7ter", "bei", "Glo\u00b7cken\u00b7schall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPR", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Sie sahn sich lang' nicht wieder,", "tokens": ["Sie", "sahn", "sich", "lang'", "nicht", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie kannten sich nicht mehr,", "tokens": ["Sie", "kann\u00b7ten", "sich", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das M\u00e4dchen mit vollem Mieder,", "tokens": ["Das", "M\u00e4d\u00b7chen", "mit", "vol\u00b7lem", "Mie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Knabe ein M\u00f6nch schon w\u00e4r;", "tokens": ["Der", "Kna\u00b7be", "ein", "M\u00f6nch", "schon", "w\u00e4r", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Diesseit und jenseit am Wasserfall", "tokens": ["Dies\u00b7seit", "und", "jen\u00b7seit", "am", "Was\u00b7ser\u00b7fall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "APPR", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Kamen und riefen sie sich im Thal.", "tokens": ["Ka\u00b7men", "und", "rie\u00b7fen", "sie", "sich", "im", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Das M\u00e4dchen ruft so helle,", "tokens": ["Das", "M\u00e4d\u00b7chen", "ruft", "so", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Knabe singt so tief;", "tokens": ["Der", "Kna\u00b7be", "singt", "so", "tief", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verstehn sich endlich doch schnelle,", "tokens": ["Ver\u00b7stehn", "sich", "end\u00b7lich", "doch", "schnel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Als alles im Hause schlief;", "tokens": ["Als", "al\u00b7les", "im", "Hau\u00b7se", "schlief", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Diesseit und jenseit am Wasserfall", "tokens": ["Dies\u00b7seit", "und", "jen\u00b7seit", "am", "Was\u00b7ser\u00b7fall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "APPR", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Springen im Mondschein die Fische all.", "tokens": ["Sprin\u00b7gen", "im", "Mond\u00b7schein", "die", "Fi\u00b7sche", "all", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "PIAT", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Froh in der n\u00e4cht'gen Frische,", "tokens": ["Froh", "in", "der", "n\u00e4cht'\u00b7gen", "Fri\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie k\u00fchlen sich im Flu\u00df,", "tokens": ["Sie", "k\u00fch\u00b7len", "sich", "im", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie k\u00f6nnen nicht schwimmen wie Fische,", "tokens": ["Sie", "k\u00f6n\u00b7nen", "nicht", "schwim\u00b7men", "wie", "Fi\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "KOKOM", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und suchen sich doch zum Ku\u00df;", "tokens": ["Und", "su\u00b7chen", "sich", "doch", "zum", "Ku\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VV