{"textgrid.poem.59063": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "282. Kindlich gesinnt", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lern stets vergn\u00fcgt sein, wie dir's ist,", "tokens": ["Lern", "stets", "ver\u00b7gn\u00fcgt", "sein", ",", "wie", "dir's", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVPP", "VAINF", "$,", "PWAV", "PIS", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gott lieben ohne Kunst und List,", "tokens": ["Gott", "lie\u00b7ben", "oh\u00b7ne", "Kunst", "und", "List", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Einf\u00e4ltig ihn in allem schauen,", "tokens": ["Ein\u00b7f\u00e4l\u00b7tig", "ihn", "in", "al\u00b7lem", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dir nichts, ihm alles anvertrauen!", "tokens": ["Dir", "nichts", ",", "ihm", "al\u00b7les", "an\u00b7ver\u00b7trau\u00b7en", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "$,", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}