{"textgrid.poem.52015": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "37.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo dir der von Florentz ein Gottes L\u00e4strer ist,", "tokens": ["Wo", "dir", "der", "von", "Flo\u00b7rentz", "ein", "Got\u00b7tes", "L\u00e4st\u00b7rer", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "APPR", "NE", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "-+--++-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der so viel R\u00e4nck' und List im Herrschen auffgeschrieben,", "tokens": ["Der", "so", "viel", "R\u00e4nck", "und", "List", "im", "Herr\u00b7schen", "auff\u00b7ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "KON", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie wird dem, der getaufft, der seine Lehr erkiest:", "tokens": ["Wie", "wird", "dem", ",", "der", "ge\u00b7taufft", ",", "der", "sei\u00b7ne", "Lehr", "er\u00b7kiest", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "$,", "PRELS", "VVPP", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die gr\u00f6sten sind: die, was er hat geschrieben, \u00fcben.", "tokens": ["Die", "gr\u00f6s\u00b7ten", "sind", ":", "die", ",", "was", "er", "hat", "ge\u00b7schrie\u00b7ben", ",", "\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "$.", "ART", "$,", "PWS", "PPER", "VAFIN", "VVPP", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}