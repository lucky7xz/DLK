{"dta.poem.4444": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df mich, zu meines Vaterlandes Ehre,", "tokens": ["Da\u00df", "mich", ",", "zu", "mei\u00b7nes", "Va\u00b7ter\u00b7lan\u00b7des", "Eh\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der zungenvolle Ruf in fremden L\u00e4ndern nennt,", "tokens": ["Der", "zun\u00b7gen\u00b7vol\u00b7le", "Ruf", "in", "frem\u00b7den", "L\u00e4n\u00b7dern", "nennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und da\u00df mein Saytenspiel nun th\u00f6nt, bis zum Geh\u00f6re", "tokens": ["Und", "da\u00df", "mein", "Say\u00b7ten\u00b7spiel", "nun", "th\u00f6nt", ",", "bis", "zum", "Ge\u00b7h\u00f6\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Weisen, der die Geister kennt;", "tokens": ["Des", "Wei\u00b7sen", ",", "der", "die", "Geis\u00b7ter", "kennt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}