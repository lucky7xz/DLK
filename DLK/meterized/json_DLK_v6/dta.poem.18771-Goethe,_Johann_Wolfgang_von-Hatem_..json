{"dta.poem.18771": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Hatem .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht Gelegenheit macht Diebe,", "tokens": ["Nicht", "Ge\u00b7le\u00b7gen\u00b7heit", "macht", "Die\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie ist selbst der gr\u00f6sste Dieb,", "tokens": ["Sie", "ist", "selbst", "der", "gr\u00f6ss\u00b7te", "Dieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn sie stahl den Rest der Liebe", "tokens": ["Denn", "sie", "stahl", "den", "Rest", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die mir noch im Herzen blieb.", "tokens": ["Die", "mir", "noch", "im", "Her\u00b7zen", "blieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dir hat sie ihn \u00fcbergeben", "tokens": ["Dir", "hat", "sie", "ihn", "\u00fc\u00b7ber\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PPER", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Meines Lebens Vollgewinn,", "tokens": ["Mei\u00b7nes", "Le\u00b7bens", "Voll\u00b7ge\u00b7winn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dass ich nun, verarmt, mein Leben", "tokens": ["Dass", "ich", "nun", ",", "ver\u00b7armt", ",", "mein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "$,", "VVPP", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur von dir gew\u00e4rtig bin.", "tokens": ["Nur", "von", "dir", "ge\u00b7w\u00e4r\u00b7tig", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch ich f\u00fchle schon Erbarmen", "tokens": ["Doch", "ich", "f\u00fch\u00b7le", "schon", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Im Carfunkel deines Blicks", "tokens": ["Im", "Car\u00b7fun\u00b7kel", "dei\u00b7nes", "Blicks"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und erfreu\u2019 in deinen Armen", "tokens": ["Und", "er\u00b7freu'", "in", "dei\u00b7nen", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich erneuerten Geschicks.", "tokens": ["Mich", "er\u00b7neu\u00b7er\u00b7ten", "Ge\u00b7schicks", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}