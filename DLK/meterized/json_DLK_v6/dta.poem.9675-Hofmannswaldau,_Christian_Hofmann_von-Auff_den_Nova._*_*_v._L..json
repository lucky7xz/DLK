{"dta.poem.9675": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auff den Nova.  \n * * v. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nova will den besten freund/ weil er lebet/ gar nicht kennen/", "tokens": ["No\u00b7va", "will", "den", "bes\u00b7ten", "freund", "/", "weil", "er", "le\u00b7bet", "/", "gar", "nicht", "ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "ADJA", "NN", "$(", "KOUS", "PPER", "VVFIN", "$(", "ADV", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Trotz! demselben der ihn solt vetter oder schwager nennen:", "tokens": ["Trotz", "!", "dem\u00b7sel\u00b7ben", "der", "ihn", "solt", "vet\u00b7ter", "o\u00b7der", "schwa\u00b7ger", "nen\u00b7nen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PDAT", "ART", "PPER", "VMFIN", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Doch wen\u0303 reich\u2019 aus ihrer zahl/ w\u00e4ren es auch bauren/ sterben;", "tokens": ["Doch", "we\u00f1", "reich'", "aus", "ih\u00b7rer", "zahl", "/", "w\u00e4\u00b7ren", "es", "auch", "bau\u00b7ren", "/", "ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "APPR", "PPOSAT", "NN", "$(", "VAFIN", "PPER", "ADV", "VVINF", "$(", "VVINF", "$."], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Will er von denselbigen/ als der nechste vetter/ erben.", "tokens": ["Will", "er", "von", "den\u00b7sel\u00b7bi\u00b7gen", "/", "als", "der", "nechs\u00b7te", "vet\u00b7ter", "/", "er\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PDAT", "$(", "KOUS", "ART", "ADJA", "NN", "$(", "VVINF", "$."], "meter": "---+-+-+-+-+-+-", "measure": "unknown.measure.hexa"}}}}}