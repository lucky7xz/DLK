{"textgrid.poem.26362": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Morgenlicht um die weichen Glieder, schleichen Knaben, M\u00e4gde nieder vom Wald.", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Morgenlicht um die weichen Glieder, schleichen Knaben, M\u00e4gde nieder vom Wald.", "tokens": ["Mor\u00b7gen\u00b7licht", "um", "die", "wei\u00b7chen", "Glie\u00b7der", ",", "schlei\u00b7chen", "Kna\u00b7ben", ",", "M\u00e4g\u00b7de", "nie\u00b7der", "vom", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,", "ADJA", "NN", "$,", "NN", "PTKVZ", "APPRART", "NN", "$."], "meter": "+-+--+-+-+-+-+-+--+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Schwarz lagert Granit, st\u00fcrzt breit in Stufen,", "tokens": ["Schwarz", "la\u00b7gert", "Gra\u00b7nit", ",", "st\u00fcrzt", "breit", "in", "Stu\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "$,", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Die Greise, die blinden, finden nur tastend den Schritt,", "tokens": ["Die", "Grei\u00b7se", ",", "die", "blin\u00b7den", ",", "fin\u00b7den", "nur", "tas\u00b7tend", "den", "Schritt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Fr\u00fchlicht sticht in die Wunden.", "tokens": ["Fr\u00fch\u00b7licht", "sticht", "in", "die", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Weiter, weiter lockt dunkel der Weg, funkelt vom schwarzen Gestein.", "tokens": ["Wei\u00b7ter", ",", "wei\u00b7ter", "lockt", "dun\u00b7kel", "der", "Weg", ",", "fun\u00b7kelt", "vom", "schwar\u00b7zen", "Ge\u00b7stein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "ADJD", "ART", "NN", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+---+--+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Kein Baum mehr, kein Halm kein Gras,", "tokens": ["Kein", "Baum", "mehr", ",", "kein", "Halm", "kein", "Gras", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "$,", "PIAT", "NN", "PIAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Salzbitter duftet die Luft,", "tokens": ["Salz\u00b7bit\u00b7ter", "duf\u00b7tet", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Donner zittert im Fels, senkrecht st\u00fcrzen die Stufen,", "tokens": ["Don\u00b7ner", "zit\u00b7tert", "im", "Fels", ",", "sen\u00b7krecht", "st\u00fcr\u00b7zen", "die", "Stu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Donner rufen im Stein, unten kracht brandend die Flut,", "tokens": ["Don\u00b7ner", "ru\u00b7fen", "im", "Stein", ",", "un\u00b7ten", "kracht", "bran\u00b7dend", "die", "Flut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,", "ADV", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+--+--++--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Unten im Nachtschattenschein ruht dunkel das Meer.", "tokens": ["Un\u00b7ten", "im", "Nacht\u00b7schat\u00b7ten\u00b7schein", "ruht", "dun\u00b7kel", "das", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "+--++-+-+--+", "measure": "iambic.hexa.chol"}, "line.6": {"text": "Das Meer eine m\u00e4chtig blauende Flamme, flachgereckt,", "tokens": ["Das", "Meer", "ei\u00b7ne", "m\u00e4ch\u00b7tig", "blau\u00b7en\u00b7de", "Flam\u00b7me", ",", "flach\u00b7ge\u00b7reckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJD", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Leckt drohend zum Rande des Himmels.", "tokens": ["Leckt", "dro\u00b7hend", "zum", "Ran\u00b7de", "des", "Him\u00b7mels", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Schwer stockt der Schritt. Die Greise greifen mit steifen Armen.", "tokens": ["Schwer", "stockt", "der", "Schritt", ".", "Die", "Grei\u00b7se", "grei\u00b7fen", "mit", "stei\u00b7fen", "Ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$.", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Am nassen Granit fault dunkel der Tang, die M\u00e4gde b\u00fccken sich nieder.", "tokens": ["Am", "nas\u00b7sen", "Gra\u00b7nit", "fault", "dun\u00b7kel", "der", "Tang", ",", "die", "M\u00e4g\u00b7de", "b\u00fc\u00b7cken", "sich", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ADJD", "ART", "NN", "$,", "ART", "NN", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+--+--+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "G\u00fcrten den kalten blutroten Tang um die eisblassen Glieder.", "tokens": ["G\u00fcr\u00b7ten", "den", "kal\u00b7ten", "blut\u00b7ro\u00b7ten", "Tang", "um", "die", "eis\u00b7blas\u00b7sen", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+-+-+-+-", "measure": "iambic.septa.invert"}, "line.11": {"text": "D\u00fcster die Knaben \u00e4ugen l\u00fcstern zur Flut.", "tokens": ["D\u00fcs\u00b7ter", "die", "Kna\u00b7ben", "\u00e4u\u00b7gen", "l\u00fcs\u00b7tern", "zur", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.12": {"text": "Die Flut schwemmt St\u00e4mme zum Strand, rostrote Kiefern,", "tokens": ["Die", "Flut", "schwemmt", "St\u00e4m\u00b7me", "zum", "Strand", ",", "rost\u00b7ro\u00b7te", "Kie\u00b7fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "APPRART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Tot prallt das Holz an die Stufen.", "tokens": ["Tot", "prallt", "das", "Holz", "an", "die", "Stu\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Wellen rufen und st\u00fcrzen. Mit eisigen Augen glotzt brausend der Schaum.", "tokens": ["Wel\u00b7len", "ru\u00b7fen", "und", "st\u00fcr\u00b7zen", ".", "Mit", "ei\u00b7si\u00b7gen", "Au\u00b7gen", "glotzt", "brau\u00b7send", "der", "Schaum", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "VVINF", "$.", "APPR", "ADJA", "NN", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "+-+--+--+--+--+--+", "measure": "trochaic.septa.relaxed"}, "line.15": {"text": "Wei\u00df funkeln M\u00f6wen. Schreie gellen. Dunkel uners\u00e4ttlich wiegt sich der Meerraum.", "tokens": ["Wei\u00df", "fun\u00b7keln", "M\u00f6\u00b7wen", ".", "Schrei\u00b7e", "gel\u00b7len", ".", "Dun\u00b7kel", "un\u00b7er\u00b7s\u00e4tt\u00b7lich", "wiegt", "sich", "der", "Meer\u00b7raum", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "$.", "NN", "VVPP", "$.", "NN", "ADJD", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+--+-", "measure": "iambic.octa.plus"}, "line.16": {"text": "Die Knaben fangen die St\u00e4mme. Sch\u00e4len Rinden, langen Bast,", "tokens": ["Die", "Kna\u00b7ben", "fan\u00b7gen", "die", "St\u00e4m\u00b7me", ".", "Sch\u00e4\u00b7len", "Rin\u00b7den", ",", "lan\u00b7gen", "Bast", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$.", "NN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.17": {"text": "Binden St\u00e4mme und Stangen zum Flo\u00df.", "tokens": ["Bin\u00b7den", "St\u00e4m\u00b7me", "und", "Stan\u00b7gen", "zum", "Flo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.18": {"text": "Greise, Knaben, M\u00e4gde besteigen schweigend das Holz,", "tokens": ["Grei\u00b7se", ",", "Kna\u00b7ben", ",", "M\u00e4g\u00b7de", "be\u00b7stei\u00b7gen", "schwei\u00b7gend", "das", "Holz", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+--+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Sie sto\u00dfen vom schaukelnden Strand,", "tokens": ["Sie", "sto\u00b7\u00dfen", "vom", "schau\u00b7keln\u00b7den", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.20": {"text": "Gierig strahlen die Blicke, drau\u00dfen gl\u00fcht finster der Meerrand.", "tokens": ["Gie\u00b7rig", "strah\u00b7len", "die", "Bli\u00b7cke", ",", "drau\u00b7\u00dfen", "gl\u00fcht", "fins\u00b7ter", "der", "Meer\u00b7rand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.21": {"text": "Die Sonne r\u00fcckt wei\u00df zum Zenith.", "tokens": ["Die", "Son\u00b7ne", "r\u00fcckt", "wei\u00df", "zum", "Ze\u00b7nith", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Ein graues glei\u00dfendes Auge das Meer und z\u00fcckt nach der Sonne.", "tokens": ["Ein", "grau\u00b7es", "glei\u00b7\u00dfen\u00b7des", "Au\u00b7ge", "das", "Meer", "und", "z\u00fcckt", "nach", "der", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "ART", "NN", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Schattenschwarz hocken Klippen nachtfinster im Mittaglicht,", "tokens": ["Schat\u00b7ten\u00b7schwarz", "ho\u00b7cken", "Klip\u00b7pen", "nacht\u00b7fins\u00b7ter", "im", "Mit\u00b7tag\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "+-+--+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "Auf den sargschwarzen Brocken kauern Meerv\u00f6gel, dunkel, rucken und lauern mit H\u00e4lsen und Krallen.", "tokens": ["Auf", "den", "sarg\u00b7schwar\u00b7zen", "Bro\u00b7cken", "kau\u00b7ern", "Meer\u00b7v\u00f6\u00b7gel", ",", "dun\u00b7kel", ",", "ru\u00b7cken", "und", "lau\u00b7ern", "mit", "H\u00e4l\u00b7sen", "und", "Kral\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$,", "ADJD", "$,", "VVINF", "KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+--+-+--+--+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.25": {"text": "Wellenan, wellenab st\u00fcrmt das Flo\u00df.", "tokens": ["Wel\u00b7le\u00b7nan", ",", "wel\u00b7len\u00b7ab", "st\u00fcrmt", "das", "Flo\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.26": {"text": "Wei\u00df, schaumbespritzt, Salzkristalle im Haar, sitzt die Menschenschar,", "tokens": ["Wei\u00df", ",", "schaum\u00b7be\u00b7spritzt", ",", "Salz\u00b7kris\u00b7tal\u00b7le", "im", "Haar", ",", "sitzt", "die", "Men\u00b7schen\u00b7schar", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "NN", "APPRART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Die Augen geweitet, dunkel lustgro\u00df.", "tokens": ["Die", "Au\u00b7gen", "ge\u00b7wei\u00b7tet", ",", "dun\u00b7kel", "lust\u00b7gro\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ADJD", "VVFIN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Grab an Grab brechen die Wellen, die Gr\u00e4ber leben und sprechen.", "tokens": ["Grab", "an", "Grab", "bre\u00b7chen", "die", "Wel\u00b7len", ",", "die", "Gr\u00e4\u00b7ber", "le\u00b7ben", "und", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "+-++--+--+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.29": {"text": "Greisenwei\u00df liegen unter den Greisen die Knaben und M\u00e4gde,", "tokens": ["Grei\u00b7sen\u00b7wei\u00df", "lie\u00b7gen", "un\u00b7ter", "den", "Grei\u00b7sen", "die", "Kna\u00b7ben", "und", "M\u00e4g\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.30": {"text": "Brust, Gesicht, Arme biegen sich \u00fcber das Flo\u00df,", "tokens": ["Brust", ",", "Ge\u00b7sicht", ",", "Ar\u00b7me", "bie\u00b7gen", "sich", "\u00fc\u00b7ber", "das", "Flo\u00df", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-++-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.31": {"text": "Lippen und Augen saugen die Tiefe.", "tokens": ["Lip\u00b7pen", "und", "Au\u00b7gen", "sau\u00b7gen", "die", "Tie\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.32": {"text": "\u00bbkommt zum Dunkel hinab,", "tokens": ["\u00bb", "kommt", "zum", "Dun\u00b7kel", "hin\u00b7ab", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.33": {"text": "Sonne bl\u00fcht dunkel im gl\u00fchenden Grab,", "tokens": ["Son\u00b7ne", "bl\u00fcht", "dun\u00b7kel", "im", "gl\u00fc\u00b7hen\u00b7den", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.34": {"text": "Dunkel l\u00f6schen die Wunden.\u00ab", "tokens": ["Dun\u00b7kel", "l\u00f6\u00b7schen", "die", "Wun\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.35": {"text": "Schwarzgro\u00df g\u00e4hnt eine Welle empor. Schwarz funkelt die M\u00e4hne,", "tokens": ["Schwarz\u00b7gro\u00df", "g\u00e4hnt", "ei\u00b7ne", "Wel\u00b7le", "em\u00b7por", ".", "Schwarz", "fun\u00b7kelt", "die", "M\u00e4h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "