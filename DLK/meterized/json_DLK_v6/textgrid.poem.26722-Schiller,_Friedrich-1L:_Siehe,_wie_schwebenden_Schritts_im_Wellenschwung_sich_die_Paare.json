{"textgrid.poem.26722": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Siehe, wie schwebenden Schritts im Wellenschwung sich die Paare", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siehe, wie schwebenden Schritts im Wellenschwung sich die Paare", "tokens": ["Sie\u00b7he", ",", "wie", "schwe\u00b7ben\u00b7den", "Schritts", "im", "Wel\u00b7len\u00b7schwung", "sich", "die", "Paa\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "PWAV", "ADJA", "NN", "APPRART", "NN", "PRF", "ART", "NN"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Drehen, den Boden ber\u00fchrt kaum der gefl\u00fcgelte Fu\u00df.", "tokens": ["Dre\u00b7hen", ",", "den", "Bo\u00b7den", "be\u00b7r\u00fchrt", "kaum", "der", "ge\u00b7fl\u00fc\u00b7gel\u00b7te", "Fu\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Seh ich fl\u00fcchtige Schatten, befreit von der Schwere des Leibes?", "tokens": ["Seh", "ich", "fl\u00fcch\u00b7ti\u00b7ge", "Schat\u00b7ten", ",", "be\u00b7freit", "von", "der", "Schwe\u00b7re", "des", "Lei\u00b7bes", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,", "VVPP", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Schlingen im Mondlicht dort Elfen den luftigen Reihn?", "tokens": ["Schlin\u00b7gen", "im", "Mond\u00b7licht", "dort", "El\u00b7fen", "den", "luf\u00b7ti\u00b7gen", "Reihn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADV", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.5": {"text": "Wie, vom Zephir gewiegt, der leichte Rauch in die Luft flie\u00dft,", "tokens": ["Wie", ",", "vom", "Ze\u00b7phir", "ge\u00b7wiegt", ",", "der", "leich\u00b7te", "Rauch", "in", "die", "Luft", "flie\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPRART", "NN", "VVPP", "$,", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Wie sich leise der Kahn schaukelt auf silberner Flut,", "tokens": ["Wie", "sich", "lei\u00b7se", "der", "Kahn", "schau\u00b7kelt", "auf", "sil\u00b7ber\u00b7ner", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADJD", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "H\u00fcpft der gelehrige Fu\u00df auf des Takts melodischer Woge,", "tokens": ["H\u00fcpft", "der", "ge\u00b7leh\u00b7ri\u00b7ge", "Fu\u00df", "auf", "des", "Takts", "me\u00b7lo\u00b7di\u00b7scher", "Wo\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "S\u00e4uselndes Saitenget\u00f6n hebt den \u00e4therischen Leib.", "tokens": ["S\u00e4u\u00b7seln\u00b7des", "Sai\u00b7ten\u00b7ge\u00b7t\u00f6n", "hebt", "den", "\u00e4t\u00b7he\u00b7ri\u00b7schen", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--++-+-+-+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Jetzt, als wollt es mit Macht durchrei\u00dfen die Kette des Tanzes,", "tokens": ["Jetzt", ",", "als", "wollt", "es", "mit", "Macht", "durc\u00b7hrei\u00b7\u00dfen", "die", "Ket\u00b7te", "des", "Tan\u00b7zes", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "VMFIN", "PPER", "APPR", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Schwingt sich ein mutiges Paar dort in den dichtesten Reihn.", "tokens": ["Schwingt", "sich", "ein", "mu\u00b7ti\u00b7ges", "Paar", "dort", "in", "den", "dich\u00b7tes\u00b7ten", "Reihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Schnell vor ihm her entsteht ihm die Bahn, die hinter ihm schwindet,", "tokens": ["Schnell", "vor", "ihm", "her", "ent\u00b7steht", "ihm", "die", "Bahn", ",", "die", "hin\u00b7ter", "ihm", "schwin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "APZR", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "---+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Wie durch magische Hand \u00f6ffnet und schlie\u00dft sich der Weg.", "tokens": ["Wie", "durch", "ma\u00b7gi\u00b7sche", "Hand", "\u00f6ff\u00b7net", "und", "schlie\u00dft", "sich", "der", "Weg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.13": {"text": "Sieh! jetzt schwand es dem Blick, in wildem Gewirr durcheinander", "tokens": ["Sieh", "!", "jetzt", "schwand", "es", "dem", "Blick", ",", "in", "wil\u00b7dem", "Ge\u00b7wirr", "durch\u00b7ein\u00b7an\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPER", "ART", "NN", "$,", "APPR", "ADJA", "NN", "PTKVZ"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.14": {"text": "St\u00fcrzt der zierliche Bau dieser beweglichen Welt.", "tokens": ["St\u00fcrzt", "der", "zier\u00b7li\u00b7che", "Bau", "die\u00b7ser", "be\u00b7weg\u00b7li\u00b7chen", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PDAT", "ADJA", "NN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "Nein, dort schwebt es frohlockend herauf, der Knoten entwirrt sich,", "tokens": ["Nein", ",", "dort", "schwebt", "es", "froh\u00b7lo\u00b7ckend", "her\u00b7auf", ",", "der", "Kno\u00b7ten", "ent\u00b7wirrt", "sich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "ART", "NN", "VVFIN", "PRF", "$,"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Nur mit ver\u00e4ndertem Reiz stellet die Regel sich her.", "tokens": ["Nur", "mit", "ver\u00b7\u00e4n\u00b7der\u00b7tem", "Reiz", "stel\u00b7let", "die", "Re\u00b7gel", "sich", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.17": {"text": "Ewig zerst\u00f6rt, es erzeugt sich ewig die drehende Sch\u00f6pfung,", "tokens": ["E\u00b7wig", "zer\u00b7st\u00f6rt", ",", "es", "er\u00b7zeugt", "sich", "e\u00b7wig", "die", "dre\u00b7hen\u00b7de", "Sch\u00f6p\u00b7fung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "PPER", "VVFIN", "PRF", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Und ein stilles Gesetz lenkt der Verwandlungen Spiel.", "tokens": ["Und", "ein", "stil\u00b7les", "Ge\u00b7setz", "lenkt", "der", "Ver\u00b7wand\u00b7lun\u00b7gen", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.19": {"text": "Sprich, wie geschiehts, da\u00df rastlos erneut die Bildungen schwanken", "tokens": ["Sprich", ",", "wie", "ge\u00b7schiehts", ",", "da\u00df", "rast\u00b7los", "er\u00b7neut", "die", "Bil\u00b7dun\u00b7gen", "schwan\u00b7ken"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "PWAV", "VVPP", "$,", "KOUS", "ADJD", "VVFIN", "ART", "NN", "VVINF"], "meter": "+--+-+--+--+-+-", "measure": "iambic.hexa.invert"}, "line.20": {"text": "Und die Ruhe besteht in der bewegten Gestalt?", "tokens": ["Und", "die", "Ru\u00b7he", "be\u00b7steht", "in", "der", "be\u00b7weg\u00b7ten", "Ge\u00b7stalt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.21": {"text": "Jeder ein Herrscher, frei, nur dem eigenen Herzen gehorchet", "tokens": ["Je\u00b7der", "ein", "Herr\u00b7scher", ",", "frei", ",", "nur", "dem", "ei\u00b7ge\u00b7nen", "Her\u00b7zen", "ge\u00b7hor\u00b7chet"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "ART", "NN", "$,", "ADJD", "$,", "ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.22": {"text": "Und im eilenden Lauf findet die einzige Bahn?", "tokens": ["Und", "im", "ei\u00b7len\u00b7den", "Lauf", "fin\u00b7det", "die", "ein\u00b7zi\u00b7ge", "Bahn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--++-+-+-+", "measure": "anapaest.di.plus"}, "line.23": {"text": "Willst du es wissen? Es ist des Wohllauts m\u00e4chtige Gottheit,", "tokens": ["Willst", "du", "es", "wis\u00b7sen", "?", "Es", "ist", "des", "Wohl\u00b7lauts", "m\u00e4ch\u00b7ti\u00b7ge", "Got\u00b7theit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$.", "PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.24": {"text": "Die zum geselligen Tanz ordnet den tobenden Sprung,", "tokens": ["Die", "zum", "ge\u00b7sel\u00b7li\u00b7gen", "Tanz", "ord\u00b7net", "den", "to\u00b7ben\u00b7den", "Sprung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Die, der Nemesis gleich, an des Rhythmus goldenem Z\u00fcgel", "tokens": ["Die", ",", "der", "Ne\u00b7me\u00b7sis", "gleich", ",", "an", "des", "Rhyth\u00b7mus", "gol\u00b7de\u00b7nem", "Z\u00fc\u00b7gel"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "ART", "NN", "ADV", "$,", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.26": {"text": "Lenkt die brausende Lust und die verwilderte z\u00e4hmt.", "tokens": ["Lenkt", "die", "brau\u00b7sen\u00b7de", "Lust", "und", "die", "ver\u00b7wil\u00b7der\u00b7te", "z\u00e4hmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "KON", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.27": {"text": "Und dir rauschen umsonst die Harmonien des Weltalls,", "tokens": ["Und", "dir", "rau\u00b7schen", "um\u00b7sonst", "die", "Har\u00b7mo\u00b7ni\u00b7en", "des", "Welt\u00b7alls", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+--+-+-+-", "measure": "anapaest.tri.plus"}, "line.28": {"text": "Dich ergreift nicht der Strom dieses erhabnen Gesangs,", "tokens": ["Dich", "er\u00b7greift", "nicht", "der", "Strom", "die\u00b7ses", "er\u00b7hab\u00b7nen", "Ge\u00b7sangs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.29": {"text": "Nicht der begeisternde Takt, den alle Wesen dir schlagen,", "tokens": ["Nicht", "der", "be\u00b7geis\u00b7tern\u00b7de", "Takt", ",", "den", "al\u00b7le", "We\u00b7sen", "dir", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "$,", "ART", "PIAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "Nicht der wirbelnde Tanz, der durch den ewigen Raum", "tokens": ["Nicht", "der", "wir\u00b7beln\u00b7de", "Tanz", ",", "der", "durch", "den", "e\u00b7wi\u00b7gen", "Raum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.31": {"text": "Leuchtende Sonnen schwingt in k\u00fchn gewundenen Bahnen?", "tokens": ["Leuch\u00b7ten\u00b7de", "Son\u00b7nen", "schwingt", "in", "k\u00fchn", "ge\u00b7wun\u00b7de\u00b7nen", "Bah\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.32": {"text": "Das du im Spiele doch ehrst, fliehst du im Handeln, das Ma\u00df.", "tokens": ["Das", "du", "im", "Spie\u00b7le", "doch", "ehrst", ",", "fliehst", "du", "im", "Han\u00b7deln", ",", "das", "Ma\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "ADV", "VVFIN", "$,", "VVFIN", "PPER", "APPRART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}}}}}