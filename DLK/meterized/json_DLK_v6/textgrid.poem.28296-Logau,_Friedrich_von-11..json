{"textgrid.poem.28296": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich m\u00f6chte wissen, wie es k\u00e4me,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "wis\u00b7sen", ",", "wie", "es", "k\u00e4\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df unser Haab und Gut zuneme.", "tokens": ["Da\u00df", "un\u00b7ser", "Ha\u00b7ab", "und", "Gut", "zu\u00b7ne\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "ADJD", "VVFIN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Was nicht au\u00df Pflicht wir geben m\u00fcssen,", "tokens": ["Was", "nicht", "au\u00df", "Pflicht", "wir", "ge\u00b7ben", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "APPR", "NN", "PPER", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Soll H\u00f6fligkeit zusammen schissen;", "tokens": ["Soll", "H\u00f6f\u00b7lig\u00b7keit", "zu\u00b7sam\u00b7men", "schis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So was f\u00fcrs Maul noch \u00fcbrig blieben,", "tokens": ["So", "was", "f\u00fcrs", "Maul", "noch", "\u00fcb\u00b7rig", "blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "APPRART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So bleibt es doch nicht f\u00fcr den Dieben;", "tokens": ["So", "bleibt", "es", "doch", "nicht", "f\u00fcr", "den", "Die\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Was gleich die Todten schuldig waren,", "tokens": ["Was", "gleich", "die", "Tod\u00b7ten", "schul\u00b7dig", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das b\u00fcssen wir mit unsren Haaren;", "tokens": ["Das", "b\u00fcs\u00b7sen", "wir", "mit", "un\u00b7sren", "Haa\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Was wir gehabt und nicht mehr haben,", "tokens": ["Was", "wir", "ge\u00b7habt", "und", "nicht", "mehr", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAPP", "KON", "PTKNEG", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Davon erheischt man Scho\u00df und Gaben:", "tokens": ["Da\u00b7von", "er\u00b7heischt", "man", "Scho\u00df", "und", "Ga\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ich m\u00f6chte wissen, wie es k\u00e4me,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "wis\u00b7sen", ",", "wie", "es", "k\u00e4\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Da\u00df Gut wo einen Hauffen neme.", "tokens": ["Da\u00df", "Gut", "wo", "ei\u00b7nen", "Hauf\u00b7fen", "ne\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}}}}}