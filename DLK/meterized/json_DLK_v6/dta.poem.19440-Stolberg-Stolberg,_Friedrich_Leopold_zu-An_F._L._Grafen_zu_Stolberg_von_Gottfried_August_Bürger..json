{"dta.poem.19440": {"metadata": {"author": {"name": "Stolberg-Stolberg, Friedrich Leopold zu", "birth": "N.A.", "death": "N.A."}, "title": "An  \n F. L.  Grafen zu Stolberg  \n von  \n Gottfried August B\u00fcrger.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1779", "urn": "urn:nbn:de:kobv:b4-20589-5", "language": ["de:0.99"], "booktitle": "Stolberg-Stolberg, Christian zu; Stolberg-Stolberg, Friedrich Leopold zu: Gedichte. Leipzig, 1779."}, "poem": {"stanza.1": {"line.1": {"text": "Friz! Friz! bei den Unsterblichen, die hold", "tokens": ["Friz", "!", "Friz", "!", "bei", "den", "U\u00b7nsterb\u00b7li\u00b7chen", ",", "die", "hold"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$.", "NE", "$.", "APPR", "ART", "NN", "$,", "PRELS", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Auch meinem Leben sind! \u2014 Sie zeugen mir! \u2014", "tokens": ["Auch", "mei\u00b7nem", "Le\u00b7ben", "sind", "!", "Sie", "zeu\u00b7gen", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "$.", "$(", "PPER", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sieh, Angesichts der Ritter unsers Volks", "tokens": ["Sieh", ",", "An\u00b7ge\u00b7sichts", "der", "Rit\u00b7ter", "un\u00b7sers", "Volks"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ihrer losen Knappen, schreitest du", "tokens": ["Und", "ih\u00b7rer", "lo\u00b7sen", "Knap\u00b7pen", ",", "schrei\u00b7test", "du"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zu Truz, mit Wehr und Waffen in mein Feld,", "tokens": ["Zu", "Truz", ",", "mit", "Wehr", "und", "Waf\u00b7fen", "in", "mein", "Feld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und wirfst den Fehdehandschuh vor mich hin!", "tokens": ["Und", "wirfst", "den", "Feh\u00b7de\u00b7hand\u00b7schuh", "vor", "mich", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ha! schauerte nun auch die Menschlichkeit,", "tokens": ["Ha", "!", "schau\u00b7er\u00b7te", "nun", "auch", "die", "Menschlich\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+---+--++", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Wie Hektorn vor dem Ajax und Achill,", "tokens": ["Wie", "Hek\u00b7torn", "vor", "dem", "A\u00b7jax", "und", "A\u00b7chill", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPR", "ART", "NE", "KON", "NE", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Vor dir mich an; h\u00fcb ich ihn doch empor!", "tokens": ["Vor", "dir", "mich", "an", ";", "h\u00fcb", "ich", "ihn", "doch", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "PTKVZ", "$.", "VVFIN", "PPER", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Bei Gott! bei Gott! Du Troziger, ich mu\u00df! \u2014", "tokens": ["Bei", "Gott", "!", "bei", "Gott", "!", "Du", "Tro\u00b7zi\u00b7ger", ",", "ich", "mu\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "$.", "APPR", "NN", "$.", "PPER", "NN", "$,", "PPER", "VMFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "So gelt es denn! Sieg gelt\u2019 es, oder Tod! \u2014", "tokens": ["So", "gelt", "es", "denn", "!", "Sieg", "gelt'", "es", ",", "o\u00b7der", "Tod", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "$.", "NN", "VVFIN", "PPER", "$,", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Denn wisse! keinem Knaben sprichst du Hohn,", "tokens": ["Denn", "wis\u00b7se", "!", "kei\u00b7nem", "Kna\u00b7ben", "sprichst", "du", "Hohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PIAT", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Der seine ersten Waffen schwankend pr\u00fcft.", "tokens": ["Der", "sei\u00b7ne", "ers\u00b7ten", "Waf\u00b7fen", "schwan\u00b7kend", "pr\u00fcft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Straff sind die Sehnen meiner Jugendkraft;", "tokens": ["Straff", "sind", "die", "Seh\u00b7nen", "mei\u00b7ner", "Ju\u00b7gend\u00b7kraft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.15": {"text": "Ich bin gewandt zu ringen; meinem Arm", "tokens": ["Ich", "bin", "ge\u00b7wandt", "zu", "rin\u00b7gen", ";", "mei\u00b7nem", "Arm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "PTKZU", "VVINF", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Ist Ph\u00f6bus g\u00fcldnes Schwert ein Halmenspiel;", "tokens": ["Ist", "Ph\u00f6\u00b7bus", "g\u00fcld\u00b7nes", "Schwert", "ein", "Hal\u00b7men\u00b7spiel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Den Silberbogen des Ferntreffenden", "tokens": ["Den", "Sil\u00b7ber\u00b7bo\u00b7gen", "des", "Fern\u00b7tref\u00b7fen\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+--+---", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "Weis ich zu spannen; treffe scharf das Ziel;", "tokens": ["Weis", "ich", "zu", "span\u00b7nen", ";", "tref\u00b7fe", "scharf", "das", "Ziel", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKZU", "VVINF", "$.", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Mein K\u00f6cher rasselt goldner Pfeile voll \u2014", "tokens": ["Mein", "K\u00f6\u00b7cher", "ras\u00b7selt", "gold\u00b7ner", "Pfei\u00b7le", "voll"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Wer mag einher in meiner R\u00fcstung gehn? \u2014", "tokens": ["Wer", "mag", "ein\u00b7her", "in", "mei\u00b7ner", "R\u00fcs\u00b7tung", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VMFIN", "PIS", "APPR", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Es gelte, Friz! Sieg gelt\u2019 es, oder Tod! \u2014", "tokens": ["Es", "gel\u00b7te", ",", "Friz", "!", "Sieg", "gelt'", "es", ",", "o\u00b7der", "Tod", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "NE", "$.", "NN", "VVFIN", "PPER", "$,", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Du! huldigt dir Gesang und Sprach\u2019 allein?", "tokens": ["Du", "!", "hul\u00b7digt", "dir", "Ge\u00b7sang", "und", "Sprach'", "al\u00b7lein", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "VVFIN", "PPER", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Und waltet nicht des M\u00e4oniden Geist", "tokens": ["Und", "wal\u00b7tet", "nicht", "des", "M\u00e4o\u00b7ni\u00b7den", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.24": {"text": "Auch \u00fcber meinem Haupt? Ich rang mit ihm,", "tokens": ["Auch", "\u00fc\u00b7ber", "mei\u00b7nem", "Haupt", "?", "Ich", "rang", "mit", "ihm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Wie Herkuls Kraft mit Anteus Zauber rang.", "tokens": ["Wie", "Her\u00b7kuls", "Kraft", "mit", "An\u00b7teus", "Zau\u00b7ber", "rang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Bezwang ich ihn nicht oben in der Luft? \u2014", "tokens": ["Be\u00b7zwang", "ich", "ihn", "nicht", "o\u00b7ben", "in", "der", "Luft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "ADV", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Ich komm\u2019! ich komme dir! denn ehren mag", "tokens": ["Ich", "komm'", "!", "ich", "kom\u00b7me", "dir", "!", "denn", "eh\u00b7ren", "mag"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "$.", "KON", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Ein solcher Widersacher das Gefecht.", "tokens": ["Ein", "sol\u00b7cher", "Wi\u00b7der\u00b7sa\u00b7cher", "das", "Ge\u00b7fecht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Wie wird des Sieges Blume meinen Kranz", "tokens": ["Wie", "wird", "des", "Sie\u00b7ges", "Blu\u00b7me", "mei\u00b7nen", "Kranz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Verherlichen! \u2014 Und g\u00e4be mich der Rath", "tokens": ["Ver\u00b7her\u00b7li\u00b7chen", "!", "Und", "g\u00e4\u00b7be", "mich", "der", "Rath"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "$(", "KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "Der Himmelsherscher dir auch unterthan,", "tokens": ["Der", "Him\u00b7mels\u00b7her\u00b7scher", "dir", "auch", "un\u00b7ter\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "So k\u00f6nt\u2019 ich doch von keiner edlern Hand,", "tokens": ["So", "k\u00f6nt'", "ich", "doch", "von", "kei\u00b7ner", "ed\u00b7lern", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Als deiner, sterben, edler, starker Held!", "tokens": ["Als", "dei\u00b7ner", ",", "ster\u00b7ben", ",", "ed\u00b7ler", ",", "star\u00b7ker", "Held", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "$,", "VVFIN", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Auf! r\u00fcste dich! Sieg gilt es, oder Tod!", "tokens": ["Auf", "!", "r\u00fcs\u00b7te", "dich", "!", "Sieg", "gilt", "es", ",", "o\u00b7der", "Tod", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "PPER", "$.", "NN", "VVFIN", "PPER", "$,", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}