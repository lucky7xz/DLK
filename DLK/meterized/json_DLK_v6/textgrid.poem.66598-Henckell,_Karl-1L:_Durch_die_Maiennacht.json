{"textgrid.poem.66598": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Durch die Maiennacht", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch die Maiennacht", "tokens": ["Durch", "die", "Mai\u00b7en\u00b7nacht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Fuhr der Wintersturm,", "tokens": ["Fuhr", "der", "Win\u00b7ter\u00b7sturm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und die Fr\u00fchlingspracht", "tokens": ["Und", "die", "Fr\u00fch\u00b7lings\u00b7pracht"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Ri\u00df er nieder.", "tokens": ["Ri\u00df", "er", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Durch die junge Brust", "tokens": ["Durch", "die", "jun\u00b7ge", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Fuhr der Todeshauch,", "tokens": ["Fuhr", "der", "To\u00b7des\u00b7hauch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Traf mit grauser Lust", "tokens": ["Traf", "mit", "grau\u00b7ser", "Lust"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Meine Glieder.", "tokens": ["Mei\u00b7ne", "Glie\u00b7der", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Mu\u00df es denn geschehn,", "tokens": ["Mu\u00df", "es", "denn", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Kann's nicht anders sein,", "tokens": ["Kann's", "nicht", "an\u00b7ders", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADV", "VAINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Will ich freudig gehn", "tokens": ["Will", "ich", "freu\u00b7dig", "gehn"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADJD", "VVINF"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Und entsagen.", "tokens": ["Und", "ent\u00b7sa\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Fahre wohl, du Welt,", "tokens": ["Fah\u00b7re", "wohl", ",", "du", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Liebe, Kampf und Ruhm!", "tokens": ["Lie\u00b7be", ",", "Kampf", "und", "Ruhm", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Nur ein schlechter Held", "tokens": ["Nur", "ein", "schlech\u00b7ter", "Held"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Mag es klagen.", "tokens": ["Mag", "es", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Sinkt die Knospe hin,", "tokens": ["Sinkt", "die", "Knos\u00b7pe", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Eine neue sprie\u00dft,", "tokens": ["Ei\u00b7ne", "neu\u00b7e", "sprie\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und die Folgerin", "tokens": ["Und", "die", "Fol\u00b7ge\u00b7rin"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Sei gegr\u00fc\u00dft!", "tokens": ["Sei", "ge\u00b7gr\u00fc\u00dft", "!"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}