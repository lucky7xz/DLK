{"dta.poem.10747": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Ejusdem Sonnet  \n  J\u00f6rgen von   A v erli,   vnd Adelheiten  \n von Grauwart.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Was im Weltkreise rund allenthalb lebt vnd schwebet/", "tokens": ["Was", "im", "Welt\u00b7krei\u00b7se", "rund", "al\u00b7len\u00b7thalb", "lebt", "vnd", "schwe\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "ADJD", "ADJD", "VVFIN", "KON", "VVFIN", "$("], "meter": "--++-+---+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Warhafft erhalten wirdt durch gleich eintrechtigkeit/", "tokens": ["War\u00b7hafft", "er\u00b7hal\u00b7ten", "wirdt", "durch", "gleich", "ein\u00b7trech\u00b7tig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "APPR", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dann Gott vorkommen hat alle Zwyspaltigkeit", "tokens": ["Dann", "Gott", "vor\u00b7kom\u00b7men", "hat", "al\u00b7le", "Zwys\u00b7pal\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVINF", "VAFIN", "PIAT", "NN"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Da\u00df inn all seim Gesch\u00f6pff keins widers ander strebet.", "tokens": ["Da\u00df", "inn", "all", "seim", "Ge\u00b7sch\u00f6pff", "keins", "wi\u00b7ders", "an\u00b7der", "stre\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PIAT", "APPRART", "NN", "PIAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zwarjglicher Natur jhr eigenschafft anklebet/", "tokens": ["Zwarj\u00b7gli\u00b7cher", "Na\u00b7tur", "jhr", "ei\u00b7gen\u00b7schafft", "an\u00b7kle\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Irrdisch vnd Him\u0303lisch ding helt seine Richtigkeit.", "tokens": ["Irr\u00b7disch", "vnd", "Him\u0303\u00b7lisch", "ding", "helt", "sei\u00b7ne", "Rich\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Di\u00df alles wirckt die Lieb durch jhr Einhelligkeit/", "tokens": ["Di\u00df", "al\u00b7les", "wirckt", "die", "Lieb", "durch", "jhr", "Ein\u00b7hel\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vnd macht/ da\u00df in seim Standt nichts widersinns sich bebet/", "tokens": ["Vnd", "macht", "/", "da\u00df", "in", "seim", "Standt", "nichts", "wi\u00b7der\u00b7sinns", "sich", "be\u00b7bet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "APPR", "PPOSAT", "NN", "PIS", "VVFIN", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Lieb ist ein Bidergeist/ au\u00df Fewr vnd Lufft vereint/", "tokens": ["Lieb", "ist", "ein", "Bi\u00b7der\u00b7geist", "/", "au\u00df", "Fewr", "vnd", "Lufft", "ver\u00b7eint", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$(", "APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ders Hertz mit Girdt enttz\u00fcndt/ den mut mit Luste k\u00fchlet/", "tokens": ["Ders", "Hertz", "mit", "Girdt", "entt\u00b7z\u00fcndt", "/", "den", "mut", "mit", "Lus\u00b7te", "k\u00fch\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "VVPP", "$(", "ART", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da eins Gem\u00fchts vnd Willn ein par Ehvolck sich meint/", "tokens": ["Da", "eins", "Ge\u00b7m\u00fchts", "vnd", "Willn", "ein", "par", "Eh\u00b7volck", "sich", "meint", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "KON", "NE", "ART", "FM", "NN", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Solch jnre Brunst vnd Hitz mit frischer labung f\u00fclet/", "tokens": ["Solch", "jn\u00b7re", "Brunst", "vnd", "Hitz", "mit", "fri\u00b7scher", "la\u00b7bung", "f\u00fc\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "KON", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dem Edlen ", "tokens": ["Dem", "Ed\u00b7len"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Die Seel Menschlicher. Seel ist Flam\u0303br\u00fcnstige Liebe.", "tokens": ["Die", "Seel", "Menschli\u00b7cher", ".", "Seel", "ist", "Flam\u0303\u00b7br\u00fcns\u00b7ti\u00b7ge", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$.", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+--+-++--+-", "measure": "iambic.penta.relaxed"}}}}}