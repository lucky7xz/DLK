{"textgrid.poem.56353": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Der Szekler Landtag", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich will mich f\u00fcr das Faktum nicht verb\u00fcrgen,", "tokens": ["Ich", "will", "mich", "f\u00fcr", "das", "Fak\u00b7tum", "nicht", "ver\u00b7b\u00fcr\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich trag es vor, wie ich's geschrieben fand,", "tokens": ["Ich", "trag", "es", "vor", ",", "wie", "ich's", "ge\u00b7schrie\u00b7ben", "fand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "PIS", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schlagt die Geschichte nach von Siebenb\u00fcrgen.", "tokens": ["Schlagt", "die", "Ge\u00b7schich\u00b7te", "nach", "von", "Sie\u00b7ben\u00b7b\u00fcr\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "APPR", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Als einst der Sichel reif der Weizen stand", "tokens": ["Als", "einst", "der", "Si\u00b7chel", "reif", "der", "Wei\u00b7zen", "stand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In der Gespannschaft Szekl, da kam ein Regen,", "tokens": ["In", "der", "Ge\u00b7spann\u00b7schaft", "Szekl", ",", "da", "kam", "ein", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wovor des Landmanns sch\u00f6nste Hoffnung schwand.", "tokens": ["Wo\u00b7vor", "des", "Land\u00b7manns", "sch\u00f6ns\u00b7te", "Hoff\u00b7nung", "schwand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es wollte nicht der b\u00f6se West sich legen,", "tokens": ["Es", "woll\u00b7te", "nicht", "der", "b\u00f6\u00b7se", "West", "sich", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ART", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Es regnete der Regen alle Tage,", "tokens": ["Es", "reg\u00b7ne\u00b7te", "der", "Re\u00b7gen", "al\u00b7le", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und auf dem Feld verdarb der Gottessegen.", "tokens": ["Und", "auf", "dem", "Feld", "ver\u00b7darb", "der", "Got\u00b7tes\u00b7se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Geh\u00f6rt des Volkes laut erhobne Klage,", "tokens": ["Ge\u00b7h\u00f6rt", "des", "Vol\u00b7kes", "laut", "er\u00b7hob\u00b7ne", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Gefiel es, einen Landtag auszuschreiben,", "tokens": ["Ge\u00b7fiel", "es", ",", "ei\u00b7nen", "Land\u00b7tag", "aus\u00b7zu\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Um Rat zu halten \u00fcber diese Plage.", "tokens": ["Um", "Rat", "zu", "hal\u00b7ten", "\u00fc\u00b7ber", "die\u00b7se", "Pla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "PTKZU", "VVINF", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Die Landesboten lie\u00dfen nicht sich treiben,", "tokens": ["Die", "Lan\u00b7des\u00b7bo\u00b7ten", "lie\u00b7\u00dfen", "nicht", "sich", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Sie kamen gern, entschlossen gut zu tagen,", "tokens": ["Sie", "ka\u00b7men", "gern", ",", "ent\u00b7schlos\u00b7sen", "gut", "zu", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "VVFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Und Satzungen und Br\u00e4uchen treu zu bleiben.", "tokens": ["Und", "Sat\u00b7zun\u00b7gen", "und", "Br\u00e4u\u00b7chen", "treu", "zu", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.16": {"text": "Da wurde denn, nach br\u00e4uchlichen Gelagen,", "tokens": ["Da", "wur\u00b7de", "denn", ",", "nach", "br\u00e4uch\u00b7li\u00b7chen", "Ge\u00b7la\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Der Tag er\u00f6ffnet, und mit Ernst und Kraft", "tokens": ["Der", "Tag", "er\u00b7\u00f6ff\u00b7net", ",", "und", "mit", "Ernst", "und", "Kraft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "KON", "APPR", "NE", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Der Fall vom Landesmarschall vorgetragen:", "tokens": ["Der", "Fall", "vom", "Lan\u00b7des\u00b7mar\u00b7schall", "vor\u00b7ge\u00b7tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "\u00bbund nun, hochm\u00f6gende Genossenschaft,", "tokens": ["\u00bb", "und", "nun", ",", "hoch\u00b7m\u00f6\u00b7gen\u00b7de", "Ge\u00b7nos\u00b7sen\u00b7schaft", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "KON", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Wei\u00df einer Rat? Wer ist es, der zur Stunde", "tokens": ["Wei\u00df", "ei\u00b7ner", "Rat", "?", "Wer", "ist", "es", ",", "der", "zur", "Stun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "PWS", "VAFIN", "PPER", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Die Ernte trocken in die Scheune schafft?\u00ab", "tokens": ["Die", "Ern\u00b7te", "tro\u00b7cken", "in", "die", "Scheu\u00b7ne", "schafft", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Es herrschte tiefes Schweigen in der Runde,", "tokens": ["Es", "herrschte", "tie\u00b7fes", "Schwei\u00b7gen", "in", "der", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.23": {"text": "Doch nahm zuletzt das Wort ein w\u00fcrd'ger Greise", "tokens": ["Doch", "nahm", "zu\u00b7letzt", "das", "Wort", "ein", "w\u00fcrd'\u00b7ger", "Grei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Und sprach gewichtig mit beredtem Munde:", "tokens": ["Und", "sprach", "ge\u00b7wich\u00b7tig", "mit", "be\u00b7red\u00b7tem", "Mun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "\u00bbder Fall ist ernst, mit nichten w\u00e4r es weise,", "tokens": ["\u00bb", "der", "Fall", "ist", "ernst", ",", "mit", "nich\u00b7ten", "w\u00e4r", "es", "wei\u00b7se", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ADJD", "$,", "APPR", "PIS", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Mit \u00fcbereiltem Ratschlu\u00df einzugreifen;", "tokens": ["Mit", "\u00fc\u00b7be\u00b7reil\u00b7tem", "Rat\u00b7schlu\u00df", "ein\u00b7zu\u00b7grei\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Wir handeln nicht un\u00fcberlegter Weise.", "tokens": ["Wir", "han\u00b7deln", "nicht", "un\u00b7\u00fc\u00b7ber\u00b7leg\u00b7ter", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Drum ist mein Antrag, ohne weit zu schweifen:", "tokens": ["Drum", "ist", "mein", "An\u00b7trag", ",", "oh\u00b7ne", "weit", "zu", "schwei\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPOSAT", "NN", "$,", "KOUI", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "La\u00dft uns auf n\u00e4chsten Samstag uns vertagen;", "tokens": ["La\u00dft", "uns", "auf", "n\u00e4chs\u00b7ten", "Sams\u00b7tag", "uns", "ver\u00b7ta\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Die Zeit bringt Rat, sie wird die Sache reifen.\u00ab", "tokens": ["Die", "Zeit", "bringt", "Rat", ",", "sie", "wird", "die", "Sa\u00b7che", "rei\u00b7fen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Beschlossen ward, worauf er angetragen.", "tokens": ["Be\u00b7schlos\u00b7sen", "ward", ",", "wo\u00b7rauf", "er", "an\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "PWAV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Die Frist verstrich bei ew'gen Regenschauern,", "tokens": ["Die", "Frist", "ver\u00b7strich", "bei", "ew'\u00b7gen", "Re\u00b7gen\u00b7schau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Hinbr\u00fcten drauf und br\u00e4uchlichen Gelagen;", "tokens": ["Hin\u00b7br\u00fc\u00b7ten", "drauf", "und", "br\u00e4uch\u00b7li\u00b7chen", "Ge\u00b7la\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Der Samstag kam und sah dieselben Mauern", "tokens": ["Der", "Sams\u00b7tag", "kam", "und", "sah", "die\u00b7sel\u00b7ben", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Umfassen noch des Landes Rat und Hort,", "tokens": ["Um\u00b7fas\u00b7sen", "noch", "des", "Lan\u00b7des", "Rat", "und", "Hort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Und sah den leid'gen Regen ewig dauern.", "tokens": ["Und", "sah", "den", "lei\u00b7d'\u00b7gen", "Re\u00b7gen", "e\u00b7wig", "dau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Der Landesmarschall sprach ein ernstes Wort:", "tokens": ["Der", "Lan\u00b7des\u00b7mar\u00b7schall", "sprach", "ein", "erns\u00b7tes", "Wort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "\u00bbhochm\u00f6gende, nun tut nach eurer Pflicht,", "tokens": ["\u00bb", "hoch\u00b7m\u00f6\u00b7gen\u00b7de", ",", "nun", "tut", "nach", "eu\u00b7rer", "Pflicht", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "$,", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Ihr seht, der Regen regnet ewig fort.", "tokens": ["Ihr", "seht", ",", "der", "Re\u00b7gen", "reg\u00b7net", "e\u00b7wig", "fort", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Wer ist es, der das Wort der Weisheit spricht?", "tokens": ["Wer", "ist", "es", ",", "der", "das", "Wort", "der", "Weis\u00b7heit", "spricht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Wer bringt in unsres Sinnens d\u00fcstre Nacht", "tokens": ["Wer", "bringt", "in", "uns\u00b7res", "Sin\u00b7nens", "d\u00fcst\u00b7re", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "PPOSAT", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Das lang erwartete, begehrte Licht?", "tokens": ["Das", "lang", "er\u00b7war\u00b7te\u00b7te", ",", "be\u00b7gehr\u00b7te", "Licht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Zur Tat! ihr habt erwogen und bedacht.", "tokens": ["Zur", "Tat", "!", "ihr", "habt", "er\u00b7wo\u00b7gen", "und", "be\u00b7dacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "PPER", "VAFIN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Ich wende mich zuerst an diesen Alten,", "tokens": ["Ich", "wen\u00b7de", "mich", "zu\u00b7erst", "an", "die\u00b7sen", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Des Scharfsinn einmal schon uns Trost gebracht:", "tokens": ["Des", "Scharf\u00b7sinn", "ein\u00b7mal", "schon", "uns", "Trost", "ge\u00b7bracht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Ehrw\u00fcrd'ger Greis, la\u00df deine Weisheit walten.\u00ab", "tokens": ["Ehr\u00b7w\u00fcrd'\u00b7ger", "Greis", ",", "la\u00df", "dei\u00b7ne", "Weis\u00b7heit", "wal\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "VVIMP", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "Der stand und sprach: \u00bbIch bin ein alter Mann,", "tokens": ["Der", "stand", "und", "sprach", ":", "\u00bb", "Ich", "bin", "ein", "al\u00b7ter", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "w