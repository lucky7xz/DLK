{"textgrid.poem.51000": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "3", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Fischlein steht am k\u00fchlen Grund,", "tokens": ["Ein", "Fisc\u00b7hlein", "steht", "am", "k\u00fch\u00b7len", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durchsichtig flie\u00dfen die Wogen,", "tokens": ["Durch\u00b7sich\u00b7tig", "flie\u00b7\u00dfen", "die", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Und senkrecht ob ihm hat sein Rund", "tokens": ["Und", "sen\u00b7krecht", "ob", "ihm", "hat", "sein", "Rund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOUS", "PPER", "VAFIN", "PPOSAT", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ein schwebender Falk gezogen.", "tokens": ["Ein", "schwe\u00b7ben\u00b7der", "Falk", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Der ist so lerchenklein zu sehn", "tokens": ["Der", "ist", "so", "ler\u00b7chen\u00b7klein", "zu", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zuh\u00f6chst im Himmelsdome;", "tokens": ["Zu\u00b7h\u00f6chst", "im", "Him\u00b7mels\u00b7do\u00b7me", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er sieht das Fischlein ruhig stehn,", "tokens": ["Er", "sieht", "das", "Fisc\u00b7hlein", "ru\u00b7hig", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gl\u00e4nzend im tiefen Strome!", "tokens": ["Gl\u00e4n\u00b7zend", "im", "tie\u00b7fen", "Stro\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Und dieses auch hinwieder sieht", "tokens": ["Und", "die\u00b7ses", "auch", "hin\u00b7wie\u00b7der", "sieht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ins Blaue durch seine Welle;", "tokens": ["Ins", "Blau\u00b7e", "durch", "sei\u00b7ne", "Wel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich glaube gar, das Sehnen zieht", "tokens": ["Ich", "glau\u00b7be", "gar", ",", "das", "Seh\u00b7nen", "zieht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eins an des andern Stelle!", "tokens": ["Eins", "an", "des", "an\u00b7dern", "Stel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}