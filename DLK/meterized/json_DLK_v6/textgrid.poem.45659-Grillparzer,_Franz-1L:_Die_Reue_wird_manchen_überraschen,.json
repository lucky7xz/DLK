{"textgrid.poem.45659": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Reue wird manchen \u00fcberraschen,", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Reue wird manchen \u00fcberraschen,", "tokens": ["Die", "Reu\u00b7e", "wird", "man\u00b7chen", "\u00fc\u00b7berr\u00b7a\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "ADJA", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wenn seine Worte werden zu Sachen:", "tokens": ["Wenn", "sei\u00b7ne", "Wor\u00b7te", "wer\u00b7den", "zu", "Sa\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ihr habt daher den Pelz gewaschen,", "tokens": ["Ihr", "habt", "da\u00b7her", "den", "Pelz", "ge\u00b7wa\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ohne ihn na\u00df zu machen.", "tokens": ["Oh\u00b7ne", "ihn", "na\u00df", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}