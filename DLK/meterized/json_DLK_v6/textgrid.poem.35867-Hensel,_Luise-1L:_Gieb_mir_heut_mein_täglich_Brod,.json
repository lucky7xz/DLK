{"textgrid.poem.35867": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gieb mir heut mein t\u00e4glich Brod,", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gieb mir heut mein t\u00e4glich Brod,", "tokens": ["Gieb", "mir", "heut", "mein", "t\u00e4g\u00b7lich", "Brod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das Geheimni\u00df Deiner Liebe!", "tokens": ["Das", "Ge\u00b7heim\u00b7ni\u00df", "Dei\u00b7ner", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gro\u00df w\u00e4r' meiner Seele Noth,", "tokens": ["Gro\u00df", "w\u00e4r'", "mei\u00b7ner", "See\u00b7le", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn sie ohne Nahrung bliebe.", "tokens": ["Wenn", "sie", "oh\u00b7ne", "Nah\u00b7rung", "blie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was dem Moose linder Thau,", "tokens": ["Was", "dem", "Moo\u00b7se", "lin\u00b7der", "Thau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sonnenschein der Sonnenwende,", "tokens": ["Son\u00b7nen\u00b7schein", "der", "Son\u00b7nen\u00b7wen\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Milder Regen d\u00fcrrer Au,", "tokens": ["Mil\u00b7der", "Re\u00b7gen", "d\u00fcr\u00b7rer", "Au", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist mir diese Lebensspende. \u2013", "tokens": ["Ist", "mir", "die\u00b7se", "Le\u00b7bens\u00b7spen\u00b7de", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "PDAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nimmer fa\u00dft ein Menschensinn,", "tokens": ["Nim\u00b7mer", "fa\u00dft", "ein", "Men\u00b7schen\u00b7sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Herr! die Gr\u00f6\u00dfe Deiner Liebe;", "tokens": ["Herr", "!", "die", "Gr\u00f6\u00b7\u00dfe", "Dei\u00b7ner", "Lie\u00b7be", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimm zum Dank mein Leben hin,", "tokens": ["Nimm", "zum", "Dank", "mein", "Le\u00b7ben", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "All mein Sinnen, meine Triebe.", "tokens": ["All", "mein", "Sin\u00b7nen", ",", "mei\u00b7ne", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und den Armen, die noch fern", "tokens": ["Und", "den", "Ar\u00b7men", ",", "die", "noch", "fern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Drau\u00dfen irren vor der Th\u00fcre,", "tokens": ["Drau\u00b7\u00dfen", "ir\u00b7ren", "vor", "der", "Th\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sende Deiner Wahrheit Stern,", "tokens": ["Sen\u00b7de", "Dei\u00b7ner", "Wahr\u00b7heit", "Stern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der sie zu der H\u00fcrde f\u00fchre,", "tokens": ["Der", "sie", "zu", "der", "H\u00fcr\u00b7de", "f\u00fch\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wo der siebenfache Quell", "tokens": ["Wo", "der", "sie\u00b7ben\u00b7fa\u00b7che", "Quell"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Heilt der Seele tiefste Wunden", "tokens": ["Heilt", "der", "See\u00b7le", "tiefs\u00b7te", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und am Born, krystallenhell,", "tokens": ["Und", "am", "Born", ",", "krys\u00b7tal\u00b7len\u00b7hell", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPRART", "NE", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00fc\u00dfe Weide wird gefunden.", "tokens": ["S\u00fc\u00b7\u00dfe", "Wei\u00b7de", "wird", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "O, mich dauert ihre Noth:", "tokens": ["O", ",", "mich", "dau\u00b7ert", "ih\u00b7re", "Noth", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Irr' gehn Viele, tief umnachtet;", "tokens": ["Irr'", "gehn", "Vie\u00b7le", ",", "tief", "um\u00b7nach\u00b7tet", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "$,", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Brich, o Herr! Dein Lebensbrod", "tokens": ["Brich", ",", "o", "Herr", "!", "Dein", "Le\u00b7bens\u00b7brod"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "FM", "NN", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jeder Seele, die da schmachtet!", "tokens": ["Je\u00b7der", "See\u00b7le", ",", "die", "da", "schmach\u00b7tet", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}