{"textgrid.poem.25508": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Horchend lauscht' ich manche Nacht,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Horchend lauscht' ich manche Nacht,", "tokens": ["Hor\u00b7chend", "lauscht'", "ich", "man\u00b7che", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob sich ", "tokens": ["Ob", "sich"], "token_info": ["word", "word"], "pos": ["KOUS", "PRF"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Hurtig war ich auf, und sacht", "tokens": ["Hur\u00b7tig", "war", "ich", "auf", ",", "und", "sacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Trippelt' ich zur Kammerth\u00fcre,", "tokens": ["Trip\u00b7pelt'", "ich", "zur", "Kam\u00b7mert\u00b7h\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sa\u00df bei meines L\u00e4mpchens Schein", "tokens": ["Sa\u00df", "bei", "mei\u00b7nes", "L\u00e4mp\u00b7chens", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ganze N\u00e4chte, ganz allein.", "tokens": ["Gan\u00b7ze", "N\u00e4ch\u00b7te", ",", "ganz", "al\u00b7lein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nicht f\u00fcr einer F\u00fcrstin Pracht", "tokens": ["Nicht", "f\u00fcr", "ei\u00b7ner", "F\u00fcrs\u00b7tin", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00e4tt' ich sonst, im \u00f6den Zimmer", "tokens": ["H\u00e4tt'", "ich", "sonst", ",", "im", "\u00f6\u00b7den", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ganz allein, nur Eine Nacht", "tokens": ["Ganz", "al\u00b7lein", ",", "nur", "Ei\u00b7ne", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Selbst bei tausend Kerzen Schimmer", "tokens": ["Selbst", "bei", "tau\u00b7send", "Ker\u00b7zen", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "CARD", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Gegen Schrecken mich gewehrt:", "tokens": ["Ge\u00b7gen", "Schre\u00b7cken", "mich", "ge\u00b7wehrt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Was die Liebe doch nicht lehrt!", "tokens": ["Was", "die", "Lie\u00b7be", "doch", "nicht", "lehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Schnarchend lag ", "tokens": ["Schnar\u00b7chend", "lag"], "token_info": ["word", "word"], "pos": ["ADJD", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.14": {"text": "Lang und breit auf meinem Schoo\u00dfe,", "tokens": ["Lang", "und", "breit", "auf", "mei\u00b7nem", "Schoo\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Und ermuntert' er sich ja,", "tokens": ["Und", "er\u00b7mun\u00b7tert'", "er", "sich", "ja", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Ha! was macht' er dann f\u00fcr gro\u00dfe", "tokens": ["Ha", "!", "was", "macht'", "er", "dann", "f\u00fcr", "gro\u00b7\u00dfe"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PWS", "VVFIN", "PPER", "ADV", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Wunderaugen, da\u00df ich Ding", "tokens": ["Wun\u00b7der\u00b7au\u00b7gen", ",", "da\u00df", "ich", "Ding"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Gar nicht mehr zu Bette ging.", "tokens": ["Gar", "nicht", "mehr", "zu", "Bet\u00b7te", "ging", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.19": {"text": "Diese Netze strickt' ich dann,", "tokens": ["Die\u00b7se", "Net\u00b7ze", "strickt'", "ich", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Und bei jedem Knoten flogen", "tokens": ["Und", "bei", "je\u00b7dem", "Kno\u00b7ten", "flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Hundert Seufzer zu dem Mann',", "tokens": ["Hun\u00b7dert", "Seuf\u00b7zer", "zu", "dem", "Mann'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Der mich selbst ins Netz gezogen;", "tokens": ["Der", "mich", "selbst", "ins", "Netz", "ge\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Was? gezogen? nein doch, nein!", "tokens": ["Was", "?", "ge\u00b7zo\u00b7gen", "?", "nein", "doch", ",", "nein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "$.", "VVPP", "$.", "PTKANT", "ADV", "$,", "PTKANT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Lief ich denn nicht selbst hinein?", "tokens": ["Lief", "ich", "denn", "nicht", "selbst", "hin\u00b7ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Ist es nicht so gut darin?", "tokens": ["Ist", "es", "nicht", "so", "gut", "da\u00b7rin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "ADJD", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Zehnmal besser als im Freien?", "tokens": ["Zehn\u00b7mal", "bes\u00b7ser", "als", "im", "Frei\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Kannst du Gl\u00fcck! so wie ich bin,", "tokens": ["Kannst", "du", "Gl\u00fcck", "!", "so", "wie", "ich", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "$.", "ADV", "KOKOM", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Einen Wunsch mir noch verleihen?", "tokens": ["Ei\u00b7nen", "Wunsch", "mir", "noch", "ver\u00b7lei\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Aber st\u00f6\u00dft er mich hinaus \u2013", "tokens": ["A\u00b7ber", "st\u00f6\u00dft", "er", "mich", "hin\u00b7aus", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APZR", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Dann ist alles, alles aus!", "tokens": ["Dann", "ist", "al\u00b7les", ",", "al\u00b7les", "aus", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "$,", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}