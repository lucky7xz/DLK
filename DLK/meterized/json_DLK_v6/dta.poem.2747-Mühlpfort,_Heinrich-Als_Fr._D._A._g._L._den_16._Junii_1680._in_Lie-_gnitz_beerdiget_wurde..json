{"dta.poem.2747": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Als Fr. D. A. g. L. den 16. Junii 1680. in Lie-  \n gnitz beerdiget wurde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aegypten mahlt uns f\u00fcr durch einen grossen Drachen/", "tokens": ["A\u00b7e\u00b7gyp\u00b7ten", "mahlt", "uns", "f\u00fcr", "durch", "ei\u00b7nen", "gros\u00b7sen", "Dra\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Der in die tieffe Klufft sich einer H\u00f6le dringt/", "tokens": ["Der", "in", "die", "tief\u00b7fe", "Klufft", "sich", "ei\u00b7ner", "H\u00f6\u00b7le", "dringt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "PRF", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und den geschuppten Leib in gr\u00fcne Zirkel schlingt/", "tokens": ["Und", "den", "ge\u00b7schupp\u00b7ten", "Leib", "in", "gr\u00fc\u00b7ne", "Zir\u00b7kel", "schlingt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bi\u00df er sich selbst verzehrt durch seinen eignen Rachen/", "tokens": ["Bi\u00df", "er", "sich", "selbst", "ver\u00b7zehrt", "durch", "sei\u00b7nen", "eig\u00b7nen", "Ra\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df eben so die Zeit in ihrem Abgrund sey/", "tokens": ["Da\u00df", "e\u00b7ben", "so", "die", "Zeit", "in", "ih\u00b7rem", "Ab\u00b7grund", "sey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und was sie heute baut offt morgen bricht entzwey.", "tokens": ["Und", "was", "sie", "heu\u00b7te", "baut", "offt", "mor\u00b7gen", "bricht", "ent\u00b7zwey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "ADV", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es mahlt der Dichter Witz ihr Blitz-geschwinde Fl\u00fcgel/", "tokens": ["Es", "mahlt", "der", "Dich\u00b7ter", "Witz", "ihr", "Blitz\u00b7ge\u00b7schwin\u00b7de", "Fl\u00fc\u00b7gel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und waffnet ihre Hand mit einem leichten Pfeil.", "tokens": ["Und", "waff\u00b7net", "ih\u00b7re", "Hand", "mit", "ei\u00b7nem", "leich\u00b7ten", "Pfeil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Adler schwingt sich nicht in so geschwinder Eil", "tokens": ["Der", "Ad\u00b7ler", "schwingt", "sich", "nicht", "in", "so", "ge\u00b7schwin\u00b7der", "Eil"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKNEG", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "(der doch der L\u00fcffte Printz) hoch \u00fcber Berg und H\u00fcgel.", "tokens": ["(", "der", "doch", "der", "L\u00fcff\u00b7te", "Printz", ")", "hoch", "\u00fc\u00b7ber", "Berg", "und", "H\u00fc\u00b7gel", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADV", "ART", "NN", "NN", "$(", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Kein Schiff das guter Wind mit vollen Segeln f\u00fchrt/", "tokens": ["Kein", "Schiff", "das", "gu\u00b7ter", "Wind", "mit", "vol\u00b7len", "Se\u00b7geln", "f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Hat bey dem Zwey-Gestirn so schnell den Port ber\u00fchrt.", "tokens": ["Hat", "bey", "dem", "Zwey\u00b7Ge\u00b7stirn", "so", "schnell", "den", "Port", "be\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "ADJD", "ART", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Was mehr/ so hat die Zeit geschwinde Tiger F\u00fcsse", "tokens": ["Was", "mehr", "/", "so", "hat", "die", "Zeit", "ge\u00b7schwin\u00b7de", "Ti\u00b7ger", "F\u00fcs\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "$(", "ADV", "VAFIN", "ART", "NN", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Jhr Sprung ereilet auch die m\u00e4chtigste Gewalt;", "tokens": ["Ihr", "Sprung", "er\u00b7ei\u00b7let", "auch", "die", "m\u00e4ch\u00b7tigs\u00b7te", "Ge\u00b7walt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So \u00e4ndert sie noch mehr als Proteus die Gestalt/", "tokens": ["So", "\u00e4n\u00b7dert", "sie", "noch", "mehr", "als", "Pro\u00b7teus", "die", "Ge\u00b7stalt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIAT", "KOKOM", "NE", "ART", "NN", "$("], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Und gegen ihrer Flucht sind langsam Strohm und Fl\u00fcsse.", "tokens": ["Und", "ge\u00b7gen", "ih\u00b7rer", "Flucht", "sind", "lang\u00b7sam", "Strohm", "und", "Fl\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VAFIN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ein Rad scheint tr\u00e4g und faul das doch unendlich geht/", "tokens": ["Ein", "Rad", "scheint", "tr\u00e4g", "und", "faul", "das", "doch", "un\u00b7end\u00b7lich", "geht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "PDS", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "F\u00fcr ihrer Fertigkeit entschl\u00e4ffet der Magnet.", "tokens": ["F\u00fcr", "ih\u00b7rer", "Fer\u00b7tig\u00b7keit", "ent\u00b7schl\u00e4f\u00b7fet", "der", "Mag\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "++-+-+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Ist nun die Zeit so schnell und so ein fressend Feuer/", "tokens": ["Ist", "nun", "die", "Zeit", "so", "schnell", "und", "so", "ein", "fres\u00b7send", "Feu\u00b7er", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "ADJD", "KON", "ADV", "ART", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Greifft ihre Grausamkeit auch Ertz und Marmel an?", "tokens": ["Greifft", "ih\u00b7re", "Grau\u00b7sam\u00b7keit", "auch", "Ertz", "und", "Mar\u00b7mel", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Zermalmt manch K\u00f6nigreich ihr uners\u00e4ttlich Zahn?", "tokens": ["Zer\u00b7malmt", "manch", "K\u00f6\u00b7nig\u00b7reich", "ihr", "un\u00b7er\u00b7s\u00e4tt\u00b7lich", "Zahn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und sind durch sie gest\u00fcrtzt die sch\u00f6nsten Ungeheuer?", "tokens": ["Und", "sind", "durch", "sie", "ge\u00b7st\u00fcrtzt", "die", "sch\u00f6ns\u00b7ten", "Un\u00b7ge\u00b7heu\u00b7er", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPER", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ist das Mausol nur Asch und der Colossus Staub", "tokens": ["Ist", "das", "Mau\u00b7sol", "nur", "Asch", "und", "der", "Co\u00b7los\u00b7sus", "Staub"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADV", "NN", "KON", "ART", "NE", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "Dianens Tempel selbst der Eitelkeiten Raub.", "tokens": ["Di\u00b7a\u00b7nens", "Tem\u00b7pel", "selbst", "der", "Ei\u00b7tel\u00b7kei\u00b7ten", "Raub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wie kan der arme Mensch erbaut aus schlechter Erden/", "tokens": ["Wie", "kan", "der", "ar\u00b7me", "Mensch", "er\u00b7baut", "aus", "schlech\u00b7ter", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "ADJA", "NN", "VVPP", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ein Tohn den jeder Sto\u00df in tausend St\u00fccke bricht/", "tokens": ["Ein", "Tohn", "den", "je\u00b7der", "Sto\u00df", "in", "tau\u00b7send", "St\u00fc\u00b7cke", "bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PIAT", "NN", "APPR", "CARD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ein Leim aus einem Klo\u00df von Anfang zugericht/", "tokens": ["Ein", "Leim", "aus", "ei\u00b7nem", "Klo\u00df", "von", "An\u00b7fang", "zu\u00b7ge\u00b7richt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Von der Bew\u00e4ltigung der Zeit befreyet werden?", "tokens": ["Von", "der", "Be\u00b7w\u00e4l\u00b7ti\u00b7gung", "der", "Zeit", "be\u00b7fre\u00b7yet", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Er ist ja weiter nichts als nur ein Spiel der Zeit/", "tokens": ["Er", "ist", "ja", "wei\u00b7ter", "nichts", "als", "nur", "ein", "Spiel", "der", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PIS", "KOKOM", "ADV", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und Grab und Wiege sind von schlechtem Unterscheid.", "tokens": ["Und", "Grab", "und", "Wie\u00b7ge", "sind", "von", "schlech\u00b7tem", "Un\u00b7ter\u00b7scheid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Doch weil der h\u00f6chste GOtt den Scepter ihm gegeben/", "tokens": ["Doch", "weil", "der", "h\u00f6chs\u00b7te", "Gott", "den", "Scep\u00b7ter", "ihm", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "ART", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und die gevierdte Welt ihm unterthan gemacht/", "tokens": ["Und", "die", "ge\u00b7vierd\u00b7te", "Welt", "ihm", "un\u00b7ter\u00b7than", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PPER", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "So ist er billich auch auf Schuldigkeit bedacht", "tokens": ["So", "ist", "er", "bil\u00b7lich", "auch", "auf", "Schul\u00b7dig\u00b7keit", "be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADV", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wie er m\u00f6g in der Zeit wohl und vern\u00fcnfftig leben.", "tokens": ["Wie", "er", "m\u00f6g", "in", "der", "Zeit", "wohl", "und", "ver\u00b7n\u00fcnff\u00b7tig", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "APPR", "ART", "NN", "ADV", "KON", "ADJD", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.35": {"text": "Viel brauchen sie zur Lust/ viel brauchen sie zur Quaal.", "tokens": ["Viel", "brau\u00b7chen", "sie", "zur", "Lust", "/", "viel", "brau\u00b7chen", "sie", "zur", "Qua\u00b7al", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$(", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Den meisten die nichts thun entschlippt sie wie ein Aal.", "tokens": ["Den", "meis\u00b7ten", "die", "nichts", "thun", "ent\u00b7schlippt", "sie", "wie", "ein", "Aal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ART", "PIS", "VVINF", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.37": {"text": "Noch mehr verirren sich in gantz verkehrten Wegen/", "tokens": ["Noch", "mehr", "ver\u00b7ir\u00b7ren", "sich", "in", "gantz", "ver\u00b7kehr\u00b7ten", "We\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "APPR", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und kreutzen wie ein Schiff auf der Gedancken Meer/", "tokens": ["Und", "kreut\u00b7zen", "wie", "ein", "Schiff", "auf", "der", "Ge\u00b7dan\u00b7cken", "Meer", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Von der Begierden Sturm getrieben hin und her/", "tokens": ["Von", "der", "Be\u00b7gier\u00b7den", "Sturm", "ge\u00b7trie\u00b7ben", "hin", "und", "her", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "PTKVZ", "KON", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und wollen Ancker an bey Scyll und Syrten legen.", "tokens": ["Und", "wol\u00b7len", "An\u00b7cker", "an", "bey", "Scyll", "und", "Syr\u00b7ten", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "APPR", "APPR", "NE", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Erkiesen vor das Haupt der Hoffnung/ die Malee/", "tokens": ["Er\u00b7kie\u00b7sen", "vor", "das", "Haupt", "der", "Hoff\u00b7nung", "/", "die", "Ma\u00b7lee", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$(", "ART", "NN", "$("], "meter": "