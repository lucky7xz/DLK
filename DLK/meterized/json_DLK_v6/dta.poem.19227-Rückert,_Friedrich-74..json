{"dta.poem.19227": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "74.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Mistel, wenn sie kocht f\u00fcr dich den Vogelleim,", "tokens": ["Die", "Mis\u00b7tel", ",", "wenn", "sie", "kocht", "f\u00fcr", "dich", "den", "Vo\u00b7gel\u00b7leim", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Sohn, sorgt nur damit f\u00fcr ihren Samenkeim.", "tokens": ["Mein", "Sohn", ",", "sorgt", "nur", "da\u00b7mit", "f\u00fcr", "ih\u00b7ren", "Sa\u00b7men\u00b7keim", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ADV", "PAV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie kann im Boden nicht gleich andern Pflanzen wurzeln,", "tokens": ["Sie", "kann", "im", "Bo\u00b7den", "nicht", "gleich", "an\u00b7dern", "Pflan\u00b7zen", "wur\u00b7zeln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "PTKNEG", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Nur Nahrung saugen aus Baum\u00e4sten oder Sturzeln.", "tokens": ["Nur", "Nah\u00b7rung", "sau\u00b7gen", "aus", "Bau\u00b7m\u00e4s\u00b7ten", "o\u00b7der", "Stur\u00b7zeln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und nimmer w\u00fcrde sie Nachkommenschaft erzielen,", "tokens": ["Und", "nim\u00b7mer", "w\u00fcr\u00b7de", "sie", "Nach\u00b7kom\u00b7men\u00b7schaft", "er\u00b7zie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn ihre Samen hoch vom Baum zur Erde fielen.", "tokens": ["Wenn", "ih\u00b7re", "Sa\u00b7men", "hoch", "vom", "Baum", "zur", "Er\u00b7de", "fie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "APPRART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dis zu verhindern ist die Klebrigkeit bestimmt", "tokens": ["Dis", "zu", "ver\u00b7hin\u00b7dern", "ist", "die", "Kleb\u00b7rig\u00b7keit", "be\u00b7stimmt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PTKZU", "VVINF", "VAFIN", "ART", "NN", "VVPP"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Dem K\u00f6rnchen, das in halbdurchsichtiger Beere schwimmt.", "tokens": ["Dem", "K\u00f6rn\u00b7chen", ",", "das", "in", "halb\u00b7durch\u00b7sich\u00b7ti\u00b7ger", "Bee\u00b7re", "schwimmt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Das K\u00f6rnchen kommt im Fall hier oder dort zu kleben", "tokens": ["Das", "K\u00f6rn\u00b7chen", "kommt", "im", "Fall", "hier", "o\u00b7der", "dort", "zu", "kle\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ADV", "KON", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An einen Zweig, und wird nicht lang unschl\u00fcssig schweben.", "tokens": ["An", "ei\u00b7nen", "Zweig", ",", "und", "wird", "nicht", "lang", "un\u00b7schl\u00fcs\u00b7sig", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KON", "VAFIN", "PTKNEG", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Da wo es anklebt, wird's geschwind ein W\u00fcrzlein schlagen,", "tokens": ["Da", "wo", "es", "an\u00b7klebt", ",", "wird's", "ge\u00b7schwind", "ein", "W\u00fcrz\u00b7lein", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann treiben einen Spro\u00df, und wieder Beeren tragen.", "tokens": ["Dann", "trei\u00b7ben", "ei\u00b7nen", "Spro\u00df", ",", "und", "wie\u00b7der", "Bee\u00b7ren", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Viel anders aber treibt es untenher und oben", "tokens": ["Viel", "an\u00b7ders", "a\u00b7ber", "treibt", "es", "un\u00b7ten\u00b7her", "und", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "PPER", "ADJD", "KON", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als andre Pflanzen, die sich frei vom Boden hoben.", "tokens": ["Als", "and\u00b7re", "Pflan\u00b7zen", ",", "die", "sich", "frei", "vom", "Bo\u00b7den", "ho\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,", "PRELS", "PRF", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Denn senkrecht senken sie die Wurzel all nach unten,", "tokens": ["Denn", "sen\u00b7krecht", "sen\u00b7ken", "sie", "die", "Wur\u00b7zel", "all", "nach", "un\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "PIAT", "APPR", "ADV", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Und gradauf oben steigt ihr gr\u00fcnes Blatt zum bunten.", "tokens": ["Und", "grad\u00b7auf", "o\u00b7ben", "steigt", "ihr", "gr\u00fc\u00b7nes", "Blatt", "zum", "bun\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Die Mistel aber mu\u00df sich fremdem Stamm bequemen,", "tokens": ["Die", "Mis\u00b7tel", "a\u00b7ber", "mu\u00df", "sich", "frem\u00b7dem", "Stamm", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "PRF", "ADJA", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie er gewachsen ist, danach ihr Wachsthum nehmen.", "tokens": ["Wie", "er", "ge\u00b7wach\u00b7sen", "ist", ",", "da\u00b7nach", "ihr", "Wach\u00b7sthum", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "VAFIN", "$,", "PAV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Ob oben, unten, ob sie h\u00fcben sitzt ob dr\u00fcben", "tokens": ["Ob", "o\u00b7ben", ",", "un\u00b7ten", ",", "ob", "sie", "h\u00fc\u00b7ben", "sitzt", "ob", "dr\u00fc\u00b7ben"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "$,", "ADV", "$,", "KOUS", "PPER", "ADV", "VVFIN", "KOUS", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am Stamm, danach mu\u00df sie verschiedne K\u00fcnste \u00fcben.", "tokens": ["Am", "Stamm", ",", "da\u00b7nach", "mu\u00df", "sie", "ver\u00b7schied\u00b7ne", "K\u00fcns\u00b7te", "\u00fc\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PAV", "VMFIN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Bald abwerts, bald hinauf, bald mehr und minder schief", "tokens": ["Bald", "ab\u00b7werts", ",", "bald", "hin\u00b7auf", ",", "bald", "mehr", "und", "min\u00b7der", "schief"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "ADV", "PTKVZ", "$,", "ADV", "ADV", "KON", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wei\u00df sie die Wurzel einzuschieben stark und tief,", "tokens": ["Wei\u00df", "sie", "die", "Wur\u00b7zel", "ein\u00b7zu\u00b7schie\u00b7ben", "stark", "und", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVIZU", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "In jeder Richtung dann den Stengel zu entfalten,", "tokens": ["In", "je\u00b7der", "Rich\u00b7tung", "dann", "den", "Sten\u00b7gel", "zu", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und auch kopfuntersich die Schwebe wol zu halten.", "tokens": ["Und", "auch", "ko\u00b7pfun\u00b7ter\u00b7sich", "die", "Schwe\u00b7be", "wol", "zu", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}