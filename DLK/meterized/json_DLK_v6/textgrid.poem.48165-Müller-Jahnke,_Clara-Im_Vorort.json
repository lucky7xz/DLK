{"textgrid.poem.48165": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Im Vorort", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchwinternacht, Spr\u00fchregen st\u00e4ubt", "tokens": ["Fr\u00fch\u00b7win\u00b7ter\u00b7nacht", ",", "Spr\u00fch\u00b7re\u00b7gen", "st\u00e4ubt"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "VVFIN"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "durch Vorortstra\u00dfen, stumm und leer;", "tokens": ["durch", "Vor\u00b7ort\u00b7stra\u00b7\u00dfen", ",", "stumm", "und", "leer", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ein leises, dumpfes Donnern treibt", "tokens": ["ein", "lei\u00b7ses", ",", "dum\u00b7pfes", "Don\u00b7nern", "treibt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "der Nachtwind nur vom Bahndamm her.", "tokens": ["der", "Nacht\u00b7wind", "nur", "vom", "Bahn\u00b7damm", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durch bl\u00e4tterlose Pappelreihn", "tokens": ["Durch", "bl\u00e4t\u00b7ter\u00b7lo\u00b7se", "Pap\u00b7pel\u00b7reihn"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "die blassen Nebel brau'n und ziehn \u2013", "tokens": ["die", "blas\u00b7sen", "Ne\u00b7bel", "brau'n", "und", "ziehn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "im Osten loht's wie Feuerschein:", "tokens": ["im", "Os\u00b7ten", "loht's", "wie", "Feu\u00b7er\u00b7schein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da liegt Berlin.", "tokens": ["Da", "liegt", "Ber\u00b7lin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Wie Feuerschein die ganze Nacht!", "tokens": ["Wie", "Feu\u00b7er\u00b7schein", "die", "gan\u00b7ze", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Menschheit Wesen scheint vertauscht,", "tokens": ["Der", "Menschheit", "We\u00b7sen", "scheint", "ver\u00b7tauscht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "wie hab ich oft, vom Traum erwacht,", "tokens": ["wie", "hab", "ich", "oft", ",", "vom", "Traum", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "das ferne rote Licht belauscht!", "tokens": ["das", "fer\u00b7ne", "ro\u00b7te", "Licht", "be\u00b7lauscht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das sang mir durch die Zeit der Ruh", "tokens": ["Das", "sang", "mir", "durch", "die", "Zeit", "der", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "die M\u00e4r vom ewgen Widerstreit,", "tokens": ["die", "M\u00e4r", "vom", "ew\u00b7gen", "Wi\u00b7der\u00b7streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "den Lockruf aller L\u00fcste zu \u2013", "tokens": ["den", "Lock\u00b7ruf", "al\u00b7ler", "L\u00fcs\u00b7te", "zu", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "PTKZU", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "und singt das alte Lied vom Leid.", "tokens": ["und", "singt", "das", "al\u00b7te", "Lied", "vom", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und durch den roten D\u00e4mmer schaun", "tokens": ["Und", "durch", "den", "ro\u00b7ten", "D\u00e4m\u00b7mer", "schaun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mich irre Augen heischend an:", "tokens": ["mich", "ir\u00b7re", "Au\u00b7gen", "hei\u00b7schend", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "im Federhut erloschne Fraun,", "tokens": ["im", "Fe\u00b7der\u00b7hut", "er\u00b7lo\u00b7schne", "Fraun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "im Efeukranz der trunkne Mann,", "tokens": ["im", "E\u00b7feu\u00b7kranz", "der", "trunk\u00b7ne", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und Kinder, zitternd, frostdurchbebt, \u2013", "tokens": ["und", "Kin\u00b7der", ",", "zit\u00b7ternd", ",", "frost\u00b7durch\u00b7bebt", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "NN", "$,", "VVPP", "$,", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "das st\u00f6hnt und kichert, schluchzt und braust:", "tokens": ["das", "st\u00f6hnt", "und", "ki\u00b7chert", ",", "schluchzt", "und", "braust", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "und aus dem Hexensabbat hebt", "tokens": ["und", "aus", "dem", "He\u00b7xens\u00b7ab\u00b7bat", "hebt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "sich hammerhart die Arbeitsfaust!", "tokens": ["sich", "ham\u00b7mer\u00b7hart", "die", "Ar\u00b7beits\u00b7faust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Fr\u00fchwinternacht. Der Regen spr\u00fcht", "tokens": ["Fr\u00fch\u00b7win\u00b7ter\u00b7nacht", ".", "Der", "Re\u00b7gen", "spr\u00fcht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "ART", "NN", "VVFIN"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "durch Vorortstra\u00dfen, tot und leer;", "tokens": ["durch", "Vor\u00b7ort\u00b7stra\u00b7\u00dfen", ",", "tot", "und", "leer", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ein funkelnd H\u00f6llenauge, gl\u00fcht", "tokens": ["ein", "fun\u00b7kelnd", "H\u00f6l\u00b7len\u00b7au\u00b7ge", ",", "gl\u00fcht"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJD", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "das Haltsignal vom Bahnhof her.", "tokens": ["das", "Halt\u00b7sig\u00b7nal", "vom", "Bahn\u00b7hof", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durch bl\u00e4tterlose Pappelreihn", "tokens": ["Durch", "bl\u00e4t\u00b7ter\u00b7lo\u00b7se", "Pap\u00b7pel\u00b7reihn"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "die n\u00e4chtgen Nebel westw\u00e4rts ziehn \u2013", "tokens": ["die", "n\u00e4cht\u00b7gen", "Ne\u00b7bel", "west\u00b7w\u00e4rts", "ziehn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "im Osten flammt's wir Fr\u00fchrotschein:", "tokens": ["im", "Os\u00b7ten", "flammt's", "wir", "Fr\u00fch\u00b7rot\u00b7schein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das ist Berlin!", "tokens": ["Das", "ist", "Ber\u00b7lin", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}