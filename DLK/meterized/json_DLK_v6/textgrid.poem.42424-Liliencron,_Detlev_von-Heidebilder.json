{"textgrid.poem.42424": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Heidebilder", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tiefeinsamkeit spannt weit die sch\u00f6nen Fl\u00fcgel,", "tokens": ["Tie\u00b7fein\u00b7sam\u00b7keit", "spannt", "weit", "die", "sch\u00f6\u00b7nen", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Weit \u00fcber stille Felder aus.", "tokens": ["Weit", "\u00fc\u00b7ber", "stil\u00b7le", "Fel\u00b7der", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie ferne K\u00fcsten grenzen graue H\u00fcgel,", "tokens": ["Wie", "fer\u00b7ne", "K\u00fcs\u00b7ten", "gren\u00b7zen", "grau\u00b7e", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie sch\u00fctzen vor dem Menschengraus.", "tokens": ["Sie", "sch\u00fct\u00b7zen", "vor", "dem", "Men\u00b7schen\u00b7graus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Im Fr\u00fchling rauscht in mittern\u00e4chtiger Stunde", "tokens": ["Im", "Fr\u00fch\u00b7ling", "rauscht", "in", "mit\u00b7ter\u00b7n\u00e4ch\u00b7ti\u00b7ger", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Wildgans hoch in raschem Flug.", "tokens": ["Die", "Wild\u00b7gans", "hoch", "in", "ra\u00b7schem", "Flug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das alte Gaukelspiel: in weiter Runde", "tokens": ["Das", "al\u00b7te", "Gau\u00b7kel\u00b7spiel", ":", "in", "wei\u00b7ter", "Run\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "H\u00f6r' ich Gesang im Wolkenzug.", "tokens": ["H\u00f6r'", "ich", "Ge\u00b7sang", "im", "Wol\u00b7ken\u00b7zug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Verschlafen sinkt der Mond in schwarze Gr\u00fcnde,", "tokens": ["Ver\u00b7schla\u00b7fen", "sinkt", "der", "Mond", "in", "schwar\u00b7ze", "Gr\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Begl\u00e4nzt noch einmal Schilf und Rohr.", "tokens": ["Be\u00b7gl\u00e4nzt", "noch", "ein\u00b7mal", "Schilf", "und", "Rohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gelangweilt ob so mancher holden S\u00fcnde,", "tokens": ["Ge\u00b7lang\u00b7weilt", "ob", "so", "man\u00b7cher", "hol\u00b7den", "S\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOUS", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verl\u00e4\u00dft er Garten, Wald und Moor.", "tokens": ["Ver\u00b7l\u00e4\u00dft", "er", "Gar\u00b7ten", ",", "Wald", "und", "Moor", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Mittagsonne br\u00fctet auf der Heide,", "tokens": ["Die", "Mit\u00b7ta\u00b7gson\u00b7ne", "br\u00fc\u00b7tet", "auf", "der", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im S\u00fcden droht ein schwarzer Ring.", "tokens": ["Im", "S\u00fc\u00b7den", "droht", "ein", "schwar\u00b7zer", "Ring", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verdurstet h\u00e4ngt das magere Getreide,", "tokens": ["Ver\u00b7durs\u00b7tet", "h\u00e4ngt", "das", "ma\u00b7ge\u00b7re", "Ge\u00b7trei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Behaglich treibt der Schmetterling.", "tokens": ["Be\u00b7hag\u00b7lich", "treibt", "der", "Schmet\u00b7ter\u00b7ling", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ermattet ruhn der Hirt und seine Schafe,", "tokens": ["Er\u00b7mat\u00b7tet", "ruhn", "der", "Hirt", "und", "sei\u00b7ne", "Scha\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Ente tr\u00e4umt im Binsenkraut,", "tokens": ["Die", "En\u00b7te", "tr\u00e4umt", "im", "Bin\u00b7sen\u00b7kraut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Ringelnatter sonnt in tr\u00e4gem Schlafe", "tokens": ["Die", "Rin\u00b7gel\u00b7nat\u00b7ter", "sonnt", "in", "tr\u00e4\u00b7gem", "Schla\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Unregbar ihre Tigerhaut.", "tokens": ["Un\u00b7reg\u00b7bar", "ih\u00b7re", "Ti\u00b7ger\u00b7haut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Im Zickzack zuckt ein Blitz, und Wasserfluten", "tokens": ["Im", "Zick\u00b7zack", "zuckt", "ein", "Blitz", ",", "und", "Was\u00b7ser\u00b7flu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entst\u00fcrzen gierig feuchtem Zelt.", "tokens": ["Ent\u00b7st\u00fcr\u00b7zen", "gie\u00b7rig", "feuch\u00b7tem", "Zelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es jauchzt der Sturm und peitscht mit seinen Ruten", "tokens": ["Es", "jauchzt", "der", "Sturm", "und", "peitscht", "mit", "sei\u00b7nen", "Ru\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erl\u00f6send meine Heidewelt.", "tokens": ["Er\u00b7l\u00f6\u00b7send", "mei\u00b7ne", "Hei\u00b7de\u00b7welt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "In Herbstestagen bricht mit starkem Fl\u00fcgel", "tokens": ["In", "Herbs\u00b7tes\u00b7ta\u00b7gen", "bricht", "mit", "star\u00b7kem", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Reiher durch den Nebelduft.", "tokens": ["Der", "Rei\u00b7her", "durch", "den", "Ne\u00b7bel\u00b7duft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie still es ist, kaum h\u00f6r' ich um den H\u00fcgel", "tokens": ["Wie", "still", "es", "ist", ",", "kaum", "h\u00f6r'", "ich", "um", "den", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch einen Laut in weiter Luft.", "tokens": ["Noch", "ei\u00b7nen", "Laut", "in", "wei\u00b7ter", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Auf eines Birkenst\u00e4mmchens schwanker Krone", "tokens": ["Auf", "ei\u00b7nes", "Bir\u00b7ken\u00b7st\u00e4mm\u00b7chens", "schwan\u00b7ker", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ruht sich ein Wanderfalke aus.", "tokens": ["Ruht", "sich", "ein", "Wan\u00b7der\u00b7fal\u00b7ke", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch schl\u00e4ft er nicht, von seinem leichten Throne", "tokens": ["Doch", "schl\u00e4ft", "er", "nicht", ",", "von", "sei\u00b7nem", "leich\u00b7ten", "Thro\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aeugt er durchdringend scharf hinaus.", "tokens": ["A\u00b7e\u00b7ugt", "er", "durch\u00b7drin\u00b7gend", "scharf", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "+----+-+-+", "measure": "dactylic.init"}}, "stanza.9": {"line.1": {"text": "Der alte Bauer mit verhaltnem Schritte", "tokens": ["Der", "al\u00b7te", "Bau\u00b7er", "mit", "ver\u00b7halt\u00b7nem", "Schrit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schleicht neben seinem Wagen Torf.", "tokens": ["Schleicht", "ne\u00b7ben", "sei\u00b7nem", "Wa\u00b7gen", "Torf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NE", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und holpernd, stolpernd schleppt mit lahmem Tritte", "tokens": ["Und", "hol\u00b7pernd", ",", "stol\u00b7pernd", "schleppt", "mit", "lah\u00b7mem", "Trit\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "ADJD", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der alte Schimmel ihn in's Dorf.", "tokens": ["Der", "al\u00b7te", "Schim\u00b7mel", "ihn", "in's", "Dorf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Die Sonne leiht dem Schnee das Prachtgeschmeide,", "tokens": ["Die", "Son\u00b7ne", "leiht", "dem", "Schnee", "das", "Pracht\u00b7ge\u00b7schmei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch ach! wie kurz ist Schein und Licht.", "tokens": ["Doch", "ach", "!", "wie", "kurz", "ist", "Schein", "und", "Licht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PWAV", "ADJD", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Nebel tropft, und traurig zieht im Leide", "tokens": ["Ein", "Ne\u00b7bel", "tropft", ",", "und", "trau\u00b7rig", "zieht", "im", "Lei\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ADJD", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Landschaft ihren Schleier dicht.", "tokens": ["Die", "Land\u00b7schaft", "ih\u00b7ren", "Schlei\u00b7er", "dicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ein H\u00e4slein nur f\u00fchlt noch des Lebens W\u00e4rme,", "tokens": ["Ein", "H\u00e4s\u00b7lein", "nur", "f\u00fchlt", "noch", "des", "Le\u00b7bens", "W\u00e4r\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Am Weidenstumpfe hockt es bang.", "tokens": ["Am", "Wei\u00b7den\u00b7stump\u00b7fe", "hockt", "es", "bang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch kreischen hungrig schon die Rabenschw\u00e4rme", "tokens": ["Doch", "krei\u00b7schen", "hung\u00b7rig", "schon", "die", "Ra\u00b7ben\u00b7schw\u00e4r\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und hacken auf den sichern Fang.", "tokens": ["Und", "ha\u00b7cken", "auf", "den", "si\u00b7chern", "Fang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Bis auf den schwarzen Schlammgrund sind gefroren", "tokens": ["Bis", "auf", "den", "schwar\u00b7zen", "Schlamm\u00b7grund", "sind", "ge\u00b7fro\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Wasserl\u00f6cher und der See.", "tokens": ["Die", "Was\u00b7ser\u00b7l\u00f6\u00b7cher", "und", "der", "See", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zuweilen geht ein Wimmern, wie verloren,", "tokens": ["Zu\u00b7wei\u00b7len", "geht", "ein", "Wim\u00b7mern", ",", "wie", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PWAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann stirbt im toten Wald ein Reh.", "tokens": ["Dann", "stirbt", "im", "to\u00b7ten", "Wald", "ein", "Reh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Tiefeinsamkeit, es schlingt um deine Pforte", "tokens": ["Tie\u00b7fein\u00b7sam\u00b7keit", ",", "es", "schlingt", "um", "dei\u00b7ne", "Pfor\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Die Erika das rote Band.", "tokens": ["Die", "E\u00b7ri\u00b7ka", "das", "ro\u00b7te", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Menschen leer, was braucht es noch der Worte,", "tokens": ["Von", "Men\u00b7schen", "leer", ",", "was", "braucht", "es", "noch", "der", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "PWS", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sei mir gegr\u00fc\u00dft du stilles Land.", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00dft", "du", "stil\u00b7les", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}