{"textgrid.poem.63960": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "1L: Soviel Ma\u00dflieb, als da prangen,", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.57", "nl:0.42"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Soviel Ma\u00dflieb, als da prangen,", "tokens": ["So\u00b7viel", "Ma\u00df\u00b7lieb", ",", "als", "da", "pran\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOUS", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So viel Donen als gestellt,", "tokens": ["So", "viel", "Do\u00b7nen", "als", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "KOKOM", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Muntere V\u00f6glein, die da sangen,", "tokens": ["Mun\u00b7te\u00b7re", "V\u00f6\u00b7glein", ",", "die", "da", "san\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Gr\u00fcne J\u00e4ger auf dem Feld;", "tokens": ["Gr\u00fc\u00b7ne", "J\u00e4\u00b7ger", "auf", "dem", "Feld", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie dem B\u00e4chlein Wellen rinnen,", "tokens": ["Wie", "dem", "B\u00e4ch\u00b7lein", "Wel\u00b7len", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So viel mal hab' ich mein Sinnen", "tokens": ["So", "viel", "mal", "hab'", "ich", "mein", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Liebste mein, auf dich gestellt.", "tokens": ["Liebs\u00b7te", "mein", ",", "auf", "dich", "ge\u00b7stellt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "$,", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Alle Perlen, die da prangen,", "tokens": ["Al\u00b7le", "Per\u00b7len", ",", "die", "da", "pran\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Zart auf Seide spielen sie,", "tokens": ["Zart", "auf", "Sei\u00b7de", "spie\u00b7len", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NE", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dir um Haupt und Schultern hangen,", "tokens": ["Dir", "um", "Haupt", "und", "Schul\u00b7tern", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, wie bitter find' ich die.", "tokens": ["Ach", ",", "wie", "bit\u00b7ter", "find'", "ich", "die", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADJD", "VVFIN", "PPER", "ART", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deine Locken, die da wehen,", "tokens": ["Dei\u00b7ne", "Lo\u00b7cken", ",", "die", "da", "we\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lose hin im Winde gehen,", "tokens": ["Lo\u00b7se", "hin", "im", "Win\u00b7de", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "K\u00f6nnt' ich halten, halten sie!", "tokens": ["K\u00f6nnt'", "ich", "hal\u00b7ten", ",", "hal\u00b7ten", "sie", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "K\u00f6nnt' ich schau'n in deine Augen,", "tokens": ["K\u00f6nnt'", "ich", "schau'n", "in", "dei\u00b7ne", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine Hand in meiner ruh'n,", "tokens": ["Dei\u00b7ne", "Hand", "in", "mei\u00b7ner", "ruh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, dann wollt' ich alles taugen,", "tokens": ["Ach", ",", "dann", "wollt'", "ich", "al\u00b7les", "tau\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VMFIN", "PPER", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, wie wollt' ich alles tun.", "tokens": ["Ach", ",", "wie", "wollt'", "ich", "al\u00b7les", "tun", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn mein armes Herz wollt' brechen,", "tokens": ["Wenn", "mein", "ar\u00b7mes", "Herz", "wollt'", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "M\u00fc\u00dft' ich alle niederstechen,", "tokens": ["M\u00fc\u00dft'", "ich", "al\u00b7le", "nie\u00b7ders\u00b7te\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Um vor dir erst auszuruh'n.", "tokens": ["Um", "vor", "dir", "erst", "aus\u00b7zu\u00b7ruh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUI", "APPR", "PPER", "ADV", "APPR", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Deine Stimme klang wie Glocken", "tokens": ["Dei\u00b7ne", "Stim\u00b7me", "klang", "wie", "Glo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "KOKOM", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich stand am Kirchentor,", "tokens": ["Und", "ich", "stand", "am", "Kir\u00b7chen\u00b7tor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, wie war mein Herz erschrocken,", "tokens": ["Ach", ",", "wie", "war", "mein", "Herz", "er\u00b7schro\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein Reif dein Schleier fror.", "tokens": ["Wie", "ein", "Reif", "dein", "Schlei\u00b7er", "fror", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie hab' ich dich gr\u00fc\u00dfen k\u00f6nnen,", "tokens": ["Wie", "hab'", "ich", "dich", "gr\u00fc\u00b7\u00dfen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "VVINF", "VMINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die mir doch kein Mensch mag g\u00f6nnen,", "tokens": ["Die", "mir", "doch", "kein", "Mensch", "mag", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "O, warum sind wir nicht gleich!", "tokens": ["O", ",", "wa\u00b7rum", "sind", "wir", "nicht", "gleich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VAFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und ich liege nun im Tauen,", "tokens": ["Und", "ich", "lie\u00b7ge", "nun", "im", "Tau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat mein Herz mich wach gemacht,", "tokens": ["Hat", "mein", "Herz", "mich", "wach", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bald wird schon der Morgen grauen,", "tokens": ["Bald", "wird", "schon", "der", "Mor\u00b7gen", "grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Fremde Sterne sinken sacht,", "tokens": ["Frem\u00b7de", "Ster\u00b7ne", "sin\u00b7ken", "sacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Neben mir greif' ich in Saiten,", "tokens": ["Ne\u00b7ben", "mir", "greif'", "ich", "in", "Sai\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Wend' das Aug' an Himmelsweiten,", "tokens": ["Wend'", "das", "Aug'", "an", "Him\u00b7mels\u00b7wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ich sag' dir gute Nacht.", "tokens": ["Und", "ich", "sag'", "dir", "gu\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.6": {"line.1": {"text": "Gute Nacht, wo du auch ruhest", "tokens": ["Gu\u00b7te", "Nacht", ",", "wo", "du", "auch", "ru\u00b7hest"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PWAV", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem hohen Sternenschein.", "tokens": ["In", "dem", "ho\u00b7hen", "Ster\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gute Nacht, was du auch tuest,", "tokens": ["Gu\u00b7te", "Nacht", ",", "was", "du", "auch", "tu\u00b7est", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gute Nacht dir ganz allein.", "tokens": ["Gu\u00b7te", "Nacht", "dir", "ganz", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fcr dein Leben, f\u00fcr dein Sterben", "tokens": ["F\u00fcr", "dein", "Le\u00b7ben", ",", "f\u00fcr", "dein", "Ster\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Will ich einsam nun verderben,", "tokens": ["Will", "ich", "ein\u00b7sam", "nun", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Will mich betten hart auf Stein.", "tokens": ["Will", "mich", "bet\u00b7ten", "hart", "auf", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "O, mein Herz schl\u00e4gt dicht zusammen,", "tokens": ["O", ",", "mein", "Herz", "schl\u00e4gt", "dicht", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So der Odem mich verl\u00e4\u00dft,", "tokens": ["So", "der", "O\u00b7dem", "mich", "ver\u00b7l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mein Haupt schmerzt wild von Flammen,", "tokens": ["Und", "mein", "Haupt", "schmerzt", "wild", "von", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Brust ist mir so fest.", "tokens": ["Und", "die", "Brust", "ist", "mir", "so", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "So gefesselt mu\u00df ich gehen", "tokens": ["So", "ge\u00b7fes\u00b7selt", "mu\u00df", "ich", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die Lieb' im Haupt mir stehen", "tokens": ["Und", "die", "Lieb'", "im", "Haupt", "mir", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "PPER", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Und das Leid ins Herz gepre\u00dft.", "tokens": ["Und", "das", "Leid", "ins", "Herz", "ge\u00b7pre\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}