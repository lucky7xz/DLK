{"textgrid.poem.45646": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Stammbuch", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin alt und du bist jung,", "tokens": ["Ich", "bin", "alt", "und", "du", "bist", "jung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "PPER", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein Denkbuch mu\u00df sich darum eilen,", "tokens": ["Dein", "Denk\u00b7buch", "mu\u00df", "sich", "da\u00b7rum", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PRF", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sonst rei\u00dft mich fort der Zeiten Schwung.", "tokens": ["Sonst", "rei\u00dft", "mich", "fort", "der", "Zei\u00b7ten", "Schwung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch kann ich noch Empfindung teilen,", "tokens": ["Doch", "kann", "ich", "noch", "Emp\u00b7fin\u00b7dung", "tei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und liesest einst du diese Zeilen,", "tokens": ["Und", "lie\u00b7sest", "einst", "du", "die\u00b7se", "Zei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wach auf dir die Erinnerung.", "tokens": ["Wach", "auf", "dir", "die", "E\u00b7rin\u00b7ne\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}}}}