{"textgrid.poem.55132": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Ganymed", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie im Morgenglanze", "tokens": ["Wie", "im", "Mor\u00b7gen\u00b7glan\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Du rings mich angl\u00fchst,", "tokens": ["Du", "rings", "mich", "an\u00b7gl\u00fchst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Fr\u00fchling, Geliebter!", "tokens": ["Fr\u00fch\u00b7ling", ",", "Ge\u00b7lieb\u00b7ter", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mit tausendfacher Liebeswonne", "tokens": ["Mit", "tau\u00b7send\u00b7fa\u00b7cher", "Lie\u00b7bes\u00b7won\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sich an mein Herz dr\u00e4ngt", "tokens": ["Sich", "an", "mein", "Herz", "dr\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Deiner ewigen W\u00e4rme Heilig Gef\u00fchl,", "tokens": ["Dei\u00b7ner", "e\u00b7wi\u00b7gen", "W\u00e4r\u00b7me", "Hei\u00b7lig", "Ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Unendliche Sch\u00f6ne!", "tokens": ["Un\u00b7end\u00b7li\u00b7che", "Sch\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Da\u00df ich dich fassen m\u00f6cht", "tokens": ["Da\u00df", "ich", "dich", "fas\u00b7sen", "m\u00f6cht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In diesen Arm!", "tokens": ["In", "die\u00b7sen", "Arm", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ach, an deinem Busen", "tokens": ["Ach", ",", "an", "dei\u00b7nem", "Bu\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Lieg ich, schmachte,", "tokens": ["Lieg", "ich", ",", "schmach\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$,", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und deine Blumen, dein Gras", "tokens": ["Und", "dei\u00b7ne", "Blu\u00b7men", ",", "dein", "Gras"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Dr\u00e4ngen sich an mein Herz.", "tokens": ["Dr\u00e4n\u00b7gen", "sich", "an", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Du k\u00fchlst den brennenden", "tokens": ["Du", "k\u00fchlst", "den", "bren\u00b7nen\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.6": {"text": "Durst meines Busens,", "tokens": ["Durst", "mei\u00b7nes", "Bu\u00b7sens", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Lieblicher Morgenwind!", "tokens": ["Lieb\u00b7li\u00b7cher", "Mor\u00b7gen\u00b7wind", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Ruft drein die Nachtigall", "tokens": ["Ruft", "drein", "die", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Liebend nach mir aus dem Nebeltal.", "tokens": ["Lie\u00b7bend", "nach", "mir", "aus", "dem", "Ne\u00b7bel\u00b7tal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "APPR", "ART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Ich komm, ich komme!", "tokens": ["Ich", "komm", ",", "ich", "kom\u00b7me", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Wohin? Ach, wohin?", "tokens": ["Wo\u00b7hin", "?", "Ach", ",", "wo\u00b7hin", "?"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "$.", "ITJ", "$,", "PWAV", "$."], "meter": "--+-+", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Hinauf! Hinauf strebt's.", "tokens": ["Hin\u00b7auf", "!", "Hin\u00b7auf", "strebt'", "s."], "token_info": ["word", "punct", "word", "word", "abbreviation"], "pos": ["ADV", "$.", "ADV", "VVFIN", "NE"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Es schweben die Wolken", "tokens": ["Es", "schwe\u00b7ben", "die", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Abw\u00e4rts, die Wolken", "tokens": ["Ab\u00b7w\u00e4rts", ",", "die", "Wol\u00b7ken"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$,", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Neigen sich der sehnenden Liebe.", "tokens": ["Nei\u00b7gen", "sich", "der", "seh\u00b7nen\u00b7den", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.5": {"text": "Mir! Mir!", "tokens": ["Mir", "!", "Mir", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "++", "measure": "spondeus"}, "line.6": {"text": "In euerm Scho\u00dfe", "tokens": ["In", "eu\u00b7erm", "Scho\u00b7\u00dfe"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Aufw\u00e4rts!", "tokens": ["Auf\u00b7w\u00e4rts", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Umfangend umfangen!", "tokens": ["Um\u00b7fan\u00b7gend", "um\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Aufw\u00e4rts an deinen Busen,", "tokens": ["Auf\u00b7w\u00e4rts", "an", "dei\u00b7nen", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.10": {"text": "Alliebender Vater!", "tokens": ["Al\u00b7lie\u00b7ben\u00b7der", "Va\u00b7ter", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}