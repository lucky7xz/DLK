{"textgrid.poem.66340": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Dem blinden Fl\u00f6tenspieler D\u00fclon auf die Reise", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du guter D\u00fclon klage nicht,", "tokens": ["Du", "gu\u00b7ter", "D\u00fc\u00b7lon", "kla\u00b7ge", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Nacht umflort dein Angesicht;", "tokens": ["Da\u00df", "Nacht", "um\u00b7flort", "dein", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hast du nicht tiefes Herzgef\u00fchl?", "tokens": ["Hast", "du", "nicht", "tie\u00b7fes", "Herz\u00b7ge\u00b7f\u00fchl", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht zauberisches Fl\u00f6tenspiel?", "tokens": ["Nicht", "zau\u00b7be\u00b7ri\u00b7sches", "Fl\u00f6\u00b7ten\u00b7spiel", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und dennoch sang er Ilium", "tokens": ["Und", "den\u00b7noch", "sang", "er", "I\u00b7lium"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Und des Odysseus Wanderschaft", "tokens": ["Und", "des", "O\u00b7dys\u00b7seus", "Wan\u00b7der\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit voller Sch\u00f6pfer-Geisteskraft.", "tokens": ["Mit", "vol\u00b7ler", "Sch\u00f6p\u00b7fer\u00b7Geis\u00b7tes\u00b7kraft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Blind sa\u00df der Zeltenbarde da,", "tokens": ["Blind", "sa\u00df", "der", "Zel\u00b7ten\u00b7bar\u00b7de", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sah, was kaum ein Dichter sah.", "tokens": ["Und", "sah", ",", "was", "kaum", "ein", "Dich\u00b7ter", "sah", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den St\u00fcrmen gleich des Ozeans,", "tokens": ["Den", "St\u00fcr\u00b7men", "gleich", "des", "O\u00b7ze\u00b7ans", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erscholl die Harfe ", "tokens": ["Er\u00b7scholl", "die", "Har\u00b7fe"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Das Chaos und die H\u00f6llennacht,", "tokens": ["Das", "Chaos", "und", "die", "H\u00f6l\u00b7len\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und malte, ohne Augenstrahl,", "tokens": ["Und", "mal\u00b7te", ",", "oh\u00b7ne", "Au\u00b7gen\u00b7strahl", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Weiber sch\u00f6nes Ideal.", "tokens": ["Der", "Wei\u00b7ber", "sch\u00f6\u00b7nes", "I\u00b7deal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Dringt in das Reich der Fabel ein;", "tokens": ["Dringt", "in", "das", "Reich", "der", "Fa\u00b7bel", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und seine Gei\u00dfel, k\u00fchn und stark,", "tokens": ["Und", "sei\u00b7ne", "Gei\u00b7\u00dfel", ",", "k\u00fchn", "und", "stark", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Trifft b\u00f6se F\u00fcrsten bis aufs Mark.", "tokens": ["Trifft", "b\u00f6\u00b7se", "F\u00fcrs\u00b7ten", "bis", "aufs", "Mark", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die lichtberaubte ", "tokens": ["Die", "licht\u00b7be\u00b7raub\u00b7te"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Schwingt ihre Saiten so gewi\u00df,", "tokens": ["Schwingt", "ih\u00b7re", "Sai\u00b7ten", "so", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df vor der Macht des Genius", "tokens": ["Da\u00df", "vor", "der", "Macht", "des", "Ge\u00b7nius"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Der H\u00f6rer wonneschauern mu\u00df.", "tokens": ["Der", "H\u00f6\u00b7rer", "won\u00b7ne\u00b7schau\u00b7ern", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Gar gut ist Gott, der uns gemacht:", "tokens": ["Gar", "gut", "ist", "Gott", ",", "der", "uns", "ge\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Deckt er den \u00e4u\u00dfern Blick mit Nacht,", "tokens": ["Deckt", "er", "den", "\u00e4u\u00b7\u00dfern", "Blick", "mit", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So sch\u00e4rft er, zu der Seele Gl\u00fcck,", "tokens": ["So", "sch\u00e4rft", "er", ",", "zu", "der", "See\u00b7le", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit hellerm Strahl den innern Blick.", "tokens": ["Mit", "hel\u00b7lerm", "Strahl", "den", "in\u00b7nern", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Drum, guter ", "tokens": ["Drum", ",", "gu\u00b7ter"], "token_info": ["word", "punct", "word"], "pos": ["PAV", "$,", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Da\u00df Nacht umflort dein Angesicht.", "tokens": ["Da\u00df", "Nacht", "um\u00b7flort", "dein", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gott gab dir tiefres Herzgef\u00fchl,", "tokens": ["Gott", "gab", "dir", "tief\u00b7res", "Herz\u00b7ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Zauber in dein Fl\u00f6tenspiel.", "tokens": ["Und", "Zau\u00b7ber", "in", "dein", "Fl\u00f6\u00b7ten\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "O ", "tokens": ["O"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Einst \u00f6ffnen deine Augen sich,", "tokens": ["Einst", "\u00f6ff\u00b7nen", "dei\u00b7ne", "Au\u00b7gen", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann siehst du Gottes Herrlichkeit,", "tokens": ["Dann", "siehst", "du", "Got\u00b7tes", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und fl\u00f6test ihm aus Dankbarkeit.", "tokens": ["Und", "fl\u00f6\u00b7test", "ihm", "aus", "Dank\u00b7bar\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}