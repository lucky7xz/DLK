{"textgrid.poem.61944": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[holdes Bl\u00fcmlein, Du willst n\u00fctzen]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Holdes Bl\u00fcmlein, Du willst n\u00fctzen,", "tokens": ["Hol\u00b7des", "Bl\u00fcm\u00b7lein", ",", "Du", "willst", "n\u00fct\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der weiten gr\u00fcnen Au?", "tokens": ["Auf", "der", "wei\u00b7ten", "gr\u00fc\u00b7nen", "Au", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh', die Sonne scheint so golden,", "tokens": ["Sieh'", ",", "die", "Son\u00b7ne", "scheint", "so", "gol\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Himmel, er ist blau!", "tokens": ["Und", "der", "Him\u00b7mel", ",", "er", "ist", "blau", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Hohe Pl\u00e4ne, k\u00fchne Pl\u00e4ne", "tokens": ["Ho\u00b7he", "Pl\u00e4\u00b7ne", ",", "k\u00fch\u00b7ne", "Pl\u00e4\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werden Dir das Blut erhitzen,", "tokens": ["Wer\u00b7den", "Dir", "das", "Blut", "er\u00b7hit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Holdes Bl\u00fcmlein, um Dich schau:", "tokens": ["Hol\u00b7des", "Bl\u00fcm\u00b7lein", ",", "um", "Dich", "schau", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KOUI", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Pl\u00e4ne werden meistens grau.", "tokens": ["Pl\u00e4\u00b7ne", "wer\u00b7den", "meis\u00b7tens", "grau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "R\u00f6selein sich tiefer b\u00fcckte,", "tokens": ["R\u00f6\u00b7se\u00b7lein", "sich", "tie\u00b7fer", "b\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was das kleine Herz entz\u00fcckte.", "tokens": ["Was", "das", "klei\u00b7ne", "Herz", "ent\u00b7z\u00fcck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kalter Zweifel will's ihm rauben!", "tokens": ["Kal\u00b7ter", "Zwei\u00b7fel", "will's", "ihm", "rau\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bban das Sch\u00f6ne will ich glauben\u00ab", "tokens": ["\u00bb", "an", "das", "Sch\u00f6\u00b7ne", "will", "ich", "glau\u00b7ben", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u2013 Sprach es \u2013 \u00bbob auch Bl\u00e4ttlein sich entlauben\u00ab", "tokens": ["\u2013", "Sprach", "es", "\u2013", "\u00bb", "ob", "auch", "Bl\u00e4tt\u00b7lein", "sich", "ent\u00b7lau\u00b7ben", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "PPER", "$(", "$(", "KOUS", "ADV", "NN", "PRF", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und dem R\u00f6slein alles gl\u00fcckte! \u2013", "tokens": ["Und", "dem", "R\u00f6s\u00b7lein", "al\u00b7les", "gl\u00fcck\u00b7te", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "PIS", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}