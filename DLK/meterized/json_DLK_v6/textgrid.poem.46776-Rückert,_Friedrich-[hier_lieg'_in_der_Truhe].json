{"textgrid.poem.46776": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[hier lieg' in der Truhe]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier lieg' in der Truhe,", "tokens": ["Hier", "lieg'", "in", "der", "Tru\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Was euch angeh\u00f6rt;", "tokens": ["Was", "euch", "an\u00b7ge\u00b7h\u00f6rt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Heilig sei's und ruhe,", "tokens": ["Hei\u00b7lig", "sei's", "und", "ru\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wie ihr, ungest\u00f6rt.", "tokens": ["Wie", "ihr", ",", "un\u00b7ge\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Kleidchen in der Truhe,", "tokens": ["Kleid\u00b7chen", "in", "der", "Tru\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Leibchen in dem Grab.", "tokens": ["Leib\u00b7chen", "in", "dem", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Eure Kinderschuhe", "tokens": ["Eu\u00b7re", "Kin\u00b7der\u00b7schu\u00b7he"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Tretet ihr nicht ab.", "tokens": ["Tre\u00b7tet", "ihr", "nicht", "ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "T\u00e4glich aus der Truhe", "tokens": ["T\u00e4g\u00b7lich", "aus", "der", "Tru\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Nehm' ich Kleid um Kleid,", "tokens": ["Nehm'", "ich", "Kleid", "um", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df dem Herzen thue", "tokens": ["Da\u00df", "dem", "Her\u00b7zen", "thue"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wohl das sanfte Leid.", "tokens": ["Wohl", "das", "sanf\u00b7te", "Leid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}