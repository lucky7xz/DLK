{"dta.poem.18892": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Blumen- und Zeichenwechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Die W\u00e4chter sind geb\u00e4ndiget", "tokens": ["Die", "W\u00e4ch\u00b7ter", "sind", "ge\u00b7b\u00e4n\u00b7di\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NE", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch s\u00fcsse Liebesthaten;", "tokens": ["Durch", "s\u00fcs\u00b7se", "Lie\u00b7best\u00b7ha\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch wie wir uns verst\u00e4ndiget", "tokens": ["Doch", "wie", "wir", "uns", "ver\u00b7st\u00e4n\u00b7di\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das wollen wir verrathen;", "tokens": ["Das", "wol\u00b7len", "wir", "ver\u00b7ra\u00b7then", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Denn, Liebchen, was uns Gl\u00fcck gebracht", "tokens": ["Denn", ",", "Lieb\u00b7chen", ",", "was", "uns", "Gl\u00fcck", "ge\u00b7bracht"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "NN", "$,", "PRELS", "PPER", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das muss auch andren nutzen,", "tokens": ["Das", "muss", "auch", "an\u00b7dren", "nut\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So wollen wir der Liebesnacht", "tokens": ["So", "wol\u00b7len", "wir", "der", "Lie\u00b7bes\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die d\u00fcstern Lampen putzen.", "tokens": ["Die", "d\u00fcs\u00b7tern", "Lam\u00b7pen", "put\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und wer sodann mit uns erreicht", "tokens": ["Und", "wer", "so\u00b7dann", "mit", "uns", "er\u00b7reicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Das Ohr recht abzufeimen,", "tokens": ["Das", "Ohr", "recht", "ab\u00b7zu\u00b7fei\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Und liebt wie wir, dem wird es leicht", "tokens": ["Und", "liebt", "wie", "wir", ",", "dem", "wird", "es", "leicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "PPER", "$,", "PDS", "VAFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Den rechten Sinn zu reimen.", "tokens": ["Den", "rech\u00b7ten", "Sinn", "zu", "rei\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Ich schickte dir, du schicktest mir,", "tokens": ["Ich", "schick\u00b7te", "dir", ",", "du", "schick\u00b7test", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Es war sogleich verstanden.", "tokens": ["Es", "war", "sog\u00b7leich", "ver\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}