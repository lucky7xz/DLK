{"textgrid.poem.41941": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du reiches Thal' du Zeuge deutscher Macht,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du reiches Thal' du Zeuge deutscher Macht,", "tokens": ["Du", "rei\u00b7ches", "Thal'", "du", "Zeu\u00b7ge", "deut\u00b7scher", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPER", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit stiller Scheu bettet' ich deine Schranken.", "tokens": ["Mit", "stil\u00b7ler", "Scheu", "bet\u00b7tet'", "ich", "dei\u00b7ne", "Schran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hier blitzt' es einst von muthigen Gedanken", "tokens": ["Hier", "blitzt'", "es", "einst", "von", "mut\u00b7hi\u00b7gen", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Und k\u00fchner That in heil'ger Waldesnacht.", "tokens": ["Und", "k\u00fch\u00b7ner", "That", "in", "heil'\u00b7ger", "Wal\u00b7des\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Heil uns! Vergolten ist die Sachsenschlacht!", "tokens": ["Heil", "uns", "!", "Ver\u00b7gol\u00b7ten", "ist", "die", "Sach\u00b7sen\u00b7schlacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Gebrochen ist der stolze Hohn der Franken", "tokens": ["Ge\u00b7bro\u00b7chen", "ist", "der", "stol\u00b7ze", "Hohn", "der", "Fran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch deutsches Schwert. Die alten Eichen sanken;", "tokens": ["Durch", "deut\u00b7sches", "Schwert", ".", "Die", "al\u00b7ten", "Ei\u00b7chen", "san\u00b7ken", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Wie um's Gebirg die grauen Nebel schweben,", "tokens": ["Wie", "um's", "Ge\u00b7birg", "die", "grau\u00b7en", "Ne\u00b7bel", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So stehn sie hoch auf ihren Felsenzinnen", "tokens": ["So", "stehn", "sie", "hoch", "auf", "ih\u00b7ren", "Fel\u00b7sen\u00b7zin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und schaun hinab in's freie deutsche Land.", "tokens": ["Und", "schaun", "hin\u00b7ab", "in's", "frei\u00b7e", "deut\u00b7sche", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Im Thale bl\u00fcht ein wunderbares Leben,", "tokens": ["Im", "Tha\u00b7le", "bl\u00fcht", "ein", "wun\u00b7der\u00b7ba\u00b7res", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es gr\u00fcnt der Wald, die hellen B\u00e4che rinnen,", "tokens": ["Es", "gr\u00fcnt", "der", "Wald", ",", "die", "hel\u00b7len", "B\u00e4\u00b7che", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und fr\u00f6hlich prangt der Herbst im Lenzgewand.", "tokens": ["Und", "fr\u00f6h\u00b7lich", "prangt", "der", "Herbst", "im", "Lenz\u00b7ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}