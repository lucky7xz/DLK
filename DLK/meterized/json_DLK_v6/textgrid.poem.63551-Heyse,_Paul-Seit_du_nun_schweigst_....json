{"textgrid.poem.63551": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Seit du nun schweigst ...", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seit du nun schweigst, sind mir die Dinge stumm.", "tokens": ["Seit", "du", "nun", "schweigst", ",", "sind", "mir", "die", "Din\u00b7ge", "stumm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,", "VAFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit seelenlosen Augen sehn mich an", "tokens": ["Mit", "see\u00b7len\u00b7lo\u00b7sen", "Au\u00b7gen", "sehn", "mich", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die liebsten Menschen. Jedes Heiligtum", "tokens": ["Die", "liebs\u00b7ten", "Men\u00b7schen", ".", "Je\u00b7des", "Hei\u00b7lig\u00b7tum"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Find' ich verschlossen, poch' ich je daran.", "tokens": ["Find'", "ich", "ver\u00b7schlos\u00b7sen", ",", "poch'", "ich", "je", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$,", "VVIMP", "PPER", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gab deine Stimme doch die Melodie", "tokens": ["Gab", "dei\u00b7ne", "Stim\u00b7me", "doch", "die", "Me\u00b7lo\u00b7die"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu meines Lebens Lied. Du warst das Ma\u00df,", "tokens": ["Zu", "mei\u00b7nes", "Le\u00b7bens", "Lied", ".", "Du", "warst", "das", "Ma\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Wert und Unwert meiner Welt verlieh;", "tokens": ["Das", "Wert", "und", "Un\u00b7wert", "mei\u00b7ner", "Welt", "ver\u00b7lieh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In dir geno\u00df ich erst, was ich besa\u00df.", "tokens": ["In", "dir", "ge\u00b7no\u00df", "ich", "erst", ",", "was", "ich", "be\u00b7sa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ADV", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun du mir fehlst, bin ich mir selbst entr\u00fcckt,", "tokens": ["Nun", "du", "mir", "fehlst", ",", "bin", "ich", "mir", "selbst", "ent\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mi\u00dfklang mein Denken, mein Empfinden Streit.", "tokens": ["Mi\u00df\u00b7klang", "mein", "Den\u00b7ken", ",", "mein", "Emp\u00b7fin\u00b7den", "Streit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Sch\u00f6ne spielt mit mir, das Wahre dr\u00fcckt", "tokens": ["Das", "Sch\u00f6\u00b7ne", "spielt", "mit", "mir", ",", "das", "Wah\u00b7re", "dr\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$,", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dies Herz zusammen, das es sonst befreit.", "tokens": ["Dies", "Herz", "zu\u00b7sam\u00b7men", ",", "das", "es", "sonst", "be\u00b7freit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "PTKVZ", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Des Lebens Krone fiel aus meinem Haar,", "tokens": ["Des", "Le\u00b7bens", "Kro\u00b7ne", "fiel", "aus", "mei\u00b7nem", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Jedwede Herrschgewalt ist mir entrungen,", "tokens": ["Jed\u00b7we\u00b7de", "Herrschge\u00b7walt", "ist", "mir", "ent\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und selbst das Lied, das noch mein eigen war,", "tokens": ["Und", "selbst", "das", "Lied", ",", "das", "noch", "mein", "ei\u00b7gen", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PRELS", "ADV", "PPOSAT", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hat mir der Schmerz tyrannisch abgezwungen.", "tokens": ["Hat", "mir", "der", "Schmerz", "ty\u00b7ran\u00b7nisch", "ab\u00b7ge\u00b7zwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}