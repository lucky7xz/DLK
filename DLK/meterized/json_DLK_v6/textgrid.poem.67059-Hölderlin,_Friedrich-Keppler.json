{"textgrid.poem.67059": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Keppler", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unter den Sternen ergehet sich", "tokens": ["Un\u00b7ter", "den", "Ster\u00b7nen", "er\u00b7ge\u00b7het", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PRF"], "meter": "+--+--+--", "measure": "dactylic.tri"}, "line.2": {"text": "Mein Geist, die Gefilde des Uranus", "tokens": ["Mein", "Geist", ",", "die", "Ge\u00b7fil\u00b7de", "des", "U\u00b7ra\u00b7nus"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "\u00dcberhin schwebt er und sinnt; einsam ist", "tokens": ["\u00dc\u00b7berh\u00b7in", "schwebt", "er", "und", "sinnt", ";", "ein\u00b7sam", "ist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "$.", "ADJD", "VAFIN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Und gewagt, ehernen Tritt heischet die Bahn.", "tokens": ["Und", "ge\u00b7wagt", ",", "e\u00b7her\u00b7nen", "Tritt", "hei\u00b7schet", "die", "Bahn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "$,", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Wandle mit Kraft, wie der Held, einher!", "tokens": ["Wand\u00b7le", "mit", "Kraft", ",", "wie", "der", "Held", ",", "ein\u00b7her", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "PWAV", "ART", "NN", "$,", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Erhebe die Miene! doch nicht zu stolz,", "tokens": ["Er\u00b7he\u00b7be", "die", "Mie\u00b7ne", "!", "doch", "nicht", "zu", "stolz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "ADV", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Denn es naht, siehe es naht, hoch herab", "tokens": ["Denn", "es", "naht", ",", "sie\u00b7he", "es", "naht", ",", "hoch", "her\u00b7ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "VVIMP", "PPER", "VVFIN", "$,", "ADJD", "ADV"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vom Gefild, wo der Triumph jubelt, der Mann,", "tokens": ["Vom", "Ge\u00b7fild", ",", "wo", "der", "Tri\u00b7umph", "ju\u00b7belt", ",", "der", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Welcher den Denker in Albion,", "tokens": ["Wel\u00b7cher", "den", "Den\u00b7ker", "in", "Al\u00b7bi\u00b7on", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ART", "NN", "APPR", "NE", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Den Sp\u00e4her des Himmels um Mitternacht,", "tokens": ["Den", "Sp\u00e4\u00b7her", "des", "Him\u00b7mels", "um", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ins Gefild tiefern Beschauns leitete,", "tokens": ["Ins", "Ge\u00b7fild", "tie\u00b7fern", "Be\u00b7schauns", "lei\u00b7te\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-++-+-+-+", "measure": "zehnsilber"}, "line.4": {"text": "Und voran leuchtend sich wagt' ins Labyrinth,", "tokens": ["Und", "vo\u00b7ran", "leuch\u00b7tend", "sich", "wagt'", "ins", "La\u00b7by\u00b7rinth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "PRF", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Da\u00df der erhabenen Themse Stolz,", "tokens": ["Da\u00df", "der", "er\u00b7ha\u00b7be\u00b7nen", "Them\u00b7se", "Stolz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Geiste sich beugend vor seinem Grab,", "tokens": ["Im", "Geis\u00b7te", "sich", "beu\u00b7gend", "vor", "sei\u00b7nem", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ins Gefild w\u00fcrdigern Lohns nach ihm rief:", "tokens": ["Ins", "Ge\u00b7fild", "w\u00fcr\u00b7di\u00b7gern", "Lohns", "nach", "ihm", "rief", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NE", "APPR", "PPER", "VVFIN", "$."], "meter": "--+---+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "\u00bbdu begannst, Suevias Sohn! wo es dem Blick", "tokens": ["\u00bb", "du", "be\u00b7gannst", ",", "Sue\u00b7vi\u00b7as", "Sohn", "!", "wo", "es", "dem", "Blick"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "$,", "NE", "NN", "$.", "PWAV", "PPER", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.5": {"line.1": {"text": "Aller Jahrtausende schwindelte;", "tokens": ["Al\u00b7ler", "Jahr\u00b7tau\u00b7sen\u00b7de", "schwin\u00b7del\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Und ha! ich vollende, was du begannst,", "tokens": ["Und", "ha", "!", "ich", "voll\u00b7en\u00b7de", ",", "was", "du", "be\u00b7gannst", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$.", "PPER", "ADJA", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Denn voran leuchtetest du, Herrlicher!", "tokens": ["Denn", "vo\u00b7ran", "leuch\u00b7te\u00b7test", "du", ",", "Herr\u00b7li\u00b7cher", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Im Labyrinth, Strahlen beschwurst du in die Nacht.", "tokens": ["Im", "La\u00b7by\u00b7rinth", ",", "Strah\u00b7len", "be\u00b7schwurst", "du", "in", "die", "Nacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "M\u00f6ge verzehren des Lebens Mark", "tokens": ["M\u00f6\u00b7ge", "ver\u00b7zeh\u00b7ren", "des", "Le\u00b7bens", "Mark"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "ART", "NN", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Die Flamm in der Brust \u2013 ich ereile dich,", "tokens": ["Die", "Flamm", "in", "der", "Brust", "\u2013", "ich", "er\u00b7ei\u00b7le", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$(", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ich vollend's! denn sie ist gro\u00df, ernst und gro\u00df,", "tokens": ["Ich", "vollen\u00b7d's", "!", "denn", "sie", "ist", "gro\u00df", ",", "ernst", "und", "gro\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$.", "KON", "PPER", "VAFIN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Deine Bahn, h\u00f6hnet des Golds, lohnet sich selbst.\u00ab", "tokens": ["Dei\u00b7ne", "Bahn", ",", "h\u00f6h\u00b7net", "des", "Golds", ",", "loh\u00b7net", "sich", "selbst", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "$,", "VVFIN", "PRF", "ADV", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.7": {"line.1": {"text": "Wonne Walhallas! und ihn gebar", "tokens": ["Won\u00b7ne", "Wal\u00b7hal\u00b7las", "!", "und", "ihn", "ge\u00b7bar"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$.", "KON", "PPER", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Mein Vaterland? ihn, den die Themse pries?", "tokens": ["Mein", "Va\u00b7ter\u00b7land", "?", "ihn", ",", "den", "die", "Them\u00b7se", "pries", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der zuerst ins Labyrinth Strahlen schuf,", "tokens": ["Der", "zu\u00b7erst", "ins", "La\u00b7by\u00b7rinth", "Strah\u00b7len", "schuf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und den Pfad, hin an dem Pol, wies dem Gestirn.", "tokens": ["Und", "den", "Pfad", ",", "hin", "an", "dem", "Pol", ",", "wies", "dem", "Ge\u00b7stirn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.8": {"line.1": {"text": "Heklas Gedonner verg\u00e4\u00df ich so,", "tokens": ["He\u00b7klas", "Ge\u00b7don\u00b7ner", "ver\u00b7g\u00e4\u00df", "ich", "so", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Und, ging' ich auf Ottern, ich bebte nicht", "tokens": ["Und", ",", "ging'", "ich", "auf", "Ot\u00b7tern", ",", "ich", "beb\u00b7te", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "VVFIN", "PPER", "APPR", "NN", "$,", "PPER", "VVFIN", "PTKNEG"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "In dem Stolz, da\u00df er aus dir, Suevia!", "tokens": ["In", "dem", "Stolz", ",", "da\u00df", "er", "aus", "dir", ",", "Sue\u00b7via", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "PPER", "APPR", "PPER", "$,", "NE", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sich erhub, unser der Dank Albions ist.", "tokens": ["Sich", "er\u00b7hub", ",", "un\u00b7ser", "der", "Dank", "Al\u00b7bions", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "PPOSAT", "ART", "NN", "NE", "VAFIN", "$."], "meter": "+-+---+-+-", "measure": "unknown.measure.tetra"}}, "stanza.9": {"line.1": {"text": "Mutter der Redlichen! Suevia!", "tokens": ["Mut\u00b7ter", "der", "Red\u00b7li\u00b7chen", "!", "Sue\u00b7via", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "NE", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Du stille! dir jauchzen Aeonen zu,", "tokens": ["Du", "stil\u00b7le", "!", "dir", "jauch\u00b7zen", "A\u00b7e\u00b7o\u00b7nen", "zu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Du erzogst M\u00e4nner des Lichts ohne Zahl,", "tokens": ["Du", "er\u00b7zogst", "M\u00e4n\u00b7ner", "des", "Lichts", "oh\u00b7ne", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Des Geschlechts Mund, das da kommt, huldiget dir.", "tokens": ["Des", "Ge\u00b7schlechts", "Mund", ",", "das", "da", "kommt", ",", "hul\u00b7di\u00b7get", "dir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "VVFIN", "PPER", "$."], "meter": "--++--+-+-+", "measure": "iambic.penta.relaxed"}}}}}