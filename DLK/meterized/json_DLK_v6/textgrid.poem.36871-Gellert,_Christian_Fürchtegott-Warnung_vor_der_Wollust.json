{"textgrid.poem.36871": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Warnung vor der Wollust", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Wollust Reiz zu widerstreben,", "tokens": ["Der", "Wol\u00b7lust", "Reiz", "zu", "wi\u00b7der\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dies, Jugend, liebst du Gl\u00fcck und Leben,", "tokens": ["Dies", ",", "Ju\u00b7gend", ",", "liebst", "du", "Gl\u00fcck", "und", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "NN", "$,", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df t\u00e4glich deine Weisheit sein.", "tokens": ["La\u00df", "t\u00e4g\u00b7lich", "dei\u00b7ne", "Weis\u00b7heit", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entflieh der schmeichelnden Begierde;", "tokens": ["Ent\u00b7flieh", "der", "schmei\u00b7cheln\u00b7den", "Be\u00b7gier\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie raubet dir des Herzens Zierde,", "tokens": ["Sie", "rau\u00b7bet", "dir", "des", "Her\u00b7zens", "Zier\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ihre Freuden werden Pein.", "tokens": ["Und", "ih\u00b7re", "Freu\u00b7den", "wer\u00b7den", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df, ihr die Nahrung zu verwehren,", "tokens": ["La\u00df", ",", "ihr", "die", "Nah\u00b7rung", "zu", "ver\u00b7weh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nie Speis und Trank dein Herz beschweren,", "tokens": ["Nie", "Speis", "und", "Trank", "dein", "Herz", "be\u00b7schwe\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und sei ein Freund der N\u00fcchternheit.", "tokens": ["Und", "sei", "ein", "Freund", "der", "N\u00fcch\u00b7tern\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Versage dir, dich zu besiegen,", "tokens": ["Ver\u00b7sa\u00b7ge", "dir", ",", "dich", "zu", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Auch \u00f6fters ein erlaubt Vergn\u00fcgen,", "tokens": ["Auch", "\u00f6f\u00b7ters", "ein", "er\u00b7laubt", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und steure deiner Sinnlichkeit.", "tokens": ["Und", "steu\u00b7re", "dei\u00b7ner", "Sinn\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "La\u00df nicht dein Auge dir gebieten;", "tokens": ["La\u00df", "nicht", "dein", "Au\u00b7ge", "dir", "ge\u00b7bie\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sei, die Wollust zu verh\u00fcten,", "tokens": ["Und", "sei", ",", "die", "Wol\u00b7lust", "zu", "ver\u00b7h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Stets schamhaft gegen deinen Leib.", "tokens": ["Stets", "scham\u00b7haft", "ge\u00b7gen", "dei\u00b7nen", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entflieh des Witzlings freien Scherzen,", "tokens": ["Ent\u00b7flieh", "des", "Witz\u00b7lings", "frei\u00b7en", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und such im Umgang edler Herzen", "tokens": ["Und", "such", "im", "Um\u00b7gang", "ed\u00b7ler", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dir Beispiel, Witz und Zeitvertreib.", "tokens": ["Dir", "Bei\u00b7spiel", ",", "Witz", "und", "Zeit\u00b7ver\u00b7treib", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Mensch, zu Flei\u00df und Arbeit tr\u00e4ge,", "tokens": ["Der", "Mensch", ",", "zu", "Flei\u00df", "und", "Ar\u00b7beit", "tr\u00e4\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00e4llt auf des M\u00fc\u00dfigganges Wege", "tokens": ["F\u00e4llt", "auf", "des", "M\u00fc\u00b7\u00dfig\u00b7gan\u00b7ges", "We\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Leicht in das Netz des B\u00f6sewichts.", "tokens": ["Leicht", "in", "das", "Netz", "des", "B\u00f6\u00b7se\u00b7wichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Unschuld Schutzwehr sind Gesch\u00e4fte.", "tokens": ["Der", "Un\u00b7schuld", "Schutz\u00b7wehr", "sind", "Ge\u00b7sch\u00e4f\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Entzieh der Wollust ihre Kr\u00e4fte", "tokens": ["Ent\u00b7zieh", "der", "Wol\u00b7lust", "ih\u00b7re", "Kr\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im Schwei\u00dfe deines Angesichts.", "tokens": ["Im", "Schwei\u00b7\u00dfe", "dei\u00b7nes", "An\u00b7ge\u00b7sichts", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Erwacht ihr Trieb, dich zu bek\u00e4mpfen;", "tokens": ["Er\u00b7wacht", "ihr", "Trieb", ",", "dich", "zu", "be\u00b7k\u00e4mp\u00b7fen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So wach auch du, ihn fr\u00fch zu d\u00e4mpfen,", "tokens": ["So", "wach", "auch", "du", ",", "ihn", "fr\u00fch", "zu", "d\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "$,", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Eh er die Freiheit dir verwehrt.", "tokens": ["Eh", "er", "die", "Frei\u00b7heit", "dir", "ver\u00b7wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ihn bald in der Geburt ersticken,", "tokens": ["Ihn", "bald", "in", "der", "Ge\u00b7burt", "er\u00b7sti\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ist leicht; schwer ist's, ihn unterdr\u00fccken,", "tokens": ["Ist", "leicht", ";", "schwer", "ist's", ",", "ihn", "un\u00b7ter\u00b7dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$.", "ADJD", "VAFIN", "$,", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn ihn dein Herz zuvor gen\u00e4hrt.", "tokens": ["Wenn", "ihn", "dein", "Herz", "zu\u00b7vor", "ge\u00b7n\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Oft kleiden sich des Lasters Triebe", "tokens": ["Oft", "klei\u00b7den", "sich", "des", "Las\u00b7ters", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In die Gestalt erlaubter Liebe,", "tokens": ["In", "die", "Ge\u00b7stalt", "er\u00b7laub\u00b7ter", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und du erblickst nicht die Gefahr.", "tokens": ["Und", "du", "er\u00b7blickst", "nicht", "die", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein langer Umgang macht dich freier;", "tokens": ["Ein", "lan\u00b7ger", "Um\u00b7gang", "macht", "dich", "frei\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und oft wird ein verbotnes Feuer", "tokens": ["Und", "oft", "wird", "ein", "ver\u00b7bot\u00b7nes", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Aus dem, was anfangs Freundschaft war.", "tokens": ["Aus", "dem", ",", "was", "an\u00b7fangs", "Freund\u00b7schaft", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Dein f\u00fchlend Herz wird sich's verzeihen;", "tokens": ["Dein", "f\u00fch\u00b7lend", "Herz", "wird", "sich's", "ver\u00b7zei\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "CARD", "NN", "VAFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es wird des Lasters Ausbruch scheuen,", "tokens": ["Es", "wird", "des", "Las\u00b7ters", "Aus\u00b7bruch", "scheu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Indem es seinen Trieb ern\u00e4hrt.", "tokens": ["In\u00b7dem", "es", "sei\u00b7nen", "Trieb", "er\u00b7n\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du wirst dich stark und sicher glauben,", "tokens": ["Du", "wirst", "dich", "stark", "und", "si\u00b7cher", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und kleine Fehler dir erlauben,", "tokens": ["Und", "klei\u00b7ne", "Feh\u00b7ler", "dir", "er\u00b7lau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bis deine Tugend sich entehrt.", "tokens": ["Bis", "dei\u00b7ne", "Tu\u00b7gend", "sich", "ent\u00b7ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch nein, du sollst sie nicht entehren,", "tokens": ["Doch", "nein", ",", "du", "sollst", "sie", "nicht", "en\u00b7teh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du sollst dir stets die Tat verwehren;", "tokens": ["Du", "sollst", "dir", "stets", "die", "Tat", "ver\u00b7weh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist drum dein Herz schon tugendhaft?", "tokens": ["Ist", "drum", "dein", "Herz", "schon", "tu\u00b7gend\u00b7haft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist's S\u00fcnde nur, die Tat vollbringen?", "tokens": ["Ist's", "S\u00fcn\u00b7de", "nur", ",", "die", "Tat", "voll\u00b7brin\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sollst du nicht auch den Trieb bezwingen,", "tokens": ["Sollst", "du", "nicht", "auch", "den", "Trieb", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht auch den Wunsch der Leidenschaft?", "tokens": ["Nicht", "auch", "den", "Wunsch", "der", "Lei\u00b7den\u00b7schaft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Begierden sind es, die uns sch\u00e4nden;", "tokens": ["Be\u00b7gier\u00b7den", "sind", "es", ",", "die", "uns", "sch\u00e4n\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ohne da\u00df wir sie vollenden,", "tokens": ["Und", "oh\u00b7ne", "da\u00df", "wir", "sie", "voll\u00b7en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "KOUS", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verletzen wir schon unsre Pflicht.", "tokens": ["Ver\u00b7let\u00b7zen", "wir", "schon", "uns\u00b7re", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn du vor ihnen nicht err\u00f6test,", "tokens": ["Wenn", "du", "vor", "ih\u00b7nen", "nicht", "er\u00b7r\u00f6\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht durch den Geist die L\u00fcste t\u00f6test:", "tokens": ["Nicht", "durch", "den", "Geist", "die", "L\u00fcs\u00b7te", "t\u00f6\u00b7test", ":"], "token_info": ["word", "word", "word", "word", "word", "