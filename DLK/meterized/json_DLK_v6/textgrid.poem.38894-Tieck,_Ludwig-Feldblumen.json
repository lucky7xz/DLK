{"textgrid.poem.38894": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Feldblumen", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du gehst vor\u00fcber,", "tokens": ["Du", "gehst", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "O Lieber!", "tokens": ["O", "Lie\u00b7ber", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "ADJD", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und siehst nicht,", "tokens": ["Und", "siehst", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,"], "meter": "++-", "measure": "unknown.measure.di"}, "line.4": {"text": "F\u00fchlst nicht,", "tokens": ["F\u00fchlst", "nicht", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Wie sch\u00f6n das gr\u00fcne Gras,", "tokens": ["Wie", "sch\u00f6n", "das", "gr\u00fc\u00b7ne", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wie erfrischend und k\u00fchl und na\u00df,", "tokens": ["Wie", "er\u00b7fri\u00b7schend", "und", "k\u00fchl", "und", "na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Und dazwischen die goldnen Sterne;", "tokens": ["Und", "da\u00b7zwi\u00b7schen", "die", "gold\u00b7nen", "Ster\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mu\u00dft du denn stets nach der Ferne?", "tokens": ["Mu\u00dft", "du", "denn", "stets", "nach", "der", "Fer\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}