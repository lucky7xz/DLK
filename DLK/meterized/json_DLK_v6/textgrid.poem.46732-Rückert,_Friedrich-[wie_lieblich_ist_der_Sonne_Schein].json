{"textgrid.poem.46732": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wie lieblich ist der Sonne Schein]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie lieblich ist der Sonne Schein,", "tokens": ["Wie", "lieb\u00b7lich", "ist", "der", "Son\u00b7ne", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie lau der L\u00fcfte Flut!", "tokens": ["Wie", "lau", "der", "L\u00fcf\u00b7te", "Flut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie k\u00f6nnt' ein Herz nun fr\u00f6hlich sein,", "tokens": ["Wie", "k\u00f6nnt'", "ein", "Herz", "nun", "fr\u00f6h\u00b7lich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das h\u00e4tte Freudenmuth.", "tokens": ["Das", "h\u00e4t\u00b7te", "Freu\u00b7den\u00b7muth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mein Herz, und h\u00e4tt' es Freudenmuth,", "tokens": ["Mein", "Herz", ",", "und", "h\u00e4tt'", "es", "Freu\u00b7den\u00b7muth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So sollt' es fr\u00f6hlich sein;", "tokens": ["So", "sollt'", "es", "fr\u00f6h\u00b7lich", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es dringt der Sonne Liebesglut", "tokens": ["Es", "dringt", "der", "Son\u00b7ne", "Lie\u00b7bes\u00b7glut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So schmeichelnd auf mich ein.", "tokens": ["So", "schmei\u00b7chelnd", "auf", "mich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du dringst auf mich vergebens ein", "tokens": ["Du", "dringst", "auf", "mich", "ver\u00b7ge\u00b7bens", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "PPER", "ADV", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit deiner Liebesglut;", "tokens": ["Mit", "dei\u00b7ner", "Lie\u00b7bes\u00b7glut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich soll und darf nicht fr\u00f6hlich sein,", "tokens": ["Ich", "soll", "und", "darf", "nicht", "fr\u00f6h\u00b7lich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So leid es auch mir thut.", "tokens": ["So", "leid", "es", "auch", "mir", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich kann, so leid es auch mir thut,", "tokens": ["Ich", "kann", ",", "so", "leid", "es", "auch", "mir", "thut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "ADV", "ADJD", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und mag nicht fr\u00f6hlich sein;", "tokens": ["Und", "mag", "nicht", "fr\u00f6h\u00b7lich", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Sonne, wenn du's meinest gut,", "tokens": ["O", "Son\u00b7ne", ",", "wenn", "du's", "mei\u00b7nest", "gut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "PIS", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So lade mich nicht ein!", "tokens": ["So", "la\u00b7de", "mich", "nicht", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O Sonne, lade mich nicht ein,", "tokens": ["O", "Son\u00b7ne", ",", "la\u00b7de", "mich", "nicht", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du meinest es so gut,", "tokens": ["Du", "mei\u00b7nest", "es", "so", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Allein du weckest nur die Pein,", "tokens": ["Al\u00b7lein", "du", "we\u00b7ckest", "nur", "die", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die mir im Herzen ruht.", "tokens": ["Die", "mir", "im", "Her\u00b7zen", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Pein, die mir im Herzen ruht,", "tokens": ["Die", "Pein", ",", "die", "mir", "im", "Her\u00b7zen", "ruht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erweckst du diese Pein,", "tokens": ["Er\u00b7weckst", "du", "die\u00b7se", "Pein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So wird kein andres frohes Blut", "tokens": ["So", "wird", "kein", "and\u00b7res", "fro\u00b7hes", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An dir heut' fr\u00f6hlich sein.", "tokens": ["An", "dir", "heut'", "fr\u00f6h\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Doch la\u00df mit dir heut fr\u00f6hlich sein", "tokens": ["Doch", "la\u00df", "mit", "dir", "heut", "fr\u00f6h\u00b7lich", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "APPR", "PPER", "ADV", "ADJD", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur jedes frohe Blut,", "tokens": ["Nur", "je\u00b7des", "fro\u00b7he", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mich im Dunkeln la\u00df allein", "tokens": ["Und", "mich", "im", "Dun\u00b7keln", "la\u00df", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ausbr\u00fcten diese Brut.", "tokens": ["Aus\u00b7br\u00fc\u00b7ten", "die\u00b7se", "Brut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ausbr\u00fcten diese Schmerzensbrut", "tokens": ["Aus\u00b7br\u00fc\u00b7ten", "die\u00b7se", "Schmer\u00b7zens\u00b7brut"], "token_info": ["word", "word", "word"], "pos": ["NN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Schmerzen ganz allein,", "tokens": ["In", "Schmer\u00b7zen", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir hilft dazu nicht laue Flut", "tokens": ["Mir", "hilft", "da\u00b7zu", "nicht", "lau\u00b7e", "Flut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "PTKNEG", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Luft und Sonnenschein.", "tokens": ["Von", "Luft", "und", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}