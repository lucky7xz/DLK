{"textgrid.poem.66329": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Der ewige Jude", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus einem finstern Gekl\u00fcfte Karmels", "tokens": ["Aus", "ei\u00b7nem", "fins\u00b7tern", "Ge\u00b7kl\u00fcf\u00b7te", "Kar\u00b7mels"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NE"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kroch Ahasver. Bald sind's zweitausend Jahre,", "tokens": ["Kroch", "A\u00b7has\u00b7ver", ".", "Bald", "sin\u00b7d's", "zweit\u00b7au\u00b7send", "Jah\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "ADV", "VAFIN", "CARD", "NN", "$,"], "meter": "+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Seit Unruh' ihn durch alle Lande peitschte.", "tokens": ["Seit", "Un\u00b7ruh'", "ihn", "durch", "al\u00b7le", "Lan\u00b7de", "peitschte", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als Jesus einst die Last des Kreuzes trug,", "tokens": ["Als", "Je\u00b7sus", "einst", "die", "Last", "des", "Kreu\u00b7zes", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und rasten wollt' vor Ahasveros Th\u00fcr';", "tokens": ["Und", "ras\u00b7ten", "wollt'", "vor", "A\u00b7has\u00b7ve\u00b7ros", "Th\u00fcr'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ach! da versagt' ihm Ahasver die Rast,", "tokens": ["Ach", "!", "da", "ver\u00b7sagt'", "ihm", "A\u00b7has\u00b7ver", "die", "Rast", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und stie\u00df den Mittler trotzig von der Th\u00fcr:", "tokens": ["Und", "stie\u00df", "den", "Mitt\u00b7ler", "trot\u00b7zig", "von", "der", "Th\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und Jesus schwankt' und sank mit seiner Last.", "tokens": ["Und", "Je\u00b7sus", "schwankt'", "und", "sank", "mit", "sei\u00b7ner", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Doch er verstummt. Ein Todesengel trat", "tokens": ["Doch", "er", "ver\u00b7stummt", ".", "Ein", "To\u00b7de\u00b7sen\u00b7gel", "trat"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Vor Ahasveros hin und sprach im Grimme:", "tokens": ["Vor", "A\u00b7has\u00b7ve\u00b7ros", "hin", "und", "sprach", "im", "Grim\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "\u00bbdie Ruh' hast du dem Menschensohn versagt;", "tokens": ["\u00bb", "die", "Ruh'", "hast", "du", "dem", "Men\u00b7schen\u00b7sohn", "ver\u00b7sagt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Auch dir sei sie, Unmenschlicher! versagt,", "tokens": ["Auch", "dir", "sei", "sie", ",", "Un\u00b7menschli\u00b7cher", "!", "ver\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PPER", "$,", "NN", "$.", "VVPP", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.13": {"text": "Bis da\u00df er k\u00f6mmt!\u00ab", "tokens": ["Bis", "da\u00df", "er", "k\u00f6mmt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "KOUS", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Ein schwarzer h\u00f6llentflohner", "tokens": ["Ein", "schwar\u00b7zer", "h\u00f6l\u00b7lent\u00b7floh\u00b7ner"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "D\u00e4mon gei\u00dfelt nun dich, Ahasver,", "tokens": ["D\u00e4\u00b7mon", "gei\u00b7\u00dfelt", "nun", "dich", ",", "A\u00b7has\u00b7ver", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "PPER", "$,", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.16": {"text": "Von Land zu Land. Des Sterbens s\u00fc\u00dfer Trost,", "tokens": ["Von", "Land", "zu", "Land", ".", "Des", "Ster\u00b7bens", "s\u00fc\u00b7\u00dfer", "Trost", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$.", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Der Grabesruhe Trost ist dir versagt!", "tokens": ["Der", "Gra\u00b7bes\u00b7ru\u00b7he", "Trost", "ist", "dir", "ver\u00b7sagt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Aus einem finsteren Gekl\u00fcfte Karmels", "tokens": ["Aus", "ei\u00b7nem", "fins\u00b7te\u00b7ren", "Ge\u00b7kl\u00fcf\u00b7te", "Kar\u00b7mels"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Trat Ahasver. Er sch\u00fcttelte den Staub", "tokens": ["Trat", "A\u00b7has\u00b7ver", ".", "Er", "sch\u00fct\u00b7tel\u00b7te", "den", "Staub"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Aus seinem Barte, nahm der aufgeth\u00fcrmten", "tokens": ["Aus", "sei\u00b7nem", "Bar\u00b7te", ",", "nahm", "der", "auf\u00b7ge\u00b7th\u00fcrm\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Todtensch\u00e4del einen, schleudert' ihn", "tokens": ["Tod\u00b7ten\u00b7sch\u00e4\u00b7del", "ei\u00b7nen", ",", "schleu\u00b7dert'", "ihn"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "$,", "VVFIN", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Hinab vom Karmel, da\u00df er h\u00fcpft' und scholl", "tokens": ["Hin\u00b7ab", "vom", "Kar\u00b7mel", ",", "da\u00df", "er", "h\u00fcpft'", "und", "scholl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "KOUS", "PPER", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und splitterte. \u00bbDer war mein Vater!\u00ab br\u00fcllte", "tokens": ["Und", "split\u00b7ter\u00b7te", ".", "\u00bb", "Der", "war", "mein", "Va\u00b7ter", "!", "\u00ab", "br\u00fcll\u00b7te"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["KON", "VVFIN", "$.", "$(", "PDS", "VAFIN", "PPOSAT", "NN", "$.", "$(", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ahasveros. Noch ein Sch\u00e4del! Ha,", "tokens": ["A\u00b7has\u00b7ve\u00b7ros", ".", "Noch", "ein", "Sch\u00e4\u00b7del", "!", "Ha", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "ADV", "ART", "NN", "$.", "ITJ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Noch sieben Sch\u00e4del polterten hinab", "tokens": ["Noch", "sie\u00b7ben", "Sch\u00e4\u00b7del", "pol\u00b7ter\u00b7ten", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "CARD", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Von Fels zu Fels! \u00bbUnd die \u2013 und die\u00ab mit stierem,", "tokens": ["Von", "Fels", "zu", "Fels", "!", "\u00bb", "Und", "die", "\u2013", "und", "die", "\u00ab", "mit", "stie\u00b7rem", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NE", "$.", "$(", "KON", "ART", "$(", "KON", "ART", "$(", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Vorgequollnem Auge rast's der Jude:", "tokens": ["Vor\u00b7ge\u00b7quoll\u00b7nem", "Au\u00b7ge", "rast's", "der", "Ju\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "\u00bbund die \u2013 und die \u2013 sind meine Weiber \u2013 Ha!\u00ab", "tokens": ["\u00bb", "und", "die", "\u2013", "und", "die", "\u2013", "sind", "mei\u00b7ne", "Wei\u00b7ber", "\u2013", "Ha", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "KON", "ART", "$(", "KON", "ART", "$(", "VAFIN", "PPOSAT", "NN", "$(", "ITJ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Noch immer rollten Sch\u00e4del. \u00bbDie und die,\u00ab", "tokens": ["Noch", "im\u00b7mer", "roll\u00b7ten", "Sch\u00e4\u00b7del", ".", "\u00bb", "Die", "und", "die", ",", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$.", "$(", "ART", "KON", "ART", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Br\u00fcllt' Ahasver, \u00bbsind meine Kinder, ha!", "tokens": ["Br\u00fcllt'", "A\u00b7has\u00b7ver", ",", "\u00bb", "sind", "mei\u00b7ne", "Kin\u00b7der", ",", "ha", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$,", "$(", "VAFIN", "PPOSAT", "NN", "$,", "ITJ", "$."], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Ich kann nicht sterben! Ach, das furchtbarste Gericht", "tokens": ["Ich", "kann", "nicht", "ster\u00b7ben", "!", "Ach", ",", "das", "furcht\u00b7bars\u00b7te", "Ge\u00b7richt"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$.", "ITJ", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.15": {"text": "H\u00e4ngt schreckenbr\u00fcllend ewig \u00fcber mir.", "tokens": ["H\u00e4ngt", "schre\u00b7cken\u00b7br\u00fcl\u00b7lend", "e\u00b7wig", "\u00fc\u00b7ber", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Jerusalem sank. Ich knirschte den S\u00e4ugling,", "tokens": ["Je\u00b7ru\u00b7sa\u00b7lem", "sank", ".", "Ich", "knirschte", "den", "S\u00e4ug\u00b7ling", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ich rannt' in die Flamme. Ich fluchte dem R\u00f6mer;", "tokens": ["Ich", "rannt'", "in", "die", "Flam\u00b7me", ".", "Ich", "fluch\u00b7te", "dem", "R\u00f6\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Doch, ach! doch, ach! der rastlose Fluch", "tokens": ["Doch", ",", "ach", "!", "doch", ",", "ach", "!", "der", "rast\u00b7lo\u00b7se", "Fluch"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "ITJ", "$.", "ADV", "$,", "ITJ", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Hielt mich am Haar, und ich starb nicht.", "tokens": ["Hielt", "mich", "am", "Haar", ",", "und", "ich", "starb", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "KON", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Roma, die Riesin, st\u00fcrzte in Tr\u00fcmmer;", "tokens": ["Ro\u00b7ma", ",", "die", "Rie\u00b7sin", ",", "st\u00fcrz\u00b7te", "in", "Tr\u00fcm\u00b7mer", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich stellte mich unter die st\u00fcrzende Riesin,", "tokens": ["Ich", "stell\u00b7te", "mich", "un\u00b7ter", "die", "st\u00fcr\u00b7zen\u00b7de", "Rie\u00b7sin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+---+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Doch sie fiel und zermalmte mich nicht.", "tokens": ["Doch", "sie", "fiel", "und", "zer\u00b7malm\u00b7te", "mich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Nationen entstanden und sanken vor mir;", "tokens": ["Na\u00b7ti\u00b7o\u00b7nen", "ent\u00b7stan\u00b7den", "und", "san\u00b7ken", "vor", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "VVFIN", "APPR", "PPER", "$."], "meter": "--+--+--+-++", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Ich aber blieb, und starb nicht!", "tokens": ["Ich", "a\u00b7ber", "blieb", ",", "und", "starb", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von wolkengeg\u00fcrteten Klippen st\u00fcrzt' ich", "tokens": ["Von", "wol\u00b7ken\u00b7ge\u00b7g\u00fcr\u00b7te\u00b7ten", "Klip\u00b7pen", "st\u00fcrzt'", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Hinunter ins Meer; doch strudelnde Wellen", "tokens": ["Hin\u00b7un\u00b7ter", "ins", "Meer", ";", "doch", "stru\u00b7deln\u00b7de", "Wel\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$.", "ADV", "ADJA", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "W\u00e4lzten mich ans Ufer, und des Seins", "tokens": ["W\u00e4lz\u00b7ten", "mich", "ans", "U\u00b7fer", ",", "und", "des", "Seins"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "KON", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Flammenpfeil durchstach mich wieder.", "tokens": ["Flam\u00b7men\u00b7pfeil", "durch\u00b7stach", "mich", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Hinab sah ich in Aetnas grausen Schlund,", "tokens": ["Hin\u00b7ab", "sah", "ich", "in", "A\u00b7et\u00b7nas", "grau\u00b7sen", "Schlund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Und w\u00fcthete hinab in seinen Schlund:", "tokens": ["Und", "w\u00fct\u00b7he\u00b7te", "hin\u00b7ab", "in", "sei\u00b7nen", "Schlund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Da br\