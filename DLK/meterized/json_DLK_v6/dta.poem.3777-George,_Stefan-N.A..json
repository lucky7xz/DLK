{"dta.poem.3777": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1892", "urn": "urn:nbn:de:kobv:b4-200905191524", "language": ["de:0.99"], "booktitle": "George, Stefan: Algabal. Paris u. a., 1892."}, "poem": {"stanza.1": {"line.1": {"text": "Wie mich so ernster worte folge st\u00f6rt:", "tokens": ["Wie", "mich", "so", "erns\u00b7ter", "wor\u00b7te", "fol\u00b7ge", "st\u00f6rt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJA", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dein tadel weil mein geist nicht dir geh\u00f6rt", "tokens": ["Dein", "ta\u00b7del", "weil", "mein", "geist", "nicht", "dir", "ge\u00b7h\u00f6rt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KOUS", "PPOSAT", "NN", "PTKNEG", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dass ich ihn achtlos ohne that verhauchte", "tokens": ["Dass", "ich", "ihn", "acht\u00b7los", "oh\u00b7ne", "that", "ver\u00b7hauch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "APPR", "VVFIN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gedenkt es dir wie viele speere pfiffen", "tokens": ["Ge\u00b7denkt", "es", "dir", "wie", "vie\u00b7le", "spee\u00b7re", "pfif\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "KOKOM", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als ich im Osten um die krone rang", "tokens": ["Als", "ich", "im", "Os\u00b7ten", "um", "die", "kro\u00b7ne", "rang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und lob und vorwurf dem Verwegnen klang", "tokens": ["Und", "lob", "und", "vor\u00b7wurf", "dem", "Ver\u00b7weg\u00b7nen", "klang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "VVFIN", "ART", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Der damals noch die erde nicht begriffen?", "tokens": ["Der", "da\u00b7mals", "noch", "die", "er\u00b7de", "nicht", "be\u00b7grif\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nicht ohnmacht r\u00e4t mir ab von eurem handeln", "tokens": ["Nicht", "ohn\u00b7macht", "r\u00e4t", "mir", "ab", "von", "eu\u00b7rem", "han\u00b7deln"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "VVFIN", "PPER", "PTKVZ", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ich habe euren handels wahn erfasst", "tokens": ["Ich", "ha\u00b7be", "eu\u00b7ren", "han\u00b7dels", "wahn", "er\u00b7fasst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O lass mich unger\u00fchmt und ungehasst", "tokens": ["O", "lass", "mich", "un\u00b7ge\u00b7r\u00fchmt", "und", "un\u00b7ge\u00b7hasst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und frei in den bedingten bahnen wandeln", "tokens": ["Und", "frei", "in", "den", "be\u00b7ding\u00b7ten", "bah\u00b7nen", "wan\u00b7deln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wolle nicht den bruder mir entfremden", "tokens": ["Und", "wol\u00b7le", "nicht", "den", "bru\u00b7der", "mir", "ent\u00b7frem\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PTKNEG", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u2014 Erkannt ich doch im schlaf dein augenmerk? \u2014", "tokens": ["Er\u00b7kannt", "ich", "doch", "im", "schlaf", "dein", "au\u00b7gen\u00b7merk", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "APPRART", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Du fesselst eifrig ihn an bl\u00f6des werk", "tokens": ["Du", "fes\u00b7selst", "eif\u00b7rig", "ihn", "an", "bl\u00f6\u00b7des", "werk"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dein zwang verkleidet ihn mit sklavenhemden", "tokens": ["Dein", "zwang", "ver\u00b7klei\u00b7det", "ihn", "mit", "skla\u00b7ven\u00b7hem\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sieh ich bin zart wie eine apfelbl\u00fcte", "tokens": ["Sieh", "ich", "bin", "zart", "wie", "ei\u00b7ne", "ap\u00b7fel\u00b7bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "VAFIN", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und friedenfroher denn ein neues lamm", "tokens": ["Und", "frie\u00b7den\u00b7fro\u00b7her", "denn", "ein", "neu\u00b7es", "lamm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch liegen eisen stein und feuerschwamm", "tokens": ["Doch", "lie\u00b7gen", "ei\u00b7sen", "stein", "und", "feu\u00b7er\u00b7schwamm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "VVPP", "VAINF", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gef\u00e4hrlich in ersch\u00fcttertem gem\u00fcte", "tokens": ["Ge\u00b7f\u00e4hr\u00b7lich", "in", "er\u00b7sch\u00fct\u00b7ter\u00b7tem", "ge\u00b7m\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Hernieder steig ich eine marmortreppe", "tokens": ["Her\u00b7nie\u00b7der", "steig", "ich", "ei\u00b7ne", "mar\u00b7mor\u00b7trep\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Ein leichnam ohne haupt inmitten ruht", "tokens": ["Ein", "leich\u00b7nam", "oh\u00b7ne", "haupt", "in\u00b7mit\u00b7ten", "ruht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dort sickert meines teuren bruders blut", "tokens": ["Dort", "si\u00b7ckert", "mei\u00b7nes", "teu\u00b7ren", "bru\u00b7ders", "blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich raffe leise nur die purpurschleppe", "tokens": ["Ich", "raf\u00b7fe", "lei\u00b7se", "nur", "die", "pur\u00b7pur\u00b7schlep\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}