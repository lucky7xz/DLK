{"dta.poem.20663": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  den kleinen von K.  \n \u00fcber  \n die Landkarte von Persien, Griechenland  \n und  \n ganz Asien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Wilhelm! Strecke nicht die zarte                 ", "tokens": ["Mein", "Wil\u00b7helm", "!", "Stre\u00b7cke", "nicht", "die", "zar\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NE", "$.", "NN", "PTKNEG", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die kaum gebaute Kinderhand", "tokens": ["Die", "kaum", "ge\u00b7bau\u00b7te", "Kin\u00b7der\u00b7hand"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach dieser kunstbezognen Karte,", "tokens": ["Nach", "die\u00b7ser", "kunst\u00b7be\u00b7zog\u00b7nen", "Kar\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du kennest weder See noch Land.", "tokens": ["Du", "ken\u00b7nest", "we\u00b7der", "See", "noch", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn sieben Sommer sind entwichen", "tokens": ["Wenn", "sie\u00b7ben", "Som\u00b7mer", "sind", "ent\u00b7wi\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "CARD", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Dein Verstand begreifen kann,", "tokens": ["Und", "Dein", "Ver\u00b7stand", "be\u00b7grei\u00b7fen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da staunst Du bey den feinen Strichen", "tokens": ["Da", "staunst", "Du", "bey", "den", "fei\u00b7nen", "Stri\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Alexanders Feldzug an.", "tokens": ["Des", "A\u00b7lex\u00b7an\u00b7ders", "Feld\u00b7zug", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dir zeiget Deiner Mutter Finger", "tokens": ["Dir", "zei\u00b7get", "Dei\u00b7ner", "Mut\u00b7ter", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Viel K\u00f6nigreiche, die der Gang", "tokens": ["Viel", "K\u00f6\u00b7nig\u00b7rei\u00b7che", ",", "die", "der", "Gang"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Anblick von dem St\u00e4dtezwinger", "tokens": ["Und", "An\u00b7blick", "von", "dem", "St\u00e4d\u00b7te\u00b7zwin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schnell unter seine F\u00fc\u00dfe zwang.", "tokens": ["Schnell", "un\u00b7ter", "sei\u00b7ne", "F\u00fc\u00b7\u00dfe", "zwang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du siehst den Hellespont und h\u00f6rest", "tokens": ["Du", "siehst", "den", "Hel\u00b7les\u00b7pont", "und", "h\u00f6\u00b7rest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Xerxes stolzen Uebermuth,", "tokens": ["Von", "Xe\u00b7rxes", "stol\u00b7zen", "Ue\u00b7ber\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du siehst Thermopyl\u00e4 und ehrest", "tokens": ["Du", "siehst", "Ther\u00b7mo\u00b7py\u00b7l\u00e4", "und", "eh\u00b7rest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Griechen edle Heldenwuth.", "tokens": ["Der", "Grie\u00b7chen", "ed\u00b7le", "Hel\u00b7den\u00b7wuth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Das Vaterland der alten Dichter,", "tokens": ["Das", "Va\u00b7ter\u00b7land", "der", "al\u00b7ten", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die schon Jahrtausende gegl\u00e4nzt", "tokens": ["Die", "schon", "Jahr\u00b7tau\u00b7sen\u00b7de", "ge\u00b7gl\u00e4nzt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie um den Mond die Sternenlichter,", "tokens": ["Wie", "um", "den", "Mond", "die", "Ster\u00b7nen\u00b7lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von der Unsterblichkeit gekr\u00e4nzt.", "tokens": ["Von", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", "ge\u00b7kr\u00e4nzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Athen und Lesbos siehst Du Knabe,", "tokens": ["A\u00b7then", "und", "Les\u00b7bos", "siehst", "Du", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "VVFIN", "PPER", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und Theben und Laced\u00e4mon,", "tokens": ["Und", "The\u00b7ben", "und", "La\u00b7ce\u00b7d\u00e4\u00b7mon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und fragst nach des Homerus Grabe,", "tokens": ["Und", "fragst", "nach", "des", "Ho\u00b7me\u00b7rus", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NE", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und fragst: Wo schl\u00e4ft Anacreon?", "tokens": ["Und", "fragst", ":", "Wo", "schl\u00e4ft", "A\u00b7na\u00b7cre\u00b7on", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PWAV", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wo Sophoeles? Achill? und jener", "tokens": ["Wo", "So\u00b7phoe\u00b7les", "?", "A\u00b7chill", "?", "und", "je\u00b7ner"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWAV", "NE", "$.", "NE", "$.", "KON", "PDS"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ber\u00fchmte Pindar? und Socrat,", "tokens": ["Be\u00b7r\u00fchm\u00b7te", "Pin\u00b7dar", "?", "und", "So\u00b7crat", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Philosoph, der als ein sch\u00f6ner", "tokens": ["Der", "Phi\u00b7lo\u00b7soph", ",", "der", "als", "ein", "sch\u00f6\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "KOUS", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erhabner Geist im Tode that?", "tokens": ["Er\u00b7hab\u00b7ner", "Geist", "im", "To\u00b7de", "that", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wo schlummern Helden? wo die Weisen,", "tokens": ["Wo", "schlum\u00b7mern", "Hel\u00b7den", "?", "wo", "die", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$.", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von welchen die Geschichte sagt,", "tokens": ["Von", "wel\u00b7chen", "die", "Ge\u00b7schich\u00b7te", "sagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach welchen auf entfernten Reisen", "tokens": ["Nach", "wel\u00b7chen", "auf", "ent\u00b7fern\u00b7ten", "Rei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des J\u00fcnglings Neubegierde fragt?", "tokens": ["Des", "J\u00fcng\u00b7lings", "Neu\u00b7be\u00b7gier\u00b7de", "fragt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "O beste Mutter! alle diese", "tokens": ["O", "bes\u00b7te", "Mut\u00b7ter", "!", "al\u00b7le", "die\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJA", "NN", "$.", "PIAT", "PDS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ber\u00fchmte M\u00e4nner sind dahin,", "tokens": ["Be\u00b7r\u00fchm\u00b7te", "M\u00e4n\u00b7ner", "sind", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Herbstgras von gem\u00e4hter Wiese;", "tokens": ["Wie", "Herbst\u00b7gras", "von", "ge\u00b7m\u00e4h\u00b7ter", "Wie\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nichts schont des Schicksals Eigensinn.", "tokens": ["Nichts", "schont", "des", "Schick\u00b7sals", "Ei\u00b7gen\u00b7sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Nichts blieb zur\u00fcck als ihre Tugend", "tokens": ["Nichts", "blieb", "zu\u00b7r\u00fcck", "als", "ih\u00b7re", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PTKVZ", "KOKOM", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ihres gro\u00dfen Geistes Ruhm \u2014", "tokens": ["Und", "ih\u00b7res", "gro\u00b7\u00dfen", "Geis\u00b7tes", "Ruhm"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geliebte! Treib Du meine Jugend", "tokens": ["Ge\u00b7lieb\u00b7te", "!", "Treib", "Du", "mei\u00b7ne", "Ju\u00b7gend"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "NN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stets in der Weisheit Heiligthum!", "tokens": ["Stets", "in", "der", "Weis\u00b7heit", "Hei\u00b7lig\u00b7thum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}