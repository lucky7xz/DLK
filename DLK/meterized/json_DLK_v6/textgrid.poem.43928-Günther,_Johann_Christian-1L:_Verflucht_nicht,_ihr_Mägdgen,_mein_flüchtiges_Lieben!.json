{"textgrid.poem.43928": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Verflucht nicht, ihr M\u00e4gdgen, mein fl\u00fcchtiges Lieben!", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verflucht nicht, ihr M\u00e4gdgen, mein fl\u00fcchtiges Lieben!", "tokens": ["Ver\u00b7flucht", "nicht", ",", "ihr", "M\u00e4gd\u00b7gen", ",", "mein", "fl\u00fcch\u00b7ti\u00b7ges", "Lie\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Jugend, ihr wist's wohl, hat Feuer und Muth;", "tokens": ["Die", "Ju\u00b7gend", ",", "ihr", "wist's", "wohl", ",", "hat", "Feu\u00b7er", "und", "Muth", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "ADV", "$,", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Es kauft ja ein jeder am liebsten frisch Gut,", "tokens": ["Es", "kauft", "ja", "ein", "je\u00b7der", "am", "liebs\u00b7ten", "frisch", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "PIAT", "PTKA", "ADJD", "ADJD", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Drum las ich mich niemahls den Vorwurf betr\u00fcben,", "tokens": ["Drum", "las", "ich", "mich", "nie\u00b7mahls", "den", "Vor\u00b7wurf", "be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Ich w\u00e4re von Flandern und striche herum;", "tokens": ["Ich", "w\u00e4\u00b7re", "von", "Flan\u00b7dern", "und", "stri\u00b7che", "he\u00b7rum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Das thu ich und dencke: Wer schiert sich was drum!", "tokens": ["Das", "thu", "ich", "und", "den\u00b7cke", ":", "Wer", "schiert", "sich", "was", "drum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "KON", "VVFIN", "$.", "PWS", "VVFIN", "PRF", "PWS", "PAV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Wir sind doch nicht alle vor eine gebohren", "tokens": ["Wir", "sind", "doch", "nicht", "al\u00b7le", "vor", "ei\u00b7ne", "ge\u00b7boh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "PIS", "APPR", "ART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und haben nicht alle solch Fleisch und solch Bein,", "tokens": ["Und", "ha\u00b7ben", "nicht", "al\u00b7le", "solch", "Fleisch", "und", "solch", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "PIAT", "PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Der Ersten, der Besten best\u00e4ndig zu seyn.", "tokens": ["Der", "Ers\u00b7ten", ",", "der", "Bes\u00b7ten", "be\u00b7st\u00e4n\u00b7dig", "zu", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Der Lobspruch der Treue verf\u00fchrt nur die Thoren;", "tokens": ["Der", "Lob\u00b7spruch", "der", "Treu\u00b7e", "ver\u00b7f\u00fchrt", "nur", "die", "Tho\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Was schadet's der Liebsten, die unser begehrt,", "tokens": ["Was", "scha\u00b7det's", "der", "Liebs\u00b7ten", ",", "die", "un\u00b7ser", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "PRELS", "PPOSAT", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Wenn man gleich zuweilen den Nebenweg f\u00e4hrt?", "tokens": ["Wenn", "man", "gleich", "zu\u00b7wei\u00b7len", "den", "Ne\u00b7ben\u00b7weg", "f\u00e4hrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}, "stanza.3": {"line.1": {"text": "Der Wechsel vergn\u00fcget die menschlichen Sinnen,", "tokens": ["Der", "Wech\u00b7sel", "ver\u00b7gn\u00fc\u00b7get", "die", "menschli\u00b7chen", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Dies lehrt uns der Umgang und auch die Natur;", "tokens": ["Dies", "lehrt", "uns", "der", "Um\u00b7gang", "und", "auch", "die", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "KON", "ADV", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Das Weltlicht ver\u00e4ndert fast st\u00fcndlich die Spur,", "tokens": ["Das", "Welt\u00b7licht", "ver\u00b7\u00e4n\u00b7dert", "fast", "st\u00fcnd\u00b7lich", "die", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und einerley Farbe wird selten gewinnen;", "tokens": ["Und", "ei\u00b7ner\u00b7ley", "Far\u00b7be", "wird", "sel\u00b7ten", "ge\u00b7win\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADJD", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Auch Zucker bringt Eckel durch steten Genu\u00df,", "tokens": ["Auch", "Zu\u00b7cker", "bringt", "E\u00b7ckel", "durch", "ste\u00b7ten", "Ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Und Fleisch alle Tage n\u00e4hrt blos den Verdru\u00df.", "tokens": ["Und", "Fleisch", "al\u00b7le", "Ta\u00b7ge", "n\u00e4hrt", "blos", "den", "Ver\u00b7dru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PIAT", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Im Geigen entz\u00fcckt uns ein k\u00fcnstliches Greifen,", "tokens": ["Im", "Gei\u00b7gen", "ent\u00b7z\u00fcckt", "uns", "ein", "k\u00fcnst\u00b7li\u00b7ches", "Grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Das vielerley Stimmen und Tacte vermengt;", "tokens": ["Das", "vie\u00b7ler\u00b7ley", "Stim\u00b7men", "und", "Tac\u00b7te", "ver\u00b7mengt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Denn da\u00df oft der Bogen die Quinte zersprengt,", "tokens": ["Denn", "da\u00df", "oft", "der", "Bo\u00b7gen", "die", "Quin\u00b7te", "zer\u00b7sprengt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Macht, weil die Finger so lange drauf schleifen;", "tokens": ["Macht", ",", "weil", "die", "Fin\u00b7ger", "so", "lan\u00b7ge", "drauf", "schlei\u00b7fen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "ADV", "ADV", "PAV", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Und da\u00df auch der Ehstand die Liebe verg\u00e4llt,", "tokens": ["Und", "da\u00df", "auch", "der", "Eh\u00b7stand", "die", "Lie\u00b7be", "ver\u00b7g\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Macht, weil er die Freyheit im Kercker beh\u00e4lt.", "tokens": ["Macht", ",", "weil", "er", "die", "Frey\u00b7heit", "im", "Ker\u00b7cker", "be\u00b7h\u00e4lt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+---+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Bedenckt euch, ihr M\u00e4gdgen, was wollt ihr viel sagen?", "tokens": ["Be\u00b7denckt", "euch", ",", "ihr", "M\u00e4gd\u00b7gen", ",", "was", "wollt", "ihr", "viel", "sa\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ihr \u00e4ndert ja j\u00e4hrlich Schmuck, Spizen und Kleid;", "tokens": ["Ihr", "\u00e4n\u00b7dert", "ja", "j\u00e4hr\u00b7lich", "Schmuck", ",", "Spi\u00b7zen", "und", "Kleid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+--+-++--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und wen ihr jezt selber durch Kuppler gefreyt,", "tokens": ["Und", "wen", "ihr", "jezt", "sel\u00b7ber", "durch", "Kupp\u00b7ler", "ge\u00b7freyt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Dem gebt ihr das Jawort im Korbe zu tragen;", "tokens": ["Dem", "gebt", "ihr", "das", "Ja\u00b7wort", "im", "Kor\u00b7be", "zu", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Da\u00df mancher den Proteus nur Fabelwerck nennt,", "tokens": ["Da\u00df", "man\u00b7cher", "den", "Pro\u00b7teus", "nur", "Fa\u00b7bel\u00b7werck", "nennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Macht, weil er die Farben der Sch\u00f6nheit nicht kennt.", "tokens": ["Macht", ",", "weil", "er", "die", "Far\u00b7ben", "der", "Sch\u00f6n\u00b7heit", "nicht", "kennt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Die Eifersucht ha\u00df ich noch \u00e4rger als Schlangen;", "tokens": ["Die", "Ei\u00b7fer\u00b7sucht", "ha\u00df", "ich", "noch", "\u00e4r\u00b7ger", "als", "Schlan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "KOKOM", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und h\u00e4tt ich ein M\u00e4gdgen von englischer Pracht,", "tokens": ["Und", "h\u00e4tt", "ich", "ein", "M\u00e4gd\u00b7gen", "von", "eng\u00b7li\u00b7scher", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und w\u00fcrd ich auch st\u00fcndlich zum Schwager gemacht,", "tokens": ["Und", "w\u00fcrd", "ich", "auch", "st\u00fcnd\u00b7lich", "zum", "Schwa\u00b7ger", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ich wollte sie wohl nicht gerichtlich belangen;", "tokens": ["Ich", "woll\u00b7te", "sie", "wohl", "nicht", "ge\u00b7richt\u00b7lich", "be\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "ADJD", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Ich spr\u00e4che: Mein Engel, ach z\u00fcrne nur nicht!", "tokens": ["Ich", "spr\u00e4\u00b7che", ":", "Mein", "En\u00b7gel", ",", "ach", "z\u00fcr\u00b7ne", "nur", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "NN", "$,", "ITJ", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Dies sind ja die Besten, wo jedermann bricht.", "tokens": ["Dies", "sind", "ja", "die", "Bes\u00b7ten", ",", "wo", "je\u00b7der\u00b7mann", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Nur kommt mir nicht etwan mit albernen Po\u00dfen", "tokens": ["Nur", "kommt", "mir", "nicht", "et\u00b7wan", "mit", "al\u00b7ber\u00b7nen", "Po\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und r\u00fcckt mir die starcken Versprechungen vor!", "tokens": ["Und", "r\u00fcckt", "mir", "die", "star\u00b7cken", "Ver\u00b7spre\u00b7chun\u00b7gen", "vor", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Im Lieben hat warlich die Rache kein Ohr.", "tokens": ["Im", "Lie\u00b7ben", "hat", "war\u00b7lich", "die", "Ra\u00b7che", "kein", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VAFIN", "ADV", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ich schw\u00f6re verbindlich, bis da\u00df ich's geno\u00dfen;", "tokens": ["Ich", "schw\u00f6\u00b7re", "ver\u00b7bind\u00b7lich", ",", "bis", "da\u00df", "ich's", "ge\u00b7no\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "KON", "KOUS", "PIS", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und bin ich dann fertig, so schwenck ich den Hut", "tokens": ["Und", "bin", "ich", "dann", "fer\u00b7tig", ",", "so", "schwenck", "ich", "den", "Hut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und gehe zur andern, die eben das thut.", "tokens": ["Und", "ge\u00b7he", "zur", "an\u00b7dern", ",", "die", "e\u00b7ben", "das", "thut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "$,", "PRELS", "ADV", "PDS", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.8": {"line.1": {"text": "Ich habe, das glaubt nur, ein ziemlich Gewi\u00dfen,", "tokens": ["Ich", "ha\u00b7be", ",", "das", "glaubt", "nur", ",", "ein", "ziem\u00b7lich", "Ge\u00b7wi\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PDS", "VVFIN", "ADV", "$,", "ART", "ADV", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Worein schon mein Scherzen manch Duzend begr\u00e4bt,", "tokens": ["Wo\u00b7rein", "schon", "mein", "Scher\u00b7zen", "manch", "Du\u00b7zend", "be\u00b7gr\u00e4bt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die, wo ich auf Erden gewohnt und gelebt,", "tokens": ["Die", ",", "wo", "ich", "auf", "Er\u00b7den", "ge\u00b7wohnt", "und", "ge\u00b7lebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "PPER", "APPR", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Mein z\u00e4rtliches Leiden befriedigen m\u00fc\u00dfen.", "tokens": ["Mein", "z\u00e4rt\u00b7li\u00b7ches", "Lei\u00b7den", "be\u00b7frie\u00b7di\u00b7gen", "m\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "VMINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Kommt, artige Kinder, kommt heufig heran,", "tokens": ["Kommt", ",", "ar\u00b7ti\u00b7ge", "Kin\u00b7der", ",", "kommt", "heu\u00b7fig", "he\u00b7ran", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Dieweil ich noch manche beherbergen kan.", "tokens": ["Die\u00b7weil", "ich", "noch", "man\u00b7che", "be\u00b7her\u00b7ber\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.9": {"line.1": {"text": "Mein Eigensinn legt sich auch hier auf das Wehlen;", "tokens": ["Mein", "Ei\u00b7gen\u00b7sinn", "legt", "sich", "auch", "hier", "auf", "das", "Weh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Versteht sich ein M\u00e4gdgen auf K\u00fc\u00dfen und Scherz", "tokens": ["Ver\u00b7steht", "sich", "ein", "M\u00e4gd\u00b7gen", "auf", "K\u00fc\u00b7\u00dfen", "und", "Scherz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und hat sie kein geizig noch murrisches Herz,", "tokens": ["Und", "hat", "sie", "kein", "gei\u00b7zig", "noch", "mur\u00b7ri\u00b7sches", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PIAT", "ADJD", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "So wird sie mein Abschlag warhaftig nicht qu\u00e4len;", "tokens": ["So", "wird", "sie", "mein", "Ab\u00b7schlag", "war\u00b7haf\u00b7tig", "nicht", "qu\u00e4\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ADJD", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Ja, will sie es anfangs auch gleich nicht verstehn,", "tokens": ["Ja", ",", "will", "sie", "es", "an\u00b7fangs", "auch", "gleich", "nicht", "ver\u00b7stehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VMFIN", "PPER", "PPER", "ADV", "ADV", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "So tr\u00f6st ich mich immer: Es wird schon noch gehn.", "tokens": ["So", "tr\u00f6st", "ich", "mich", "im\u00b7mer", ":", "Es", "wird", "schon", "noch", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "$.", "PPER", "VAFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.10": {"line.1": {"text": "Bald locken mich schwarze, bald feurige Kohlen,", "tokens": ["Bald", "lo\u00b7cken", "mich", "schwar\u00b7ze", ",", "bald", "feu\u00b7ri\u00b7ge", "Koh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Bald ziehn mich die Blonden, bald reizt mich die Brust;", "tokens": ["Bald", "ziehn", "mich", "die", "Blon\u00b7den", ",", "bald", "reizt", "mich", "die", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die T\u00e4nzerin scheint mir geschickter zur Lust,", "tokens": ["Die", "T\u00e4n\u00b7ze\u00b7rin", "scheint", "mir", "ge\u00b7schick\u00b7ter", "zur", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ich weis auch bey Lahmen mein Gl\u00fccke zu holen.", "tokens": ["Ich", "weis", "auch", "bey", "Lah\u00b7men", "mein", "Gl\u00fc\u00b7cke", "zu", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "ADV", "APPR", "NE", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Klug, th\u00f6richt, frey, furchtsam, starck, lang oder klein,", "tokens": ["Klug", ",", "th\u00f6\u00b7richt", ",", "frey", ",", "furcht\u00b7sam", ",", "starck", ",", "lang", "o\u00b7der", "klein", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Sie seyn, wie sie wollen, ich finde mich drein.", "tokens": ["Sie", "seyn", ",", "wie", "sie", "wol\u00b7len", ",", "ich", "fin\u00b7de", "mich", "drein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAINF", "$,", "PWAV", "PPER", "VMFIN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.11": {"line.1": {"text": "Ich g\u00f6nne ja jedem sein eigen Erg\u00f6zen,", "tokens": ["Ich", "g\u00f6n\u00b7ne", "ja", "je\u00b7dem", "sein", "ei\u00b7gen", "Er\u00b7g\u00f6\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Drum last mich zufrieden und g\u00f6nnt es auch mir.", "tokens": ["Drum", "last", "mich", "zu\u00b7frie\u00b7den", "und", "g\u00f6nnt", "es", "auch", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "PPER", "ADV", "PPER", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Denckt jemand zu lieben, der thu es noch hier,", "tokens": ["Denckt", "je\u00b7mand", "zu", "lie\u00b7ben", ",", "der", "thu", "es", "noch", "hier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKZU", "VVINF", "$,", "PRELS", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Eh Zeit und Verh\u00e4ngn\u00fc\u00df den Scheidebrief sezen.", "tokens": ["Eh", "Zeit", "und", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "den", "Schei\u00b7de\u00b7brief", "se\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-+--", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Die Welt hat nichts S\u00fc\u00dfers als dies, was man liebt,", "tokens": ["Die", "Welt", "hat", "nichts", "S\u00fc\u00b7\u00dfers", "als", "dies", ",", "was", "man", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "NN", "KOKOM", "PDS", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Drum leb ich und liebe, so lang es was giebt.", "tokens": ["Drum", "leb", "ich", "und", "lie\u00b7be", ",", "so", "lang", "es", "was", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "KON", "VVFIN", "$,", "ADV", "ADJD", "PPER", "PIS", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}