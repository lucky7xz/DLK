{"textgrid.poem.37238": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[la\u00df doch das ew'ge Fragen]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df doch das ew'ge Fragen,", "tokens": ["La\u00df", "doch", "das", "ew'\u00b7ge", "Fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verehrter alter Freund.", "tokens": ["Ver\u00b7ehr\u00b7ter", "al\u00b7ter", "Freund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich will von selbst schon sagen,", "tokens": ["Ich", "will", "von", "selbst", "schon", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was mir vonn\u00f6ten scheint.", "tokens": ["Was", "mir", "von\u00b7n\u00f6\u00b7ten", "scheint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du sagst vielleicht dagegen:", "tokens": ["Du", "sagst", "viel\u00b7leicht", "da\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Man fragt doch wohl einmal.", "tokens": ["Man", "fragt", "doch", "wohl", "ein\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gewi\u00df! Nur allerwegen", "tokens": ["Ge\u00b7wi\u00df", "!", "Nur", "al\u00b7ler\u00b7we\u00b7gen"], "token_info": ["word", "punct", "word", "word"], "pos": ["PTKANT", "$.", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist mir's nicht ganz egal.", "tokens": ["Ist", "mir's", "nicht", "ganz", "e\u00b7gal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Bei deinem Fragestellen", "tokens": ["Bei", "dei\u00b7nem", "Fra\u00b7ge\u00b7stel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hat eines mich frappiert:", "tokens": ["Hat", "ei\u00b7nes", "mich", "frap\u00b7piert", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du fragst so gern nach F\u00e4llen,", "tokens": ["Du", "fragst", "so", "gern", "nach", "F\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wobei ich mich blamiert.", "tokens": ["Wo\u00b7bei", "ich", "mich", "bla\u00b7miert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}