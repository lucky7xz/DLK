{"dta.poem.8861": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Xl.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Erfahren hab' ich dies und das, und das und dies er-", "tokens": ["Er\u00b7fah\u00b7ren", "hab'", "ich", "dies", "und", "das", ",", "und", "das", "und", "dies", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "PDS", "KON", "PDS", "$,", "KON", "PDS", "KON", "PDS", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es zog der ungestillte Geist mich wandernd oft im Land", "tokens": ["Es", "zog", "der", "un\u00b7ge\u00b7still\u00b7te", "Geist", "mich", "wan\u00b7dernd", "oft", "im", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Und wieder stille sa\u00df ich dann, und an den B\u00fcchern", "tokens": ["Und", "wie\u00b7der", "stil\u00b7le", "sa\u00df", "ich", "dann", ",", "und", "an", "den", "B\u00fc\u00b7chern"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "VVFIN", "PPER", "ADV", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verglommen ist die Hitze halb, die junge Seelen ganz", "tokens": ["Ver\u00b7glom\u00b7men", "ist", "die", "Hit\u00b7ze", "halb", ",", "die", "jun\u00b7ge", "See\u00b7len", "ganz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ADJD", "$,", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Denn oft verzehrte mich der Ha\u00df, und vor der Liebe", "tokens": ["Denn", "oft", "ver\u00b7zehr\u00b7te", "mich", "der", "Ha\u00df", ",", "und", "vor", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch schien ich mir zu nichts bestimmt, als nur das", "tokens": ["Doch", "schien", "ich", "mir", "zu", "nichts", "be\u00b7stimmt", ",", "als", "nur", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKA", "PIS", "VVPP", "$,", "KOUS", "ADV", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Zu kr\u00f6nen durch erhabnes Lob, und solche Kronen webt'", "tokens": ["Zu", "kr\u00f6\u00b7nen", "durch", "er\u00b7hab\u00b7nes", "Lob", ",", "und", "sol\u00b7che", "Kro\u00b7nen", "webt'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "APPR", "ADJA", "NN", "$,", "KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Was k\u00fcnftig mir beschieden sey, verk\u00fcnde kein Orakel", "tokens": ["Was", "k\u00fcnf\u00b7tig", "mir", "be\u00b7schie\u00b7den", "sey", ",", "ver\u00b7k\u00fcn\u00b7de", "kein", "O\u00b7ra\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ADJD", "PPER", "VVPP", "VAFIN", "$,", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.9": {"text": "Denn dieser Sorg' und Bangigkeit um K\u00fcnftiges ent-", "tokens": ["Denn", "die\u00b7ser", "Sor\u00b7g'", "und", "Ban\u00b7gig\u00b7keit", "um", "K\u00fcnf\u00b7ti\u00b7ges", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "KON", "NN", "APPR", "NN", "TRUNC"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}