{"textgrid.poem.42575": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An die M\u00e4dchen", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr besonders dauret mich,", "tokens": ["Ihr", "be\u00b7son\u00b7ders", "dau\u00b7ret", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Arme M\u00e4dchen, inniglich,", "tokens": ["Ar\u00b7me", "M\u00e4d\u00b7chen", ",", "in\u00b7nig\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ihr just in Zeiten fielet,", "tokens": ["Da\u00df", "ihr", "just", "in", "Zei\u00b7ten", "fie\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo man wenig tanzt und spielet.", "tokens": ["Wo", "man", "we\u00b7nig", "tanzt", "und", "spie\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eine M\u00e4dchenjugend ist", "tokens": ["Ei\u00b7ne", "M\u00e4d\u00b7chen\u00b7ju\u00b7gend", "ist"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Abgebl\u00fcht in kurzer Frist;", "tokens": ["Ab\u00b7ge\u00b7bl\u00fcht", "in", "kur\u00b7zer", "Frist", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00fcsset ihr nun Bl\u00fcte tragen", "tokens": ["M\u00fcs\u00b7set", "ihr", "nun", "Bl\u00fc\u00b7te", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In so rauhen, tr\u00fcben Tagen!", "tokens": ["In", "so", "rau\u00b7hen", ",", "tr\u00fc\u00b7ben", "Ta\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja! mir d\u00fcnket oft so sehr", "tokens": ["Ja", "!", "mir", "d\u00fcn\u00b7ket", "oft", "so", "sehr"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "ADV", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Eure Jugend freudenleer,", "tokens": ["Eu\u00b7re", "Ju\u00b7gend", "freu\u00b7den\u00b7leer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df euch keine Zuflucht bliebe", "tokens": ["Da\u00df", "euch", "kei\u00b7ne", "Zu\u00b7flucht", "blie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als die wahre, fromme Liebe.", "tokens": ["Als", "die", "wah\u00b7re", ",", "from\u00b7me", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}