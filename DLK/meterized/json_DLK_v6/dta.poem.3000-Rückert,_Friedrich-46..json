{"dta.poem.3000": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "46.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als die Erscheinungen dir allererst erschienen,", "tokens": ["Als", "die", "Er\u00b7schei\u00b7nun\u00b7gen", "dir", "al\u00b7le\u00b7rerst", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sahst du sie regellos, und kein Gesetz in ihnen.", "tokens": ["Sahst", "du", "sie", "re\u00b7gel\u00b7los", ",", "und", "kein", "Ge\u00b7setz", "in", "ih\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "$,", "KON", "PIAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mit Freude wurdest du dann ein Gesetz gewahr,", "tokens": ["Mit", "Freu\u00b7de", "wur\u00b7dest", "du", "dann", "ein", "Ge\u00b7setz", "ge\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und unterordnen willst du ihm nun Alles gar.", "tokens": ["Und", "un\u00b7ter\u00b7ord\u00b7nen", "willst", "du", "ihm", "nun", "Al\u00b7les", "gar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PPER", "ADV", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Warum bedenkst du nicht?: da wo du hast entdeckt", "tokens": ["Wa\u00b7rum", "be\u00b7denkst", "du", "nicht", "?", ":", "da", "wo", "du", "hast", "ent\u00b7deckt"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PTKNEG", "$.", "$.", "ADV", "PWAV", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Regeln eine, sind wol andre noch versteckt.", "tokens": ["Der", "Re\u00b7geln", "ei\u00b7ne", ",", "sind", "wol", "and\u00b7re", "noch", "ver\u00b7steckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "$,", "VAFIN", "ADV", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}