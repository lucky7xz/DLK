{"dta.poem.19498": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Firnelicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Wie pocht' das Herz mir in der Brust,", "tokens": ["Wie", "pocht'", "das", "Herz", "mir", "in", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Trotz meiner jungen Wanderlust,", "tokens": ["Trotz", "mei\u00b7ner", "jun\u00b7gen", "Wan\u00b7der\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da, heimgewendet, ich erschaut'", "tokens": ["Da", ",", "heim\u00b7ge\u00b7wen\u00b7det", ",", "ich", "er\u00b7schaut'"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Schneegebirge, s\u00fc\u00df umblaut,", "tokens": ["Die", "Schnee\u00b7ge\u00b7bir\u00b7ge", ",", "s\u00fc\u00df", "um\u00b7blaut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das gro\u00dfe stille Leuchten!", "tokens": ["Das", "gro\u00b7\u00dfe", "stil\u00b7le", "Leuch\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich athmet' eilig, wie auf Raub,", "tokens": ["Ich", "ath\u00b7met'", "ei\u00b7lig", ",", "wie", "auf", "Raub", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der M\u00e4rkte Dunst, der St\u00e4dte Staub.", "tokens": ["Der", "M\u00e4rk\u00b7te", "Dunst", ",", "der", "St\u00e4d\u00b7te", "Staub", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sah den Kampf. Was sagest du,", "tokens": ["Ich", "sah", "den", "Kampf", ".", "Was", "sa\u00b7gest", "du", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "PWS", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein reines Firnelicht, dazu,", "tokens": ["Mein", "rei\u00b7nes", "Fir\u00b7ne\u00b7licht", ",", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du gro\u00dfes stilles Leuchten?", "tokens": ["Du", "gro\u00b7\u00dfes", "stil\u00b7les", "Leuch\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nie prahlt' ich mit der Heimath noch", "tokens": ["Nie", "prahlt'", "ich", "mit", "der", "Hei\u00b7math", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und liebe sie von Herzen doch,", "tokens": ["Und", "lie\u00b7be", "sie", "von", "Her\u00b7zen", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In meinem Wesen und Gedicht", "tokens": ["In", "mei\u00b7nem", "We\u00b7sen", "und", "Ge\u00b7dicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "All\u00fcberall ist Firnelicht,", "tokens": ["Al\u00b7l\u00fc\u00b7be\u00b7rall", "ist", "Fir\u00b7ne\u00b7licht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das gro\u00dfe stille Leuchten.", "tokens": ["Das", "gro\u00b7\u00dfe", "stil\u00b7le", "Leuch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Was kann ich f\u00fcr die Heimath thun,", "tokens": ["Was", "kann", "ich", "f\u00fcr", "die", "Hei\u00b7math", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bevor ich geh' im Grabe ruhn?", "tokens": ["Be\u00b7vor", "ich", "geh'", "im", "Gra\u00b7be", "ruhn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was geb ich, das dem Tod entflieht?", "tokens": ["Was", "geb", "ich", ",", "das", "dem", "Tod", "ent\u00b7flieht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vielleicht ein Wort, vielleicht ein Lied,", "tokens": ["Viel\u00b7leicht", "ein", "Wort", ",", "viel\u00b7leicht", "ein", "Lied", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein kleines stilles Leuchten!", "tokens": ["Ein", "klei\u00b7nes", "stil\u00b7les", "Leuch\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}