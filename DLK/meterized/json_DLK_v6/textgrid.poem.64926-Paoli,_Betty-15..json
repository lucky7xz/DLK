{"textgrid.poem.64926": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zerrei\u00df der Erde Schoo\u00df mit spitzen Eisens Macht,", "tokens": ["Zer\u00b7rei\u00df", "der", "Er\u00b7de", "Schoo\u00df", "mit", "spit\u00b7zen", "Ei\u00b7sens", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du f\u00f6rderst nimmer Blei aus reinem Goldesschacht.", "tokens": ["Du", "f\u00f6r\u00b7derst", "nim\u00b7mer", "Blei", "aus", "rei\u00b7nem", "Gol\u00b7des\u00b7schacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So auch, ob noch so tief vom Hassespfeil gespalten,", "tokens": ["So", "auch", ",", "ob", "noch", "so", "tief", "vom", "Has\u00b7se\u00b7spfeil", "ge\u00b7spal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "ADV", "ADV", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird sich aus edler Brust Unw\u00fcrd'ges nie entfalten.", "tokens": ["Wird", "sich", "aus", "ed\u00b7ler", "Brust", "Un\u00b7w\u00fcrd'\u00b7ges", "nie", "ent\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "APPR", "ADJA", "NN", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexan