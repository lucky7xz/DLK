{"textgrid.poem.54897": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was soll ich mehr noch sehn? nun grimme Pestilentzen/", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was soll ich mehr noch sehn? nun grimme Pestilentzen/", "tokens": ["Was", "soll", "ich", "mehr", "noch", "sehn", "?", "nun", "grim\u00b7me", "Pes\u00b7ti\u00b7lent\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$.", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun bleicher Hunger-Angst verw\u00fcstet deine Gr\u00e4ntzen;", "tokens": ["Nun", "blei\u00b7cher", "Hun\u00b7ger\u00b7Angst", "ver\u00b7w\u00fcs\u00b7tet", "dei\u00b7ne", "Gr\u00e4nt\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nun der Carthaunen Blitz/ nun Hauptmann und Soldat/", "tokens": ["Nun", "der", "Car\u00b7thau\u00b7nen", "Blitz", "/", "nun", "Haupt\u00b7mann", "und", "Sol\u00b7dat", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$(", "ADV", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An unserm Gut und Blut sich statt gefressen hat;", "tokens": ["An", "un\u00b7serm", "Gut", "und", "Blut", "sich", "statt", "ge\u00b7fres\u00b7sen", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "PRF", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zeucht eine Nacht noch auf/ voll tausendfacher Plagen:", "tokens": ["Zeucht", "ei\u00b7ne", "Nacht", "noch", "auf", "/", "voll", "tau\u00b7send\u00b7fa\u00b7cher", "Pla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "APPR", "$(", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Recht eine Nacht voll Nacht/ voll Ach! und Jammer-Klagen:", "tokens": ["Recht", "ei\u00b7ne", "Nacht", "voll", "Nacht", "/", "voll", "Ach", "!", "und", "Jam\u00b7mer\u00b7Kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "NN", "$(", "ADJD", "ITJ", "$.", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und rei\u00dft/ O Freystadt/ was bi\u00dfher noch von dir stund", "tokens": ["Und", "rei\u00dft", "/", "O", "Freys\u00b7tadt", "/", "was", "bi\u00df\u00b7her", "noch", "von", "dir", "stund"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$(", "NE", "NN", "$(", "PWS", "ADV", "ADV", "APPR", "PPER", "VVFIN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Gleich einem Cederbaum mit Ast und Stumpff zu Grund/", "tokens": ["Gleich", "ei\u00b7nem", "Ce\u00b7der\u00b7baum", "mit", "Ast", "und", "Stumpff", "zu", "Grund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "KON", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Eh'r iemand di\u00df vermeint. Die Sonne war gewichen/", "tokens": ["Eh'r", "ie\u00b7mand", "di\u00df", "ver\u00b7meint", ".", "Die", "Son\u00b7ne", "war", "ge\u00b7wi\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PDS", "VVFIN", "$.", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Himmel stund besternt; und ", "tokens": ["Der", "Him\u00b7mel", "stund", "bes\u00b7ternt", ";", "und"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ADJD", "VVPP", "$.", "KON"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Mit seiner Tr\u00e4ume Schaar; der Sorgen Feind die Ruh'/", "tokens": ["Mit", "sei\u00b7ner", "Tr\u00e4u\u00b7me", "Schaar", ";", "der", "Sor\u00b7gen", "Feind", "die", "Ruh'", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$.", "ART", "NN", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Schlo\u00df der nun m\u00fcden Schaar die tr\u00e4gen Augen zu.", "tokens": ["Schlo\u00df", "der", "nun", "m\u00fc\u00b7den", "Schaar", "die", "tr\u00e4\u00b7gen", "Au\u00b7gen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADV", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Als das Geschrey angieng! O was f\u00fcr Donner-Schl\u00e4ge", "tokens": ["Als", "das", "Ge\u00b7schrey", "an\u00b7gieng", "!", "O", "was", "f\u00fcr", "Don\u00b7ner\u00b7Schl\u00e4\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$.", "ITJ", "PWS", "APPR", "NN"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.14": {"text": "Empfind ich noch in mir/ wenn ich den Blick erwege:", "tokens": ["Emp\u00b7find", "ich", "noch", "in", "mir", "/", "wenn", "ich", "den", "Blick", "er\u00b7we\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "PPER", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Den ersten Jammerblick: Die schnelle Lufft ersau\u00dft/", "tokens": ["Den", "ers\u00b7ten", "Jam\u00b7mer\u00b7blick", ":", "Die", "schnel\u00b7le", "Lufft", "er\u00b7sau\u00dft", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der Monden fleucht best\u00fcrtzt/ der Winde w\u00fctten braust/", "tokens": ["Der", "Mon\u00b7den", "fleucht", "be\u00b7st\u00fcrtzt", "/", "der", "Win\u00b7de", "w\u00fct\u00b7ten", "braust", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$(", "ART", "NN", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und Freystadt kracht im brand': Es steigen Dampff und Flammen/", "tokens": ["Und", "Freys\u00b7tadt", "kracht", "im", "brand'", ":", "Es", "stei\u00b7gen", "Dampff", "und", "Flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPRART", "VVFIN", "$.", "PPER", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und Funcken Himmel' an: Dort f\u00e4lt ein Hau\u00df zusammen", "tokens": ["Und", "Fun\u00b7cken", "Him\u00b7mel'", "an", ":", "Dort", "f\u00e4lt", "ein", "Hau\u00df", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "NN", "PTKVZ", "$.", "ADV", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und schl\u00e4gt das ander' ein. Was nicht von diesem schmaucht", "tokens": ["Und", "schl\u00e4gt", "das", "an\u00b7der'", "ein", ".", "Was", "nicht", "von", "die\u00b7sem", "schmaucht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "PIS", "PTKVZ", "$.", "PWS", "PTKNEG", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ist schon Staub/ Asch' und Grau\u00df: Wo jener Hauffen raucht/", "tokens": ["Ist", "schon", "Staub", "/", "Asch'", "und", "Grau\u00df", ":", "Wo", "je\u00b7ner", "Hauf\u00b7fen", "raucht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "$(", "NE", "KON", "NN", "$.", "PWAV", "PDAT", "NN", "VVFIN", "$("], "meter": "+-++-+-+-+-+", "measure": "unknown.measure.septa"}, "line.21": {"text": "War vor der sch\u00f6nste Saal: Wo sind der Th\u00fcrme Spitzen?", "tokens": ["War", "vor", "der", "sch\u00f6ns\u00b7te", "Saal", ":", "Wo", "sind", "der", "Th\u00fcr\u00b7me", "Spit\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "$.", "PWAV", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wo ist das Rathhau\u00df hin? Und wo die Richter sitzen?", "tokens": ["Wo", "ist", "das", "Rath\u00b7hau\u00df", "hin", "?", "Und", "wo", "die", "Rich\u00b7ter", "sit\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "PTKVZ", "$.", "KON", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die Kirchen brasselt auch! soll denn kein Ertzt noch Stein/", "tokens": ["Die", "Kir\u00b7chen", "bras\u00b7selt", "auch", "!", "soll", "denn", "kein", "Ertzt", "noch", "Stein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$.", "VMFIN", "ADV", "PIAT", "NN", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "O Freystadt/ frey an dir von seinem Sterben seyn?", "tokens": ["O", "Freys\u00b7tadt", "/", "frey", "an", "dir", "von", "sei\u00b7nem", "Ster\u00b7ben", "seyn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "ADJD", "APPR", "PPER", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Sch\u00fctzt keiner Mauren Krafft? sind keiner Retter H\u00e4nde?", "tokens": ["Sch\u00fctzt", "kei\u00b7ner", "Mau\u00b7ren", "Krafft", "?", "sind", "kei\u00b7ner", "Ret\u00b7ter", "H\u00e4n\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "NN", "$.", "VAFIN", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ist alles helffen aus/ und gehn die kleinen W\u00e4nde", "tokens": ["Ist", "al\u00b7les", "helf\u00b7fen", "aus", "/", "und", "gehn", "die", "klei\u00b7nen", "W\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "VVFIN", "PTKVZ", "$(", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Zusammt den grossen ein? O ja! di\u00df ist der Schlu\u00df/", "tokens": ["Zu\u00b7sammt", "den", "gros\u00b7sen", "ein", "?", "O", "ja", "!", "di\u00df", "ist", "der", "Schlu\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "PTKVZ", "$.", "ITJ", "ITJ", "$.", "PDS", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der alles/ was noch stund/ zu boden werffen mu\u00df!", "tokens": ["Der", "al\u00b7les", "/", "was", "noch", "stund", "/", "zu", "bo\u00b7den", "werf\u00b7fen", "mu\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$(", "PWS", "ADV", "ADJD", "$(", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "So sinckt ein krancker Leib/ den schon der Todt erkohren/", "tokens": ["So", "sinckt", "ein", "kran\u00b7cker", "Leib", "/", "den", "schon", "der", "Todt", "er\u00b7koh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$(", "ART", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "(der Artzt thu'/ was er kan/ sein bessern ist verlohren.)", "tokens": ["(", "der", "Artzt", "thu'", "/", "was", "er", "kan", "/", "sein", "bes\u00b7sern", "ist", "ver\u00b7loh\u00b7ren", ".", ")"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "$(", "PWS", "PPER", "VMFIN", "$(", "PPOSAT", "ADJA", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So wird die grosse Welt/ auf angesetzte Zeit", "tokens": ["So", "wird", "die", "gros\u00b7se", "Welt", "/", "auf", "an\u00b7ge\u00b7setz\u00b7te", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$(", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Durch schweffellichte Glut des Donners abgemayt/", "tokens": ["Durch", "schwef\u00b7fel\u00b7lich\u00b7te", "Glut", "des", "Don\u00b7ners", "ab\u00b7ge\u00b7mayt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Verlodern und vergehn! was seh' ich dort f\u00fcr Hauffen?", "tokens": ["Ver\u00b7lo\u00b7dern", "und", "ver\u00b7gehn", "!", "was", "seh'", "ich", "dort", "f\u00fcr", "Hauf\u00b7fen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$.", "PWS", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Best\u00fcrtzt und Thr\u00e4nen-voll! mit ihren Kindern lauffen?", "tokens": ["Be\u00b7st\u00fcrtzt", "und", "Thr\u00e4\u00b7nen\u00b7voll", "!", "mit", "ih\u00b7ren", "Kin\u00b7dern", "lauf\u00b7fen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "NN", "$.", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "O Kinder! die ihr kaum das Vaterland erkannt/", "tokens": ["O", "Kin\u00b7der", "!", "die", "ihr", "kaum", "das", "Va\u00b7ter\u00b7land", "er\u00b7kannt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PRELS", "PPER", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Schaut wie/ was euch gebaut/ noch eh' ihr hin/ verbrandt.", "tokens": ["Schaut", "wie", "/", "was", "euch", "ge\u00b7baut", "/", "noch", "eh'", "ihr", "hin", "/", "ver\u00b7brandt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "KOKOM", "$(", "PWS", "PPER", "VVPP", "$(", "ADV", "KOUS", "PPER", "PTKVZ", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Stadt! hochgest\u00fcrtzte Stadt! must du dir selbst anz\u00fcnden", "tokens": ["Stadt", "!", "hoch\u00b7ge\u00b7st\u00fcrtz\u00b7te", "Stadt", "!", "must", "du", "dir", "selbst", "an\u00b7z\u00fcn\u00b7den"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADJA", "NN", "$.", "VMFIN", "PPER", "PPER", "ADV", "VVIZU"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Den Holtzsto\u00df/ auf dem Zier/ und Gut/ und Lust verschwinden.", "tokens": ["Den", "Holtz\u00b7sto\u00df", "/", "auf", "dem", "Zier", "/", "und", "Gut", "/", "und", "Lust", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPR", "ART", "NN", "$(", "KON", "NN", "$(", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Hat doch des Himmels-Zorn: Hat doch das scharffe Schwerdt:", "tokens": ["Hat", "doch", "des", "Him\u00b7mels\u00b7Zorn", ":", "Hat", "doch", "das", "scharf\u00b7fe", "Schwerdt", ":"], "token_info": ["word", "