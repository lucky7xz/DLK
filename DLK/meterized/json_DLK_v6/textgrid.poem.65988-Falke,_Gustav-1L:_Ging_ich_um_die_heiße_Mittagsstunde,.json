{"textgrid.poem.65988": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ging ich um die hei\u00dfe Mittagsstunde,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ging ich um die hei\u00dfe Mittagsstunde,", "tokens": ["Ging", "ich", "um", "die", "hei\u00b7\u00dfe", "Mit\u00b7tags\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die gewitterschw\u00fcle, durch die \u00f6de", "tokens": ["Die", "ge\u00b7wit\u00b7ter\u00b7schw\u00fc\u00b7le", ",", "durch", "die", "\u00f6\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sonnige Vorstadtgasse meinen Pflichtweg,", "tokens": ["Son\u00b7ni\u00b7ge", "Vor\u00b7stadt\u00b7gas\u00b7se", "mei\u00b7nen", "Pflicht\u00b7weg", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Wie dem weiten, aufgesperrten Rachen", "tokens": ["Wie", "dem", "wei\u00b7ten", ",", "auf\u00b7ge\u00b7sperr\u00b7ten", "Ra\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Einer plumpen kalten Brunnenfratze", "tokens": ["Ei\u00b7ner", "plum\u00b7pen", "kal\u00b7ten", "Brun\u00b7nen\u00b7frat\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Breit entst\u00fcrzt und mit Ger\u00e4usch der Sprudel,", "tokens": ["Breit", "ent\u00b7st\u00fcrzt", "und", "mit", "Ge\u00b7r\u00e4usch", "der", "Spru\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Blasen werfend, regenbogenfarbig,", "tokens": ["Bla\u00b7sen", "wer\u00b7fend", ",", "re\u00b7gen\u00b7bo\u00b7gen\u00b7far\u00b7big", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "VVPP", "$,", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Also w\u00e4lzte aus dem gro\u00dfen, roten", "tokens": ["Al\u00b7so", "w\u00e4lz\u00b7te", "aus", "dem", "gro\u00b7\u00dfen", ",", "ro\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "$,", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Schulgeb\u00e4ude sich ein Schwarm von M\u00e4dchen", "tokens": ["Schul\u00b7ge\u00b7b\u00e4u\u00b7de", "sich", "ein", "Schwarm", "von", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Auf die Stra\u00dfe, in die helle Sonne.", "tokens": ["Auf", "die", "Stra\u00b7\u00dfe", ",", "in", "die", "hel\u00b7le", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Jede Gr\u00f6\u00dfe, jede Farbe: Blonde,", "tokens": ["Je\u00b7de", "Gr\u00f6\u00b7\u00dfe", ",", "je\u00b7de", "Far\u00b7be", ":", "Blon\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$.", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Braune, Schwarze. Flechten, Z\u00f6pfchen, Locken.", "tokens": ["Brau\u00b7ne", ",", "Schwar\u00b7ze", ".", "Flech\u00b7ten", ",", "Z\u00f6pf\u00b7chen", ",", "Lo\u00b7cken", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$.", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Freigelassene! Welch' ein L\u00e4rmen, Schreien,", "tokens": ["Frei\u00b7ge\u00b7las\u00b7se\u00b7ne", "!", "Welch'", "ein", "L\u00e4r\u00b7men", ",", "Schrei\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "PIAT", "ART", "NN", "$,", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Plappern, Springen, Lachen, Kreischen, Schelten!", "tokens": ["Plap\u00b7pern", ",", "Sprin\u00b7gen", ",", "La\u00b7chen", ",", "Krei\u00b7schen", ",", "Schel\u00b7ten", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Aus den offnen Fenstern doch der Schule", "tokens": ["Aus", "den", "off\u00b7nen", "Fens\u00b7tern", "doch", "der", "Schu\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schallen kr\u00e4ftig frische Knabenstimmen,", "tokens": ["Schal\u00b7len", "kr\u00e4f\u00b7tig", "fri\u00b7sche", "Kna\u00b7bens\u00b7tim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Lautes, taktgem\u00e4\u00dfes Fibellesen,", "tokens": ["Lau\u00b7tes", ",", "takt\u00b7ge\u00b7m\u00e4\u00b7\u00dfes", "Fi\u00b7bel\u00b7le\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Jede Silbe scharf hervorgesto\u00dfen.", "tokens": ["Je\u00b7de", "Sil\u00b7be", "scharf", "her\u00b7vor\u00b7ge\u00b7sto\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Aber alles \u00fcbert\u00f6nen pl\u00f6tzlich", "tokens": ["A\u00b7ber", "al\u00b7les", "\u00fc\u00b7ber\u00b7t\u00f6\u00b7nen", "pl\u00f6tz\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "ADV", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aus dem dritten Stockwerk eines Hauses,", "tokens": ["Aus", "dem", "drit\u00b7ten", "Stock\u00b7werk", "ei\u00b7nes", "Hau\u00b7ses", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Einer Mietskaserne gegen\u00fcber,", "tokens": ["Ei\u00b7ner", "Miets\u00b7ka\u00b7ser\u00b7ne", "ge\u00b7gen\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Lange, schreckliche Posaunenkl\u00e4nge.", "tokens": ["Lan\u00b7ge", ",", "schreck\u00b7li\u00b7che", "Po\u00b7sau\u00b7nen\u00b7kl\u00e4n\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Immer die vier gleichen Takte qu\u00e4lend,", "tokens": ["Im\u00b7mer", "die", "vier", "glei\u00b7chen", "Tak\u00b7te", "qu\u00e4\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "CARD", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+++-+-+-", "measure": "unknown.measure.hexa"}, "line.6": {"text": "Qualvoll in die Welt hinausgeblasen.", "tokens": ["Qual\u00b7voll", "in", "die", "Welt", "hin\u00b7aus\u00b7ge\u00b7bla\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Ist es eines kleinen Tanzorchesters", "tokens": ["Ist", "es", "ei\u00b7nes", "klei\u00b7nen", "Tan\u00b7zor\u00b7ches\u00b7ters"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Posaunist, der sich da oben abqu\u00e4lt?", "tokens": ["Po\u00b7sau\u00b7nist", ",", "der", "sich", "da", "o\u00b7ben", "ab\u00b7qu\u00e4lt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "Ist ein Dilettant es, kunstbegeistert?", "tokens": ["Ist", "ein", "Di\u00b7let\u00b7tant", "es", ",", "kunst\u00b7be\u00b7geis\u00b7tert", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "$,", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Ach, der Weg zur Kunst, zu jeder, jeder", "tokens": ["Ach", ",", "der", "Weg", "zur", "Kunst", ",", "zu", "je\u00b7der", ",", "je\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ITJ", "$,", "ART", "NN", "APPRART", "NN", "$,", "APPR", "PIS", "$,", "PIAT"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Ist so schwer. So viele Stufen f\u00fchren", "tokens": ["Ist", "so", "schwer", ".", "So", "vie\u00b7le", "Stu\u00b7fen", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJD", "$.", "ADV", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Aufw\u00e4rts nach den lichten, reinen H\u00f6hen,", "tokens": ["Auf\u00b7w\u00e4rts", "nach", "den", "lich\u00b7ten", ",", "rei\u00b7nen", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Auf den untersten, den breitgelagert", "tokens": ["Auf", "den", "un\u00b7ters\u00b7ten", ",", "den", "breit\u00b7ge\u00b7la\u00b7gert"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ART", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Freiger\u00e4umigen, dies Sto\u00dfen, Dr\u00e4ngen,", "tokens": ["Frei\u00b7ge\u00b7r\u00e4u\u00b7mi\u00b7gen", ",", "dies", "Sto\u00b7\u00dfen", ",", "Dr\u00e4n\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PDS", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Dies Gewimmel. Aber m\u00e4hlich aufw\u00e4rts", "tokens": ["Dies", "Ge\u00b7wim\u00b7mel", ".", "A\u00b7ber", "m\u00e4h\u00b7lich", "auf\u00b7w\u00e4rts"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "NN", "$.", "KON", "ADJD", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Lichtet sich's, und sp\u00e4rlich nur bev\u00f6lkert", "tokens": ["Lich\u00b7tet", "sich's", ",", "und", "sp\u00e4r\u00b7lich", "nur", "be\u00b7v\u00f6l\u00b7kert"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "KON", "ADJD", "ADV", "VVPP"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.17": {"text": "Sehn die h\u00f6chsten \u00fcber Zeit und Raum weg,", "tokens": ["Sehn", "die", "h\u00f6chs\u00b7ten", "\u00fc\u00b7ber", "Zeit", "und", "Raum", "weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Und die Spitze? Und die h\u00f6chste H\u00f6he?", "tokens": ["Und", "die", "Spit\u00b7ze", "?", "Und", "die", "h\u00f6chs\u00b7te", "H\u00f6\u00b7he", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$.", "KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Hat sie je ein Sterblicher erklommen?", "tokens": ["Hat", "sie", "je", "ein", "Sterb\u00b7li\u00b7cher", "er\u00b7klom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Oder harrt noch einsam sie des Kommers,", "tokens": ["O\u00b7der", "harrt", "noch", "ein\u00b7sam", "sie", "des", "Kom\u00b7mers", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Der von dort mit seinem Finger leise", "tokens": ["Der", "von", "dort", "mit", "sei\u00b7nem", "Fin\u00b7ger", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADV", "APPR", "PPOSAT", "NN", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "An die Fackel r\u00fchrt, die alles Licht giebt.", "tokens": ["An", "die", "Fa\u00b7ckel", "r\u00fchrt", ",", "die", "al\u00b7les", "Licht", "giebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Hinter mir lag l\u00e4ngst die hei\u00dfe Gasse,", "tokens": ["Hin\u00b7ter", "mir", "lag", "l\u00e4ngst", "die", "hei\u00b7\u00dfe", "Gas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.24": {"text": "Aber immer klang mir in den Ohren", "tokens": ["A\u00b7ber", "im\u00b7mer", "klang", "mir", "in", "den", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "Noch das qualvoll unverdrossene Blasen,", "tokens": ["Noch", "das", "qual\u00b7voll", "un\u00b7ver\u00b7dros\u00b7se\u00b7ne", "Bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.26": {"text": "Wie das St\u00f6hnen einer kranken Seele,", "tokens": ["Wie", "das", "St\u00f6h\u00b7nen", "ei\u00b7ner", "kran\u00b7ken", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": "Die mit ihrem Erdenfluch sich abringt,", "tokens": ["Die", "mit", "ih\u00b7rem", "Er\u00b7den\u00b7fluch", "sich", "ab\u00b7ringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Leidend, sieglos, aber stolz und st\u00f6rrig:", "tokens": ["Lei\u00b7dend", ",", "sieg\u00b7los", ",", "a\u00b7ber", "stolz", "und", "st\u00f6r\u00b7rig", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.29": {"text": "Es muss sein!", "tokens": ["Es", "muss", "sein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VAINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}