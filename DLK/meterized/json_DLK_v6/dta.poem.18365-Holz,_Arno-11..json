{"dta.poem.18365": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Phantastisch wieder aufgethan", "tokens": ["Phan\u00b7tas\u00b7tisch", "wie\u00b7der", "auf\u00b7ge\u00b7than"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und seine goldne Tricolore", "tokens": ["Und", "sei\u00b7ne", "gold\u00b7ne", "Tri\u00b7co\u00b7lo\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Weht hoch aus jedem Wolkenkahn.", "tokens": ["Weht", "hoch", "aus", "je\u00b7dem", "Wol\u00b7ken\u00b7kahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur hier in diesen dumpfen Mauern", "tokens": ["Nur", "hier", "in", "die\u00b7sen", "dum\u00b7pfen", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zum Fluch wird er dem Proletar \u2014", "tokens": ["Zum", "Fluch", "wird", "er", "dem", "Pro\u00b7le\u00b7tar"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In allen Ecken seh ich lauern,", "tokens": ["In", "al\u00b7len", "E\u00b7cken", "seh", "ich", "lau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "In allen Winkeln seh ich kauern", "tokens": ["In", "al\u00b7len", "Win\u00b7keln", "seh", "ich", "kau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "D\u00e4monen, die die Nacht gebar!", "tokens": ["D\u00e4\u00b7mo\u00b7nen", ",", "die", "die", "Nacht", "ge\u00b7bar", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein letztes Licht ist l\u00e4ngst erloschen", "tokens": ["Mein", "letz\u00b7tes", "Licht", "ist", "l\u00e4ngst", "er\u00b7lo\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und fahl durchs Fenster lugt die Noth,", "tokens": ["Und", "fahl", "durchs", "Fens\u00b7ter", "lugt", "die", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn dies hier ist der letzte Groschen", "tokens": ["Denn", "dies", "hier", "ist", "der", "letz\u00b7te", "Gro\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und dies das letzte St\u00fcckchen Brod!", "tokens": ["Und", "dies", "das", "letz\u00b7te", "St\u00fcck\u00b7chen", "Brod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verlacht, verludert und verloren,", "tokens": ["Ver\u00b7lacht", ",", "ver\u00b7lu\u00b7dert", "und", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das alte \u201eWeder Gl\u00fcck noch Stern!\u201c", "tokens": ["Das", "al\u00b7te", "\u201e", "We\u00b7der", "Gl\u00fcck", "noch", "Stern", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "$(", "KON", "NN", "ADV", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fcrwahr, ich bin der Thor der Thoren!", "tokens": ["F\u00fcr\u00b7wahr", ",", "ich", "bin", "der", "Thor", "der", "Tho\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "O Mutter, w\u00e4r ich nie geboren!", "tokens": ["O", "Mut\u00b7ter", ",", "w\u00e4r", "ich", "nie", "ge\u00b7bo\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "O sch\u00f6ne Zeit, wie liegst du fern!", "tokens": ["O", "sch\u00f6\u00b7ne", "Zeit", ",", "wie", "liegst", "du", "fern", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PWAV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf wilder, meerverschlagner Planke,", "tokens": ["Auf", "wil\u00b7der", ",", "meer\u00b7ver\u00b7schlag\u00b7ner", "Plan\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Schiffer bin ich, der versinkt;", "tokens": ["Ein", "Schif\u00b7fer", "bin", "ich", ",", "der", "ver\u00b7sinkt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein letzter Stern ist ein Gedanke,", "tokens": ["Mein", "letz\u00b7ter", "Stern", "ist", "ein", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der leuchtend mir vom Himmel blinkt.", "tokens": ["Der", "leuch\u00b7tend", "mir", "vom", "Him\u00b7mel", "blinkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein fernes Eiland seh ich ragen,", "tokens": ["Ein", "fer\u00b7nes", "Ei\u00b7land", "seh", "ich", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch wirft die Fluth mich stets zur\u00fcck;", "tokens": ["Doch", "wirft", "die", "Fluth", "mich", "stets", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O, will's denn immer noch nicht tagen?", "tokens": ["O", ",", "will's", "denn", "im\u00b7mer", "noch", "nicht", "ta\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "ADV", "ADV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Noch gilt's zu wetten und zu wagen,", "tokens": ["Noch", "gilt's", "zu", "wet\u00b7ten", "und", "zu", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Denn jenes Eiland wiegt mein Gl\u00fcck!", "tokens": ["Denn", "je\u00b7nes", "Ei\u00b7land", "wiegt", "mein", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Schon thut mir, wie wenn Glocken klingen,", "tokens": ["Schon", "thut", "mir", ",", "wie", "wenn", "Glo\u00b7cken", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOKOM", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Zukunft ihre Wunder kund \u2014", "tokens": ["Die", "Zu\u00b7kunft", "ih\u00b7re", "Wun\u00b7der", "kund"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Stammeln nur ist jetzt mein Singen,", "tokens": ["Ein", "Stam\u00b7meln", "nur", "ist", "jetzt", "mein", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ein Stammeln wie aus Kindermund!", "tokens": ["Ein", "Stam\u00b7meln", "wie", "aus", "Kin\u00b7der\u00b7mund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du Sch\u00f6pfer aller Harmonieen,", "tokens": ["Du", "Sch\u00f6p\u00b7fer", "al\u00b7ler", "Har\u00b7mo\u00b7ni\u00b7e\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+----", "measure": "unknown.measure.tri"}, "line.6": {"text": "O, gieb mir Luft, o gieb mir Licht!", "tokens": ["O", ",", "gieb", "mir", "Luft", ",", "o", "gieb", "mir", "Licht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "PPER", "NN", "$,", "FM", "VVIMP", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im Staube sieh mich vor dir knieen,", "tokens": ["Im", "Stau\u00b7be", "sieh", "mich", "vor", "dir", "kni\u00b7e\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Denn eine Welt von Melodieen", "tokens": ["Denn", "ei\u00b7ne", "Welt", "von", "Me\u00b7lo\u00b7di\u00b7e\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Geht unter, wenn dies Herz zerbricht!", "tokens": ["Geht", "un\u00b7ter", ",", "wenn", "dies", "Herz", "zer\u00b7bricht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "$,", "KOUS", "PDS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}