{"dta.poem.5373": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von diesem Besten nun des Menschen ist das Dencken", "tokens": ["Von", "die\u00b7sem", "Bes\u00b7ten", "nun", "des", "Men\u00b7schen", "ist", "das", "Den\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADV", "ART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Voll Ehrfurcht, Lust und Danck auf GOttes weise Macht.", "tokens": ["Voll", "Ehr\u00b7furcht", ",", "Lust", "und", "Danck", "auf", "Got\u00b7tes", "wei\u00b7se", "Macht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "NN", "KON", "NN", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf denn! der Seelen Kraft auf seiner Wercke Pracht", "tokens": ["Auf", "denn", "!", "der", "See\u00b7len", "Kraft", "auf", "sei\u00b7ner", "Wer\u00b7cke", "Pracht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "$.", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit Andacht und mit Lust zu lencken,", "tokens": ["Mit", "An\u00b7dacht", "und", "mit", "Lust", "zu", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und unser Bestes ihm, ein frohes Hertz, zu schencken!", "tokens": ["Und", "un\u00b7ser", "Bes\u00b7tes", "ihm", ",", "ein", "fro\u00b7hes", "Hertz", ",", "zu", "schen\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "$,", "ART", "ADJA", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}