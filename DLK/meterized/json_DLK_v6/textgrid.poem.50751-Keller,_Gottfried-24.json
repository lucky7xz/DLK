{"textgrid.poem.50751": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "24", "genre": "verse", "period": "N.A.", "pub_year": 1846, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ein Poet ein St\u00fcck vom ew'gen Leben", "tokens": ["Wenn", "ein", "Po\u00b7et", "ein", "St\u00fcck", "vom", "ew'\u00b7gen", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Herzen tr\u00e4gt schon hier als Morgengabe,", "tokens": ["Im", "Her\u00b7zen", "tr\u00e4gt", "schon", "hier", "als", "Mor\u00b7gen\u00b7ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "ADV", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn in Verkl\u00e4rung alle Dinge schweben,", "tokens": ["Wenn", "in", "Ver\u00b7kl\u00e4\u00b7rung", "al\u00b7le", "Din\u00b7ge", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die er ber\u00fchrt mit seinem Zauberstabe,", "tokens": ["Die", "er", "be\u00b7r\u00fchrt", "mit", "sei\u00b7nem", "Zau\u00b7ber\u00b7sta\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und er den Blick nach dem, was \u00fcberm Grabe,", "tokens": ["Und", "er", "den", "Blick", "nach", "dem", ",", "was", "\u00fc\u00b7berm", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "APPR", "ART", "$,", "PRELS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Unsterblichkeitgetr\u00e4nkt, ", "tokens": ["U\u00b7nsterb\u00b7lich\u00b7keit\u00b7ge\u00b7tr\u00e4nkt", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Oh, was er auch im Rausch gesungen habe \u2013", "tokens": ["Oh", ",", "was", "er", "auch", "im", "Rausch", "ge\u00b7sun\u00b7gen", "ha\u00b7be", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "PPER", "ADV", "APPRART", "NN", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wenn, sonnend sich auf seinem Maienthron,", "tokens": ["Wenn", ",", "son\u00b7nend", "sich", "auf", "sei\u00b7nem", "Mai\u00b7ent\u00b7hron", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KON", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Buntschillernd eine Schlange sich erhebt,", "tokens": ["Bunt\u00b7schil\u00b7lernd", "ei\u00b7ne", "Schlan\u00b7ge", "sich", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So ist sie ", "tokens": ["So", "ist", "sie"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Jedoch der Atheist von Profession,", "tokens": ["Je\u00b7doch", "der", "A\u00b7theist", "von", "Pro\u00b7fes\u00b7si\u00b7on", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der nur vom Atheismus-Knochen lebt,", "tokens": ["Der", "nur", "vom", "A\u00b7the\u00b7is\u00b7mus\u00b7Kno\u00b7chen", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist eine eingefleischte Blasphemie.", "tokens": ["Ist", "ei\u00b7ne", "ein\u00b7ge\u00b7fleischte", "Blas\u00b7phe\u00b7mie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}