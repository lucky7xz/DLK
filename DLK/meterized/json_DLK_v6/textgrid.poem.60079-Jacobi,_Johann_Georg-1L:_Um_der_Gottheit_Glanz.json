{"textgrid.poem.60079": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Um der Gottheit Glanz", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um der Gottheit Glanz", "tokens": ["Um", "der", "Got\u00b7theit", "Glanz"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Hatten jauchzende Sonnen", "tokens": ["Hat\u00b7ten", "jauch\u00b7zen\u00b7de", "Son\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Ihren Lauf begonnen,", "tokens": ["Ih\u00b7ren", "Lauf", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Engel ihren Feyertanz;", "tokens": ["En\u00b7gel", "ih\u00b7ren", "Fe\u00b7yer\u00b7tanz", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus der Gottheit Glanz,", "tokens": ["Aus", "der", "Got\u00b7theit", "Glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.6": {"text": "Engeln gleich, im Jubel gebohren,", "tokens": ["En\u00b7geln", "gleich", ",", "im", "Ju\u00b7bel", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Mischte sich, zur F\u00fchrerinn erkoren,", "tokens": ["Mischte", "sich", ",", "zur", "F\u00fch\u00b7re\u00b7rinn", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Unschuld in den Tanz.", "tokens": ["Un\u00b7schuld", "in", "den", "Tanz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Dort, auf leichter Flur,", "tokens": ["Dort", ",", "auf", "leich\u00b7ter", "Flur", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Im unsterblichen Lenze", "tokens": ["Im", "uns\u00b7terb\u00b7li\u00b7chen", "Len\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Bl\u00fchn der Unschuld Kr\u00e4nze,", "tokens": ["Bl\u00fchn", "der", "Un\u00b7schuld", "Kr\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Folgt der Seraph ihrer Spur;", "tokens": ["Folgt", "der", "Se\u00b7raph", "ih\u00b7rer", "Spur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber auf der Flur", "tokens": ["A\u00b7ber", "auf", "der", "Flur"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Unterm Mond, im Schatten der Erde,", "tokens": ["Un\u00b7term", "Mond", ",", "im", "Schat\u00b7ten", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Wandelt sie mit kindlicher Geberde", "tokens": ["Wan\u00b7delt", "sie", "mit", "kind\u00b7li\u00b7cher", "Ge\u00b7ber\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Bey der Einfalt nur;", "tokens": ["Bey", "der", "Ein\u00b7falt", "nur", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Will im Mayenlicht", "tokens": ["Will", "im", "Ma\u00b7yen\u00b7licht"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "APPRART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Hier an irdischen B\u00e4chen", "tokens": ["Hier", "an", "ir\u00b7di\u00b7schen", "B\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Volle Rosen brechen;", "tokens": ["Vol\u00b7le", "Ro\u00b7sen", "bre\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und die Dornen kennt sie nicht.", "tokens": ["Und", "die", "Dor\u00b7nen", "kennt", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hier vom Mayenlicht", "tokens": ["Hier", "vom", "Ma\u00b7yen\u00b7licht"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Aufgeweckt am t\u00e4uschenden Morgen,", "tokens": ["Auf\u00b7ge\u00b7weckt", "am", "t\u00e4u\u00b7schen\u00b7den", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "L\u00e4chelt sie herbey die nahen Sorgen \u2013", "tokens": ["L\u00e4\u00b7chelt", "sie", "her\u00b7bey", "die", "na\u00b7hen", "Sor\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Ach, und wei\u00df es nicht!", "tokens": ["Ach", ",", "und", "wei\u00df", "es", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Mit der Engels-Hand", "tokens": ["Mit", "der", "En\u00b7gels\u00b7Hand"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Unsre L\u00e4mmer zu weiden,", "tokens": ["Uns\u00b7re", "L\u00e4m\u00b7mer", "zu", "wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Geht auf armen Heiden", "tokens": ["Geht", "auf", "ar\u00b7men", "Hei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Sie, von Wenigen gekannt;", "tokens": ["Sie", ",", "von", "We\u00b7ni\u00b7gen", "ge\u00b7kannt", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber, auch verbannt,", "tokens": ["A\u00b7ber", ",", "auch", "ver\u00b7bannt", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Gibt sie noch, in niedriger H\u00fclle,", "tokens": ["Gibt", "sie", "noch", ",", "in", "nied\u00b7ri\u00b7ger", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Wonn' und Trost und Herrlichkeit die F\u00fclle", "tokens": ["Wonn'", "und", "Trost", "und", "Herr\u00b7lich\u00b7keit", "die", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Seelen, ihr verwandt.", "tokens": ["See\u00b7len", ",", "ihr", "ver\u00b7wandt", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Ach! sie selber flieht", "tokens": ["Ach", "!", "sie", "sel\u00b7ber", "flieht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "ADV", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Mit den kindlichen Scherzen;", "tokens": ["Mit", "den", "kind\u00b7li\u00b7chen", "Scher\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Doch in keinem Herzen", "tokens": ["Doch", "in", "kei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Stirbt ihr holdes Wiegenlied:", "tokens": ["Stirbt", "ihr", "hol\u00b7des", "Wie\u00b7gen\u00b7lied", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer den S\u00e4ugling sieht", "tokens": ["Wer", "den", "S\u00e4ug\u00b7ling", "sieht"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "An die Brust der Mutter sich dr\u00fccken,", "tokens": ["An", "die", "Brust", "der", "Mut\u00b7ter", "sich", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "O, der f\u00fchlt, da\u00df ihn mit Himmelsblicken", "tokens": ["O", ",", "der", "f\u00fchlt", ",", "da\u00df", "ihn", "mit", "Him\u00b7mels\u00b7bli\u00b7cken"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Unschuld an sich zieht.", "tokens": ["Un\u00b7schuld", "an", "sich", "zieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Wenn dein Warnen schon", "tokens": ["Wenn", "dein", "War\u00b7nen", "schon"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Oft den Frevler emp\u00f6ret,", "tokens": ["Oft", "den", "Frev\u00b7ler", "em\u00b7p\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Unschuld! dennoch h\u00f6ret", "tokens": ["Un\u00b7schuld", "!", "den\u00b7noch", "h\u00f6\u00b7ret"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADJD", "$.", "ADV", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Sp\u00e4ter er den ernsten Ton.", "tokens": ["Sp\u00e4\u00b7ter", "er", "den", "erns\u00b7ten", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jeder Erden-Sohn", "tokens": ["Je\u00b7der", "Er\u00b7den\u00b7Sohn"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Fleht zu dir am letzten der Tage,", "tokens": ["Fleht", "zu", "dir", "am", "letz\u00b7ten", "der", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPRART", "ADJA", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Da\u00df ihn nicht dein Auge dort verklage", "tokens": ["Da\u00df", "ihn", "nicht", "dein", "Au\u00b7ge", "dort", "ver\u00b7kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "PPOSAT", "NN", "ADV", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Vor des Richters Thron.", "tokens": ["Vor", "des", "Rich\u00b7ters", "Thron", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Aus der Gottheit Glanz", "tokens": ["Aus", "der", "Got\u00b7theit", "Glanz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "--+-+", "measure": "anapaest.init"}, "line.2": {"text": "Sind die Seelen gebohren,", "tokens": ["Sind", "die", "See\u00b7len", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Allesammt erkoren,", "tokens": ["Al\u00b7le\u00b7sammt", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Dich zu sehn im Sternenkranz;", "tokens": ["Dich", "zu", "sehn", "im", "Ster\u00b7nen\u00b7kranz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Um der Gottheit Glanz", "tokens": ["Um", "der", "Got\u00b7theit", "Glanz"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "H\u00e4lt mit dir, dem sch\u00f6nsten der Engel,", "tokens": ["H\u00e4lt", "mit", "dir", ",", "dem", "sch\u00f6ns\u00b7ten", "der", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "PRELS", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Jeder Geist in Welten ohne M\u00e4ngel", "tokens": ["Je\u00b7der", "Geist", "in", "Wel\u00b7ten", "oh\u00b7ne", "M\u00e4n\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Seinen Feyertanz.", "tokens": ["Sei\u00b7nen", "Fe\u00b7yer\u00b7tanz", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}