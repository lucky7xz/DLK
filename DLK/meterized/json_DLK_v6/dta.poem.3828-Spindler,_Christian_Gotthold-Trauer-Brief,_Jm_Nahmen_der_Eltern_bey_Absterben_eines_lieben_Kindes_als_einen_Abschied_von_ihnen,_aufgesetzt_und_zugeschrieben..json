{"dta.poem.3828": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Trauer-Brief,  \n Jm Nahmen der Eltern/ bey Absterben eines lieben Kindes/  \n als einen Abschied von ihnen, aufgesetzt und zugeschrieben.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Gehe nur, geliebtes Hertze, auserlesnes Liebes-", "tokens": ["Ge\u00b7he", "nur", ",", "ge\u00b7lieb\u00b7tes", "Hert\u00b7ze", ",", "au\u00b7ser\u00b7les\u00b7nes", "Lie\u00b7bes"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "ADV", "$,", "ADJA", "VVFIN", "$,", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Pfand,", "tokens": ["Pfand", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Gehe aus dem Angst-Egypten, gehe ins gelobte", "tokens": ["Ge\u00b7he", "aus", "dem", "Angst\u00b7Egyp\u00b7ten", ",", "ge\u00b7he", "ins", "ge\u00b7lob\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "$,", "VVFIN", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Land;", "tokens": ["Land", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Unsre Z\u00e4hren, unser Ach, \u00e4ndert doch nicht dein", "tokens": ["Uns\u00b7re", "Z\u00e4h\u00b7ren", ",", "un\u00b7ser", "Ach", ",", "\u00e4n\u00b7dert", "doch", "nicht", "dein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "PTKNEG", "PPOSAT"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Beginnen,", "tokens": ["Be\u00b7gin\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Nein, du eilest, nein, du fliehst, ach du eilest ja", "tokens": ["Nein", ",", "du", "ei\u00b7lest", ",", "nein", ",", "du", "fliehst", ",", "ach", "du", "ei\u00b7lest", "ja"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "$,", "PTKANT", "$,", "PPER", "VVFIN", "$,", "XY", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "von hinnen.", "tokens": ["von", "hin\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "ADV", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "O! du Sohn, du H\u00f6chstgeliebter! o du jung und", "tokens": ["O", "!", "du", "Sohn", ",", "du", "H\u00f6chst\u00b7ge\u00b7lieb\u00b7ter", "!", "o", "du", "jung", "und"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "PPER", "NN", "$,", "PPER", "NN", "$.", "FM", "PPER", "ADJD", "KON"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "frommes Lamm!", "tokens": ["from\u00b7mes", "Lamm", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Du suchst JEsum, deinen Heiland, deinen Seelen-", "tokens": ["Du", "suchst", "Je\u00b7sum", ",", "dei\u00b7nen", "Hei\u00b7land", ",", "dei\u00b7nen", "See\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.12": {"text": "Br\u00e4utigam.", "tokens": ["Br\u00e4u\u00b7ti\u00b7gam", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Denn du scheuest nicht den Tod, dir bringt er kein", "tokens": ["Denn", "du", "scheu\u00b7est", "nicht", "den", "Tod", ",", "dir", "bringt", "er", "kein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "PIAT"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.14": {"text": "banges Grauen,", "tokens": ["ban\u00b7ges", "Grau\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Nein, er eben zeigt den Ort, wo du kanst den", "tokens": ["Nein", ",", "er", "e\u00b7ben", "zeigt", "den", "Ort", ",", "wo", "du", "kanst", "den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "ADV", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "VMFIN", "ART"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Heyland schauen.", "tokens": ["Hey\u00b7land", "schau\u00b7en", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Du gehst nun in Freuden-Tempel, in die Burg", "tokens": ["Du", "gehst", "nun", "in", "Freu\u00b7den\u00b7Tem\u00b7pel", ",", "in", "die", "Burg"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "von Sion ein,", "tokens": ["von", "Si\u00b7on", "ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Wo viel tausend Chor der Engel, und der Auser-", "tokens": ["Wo", "viel", "tau\u00b7send", "Chor", "der", "En\u00b7gel", ",", "und", "der", "Aus\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "CARD", "NN", "ART", "NN", "$,", "KON", "ART", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.20": {"text": "wehlten seyn.", "tokens": ["wehl\u00b7ten", "seyn", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VAINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.21": {"text": "Aber wir, geliebtes Kind! wir, die sehr betr\u00fcb-", "tokens": ["A\u00b7ber", "wir", ",", "ge\u00b7lieb\u00b7tes", "Kind", "!", "wir", ",", "die", "sehr", "be\u00b7tr\u00fcb"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "ADJA", "NN", "$.", "PPER", "$,", "PRELS", "ADV", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "ten Eltern,", "tokens": ["ten", "El\u00b7tern", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.23": {"text": "M\u00fcssen ein geh\u00e4ufftes Ach, Angst und Kummer-", "tokens": ["M\u00fcs\u00b7sen", "ein", "ge\u00b7h\u00e4uff\u00b7tes", "Ach", ",", "Angst", "und", "Kum\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN", "$,", "NN", "KON", "TRUNC"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.24": {"text": "Z\u00e4hren keltern.", "tokens": ["Z\u00e4h\u00b7ren", "kel\u00b7tern", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.25": {"text": "Dein Gang geht zwar in den Himmel, doch wir", "tokens": ["Dein", "Gang", "geht", "zwar", "in", "den", "Him\u00b7mel", ",", "doch", "wir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$,", "KON", "PPER"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "bleiben auf der Welt,", "tokens": ["blei\u00b7ben", "auf", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.27": {"text": "Die vor uns, da du gestorben, kein Vergn\u00fcgen", "tokens": ["Die", "vor", "uns", ",", "da", "du", "ge\u00b7stor\u00b7ben", ",", "kein", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "APPR", "PPER", "$,", "KOUS", "PPER", "VVPP", "$,", "PIAT", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.28": {"text": "in sich h\u00e4lt.", "tokens": ["in", "sich", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.29": {"text": "Denn so lange wir hier sind, werden wir dich", "tokens": ["Denn", "so", "lan\u00b7ge", "wir", "hier", "sind", ",", "wer\u00b7den", "wir", "dich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "PPER", "ADV", "VAFIN", "$,", "VAFIN", "PPER", "PRF"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.30": {"text": "auch bedauren,", "tokens": ["auch", "be\u00b7dau\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.31": {"text": "Ja wir werden dich, o Sohn! mit dem gr\u00f6sten", "tokens": ["Ja", "wir", "wer\u00b7den", "dich", ",", "o", "Sohn", "!", "mit", "dem", "gr\u00f6s\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "PPER", "VAFIN", "PPER", "$,", "FM", "NN", "$.", "APPR", "ART", "ADJA"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.32": {"text": "Schmertz betrauren,", "tokens": ["Schmertz", "be\u00b7trau\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.33": {"text": "Vis uns einst dein JEsus ruffet, bis es heist: Es", "tokens": ["Vis", "uns", "einst", "dein", "Je\u00b7sus", "ruf\u00b7fet", ",", "bis", "es", "heist", ":", "Es"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "PPER", "ADV", "PPOSAT", "NE", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$.", "PPER"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.34": {"text": "ist vollbracht!", "tokens": ["ist", "voll\u00b7bracht", "!"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.35": {"text": "Sterblich Theil von unsern Hertzen, ruhe wohl!", "tokens": ["Sterb\u00b7lich", "Theil", "von", "un\u00b7sern", "Hert\u00b7zen", ",", "ru\u00b7he", "wohl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.36": {"text": "zu guter Nacht!", "tokens": ["zu", "gu\u00b7ter", "Nacht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}