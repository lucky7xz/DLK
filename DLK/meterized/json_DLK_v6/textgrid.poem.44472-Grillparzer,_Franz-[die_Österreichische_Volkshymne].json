{"textgrid.poem.44472": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[die \u00d6sterreichische Volkshymne]", "genre": "verse", "period": "N.A.", "pub_year": 1835, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott erhalte unsern Kaiser,", "tokens": ["Gott", "er\u00b7hal\u00b7te", "un\u00b7sern", "Kai\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsern guten Ferdinand!", "tokens": ["Un\u00b7sern", "gu\u00b7ten", "Fer\u00b7di\u00b7nand", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der du Throne h\u00e4ltst und H\u00e4user,", "tokens": ["Der", "du", "Thro\u00b7ne", "h\u00e4ltst", "und", "H\u00e4u\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "VVFIN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schirm ihn, Herr, mit starker Hand,", "tokens": ["Schirm", "ihn", ",", "Herr", ",", "mit", "star\u00b7ker", "Hand", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df, statt Lorbeer, \u00d6lbaumreiser", "tokens": ["La\u00df", ",", "statt", "Lor\u00b7beer", ",", "\u00d6l\u00b7baum\u00b7rei\u00b7ser"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["VVIMP", "$,", "APPR", "NE", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sprossen, wo er liebend stand;", "tokens": ["Spros\u00b7sen", ",", "wo", "er", "lie\u00b7bend", "stand", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gott erhalte unsern Kaiser,", "tokens": ["Gott", "er\u00b7hal\u00b7te", "un\u00b7sern", "Kai\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unsern edeln Ferdinand.", "tokens": ["Un\u00b7sern", "e\u00b7deln", "Fer\u00b7di\u00b7nand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "H\u00f6her als kein Weltenst\u00fcrmer", "tokens": ["H\u00f6\u00b7her", "als", "kein", "Wel\u00b7ten\u00b7st\u00fcr\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KOKOM", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Strahl er in der Gnade Licht;", "tokens": ["Strahl", "er", "in", "der", "Gna\u00b7de", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeden Rechtes ein Beschirmer,", "tokens": ["Je\u00b7den", "Rech\u00b7tes", "ein", "Be\u00b7schir\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Folg ihm dienend jede Pflicht.", "tokens": ["Folg", "ihm", "die\u00b7nend", "je\u00b7de", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur ein Guter ist ein Weiser,", "tokens": ["Nur", "ein", "Gu\u00b7ter", "ist", "ein", "Wei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fluch ist ohne Herz Verstand,", "tokens": ["Fluch", "ist", "oh\u00b7ne", "Herz", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Darum jubeln wir dem Kaiser,", "tokens": ["Da\u00b7rum", "ju\u00b7beln", "wir", "dem", "Kai\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unserm guten Ferdinand!", "tokens": ["Un\u00b7serm", "gu\u00b7ten", "Fer\u00b7di\u00b7nand", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und die ihm zur Seite thronet,", "tokens": ["Und", "die", "ihm", "zur", "Sei\u00b7te", "thro\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fromm wie wenig, mild wie er,", "tokens": ["Fromm", "wie", "we\u00b7nig", ",", "mild", "wie", "er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "PIS", "$,", "ADJD", "KOKOM", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hier schon als im Jenseits wohnet,", "tokens": ["Hier", "schon", "als", "im", "Jen\u00b7seits", "woh\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOKOM", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Segen spendet um sich her,", "tokens": ["Se\u00b7gen", "spen\u00b7det", "um", "sich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis der letzte Pulsschlag leiser,", "tokens": ["Bis", "der", "letz\u00b7te", "Puls\u00b7schlag", "lei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "La\u00df sie wandeln Hand in Hand;", "tokens": ["La\u00df", "sie", "wan\u00b7deln", "Hand", "in", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Segn in ihr auch unsern Kaiser,", "tokens": ["Segn", "in", "ihr", "auch", "un\u00b7sern", "Kai\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unsern guten Ferdinand!", "tokens": ["Un\u00b7sern", "gu\u00b7ten", "Fer\u00b7di\u00b7nand", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mag dann Feind und Bosheit dr\u00e4uen,", "tokens": ["Mag", "dann", "Feind", "und", "Bos\u00b7heit", "dr\u00e4u\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich erheben eine Welt,", "tokens": ["Sich", "er\u00b7he\u00b7ben", "ei\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer verlangt auch nach dem Neuen,", "tokens": ["Wer", "ver\u00b7langt", "auch", "nach", "dem", "Neu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem das Alte wohlgef\u00e4llt,", "tokens": ["Dem", "das", "Al\u00b7te", "wohl\u00b7ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00fcht euch ab, des Fremden Preiser,", "tokens": ["M\u00fcht", "euch", "ab", ",", "des", "Frem\u00b7den", "Prei\u00b7ser", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seht hier einig F\u00fcrst und Land.", "tokens": ["Seht", "hier", "ei\u00b7nig", "F\u00fcrst", "und", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gott mit uns und unserm Kaiser,", "tokens": ["Gott", "mit", "uns", "und", "un\u00b7serm", "Kai\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wir mit Gott f\u00fcr Ferdinand.", "tokens": ["Wir", "mit", "Gott", "f\u00fcr", "Fer\u00b7di\u00b7nand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}