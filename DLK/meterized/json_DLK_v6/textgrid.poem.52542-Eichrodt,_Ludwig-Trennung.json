{"textgrid.poem.52542": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Trennung", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich steh bei meinen vielen B\u00fcchern;", "tokens": ["Ich", "steh", "bei", "mei\u00b7nen", "vie\u00b7len", "B\u00fc\u00b7chern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich geh spazieren durch den Wald \u2013", "tokens": ["Ich", "geh", "spa\u00b7zie\u00b7ren", "durch", "den", "Wald", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wei\u00df dabei von keinem kl\u00fcgern,", "tokens": ["Und", "wei\u00df", "da\u00b7bei", "von", "kei\u00b7nem", "kl\u00fc\u00b7gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von keinem sch\u00f6nern Aufenthalt.", "tokens": ["Von", "kei\u00b7nem", "sch\u00f6\u00b7nern", "Auf\u00b7ent\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich sitz in meiner trauten Schenke,", "tokens": ["Ich", "sitz", "in", "mei\u00b7ner", "trau\u00b7ten", "Schen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bei lieben Freunden und beim Wein,", "tokens": ["Bei", "lie\u00b7ben", "Freun\u00b7den", "und", "beim", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und weil ich just nicht an dich denke,", "tokens": ["Und", "weil", "ich", "just", "nicht", "an", "dich", "den\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PTKNEG", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So glaub ich \u00fcberfroh zu sein.", "tokens": ["So", "glaub", "ich", "\u00fc\u00b7ber\u00b7froh", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da \u00fcbermannt mich oft ein Sehnen,", "tokens": ["Da", "\u00fc\u00b7berm\u00b7annt", "mich", "oft", "ein", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Zufall hat mirs angethan,", "tokens": ["Der", "Zu\u00b7fall", "hat", "mirs", "an\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mir entst\u00fcrzen schier die Thr\u00e4nen,", "tokens": ["Und", "mir", "ent\u00b7st\u00fcr\u00b7zen", "schier", "die", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und bittre Wehmuth fa\u00dft mich an.", "tokens": ["Und", "bitt\u00b7re", "Weh\u00b7muth", "fa\u00dft", "mich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dann kann mich, ach, nur das erfreuen,", "tokens": ["Dann", "kann", "mich", ",", "ach", ",", "nur", "das", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "ITJ", "$,", "ADV", "PDS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df gleicher Schmerz zu dir auch spricht,", "tokens": ["Da\u00df", "glei\u00b7cher", "Schmerz", "zu", "dir", "auch", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df er sich t\u00e4glich wird erneuen \u2013", "tokens": ["Da\u00df", "er", "sich", "t\u00e4g\u00b7lich", "wird", "er\u00b7neu\u00b7en", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und dennoch, w\u00fcnsch ich dir ihn nicht.", "tokens": ["Und", "den\u00b7noch", ",", "w\u00fcnsch", "ich", "dir", "ihn", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "VVFIN", "PPER", "PPER", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}