{"dta.poem.3102": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "147.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du verachten willst, was andre vor dir dachten;", "tokens": ["Wenn", "du", "ver\u00b7ach\u00b7ten", "willst", ",", "was", "and\u00b7re", "vor", "dir", "dach\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "VMFIN", "$,", "PRELS", "PIS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie sollen, was du denkst, die nach dir denken, achten?", "tokens": ["Wie", "sol\u00b7len", ",", "was", "du", "denkst", ",", "die", "nach", "dir", "den\u00b7ken", ",", "ach\u00b7ten", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VMFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "PRELS", "APPR", "PPER", "VVINF", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ja, deinem Denken selbst kanst du kein Zutraun schenken,", "tokens": ["Ja", ",", "dei\u00b7nem", "Den\u00b7ken", "selbst", "kanst", "du", "kein", "Zu\u00b7traun", "schen\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "ADV", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du kein Zutraun hast zu andrer Denker Denken.", "tokens": ["Wenn", "du", "kein", "Zu\u00b7traun", "hast", "zu", "an\u00b7drer", "Den\u00b7ker", "Den\u00b7ken", "."], "token_info": ["word", "word", "word