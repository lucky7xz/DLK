{"textgrid.poem.59933": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcrcht nicht, da\u00df ich in das Gemeine", "tokens": ["F\u00fcrcht", "nicht", ",", "da\u00df", "ich", "in", "das", "Ge\u00b7mei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Rohe mich vertiefe,", "tokens": ["Und", "Ro\u00b7he", "mich", "ver\u00b7tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Solange ich von gutem Weine", "tokens": ["So\u00b7lan\u00b7ge", "ich", "von", "gu\u00b7tem", "Wei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und guten Witzen triefe.", "tokens": ["Und", "gu\u00b7ten", "Wit\u00b7zen", "trie\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Von manchem Liedesedelsteine", "tokens": ["Von", "man\u00b7chem", "Lie\u00b7de\u00b7se\u00b7del\u00b7stei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Glanz verborgen schliefe,", "tokens": ["Der", "Glanz", "ver\u00b7bor\u00b7gen", "schlie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn ihn der Duft von gutem Weine", "tokens": ["Wenn", "ihn", "der", "Duft", "von", "gu\u00b7tem", "Wei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht in das Dasein riefe.", "tokens": ["Nicht", "in", "das", "Da\u00b7sein", "rie\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wo bliebe der h\u00f6chste Berg, wenn seine", "tokens": ["Wo", "blie\u00b7be", "der", "h\u00f6chs\u00b7te", "Berg", ",", "wenn", "sei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "$,", "KOUS", "PPOSAT"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "H\u00f6he blo\u00df aufw\u00e4rts liefe?", "tokens": ["H\u00f6\u00b7he", "blo\u00df", "auf\u00b7w\u00e4rts", "lie\u00b7fe", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Zu F\u00fc\u00dfen wachsen ihm die Weine.", "tokens": ["Zu", "F\u00fc\u00b7\u00dfen", "wach\u00b7sen", "ihm", "die", "Wei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er h\u00e4lt sich durch die Tiefe!", "tokens": ["Er", "h\u00e4lt", "sich", "durch", "die", "Tie\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und so erkenne du auch meine", "tokens": ["Und", "so", "er\u00b7ken\u00b7ne", "du", "auch", "mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00f6he in meiner Tiefe:", "tokens": ["H\u00f6\u00b7he", "in", "mei\u00b7ner", "Tie\u00b7fe", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Solang ich sie bei gutem Weine", "tokens": ["So\u00b7lang", "ich", "sie", "bei", "gu\u00b7tem", "Wei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch guten Witz verbriefe!", "tokens": ["Durch", "gu\u00b7ten", "Witz", "ver\u00b7brie\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}