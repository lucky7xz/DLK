{"textgrid.poem.63219": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "1L: In den lederbraunen Baren", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In den lederbraunen Baren", "tokens": ["In", "den", "le\u00b7der\u00b7brau\u00b7nen", "Ba\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sitzen sie bei Drink und Vermouth.", "tokens": ["Sit\u00b7zen", "sie", "bei", "Drink", "und", "Ver\u00b7mouth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Die da werden, die da waren,", "tokens": ["Die", "da", "wer\u00b7den", ",", "die", "da", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAINF", "$,", "PRELS", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Smoking deutet Schwermut.", "tokens": ["Und", "der", "Smo\u00b7king", "deu\u00b7tet", "Schwer\u00b7mut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Manche mit entfleischten Rippen", "tokens": ["Man\u00b7che", "mit", "ent\u00b7fleischten", "Rip\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Speien Eiter in die Gl\u00e4ser,", "tokens": ["Spei\u00b7en", "Ei\u00b7ter", "in", "die", "Gl\u00e4\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4hrend ihre Finger tippen", "tokens": ["W\u00e4h\u00b7rend", "ih\u00b7re", "Fin\u00b7ger", "tip\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Takt dem goldnen Tangobl\u00e4ser.", "tokens": ["Takt", "dem", "gold\u00b7nen", "Tan\u00b7go\u00b7bl\u00e4\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was sie denken, schallt entfernter", "tokens": ["Was", "sie", "den\u00b7ken", ",", "schallt", "ent\u00b7fern\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "VVINF", "$,", "VVFIN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als die m\u00fcde Kirchturmschelle.", "tokens": ["Als", "die", "m\u00fc\u00b7de", "Kirch\u00b7turm\u00b7schel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seht: der Himmel scheint besternter", "tokens": ["Seht", ":", "der", "Him\u00b7mel", "scheint", "bes\u00b7tern\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "ART", "NN", "VVFIN", "ADJD"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Und die Erde dreht sich schnelle.", "tokens": ["Und", "die", "Er\u00b7de", "dreht", "sich", "schnel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Im entlaubten Fruktidore", "tokens": ["Im", "ent\u00b7laub\u00b7ten", "Fruk\u00b7ti\u00b7do\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00f6lbt sich Brust zur Frucht gewaltsam.", "tokens": ["W\u00f6lbt", "sich", "Brust", "zur", "Frucht", "ge\u00b7walt\u00b7sam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NN", "APPRART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsre atmenden Motore", "tokens": ["Uns\u00b7re", "at\u00b7men\u00b7den", "Mo\u00b7to\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sausen nachtw\u00e4rts unaufhaltsam.", "tokens": ["Sau\u00b7sen", "nacht\u00b7w\u00e4rts", "un\u00b7auf\u00b7halt\u00b7sam", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}