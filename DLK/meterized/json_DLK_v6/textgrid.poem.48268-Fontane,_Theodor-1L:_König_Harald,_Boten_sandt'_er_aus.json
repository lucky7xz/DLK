{"textgrid.poem.48268": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "1L: K\u00f6nig Harald, Boten sandt' er aus", "genre": "verse", "period": "N.A.", "pub_year": 1853, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nig Harald, Boten sandt' er aus", "tokens": ["K\u00f6\u00b7nig", "Ha\u00b7rald", ",", "Bo\u00b7ten", "sandt'", "er", "aus"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "NN", "VVFIN", "PPER", "APPR"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "An die K\u00fcste, die sollten erkunden,", "tokens": ["An", "die", "K\u00fcs\u00b7te", ",", "die", "soll\u00b7ten", "er\u00b7kun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "VMFIN", "VVINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ob Herzog Wilhelm und sein Heer", "tokens": ["Ob", "Her\u00b7zog", "Wil\u00b7helm", "und", "sein", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "NE", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Weg \u00fcbers Meer gefunden.", "tokens": ["Den", "Weg", "\u00fc\u00b7bers", "Meer", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und am dritten Tag, der Nebel lag", "tokens": ["Und", "am", "drit\u00b7ten", "Tag", ",", "der", "Ne\u00b7bel", "lag"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "\u00dcbers Land in breiten Schichten,", "tokens": ["\u00dc\u00b7bers", "Land", "in", "brei\u00b7ten", "Schich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da waren die Boten wieder daheim,", "tokens": ["Da", "wa\u00b7ren", "die", "Bo\u00b7ten", "wie\u00b7der", "da\u00b7heim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und der eine begann zu berichten:", "tokens": ["Und", "der", "ei\u00b7ne", "be\u00b7gann", "zu", "be\u00b7rich\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "\u00bbdie Klippe von ", "tokens": ["\u00bb", "die", "Klip\u00b7pe", "von"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "ART", "NN", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und das Meer, wohl hat es gebrandet,", "tokens": ["Und", "das", "Meer", ",", "wohl", "hat", "es", "ge\u00b7bran\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Vergebens die Brandung, vergebens der Stein,", "tokens": ["Ver\u00b7ge\u00b7bens", "die", "Bran\u00b7dung", ",", "ver\u00b7ge\u00b7bens", "der", "Stein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Seine R\u00fcstung ist wie von Silber und Gold,", "tokens": ["Sei\u00b7ne", "R\u00fcs\u00b7tung", "ist", "wie", "von", "Sil\u00b7ber", "und", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sein Antlitz ist wie von Eisen,", "tokens": ["Sein", "Ant\u00b7litz", "ist", "wie", "von", "Ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Aber sein Heer, dem schlottern die Knie,", "tokens": ["A\u00b7ber", "sein", "Heer", ",", "dem", "schlot\u00b7tern", "die", "Knie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Das wird fliehn und den R\u00fccken uns weisen.", "tokens": ["Das", "wird", "fliehn", "und", "den", "R\u00fc\u00b7cken", "uns", "wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVINF", "KON", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Seine Ritter sehn hohl und hager drein,", "tokens": ["Sei\u00b7ne", "Rit\u00b7ter", "sehn", "hohl", "und", "ha\u00b7ger", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "KON", "ADJD", "PTKVZ", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wie in m\u00f6nchisch Gr\u00fcbeln verloren,", "tokens": ["Wie", "in", "m\u00f6n\u00b7chisch", "Gr\u00fc\u00b7beln", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJD", "NN", "VVPP", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie haben nicht Kinn- und nicht Backenbart,", "tokens": ["Sie", "ha\u00b7ben", "nicht", "Kinn", "und", "nicht", "Ba\u00b7cken\u00b7bart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "TRUNC", "KON", "PTKNEG", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Im ganzen Normannenlager gibt's", "tokens": ["Im", "gan\u00b7zen", "Nor\u00b7man\u00b7nen\u00b7la\u00b7ger", "gibt's"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "NE"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nur Beten und Messesingen,", "tokens": ["Nur", "Be\u00b7ten", "und", "Mes\u00b7se\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das ganze Heer ist ein Priesterheer,", "tokens": ["Das", "gan\u00b7ze", "Heer", "ist", "ein", "Pries\u00b7ter\u00b7heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und du wirst es im Spiele bezwingen.\u00ab", "tokens": ["Und", "du", "wirst", "es", "im", "Spie\u00b7le", "be\u00b7zwin\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "APPRART", "NN", "VVINF", "$.", "$("], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.7": {"line.1": {"text": "K\u00f6nig Harald aber, er sah vor sich hin,", "tokens": ["K\u00f6\u00b7nig", "Ha\u00b7rald", "a\u00b7ber", ",", "er", "sah", "vor", "sich", "hin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "$,", "PPER", "VVFIN", "APPR", "PRF", "PTKVZ", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "In finstres Sinnen verloren,", "tokens": ["In", "finst\u00b7res", "Sin\u00b7nen", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dann sprach er: \u00bbIch wei\u00df, sie fechten wie wir,", "tokens": ["Dann", "sprach", "er", ":", "\u00bb", "Ich", "wei\u00df", ",", "sie", "fech\u00b7ten", "wie", "wir", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "PPER", "VVFIN", "$,", "PPER", "VVINF", "KOKOM", "PPER", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Obwohl sie ", "tokens": ["Ob\u00b7wohl", "sie"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}}}}}