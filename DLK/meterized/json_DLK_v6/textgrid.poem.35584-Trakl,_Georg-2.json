{"textgrid.poem.35584": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Abend s\u00e4umt die Pest ihr blau Gewand", "tokens": ["Am", "A\u00b7bend", "s\u00e4umt", "die", "Pest", "ihr", "blau", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und leise schlie\u00dft die T\u00fcr ein finstrer Gast.", "tokens": ["Und", "lei\u00b7se", "schlie\u00dft", "die", "T\u00fcr", "ein", "finst\u00b7rer", "Gast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durchs Fenster sinkt des Ahorns schwarze Last;", "tokens": ["Durchs", "Fens\u00b7ter", "sinkt", "des", "A\u00b7horns", "schwar\u00b7ze", "Last", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Knabe legt die Stirn in ihre Hand.", "tokens": ["Ein", "Kna\u00b7be", "legt", "die", "Stirn", "in", "ih\u00b7re", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Oft sinken ihre Lider b\u00f6s und schwer.", "tokens": ["Oft", "sin\u00b7ken", "ih\u00b7re", "Li\u00b7der", "b\u00f6s", "und", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Kindes H\u00e4nde rinnen durch ihr Haar", "tokens": ["Des", "Kin\u00b7des", "H\u00e4n\u00b7de", "rin\u00b7nen", "durch", "ihr", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und seine Tr\u00e4nen st\u00fcrzen hei\u00df und klar", "tokens": ["Und", "sei\u00b7ne", "Tr\u00e4\u00b7nen", "st\u00fcr\u00b7zen", "hei\u00df", "und", "klar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In ihre Augenh\u00f6hlen schwarz und leer.", "tokens": ["In", "ih\u00b7re", "Au\u00b7gen\u00b7h\u00f6h\u00b7len", "schwarz", "und", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein Nest von scharlachfarbnen Schlangen b\u00e4umt", "tokens": ["Ein", "Nest", "von", "schar\u00b7lach\u00b7farb\u00b7nen", "Schlan\u00b7gen", "b\u00e4umt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sich tr\u00e4g in ihrem aufgew\u00fchlten Scho\u00df.", "tokens": ["Sich", "tr\u00e4g", "in", "ih\u00b7rem", "auf\u00b7ge\u00b7w\u00fchl\u00b7ten", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Arme lassen ein Erstorbenes los,", "tokens": ["Die", "Ar\u00b7me", "las\u00b7sen", "ein", "E\u00b7rstor\u00b7be\u00b7nes", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Das eines Teppichs Traurigkeit ums\u00e4umt.", "tokens": ["Das", "ei\u00b7nes", "Tep\u00b7pichs", "Trau\u00b7rig\u00b7keit", "um\u00b7s\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}