{"dta.poem.21168": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Prof. G\u00e4rtner.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein G\u00e4rtner, sieh, der rauhe Harz", "tokens": ["Mein", "G\u00e4rt\u00b7ner", ",", "sieh", ",", "der", "rau\u00b7he", "Harz"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gl\u00e4nzt, wei\u00df von hohem Schnee;", "tokens": ["Gl\u00e4nzt", ",", "wei\u00df", "von", "ho\u00b7hem", "Schnee", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und von bereiften Kiefern h\u00e4ngt", "tokens": ["Und", "von", "be\u00b7reif\u00b7ten", "Kie\u00b7fern", "h\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kandirtes Eis herab!", "tokens": ["Kan\u00b7dir\u00b7tes", "Eis", "her\u00b7ab", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Ocker rauschet stiller fort,", "tokens": ["Die", "O\u00b7cker", "rau\u00b7schet", "stil\u00b7ler", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die blaue Well\u2019 erstarrt;", "tokens": ["Die", "blau\u00b7e", "Well'", "er\u00b7starrt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und \u00fcber kahle Felder f\u00e4hrt", "tokens": ["Und", "\u00fc\u00b7ber", "kah\u00b7le", "Fel\u00b7der", "f\u00e4hrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der flockenreiche Sturm.", "tokens": ["Der", "flo\u00b7cken\u00b7rei\u00b7che", "Sturm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Komm an den freundlichen Camin!", "tokens": ["Komm", "an", "den", "freund\u00b7li\u00b7chen", "Ca\u00b7min", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit unsparsamer Hand", "tokens": ["Mit", "un\u00b7spar\u00b7sa\u00b7mer", "Hand"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Th\u00fcrm ich, den jungen Buchenwald,", "tokens": ["Th\u00fcrm", "ich", ",", "den", "jun\u00b7gen", "Bu\u00b7chen\u00b7wald", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu hellen Flammen auf.", "tokens": ["Zu", "hel\u00b7len", "Flam\u00b7men", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die reine Quelle brauset schon", "tokens": ["Die", "rei\u00b7ne", "Quel\u00b7le", "brau\u00b7set", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jm ehernen Gef\u00e4\u00df.", "tokens": ["Jm", "e\u00b7her\u00b7nen", "Ge\u00b7f\u00e4\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die g\u00fcldne Frucht Hesperiens", "tokens": ["Die", "g\u00fcld\u00b7ne", "Frucht", "Hes\u00b7pe\u00b7ri\u00b7ens"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Saugt hellen Zucker ein.", "tokens": ["Saugt", "hel\u00b7len", "Zu\u00b7cker", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und nun dampft aus dem irdnen Meer", "tokens": ["Und", "nun", "dampft", "aus", "dem", "ird\u00b7nen", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Der k\u00f6nigliche Punsch.", "tokens": ["Der", "k\u00f6\u00b7nig\u00b7li\u00b7che", "Pun\u00b7sch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Heil, England, dir! Heil dir! o Mann,", "tokens": ["Heil", ",", "En\u00b7gland", ",", "dir", "!", "Heil", "dir", "!", "o", "Mann", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "PPER", "$.", "NN", "PPER", "$.", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der uns den Punsch erfand!", "tokens": ["Der", "uns", "den", "Pun\u00b7sch", "er\u00b7fand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Jetzt lachen wir des Winters Wuth,", "tokens": ["Jetzt", "la\u00b7chen", "wir", "des", "Win\u00b7ters", "Wuth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der um die Fenster st\u00fcrmt;", "tokens": ["Der", "um", "die", "Fens\u00b7ter", "st\u00fcrmt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sprechen Weisheit, hochentz\u00fcckt,", "tokens": ["Und", "spre\u00b7chen", "Weis\u00b7heit", ",", "ho\u00b7chent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Indem die Schale raucht.", "tokens": ["In\u00b7dem", "die", "Scha\u00b7le", "raucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}