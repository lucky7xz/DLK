{"dta.poem.19184": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sieh die Verfassungen der V\u00f6lkerstaaten hie,", "tokens": ["Sieh", "die", "Ver\u00b7fas\u00b7sun\u00b7gen", "der", "V\u00f6l\u00b7ker\u00b7staa\u00b7ten", "hie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Ameisenrepublik und Bienenmonarchie.", "tokens": ["A\u00b7mei\u00b7sen\u00b7re\u00b7pub\u00b7lik", "und", "Bie\u00b7nen\u00b7mo\u00b7nar\u00b7chie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die fliegen in der Luft, die kriechen an der Erde;", "tokens": ["Die", "flie\u00b7gen", "in", "der", "Luft", ",", "die", "krie\u00b7chen", "an", "der", "Er\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die sammeln Bl\u00fctenduft, die K\u00f6rner mit Beschwerde.", "tokens": ["Die", "sam\u00b7meln", "Bl\u00fc\u00b7ten\u00b7duft", ",", "die", "K\u00f6r\u00b7ner", "mit", "Be\u00b7schwer\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dort waltet ein Gesetz, und hier ein Oberhaupt,", "tokens": ["Dort", "wal\u00b7tet", "ein", "Ge\u00b7setz", ",", "und", "hier", "ein", "O\u00b7ber\u00b7haupt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier wird geschaut, was dort unsichtbar wird geglaubt.", "tokens": ["Hier", "wird", "ge\u00b7schaut", ",", "was", "dort", "un\u00b7sicht\u00b7bar", "wird", "ge\u00b7glaubt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Bienenstaat ist hin, wann stirbt die K\u00f6niginn;", "tokens": ["Der", "Bie\u00b7nen\u00b7staat", "ist", "hin", ",", "wann", "stirbt", "die", "K\u00f6\u00b7ni\u00b7ginn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "PWAV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ameisenreich besteht, unsterblich ist sein Sinn.", "tokens": ["A\u00b7mei\u00b7sen\u00b7reich", "be\u00b7steht", ",", "uns\u00b7terb\u00b7lich", "ist", "sein", "Sinn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Mit Andacht sammle du in reinlichen Geschirren", "tokens": ["Mit", "An\u00b7dacht", "samm\u00b7le", "du", "in", "rein\u00b7li\u00b7chen", "Ge\u00b7schir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Bienen Honigseim, und von Ameisen Mirren.", "tokens": ["Von", "Bie\u00b7nen", "Ho\u00b7ni\u00b7gseim", ",", "und", "von", "A\u00b7mei\u00b7sen", "Mir\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}}}}}