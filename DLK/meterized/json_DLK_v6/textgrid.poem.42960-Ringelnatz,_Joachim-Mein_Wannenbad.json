{"textgrid.poem.42960": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Mein Wannenbad", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es mu\u00df wieder mal sein.", "tokens": ["Es", "mu\u00df", "wie\u00b7der", "mal", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Also: Ich steige hinein", "tokens": ["Al\u00b7so", ":", "Ich", "stei\u00b7ge", "hin\u00b7ein"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$.", "PPER", "VVFIN", "PTKVZ"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "In zirka zwei Kubikmeter See.", "tokens": ["In", "zir\u00b7ka", "zwei", "Ku\u00b7bik\u00b7me\u00b7ter", "See", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "CARD", "NN", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Bis \u00fcbern Bauch tut es weh.", "tokens": ["Bis", "\u00fc\u00b7bern", "Bauch", "tut", "es", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Das H\u00e4hnchen pl\u00e4tschert in schamlosem Ton,", "tokens": ["Das", "H\u00e4hn\u00b7chen", "pl\u00e4t\u00b7schert", "in", "scham\u00b7lo\u00b7sem", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ich atme und schnupfe den Fichtenozon,", "tokens": ["Ich", "at\u00b7me", "und", "schnup\u00b7fe", "den", "Fich\u00b7te\u00b7no\u00b7zon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Beobachte, wie die Str\u00f6mung l\u00e4uft,", "tokens": ["Be\u00b7ob\u00b7ach\u00b7te", ",", "wie", "die", "Str\u00f6\u00b7mung", "l\u00e4uft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Wie dann clam, langsam mein Schwamm sich bes\u00e4uft.", "tokens": ["Wie", "dann", "clam", ",", "lang\u00b7sam", "mein", "Schwamm", "sich", "be\u00b7s\u00e4uft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "$,", "ADJD", "PPOSAT", "NN", "PRF", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Und ich ers\u00e4ufe, um allen D\u00fcrsten", "tokens": ["Und", "ich", "er\u00b7s\u00e4u\u00b7fe", ",", "um", "al\u00b7len", "D\u00fcrs\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUI", "PIAT", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Gerecht zu werden, verschiedene B\u00fcrsten.", "tokens": ["Ge\u00b7recht", "zu", "wer\u00b7den", ",", "ver\u00b7schie\u00b7de\u00b7ne", "B\u00fcrs\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VAINF", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Ich seife, schrubbe, ich sp\u00fcle froh.", "tokens": ["Ich", "sei\u00b7fe", ",", "schrub\u00b7be", ",", "ich", "sp\u00fc\u00b7le", "froh", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Ich suche auf Ausguck", "tokens": ["Ich", "su\u00b7che", "auf", "Aus\u00b7guck"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Vergebens nach einem ertrinkenden Floh,", "tokens": ["Ver\u00b7ge\u00b7bens", "nach", "ei\u00b7nem", "er\u00b7trin\u00b7ken\u00b7den", "Floh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.14": {"text": "Doch fort ist der Hausjuck.", "tokens": ["Doch", "fort", "ist", "der", "Haus\u00b7juck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "VAFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Ich lehne mich weit und tief zur\u00fcck,", "tokens": ["Ich", "leh\u00b7ne", "mich", "weit", "und", "tief", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Genie\u00dfe schaukelndes M\u00f6wengl\u00fcck.", "tokens": ["Ge\u00b7nie\u00b7\u00dfe", "schau\u00b7keln\u00b7des", "M\u00f6\u00b7wen\u00b7gl\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Da taucht aus der blinkenden Fl\u00e4che, wie", "tokens": ["Da", "taucht", "aus", "der", "blin\u00b7ken\u00b7den", "Fl\u00e4\u00b7che", ",", "wie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "PWAV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.18": {"text": "Eine Robinsoninsel, pl\u00f6tzlich ein Knie;", "tokens": ["Ei\u00b7ne", "Ro\u00b7bin\u00b7so\u00b7nin\u00b7sel", ",", "pl\u00f6tz\u00b7lich", "ein", "Knie", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "ART", "NN", "$."], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.19": {"text": "Dann \u2013 massig \u2013 mein Bauch \u2013 eines Walfisches Speck.", "tokens": ["Dann", "\u2013", "mas\u00b7sig", "\u2013", "mein", "Bauch", "\u2013", "ei\u00b7nes", "Wal\u00b7fi\u00b7sches", "Speck", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "ADJD", "$(", "PPOSAT", "NN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.20": {"text": "Und nun auf Wellen (nach meinem Belieben", "tokens": ["Und", "nun", "auf", "Wel\u00b7len", "(", "nach", "mei\u00b7nem", "Be\u00b7lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN", "$(", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Herangezogen, davongetrieben),", "tokens": ["Her\u00b7an\u00b7ge\u00b7zo\u00b7gen", ",", "da\u00b7von\u00b7ge\u00b7trie\u00b7ben", ")", ","], "token_info": ["word", "punct", "word", "punct", "punct"], "pos": ["NN", "$,", "PAV", "$(", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Als Wogenschaum spielt mein eigenster Dreck.", "tokens": ["Als", "Wo\u00b7gen\u00b7schaum", "spielt", "mein", "ei\u00b7gens\u00b7ter", "Dreck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Und da auf dem Gipfel neptunischer Lust,", "tokens": ["Und", "da", "auf", "dem", "Gip\u00b7fel", "nep\u00b7tu\u00b7ni\u00b7scher", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Klebt sich der Waschlappen mir an die Brust.", "tokens": ["Klebt", "sich", "der", "Waschlap\u00b7pen", "mir", "an", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.25": {"text": "Brust, Wanne und W\u00e4nde m\u00f6chten zerspringen,", "tokens": ["Brust", ",", "Wan\u00b7ne", "und", "W\u00e4n\u00b7de", "m\u00f6ch\u00b7ten", "zer\u00b7sprin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "KON", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Denn ich beginne nun, dr\u00f6hnend zu singen", "tokens": ["Denn", "ich", "be\u00b7gin\u00b7ne", "nun", ",", "dr\u00f6h\u00b7nend", "zu", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "Die allerschwersten Opernkaliber.", "tokens": ["Die", "al\u00b7ler\u00b7schwers\u00b7ten", "O\u00b7pern\u00b7ka\u00b7li\u00b7ber", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Das Thermometer steigt \u00fcber Fieber,", "tokens": ["Das", "Ther\u00b7mo\u00b7me\u00b7ter", "steigt", "\u00fc\u00b7ber", "Fie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Das Feuer braust, und der Ofen gl\u00fcht,", "tokens": ["Das", "Feu\u00b7er", "braust", ",", "und", "der", "O\u00b7fen", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Aber ich bin schon so abgebr\u00fcht,", "tokens": ["A\u00b7ber", "ich", "bin", "schon", "so", "ab\u00b7ge\u00b7br\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.31": {"text": "Da\u00df mich gelegentlich Explosionen \u2013", "tokens": ["Da\u00df", "mich", "ge\u00b7le\u00b7gent\u00b7lich", "Ex\u00b7plo\u00b7si\u00b7o\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "NN", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.32": {"text": "\u2013 Wenn's an mir vorbeigeht \u2013 \u2013", "tokens": ["\u2013", "Wenn's", "an", "mir", "vor\u00b7bei\u00b7geht", "\u2013", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PIS", "APPR", "PPER", "VVFIN", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Erfreun, weil manchmal dabei was entzweigeht,", "tokens": ["Er\u00b7freun", ",", "weil", "manch\u00b7mal", "da\u00b7bei", "was", "ent\u00b7zwei\u00b7geht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ADV", "PAV", "PWS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Was Leute betrifft, die unter mir wohnen.", "tokens": ["Was", "Leu\u00b7te", "be\u00b7tr\u00b7ifft", ",", "die", "un\u00b7ter", "mir", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$,", "PRELS", "APPR", "PPER", "VVINF", "$."], "meter": "-+--++-+--+-", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Ich lasse an verschiedenen Stellen", "tokens": ["Ich", "las\u00b7se", "an", "ver\u00b7schie\u00b7de\u00b7nen", "Stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.36": {"text": "Nach meinem Wunsch flinke Bl\u00e4schen entquellen,", "tokens": ["Nach", "mei\u00b7nem", "Wunsch", "flin\u00b7ke", "Bl\u00e4\u00b7schen", "ent\u00b7quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NN", "VVINF", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.37": {"text": "Erhebe mich mannhaft ins Duschengebraus.", "tokens": ["Er\u00b7he\u00b7be", "mich", "mann\u00b7haft", "ins", "Du\u00b7schen\u00b7ge\u00b7braus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.38": {"text": "Ich b\u00fcck mich. Der St\u00f6psel r\u00fclpst sich hinaus,", "tokens": ["Ich", "b\u00fcck", "mich", ".", "Der", "St\u00f6p\u00b7sel", "r\u00fclpst", "sich", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "ART", "NN", "VVFIN", "PRF", "APZR", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.39": {"text": "Und w\u00e4hrend die Fluten sich gurgelnd verschl\u00fcrfen,", "tokens": ["Und", "w\u00e4h\u00b7rend", "die", "Flu\u00b7ten", "sich", "gur\u00b7gelnd", "ver\u00b7schl\u00fcr\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.40": {"text": "Spannt mich das Bewu\u00dftsein wie himmlischer Zauber,", "tokens": ["Spannt", "mich", "das", "Be\u00b7wu\u00df\u00b7tsein", "wie", "himm\u00b7li\u00b7scher", "Zau\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.41": {"text": "Mich \u00fcberall heute zeigen zu d\u00fcrfen,", "tokens": ["Mich", "\u00fc\u00b7be\u00b7rall", "heu\u00b7te", "zei\u00b7gen", "zu", "d\u00fcr\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVINF", "PTKZU", "VMINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.42": {"text": "Denn ich bin sauber. \u2013", "tokens": ["Denn", "ich", "bin", "sau\u00b7ber", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}}}}}