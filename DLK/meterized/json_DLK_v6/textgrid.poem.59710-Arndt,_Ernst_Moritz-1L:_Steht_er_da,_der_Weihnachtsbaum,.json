{"textgrid.poem.59710": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Steht er da, der Weihnachtsbaum,", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Steht er da, der Weihnachtsbaum,", "tokens": ["Steht", "er", "da", ",", "der", "Weih\u00b7nachts\u00b7baum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ein bunter, goldner Traum,", "tokens": ["Wie", "ein", "bun\u00b7ter", ",", "gold\u00b7ner", "Traum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Spiegelt Unschuldkindergl\u00fcck,", "tokens": ["Spie\u00b7gelt", "Un\u00b7schuld\u00b7kin\u00b7der\u00b7gl\u00fcck", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "All sein Paradies zur\u00fcck.", "tokens": ["All", "sein", "Pa\u00b7ra\u00b7dies", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wir schaun und denken dann,", "tokens": ["Und", "wir", "schaun", "und", "den\u00b7ken", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "KON", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie uns heut das Heil begann,", "tokens": ["Wie", "uns", "heut", "das", "Heil", "be\u00b7gann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie das Kindlein Jesus Christ", "tokens": ["Wie", "das", "Kin\u00b7dlein", "Je\u00b7sus", "Christ"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NE", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Heut zur Welt geboren ist;", "tokens": ["Heut", "zur", "Welt", "ge\u00b7bo\u00b7ren", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie das Kind von Himmelsart", "tokens": ["Wie", "das", "Kind", "von", "Him\u00b7mel\u00b7sart"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lag auf Stroh und Halmen hart,", "tokens": ["Lag", "auf", "Stroh", "und", "Hal\u00b7men", "hart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie der Menschheit Hort und Trost", "tokens": ["Wie", "der", "Menschheit", "Hort", "und", "Trost"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NE", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Erdenelend hat erlost.", "tokens": ["Er\u00b7de\u00b7ne\u00b7lend", "hat", "er\u00b7lost", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Also stehn und schauen wir", "tokens": ["Al\u00b7so", "stehn", "und", "schau\u00b7en", "wir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "KON", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gottes Lust und Gnade hier:", "tokens": ["Got\u00b7tes", "Lust", "und", "Gna\u00b7de", "hier", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was uns in dem Kindlein zart", "tokens": ["Was", "uns", "in", "dem", "Kin\u00b7dlein", "zart"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Alles heut geboren ward.", "tokens": ["Al\u00b7les", "heut", "ge\u00b7bo\u00b7ren", "ward", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Bl\u00fch' denn, leuchte, goldner Baum,", "tokens": ["Bl\u00fch'", "denn", ",", "leuch\u00b7te", ",", "gold\u00b7ner", "Baum", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Erdentraum und Himmelstraum,", "tokens": ["Er\u00b7den\u00b7traum", "und", "Him\u00b7mel\u00b7straum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bl\u00fch und leucht' in Ewigkeit", "tokens": ["Bl\u00fch", "und", "leucht'", "in", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die arme Zeitlichkeit!", "tokens": ["Durch", "die", "ar\u00b7me", "Zeit\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sei uns Bild und sei uns Schein,", "tokens": ["Sei", "uns", "Bild", "und", "sei", "uns", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "KON", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df wir sollen fr\u00f6hlich sein,", "tokens": ["Da\u00df", "wir", "sol\u00b7len", "fr\u00f6h\u00b7lich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fr\u00f6hlich durch den s\u00fc\u00dfen Christ,", "tokens": ["Fr\u00f6h\u00b7lich", "durch", "den", "s\u00fc\u00b7\u00dfen", "Christ", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der des Lebens Leuchte ist.", "tokens": ["Der", "des", "Le\u00b7bens", "Leuch\u00b7te", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sei uns Bild und sei uns Schein,", "tokens": ["Sei", "uns", "Bild", "und", "sei", "uns", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "KON", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df wir sollen tapfer sein", "tokens": ["Da\u00df", "wir", "sol\u00b7len", "tap\u00b7fer", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "ADJD", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf des Lebens Pilgerbahn,", "tokens": ["Auf", "des", "Le\u00b7bens", "Pil\u00b7ger\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00e4mpfend gegen Lug und Wahn.", "tokens": ["K\u00e4mp\u00b7fend", "ge\u00b7gen", "Lug", "und", "Wahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NE", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Sei uns Bild und sei uns Schein,", "tokens": ["Sei", "uns", "Bild", "und", "sei", "uns", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "KON", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df wir sollen heilig sein,", "tokens": ["Da\u00df", "wir", "sol\u00b7len", "hei\u00b7lig", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rein wie Licht und himmelklar,", "tokens": ["Rein", "wie", "Licht", "und", "him\u00b7mel\u00b7klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie das Kindlein Jesus war.", "tokens": ["Wie", "das", "Kin\u00b7dlein", "Je\u00b7sus", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NE", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}