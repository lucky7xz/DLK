{"textgrid.poem.59536": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lehr' mich beten,", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lehr' mich beten,", "tokens": ["Lehr'", "mich", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Gott der Herrlichkeit,", "tokens": ["Gott", "der", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Kindlich vor dich treten,", "tokens": ["Kind\u00b7lich", "vor", "dich", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wie das Herz gebeut.", "tokens": ["Wie", "das", "Herz", "ge\u00b7beut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Mach' unschuldig,", "tokens": ["Mach'", "un\u00b7schul\u00b7dig", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Mache fromm dein Kind,", "tokens": ["Ma\u00b7che", "fromm", "dein", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Denn die Welt ist schuldig,", "tokens": ["Denn", "die", "Welt", "ist", "schul\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "\u00dcbervoll voll S\u00fcnd'.", "tokens": ["\u00dc\u00b7ber\u00b7voll", "voll", "S\u00fcnd'", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Nach dem Bilde", "tokens": ["Nach", "dem", "Bil\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Schufest du mich dein,", "tokens": ["Schu\u00b7fest", "du", "mich", "dein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PPOSAT", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Vater aller Milde,", "tokens": ["Va\u00b7ter", "al\u00b7ler", "Mil\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "La\u00df mich heilig sein!", "tokens": ["La\u00df", "mich", "hei\u00b7lig", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Nimm die Erde,", "tokens": ["Nimm", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Nimm die Schuld von mir!", "tokens": ["Nimm", "die", "Schuld", "von", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df ich Engel werde,", "tokens": ["Da\u00df", "ich", "En\u00b7gel", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "VAFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wohne du in mir!", "tokens": ["Woh\u00b7ne", "du", "in", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "O Gedanke!", "tokens": ["O", "Ge\u00b7dan\u00b7ke", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Himmelschein voll Licht!", "tokens": ["Him\u00b7mel\u00b7schein", "voll", "Licht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Erd' und Himmel wanke,", "tokens": ["Erd'", "und", "Him\u00b7mel", "wan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Gott verl\u00e4\u00dft mich nicht.", "tokens": ["Gott", "ver\u00b7l\u00e4\u00dft", "mich", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}