{"textgrid.poem.41257": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Philosophen. Redner", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Weisen von Stagir entehret eine Metze:", "tokens": ["Den", "Wei\u00b7sen", "von", "Sta\u00b7gir", "en\u00b7teh\u00b7ret", "ei\u00b7ne", "Met\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Demosthenes spricht als ein Held;", "tokens": ["De\u00b7mos\u00b7the\u00b7nes", "spricht", "als", "ein", "Held", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Doch er verl\u00e4\u00dft Schild, Schlacht und Feld:", "tokens": ["Doch", "er", "ver\u00b7l\u00e4\u00dft", "Schild", ",", "Schlacht", "und", "Feld", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Harpalus Geschenk ersticket sein Geschw\u00e4tze;", "tokens": ["Und", "Har\u00b7pa\u00b7lus", "Ge\u00b7schenk", "er\u00b7sti\u00b7cket", "sein", "Ge\u00b7schw\u00e4t\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein Diogen verf\u00e4lscht das Geld:", "tokens": ["Ein", "Dio\u00b7gen", "ver\u00b7f\u00e4lscht", "das", "Geld", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ein Seneca verdammt und sammlet Sch\u00e4tze.", "tokens": ["Ein", "Se\u00b7ne\u00b7ca", "ver\u00b7dammt", "und", "samm\u00b7let", "Sch\u00e4t\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVPP", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das ist der Lehrer Art; das ist der Lauf der Welt.", "tokens": ["Das", "ist", "der", "Leh\u00b7rer", "Art", ";", "das", "ist", "der", "Lauf", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$.", "PDS", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Erbauliche Gesetze,", "tokens": ["Er\u00b7bau\u00b7li\u00b7che", "Ge\u00b7set\u00b7ze", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Die ihr Gebieter selbst nicht h\u00e4lt!", "tokens": ["Die", "ihr", "Ge\u00b7bie\u00b7ter", "selbst", "nicht", "h\u00e4lt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}