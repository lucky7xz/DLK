{"textgrid.poem.58285": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "323. Im Geist mu\u00df man Gott suchen", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott ist ein Geist, drum wer ihn finden will,", "tokens": ["Gott", "ist", "ein", "Geist", ",", "drum", "wer", "ihn", "fin\u00b7den", "will", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "PAV", "PWS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mu\u00df auch im Geist, nicht in den Sinnen, leben;", "tokens": ["Mu\u00df", "auch", "im", "Geist", ",", "nicht", "in", "den", "Sin\u00b7nen", ",", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "ADV", "APPRART", "NN", "$,", "PTKNEG", "APPR", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df alles da und halt dich ihm nur still,", "tokens": ["La\u00df", "al\u00b7les", "da", "und", "halt", "dich", "ihm", "nur", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIS", "ADV", "KON", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So ist er nah und wird sich dir bald geben!", "tokens": ["So", "ist", "er", "nah", "und", "wird", "sich", "dir", "bald", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "VAFIN", "PRF", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}