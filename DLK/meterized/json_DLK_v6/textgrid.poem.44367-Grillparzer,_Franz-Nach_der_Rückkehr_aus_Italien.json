{"textgrid.poem.44367": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Nach der R\u00fcckkehr aus Italien", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So hab ich dich gesehn, genossen,", "tokens": ["So", "hab", "ich", "dich", "ge\u00b7sehn", ",", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du Land, wo Myrt und Lorbeer weht,", "tokens": ["Du", "Land", ",", "wo", "Myrt", "und", "Lor\u00b7beer", "weht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PWAV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Sch\u00f6nen Heimat und des Gro\u00dfen,", "tokens": ["Des", "Sch\u00f6\u00b7nen", "Hei\u00b7mat", "und", "des", "Gro\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo Lebenskeim aus Gr\u00e4bern sprossen,", "tokens": ["Wo", "Le\u00b7bens\u00b7keim", "aus", "Gr\u00e4\u00b7bern", "spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Des Tr\u00e4umers Traum verwirklicht steht.", "tokens": ["Des", "Tr\u00e4u\u00b7mers", "Traum", "ver\u00b7wirk\u00b7licht", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}