{"textgrid.poem.39089": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den man t\u00fcckisch uns verboten,", "tokens": ["Den", "man", "t\u00fc\u00b7ckisch", "uns", "ver\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "den die H\u00e4scher uns erschlagen,", "tokens": ["den", "die", "H\u00e4\u00b7scher", "uns", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "den mit Tr\u00e4nen wir beklagen,", "tokens": ["den", "mit", "Tr\u00e4\u00b7nen", "wir", "be\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "dieses Glas dem gro\u00dfen Toten!", "tokens": ["die\u00b7ses", "Glas", "dem", "gro\u00b7\u00dfen", "To\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da\u00df zu Nacht, bei Mondenscheine,", "tokens": ["Da\u00df", "zu", "Nacht", ",", "bei", "Mon\u00b7den\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "$,", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "jenem Pascha, der es wagte,", "tokens": ["je\u00b7nem", "Pa\u00b7scha", ",", "der", "es", "wag\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "der die Freude uns verjagte,", "tokens": ["der", "die", "Freu\u00b7de", "uns", "ver\u00b7jag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "da\u00df sein Schatten ihm erscheine!", "tokens": ["da\u00df", "sein", "Schat\u00b7ten", "ihm", "er\u00b7schei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nicht, wie wir ihn lebend sahen,", "tokens": ["Nicht", ",", "wie", "wir", "ihn", "le\u00b7bend", "sa\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PWAV", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "rebumkr\u00e4nzt die schlanken Lenden,", "tokens": ["re\u00b7bum\u00b7kr\u00e4nzt", "die", "schlan\u00b7ken", "Len\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "einen Humpen in den H\u00e4nden,", "tokens": ["ei\u00b7nen", "Hum\u00b7pen", "in", "den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "nein, als Schreckbild soll er nahen!", "tokens": ["nein", ",", "als", "Schreck\u00b7bild", "soll", "er", "na\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "NN", "VMFIN", "PPER", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Seine Glocken, seine Schellen,", "tokens": ["Sei\u00b7ne", "Glo\u00b7cken", ",", "sei\u00b7ne", "Schel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Festmusik den klugen Toren,", "tokens": ["Fest\u00b7mu\u00b7sik", "den", "klu\u00b7gen", "To\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "sollen in des Pascha Ohren", "tokens": ["sol\u00b7len", "in", "des", "Pa\u00b7scha", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "hell wie Aufruhrglocken gellen!", "tokens": ["hell", "wie", "Auf\u00b7ruhr\u00b7glo\u00b7cken", "gel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und die Pritsche, jene flinke,", "tokens": ["Und", "die", "Prit\u00b7sche", ",", "je\u00b7ne", "flin\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "die er f\u00fchrte zum Entz\u00fccken,", "tokens": ["die", "er", "f\u00fchr\u00b7te", "zum", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df sie vor des Pascha Blicken", "tokens": ["da\u00df", "sie", "vor", "des", "Pa\u00b7scha", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "drohend, wie ein Dolch ihm blinke!", "tokens": ["dro\u00b7hend", ",", "wie", "ein", "Dolch", "ihm", "blin\u00b7ke", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Also w\u00e4hrend wir hier sitzen,", "tokens": ["Al\u00b7so", "w\u00e4h\u00b7rend", "wir", "hier", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "bei Gesang und Wein vereinigt,", "tokens": ["bei", "Ge\u00b7sang", "und", "Wein", "ver\u00b7ei\u00b7nigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "von Gespenstern er gepeinigt,", "tokens": ["von", "Ge\u00b7spens\u00b7tern", "er", "ge\u00b7pei\u00b7nigt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "soll er \u00e4chzen, soll er schwitzen:", "tokens": ["soll", "er", "\u00e4ch\u00b7zen", ",", "soll", "er", "schwit\u00b7zen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "bis er endlich wird genesen", "tokens": ["bis", "er", "end\u00b7lich", "wird", "ge\u00b7ne\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und bekennen wird, verwundert,", "tokens": ["und", "be\u00b7ken\u00b7nen", "wird", ",", "ver\u00b7wun\u00b7dert", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVINF", "VAFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df im neunzehnten Jahrhundert", "tokens": ["da\u00df", "im", "neun\u00b7zehn\u00b7ten", "Jahr\u00b7hun\u00b7dert"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "er der einz'ge Narr gewesen.", "tokens": ["er", "der", "einz'\u00b7ge", "Narr", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}