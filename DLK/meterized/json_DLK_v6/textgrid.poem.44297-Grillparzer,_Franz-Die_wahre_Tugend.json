{"textgrid.poem.44297": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Die wahre Tugend", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es lebt einmal in niedrer H\u00fctte", "tokens": ["Es", "lebt", "ein\u00b7mal", "in", "nie\u00b7drer", "H\u00fct\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Klausner im Ardennerwald,", "tokens": ["Ein", "Klaus\u00b7ner", "im", "Ar\u00b7den\u00b7ner\u00b7wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Von dessen Ruhm und strenger Sitte", "tokens": ["Von", "des\u00b7sen", "Ruhm", "und", "stren\u00b7ger", "Sit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ringsum das ganze Land erschallt'.", "tokens": ["Ring\u00b7sum", "das", "gan\u00b7ze", "Land", "er\u00b7schallt'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Er betete bei Nacht und Tage,", "tokens": ["Er", "be\u00b7te\u00b7te", "bei", "Nacht", "und", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Mahl bestand aus schlechtem Brot,", "tokens": ["Sein", "Mahl", "be\u00b7stand", "aus", "schlech\u00b7tem", "Brot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er rettete, so geht die Sage,", "tokens": ["Er", "ret\u00b7te\u00b7te", ",", "so", "geht", "die", "Sa\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gar oft das Land aus Pest und Not.", "tokens": ["Gar", "oft", "das", "Land", "aus", "Pest", "und", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Einst, als ein Frost aus rauhen L\u00fcften", "tokens": ["Einst", ",", "als", "ein", "Frost", "aus", "rau\u00b7hen", "L\u00fcf\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich niedersenkt' auf die Natur,", "tokens": ["Sich", "nie\u00b7der\u00b7senkt'", "auf", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dr\u00fcckt Mi\u00dfwachs die erstorbnen Triften,", "tokens": ["Dr\u00fcckt", "Mi\u00df\u00b7wachs", "die", "ers\u00b7torb\u00b7nen", "Trif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und Mangel jede Kreatur.", "tokens": ["Und", "Man\u00b7gel", "je\u00b7de", "Kre\u00b7a\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und auch des Eremiten Schwelle", "tokens": ["Und", "auch", "des", "E\u00b7re\u00b7mi\u00b7ten", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verschont der grause Hunger nicht,", "tokens": ["Ver\u00b7schont", "der", "grau\u00b7se", "Hun\u00b7ger", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er grinst auch in die enge Zelle", "tokens": ["Er", "grinst", "auch", "in", "die", "en\u00b7ge", "Zel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Mit abgezehrtem Angesicht.", "tokens": ["Mit", "ab\u00b7ge\u00b7zehr\u00b7tem", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Alte lenkt nach jenen H\u00fctten,", "tokens": ["Der", "Al\u00b7te", "lenkt", "nach", "je\u00b7nen", "H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihn gepflegt, den matten Lauf,", "tokens": ["Die", "ihn", "ge\u00b7pflegt", ",", "den", "mat\u00b7ten", "Lauf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch pl\u00f6tzlich h\u00e4lt in seinen Schritten", "tokens": ["Doch", "pl\u00f6tz\u00b7lich", "h\u00e4lt", "in", "sei\u00b7nen", "Schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn Hunger und Ermattung auf.", "tokens": ["Ihn", "Hun\u00b7ger", "und", "Er\u00b7mat\u00b7tung", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Von Froste starren seine Glieder,", "tokens": ["Von", "Fros\u00b7te", "star\u00b7ren", "sei\u00b7ne", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An eine nahe Eiche lehnt", "tokens": ["An", "ei\u00b7ne", "na\u00b7he", "Ei\u00b7che", "lehnt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er seinen Leib und st\u00fcrzet nieder", "tokens": ["Er", "sei\u00b7nen", "Leib", "und", "st\u00fcr\u00b7zet", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "KON", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und \u00e4chzet an der Erd und st\u00f6hnt.", "tokens": ["Und", "\u00e4ch\u00b7zet", "an", "der", "Erd", "und", "st\u00f6hnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch sieh! Mit gr\u00e4\u00dflicher Geb\u00e4rde", "tokens": ["Doch", "sieh", "!", "Mit", "gr\u00e4\u00df\u00b7li\u00b7cher", "Ge\u00b7b\u00e4r\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Naht nun ein Weib, h\u00f6rt sein Geschrei,", "tokens": ["Naht", "nun", "ein", "Weib", ",", "h\u00f6rt", "sein", "Ge\u00b7schrei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erblickt den Armen auf der Erde", "tokens": ["Er\u00b7blickt", "den", "Ar\u00b7men", "auf", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und eilet schnell zu Hilf herbei.", "tokens": ["Und", "ei\u00b7let", "schnell", "zu", "Hilf", "her\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der Alte st\u00f6hnt: Ach, hab Erbarmen!", "tokens": ["Der", "Al\u00b7te", "st\u00f6hnt", ":", "Ach", ",", "hab", "Er\u00b7bar\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ITJ", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur einen kleinen Bissen Brot!", "tokens": ["Nur", "ei\u00b7nen", "klei\u00b7nen", "Bis\u00b7sen", "Brot", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist der letzte, in mir Armen", "tokens": ["Es", "ist", "der", "letz\u00b7te", ",", "in", "mir", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "APPR", "PPER", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00fchlt schon der martervollste Tod.", "tokens": ["W\u00fchlt", "schon", "der", "mar\u00b7ter\u00b7volls\u00b7te", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ich, Armer, sollte Brot dir geben?", "tokens": ["Ich", ",", "Ar\u00b7mer", ",", "soll\u00b7te", "Brot", "dir", "ge\u00b7ben", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VMFIN", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ruft sie, von herben Tr\u00e4nen schwer", "tokens": ["Ruft", "sie", ",", "von", "her\u00b7ben", "Tr\u00e4\u00b7nen", "schwer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Rollt hier ihr Blick, bei meinem Leben!", "tokens": ["Rollt", "hier", "ihr", "Blick", ",", "bei", "mei\u00b7nem", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich habe nur dies St\u00fcckchen mehr.", "tokens": ["Ich", "ha\u00b7be", "nur", "dies", "St\u00fcck\u00b7chen", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PDS", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Mit diesem will ich mich noch laben,", "tokens": ["Mit", "die\u00b7sem", "will", "ich", "mich", "noch", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Totenmahl soll es mir sein. \u2013", "tokens": ["Das", "To\u00b7ten\u00b7mahl", "soll", "es", "mir", "sein", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PPER", "VAINF", "$.", "$("], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Doch, Alter, nein, du sollst es haben,", "tokens": ["Doch", ",", "Al\u00b7ter", ",", "nein", ",", "du", "sollst", "es", "ha\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "PTKANT", "$,", "PPER", "VMFIN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hier, Lieber! Nimm es, es ist dein!", "tokens": ["Hier", ",", "Lie\u00b7ber", "!", "Nimm", "es", ",", "es", "ist", "dein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "$.", "NE", "PPER", "$,", "PPER", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ihr Busen pocht in lauten Schl\u00e4gen,", "tokens": ["Ihr", "Bu\u00b7sen", "pocht", "in", "lau\u00b7ten", "Schl\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und mit verzweiflungsvollem Sinn", "tokens": ["Und", "mit", "ver\u00b7zwei\u00b7flungs\u00b7vol\u00b7lem", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schreit sie: Ach, gib mir deinen Segen,", "tokens": ["Schreit", "sie", ":", "Ach", ",", "gib", "mir", "dei\u00b7nen", "Se\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "ITJ", "$,", "VVIMP", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hier ist das Brot, ach, nimm es hin!", "tokens": ["Hier", "ist", "das", "Brot", ",", "ach", ",", "nimm", "es", "hin", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ITJ", "$,", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Er nimmts und n\u00e4sset es mit Tr\u00e4nen,", "tokens": ["Er", "nimmts", "und", "n\u00e4s\u00b7set", "es", "mit", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich S\u00fcnder soll dich segnen? \u2013 dich?", "tokens": ["Ich", "S\u00fcn\u00b7der", "soll", "dich", "seg\u00b7nen", "?", "\u2013", "dich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PPER", "NN", "VMFIN", "PPER", "VVINF", "$.", "$(", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O, rufet er mit leisem St\u00f6hnen,", "tokens": ["O", ",", "ru\u00b7fet", "er", "mit", "lei\u00b7sem", "St\u00f6h\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weib, du bist heiliger als ich!", "tokens": ["Weib", ",", "du", "bist", "hei\u00b7li\u00b7ger", "als", "ich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADJD", "KOKOM", "PPER", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}}}}}