{"textgrid.poem.43221": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Er Dein Gatte! \u2013 er Dein Gatte!", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er Dein Gatte! \u2013 er Dein Gatte!", "tokens": ["Er", "Dein", "Gat\u00b7te", "!", "\u2013", "er", "Dein", "Gat\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$.", "$(", "PPER", "PPOSAT", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Da\u00df ich es kaum fassen kann \u2013:", "tokens": ["Da\u00df", "ich", "es", "kaum", "fas\u00b7sen", "kann", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVINF", "VMFIN", "$(", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die \u2013 die mich so lieb einst hatte,", "tokens": ["Die", "\u2013", "die", "mich", "so", "lieb", "einst", "hat\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PRELS", "PPER", "ADV", "ADJD", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebt den glatten sichern Mann?", "tokens": ["Liebt", "den", "glat\u00b7ten", "si\u00b7chern", "Mann", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Klingt das eitel? \u2013 Lache, S\u00fc\u00dfe,", "tokens": ["Klingt", "das", "ei\u00b7tel", "?", "\u2013", "La\u00b7che", ",", "S\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "$.", "$(", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber blick' mich freundlich an,", "tokens": ["A\u00b7ber", "blick'", "mich", "freund\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mich, den Deiner keuschen K\u00fcsse", "tokens": ["Mich", ",", "den", "Dei\u00b7ner", "keu\u00b7schen", "K\u00fcs\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Duftiger Zauber einst umspann.", "tokens": ["Duf\u00b7ti\u00b7ger", "Zau\u00b7ber", "einst", "um\u00b7spann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Denkst Du noch der schmalen Gasse?", "tokens": ["Denkst", "Du", "noch", "der", "schma\u00b7len", "Gas\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir gen\u00fcber lag Dein Haus,", "tokens": ["Mir", "ge\u00b7n\u00fc\u00b7ber", "lag", "Dein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich Deine Hand erfasse,", "tokens": ["Da\u00df", "ich", "Dei\u00b7ne", "Hand", "er\u00b7fas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Streckt' ich nur die meine aus.", "tokens": ["Streckt'", "ich", "nur", "die", "mei\u00b7ne", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach so nah', wenn auch geschieden,", "tokens": ["Ach", "so", "nah'", ",", "wenn", "auch", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "PTKVZ", "$,", "KOUS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lebten, liebten, lachten wir;", "tokens": ["Leb\u00b7ten", ",", "lieb\u00b7ten", ",", "lach\u00b7ten", "wir", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jene Zeit voll Gl\u00fcck und Frieden,", "tokens": ["Je\u00b7ne", "Zeit", "voll", "Gl\u00fcck", "und", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie entschwand mit Dir, \u2013 mit Dir! \u2013", "tokens": ["Sie", "ent\u00b7schwand", "mit", "Dir", ",", "\u2013", "mit", "Dir", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "$(", "APPR", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "F\u00fcr mich kamen tr\u00fcbe Tage,", "tokens": ["F\u00fcr", "mich", "ka\u00b7men", "tr\u00fc\u00b7be", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein uns\u00e4glich langes Jahr;", "tokens": ["Ein", "un\u00b7s\u00e4g\u00b7lich", "lan\u00b7ges", "Jahr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es verging in Noth und Plage,", "tokens": ["Es", "ver\u00b7ging", "in", "Noth", "und", "Pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ganz so wie es ehmals war.", "tokens": ["Ganz", "so", "wie", "es", "eh\u00b7mals", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOKOM", "PPER", "ADV", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Deine liebe Stimme fehlte,", "tokens": ["Dei\u00b7ne", "lie\u00b7be", "Stim\u00b7me", "fehl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ich auch hin\u00fcbersah,", "tokens": ["Wie", "ich", "auch", "hin\u00b7\u00fc\u00b7ber\u00b7sah", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Fensterscheiben z\u00e4hlte,", "tokens": ["Dei\u00b7ne", "Fens\u00b7ter\u00b7schei\u00b7ben", "z\u00e4hl\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du bliebst fort \u2013 was sollt' ich da?", "tokens": ["Du", "bliebst", "fort", "\u2013", "was", "sollt'", "ich", "da", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$(", "PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Traurig wurde ich und klexte", "tokens": ["Trau\u00b7rig", "wur\u00b7de", "ich", "und", "klex\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Endlos lange Bogen voll,", "tokens": ["End\u00b7los", "lan\u00b7ge", "Bo\u00b7gen", "voll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lie\u00df die Stube, die verhexte,", "tokens": ["Lie\u00df", "die", "Stu\u00b7be", ",", "die", "ver\u00b7hex\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rannte in die Welt wie toll.", "tokens": ["Rann\u00b7te", "in", "die", "Welt", "wie", "toll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "KOKOM", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ueberall dieselbe Leere. \u2013 \u2013", "tokens": ["Ue\u00b7be\u00b7rall", "die\u00b7sel\u00b7be", "Lee\u00b7re", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "PDAT", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Etwas fehlt' in meiner Brust,", "tokens": ["Et\u00b7was", "fehlt'", "in", "mei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Darum such' ich \u00fcber'm Meere", "tokens": ["Da\u00b7rum", "such'", "ich", "\u00fc\u00b7ber'm", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Neuen Kampf und neue Lust.", "tokens": ["Neu\u00b7en", "Kampf", "und", "neu\u00b7e", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Anna! \u2013 k\u00fcsse meine Wange", "tokens": ["An\u00b7na", "!", "\u2013", "k\u00fcs\u00b7se", "mei\u00b7ne", "Wan\u00b7ge"], "token_info": ["word", "punct", "punct", "word", "word", "word"], "pos": ["NE", "$.", "$(", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einmal wie vor alter Zeit;", "tokens": ["Ein\u00b7mal", "wie", "vor", "al\u00b7ter", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kind, wir scheiden heut' f\u00fcr ", "tokens": ["Kind", ",", "wir", "schei\u00b7den", "heut'", "f\u00fcr"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADV", "APPR"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "F\u00fcr die ganze Ewigkeit. \u2013 \u2013 \u2013", "tokens": ["F\u00fcr", "die", "gan\u00b7ze", "E\u00b7wig\u00b7keit", ".", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "$(", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}