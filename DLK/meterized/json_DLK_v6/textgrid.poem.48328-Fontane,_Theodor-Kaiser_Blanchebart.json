{"textgrid.poem.48328": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Kaiser Blanchebart", "genre": "verse", "period": "N.A.", "pub_year": 1871, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor seinem Heergefolge ritt,", "tokens": ["Vor", "sei\u00b7nem", "Heer\u00b7ge\u00b7fol\u00b7ge", "ritt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von seinem Volk umschart,", "tokens": ["Von", "sei\u00b7nem", "Volk", "um\u00b7schart", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Inmitten von Helden und Prinzen,", "tokens": ["In\u00b7mit\u00b7ten", "von", "Hel\u00b7den", "und", "Prin\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "An der Spitze seiner Provinzen,", "tokens": ["An", "der", "Spit\u00b7ze", "sei\u00b7ner", "Pro\u00b7vin\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der Kaiser Blanchebart.", "tokens": ["Der", "Kai\u00b7ser", "Blan\u00b7che\u00b7bart", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er gr\u00fc\u00dft und sitzt auf hohem Ro\u00df", "tokens": ["Er", "gr\u00fc\u00dft", "und", "sitzt", "auf", "ho\u00b7hem", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sinnet das und dies:", "tokens": ["Und", "sin\u00b7net", "das", "und", "dies", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "KON", "PDS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er h\u00e4lt am Sadowa-Walde,", "tokens": ["Er", "h\u00e4lt", "am", "Sa\u00b7do\u00b7wa\u00b7Wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Auf der Gravelotter Halde", "tokens": ["Auf", "der", "Gra\u00b7ve\u00b7lot\u00b7ter", "Hal\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und vor Sedan und Paris.", "tokens": ["Und", "vor", "Se\u00b7dan", "und", "Pa\u00b7ris", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "KON", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Er l\u00e4chelt still; ihm ward zu Traum", "tokens": ["Er", "l\u00e4\u00b7chelt", "still", ";", "ihm", "ward", "zu", "Traum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "PPER", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die lange Kriegesfahrt,", "tokens": ["Die", "lan\u00b7ge", "Krie\u00b7ges\u00b7fahrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es schaukeln und schwanken die Reiser,", "tokens": ["Es", "schau\u00b7keln", "und", "schwan\u00b7ken", "die", "Rei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und rings jubelt's: Es lebe der Kaiser,", "tokens": ["Und", "rings", "ju\u00b7belt's", ":", "Es", "le\u00b7be", "der", "Kai\u00b7ser", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NE", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der Kaiser Blanchebart!", "tokens": ["Der", "Kai\u00b7ser", "Blan\u00b7che\u00b7bart", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und an der Stra\u00df' und an dem Tor,", "tokens": ["Und", "an", "der", "Stra\u00df'", "und", "an", "dem", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da halten Frau und Mann,", "tokens": ["Da", "hal\u00b7ten", "Frau", "und", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sie heben empor ihren Knaben,", "tokens": ["Und", "sie", "he\u00b7ben", "em\u00b7por", "ih\u00b7ren", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "PPOSAT", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Den einzigen, den sie haben,", "tokens": ["Den", "ein\u00b7zi\u00b7gen", ",", "den", "sie", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und sprechen: \u00bbSieh ihn dir an!", "tokens": ["Und", "spre\u00b7chen", ":", "\u00bb", "Sieh", "ihn", "dir", "an", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$.", "$(", "VVIMP", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Sieh ihn dir an und vergi\u00df ihn nicht.", "tokens": ["Sieh", "ihn", "dir", "an", "und", "ver\u00b7gi\u00df", "ihn", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPER", "PTKVZ", "KON", "VVIMP", "PPER", "PTKNEG", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Der ist von sondrer Art,", "tokens": ["Der", "ist", "von", "sond\u00b7rer", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Dienst allzeit das Schwerste,", "tokens": ["Im", "Dienst", "all\u00b7zeit", "das", "Schwers\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und in Feld und Pflicht der Erste,", "tokens": ["Und", "in", "Feld", "und", "Pflicht", "der", "Ers\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "ART", "ADJA", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Das ist Kaiser Blanchebart.\u00ab", "tokens": ["Das", "ist", "Kai\u00b7ser", "Blan\u00b7che\u00b7bart", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Kaiser sah den Knaben an,", "tokens": ["Der", "Kai\u00b7ser", "sah", "den", "Kna\u00b7ben", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den \u00fcberlief es hei\u00df,", "tokens": ["Den", "\u00fc\u00b7berl\u00b7ief", "es", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Alle Herzen sprachen Segen,", "tokens": ["Al\u00b7le", "Her\u00b7zen", "spra\u00b7chen", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und hernieder fiel ein Regen", "tokens": ["Und", "her\u00b7nie\u00b7der", "fiel", "ein", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Von Bl\u00fcten rot und wei\u00df.", "tokens": ["Von", "Bl\u00fc\u00b7ten", "rot", "und", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Gott mit dir, Herr, und kommt der Tag,", "tokens": ["Gott", "mit", "dir", ",", "Herr", ",", "und", "kommt", "der", "Tag", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der noch keinem wurde gespart,", "tokens": ["Der", "noch", "kei\u00b7nem", "wur\u00b7de", "ge\u00b7spart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIS", "VAFIN", "VVPP", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Dann wie aus ", "tokens": ["Dann", "wie", "aus"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KOKOM", "APPR"], "meter": "---", "measure": "unknown.measure.zero"}, "line.4": {"text": "Werden wir singen und sagen", "tokens": ["Wer\u00b7den", "wir", "sin\u00b7gen", "und", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVFIN", "KON", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Vom Kaiser Blanchebart.", "tokens": ["Vom", "Kai\u00b7ser", "Blan\u00b7che\u00b7bart", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}