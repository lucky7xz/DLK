{"textgrid.poem.37485": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Ein galantes Abenteuer", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Morgen graut. Ich kam per Bahn", "tokens": ["Der", "Mor\u00b7gen", "graut", ".", "Ich", "kam", "per", "Bahn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stolz in der Stadt der Welfen an.", "tokens": ["Stolz", "in", "der", "Stadt", "der", "Wel\u00b7fen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Und wie ich wandle, seh' ich walten", "tokens": ["Und", "wie", "ich", "wand\u00b7le", ",", "seh'", "ich", "wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Morgenscheine f\u00fcnf Gestalten.", "tokens": ["Im", "Mor\u00b7gen\u00b7schei\u00b7ne", "f\u00fcnf", "Ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbseid mir gegr\u00fc\u00dft, ihr edlen Frauen,", "tokens": ["\u00bb", "seid", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "ihr", "ed\u00b7len", "Frau\u00b7en", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "VVPP", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "So wunderlieblich anzuschauen!\u00ab", "tokens": ["So", "wun\u00b7der\u00b7lieb\u00b7lich", "an\u00b7zu\u00b7schau\u00b7en", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "VVIZU", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbwat het he seggt?!\u00ab So t\u00f6nt's im Chor,", "tokens": ["\u00bb", "wat", "het", "he", "seggt", "?!", "\u00ab", "So", "t\u00f6nt's", "im", "Chor", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "NE", "VVFIN", "$.", "$(", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "F\u00fcnf Besen heben sich empor.", "tokens": ["F\u00fcnf", "Be\u00b7sen", "he\u00b7ben", "sich", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich stolp're in ein Kehrichtfa\u00df;", "tokens": ["Ich", "stol\u00b7p'\u00b7re", "in", "ein", "Keh\u00b7richt\u00b7fa\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Besen sind sehr d\u00fcrr und na\u00df.", "tokens": ["Die", "Be\u00b7sen", "sind", "sehr", "d\u00fcrr", "und", "na\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Kaum rett' ich mich, schon halb verdroschen,", "tokens": ["Kaum", "rett'", "ich", "mich", ",", "schon", "halb", "ver\u00b7dro\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit 25 Silbergroschen.", "tokens": ["Mit", "25", "Sil\u00b7ber\u00b7gro\u00b7schen", "."], "token_info": ["word", "number", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Das hemmt der Besengarde Lauf. \u2013", "tokens": ["Das", "hemmt", "der", "Be\u00b7sen\u00b7gar\u00b7de", "Lauf", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Bad nimmt meine Glieder auf.", "tokens": ["Ein", "Bad", "nimmt", "mei\u00b7ne", "Glie\u00b7der", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "So geht's! \u2013 Bei Damen sollst du fein", "tokens": ["So", "geht's", "!", "\u2013", "Bei", "Da\u00b7men", "sollst", "du", "fein"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$.", "$(", "APPR", "NN", "VMFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gar niemals nicht ironisch sein.", "tokens": ["Gar", "nie\u00b7mals", "nicht", "i\u00b7ron\u00b7isch", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}