{"textgrid.poem.60118": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich sah im \u00f6den Garten,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah im \u00f6den Garten,", "tokens": ["Ich", "sah", "im", "\u00f6\u00b7den", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Umkr\u00e4nzt von Eis,", "tokens": ["Um\u00b7kr\u00e4nzt", "von", "Eis", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die V\u00f6glein dich erwarten,", "tokens": ["Die", "V\u00f6\u00b7glein", "dich", "er\u00b7war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf d\u00fcrrem Reis;", "tokens": ["Auf", "d\u00fcr\u00b7rem", "Reis", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die Zeugen deiner Milde,", "tokens": ["Die", "Zeu\u00b7gen", "dei\u00b7ner", "Mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von dir gen\u00e4hrt,", "tokens": ["Von", "dir", "ge\u00b7n\u00e4hrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "So lang im Schneegefilde", "tokens": ["So", "lang", "im", "Schnee\u00b7ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Mangel w\u00e4hrt.", "tokens": ["Der", "Man\u00b7gel", "w\u00e4hrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Da schlug mein Herz gelinder;", "tokens": ["Da", "schlug", "mein", "Herz", "ge\u00b7lin\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich wurde froh,", "tokens": ["Ich", "wur\u00b7de", "froh", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und sah der Armuth Kinder,", "tokens": ["Und", "sah", "der", "Ar\u00b7muth", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die eben so,", "tokens": ["Die", "e\u00b7ben", "so", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Vergessend ihre Klagen", "tokens": ["Ver\u00b7ges\u00b7send", "ih\u00b7re", "Kla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nach dir geblickt,", "tokens": ["Nach", "dir", "ge\u00b7blickt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Weil du in b\u00f6sen Tagen", "tokens": ["Weil", "du", "in", "b\u00f6\u00b7sen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sie gern erquickt.", "tokens": ["Sie", "gern", "er\u00b7quickt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "O glaube! wenn vergebens", "tokens": ["O", "glau\u00b7be", "!", "wenn", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "$.", "KOUS", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Himmel nicht", "tokens": ["Der", "Him\u00b7mel", "nicht"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "PTKNEG"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sein Wort voll Kraft und Lebens", "tokens": ["Sein", "Wort", "voll", "Kraft", "und", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zur Erde spricht;", "tokens": ["Zur", "Er\u00b7de", "spricht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wenn jedes leise Flehen", "tokens": ["Wenn", "je\u00b7des", "lei\u00b7se", "Fle\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Empor sich schwingt,", "tokens": ["Em\u00b7por", "sich", "schwingt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Kein V\u00f6glein ungesehen", "tokens": ["Kein", "V\u00f6\u00b7glein", "un\u00b7ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vom Zweige sinkt;", "tokens": ["Vom", "Zwei\u00b7ge", "sinkt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Wenn g\u00f6ttliches Erbarmen", "tokens": ["Wenn", "g\u00f6tt\u00b7li\u00b7ches", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Frommen tr\u00e4gt,", "tokens": ["Den", "From\u00b7men", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der neben sich des armen", "tokens": ["Der", "ne\u00b7ben", "sich", "des", "ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PRF", "ART", "ADJA"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Verla\u00dfnen pflegt \u2013", "tokens": ["Ver\u00b7la\u00df\u00b7nen", "pflegt", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "So bleibet Gottes Segen", "tokens": ["So", "blei\u00b7bet", "Got\u00b7tes", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dir sicherlich;", "tokens": ["Dir", "si\u00b7cher\u00b7lich", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PRF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "So f\u00fchrt auf Dornenwegen", "tokens": ["So", "f\u00fchrt", "auf", "Dor\u00b7nen\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sein Engel dich.", "tokens": ["Sein", "En\u00b7gel", "dich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Auf nackten Winterauen", "tokens": ["Auf", "nack\u00b7ten", "Win\u00b7te\u00b7rau\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hast du geschont,", "tokens": ["Hast", "du", "ge\u00b7schont", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Den V\u00f6glein ihr Vertrauen", "tokens": ["Den", "V\u00f6\u00b7glein", "ihr", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So reich belohnt:", "tokens": ["So", "reich", "be\u00b7lohnt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wie sollte der nicht schonen,", "tokens": ["Wie", "soll\u00b7te", "der", "nicht", "scho\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der ewig liebt,", "tokens": ["Der", "e\u00b7wig", "liebt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Nicht Er dem Herzen lohnen,", "tokens": ["Nicht", "Er", "dem", "Her\u00b7zen", "loh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Alles giebt?", "tokens": ["Der", "Al\u00b7les", "giebt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}