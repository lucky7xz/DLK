{"textgrid.poem.40818": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Am Hochaltar, umflammt vom Kerzenglanze,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Hochaltar, umflammt vom Kerzenglanze,", "tokens": ["Am", "Hoc\u00b7hal\u00b7tar", ",", "um\u00b7flammt", "vom", "Ker\u00b7zen\u00b7glan\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Strahlt in des Priesters Hand die Goldmonstranze,", "tokens": ["Strahlt", "in", "des", "Pries\u00b7ters", "Hand", "die", "Gold\u00b7mons\u00b7tran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um die als Kranz, aus laut'rem Gold gegossen,", "tokens": ["Um", "die", "als", "Kranz", ",", "aus", "laut'\u00b7rem", "Gold", "ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "KOUS", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Rebenreis und eine Aehre sprossen.", "tokens": ["Ein", "Re\u00b7ben\u00b7reis", "und", "ei\u00b7ne", "A\u00b7eh\u00b7re", "spros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Traun, solche Huldigung wie beiden diesen", "tokens": ["Traun", ",", "sol\u00b7che", "Hul\u00b7di\u00b7gung", "wie", "bei\u00b7den", "die\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PIAT", "NN", "KOKOM", "PIAT", "PDAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ward keiner Reb' und Aehre je erwiesen!", "tokens": ["Ward", "kei\u00b7ner", "Reb'", "und", "A\u00b7eh\u00b7re", "je", "er\u00b7wie\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "KON", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Seht, jetzt erhebt der Priester die Monstranze", "tokens": ["Seht", ",", "jetzt", "er\u00b7hebt", "der", "Pries\u00b7ter", "die", "Mons\u00b7tran\u00b7ze"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit ihrem goldnen Reb'- und Aehrenkranze:", "tokens": ["Mit", "ih\u00b7rem", "gold\u00b7nen", "Re\u00b7b'", "und", "A\u00b7eh\u00b7ren\u00b7kran\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-++-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Und alles Volk sinkt auf die Knie' im Kreise", "tokens": ["Und", "al\u00b7les", "Volk", "sinkt", "auf", "die", "Knie'", "im", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und schl\u00e4gt ans Herz und fl\u00fcstert betend leise,", "tokens": ["Und", "schl\u00e4gt", "ans", "Herz", "und", "fl\u00fcs\u00b7tert", "be\u00b7tend", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Weihrauchs duft'ge Wolken aufw\u00e4rts ringen,", "tokens": ["Des", "Weih\u00b7rauchs", "duft'\u00b7ge", "Wol\u00b7ken", "auf\u00b7w\u00e4rts", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Glocken donnern, und die Gl\u00f6cklein klingen!", "tokens": ["Die", "Glo\u00b7cken", "don\u00b7nern", ",", "und", "die", "Gl\u00f6c\u00b7klein", "klin\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da denkt die Aehre still: Ich wollt', ich st\u00fcnde", "tokens": ["Da", "denkt", "die", "A\u00b7eh\u00b7re", "still", ":", "Ich", "wollt'", ",", "ich", "st\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$.", "PPER", "VMFIN", "$,", "PPER", "VVFIN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im Felde bei den Schwestern, frei im Winde,", "tokens": ["Im", "Fel\u00b7de", "bei", "den", "Schwes\u00b7tern", ",", "frei", "im", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie sie zu wallen leis im goldnen Reigen", "tokens": ["Wie", "sie", "zu", "wal\u00b7len", "leis", "im", "gold\u00b7nen", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PTKZU", "VVINF", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und selbst das Haupt, von Segen schwer, zu neigen!", "tokens": ["Und", "selbst", "das", "Haupt", ",", "von", "Se\u00b7gen", "schwer", ",", "zu", "nei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "APPR", "NN", "ADJD", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Da denkt die Rebe still: O k\u00f6nnt' ich sprossen", "tokens": ["Da", "denkt", "die", "Re\u00b7be", "still", ":", "O", "k\u00f6nnt'", "ich", "spros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$.", "NE", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf steilem H\u00fcgelrain bei den Genossen,", "tokens": ["Auf", "stei\u00b7lem", "H\u00fc\u00b7gel\u00b7rain", "bei", "den", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Wie sie, vom Fruchtkorb schwer, den R\u00fccken neigend", "tokens": ["Wie", "sie", ",", "vom", "Frucht\u00b7korb", "schwer", ",", "den", "R\u00fc\u00b7cken", "nei\u00b7gend"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "$,", "APPRART", "NN", "ADJD", "$,", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und selbst das Knie in stiller Andacht beugend!", "tokens": ["Und", "selbst", "das", "Knie", "in", "stil\u00b7ler", "An\u00b7dacht", "beu\u00b7gend", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}