{"textgrid.poem.63285": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir wollen aus allen Fenstern schwarze Fahnen hissen.", "tokens": ["Wir", "wol\u00b7len", "aus", "al\u00b7len", "Fens\u00b7tern", "schwar\u00b7ze", "Fah\u00b7nen", "his\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wer darf noch von einer Hoffnung wissen?", "tokens": ["Wer", "darf", "noch", "von", "ei\u00b7ner", "Hoff\u00b7nung", "wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Uns will keine Sonne, kein Mond mehr bescheinen.", "tokens": ["Uns", "will", "kei\u00b7ne", "Son\u00b7ne", ",", "kein", "Mond", "mehr", "be\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "$,", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "An den Strassenecken stehen Hunde, die p .....,", "tokens": ["An", "den", "Stras\u00b7sen\u00b7ec\u00b7ken", "ste\u00b7hen", "Hun\u00b7de", ",", "die", "p", ".....", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,", "PRELS", "XY", "$(", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.5": {"text": "Und Menschen, die weinen.", "tokens": ["Und", "Men\u00b7schen", ",", "die", "wei\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Und ein Hund springt auf einen andern Hund", "tokens": ["Und", "ein", "Hund", "springt", "auf", "ei\u00b7nen", "an\u00b7dern", "Hund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "--++-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Und Mann auf Mann: wie gleichg\u00fcltig ist das alles:", "tokens": ["Und", "Mann", "auf", "Mann", ":", "wie", "gleich\u00b7g\u00fcl\u00b7tig", "ist", "das", "al\u00b7les", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "$.", "PWAV", "ADJD", "VAFIN", "ART", "PIS", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Gut und b\u00f6se, Nord und S\u00fcd.", "tokens": ["Gut", "und", "b\u00f6\u00b7se", ",", "Nord", "und", "S\u00fcd", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "NE", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur dass uns Erl\u00f6sung f\u00fcr eine Sekunde bl\u00fcht", "tokens": ["Nur", "dass", "uns", "Er\u00b7l\u00f6\u00b7sung", "f\u00fcr", "ei\u00b7ne", "Se\u00b7kun\u00b7de", "bl\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.5": {"text": "Aus dem ewigen Dalles,", "tokens": ["Aus", "dem", "e\u00b7wi\u00b7gen", "Dal\u00b7les", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Dem ewigen Nichts,", "tokens": ["Dem", "e\u00b7wi\u00b7gen", "Nichts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Dem ewigen Ohne-Grund,", "tokens": ["Dem", "e\u00b7wi\u00b7gen", "Oh\u00b7ne\u00b7Grund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dem Dunkel des Lichts.", "tokens": ["Dem", "Dun\u00b7kel", "des", "Lichts", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Wen erpichts,", "tokens": ["Wen", "er\u00b7pichts", ","], "token_info": ["word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Hinter den Vorhang zu schauen,", "tokens": ["Hin\u00b7ter", "den", "Vor\u00b7hang", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Wo die fahrigen Mimen sich abschminken,", "tokens": ["Wo", "die", "fah\u00b7ri\u00b7gen", "Mi\u00b7men", "sich", "ab\u00b7schmin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "--+--+--++-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Alte M\u00e4dchen mit verrosteten Haarnadeln ihre Kahlk\u00f6pfe krauen,", "tokens": ["Al\u00b7te", "M\u00e4d\u00b7chen", "mit", "ver\u00b7ros\u00b7te\u00b7ten", "Haar\u00b7na\u00b7deln", "ih\u00b7re", "Kahl\u00b7k\u00f6p\u00b7fe", "krau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+--++-+-++-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.5": {"text": "Der Bariton und die Souffleuse sich in die Arme sinken?", "tokens": ["Der", "Ba\u00b7ri\u00b7ton", "und", "die", "Souf\u00b7fleu\u00b7se", "sich", "in", "die", "Ar\u00b7me", "sin\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Wo es die Naive dem Helden z\u00e4rtlich mit dem Munde macht?", "tokens": ["Wo", "es", "die", "Nai\u00b7ve", "dem", "Hel\u00b7den", "z\u00e4rt\u00b7lich", "mit", "dem", "Mun\u00b7de", "macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "---+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Gute Nacht!", "tokens": ["Gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}