{"textgrid.poem.41165": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier stieg herauf der Falschheit Bild,", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier stieg herauf der Falschheit Bild,", "tokens": ["Hier", "stieg", "her\u00b7auf", "der", "Falschheit", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Du, die dem H\u00f6llenpfuhl entsprossen,", "tokens": ["Du", ",", "die", "dem", "H\u00f6l\u00b7len\u00b7pfuhl", "ent\u00b7spros\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "W\u00e4r' noch mein Tintenfa\u00df gef\u00fcllt,", "tokens": ["W\u00e4r'", "noch", "mein", "Tin\u00b7ten\u00b7fa\u00df", "ge\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich h\u00e4tt' mit Tint' dich \u00fcbergossen.", "tokens": ["Ich", "h\u00e4tt'", "mit", "Tint'", "dich", "\u00fc\u00b7ber\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du gift'ge, verh\u00fcllte Fratze,", "tokens": ["Du", "gift'\u00b7ge", ",", "ver\u00b7h\u00fcll\u00b7te", "Frat\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Auf deinem Kopf sitzt eine Katze,", "tokens": ["Auf", "dei\u00b7nem", "Kopf", "sitzt", "ei\u00b7ne", "Kat\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "In deiner Brust der Katze Kater.", "tokens": ["In", "dei\u00b7ner", "Brust", "der", "Kat\u00b7ze", "Ka\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Fort! fort! zur\u00fcck zum Feuerkrater", "tokens": ["Fort", "!", "fort", "!", "zu\u00b7r\u00fcck", "zum", "Feu\u00b7er\u00b7kra\u00b7ter"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "PTKVZ", "$.", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der H\u00f6lle, wo du heimatlich,", "tokens": ["Der", "H\u00f6l\u00b7le", ",", "wo", "du", "hei\u00b7mat\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Nur halb klecksographier' ich dich.", "tokens": ["Nur", "halb", "kleck\u00b7so\u00b7gra\u00b7phier'", "ich", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}