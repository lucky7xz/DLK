{"textgrid.poem.45974": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[wenn es st\u00fcrmet auf den Wogen]", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn es st\u00fcrmet auf den Wogen,", "tokens": ["Wenn", "es", "st\u00fcr\u00b7met", "auf", "den", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sitzt die Schifferin zu Haus,", "tokens": ["Sitzt", "die", "Schif\u00b7fe\u00b7rin", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch ihr Herz ist hingezogen", "tokens": ["Doch", "ihr", "Herz", "ist", "hin\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf die weite See hinaus,", "tokens": ["Auf", "die", "wei\u00b7te", "See", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APZR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bei jeder Welle, die brandet", "tokens": ["Bei", "je\u00b7der", "Wel\u00b7le", ",", "die", "bran\u00b7det"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "VVFIN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Sch\u00e4umend an Ufers Rand,", "tokens": ["Sch\u00e4u\u00b7mend", "an", "U\u00b7fers", "Rand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Denkt sie, er strandet, er strandet, er strandet,", "tokens": ["Denkt", "sie", ",", "er", "stran\u00b7det", ",", "er", "stran\u00b7det", ",", "er", "stran\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Er kehret mir nimmer zum Land.", "tokens": ["Er", "keh\u00b7ret", "mir", "nim\u00b7mer", "zum", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Bei des Donners wildem Toben", "tokens": ["Bei", "des", "Don\u00b7ners", "wil\u00b7dem", "To\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sitzt die Sch\u00e4ferin zu Haus,", "tokens": ["Sitzt", "die", "Sch\u00e4\u00b7fe\u00b7rin", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch ihr Herz, das schwebet oben", "tokens": ["Doch", "ihr", "Herz", ",", "das", "schwe\u00b7bet", "o\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "PDS", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In des Wetters wildem Saus.", "tokens": ["In", "des", "Wet\u00b7ters", "wil\u00b7dem", "Saus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Bei jedem Strahle, der klirrte", "tokens": ["Bei", "je\u00b7dem", "Strah\u00b7le", ",", "der", "klirr\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schmetternd durch Donners Groll,", "tokens": ["Schmet\u00b7ternd", "durch", "Don\u00b7ners", "Groll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Denkt sie, mein Hirte, mein Hirte, mein Hirte", "tokens": ["Denkt", "sie", ",", "mein", "Hir\u00b7te", ",", "mein", "Hir\u00b7te", ",", "mein", "Hir\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Mir nimmermehr kehren soll.", "tokens": ["Mir", "nim\u00b7mer\u00b7mehr", "keh\u00b7ren", "soll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Wenn es in dem Abgrund bebet,", "tokens": ["Wenn", "es", "in", "dem", "Ab\u00b7grund", "be\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sitzt des Bergmanns Weib zu Haus,", "tokens": ["Sitzt", "des", "Berg\u00b7manns", "Weib", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch ihr treues Herz, das schwebet", "tokens": ["Doch", "ihr", "treu\u00b7es", "Herz", ",", "das", "schwe\u00b7bet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,", "PDS", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In des Schachtes dunklem Graus.", "tokens": ["In", "des", "Schach\u00b7tes", "dunk\u00b7lem", "Graus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Bei jedem Sto\u00dfe, der r\u00fcttet", "tokens": ["Bei", "je\u00b7dem", "Sto\u00b7\u00dfe", ",", "der", "r\u00fct\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hallend im dunkelen Schacht;", "tokens": ["Hal\u00b7lend", "im", "dun\u00b7ke\u00b7len", "Schacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Denkt sie, versch\u00fcttet, versch\u00fcttet, versch\u00fcttet", "tokens": ["Denkt", "sie", ",", "ver\u00b7sch\u00fct\u00b7tet", ",", "ver\u00b7sch\u00fct\u00b7tet", ",", "ver\u00b7sch\u00fct\u00b7tet"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "$,", "VVPP", "$,", "VVPP", "$,", "VVFIN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ist mein Knapp' in der Erde Nacht.", "tokens": ["Ist", "mein", "Knapp'", "in", "der", "Er\u00b7de", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Wenn die Feldschlacht tost und klirret,", "tokens": ["Wenn", "die", "Feld\u00b7schlacht", "tost", "und", "klir\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sitzt des Kriegers Weib zu Haus,", "tokens": ["Sitzt", "des", "Krie\u00b7gers", "Weib", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch ihr banges Herz, das irret", "tokens": ["Doch", "ihr", "ban\u00b7ges", "Herz", ",", "das", "ir\u00b7ret"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,", "PDS", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In des Kampfes wilden Strau\u00df.", "tokens": ["In", "des", "Kamp\u00b7fes", "wil\u00b7den", "Strau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Bei jedem Knall, jedem Hallen", "tokens": ["Bei", "je\u00b7dem", "Knall", ",", "je\u00b7dem", "Hal\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der St\u00fccke an Bergeswand", "tokens": ["Der", "St\u00fc\u00b7cke", "an", "Ber\u00b7ges\u00b7wand"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Denkt sie gefallen, gefallen, gefallen", "tokens": ["Denkt", "sie", "ge\u00b7fal\u00b7len", ",", "ge\u00b7fal\u00b7len", ",", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "VVPP", "$,", "VVPP", "$,", "VVPP"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Ist mein Held nun f\u00fcrs Vaterland.", "tokens": ["Ist", "mein", "Held", "nun", "f\u00fcrs", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Aber fern schon \u00fcber die Berge,", "tokens": ["A\u00b7ber", "fern", "schon", "\u00fc\u00b7ber", "die", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Zogen die Wetter, der Donner verhallt,", "tokens": ["Zo\u00b7gen", "die", "Wet\u00b7ter", ",", "der", "Don\u00b7ner", "ver\u00b7hallt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Horch wie die jubelnde, trunkene Lerche,", "tokens": ["Horch", "wie", "die", "ju\u00b7beln\u00b7de", ",", "trun\u00b7ke\u00b7ne", "Ler\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Tireli, Tireli, siegreich erschallt.", "tokens": ["Ti\u00b7re\u00b7li", ",", "Ti\u00b7re\u00b7li", ",", "sieg\u00b7reich", "er\u00b7schallt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "ADJD", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.10": {"line.1": {"text": "Raben zieht weiter!", "tokens": ["Ra\u00b7ben", "zieht", "wei\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Himmel wird heiter,", "tokens": ["Him\u00b7mel", "wird", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Dringe mir, dringe mir,", "tokens": ["Drin\u00b7ge", "mir", ",", "drin\u00b7ge", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Sonne hervor!", "tokens": ["Son\u00b7ne", "her\u00b7vor", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.11": {"line.1": {"text": "Jubelnde Lerche,", "tokens": ["Ju\u00b7beln\u00b7de", "Ler\u00b7che", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "\u00dcber die Berge,", "tokens": ["\u00dc\u00b7ber", "die", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Singe mir, singe mir,", "tokens": ["Sin\u00b7ge", "mir", ",", "sin\u00b7ge", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Wonne ins Ohr.", "tokens": ["Won\u00b7ne", "ins", "Ohr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.12": {"line.1": {"text": "Mit Zipre\u00df und Lorbeer kr\u00e4nzet", "tokens": ["Mit", "Zip\u00b7re\u00df", "und", "Lor\u00b7beer", "kr\u00e4n\u00b7zet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieg das freudig ernste Haupt,", "tokens": ["Sieg", "das", "freu\u00b7dig", "erns\u00b7te", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herr! wenn er mir niedergl\u00e4nzet", "tokens": ["Herr", "!", "wenn", "er", "mir", "nie\u00b7der\u00b7gl\u00e4n\u00b7zet"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "KOUS", "PPER", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem Trauergr\u00fcn umlaubt!", "tokens": ["Mit", "dem", "Trau\u00b7er\u00b7gr\u00fcn", "um\u00b7laubt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Dann sternlose Nacht sei willkommen,", "tokens": ["Dann", "stern\u00b7lo\u00b7se", "Nacht", "sei", "will\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der Herr hat gegeben den Stern,", "tokens": ["Der", "Herr", "hat", "ge\u00b7ge\u00b7ben", "den", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Der Herr hat genommen, genommen, genommen,", "tokens": ["Der", "Herr", "hat", "ge\u00b7nom\u00b7men", ",", "ge\u00b7nom\u00b7men", ",", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Gelobt sei der Wille des Herrn!", "tokens": ["Ge\u00b7lobt", "sei", "der", "Wil\u00b7le", "des", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}