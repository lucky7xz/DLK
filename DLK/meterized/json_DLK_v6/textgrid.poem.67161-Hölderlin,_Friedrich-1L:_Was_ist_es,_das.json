{"textgrid.poem.67161": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was ist es, das", "genre": "verse", "period": "N.A.", "pub_year": 1801, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist es, das", "tokens": ["Was", "ist", "es", ",", "das"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PWS", "VAFIN", "PPER", "$,", "PRELS"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "An die alten seligen K\u00fcsten", "tokens": ["An", "die", "al\u00b7ten", "se\u00b7li\u00b7gen", "K\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Mich fesselt, da\u00df ich mehr noch", "tokens": ["Mich", "fes\u00b7selt", ",", "da\u00df", "ich", "mehr", "noch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie liebe, als mein Vaterland?", "tokens": ["Sie", "lie\u00b7be", ",", "als", "mein", "Va\u00b7ter\u00b7land", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn wie in himmlische", "tokens": ["Denn", "wie", "in", "himm\u00b7li\u00b7sche"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ADJA"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Gefangenschaft verkauft", "tokens": ["Ge\u00b7fan\u00b7gen\u00b7schaft", "ver\u00b7kauft"], "token_info": ["word", "word"], "pos": ["NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dort bin ich, wo Apollo ging", "tokens": ["Dort", "bin", "ich", ",", "wo", "A\u00b7pol\u00b7lo", "ging"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PWAV", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In K\u00f6nigsgestalt,", "tokens": ["In", "K\u00f6\u00b7nigs\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Und zu unschuldigen J\u00fcnglingen sich", "tokens": ["Und", "zu", "un\u00b7schul\u00b7di\u00b7gen", "J\u00fcng\u00b7lin\u00b7gen", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "PRF"], "meter": "+-++---+-+", "measure": "zehnsilber"}, "line.10": {"text": "Herablie\u00df Zevs und S\u00f6hn in heiliger Art", "tokens": ["He\u00b7rab\u00b7lie\u00df", "Zevs", "und", "S\u00f6hn", "in", "hei\u00b7li\u00b7ger", "Art"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "KON", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Und T\u00f6chter zeugte", "tokens": ["Und", "T\u00f6ch\u00b7ter", "zeug\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Der Hohe unter den Menschen.", "tokens": ["Der", "Ho\u00b7he", "un\u00b7ter", "den", "Men\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Der hohen Gedanken", "tokens": ["Der", "ho\u00b7hen", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sind n\u00e4mlich viel", "tokens": ["Sind", "n\u00e4m\u00b7lich", "viel"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Entsprungen des Vaters Haupt", "tokens": ["Ent\u00b7sprun\u00b7gen", "des", "Va\u00b7ters", "Haupt"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und gro\u00dfe Seelen", "tokens": ["Und", "gro\u00b7\u00dfe", "See\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Von ihm zu Menschen gekommen.", "tokens": ["Von", "ihm", "zu", "Men\u00b7schen", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Geh\u00f6ret hab ich", "tokens": ["Ge\u00b7h\u00f6\u00b7ret", "hab", "ich"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Von Elis und Olympia, bin", "tokens": ["Von", "E\u00b7lis", "und", "O\u00b7lym\u00b7pia", ",", "bin"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NE", "KON", "NE", "$,", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gestanden oben auf dem Parna\u00df,", "tokens": ["Ge\u00b7stan\u00b7den", "o\u00b7ben", "auf", "dem", "Par\u00b7na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Und \u00fcber Bergen des Isthmus,", "tokens": ["Und", "\u00fc\u00b7ber", "Ber\u00b7gen", "des", "Isth\u00b7mus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Und dr\u00fcben auch", "tokens": ["Und", "dr\u00fc\u00b7ben", "auch"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Bei Smyrna und hinab", "tokens": ["Bei", "Smyr\u00b7na", "und", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Bei Ephesos bin ich gegangen;", "tokens": ["Bei", "E\u00b7phe\u00b7sos", "bin", "ich", "ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Viel hab ich Sch\u00f6nes gesehn,", "tokens": ["Viel", "hab", "ich", "Sch\u00f6\u00b7nes", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Und gesungen Gottes Bild", "tokens": ["Und", "ge\u00b7sun\u00b7gen", "Got\u00b7tes", "Bild"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hab ich, das lebet unter", "tokens": ["Hab", "ich", ",", "das", "le\u00b7bet", "un\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "PDS", "VVFIN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Menschen, aber dennoch,", "tokens": ["Den", "Men\u00b7schen", ",", "a\u00b7ber", "den\u00b7noch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Ihr alten G\u00f6tter und all", "tokens": ["Ihr", "al\u00b7ten", "G\u00f6t\u00b7ter", "und", "all"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "PIAT"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Ihr tapfern S\u00f6hne der G\u00f6tter,", "tokens": ["Ihr", "tap\u00b7fern", "S\u00f6h\u00b7ne", "der", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Noch ", "tokens": ["Noch"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Ich liebe unter euch,", "tokens": ["Ich", "lie\u00b7be", "un\u00b7ter", "euch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Wo ihr den letzten eures Geschlechts,", "tokens": ["Wo", "ihr", "den", "letz\u00b7ten", "eu\u00b7res", "Ge\u00b7schlechts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Des Hauses Kleinod mir", "tokens": ["Des", "Hau\u00b7ses", "Klei\u00b7nod", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Dem fremden Gaste verberget.", "tokens": ["Dem", "frem\u00b7den", "Gas\u00b7te", "ver\u00b7ber\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Mein Meister und Herr!", "tokens": ["Mein", "Meis\u00b7ter", "und", "Herr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "O du, mein Lehrer!", "tokens": ["O", "du", ",", "mein", "Leh\u00b7rer", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Was bist du ferne", "tokens": ["Was", "bist", "du", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Geblieben? und da", "tokens": ["Ge\u00b7blie\u00b7ben", "?", "und", "da"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVPP", "$.", "KON", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ich fragte unter den Alten,", "tokens": ["Ich", "frag\u00b7te", "un\u00b7ter", "den", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die Helden und", "tokens": ["Die", "Hel\u00b7den", "und"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "KON"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Die G\u00f6tter, warum bliebest", "tokens": ["Die", "G\u00f6t\u00b7ter", ",", "wa\u00b7rum", "blie\u00b7best"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Du aus? Und jetzt ist voll", "tokens": ["Du", "aus", "?", "Und", "jetzt", "ist", "voll"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "$.", "KON", "ADV", "VAFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Von Trauern meine Seele,", "tokens": ["Von", "Trau\u00b7ern", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Als eifertet, ihr Himmlischen, selbst,", "tokens": ["Als", "ei\u00b7fer\u00b7tet", ",", "ihr", "Himm\u00b7li\u00b7schen", ",", "selbst", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "VVPP", "$,", "PPOSAT", "NN", "$,", "ADV", "$,"], "meter": "-++--+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Da\u00df, dien ich einem, mir", "tokens": ["Da\u00df", ",", "di\u00b7en", "ich", "ei\u00b7nem", ",", "mir"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "$,", "VVFIN", "PPER", "ART", "$,", "PPER"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Das andere fehlet.", "tokens": ["Das", "an\u00b7de\u00b7re", "feh\u00b7let", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Ich wei\u00df es aber, eigene Schuld", "tokens": ["Ich", "wei\u00df", "es", "a\u00b7ber", ",", "ei\u00b7ge\u00b7ne", "Schuld"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ists! Denn zu sehr,", "tokens": ["Ists", "!", "Denn", "zu", "sehr", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "KON", "APPR", "ADV", "$,"], "meter": "++-+", "measure": "iambic.di"}, "line.3": {"text": "O Christus! h\u00e4ng ich an dir,", "tokens": ["O", "Chris\u00b7tus", "!", "h\u00e4ng", "ich", "an", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wiewohl Herakles Bruder", "tokens": ["Wie\u00b7wohl", "Her\u00b7ak\u00b7les", "Bru\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und k\u00fchn bekenn ich, du", "tokens": ["Und", "k\u00fchn", "be\u00b7kenn", "ich", ",", "du"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$,", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Bist Bruder auch des Eviers, der", "tokens": ["Bist", "Bru\u00b7der", "auch", "des", "E\u00b7viers", ",", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "NN", "ADV", "ART", "NN", "$,", "PRELS"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "An den Wagen spannte", "tokens": ["An", "den", "Wa\u00b7gen", "spann\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Die Tiger und hinab", "tokens": ["Die", "Ti\u00b7ger", "und", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Bis an den Indus", "tokens": ["Bis", "an", "den", "In\u00b7dus"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Gebietend freudigen Dienst", "tokens": ["Ge\u00b7bie\u00b7tend", "freu\u00b7di\u00b7gen", "Dienst"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Den Weinberg stiftet' und", "tokens": ["Den", "Wein\u00b7berg", "stif\u00b7tet'", "und"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Den Grimm bez\u00e4hmte der V\u00f6lker.", "tokens": ["Den", "Grimm", "be\u00b7z\u00e4hm\u00b7te", "der", "V\u00f6l\u00b7ker", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Es hindert aber eine Scham", "tokens": ["Es", "hin\u00b7dert", "a\u00b7ber", "ei\u00b7ne", "Scham"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich, dir zu vergleichen", "tokens": ["Mich", ",", "dir", "zu", "ver\u00b7glei\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Die weltlichen M\u00e4nner. Und freilich wei\u00df", "tokens": ["Die", "welt\u00b7li\u00b7chen", "M\u00e4n\u00b7ner", ".", "Und", "frei\u00b7lich", "wei\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "KON", "ADV", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ich, der dich zeugte, dein Vater,", "tokens": ["Ich", ",", "der", "dich", "zeug\u00b7te", ",", "dein", "Va\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Derselbe der,", "tokens": ["Der\u00b7sel\u00b7be", "der", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "ART", "$,"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Denn nimmer herrscht er allein.", "tokens": ["Denn", "nim\u00b7mer", "herrscht", "er", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Es h\u00e4nget aber an ", "tokens": ["Es", "h\u00e4n\u00b7get", "a\u00b7ber", "an"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Liebe. Diesesmal", "tokens": ["Die", "Lie\u00b7be", ".", "Die\u00b7ses\u00b7mal"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$.", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist n\u00e4mlich vom eigenen Herzen", "tokens": ["Ist", "n\u00e4m\u00b7lich", "vom", "ei\u00b7ge\u00b7nen", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Zu sehr gegangen der Gesang,", "tokens": ["Zu", "sehr", "ge\u00b7gan\u00b7gen", "der", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gut machen will ich den Fehl,", "tokens": ["Gut", "ma\u00b7chen", "will", "ich", "den", "Fehl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VMFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Wenn ich noch andere singe.", "tokens": ["Wenn", "ich", "noch", "an\u00b7de\u00b7re", "sin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Nie treff ich, wie ich w\u00fcnsche,", "tokens": ["Nie", "treff", "ich", ",", "wie", "ich", "w\u00fcn\u00b7sche", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Ma\u00df. Ein Gott wei\u00df aber,", "tokens": ["Das", "Ma\u00df", ".", "Ein", "Gott", "wei\u00df", "a\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Wenn kommet, was ich w\u00fcnsche, das Beste.", "tokens": ["Wenn", "kom\u00b7met", ",", "was", "ich", "w\u00fcn\u00b7sche", ",", "das", "Bes\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Denn wie der Meister", "tokens": ["Denn", "wie", "der", "Meis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Gewandelt auf Erden,", "tokens": ["Ge\u00b7wan\u00b7delt", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Ein gefangener Aar,", "tokens": ["Ein", "ge\u00b7fan\u00b7ge\u00b7ner", "Aar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Und viele, die", "tokens": ["Und", "vie\u00b7le", ",", "die"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "PIS", "$,", "PRELS"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ihn sahen, f\u00fcrchteten sich,", "tokens": ["Ihn", "sa\u00b7hen", ",", "f\u00fcrch\u00b7te\u00b7ten", "sich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PRF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dieweil sein \u00c4u\u00dferstes tat", "tokens": ["Die\u00b7weil", "sein", "\u00c4u\u00b7\u00dfers\u00b7tes", "tat"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Vater und sein Bestes unter", "tokens": ["Der", "Va\u00b7ter", "und", "sein", "Bes\u00b7tes", "un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den Menschen wirkete wirklich,", "tokens": ["Den", "Men\u00b7schen", "wir\u00b7ke\u00b7te", "wirk\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und sehr betr\u00fcbt war auch", "tokens": ["Und", "sehr", "be\u00b7tr\u00fcbt", "war", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Sohn so lange, bis er", "tokens": ["Der", "Sohn", "so", "lan\u00b7ge", ",", "bis", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "ADV", "$,", "KOUS", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gen Himmel fuhr in den L\u00fcften,", "tokens": ["Gen", "Him\u00b7mel", "fuhr", "in", "den", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Dem gleich ist gefangen die Seele der Helden.", "tokens": ["Dem", "gleich", "ist", "ge\u00b7fan\u00b7gen", "die", "See\u00b7le", "der", "Hel\u00b7den", "."], "token_info": 