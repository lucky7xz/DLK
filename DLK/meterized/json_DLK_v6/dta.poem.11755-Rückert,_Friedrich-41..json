{"dta.poem.11755": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "41.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der K\u00f6nig Adler hat das weitste K\u00f6nigreich,", "tokens": ["Der", "K\u00f6\u00b7nig", "Ad\u00b7ler", "hat", "das", "weits\u00b7te", "K\u00f6\u00b7nig\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von allen K\u00f6nigen ist ihm kein andrer gleich.", "tokens": ["Von", "al\u00b7len", "K\u00f6\u00b7ni\u00b7gen", "ist", "ihm", "kein", "an\u00b7drer", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPER", "PIAT", "PIS", "ADV", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Den weiten Himmelsraum mi\u00dft er mit seinen Schwingen,", "tokens": ["Den", "wei\u00b7ten", "Him\u00b7mels\u00b7raum", "mi\u00dft", "er", "mit", "sei\u00b7nen", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und l\u00e4\u00dft aus seiner H\u00f6h den Blick zur Erde dringen.", "tokens": ["Und", "l\u00e4\u00dft", "aus", "sei\u00b7ner", "H\u00f6h", "den", "Blick", "zur", "Er\u00b7de", "drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er hat die Sonn' im Aug' und sieht die Erde doch,", "tokens": ["Er", "hat", "die", "Sonn'", "im", "Aug'", "und", "sieht", "die", "Er\u00b7de", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "KON", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das tiefste sieht er klar, er schwebe noch so hoch.", "tokens": ["Das", "tiefs\u00b7te", "sieht", "er", "klar", ",", "er", "schwe\u00b7be", "noch", "so", "hoch", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "ADJD", "$,", "PPER", "VVFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und was am Erdengrund zur Beut' ihm mag gefallen,", "tokens": ["Und", "was", "am", "Er\u00b7den\u00b7grund", "zur", "Beut'", "ihm", "mag", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "NN", "APPRART", "NN", "PPER", "VMFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er kommt, er fa\u00dfts und tr\u00e4gts empor in seinen Krallen.", "tokens": ["Er", "kommt", ",", "er", "fa\u00dfts", "und", "tr\u00e4gts", "em\u00b7por", "in", "sei\u00b7nen", "Kral\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Auf seinem Baume sitzt der Weih und lauert still,", "tokens": ["Auf", "sei\u00b7nem", "Bau\u00b7me", "sitzt", "der", "Weih", "und", "lau\u00b7ert", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ihm zum Raube da vor\u00fcber kommen will.", "tokens": ["Was", "ihm", "zum", "Rau\u00b7be", "da", "vor\u00b7\u00fc\u00b7ber", "kom\u00b7men", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der Adler aber fliegt, es steht die Wahl ihm frei,", "tokens": ["Der", "Ad\u00b7ler", "a\u00b7ber", "fliegt", ",", "es", "steht", "die", "Wahl", "ihm", "frei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht was vorbei ihm kommt, er holt es selbst herbei.", "tokens": ["Nicht", "was", "vor\u00b7bei", "ihm", "kommt", ",", "er", "holt", "es", "selbst", "her\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PWS", "ADV", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Der Eule ist die Nacht zur Jagdzeit angewiesen,", "tokens": ["Der", "Eu\u00b7le", "ist", "die", "Nacht", "zur", "Jagd\u00b7zeit", "an\u00b7ge\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Mondschein ist ihr Freund, sie jagt nicht ohne diesen.", "tokens": ["Der", "Mond\u00b7schein", "ist", "ihr", "Freund", ",", "sie", "jagt", "nicht", "oh\u00b7ne", "die\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "APPR", "PDAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Die Bl\u00f6de sieht bei Nacht, doch gar nicht hell genung,", "tokens": ["Die", "Bl\u00f6\u00b7de", "sieht", "bei", "Nacht", ",", "doch", "gar", "nicht", "hell", "ge\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,", "ADV", "ADV", "PTKNEG", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und recht im Zwielicht nur zweideut'ger D\u00e4mmerung.", "tokens": ["Und", "recht", "im", "Zwie\u00b7licht", "nur", "zwei\u00b7deut'\u00b7ger", "D\u00e4m\u00b7me\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+--+--+--", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Drum wenn der Mond nicht scheint, kann sie bei Nacht nicht jagen,", "tokens": ["Drum", "wenn", "der", "Mond", "nicht", "scheint", ",", "kann", "sie", "bei", "Nacht", "nicht", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "ART", "NN", "PTKNEG", "VVFIN", "$,", "VMFIN", "PPER", "APPR", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und jagt zwei St\u00fcndchen nur im Sp\u00e4tlicht und vorm Tagen.", "tokens": ["Und", "jagt", "zwei", "St\u00fcnd\u00b7chen", "nur", "im", "Sp\u00e4t\u00b7licht", "und", "vorm", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "CARD", "NN", "ADV", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Der Adler aber schwingt sich mit der Sonnen auf,", "tokens": ["Der", "Ad\u00b7ler", "a\u00b7ber", "schwingt", "sich", "mit", "der", "Son\u00b7nen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und stellt auch seinen Flug nur ein mit ihrem Lauf.", "tokens": ["Und", "stellt", "auch", "sei\u00b7nen", "Flug", "nur", "ein", "mit", "ih\u00b7rem", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "ADV", "ART", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Fr\u00fch schaut er droben sie, noch eh die Welt sie sah,", "tokens": ["Fr\u00fch", "schaut", "er", "dro\u00b7ben", "sie", ",", "noch", "eh", "die", "Welt", "sie", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "PPER", "$,", "ADV", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Und schwand sie dieser l\u00e4ngst, ist noch ihr Glanz ihm nah.", "tokens": ["Und", "schwand", "sie", "die\u00b7ser", "l\u00e4ngst", ",", "ist", "noch", "ihr", "Glanz", "ihm", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDS", "VVFIN", "$,", "VAFIN", "ADV", "PPOSAT", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Und sieht er ihren Glanz dann hinterm fernsten Forst", "tokens": ["Und", "sieht", "er", "ih\u00b7ren", "Glanz", "dann", "hin\u00b7term", "ferns\u00b7ten", "Forst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich senken, senkt er sich und suchet seinen Horst.", "tokens": ["Sich", "sen\u00b7ken", ",", "senkt", "er", "sich", "und", "su\u00b7chet", "sei\u00b7nen", "Horst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "$,", "VVFIN", "PPER", "PRF", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Er hat zum Horst gew\u00e4hlt den allerfreisten Raum,", "tokens": ["Er", "hat", "zum", "Horst", "ge\u00b7w\u00e4hlt", "den", "al\u00b7ler\u00b7freis\u00b7ten", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf allerh\u00f6chstem Berg den allerh\u00f6chsten Baum.", "tokens": ["Auf", "al\u00b7ler\u00b7h\u00f6chs\u00b7tem", "Berg", "den", "al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Dort sitzt sein Adlerweib und br\u00fctet nur zwei Eier,", "tokens": ["Dort", "sitzt", "sein", "Ad\u00b7ler\u00b7weib", "und", "br\u00fc\u00b7tet", "nur", "zwei", "Ei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sie verst\u00f6ren darf kein Flatterer und Schreier.", "tokens": ["Und", "sie", "ver\u00b7st\u00f6\u00b7ren", "darf", "kein", "Flat\u00b7te\u00b7rer", "und", "Schrei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "VMFIN", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Denn keine Nachbarschaft von Vogel, Mensch und Thier", "tokens": ["Denn", "kei\u00b7ne", "Nach\u00b7bar\u00b7schaft", "von", "Vo\u00b7gel", ",", "Mensch", "und", "Thier"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "APPR", "NE", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vertr\u00e4gt der Adler, wo er hat sein Nachtquartier.", "tokens": ["Ver\u00b7tr\u00e4gt", "der", "Ad\u00b7ler", ",", "wo", "er", "hat", "sein", "Nacht\u00b7quar\u00b7tier", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Er wei\u00df aus seiner N\u00e4h die G\u00e4st' hinwegzutreiben,", "tokens": ["Er", "wei\u00df", "aus", "sei\u00b7ner", "N\u00e4h", "die", "G\u00e4st'", "hin\u00b7weg\u00b7zu\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und diese haben selbst schon keine Lust zu bleiben.", "tokens": ["Und", "die\u00b7se", "ha\u00b7ben", "selbst", "schon", "kei\u00b7ne", "Lust", "zu", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "ADV", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.17": {"line.1": {"text": "So wohnt er ungest\u00f6rt in seiner Einsamkeit,", "tokens": ["So", "wohnt", "er", "un\u00b7ge\u00b7st\u00f6rt", "in", "sei\u00b7ner", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sieht von der Erde nichts und nur den Himmel weit.", "tokens": ["Sieht", "von", "der", "Er\u00b7de", "nichts", "und", "nur", "den", "Him\u00b7mel", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PIS", "KON", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.18": {"line.1": {"text": "Die Kr\u00e4he mit Ged\u00f6rn deckt oben ihr Gemach,", "tokens": ["Die", "Kr\u00e4\u00b7he", "mit", "Ge\u00b7d\u00f6rn", "deckt", "o\u00b7ben", "ihr", "Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch nur der Himmel ist des Adlernestes Dach.", "tokens": ["Doch", "nur", "der", "Him\u00b7mel", "ist", "des", "Ad\u00b7ler\u00b7nes\u00b7tes", "Dach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.19": {"line.1": {"text": "Er l\u00e4\u00dft den Sturm der Nacht an sich vor\u00fcber brausen,", "tokens": ["Er", "l\u00e4\u00dft", "den", "Sturm", "der", "Nacht", "an", "sich", "vor\u00b7\u00fc\u00b7ber", "brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Stark wird sein str\u00e4ubendes Gefieder von dem Grausen.", "tokens": ["Stark", "wird", "sein", "str\u00e4u\u00b7ben\u00b7des", "Ge\u00b7fie\u00b7der", "von", "dem", "Grau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.20": {"line.1": {"text": "Und wenn der Sturm davon ihm eine Feder weht,", "tokens": ["Und", "wenn", "der", "Sturm", "da\u00b7von", "ihm", "ei\u00b7ne", "Fe\u00b7der", "weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein J\u00e4ger findet sie, der fr\u00fch zur Jagd ausgeht.", "tokens": ["Ein", "J\u00e4\u00b7ger", "fin\u00b7det", "sie", ",", "der", "fr\u00fch", "zur", "Jagd", "aus\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.21": {"line.1": {"text": "Er darf die Feder nicht zu andern Federn legen,", "tokens": ["Er", "darf", "die", "Fe\u00b7der", "nicht", "zu", "an\u00b7dern", "Fe\u00b7dern", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil Adlerfedern selbst den Trieb des Adlers hegen;", "tokens": ["Weil", "Ad\u00b7ler\u00b7fe\u00b7dern", "selbst", "den", "Trieb", "des", "Ad\u00b7lers", "he\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.22": {"line.1": {"text": "Und, wie der Aar hinweg die V\u00f6gel wehrt und treibt,", "tokens": ["Und", ",", "wie", "der", "Aar", "hin\u00b7weg", "die", "V\u00f6\u00b7gel", "wehrt", "und", "treibt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "APZR", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch ihre Federn sein Gefieder zehrt und reibt.", "tokens": ["Auch", "ih\u00b7re", "Fe\u00b7dern", "sein", "Ge\u00b7fie\u00b7der", "zehrt", "und", "reibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.23": {"line.1": {"text": "Der J\u00e4ger macht daraus des Pfeiles Federspiel;", "tokens": ["Der", "J\u00e4\u00b7ger", "macht", "da\u00b7raus", "des", "Pfei\u00b7les", "Fe\u00b7der\u00b7spiel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem aarbeschwingten Schaft w\u00e4hlt er den Aar zum Ziel.", "tokens": ["Dem", "aar\u00b7be\u00b7schwing\u00b7ten", "Schaft", "w\u00e4hlt", "er", "den", "Aar", "zum", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.24": {"line.1": {"text": "Der Adler in der Luft vom Pfeil getroffen spricht:", "tokens": ["Der", "Ad\u00b7ler", "in", "der", "Luft", "vom", "Pfeil", "ge\u00b7trof\u00b7fen", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPRART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nahmst du nicht von mir selbst die Kraft, du trafst mich nicht.", "tokens": ["Nahmst", "du", "nicht", "von", "mir", "selbst", "die", "Kraft", ",", "du", "trafst", "mich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "PPER", "ADV", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.25": {"line.1": {"text": "Der Adler sch\u00fcttelt aus der Brust den Pfeil, und schaut", "tokens": ["Der", "Ad\u00b7ler", "sch\u00fct\u00b7telt", "aus", "der", "Brust", "den", "Pfeil", ",", "und", "schaut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hinunter, wo f\u00fcr ihn gepflanzt ist Adlerkraut.", "tokens": ["Hin\u00b7un\u00b7ter", ",", "wo", "f\u00fcr", "ihn", "ge\u00b7pflanzt", "ist", "Ad\u00b7ler\u00b7kraut", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "APPR", "PPER", "VVPP", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.26": {"line.1": {"text": "Vom Adlerkraute heilt alsbald die Adlerwunde,", "tokens": ["Vom", "Ad\u00b7ler\u00b7krau\u00b7te", "heilt", "als\u00b7bald", "die", "Ad\u00b7ler\u00b7wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und in die L\u00fcfte schwingt sich wieder der Gesunde.", "tokens": ["Und", "in", "die", "L\u00fcf\u00b7te", "schwingt", "sich", "wie\u00b7der", "der", "Ge\u00b7sun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PRF", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.27": {"line.1": {"text": "Und wenn er einen Kreis hat um die Welt geschwungen,", "tokens": ["Und", "wenn", "er", "ei\u00b7nen", "Kreis", "hat", "um", "die", "Welt", "ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So l\u00e4\u00dft er sich aufs Nest herab zu seinen Jungen.", "tokens": ["So", "l\u00e4\u00dft", "er", "sich", "aufs", "Nest", "her\u00b7ab", "zu", "sei\u00b7nen", "Jun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.28": {"line.1": {"text": "Den beiden schaut er scharf ins Auge bis ins Mark,", "tokens": ["Den", "bei\u00b7den", "schaut", "er", "scharf", "ins", "Au\u00b7ge", "bis", "ins", "Mark", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "VVFIN", "APPRART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Pr\u00fcft ihre Krall' und Schwing', und findet beide stark.", "tokens": ["Pr\u00fcft", "ih\u00b7re", "Krall'", "und", "Schwing'", ",", "und", "fin\u00b7det", "bei\u00b7de", "stark", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "NN", "$,", "KON", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.29": {"line.1": {"text": "Sie halten sich am Nest mit scharfen Krallen fest,", "tokens": ["Sie", "hal\u00b7ten", "sich", "am", "Nest", "mit", "schar\u00b7fen", "Kral\u00b7len", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ohne Schonung st\u00f6\u00dft der Alte sie vom Nest.", "tokens": ["Doch", "oh\u00b7ne", "Scho\u00b7nung", "st\u00f6\u00dft", "der", "Al\u00b7te", "sie", "vom", "Nest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.30": {"line.1": {"text": "Denn fliegen lernt nur, wer zum Fliegen ist gezwungen,", "tokens": ["Denn", "flie\u00b7gen", "lernt", "nur", ",", "wer", "zum", "Flie\u00b7gen", "ist", "ge\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "ADV", "$,", "PWS", "APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn er zum Fliegen Kraft auch hat gleich Adlerjungen.", "tokens": ["Wenn", "er", "zum", "Flie\u00b7gen", "Kraft", "auch", "hat", "gleich", "Ad\u00b7ler\u00b7jun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "NN", "ADV", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.31": {"line.1": {"text": "Ein Junges sinkt hinab, alsob's kein Adler sei,", "tokens": ["Ein", "Jun\u00b7ges", "sinkt", "hin\u00b7ab", ",", "al\u00b7sob's", "kein", "Ad\u00b7ler", "sei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "KOUS", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das wird ein Jagdgeno\u00df f\u00fcr Eule dort und Weih.", "tokens": ["Das", "wird", "ein", "Ja