{"textgrid.poem.52840": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "London", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu London in der gro\u00dfen Stadt", "tokens": ["Zu", "Lon\u00b7don", "in", "der", "gro\u00b7\u00dfen", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von sechs Millionen Seelen", "tokens": ["Von", "sechs", "Mil\u00b7lion\u00b7en", "See\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Man gar nichts zur Zerstreuung hat", "tokens": ["Man", "gar", "nichts", "zur", "Zer\u00b7streu\u00b7ung", "hat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "ADV", "PIS", "APPRART", "NN", "VAFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Als wie das edle Stehlen.", "tokens": ["Als", "wie", "das", "ed\u00b7le", "Steh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Stadt liegt in dem Engelland", "tokens": ["Die", "Stadt", "liegt", "in", "dem", "En\u00b7gel\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und an dem Flu\u00df der Themse,", "tokens": ["Und", "an", "dem", "Flu\u00df", "der", "Them\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Matrosen laufen an dem Strand", "tokens": ["Mat\u00b7ro\u00b7sen", "lau\u00b7fen", "an", "dem", "Strand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kauen ihre Bremse.", "tokens": ["Und", "kau\u00b7en", "ih\u00b7re", "Brem\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Viel tausend Galgen man dort hat,", "tokens": ["Viel", "tau\u00b7send", "Gal\u00b7gen", "man", "dort", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "PIS", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von allen Qualit\u00e4ten,", "tokens": ["Von", "al\u00b7len", "Qua\u00b7li\u00b7t\u00e4\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es ist die wahre Wohlethat,", "tokens": ["Es", "ist", "die", "wah\u00b7re", "Woh\u00b7le\u00b7that", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn Einer dran geht fl\u00f6ten.", "tokens": ["Wenn", "Ei\u00b7ner", "dran", "geht", "fl\u00f6\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PAV", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "F\u00fcr g'meine Leut sind sie von Holz", "tokens": ["F\u00fcr", "g'\u00b7mei\u00b7ne", "Leut", "sind", "sie", "von", "Holz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "APPR", "NN"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Und Jedem nach dem Wuchse,", "tokens": ["Und", "Je\u00b7dem", "nach", "dem", "Wuch\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr Lords von Mahagoniholz,", "tokens": ["F\u00fcr", "Lords", "von", "Ma\u00b7ha\u00b7go\u00b7ni\u00b7holz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr Handwerksbursch von Buchse.", "tokens": ["F\u00fcr", "Hand\u00b7werks\u00b7bursch", "von", "Buch\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das ist von London die Geschicht'", "tokens": ["Das", "ist", "von", "Lon\u00b7don", "die", "Ge\u00b7schicht'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "NE", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Uebrigen ist Nebel,", "tokens": ["Im", "Ueb\u00b7ri\u00b7gen", "ist", "Ne\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Sonne scheint ihr Lebtag nicht,", "tokens": ["Die", "Son\u00b7ne", "scheint", "ihr", "Leb\u00b7tag", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sonst w\u00e4r' es nicht so \u00fcbel.", "tokens": ["Sonst", "w\u00e4r'", "es", "nicht", "so", "\u00fc\u00b7bel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}