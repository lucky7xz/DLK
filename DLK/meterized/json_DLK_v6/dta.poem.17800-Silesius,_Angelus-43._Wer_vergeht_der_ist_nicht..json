{"dta.poem.17800": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "43.  Wer vergeht/ der ist nicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Der S\u00fcnder ist nicht mehr. Wie? seh ich ihn doch stehn!", "tokens": ["Der", "S\u00fcn\u00b7der", "ist", "nicht", "mehr", ".", "Wie", "?", "seh", "ich", "ihn", "doch", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "$.", "PWAV", "$.", "VVFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "h\u00e4ttstu das rechte Licht/ du s\u00e4hest ihn vergehn.", "tokens": ["h\u00e4tts\u00b7tu", "das", "rech\u00b7te", "Licht", "/", "du", "s\u00e4\u00b7hest", "ihn", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$(", "PPER", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}