{"textgrid.poem.52719": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es wirft der Mensch gro\u00df und geringe", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wirft der Mensch gro\u00df und geringe", "tokens": ["Es", "wirft", "der", "Mensch", "gro\u00df", "und", "ge\u00b7rin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Versprechen in die Zukunft hin,", "tokens": ["Ver\u00b7spre\u00b7chen", "in", "die", "Zu\u00b7kunft", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er gibt sein heilig Wort f\u00fcr Dinge,", "tokens": ["Er", "gibt", "sein", "hei\u00b7lig", "Wort", "f\u00fcr", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJD", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die seine Macht am Ende fliehn.", "tokens": ["Die", "sei\u00b7ne", "Macht", "am", "En\u00b7de", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "H\u00e4lt man nicht Alle denn f\u00fcr Thoren", "tokens": ["H\u00e4lt", "man", "nicht", "Al\u00b7le", "denn", "f\u00fcr", "Tho\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PTKNEG", "PIAT", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die sich verkaufen ohne Noth \u2013?", "tokens": ["Die", "sich", "ver\u00b7kau\u00b7fen", "oh\u00b7ne", "Noth", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PRF", "VVINF", "APPR", "NN", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Tausend haben sich verschworen", "tokens": ["Und", "Tau\u00b7send", "ha\u00b7ben", "sich", "ver\u00b7schwo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und waren frisch und lebensroth!", "tokens": ["Und", "wa\u00b7ren", "frisch", "und", "le\u00b7bens\u00b7roth", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du sollst die Gegenwart erfassen,", "tokens": ["Du", "sollst", "die", "Ge\u00b7gen\u00b7wart", "er\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Genie\u00dfen deinen Augenblick,", "tokens": ["Ge\u00b7nie\u00b7\u00dfen", "dei\u00b7nen", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das K\u00fcnftige den G\u00f6ttern lassen!", "tokens": ["Das", "K\u00fcnf\u00b7ti\u00b7ge", "den", "G\u00f6t\u00b7tern", "las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn diese w\u00fcrfeln um dein Gl\u00fcck.", "tokens": ["Denn", "die\u00b7se", "w\u00fcr\u00b7feln", "um", "dein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wer ist ungl\u00fccklicher auf Erden", "tokens": ["Wer", "ist", "un\u00b7gl\u00fcck\u00b7li\u00b7cher", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als welchen Reu und Zweifel plagt?", "tokens": ["Als", "wel\u00b7chen", "Reu", "und", "Zwei\u00b7fel", "plagt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAT", "NE", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie soll man einzig gl\u00fccklich werden,", "tokens": ["Wie", "soll", "man", "ein\u00b7zig", "gl\u00fcck\u00b7lich", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PIS", "ADJD", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So man nicht ewig spielt und wagt?", "tokens": ["So", "man", "nicht", "e\u00b7wig", "spielt", "und", "wagt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PTKNEG", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}