{"dta.poem.13677": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Freunde .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Von See'n und W\u00e4ldern eine n\u00e4cht'ge Runde", "tokens": ["Von", "See'n", "und", "W\u00e4l\u00b7dern", "ei\u00b7ne", "n\u00e4cht'\u00b7ge", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sah ich, und Drachen zieh'n mit gl\u00fch'nden Schweifen,", "tokens": ["Sah", "ich", ",", "und", "Dra\u00b7chen", "zieh'n", "mit", "gl\u00fch'n\u00b7den", "Schwei\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Eicheswipfeln einen Horst von Greifen,", "tokens": ["In", "Ei\u00b7ches\u00b7wip\u00b7feln", "ei\u00b7nen", "Horst", "von", "Grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Nordlicht schr\u00e4ge leuchtend \u00fcber'm Grunde.", "tokens": ["Das", "Nord\u00b7licht", "schr\u00e4\u00b7ge", "leuch\u00b7tend", "\u00fc\u00b7ber'm", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}