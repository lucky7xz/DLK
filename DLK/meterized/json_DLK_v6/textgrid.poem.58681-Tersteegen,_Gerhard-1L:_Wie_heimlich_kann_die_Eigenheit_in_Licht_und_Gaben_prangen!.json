{"textgrid.poem.58681": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie heimlich kann die Eigenheit in Licht und Gaben prangen!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie heimlich kann die Eigenheit in Licht und Gaben prangen!", "tokens": ["Wie", "heim\u00b7lich", "kann", "die", "Ei\u00b7gen\u00b7heit", "in", "Licht", "und", "Ga\u00b7ben", "pran\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Dann lobt man Gott, solang er's macht so, wie wir's selbst verlangen,", "tokens": ["Dann", "lobt", "man", "Gott", ",", "so\u00b7lang", "er's", "macht", "so", ",", "wie", "wir's", "selbst", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "$,", "KOUS", "PIS", "VVFIN", "ADV", "$,", "PWAV", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Man nennet Gottes Wege recht, da man's selbst sehen kann,", "tokens": ["Man", "nen\u00b7net", "Got\u00b7tes", "We\u00b7ge", "recht", ",", "da", "man's", "selbst", "se\u00b7hen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "NN", "ADJD", "$,", "KOUS", "PIS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Nur in dem dunkeln Leidenstal, da geht das Tadeln an.", "tokens": ["Nur", "in", "dem", "dun\u00b7keln", "Lei\u00b7den\u00b7stal", ",", "da", "geht", "das", "Ta\u00b7deln", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Doch, Herr, du kannst dem\u00fctigen, du st\u00f6\u00dft vom Thron zur Erden", "tokens": ["Doch", ",", "Herr", ",", "du", "kannst", "de\u00b7m\u00fc\u00b7ti\u00b7gen", ",", "du", "st\u00f6\u00dft", "vom", "Thron", "zur", "Er\u00b7den"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "NN", "$,", "PPER", "VMFIN", "VVINF", "$,", "PPER", "VVFIN", "APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Und l\u00e4\u00dft ein dumm' und armes Tier dein Kind und K\u00f6nig werden,", "tokens": ["Und", "l\u00e4\u00dft", "ein", "dumm'", "und", "ar\u00b7mes", "Tier", "dein", "Kind", "und", "K\u00f6\u00b7nig", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "KON", "ADJA", "NN", "PPOSAT", "NN", "KON", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Der lobt und ehrt als K\u00f6nig dich und nennt sich selber Knecht:", "tokens": ["Der", "lobt", "und", "ehrt", "als", "K\u00f6\u00b7nig", "dich", "und", "nennt", "sich", "sel\u00b7ber", "Knecht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "KOUS", "NN", "PPER", "KON", "VVFIN", "PRF", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Dein Tun ist Wahrheit allzumal und deine Wege recht.", "tokens": ["Dein", "Tun", "ist", "Wahr\u00b7heit", "all\u00b7zu\u00b7mal", "und", "dei\u00b7ne", "We\u00b7ge", "recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "ADV", "KON", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}