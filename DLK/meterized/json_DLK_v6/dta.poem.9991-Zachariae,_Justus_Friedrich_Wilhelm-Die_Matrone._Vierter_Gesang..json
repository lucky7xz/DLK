{"dta.poem.9991": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Matrone.  \n   \n Vierter Gesang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20674-3", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schlage nun sanfter die Leyer, o Muse! Dein einsa-\nmes Lied auch", "tokens": ["Schla\u00b7ge", "nun", "sanf\u00b7ter", "die", "Le\u00b7yer", ",", "o", "Mu\u00b7se", "!", "Dein", "ein\u00b7sa", "mes", "Lied", "auch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADJD", "ART", "NN", "$,", "FM", "NN", "$.", "PPOSAT", "TRUNC", "ADJA", "NN", "ADV"], "meter": "+--+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.invert"}, "line.2": {"text": "Athme stille Melancholey, und Ruhe der Seele,", "tokens": ["Ath\u00b7me", "stil\u00b7le", "Me\u00b7lan\u00b7cho\u00b7ley", ",", "und", "Ru\u00b7he", "der", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,", "KON", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Und Entfernung vom Wirbel der Welt. Wie Tage des", "tokens": ["Und", "Ent\u00b7fer\u00b7nung", "vom", "Wir\u00b7bel", "der", "Welt", ".", "Wie", "Ta\u00b7ge", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "APPRART", "NN", "ART", "NN", "$.", "PWAV", "NN", "ART"], "meter": "--+---+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Nicht mit dem Glanze des Sommers geschm\u00fcckt, die", "tokens": ["Nicht", "mit", "dem", "Glan\u00b7ze", "des", "Som\u00b7mers", "ge\u00b7schm\u00fcckt", ",", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,", "PRELS"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Doch fehlt Anmuth auch nicht dem grauen wolkigten", "tokens": ["Doch", "fehlt", "An\u00b7muth", "auch", "nicht", "dem", "grau\u00b7en", "wol\u00b7kig\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "ADV", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Welcher das Antlitz der Sonne verdeckt; die ganze", "tokens": ["Wel\u00b7cher", "das", "Ant\u00b7litz", "der", "Son\u00b7ne", "ver\u00b7deckt", ";", "die", "gan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAT", "ART", "NN", "ART", "NN", "VVPP", "$.", "ART", "ADJA"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.7": {"text": "In sich gekehrt, und voll Ernst, und majest\u00e4tischen", "tokens": ["In", "sich", "ge\u00b7kehrt", ",", "und", "voll", "Ernst", ",", "und", "ma\u00b7jes\u00b7t\u00e4\u00b7ti\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PRF", "VVPP", "$,", "KON", "ADJD", "NN", "$,", "KON", "NN"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "So verfliessen die Tage der frommen Matrone. Die", "tokens": ["So", "ver\u00b7flies\u00b7sen", "die", "Ta\u00b7ge", "der", "from\u00b7men", "Mat\u00b7ro\u00b7ne", ".", "Die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$.", "ART"], "meter": "--+--+--+--+-+", "measure": "anapaest.tetra.plus"}, "line.9": {"text": "Frischer Wehmuth str\u00f6men nicht mehr um die Urne des", "tokens": ["Fri\u00b7scher", "Weh\u00b7muth", "str\u00f6\u00b7men", "nicht", "mehr", "um", "die", "Ur\u00b7ne", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PTKNEG", "ADV", "APPR", "ART", "NN", "ART"], "meter": "+-+-+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Aber mit stillerer Schwermuth, und melancholischen", "tokens": ["A\u00b7ber", "mit", "stil\u00b7le\u00b7rer", "Schwer\u00b7muth", ",", "und", "me\u00b7lan\u00b7cho\u00b7li\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "$,", "KON", "VVINF"], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.11": {"text": "W\u00f6lkt sich ihr Leben. Mit silbernen Locken bedecket", "tokens": ["W\u00f6lkt", "sich", "ihr", "Le\u00b7ben", ".", "Mit", "sil\u00b7ber\u00b7nen", "Lo\u00b7cken", "be\u00b7de\u00b7cket"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "$.", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.12": {"text": "Jhr ehrw\u00fcrdiges Haupt. Die alles zerst\u00f6rende Zeit hat", "tokens": ["Ihr", "ehr\u00b7w\u00fcr\u00b7di\u00b7ges", "Haupt", ".", "Die", "al\u00b7les", "zer\u00b7st\u00f6\u00b7ren\u00b7de", "Zeit", "hat"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "ART", "PIAT", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "In dem Gesicht noch blendende Tr\u00fcmmer von Sch\u00f6n-", "tokens": ["In", "dem", "Ge\u00b7sicht", "noch", "blen\u00b7den\u00b7de", "Tr\u00fcm\u00b7mer", "von", "Sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "ADJA", "NN", "APPR", "TRUNC"], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.14": {"text": "Ordnung und Reinlichkeit herrschen um sie, und der", "tokens": ["Ord\u00b7nung", "und", "Rein\u00b7lich\u00b7keit", "herr\u00b7schen", "um", "sie", ",", "und", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "PPER", "$,", "KON", "ART"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Wird dadurch milder und sanft. Jhr stiller bescheidener", "tokens": ["Wird", "da\u00b7durch", "mil\u00b7der", "und", "sanft", ".", "Ihr", "stil\u00b7ler", "be\u00b7schei\u00b7de\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PAV", "ADJD", "KON", "ADJD", "$.", "PPOSAT", "ADJA", "NN"], "meter": "+--+--+-+--+--", "measure": "dactylic.di.plus"}, "line.16": {"text": "Trauert noch immer geheim um den Mann. Entfernt", "tokens": ["Trau\u00b7ert", "noch", "im\u00b7mer", "ge\u00b7heim", "um", "den", "Mann", ".", "Ent\u00b7fernt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ADV", "ADV", "ADJD", "APPR", "ART", "NN", "$.", "NN"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.17": {"text": "Und dem wilden Ger\u00e4usche der Welt, verh\u00fcllt sie ihr", "tokens": ["Und", "dem", "wil\u00b7den", "Ge\u00b7r\u00e4u\u00b7sche", "der", "Welt", ",", "ver\u00b7h\u00fcllt", "sie", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "PPER"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.18": {"text": "Vor dem Schwarme der th\u00f6richten Freuden, vor leerer", "tokens": ["Vor", "dem", "Schwar\u00b7me", "der", "th\u00f6\u00b7rich\u00b7ten", "Freu\u00b7den", ",", "vor", "lee\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,", "APPR", "ADJA"], "meter": "--+--++-+--+-", "measure": "anapaest.di.plus"}, "line.19": {"text": "Und der Eitelkeit scheckigtem Zug. Nie hat sie der Tadel", "tokens": ["Und", "der", "Ei\u00b7tel\u00b7keit", "sche\u00b7ckig\u00b7tem", "Zug", ".", "Nie", "hat", "sie", "der", "Ta\u00b7del"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$.", "ADV", "VAFIN", "PPER", "ART", "NN"], "meter": "--+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "An dem Spieltisch gesehn, und unter den n\u00e4chtlichen", "tokens": ["An", "dem", "Spiel\u00b7tisch", "ge\u00b7sehn", ",", "und", "un\u00b7ter", "den", "n\u00e4cht\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "KON", "APPR", "ART", "ADJA"], "meter": "--+--+-+--+--", "measure": "anapaest.di.plus"}, "line.21": {"text": "Wo so viel verbl\u00fchte Gesichter ihr Alter entehren.", "tokens": ["Wo", "so", "viel", "ver\u00b7bl\u00fch\u00b7te", "Ge\u00b7sich\u00b7ter", "ihr", "Al\u00b7ter", "en\u00b7teh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "ADJA", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Still und einsam lebt sie dahin. Die w\u00fcrdigen T\u00f6chter", "tokens": ["Still", "und", "ein\u00b7sam", "lebt", "sie", "da\u00b7hin", ".", "Die", "w\u00fcr\u00b7di\u00b7gen", "T\u00f6ch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "PPER", "PAV", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.23": {"text": "Hat sie schon lang an M\u00e4nner gegeben, und lange schon", "tokens": ["Hat", "sie", "schon", "lang", "an", "M\u00e4n\u00b7ner", "ge\u00b7ge\u00b7ben", ",", "und", "lan\u00b7ge", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "APPR", "NN", "VVPP", "$,", "KON", "ADV", "ADV"], "meter": "-+-+-+---+-+-+", "measure": "unknown.measure.hexa"}, "line.24": {"text": "Von den S\u00f6hnen gesehn. Jhr reiches gesegnetes Haus", "tokens": ["Von", "den", "S\u00f6h\u00b7nen", "ge\u00b7sehn", ".", "Ihr", "rei\u00b7ches", "ge\u00b7se\u00b7gne\u00b7tes", "Haus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "$.", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.25": {"text": "Tief in gl\u00fccklicher Ruhe vergraben. Die heilige", "tokens": ["Tief", "in", "gl\u00fcck\u00b7li\u00b7cher", "Ru\u00b7he", "ver\u00b7gra\u00b7ben", ".", "Die", "hei\u00b7li\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVPP", "$.", "ART", "ADJA"], "meter": "+-+--+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.26": {"text": "Betender Furien murmelt nie drinn; auch schallt nie", "tokens": ["Be\u00b7ten\u00b7der", "Fu\u00b7ri\u00b7en", "mur\u00b7melt", "nie", "drinn", ";", "auch", "schallt", "nie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "PTKVZ", "$.", "ADV", "VVFIN", "ADV"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.27": {"text": "Pralender Andacht in horchende Gassen, und fr\u00f6hnet", "tokens": ["Pra\u00b7len\u00b7der", "An\u00b7dacht", "in", "hor\u00b7chen\u00b7de", "Gas\u00b7sen", ",", "und", "fr\u00f6h\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$,", "KON", "VVFIN"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.28": {"text": "Majest\u00e4tisch und ernstlich sitzt sie am ruhigen Abend", "tokens": ["Ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "und", "ernst\u00b7lich", "sitzt", "sie", "am", "ru\u00b7hi\u00b7gen", "A\u00b7bend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.29": {"text": "Mitten unter dem Kreis der horchenden Enkel, und", "tokens": ["Mit\u00b7ten", "un\u00b7ter", "dem", "Kreis", "der", "hor\u00b7chen\u00b7den", "En\u00b7kel", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,", "KON"], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.30": {"text": "Die noch ungebildeten Herzen mit Lehren der Tugend,", "tokens": ["Die", "noch", "un\u00b7ge\u00b7bil\u00b7de\u00b7ten", "Her\u00b7zen", "mit", "Leh\u00b7ren", "der", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.31": {"text": "Die ihr eigenes Beyspiel best\u00e4rkt. Sie wei\u00df die Ge-", "tokens": ["Die", "ihr", "ei\u00b7ge\u00b7nes", "Bey\u00b7spiel", "be\u00b7st\u00e4rkt", ".", "Sie", "wei\u00df", "die", "Ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVPP", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.32": {"text": "Lange verflossener Zeit. Der Kreis umringet sie n\u00e4her,", "tokens": ["Lan\u00b7ge", "ver\u00b7flos\u00b7se\u00b7ner", "Zeit", ".", "Der", "Kreis", "um\u00b7rin\u00b7get", "sie", "n\u00e4\u00b7her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$.", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.33": {"text": "Und h\u00e4ngt am erzehlenden Munde, bis \u00fcber die Erde", "tokens": ["Und", "h\u00e4ngt", "am", "er\u00b7zeh\u00b7len\u00b7den", "Mun\u00b7de", ",", "bis", "\u00fc\u00b7ber", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.34": {"text": "Tiefe Mitternacht f\u00e4llt, und s\u00fcsser Schlummer herab-", "tokens": ["Tie\u00b7fe", "Mit\u00b7ter\u00b7nacht", "f\u00e4llt", ",", "und", "s\u00fcs\u00b7ser", "Schlum\u00b7mer", "her\u00b7ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "$,", "KON", "ADJA", "NN", "TRUNC"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.35": {"text": "Mit dem Tode bekannt, und mit der Zukunft besch\u00e4f-", "tokens": ["Mit", "dem", "To\u00b7de", "be\u00b7kannt", ",", "und", "mit", "der", "Zu\u00b7kunft", "be\u00b7sch\u00e4f"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,", "KON", "APPR", "ART", "NN", "TRUNC"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.36": {"text": "Betet sie oft, und besuchet voll Andacht die Tempel der", "tokens": ["Be\u00b7tet", "sie", "oft", ",", "und", "be\u00b7su\u00b7chet", "voll", "An\u00b7dacht", "die", "Tem\u00b7pel", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KON", "VVFIN", "ADJD", "NN", "ART", "NN", "ART"], "meter": "+--+--+--++-+--", "measure": "dactylic.tri.plus"}, "line.37": {"text": "Ueber ihr graues Haupt sind ihr in langer Erfahrung", "tokens": ["Ue\u00b7ber", "ihr", "grau\u00b7es", "Haupt", "sind", "ihr", "in", "lan\u00b7ger", "Er\u00b7fah\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.38": {"text": "Jahre, nicht immer mit Freuden bemerkt, vor\u00fcber ge-", "tokens": ["Jah\u00b7re", ",", "nicht", "im\u00b7mer", "mit", "Freu\u00b7den", "be\u00b7merkt", ",", "vor\u00b7\u00fc\u00b7ber", "ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PTKNEG", "ADV", "APPR", "NN", "VVPP", "$,", "ADV", "TRUNC"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.39": {"text": "Doch auch Ungl\u00fcck machte sie weiser; sie ist das Orakel", "tokens": ["Doch", "auch", "Un\u00b7gl\u00fcck", "mach\u00b7te", "sie", "wei\u00b7ser", ";", "sie", "ist", "das", "O\u00b7ra\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VAFIN", "ART", "NN"], "meter": "--+-+--+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.40": {"text": "Jhrer Gegenden. Bl\u00fchender stehn die Wiesen am", "tokens": ["Ih\u00b7rer", "Ge\u00b7gen\u00b7den", ".", "Bl\u00fc\u00b7hen\u00b7der", "stehn", "die", "Wie\u00b7sen", "am"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "NN", "VVFIN", "ART", "NN", "APPRART"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.41": {"text": "Und voll reicherer Aehren die Aecker. Am lachenden", "tokens": ["Und", "voll", "rei\u00b7che\u00b7rer", "A\u00b7eh\u00b7ren", "die", "A\u00b7e\u00b7cker", ".", "Am", "la\u00b7chen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "ADJA", "NN", "ART", "NN", "$.", "APPRART", "ADJA"], "meter": "+-+-+-+--++--+--", "measure": "trochaic.septa.relaxed"}, "line.42": {"text": "Beugt sich ihr Weinstock mit v\u00f6lleren Trauben; sie", "tokens": ["Beugt", "sich", "ihr", "Wein\u00b7stock", "mit", "v\u00f6l\u00b7le\u00b7ren", "Trau\u00b7ben", ";", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$.", "PPER"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.43": {"text": "Und der Himmel erh\u00f6ret ihr Flehn. Oft hat sie dem", "tokens": ["Und", "der", "Him\u00b7mel", "er\u00b7h\u00f6\u00b7ret", "ihr", "Flehn", ".", "Oft", "hat", "sie", "dem"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "VVINF", "$.", "ADV", "VAFIN", "PPER", "ART"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.44": {"text": "Eine z\u00e4rtliche Gattin gerettet, in traurigen N\u00e4chten", "tokens": ["Ei\u00b7ne", "z\u00e4rt\u00b7li\u00b7che", "Gat\u00b7tin", "ge\u00b7ret\u00b7tet", ",", "in", "trau\u00b7ri\u00b7gen", "N\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "APPR", "ADJA", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.45": {"text": "Sie mit Trost und Beystand gest\u00e4rkt, wenn unter den", "tokens": ["Sie", "mit", "Trost", "und", "Beys\u00b7tand", "ge\u00b7st\u00e4rkt", ",", "wenn", "un\u00b7ter", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "VVPP", "$,", "KOUS", "APPR", "ART"], "meter": "--+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.46": {"text": "Ganz sie erlag, und die Freude nicht f\u00fchlte, nun Mut-", "tokens": ["Ganz", "sie", "er\u00b7lag", ",", "und", "die", "Freu\u00b7de", "nicht", "f\u00fchl\u00b7te", ",", "nun", "Mut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "$,", "KON", "ART", "NN", "PTKNEG", "VVFIN", "$,", "ADV", "TRUNC"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.47": {"text": "Kl\u00fcglich wei\u00df sie zu rathen, wenn in den Sorgen der", "tokens": ["Kl\u00fcg\u00b7lich", "wei\u00df", "sie", "zu", "ra\u00b7then", ",", "wenn", "in", "den", "Sor\u00b7gen", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PTKZU", "VVINF", "$,", "KOUS", "APPR", "ART", "NN", "ART"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.48": {"text": "Unerfahren, die j\u00fcngere Frau in Fehlern verstrickt ist.", "tokens": ["Un\u00b7er\u00b7fah\u00b7ren", ",", "die", "j\u00fcn\u00b7ge\u00b7re", "Frau", "in", "Feh\u00b7lern", "ver\u00b7strickt", "ist", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "APPR", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.49": {"text": "Bald gewinnt das verworrene Haus ein gl\u00fccklicher An", "tokens": ["Bald", "ge\u00b7winnt", "das", "ver\u00b7wor\u00b7re\u00b7ne", "Haus", "ein", "gl\u00fcck\u00b7li\u00b7cher", "An"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "--+--+--+-+--+", "measure": "anapaest.tri.plus"}, "line.50": {"text": "Durch die Ordnung der klugen Matrone. Die muthi-", "tokens": ["Durch", "die", "Ord\u00b7nung", "der", "klu\u00b7gen", "Mat\u00b7ro\u00b7ne", ".", "Die", "muthi"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$.", "ART", "TRUNC"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.51": {"text": "Ziehn mit dem Tage zum Acker. Die H\u00e4nde der fleis-", "tokens": ["Ziehn", "mit", "dem", "Ta\u00b7ge", "zum", "A\u00b7cker", ".", "Die", "H\u00e4n\u00b7de", "der", "fleis"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$.", "ART", "NN", "ART", "TRUNC"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.52": {"text": "F\u00fcllen nun wieder die staubichte Spindel, und machen", "tokens": ["F\u00fcl\u00b7len", "nun", "wie\u00b7der", "die", "stau\u00b7bich\u00b7te", "Spin\u00b7del", ",", "und", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "ADV", "ART", "ADJA", "NN", "$,", "KON", "VVINF"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.53": {"text": "Ringsum mit blendender Leinwand bedeckt. Die fei-", "tokens": ["Ring\u00b7sum", "mit", "blen\u00b7den\u00b7der", "Lein\u00b7wand", "be\u00b7deckt", ".", "Die", "fei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVPP", "$.", "ART", "TRUNC"], "meter": "+--+--++-+-+", "measure": "dactylic.di.plus"}, "line.54": {"text": "Kommen mit vollen Eutern zur\u00fcck; und der treuere", "tokens": ["Kom\u00b7men", "mit", "vol\u00b7len", "Eu\u00b7tern", "zu\u00b7r\u00fcck", ";", "und", "der", "treu\u00b7e\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "PTKVZ", "$.", "KON", "ART", "ADJA"], "meter": "+--+-+--+--++-", "measure": "iambic.hexa.invert"}, "line.55": {"text": "L\u00e4\u00dft die Scheere mit Jauchzen erklingen, und f\u00fcllet", "tokens": 