{"dta.poem.3611": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "1.  Kampf des erw\u00e4hlten Volkes .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf, auf, auf ihr Helden, waget Gut und               ", "tokens": ["Auf", ",", "auf", ",", "auf", "ihr", "Hel\u00b7den", ",", "wa\u00b7get", "Gut", "und"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "$,", "PTKVZ", "$,", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "ADJD", "KON"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Blut,", "tokens": ["Blut", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "W\u00fcrget mit vereinten Kr\u00e4ften Babels Brut!", "tokens": ["W\u00fcr\u00b7get", "mit", "ver\u00b7ein\u00b7ten", "Kr\u00e4f\u00b7ten", "Ba\u00b7bels", "Brut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "NE", "NE", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Eure Feldposaunen,", "tokens": ["Eu\u00b7re", "Feld\u00b7po\u00b7sau\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Trommeln und Kartaunen,", "tokens": ["Trom\u00b7meln", "und", "Kar\u00b7tau\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Lasset t\u00f6nen und erwecken L\u00f6wenmuth.", "tokens": ["Las\u00b7set", "t\u00f6\u00b7nen", "und", "er\u00b7we\u00b7cken", "L\u00f6\u00b7wen\u00b7muth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVINF", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Wann die Blutfahn flieget, so seyd unverzagt,", "tokens": ["Wann", "die", "Blut\u00b7fahn", "flie\u00b7get", ",", "so", "seyd", "un\u00b7ver\u00b7zagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "ADV", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Josua hat vor euch schon den Feind verjagt!", "tokens": ["Jo\u00b7sua", "hat", "vor", "euch", "schon", "den", "Feind", "ver\u00b7jagt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Unser L\u00f6we br\u00fcllet,", "tokens": ["Un\u00b7ser", "L\u00f6\u00b7we", "br\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und mit Schrecken f\u00fcllet", "tokens": ["Und", "mit", "Schre\u00b7cken", "f\u00fcl\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Das Heer der Assyrer, so sich an uns wagt.", "tokens": ["Das", "Heer", "der", "As\u00b7sy\u00b7rer", ",", "so", "sich", "an", "uns", "wagt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADV", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Auf, auf, zuckt die Schwerdter, schlaget muthig", "tokens": ["Auf", ",", "auf", ",", "zuckt", "die", "Schwerd\u00b7ter", ",", "schla\u00b7get", "mut\u00b7hig"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "$,", "PTKVZ", "$,", "VVFIN", "ART", "NN", "$,", "VVFIN", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "drein,", "tokens": ["drein", ","], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "St\u00fcrmt die Th\u00fcrme Babels, rei\u00dft die Mauern ein.", "tokens": ["St\u00fcrmt", "die", "Th\u00fcr\u00b7me", "Ba\u00b7bels", ",", "rei\u00dft", "die", "Mau\u00b7ern", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NE", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Auf, sie sollen fallen,", "tokens": ["Auf", ",", "sie", "sol\u00b7len", "fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wenn Posaunen schallen,", "tokens": ["Wenn", "Po\u00b7sau\u00b7nen", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Denn die Stunde, sie zu richten, bricht herein.", "tokens": ["Denn", "die", "Stun\u00b7de", ",", "sie", "zu", "rich\u00b7ten", ",", "bricht", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Du o Jesu f\u00fchre selbsten deinen Krieg,", "tokens": ["Du", "o", "Je\u00b7su", "f\u00fch\u00b7re", "selbs\u00b7ten", "dei\u00b7nen", "Krieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "FM", "NE", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "In uns, durch uns, mit uns, da\u00df der Feind erlieg.", "tokens": ["In", "uns", ",", "durch", "uns", ",", "mit", "uns", ",", "da\u00df", "der", "Feind", "er\u00b7lieg", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "$,", "APPR", "PPER", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "In der Kraft erscheinen", "tokens": ["In", "der", "Kraft", "er\u00b7schei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wir nun als die deinen,", "tokens": ["Wir", "nun", "als", "die", "dei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KOUS", "ART", "PPOSAT", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "K\u00f6nnen triumphiren nach erlangtem Sieg.", "tokens": ["K\u00f6n\u00b7nen", "tri\u00b7um\u00b7phi\u00b7ren", "nach", "er\u00b7lang\u00b7tem", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}