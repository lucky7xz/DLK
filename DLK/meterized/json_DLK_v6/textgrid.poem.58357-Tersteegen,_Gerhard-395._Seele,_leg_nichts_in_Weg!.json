{"textgrid.poem.58357": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "395. Seele, leg nichts in Weg!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ei frage nimmer nicht,", "tokens": ["Ei", "fra\u00b7ge", "nim\u00b7mer", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Was hier und da geschicht", "tokens": ["Was", "hier", "und", "da", "ge\u00b7schicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "KON", "ADV", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In \u00e4u\u00dferlichen Dingen!", "tokens": ["In", "\u00e4u\u00b7\u00dfer\u00b7li\u00b7chen", "Din\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du nimmst nur Bilder ein,", "tokens": ["Du", "nimmst", "nur", "Bil\u00b7der", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die dir ein Vorhang sein,", "tokens": ["Die", "dir", "ein", "Vor\u00b7hang", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Nicht blo\u00df in Gott zu dringen.", "tokens": ["Nicht", "blo\u00df", "in", "Gott", "zu", "drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}