{"dta.poem.6548": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Farben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Bogen, welchen uns die Lufft zeigt, nach den St\u00fcrmen,", "tokens": ["Der", "Bo\u00b7gen", ",", "wel\u00b7chen", "uns", "die", "Lufft", "zeigt", ",", "nach", "den", "St\u00fcr\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAT", "PPER", "ART", "NN", "VVFIN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das weite blaue Feld, zusammt den g\u00fcldnen Th\u00fcrmen,", "tokens": ["Das", "wei\u00b7te", "blau\u00b7e", "Feld", ",", "zu\u00b7sammt", "den", "g\u00fcld\u00b7nen", "Th\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Wunder-Thier\u2019 und Riesen in der Lufft,", "tokens": ["Die", "Wun\u00b7der\u00b7Thier'", "und", "Rie\u00b7sen", "in", "der", "Lufft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das seltzame Gemisch von so verwirrten Bildern,", "tokens": ["Das", "selt\u00b7za\u00b7me", "Ge\u00b7misch", "von", "so", "ver\u00b7wirr\u00b7ten", "Bil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Die auf der Wolcken Dufft,", "tokens": ["Die", "auf", "der", "Wol\u00b7cken", "Dufft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sich auf so fremde Weise schildern,", "tokens": ["Sich", "auf", "so", "frem\u00b7de", "Wei\u00b7se", "schil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sind C\u00f6rper, die durchsichtig, rein", "tokens": ["Sind", "C\u00f6r\u00b7per", ",", "die", "durch\u00b7sich\u00b7tig", ",", "rein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VAFIN", "NE", "$,", "PRELS", "ADJD", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und ohn Figuren seyn.", "tokens": ["Und", "ohn", "Fi\u00b7gu\u00b7ren", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Die Farben alle nun, die unsre Augen", "tokens": ["Die", "Far\u00b7ben", "al\u00b7le", "nun", ",", "die", "uns\u00b7re", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PIS", "ADV", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Auf so verschiedne Weis\u2019 offt zu vergn\u00fcgen taugen,", "tokens": ["Auf", "so", "ver\u00b7schied\u00b7ne", "Weis'", "offt", "zu", "ver\u00b7gn\u00fc\u00b7gen", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sind nichts als Strahlen, die verschiedentlich,", "tokens": ["Sind", "nichts", "als", "Strah\u00b7len", ",", "die", "ver\u00b7schie\u00b7dent\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "KOKOM", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Bald mehr, bald minder sich", "tokens": ["Bald", "mehr", ",", "bald", "min\u00b7der", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "ADV", "ADV", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Versammlen und verlieren:", "tokens": ["Ver\u00b7samm\u00b7len", "und", "ver\u00b7lie\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Und so, als wie die Wolcke, die sie schm\u00fcckt,", "tokens": ["Und", "so", ",", "als", "wie", "die", "Wol\u00b7cke", ",", "die", "sie", "schm\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "KOKOM", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "In sichre Winckel sie, gebrochen, r\u00fcckwerts schickt;", "tokens": ["In", "sich\u00b7re", "Win\u00b7ckel", "sie", ",", "ge\u00b7bro\u00b7chen", ",", "r\u00fcck\u00b7werts", "schickt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "$,", "VVPP", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sind ihre Theile bunt und sch\u00f6n gef\u00e4rbt zu sp\u00fchren.", "tokens": ["Sind", "ih\u00b7re", "Thei\u00b7le", "bunt", "und", "sch\u00f6n", "ge\u00b7f\u00e4rbt", "zu", "sp\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}