{"textgrid.poem.56275": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Familienfest", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Vater ging auf die Jagd in den Wald;", "tokens": ["Der", "Va\u00b7ter", "ging", "auf", "die", "Jagd", "in", "den", "Wald", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein gutes Wild ersah er sich bald.", "tokens": ["Ein", "gu\u00b7tes", "Wild", "er\u00b7sah", "er", "sich", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Er legte wohl an, er dr\u00fcckte los,", "tokens": ["Er", "leg\u00b7te", "wohl", "an", ",", "er", "dr\u00fcck\u00b7te", "los", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Sperling fiel auf das weiche Moos.", "tokens": ["Der", "Sper\u00b7ling", "fiel", "auf", "das", "wei\u00b7che", "Moos", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+----+-+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Die Br\u00fcder luden zu Schlitten den Fang,", "tokens": ["Die", "Br\u00fc\u00b7der", "lu\u00b7den", "zu", "Schlit\u00b7ten", "den", "Fang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und schleiften ihn heim, und jubelten lang.", "tokens": ["Und", "schleif\u00b7ten", "ihn", "heim", ",", "und", "ju\u00b7bel\u00b7ten", "lang", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Die T\u00f6chter schnell das Feuer gesch\u00fcrt,", "tokens": ["Die", "T\u00f6ch\u00b7ter", "schnell", "das", "Feu\u00b7er", "ge\u00b7sch\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sie rupften und sengten ihn, wie sich's geb\u00fchrt.", "tokens": ["Sie", "rupf\u00b7ten", "und", "seng\u00b7ten", "ihn", ",", "wie", "sich's", "ge\u00b7b\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "$,", "PWAV", "PIS", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Die Mutter briet und schmort' ihn gleich,", "tokens": ["Die", "Mut\u00b7ter", "briet", "und", "schmort'", "ihn", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Braten war k\u00f6stlich und schmackhaft und weich.", "tokens": ["Der", "Bra\u00b7ten", "war", "k\u00f6st\u00b7lich", "und", "schmack\u00b7haft", "und", "weich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Gesch\u00e4ftig trugen die Schwestern ihn auf;", "tokens": ["Ge\u00b7sch\u00e4f\u00b7tig", "tru\u00b7gen", "die", "Schwes\u00b7tern", "ihn", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es kamen die fr\u00f6hlichen G\u00e4ste zu Hauf.", "tokens": ["Es", "ka\u00b7men", "die", "fr\u00f6h\u00b7li\u00b7chen", "G\u00e4s\u00b7te", "zu", "Hauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Sie setzten zu Tisch sich und sa\u00dfen fest,", "tokens": ["Sie", "setz\u00b7ten", "zu", "Tisch", "sich", "und", "sa\u00b7\u00dfen", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PRF", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "und taten sich g\u00fctlich beim weidlichen Fest.", "tokens": ["und", "ta\u00b7ten", "sich", "g\u00fct\u00b7lich", "beim", "weid\u00b7li\u00b7chen", "Fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.8": {"line.1": {"text": "Sie schmausten den Sperling in guter Ruh,", "tokens": ["Sie", "schmaus\u00b7ten", "den", "Sper\u00b7ling", "in", "gu\u00b7ter", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und tranken drei F\u00e4sser des Bieres dazu.", "tokens": ["Und", "tran\u00b7ken", "drei", "F\u00e4s\u00b7ser", "des", "Bie\u00b7res", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "CARD", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}