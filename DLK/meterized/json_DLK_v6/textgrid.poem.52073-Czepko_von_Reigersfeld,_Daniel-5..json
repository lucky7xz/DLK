{"textgrid.poem.52073": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein edle Dame fuhr auff eine Gasterey,", "tokens": ["Ein", "ed\u00b7le", "Da\u00b7me", "fuhr", "auff", "ei\u00b7ne", "Gas\u00b7te\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da meine gute Freund auch in Gesellschafft waren,", "tokens": ["Da", "mei\u00b7ne", "gu\u00b7te", "Freund", "auch", "in", "Ge\u00b7sell\u00b7schafft", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADV", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gedenckt, Sie kommt kaum heim, so kindert sie zu paaren,", "tokens": ["Ge\u00b7denckt", ",", "Sie", "kommt", "kaum", "heim", ",", "so", "kin\u00b7dert", "sie", "zu", "paa\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "VVFIN", "ADV", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein Fra\u00fclein, so kommstu der R\u00f6mschen Lupe bey.", "tokens": ["Mein", "Fra\u00fc\u00b7lein", ",", "so", "komms\u00b7tu", "der", "R\u00f6m\u00b7schen", "Lu\u00b7pe", "bey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}