{"textgrid.poem.48500": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "3. Abba, mein Vater", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, Vater, kan es sein, so schone deines Sohnes!", "tokens": ["Ach", ",", "Va\u00b7ter", ",", "kan", "es", "sein", ",", "so", "scho\u00b7ne", "dei\u00b7nes", "Soh\u00b7nes", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "VMFIN", "PPER", "VAINF", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wo nicht, so sei es nur! Mir ziemt zu folgen dir.", "tokens": ["wo", "nicht", ",", "so", "sei", "es", "nur", "!", "Mir", "ziemt", "zu", "fol\u00b7gen", "dir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "ADV", "VAFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "PTKZU", "VVINF", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mensch, wenn du h\u00f6rest das, wie der des h\u00f6chsten Thrones", "tokens": ["Mensch", ",", "wenn", "du", "h\u00f6\u00b7rest", "das", ",", "wie", "der", "des", "h\u00f6chs\u00b7ten", "Thro\u00b7nes"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "VVFIN", "PDS", "$,", "PWAV", "ART", "ART", "ADJA", "NN"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.4": {"text": "alleine weiser Rat so kl\u00e4glich weinet hier", "tokens": ["al\u00b7lei\u00b7ne", "wei\u00b7ser", "Rat", "so", "kl\u00e4g\u00b7lich", "wei\u00b7net", "hier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "ADV", "ADJD", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "aus \u00c4ngsten seiner Qual, so denke, was f\u00fcr Schmerzen", "tokens": ["aus", "\u00c4ngs\u00b7ten", "sei\u00b7ner", "Qual", ",", "so", "den\u00b7ke", ",", "was", "f\u00fcr", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "du soltest stehen aus von wegen deiner Schuld!", "tokens": ["du", "sol\u00b7test", "ste\u00b7hen", "aus", "von", "we\u00b7gen", "dei\u00b7ner", "Schuld", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVFIN", "APPR", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Lern' auch die Betkunst hier und sprich, wie er, von Herzen:", "tokens": ["Lern'", "auch", "die", "Bet\u00b7kunst", "hier", "und", "sprich", ",", "wie", "er", ",", "von", "Her\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "KON", "ADJD", "$,", "PWAV", "PPER", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Kans sein, so tu's! wo nicht, so leid' ich mit Geduld.", "tokens": ["Kans", "sein", ",", "so", "tu's", "!", "wo", "nicht", ",", "so", "leid'", "ich", "mit", "Ge\u00b7duld", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VAINF", "$,", "ADV", "NE", "$.", "PWAV", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}