{"textgrid.poem.39496": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie endigt Heut? und was wird Morgen bringen?", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie endigt Heut? und was wird Morgen bringen?", "tokens": ["Wie", "en\u00b7digt", "Heut", "?", "und", "was", "wird", "Mor\u00b7gen", "brin\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "$.", "KON", "PWS", "VAFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer kann mir sagen, ob gestreute Saaten,", "tokens": ["Wer", "kann", "mir", "sa\u00b7gen", ",", "ob", "ge\u00b7streu\u00b7te", "Saa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Heilsam an sich, mir nicht zum Gift gerathen?", "tokens": ["Heil\u00b7sam", "an", "sich", ",", "mir", "nicht", "zum", "Gift", "ge\u00b7ra\u00b7then", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PRF", "$,", "PPER", "PTKNEG", "APPRART", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Was fremder Willk\u00fcr mag an mir gelingen?", "tokens": ["Was", "frem\u00b7der", "Will\u00b7k\u00fcr", "mag", "an", "mir", "ge\u00b7lin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Vergebens zeugt Erfahrung von den Dingen,", "tokens": ["Ver\u00b7ge\u00b7bens", "zeugt", "Er\u00b7fah\u00b7rung", "von", "den", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und zeichnet sorgsam auf der Vorwelt Thaten:", "tokens": ["Und", "zeich\u00b7net", "sorg\u00b7sam", "auf", "der", "Vor\u00b7welt", "Tha\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Selbst Weisheit wei\u00df untr\u00fcglich nicht zu rathen,", "tokens": ["Selbst", "Weis\u00b7heit", "wei\u00df", "un\u00b7tr\u00fcg\u00b7lich", "nicht", "zu", "ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo Kr\u00e4fte blindlings durch einander ringen.", "tokens": ["Wo", "Kr\u00e4f\u00b7te", "blind\u00b7lings", "durch", "ein\u00b7an\u00b7der", "rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Den ew'gen Schlangenkrei\u00df, der uns umfahet,", "tokens": ["Den", "ew'\u00b7gen", "Schlan\u00b7gen\u00b7krei\u00df", ",", "der", "uns", "um\u00b7fa\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "K\u00f6nnt' \u00fcberschauen nur des Schicksals W\u00e4chter;", "tokens": ["K\u00f6nnt'", "\u00fc\u00b7bersc\u00b7hau\u00b7en", "nur", "des", "Schick\u00b7sals", "W\u00e4ch\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "ADV", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Uns schwindet Eines, wenn das Andre nahet.", "tokens": ["Uns", "schwin\u00b7det", "Ei\u00b7nes", ",", "wenn", "das", "And\u00b7re", "na\u00b7het", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "KOUS", "PDS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Zukunft steht als Sphinx in d\u00fcstern Fernen,", "tokens": ["Die", "Zu\u00b7kunft", "steht", "als", "Sphinx", "in", "d\u00fcs\u00b7tern", "Fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOUS", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und schlingt hinab so Menschen wie Geschlechter,", "tokens": ["Und", "schlingt", "hin\u00b7ab", "so", "Men\u00b7schen", "wie", "Ge\u00b7schlech\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "NN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Eh' ihre R\u00e4thsel sie zu l\u00f6sen lernen.", "tokens": ["Eh'", "ih\u00b7re", "R\u00e4th\u00b7sel", "sie", "zu", "l\u00f6\u00b7sen", "ler\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}