{"textgrid.poem.65303": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Schlummernde", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein M\u00e4dchen war entschlummert", "tokens": ["Mein", "M\u00e4d\u00b7chen", "war", "ent\u00b7schlum\u00b7mert"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In einer Rosenlaube;", "tokens": ["In", "ei\u00b7ner", "Ro\u00b7sen\u00b7lau\u00b7be", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da sandt' ihr gleich Kupido", "tokens": ["Da", "sandt'", "ihr", "gleich", "Ku\u00b7pi\u00b7do"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein Heer von Liebesg\u00f6ttern.", "tokens": ["Ein", "Heer", "von", "Lie\u00b7bes\u00b7g\u00f6t\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der schlug die goldnen Fl\u00fcgel,", "tokens": ["Der", "schlug", "die", "gold\u00b7nen", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Wangen ihr zu k\u00fchlen,", "tokens": ["Die", "Wan\u00b7gen", "ihr", "zu", "k\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der band sich Myrtenstr\u00e4u\u00dfe,", "tokens": ["Der", "band", "sich", "Myr\u00b7ten\u00b7str\u00e4u\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die M\u00fccken wegzujagen,", "tokens": ["Die", "M\u00fc\u00b7cken", "weg\u00b7zu\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und Andre winkten drohend", "tokens": ["Und", "And\u00b7re", "wink\u00b7ten", "dro\u00b7hend"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Den V\u00f6geln in den L\u00fcften,", "tokens": ["Den", "V\u00f6\u00b7geln", "in", "den", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Die sie erwecken wollten", "tokens": ["Die", "sie", "er\u00b7we\u00b7cken", "woll\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Mit fr\u00f6hlichen Ges\u00e4ngen.", "tokens": ["Mit", "fr\u00f6h\u00b7li\u00b7chen", "Ge\u00b7s\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "O nektars\u00fc\u00dfer Schlummer,", "tokens": ["O", "nek\u00b7tar\u00b7s\u00fc\u00b7\u00dfer", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Wie hingest du voll Liebe,", "tokens": ["Wie", "hin\u00b7gest", "du", "voll", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "So wohlgef\u00e4llig l\u00e4chelnd,", "tokens": ["So", "wohl\u00b7ge\u00b7f\u00e4l\u00b7lig", "l\u00e4\u00b7chelnd", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "An ihren Augenwimpern!", "tokens": ["An", "ih\u00b7ren", "Au\u00b7gen\u00b7wim\u00b7pern", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Und Amoretten blickten", "tokens": ["Und", "A\u00b7mo\u00b7ret\u00b7ten", "blick\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Mit gro\u00dfen Flammenaugen", "tokens": ["Mit", "gro\u00b7\u00dfen", "Flam\u00b7men\u00b7au\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Aus ihren blonden Locken,", "tokens": ["Aus", "ih\u00b7ren", "blon\u00b7den", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Und lie\u00dfen Pfeil' auf Pfeile", "tokens": ["Und", "lie\u00b7\u00dfen", "Pfeil'", "auf", "Pfei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Wie spielend um sich fliegen.", "tokens": ["Wie", "spie\u00b7lend", "um", "sich", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Und doch, ihr kleinen Sch\u00fctzen,", "tokens": ["Und", "doch", ",", "ihr", "klei\u00b7nen", "Sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Auch spielend mit dem Bogen,", "tokens": ["Auch", "spie\u00b7lend", "mit", "dem", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Habt ihr mein Herz getroffen!", "tokens": ["Habt", "ihr", "mein", "Herz", "ge\u00b7trof\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}