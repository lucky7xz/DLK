{"textgrid.poem.40092": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Novemberfahrt", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, lacht nur, lacht! am Wege da", "tokens": ["Ja", ",", "lacht", "nur", ",", "lacht", "!", "am", "We\u00b7ge", "da"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "ADV", "$,", "VVFIN", "$.", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ihr pelzvermummten Gaffer!", "tokens": ["ihr", "pelz\u00b7ver\u00b7mumm\u00b7ten", "Gaf\u00b7fer", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Uns gab ein hei\u00dfres Blut, hahah,", "tokens": ["Uns", "gab", "ein", "hei\u00df\u00b7res", "Blut", ",", "ha\u00b7hah", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "der Wein- und Weiberschaffer!", "tokens": ["der", "Wein", "und", "Wei\u00b7ber\u00b7schaf\u00b7fer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und wenn wir etwas zittrig sind", "tokens": ["Und", "wenn", "wir", "et\u00b7was", "zitt\u00b7rig", "sind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und etwas rot die Nase,", "tokens": ["und", "et\u00b7was", "rot", "die", "Na\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "so meint nur nicht, das sei vom Wind:", "tokens": ["so", "meint", "nur", "nicht", ",", "das", "sei", "vom", "Wind", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKNEG", "$,", "PDS", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wir fahren in die Welt hinein,", "tokens": ["Wir", "fah\u00b7ren", "in", "die", "Welt", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wenn Uns es will behagen;", "tokens": ["wenn", "Uns", "es", "will", "be\u00b7ha\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "wir fahren in dem Sonnenschein,", "tokens": ["wir", "fah\u00b7ren", "in", "dem", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "den wir im Herzen tragen!", "tokens": ["den", "wir", "im", "Her\u00b7zen", "tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und wenn die olle Sonne sieht", "tokens": ["Und", "wenn", "die", "ol\u00b7le", "Son\u00b7ne", "sieht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "so junge Dreistewichte,", "tokens": ["so", "jun\u00b7ge", "Drei\u00b7ste\u00b7wich\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "dann wird sie gleich vor Angst verliebt", "tokens": ["dann", "wird", "sie", "gleich", "vor", "Angst", "ver\u00b7liebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "und macht ihr sch\u00f6nst Gesichte.", "tokens": ["und", "macht", "ihr", "sch\u00f6nst", "Ge\u00b7sich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hurrah, Novembersonnentag,", "tokens": ["Hur\u00b7rah", ",", "No\u00b7vem\u00b7ber\u00b7son\u00b7nen\u00b7tag", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "du Wunderwanderwetter:", "tokens": ["du", "Wun\u00b7der\u00b7wan\u00b7der\u00b7wet\u00b7ter", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "derweil am Herd das Zimperpack", "tokens": ["der\u00b7weil", "am", "Herd", "das", "Zim\u00b7per\u00b7pack"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "sich w\u00e4rmt den Katterletter", "tokens": ["sich", "w\u00e4rmt", "den", "Kat\u00b7ter\u00b7let\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hurrah, so stark dein herber Duft,", "tokens": ["Hur\u00b7rah", ",", "so", "stark", "dein", "her\u00b7ber", "Duft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "so w\u00fcrzig seine Schwere!", "tokens": ["so", "w\u00fcr\u00b7zig", "sei\u00b7ne", "Schwe\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Hurrrah \u2013 ich schl\u00fcrfe deine Luft,", "tokens": ["Hurr\u00b7rah", "\u2013", "ich", "schl\u00fcr\u00b7fe", "dei\u00b7ne", "Luft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "als ob es Rheinwein w\u00e4re!", "tokens": ["als", "ob", "es", "Rhein\u00b7wein", "w\u00e4\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "NE", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}