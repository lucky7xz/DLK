{"textgrid.poem.60383": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine goldene Hochzeit", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kennt ihr sie nicht, des Nordens alte Sage:", "tokens": ["Kennt", "ihr", "sie", "nicht", ",", "des", "Nor\u00b7dens", "al\u00b7te", "Sa\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von jenem Wunder an der Gr\u00f6nlandsk\u00fcste,", "tokens": ["Von", "je\u00b7nem", "Wun\u00b7der", "an", "der", "Gr\u00f6n\u00b7lands\u00b7k\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vom Lenz, den rings umstarrt die bleiche W\u00fcste,", "tokens": ["Vom", "Lenz", ",", "den", "rings", "um\u00b7starrt", "die", "blei\u00b7che", "W\u00fcs\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des eisgen Todes niegel\u00f6ste Klage?", "tokens": ["Des", "eis\u00b7gen", "To\u00b7des", "nie\u00b7ge\u00b7l\u00f6s\u00b7te", "Kla\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Durch eines ruhenden Vulkanes Spalten", "tokens": ["Durch", "ei\u00b7nes", "ru\u00b7hen\u00b7den", "Vul\u00b7ka\u00b7nes", "Spal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "War dort ein warmer Quell hervorgesprungen,", "tokens": ["War", "dort", "ein", "war\u00b7mer", "Quell", "her\u00b7vor\u00b7ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "War aus der Tief ein Lebenshauch gedrungen,", "tokens": ["War", "aus", "der", "Tief", "ein", "Le\u00b7bens\u00b7hauch", "ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die n\u00f6rdliche Oase zu erhalten.", "tokens": ["Die", "n\u00f6rd\u00b7li\u00b7che", "O\u00b7a\u00b7se", "zu", "er\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+++-+-+-", "measure": "unknown.measure.hexa"}}, "stanza.3": {"line.1": {"text": "Dort war ein Kloster, gr\u00fcne L\u00e4mmerweide,", "tokens": ["Dort", "war", "ein", "Klos\u00b7ter", ",", "gr\u00fc\u00b7ne", "L\u00e4m\u00b7mer\u00b7wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Garten prangte frisch mit Blumen, Fr\u00fcchten,", "tokens": ["Ein", "Gar\u00b7ten", "prang\u00b7te", "frisch", "mit", "Blu\u00b7men", ",", "Fr\u00fcch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und singend kamen V\u00f6gel hinzufl\u00fcchten,", "tokens": ["Und", "sin\u00b7gend", "ka\u00b7men", "V\u00f6\u00b7gel", "hin\u00b7zu\u00b7fl\u00fcch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In ein Asyl vor winterlichem Leide.", "tokens": ["In", "ein", "A\u00b7syl", "vor", "win\u00b7ter\u00b7li\u00b7chem", "Lei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Im Kloster wohnte friedlich die Gemeine;", "tokens": ["Im", "Klos\u00b7ter", "wohn\u00b7te", "fried\u00b7lich", "die", "Ge\u00b7mei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie f\u00fchrten ihre treue warme Quelle,", "tokens": ["Sie", "f\u00fchr\u00b7ten", "ih\u00b7re", "treu\u00b7e", "war\u00b7me", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die milde Freundin, traut durch jede Zelle,", "tokens": ["Die", "mil\u00b7de", "Freun\u00b7din", ",", "traut", "durch", "je\u00b7de", "Zel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch Wies und Feld und durch die gr\u00fcnen Haine.", "tokens": ["Durch", "Wies", "und", "Feld", "und", "durch", "die", "gr\u00fc\u00b7nen", "Hai\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "War Winter auch ringsum in alle Ferne,", "tokens": ["War", "Win\u00b7ter", "auch", "ring\u00b7sum", "in", "al\u00b7le", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Aus dieses Klosters frohen Paradiesen", "tokens": ["Aus", "die\u00b7ses", "Klos\u00b7ters", "fro\u00b7hen", "Pa\u00b7ra\u00b7die\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "War durch den Quell der rauhe Gast verwiesen;", "tokens": ["War", "durch", "den", "Quell", "der", "rau\u00b7he", "Gast", "ver\u00b7wie\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur heller strahlten dann bei Nacht die Sterne. \u2013", "tokens": ["Nur", "hel\u00b7ler", "strahl\u00b7ten", "dann", "bei", "Nacht", "die", "Ster\u00b7ne", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ADV", "APPR", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Zur Wehmut f\u00fchren gerne solche Kunden", "tokens": ["Zur", "Weh\u00b7mut", "f\u00fch\u00b7ren", "ger\u00b7ne", "sol\u00b7che", "Kun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf des entflohnen Gl\u00fcckes dunklen F\u00e4hrten;", "tokens": ["Auf", "des", "ent\u00b7floh\u00b7nen", "Gl\u00fc\u00b7ckes", "dunk\u00b7len", "F\u00e4hr\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Begrub das Eis nicht l\u00e4ngst die sch\u00f6nen G\u00e4rten?", "tokens": ["Be\u00b7grub", "das", "Eis", "nicht", "l\u00e4ngst", "die", "sch\u00f6\u00b7nen", "G\u00e4r\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sind Quell und Kloster nicht schon l\u00e4ngst verschwunden?", "tokens": ["Sind", "Quell", "und", "Klos\u00b7ter", "nicht", "schon", "l\u00e4ngst", "ver\u00b7schwun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "PTKNEG", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Sie sind es nicht! kein Winter wird sie morden;", "tokens": ["Sie", "sind", "es", "nicht", "!", "kein", "Win\u00b7ter", "wird", "sie", "mor\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "$.", "PIAT", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ob \u00e4u\u00dfres Leben auch im Frost zerstiebe,", "tokens": ["Ob", "\u00e4u\u00df\u00b7res", "Le\u00b7ben", "auch", "im", "Frost", "zer\u00b7stie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Innern die Oase sch\u00fctzt die Liebe,", "tokens": ["Im", "In\u00b7nern", "die", "O\u00b7a\u00b7se", "sch\u00fctzt", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die warme Quelle in des Alters Norden.", "tokens": ["Die", "war\u00b7me", "Quel\u00b7le", "in", "des", "Al\u00b7ters", "Nor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Das Kloster ist das B\u00fcndnis guter Herzen,", "tokens": ["Das", "Klos\u00b7ter", "ist", "das", "B\u00fcnd\u00b7nis", "gu\u00b7ter", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dies mag getrost die strenge Zeit erwarten,", "tokens": ["Dies", "mag", "ge\u00b7trost", "die", "stren\u00b7ge", "Zeit", "er\u00b7war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADJD", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Umrankt von einem immergr\u00fcnen Garten,", "tokens": ["Um\u00b7rankt", "von", "ei\u00b7nem", "im\u00b7mer\u00b7gr\u00fc\u00b7nen", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo Blumen bl\u00fchn und Fr\u00fchlingslieder scherzen. \u2013", "tokens": ["Wo", "Blu\u00b7men", "bl\u00fchn", "und", "Fr\u00fch\u00b7lings\u00b7lie\u00b7der", "scher\u00b7zen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "NN", "VVINF", "KON", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}