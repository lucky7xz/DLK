{"textgrid.poem.66330": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Die Wucherer", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im gro\u00dfen Dorfe ", "tokens": ["Im", "gro\u00b7\u00dfen", "Dor\u00b7fe"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Geht's um.", "tokens": ["Geht's", "um", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Sobald der W\u00e4chter Zw\u00f6lfe ruft,", "tokens": ["So\u00b7bald", "der", "W\u00e4ch\u00b7ter", "Zw\u00f6l\u00b7fe", "ruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Rumort's daher, saust in der Luft,", "tokens": ["Ru\u00b7mort's", "da\u00b7her", ",", "saust", "in", "der", "Luft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PAV", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und rast im Dorf herum.", "tokens": ["Und", "rast", "im", "Dorf", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zw\u00f6lf Geister heulen f\u00fcrchterlich:", "tokens": ["Zw\u00f6lf", "Geis\u00b7ter", "heu\u00b7len", "f\u00fcrch\u00b7ter\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbo weh!", "tokens": ["\u00bb", "o", "weh", "!"], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "FM", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Der Fluch der S\u00fcnde macht uns bang,", "tokens": ["Der", "Fluch", "der", "S\u00fcn\u00b7de", "macht", "uns", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verworfen hat uns \u2013 ach wie lang!", "tokens": ["Ver\u00b7wor\u00b7fen", "hat", "uns", "\u2013", "ach", "wie", "lang", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "$(", "XY", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der R\u00e4cher in der H\u00f6h'.\u00ab", "tokens": ["Der", "R\u00e4\u00b7cher", "in", "der", "H\u00f6h'", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da schlingt das Weib sich um den Mann", "tokens": ["Da", "schlingt", "das", "Weib", "sich", "um", "den", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herum.", "tokens": ["He\u00b7rum", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Die Kindlein schl\u00fcpfen unter's Bett,", "tokens": ["Die", "Kin\u00b7dlein", "schl\u00fcp\u00b7fen", "un\u00b7ter's", "Bett", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und alles ist zu Haberst\u00e4tt", "tokens": ["Und", "al\u00b7les", "ist", "zu", "Ha\u00b7ber\u00b7st\u00e4tt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vor Todes\u00e4ngsten stumm.", "tokens": ["Vor", "To\u00b7de\u00b7s\u00e4ngs\u00b7ten", "stumm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie betet da das ganze Dorf", "tokens": ["Wie", "be\u00b7tet", "da", "das", "gan\u00b7ze", "Dorf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So hei\u00df:", "tokens": ["So", "hei\u00df", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Wir arme Bauern bitten dich,", "tokens": ["Wir", "ar\u00b7me", "Bau\u00b7ern", "bit\u00b7ten", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gott, treibe von uns gn\u00e4diglich", "tokens": ["Gott", ",", "trei\u00b7be", "von", "uns", "gn\u00e4\u00b7di\u00b7glich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "APPR", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dies h\u00f6llische Geschmei\u00df!", "tokens": ["Dies", "h\u00f6l\u00b7li\u00b7sche", "Ge\u00b7schmei\u00df", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Pfarrer, der im Swedenborg", "tokens": ["Der", "Pfar\u00b7rer", ",", "der", "im", "Swe\u00b7den\u00b7borg"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Studirt,", "tokens": ["Stu\u00b7dirt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und als ein tiefgelehrter Mann", "tokens": ["Und", "als", "ein", "tief\u00b7ge\u00b7lehr\u00b7ter", "Mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit allen Geistern sprechen kann,", "tokens": ["Mit", "al\u00b7len", "Geis\u00b7tern", "spre\u00b7chen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wagt es, und exorzirt.", "tokens": ["Wagt", "es", ",", "und", "ex\u00b7or\u00b7zirt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Vom Grabe eines Frommen sprach", "tokens": ["Vom", "Gra\u00b7be", "ei\u00b7nes", "From\u00b7men", "sprach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Mann:", "tokens": ["Der", "Mann", ":"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "\u00bbihr Geister aus dem Schattenreich,", "tokens": ["\u00bb", "ihr", "Geis\u00b7ter", "aus", "dem", "Schat\u00b7ten\u00b7reich", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Namen Gottes frag' ich euch:", "tokens": ["Im", "Na\u00b7men", "Got\u00b7tes", "frag'", "ich", "euch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sagt, was habt ihr gethan?\u00ab", "tokens": ["Sagt", ",", "was", "habt", "ihr", "ge\u00b7than", "?", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "PWS", "VAFIN", "PPER", "VVPP", "$.", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Da kam ein Geist, wie S\u00e4ulenrauch", "tokens": ["Da", "kam", "ein", "Geist", ",", "wie", "S\u00e4u\u00b7len\u00b7rauch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PWAV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Torf.", "tokens": ["Von", "Torf", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NE", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Dem Pfarrer bebt das Herz wie Sulz.", "tokens": ["Dem", "Pfar\u00b7rer", "bebt", "das", "Herz", "wie", "Sulz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "KOKOM", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hohl sprach der Geist: \u00bbIch war der ", "tokens": ["Hohl", "sprach", "der", "Geist", ":", "\u00bb", "Ich", "war", "der"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "$.", "$(", "PPER", "VAFIN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Einmal in diesem Dorf.", "tokens": ["Ein\u00b7mal", "in", "die\u00b7sem", "Dorf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "Dies war ein ", "tokens": ["Dies", "war", "ein"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VAFIN", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Und der", "tokens": ["Und", "der"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Sind ", "tokens": ["Sind"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Sind wir zw\u00f6lf Wucherer.", "tokens": ["Sind", "wir", "zw\u00f6lf", "Wu\u00b7che\u00b7rer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "CARD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Auf unsern B\u00f6den lag die Frucht", "tokens": ["Auf", "un\u00b7sern", "B\u00f6\u00b7den", "lag", "die", "Frucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Sand.", "tokens": ["Wie", "Sand", "."], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Oft gab der Himmel Fruchtbarkeit;", "tokens": ["Oft", "gab", "der", "Him\u00b7mel", "Frucht\u00b7bar\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch wir erschufen theure Zeit", "tokens": ["Doch", "wir", "er\u00b7schu\u00b7fen", "theu\u00b7re", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gar weit umher im Land.", "tokens": ["Gar", "weit", "um\u00b7her", "im", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Denn Korn und Wein verschlossen wir", "tokens": ["Denn", "Korn", "und", "Wein", "ver\u00b7schlos\u00b7sen", "wir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Flei\u00df.", "tokens": ["Mit", "Flei\u00df", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Und brach herein die Hungersnoth", "tokens": ["Und", "brach", "her\u00b7ein", "die", "Hun\u00b7gers\u00b7noth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verkauften wir erst Wein und Brod", "tokens": ["Ver\u00b7kauf\u00b7ten", "wir", "erst", "Wein", "und", "Brod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Um teuflisch hohen Preis.", "tokens": ["Um", "teuf\u00b7lisch", "ho\u00b7hen", "Preis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Wir haben uns mit Armenblut", "tokens": ["Wir", "ha\u00b7ben", "uns", "mit", "Ar\u00b7men\u00b7blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gen\u00e4hrt.", "tokens": ["Ge\u00b7n\u00e4hrt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Wir haben der Bedr\u00e4ngten Schrei,", "tokens": ["Wir", "ha\u00b7ben", "der", "Be\u00b7dr\u00e4ng\u00b7ten", "Schrei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geblendet von der T\u00e4uscherei", "tokens": ["Ge\u00b7blen\u00b7det", "von", "der", "T\u00e4u\u00b7sche\u00b7rei"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Wuchers, nicht geh\u00f6rt.", "tokens": ["Des", "Wu\u00b7chers", ",", "nicht", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Wir starben, Geister peitschten uns", "tokens": ["Wir", "star\u00b7ben", ",", "Geis\u00b7ter", "peitschten", "uns"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hinab.", "tokens": ["Hin\u00b7ab", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Dreihundert Jahre sind es bald,", "tokens": ["Drei\u00b7hun\u00b7dert", "Jah\u00b7re", "sind", "es", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df solchen Greuelaufenthalt", "tokens": ["Da\u00df", "sol\u00b7chen", "Greu\u00b7el\u00b7au\u00b7fent\u00b7halt"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Uns Gottes Rache gab.", "tokens": ["Uns", "Got\u00b7tes", "Ra\u00b7che", "gab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Doch wird vom Fluch einst unser Geist", "tokens": ["Doch", "wird", "vom", "Fluch", "einst", "un\u00b7ser", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPRART", "NN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Befreit,", "tokens": ["Be\u00b7freit", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Wenn's hier im Dorf zw\u00f6lf Bauern gibt,", "tokens": ["Wenn's", "hier", "im", "Dorf", "zw\u00f6lf", "Bau\u00b7ern", "gibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPRART", "NN", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo jeder Treu' und Glauben liebt,", "tokens": ["Wo", "je\u00b7der", "Treu'", "und", "Glau\u00b7ben", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und schwarzen Wucher scheut.", "tokens": ["Und", "schwar\u00b7zen", "Wu\u00b7cher", "scheut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "O weh, es schaurt der Morgen schon;", "tokens": ["O", "weh", ",", "es", "schaurt", "der", "Mor\u00b7gen", "schon", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPER", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fort, fort!", "tokens": ["Fort", ",", "fort", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "O weh, noch werden wir nicht los.", "tokens": ["O", "weh", ",", "noch", "wer\u00b7den", "wir", "nicht", "los", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Jahres Segen ist zu gro\u00df,", "tokens": ["Des", "Jah\u00b7res", "Se\u00b7gen", "ist", "zu", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hinab an unsern Ort!\u00ab", "tokens": ["Hin\u00b7ab", "an", "un\u00b7sern", "Ort", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Husch, rasselt's fort. Der Pfarrer fiel", "tokens": ["Husch", ",", "ras\u00b7selt's", "fort", ".", "Der", "Pfar\u00b7rer", "fiel"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PTKVZ", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aufs Knie,", "tokens": ["Aufs", "Knie", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Und bat: Verwirf uns nicht im Grimm,", "tokens": ["Und", "bat", ":", "Ver\u00b7wirf", "uns", "nicht", "im", "Grimm", ","], "token_info": ["word", "word", "punct", "word", "word", "word",