{"dta.poem.12521": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XxXI.  \n  Uhland.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Uhland schweigt in der thatlosen Zeit. Es entsagen", "tokens": ["Uh\u00b7land", "schweigt", "in", "der", "that\u00b7lo\u00b7sen", "Zeit", ".", "Es", "ent\u00b7sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "die Besten", "tokens": ["die", "Bes\u00b7ten"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Um das verlorne Geschlecht einer verlorenen", "tokens": ["Um", "das", "ver\u00b7lor\u00b7ne", "Ge\u00b7schlecht", "ei\u00b7ner", "ver\u00b7lo\u00b7re\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "ART", "ADJA"], "meter": "-+-+--++--+--", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "M\u00fch'.", "tokens": ["M\u00fch'", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "M\u00e4nner erzog er sich nicht zu dem Hochwuchs seiner", "tokens": ["M\u00e4n\u00b7ner", "er\u00b7zog", "er", "sich", "nicht", "zu", "dem", "Hoch\u00b7wuchs", "sei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PRF", "PTKNEG", "APPR", "ART", "NN", "PPOSAT"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.6": {"text": "Gedanken,", "tokens": ["Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Und f\u00fcr die m\u00fc\u00dfige Welt sang er Romanzen", "tokens": ["Und", "f\u00fcr", "die", "m\u00fc\u00b7\u00dfi\u00b7ge", "Welt", "sang", "er", "Ro\u00b7man\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "NN"], "meter": "-+-+--++-++-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "genug.", "tokens": ["ge\u00b7nug", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+", "measure": "iambic.single"}}}}}