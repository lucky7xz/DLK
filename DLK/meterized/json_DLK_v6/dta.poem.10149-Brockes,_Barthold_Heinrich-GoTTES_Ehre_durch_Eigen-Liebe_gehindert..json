{"dta.poem.10149": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "GoTTES Ehre durch Eigen-Liebe  \n gehindert.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du stellest, lieber A., dir", "tokens": ["Du", "stel\u00b7lest", ",", "lie\u00b7ber", "A.", ",", "dir"], "token_info": ["word", "word", "punct", "word", "abbreviation", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "ADV", "APPRART", "$,", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Gottheit nicht viel anders f\u00fcr,", "tokens": ["Die", "Got\u00b7theit", "nicht", "viel", "an\u00b7ders", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "ADV", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als h\u00e4tte Sie allein, und recht absonderlich", "tokens": ["Als", "h\u00e4t\u00b7te", "Sie", "al\u00b7lein", ",", "und", "recht", "ab\u00b7son\u00b7der\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "$,", "KON", "ADJD", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die deinen, deinen Stand, und dich,", "tokens": ["Die", "dei\u00b7nen", ",", "dei\u00b7nen", "Stand", ",", "und", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "$,", "PPOSAT", "NN", "$,", "KON", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zur Absicht ihrer Macht und ihrer Lieb\u2019 erwehlet.", "tokens": ["Zur", "Ab\u00b7sicht", "ih\u00b7rer", "Macht", "und", "ih\u00b7rer", "Lieb'", "er\u00b7weh\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn ob es deinem Geist zwar nicht an Kr\u00e4fften fehlet,", "tokens": ["Denn", "ob", "es", "dei\u00b7nem", "Geist", "zwar", "nicht", "an", "Kr\u00e4ff\u00b7ten", "feh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "ADV", "PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Sch\u00f6pfer w\u00fcrdiger zu dencken;", "tokens": ["Vom", "Sch\u00f6p\u00b7fer", "w\u00fcr\u00b7di\u00b7ger", "zu", "den\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So brauchest du sie doch, aus Eigen-Liebe, nicht.", "tokens": ["So", "brau\u00b7chest", "du", "sie", "doch", ",", "aus", "Ei\u00b7gen\u00b7Lie\u00b7be", ",", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "$,", "APPR", "NN", "$,", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es wird dir gar zu schwer, dein sorgendes Gesicht", "tokens": ["Es", "wird", "dir", "gar", "zu", "schwer", ",", "dein", "sor\u00b7gen\u00b7des", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auf etwas ausser dir, und von dir abzulencken.", "tokens": ["Auf", "et\u00b7was", "aus\u00b7ser", "dir", ",", "und", "von", "dir", "ab\u00b7zu\u00b7len\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPR", "PPER", "$,", "KON", "APPR", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es hindert dich dein gar zu liebes Ich,", "tokens": ["Es", "hin\u00b7dert", "dich", "dein", "gar", "zu", "lie\u00b7bes", "Ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "ADV", "APPR", "ADJA", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In Ehr-Furcht GOTTES Werck und Gr\u00f6sse zu er-", "tokens": ["In", "Ehr\u00b7Furcht", "GoT\u00b7TES", "Werck", "und", "Gr\u00f6s\u00b7se", "zu", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NE", "NN", "KON", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df Millionen Welte sich", "tokens": ["Da\u00df", "Mil\u00b7lion\u00b7en", "Wel\u00b7te", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVFIN", "PRF"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Um Millionen Sonnen regen;", "tokens": ["Um", "Mil\u00b7lion\u00b7en", "Son\u00b7nen", "re\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "NN", "ADJA", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.5": {"text": "Die\u00df alles w\u00fcrdigst du, auf dich allein erpicht,", "tokens": ["Die\u00df", "al\u00b7les", "w\u00fcr\u00b7digst", "du", ",", "auf", "dich", "al\u00b7lein", "er\u00b7picht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "PPER", "$,", "APPR", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des ausehns und bewunderns nicht.", "tokens": ["Des", "au\u00b7sehns", "und", "be\u00b7wun\u00b7derns", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJA", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}