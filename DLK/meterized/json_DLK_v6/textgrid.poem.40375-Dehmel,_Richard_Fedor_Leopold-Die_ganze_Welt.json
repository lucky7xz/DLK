{"textgrid.poem.40375": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Die ganze Welt", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo h\u00e4ngt der gr\u00f6\u00dfte Bilderbogen?", "tokens": ["Wo", "h\u00e4ngt", "der", "gr\u00f6\u00df\u00b7te", "Bil\u00b7der\u00b7bo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Beim Kaufmann, Kinder! ungelogen!", "tokens": ["Beim", "Kauf\u00b7mann", ",", "Kin\u00b7der", "!", "un\u00b7ge\u00b7lo\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "NN", "$.", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Man braucht blos drau\u00dfen stehn zu bleiben,", "tokens": ["Man", "braucht", "blos", "drau\u00b7\u00dfen", "stehn", "zu", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "kuckt einfach durch die Ladenscheiben,", "tokens": ["kuckt", "ein\u00b7fach", "durch", "die", "La\u00b7den\u00b7schei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "da sieht man ohne alles Geld", "tokens": ["da", "sieht", "man", "oh\u00b7ne", "al\u00b7les", "Geld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "die ganze Welt.", "tokens": ["die", "gan\u00b7ze", "Welt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Man sieht die braunen Kaffeebohnen;", "tokens": ["Man", "sieht", "die", "brau\u00b7nen", "Kaf\u00b7fee\u00b7boh\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die wachsen, wo die Affen wohnen.", "tokens": ["die", "wach\u00b7sen", ",", "wo", "die", "Af\u00b7fen", "woh\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "$,", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Man sieht auf Waschblau, Reis und Mandeln", "tokens": ["Man", "sieht", "auf", "Waschblau", ",", "Reis", "und", "Man\u00b7deln"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Kameele unter Palmen wandeln,", "tokens": ["Ka\u00b7mee\u00b7le", "un\u00b7ter", "Pal\u00b7men", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "und einen Ochsen ganz bepackt", "tokens": ["und", "ei\u00b7nen", "Och\u00b7sen", "ganz", "be\u00b7packt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "mit Fleischextrakt.", "tokens": ["mit", "Flei\u00b7schex\u00b7trakt", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Man sieht auch Zimmt und Apfelsinen,", "tokens": ["Man", "sieht", "auch", "Zimmt", "und", "Ap\u00b7fel\u00b7si\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und Zuckerh\u00fcte zwischen ihnen.", "tokens": ["und", "Zu\u00b7cker\u00b7h\u00fc\u00b7te", "zwi\u00b7schen", "ih\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Man sieht auf rot lackierten Blechen", "tokens": ["Man", "sieht", "auf", "rot", "la\u00b7ckier\u00b7ten", "Ble\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Matrosen mit Chinesen sprechen;", "tokens": ["Mat\u00b7ro\u00b7sen", "mit", "Chi\u00b7ne\u00b7sen", "spre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "und manchmal steht ein bunter Mohr,", "tokens": ["und", "manch\u00b7mal", "steht", "ein", "bun\u00b7ter", "Mohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "der lacht, davor.", "tokens": ["der", "lacht", ",", "da\u00b7vor", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "VVFIN", "$,", "PAV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Am Eingang aber lehnt 'ne Leiter", "tokens": ["Am", "Ein\u00b7gang", "a\u00b7ber", "lehnt", "'ne", "Lei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "mit Hasen, H\u00fchnern und so weiter.", "tokens": ["mit", "Ha\u00b7sen", ",", "H\u00fch\u00b7nern", "und", "so", "wei\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "ADV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und manchmal h\u00e4ngt an ihren Sprossen", "tokens": ["Und", "manch\u00b7mal", "h\u00e4ngt", "an", "ih\u00b7ren", "Spros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "ein gro\u00dfer Hirsch, ganz totgeschossen;", "tokens": ["ein", "gro\u00b7\u00dfer", "Hirsch", ",", "ganz", "tot\u00b7ge\u00b7schos\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "dann kommt so'n kleiner Hundemann", "tokens": ["dann", "kommt", "so'n", "klei\u00b7ner", "Hun\u00b7de\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und schnuppert dran.", "tokens": ["und", "schnup\u00b7pert", "dran", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}