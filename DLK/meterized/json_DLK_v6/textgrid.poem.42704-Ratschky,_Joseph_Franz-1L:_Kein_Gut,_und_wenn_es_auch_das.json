{"textgrid.poem.42704": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kein Gut, und wenn es auch das ", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Gut, und wenn es auch das ", "tokens": ["Kein", "Gut", ",", "und", "wenn", "es", "auch", "das"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "KON", "KOUS", "PPER", "ADV", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wird nach Verdienst geehrt, so lang man es geniesst:", "tokens": ["Wird", "nach", "Ver\u00b7dienst", "ge\u00b7ehrt", ",", "so", "lang", "man", "es", "ge\u00b7niesst", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "VVPP", "$,", "ADV", "ADJD", "PIS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erst, wenn es uns den R\u00fccken kehrte,", "tokens": ["Erst", ",", "wenn", "es", "uns", "den", "R\u00fc\u00b7cken", "kehr\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erst dann sch\u00e4tzt man's nach seinem wahren Werthe.", "tokens": ["Erst", "dann", "sch\u00e4tzt", "man's", "nach", "sei\u00b7nem", "wah\u00b7ren", "Wert\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vergieb mir (falls dein Ohr, o Theuerster! den Witz,", "tokens": ["Ver\u00b7gieb", "mir", "(", "falls", "dein", "Ohr", ",", "o", "Theu\u00b7ers\u00b7ter", "!", "den", "Witz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$(", "KOUS", "PPOSAT", "NN", "$,", "FM", "NN", "$.", "ART", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der, Flammen gleich, aus deinem Munde lodert,", "tokens": ["Der", ",", "Flam\u00b7men", "gleich", ",", "aus", "dei\u00b7nem", "Mun\u00b7de", "lo\u00b7dert", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "NN", "ADV", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auch von den Lippen andrer fodert)", "tokens": ["Auch", "von", "den", "Lip\u00b7pen", "an\u00b7drer", "fo\u00b7dert", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn meines Briefes Frontispitz", "tokens": ["Wenn", "mei\u00b7nes", "Brie\u00b7fes", "Fron\u00b7tis\u00b7pitz"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vermuthlich dich durch diesen l\u00e4ngstbekannten", "tokens": ["Ver\u00b7muth\u00b7lich", "dich", "durch", "die\u00b7sen", "l\u00e4ngst\u00b7be\u00b7kann\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "PRF", "APPR", "PDAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gemeinsatz g\u00e4hnen macht, der in den Folianten", "tokens": ["Ge\u00b7mein\u00b7satz", "g\u00e4h\u00b7nen", "macht", ",", "der", "in", "den", "Fo\u00b7li\u00b7an\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der steifen Moralistenschaar", "tokens": ["Der", "stei\u00b7fen", "Mo\u00b7ra\u00b7lis\u00b7ten\u00b7schaar"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Schon hundertmal der Motten Speise war!", "tokens": ["Schon", "hun\u00b7dert\u00b7mal", "der", "Mot\u00b7ten", "Spei\u00b7se", "war", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Alt ist das Spr\u00fcchlein zwar, und t\u00fcchtig", "tokens": ["Alt", "ist", "das", "Spr\u00fcch\u00b7lein", "zwar", ",", "und", "t\u00fcch\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ADV", "$,", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Genug durchdroschen, Freund! doch seit mir das Geschick", "tokens": ["Ge\u00b7nug", "durch\u00b7dro\u00b7schen", ",", "Freund", "!", "doch", "seit", "mir", "das", "Ge\u00b7schick"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "$,", "NN", "$.", "ADV", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "J\u00fcngst, wie ich hoffen will, nur deines Umgangs Gl\u00fcck,", "tokens": ["J\u00fcngst", ",", "wie", "ich", "hof\u00b7fen", "will", ",", "nur", "dei\u00b7nes", "Um\u00b7gangs", "Gl\u00fcck", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "VVINF", "VMFIN", "$,", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nicht auch dein Herz entzog, ward es mir neu und wichtig.", "tokens": ["Nicht", "auch", "dein", "Herz", "ent\u00b7zog", ",", "ward", "es", "mir", "neu", "und", "wich\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPOSAT", "NN", "VVFIN", "$,", "VAFIN", "PPER", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich f\u00fchle nun, geliebter Pylades!", "tokens": ["Ich", "f\u00fch\u00b7le", "nun", ",", "ge\u00b7lieb\u00b7ter", "Py\u00b7la\u00b7des", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit banger Sehnsucht f\u00fchl' ich es,", "tokens": ["Mit", "ban\u00b7ger", "Sehn\u00b7sucht", "f\u00fchl'", "ich", "es", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was du mir warst, und bist, wie sehr ich dich vermisse.", "tokens": ["Was", "du", "mir", "warst", ",", "und", "bist", ",", "wie", "sehr", "ich", "dich", "ver\u00b7mis\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VAFIN", "$,", "KON", "VAFIN", "$,", "PWAV", "ADV", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Oft seit dem letzten unsrer K\u00fcsse", "tokens": ["Oft", "seit", "dem", "letz\u00b7ten", "uns\u00b7rer", "K\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gedenk' ich, wenn mein Blick beym Glanz des Hesperus", "tokens": ["Ge\u00b7denk'", "ich", ",", "wenn", "mein", "Blick", "beym", "Glanz", "des", "Hes\u00b7pe\u00b7rus"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "APPRART", "NN", "ART", "NE"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.6": {"text": "Mit stillem Neid zu euch den stolzen Isterfluss", "tokens": ["Mit", "stil\u00b7lem", "Neid", "zu", "euch", "den", "stol\u00b7zen", "Is\u00b7ter\u00b7fluss"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Hinunter eilen sieht, der frohen Abendstunden,", "tokens": ["Hin\u00b7un\u00b7ter", "ei\u00b7len", "sieht", ",", "der", "fro\u00b7hen", "A\u00b7bends\u00b7tun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die beym sokratischen Pokal", "tokens": ["Die", "beym", "sok\u00b7ra\u00b7ti\u00b7schen", "Po\u00b7kal"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Halb ernst, halb l\u00e4chelnd uns entschwunden.", "tokens": ["Halb", "ernst", ",", "halb", "l\u00e4\u00b7chelnd", "uns", "ent\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "ADJD", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Bald wurde feyerlich vor unserm Tribunal", "tokens": ["Bald", "wur\u00b7de", "fey\u00b7er\u00b7lich", "vor", "un\u00b7serm", "Tri\u00b7bu\u00b7nal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das Schicksal eines Reims entschieden:", "tokens": ["Das", "Schick\u00b7sal", "ei\u00b7nes", "Reims", "ent\u00b7schie\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Bald r\u00e4chten lachend wir an dummen Verseschmieden", "tokens": ["Bald", "r\u00e4ch\u00b7ten", "la\u00b7chend", "wir", "an", "dum\u00b7men", "Ver\u00b7se\u00b7schmie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Des Musengotts beschimpfte Majest\u00e4t:", "tokens": ["Des", "Mu\u00b7sen\u00b7gotts", "be\u00b7schimpf\u00b7te", "Ma\u00b7jes\u00b7t\u00e4t", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Bald gab ein plumper Musaget", "tokens": ["Bald", "gab", "ein", "plum\u00b7per", "Mu\u00b7sa\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Und unsrer kritischen Tagschreiberzunft verstecktes", "tokens": ["Und", "uns\u00b7rer", "kri\u00b7ti\u00b7schen", "Tag\u00b7schrei\u00b7ber\u00b7zunft", "ver\u00b7steck\u00b7tes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ADJA"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Gef\u00fchl zum Spott uns Stoff, bis endlich unverhofft", "tokens": ["Ge\u00b7f\u00fchl", "zum", "Spott", "uns", "Stoff", ",", "bis", "end\u00b7lich", "un\u00b7ver\u00b7hofft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "PPER", "NN", "$,", "KON", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die schwarze Mitternacht zu Bett uns rief. O noctes", "tokens": ["Die", "schwar\u00b7ze", "Mit\u00b7ter\u00b7nacht", "zu", "Bett", "uns", "rief", ".", "O", "noc\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "PPER", "VVFIN", "$.", "NE", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Caenaeque Deum! ruf' ich oft", "tokens": ["Cae\u00b7nae\u00b7que", "Deum", "!", "ruf'", "ich", "oft"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["FM.la", "FM.la", "$.", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Mit unserem Horaz inbr\u00fcnstig auf, und eile", "tokens": ["Mit", "un\u00b7se\u00b7rem", "Ho\u00b7raz", "in\u00b7br\u00fcns\u00b7tig", "auf", ",", "und", "ei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.20": {"text": "Nach meiner Stube hin, wo ich die lange Weile,", "tokens": ["Nach", "mei\u00b7ner", "Stu\u00b7be", "hin", ",", "wo", "ich", "die", "lan\u00b7ge", "Wei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,", "PWAV", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die manchmal unversehns mich armen Robinson", "tokens": ["Die", "manch\u00b7mal", "un\u00b7ver\u00b7sehns", "mich", "ar\u00b7men", "Ro\u00b7bin\u00b7son"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Auf meiner kleinen w\u00fcsten Insel", "tokens": ["Auf", "mei\u00b7ner", "klei\u00b7nen", "w\u00fcs\u00b7ten", "In\u00b7sel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "VVFIN", "NN"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.23": {"text": "Zu unterjochen sucht, durch Maro's Heldenton,", "tokens": ["Zu", "un\u00b7ter\u00b7jo\u00b7chen", "sucht", ",", "durch", "Ma\u00b7ro's", "Hel\u00b7den\u00b7ton", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "$,", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Horazens muntern Witz und Naso's Klaggewinsel,", "tokens": ["Ho\u00b7ra\u00b7zens", "mun\u00b7tern", "Witz", "und", "Na\u00b7so's", "Klag\u00b7ge\u00b7win\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KON", "NE", "NN", "$,"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.25": {"text": "Durch Swifts verw\u00e4gnen Muth und Popens Energie,", "tokens": ["Durch", "Swifts", "ver\u00b7w\u00e4g\u00b7nen", "Muth", "und", "Po\u00b7pens", "E\u00b7ner\u00b7gie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "KON", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Durch eine schlaue Blasphemie", "tokens": ["Durch", "ei\u00b7ne", "schlau\u00b7e", "Blas\u00b7phe\u00b7mie"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Des leidigen Volt\u00e4rs, durch Wielands zauberreiche", "tokens": ["Des", "lei\u00b7di\u00b7gen", "Vol\u00b7t\u00e4rs", ",", "durch", "Wie\u00b7lands", "zau\u00b7ber\u00b7rei\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NN", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Urbanit\u00e4t von Zeit zu Zeit verscheuche.", "tokens": ["Ur\u00b7ba\u00b7ni\u00b7t\u00e4t", "von", "Zeit", "zu", "Zeit", "ver\u00b7scheu\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "O Freund, wie \u00f6d' und leer scheint mir mein Aufenthalt,", "tokens": ["O", "Freund", ",", "wie", "\u00f6d'", "und", "leer", "scheint", "mir", "mein", "Auf\u00b7ent\u00b7halt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "ADJD", "KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo keiner Muse Lied erschallet, wo man, kalt", "tokens": ["Wo", "kei\u00b7ner", "Mu\u00b7se", "Lied", "er\u00b7schal\u00b7let", ",", "wo", "man", ",", "kalt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWAV", "PIAT", "NN", "NN", "VVFIN", "$,", "PWAV", "PIS", "$,", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "F\u00fcr Wollust feinrer Art, f\u00fcr geistiges Vergn\u00fcgen,", "tokens": ["F\u00fcr", "Wol\u00b7lust", "fein\u00b7rer", "Art", ",", "f\u00fcr", "geis\u0