{"textgrid.poem.60412": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Erinnerung", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst gingen wir auf einer Bergeswiese;", "tokens": ["Einst", "gin\u00b7gen", "wir", "auf", "ei\u00b7ner", "Ber\u00b7ges\u00b7wie\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Tief atmend tranken wir die Blumenseelen,", "tokens": ["Tief", "at\u00b7mend", "tran\u00b7ken", "wir", "die", "Blu\u00b7men\u00b7see\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das B\u00e4chlein kam herab, uns zu erz\u00e4hlen", "tokens": ["Das", "B\u00e4ch\u00b7lein", "kam", "her\u00b7ab", ",", "uns", "zu", "er\u00b7z\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den unverge\u00dfnen Traum vom Paradiese.", "tokens": ["Den", "un\u00b7ver\u00b7ge\u00df\u00b7nen", "Traum", "vom", "Pa\u00b7ra\u00b7die\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wir sahn das Abendrot die Gipfel f\u00e4rben,", "tokens": ["Wir", "sahn", "das", "A\u00b7ben\u00b7drot", "die", "Gip\u00b7fel", "f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es war ein Spiel vom sch\u00f6nsten Alpenlichte,", "tokens": ["Es", "war", "ein", "Spiel", "vom", "sch\u00f6ns\u00b7ten", "Al\u00b7pen\u00b7lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch wandt ich mich nach deinem Angesichte,", "tokens": ["Doch", "wandt", "ich", "mich", "nach", "dei\u00b7nem", "An\u00b7ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das strahlte mir wie Liebe ohne Sterben.", "tokens": ["Das", "strahl\u00b7te", "mir", "wie", "Lie\u00b7be", "oh\u00b7ne", "Ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "KOKOM", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Bald war den Bergen ihre Glut entschwunden", "tokens": ["Bald", "war", "den", "Ber\u00b7gen", "ih\u00b7re", "Glut", "ent\u00b7schwun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wird vielleicht so sch\u00f6n nie wieder kommen;", "tokens": ["Und", "wird", "viel\u00b7leicht", "so", "sch\u00f6n", "nie", "wie\u00b7der", "kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ADJD", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auch deinem Antlitz war der Strahl genommen,", "tokens": ["Auch", "dei\u00b7nem", "Ant\u00b7litz", "war", "der", "Strahl", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich sah ihn nicht in allen sp\u00e4tern Stunden.", "tokens": ["Ich", "sah", "ihn", "nicht", "in", "al\u00b7len", "sp\u00e4\u00b7tern", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Hat mich vielleicht in deinen Zaubermienen", "tokens": ["Hat", "mich", "viel\u00b7leicht", "in", "dei\u00b7nen", "Zau\u00b7ber\u00b7mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Widerschein der Sonne nur geblendet?", "tokens": ["Der", "Wi\u00b7der\u00b7schein", "der", "Son\u00b7ne", "nur", "ge\u00b7blen\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auch dann ein Strahl der Liebe, die nicht endet,", "tokens": ["Auch", "dann", "ein", "Strahl", "der", "Lie\u00b7be", ",", "die", "nicht", "en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN", "$,", "PRELS", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch besser w\u00e4rs, mir h\u00e4tt er nicht geschienen.", "tokens": ["Doch", "bes\u00b7ser", "w\u00e4rs", ",", "mir", "h\u00e4tt", "er", "nicht", "ge\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}