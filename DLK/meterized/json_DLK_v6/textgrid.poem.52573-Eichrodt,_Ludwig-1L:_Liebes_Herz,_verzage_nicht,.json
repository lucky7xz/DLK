{"textgrid.poem.52573": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Liebes Herz, verzage nicht,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebes Herz, verzage nicht,", "tokens": ["Lie\u00b7bes", "Herz", ",", "ver\u00b7za\u00b7ge", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df die b\u00f6sen Menschen w\u00fcthen,", "tokens": ["La\u00df", "die", "b\u00f6\u00b7sen", "Men\u00b7schen", "w\u00fct\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "An dem Himmel wohnt das Licht", "tokens": ["An", "dem", "Him\u00b7mel", "wohnt", "das", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und beleuchtet was sie br\u00fcten!", "tokens": ["Und", "be\u00b7leuch\u00b7tet", "was", "sie", "br\u00fc\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Es ist Nichts so fein gesponnen,", "tokens": ["Es", "ist", "Nichts", "so", "fein", "ge\u00b7spon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Endlich kommt es an die Sonnen.", "tokens": ["End\u00b7lich", "kommt", "es", "an", "die", "Son\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Liebe Seele, fasse Muth!", "tokens": ["Lie\u00b7be", "See\u00b7le", ",", "fas\u00b7se", "Muth", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ist auch dein St\u00fcndlein kommen \u2013", "tokens": ["Und", "ist", "auch", "dein", "St\u00fcnd\u00b7lein", "kom\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was in deiner Tiefe ruht,", "tokens": ["Was", "in", "dei\u00b7ner", "Tie\u00b7fe", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleibt dir ewig unbenommen.", "tokens": ["Bleibt", "dir", "e\u00b7wig", "un\u00b7be\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Eine Perle kann ich nennen,", "tokens": ["Ei\u00b7ne", "Per\u00b7le", "kann", "ich", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die sie dir nicht rauben k\u00f6nnen.", "tokens": ["Die", "sie", "dir", "nicht", "rau\u00b7ben", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "PTKNEG", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein Gef\u00fchl, es st\u00e4rkt, es labt!", "tokens": ["Ein", "Ge\u00b7f\u00fchl", ",", "es", "st\u00e4rkt", ",", "es", "labt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus den guten treuen Herzen,", "tokens": ["Aus", "den", "gu\u00b7ten", "treu\u00b7en", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Derer, die dich lieb gehabt,", "tokens": ["De\u00b7rer", ",", "die", "dich", "lieb", "ge\u00b7habt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "ADJD", "VAPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann der B\u00f6se dich nicht merzen.", "tokens": ["Kann", "der", "B\u00f6\u00b7se", "dich", "nicht", "mer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist die Rettung hier verloren,", "tokens": ["Ist", "die", "Ret\u00b7tung", "hier", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wirst du dorten neu geboren!", "tokens": ["Wirst", "du", "dor\u00b7ten", "neu", "ge\u00b7bo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Um so theurer nur wird sein,", "tokens": ["Um", "so", "theu\u00b7rer", "nur", "wird", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADJD", "ADV", "VAFIN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ungl\u00fcckskind, dein Angedenken;", "tokens": ["Un\u00b7gl\u00fccks\u00b7kind", ",", "dein", "An\u00b7ge\u00b7den\u00b7ken", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zweifel auch und andre Pein", "tokens": ["Zwei\u00b7fel", "auch", "und", "and\u00b7re", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "KON", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Werden dann dich nimmer kr\u00e4nken.", "tokens": ["Wer\u00b7den", "dann", "dich", "nim\u00b7mer", "kr\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur um Liebe wirst du werben,", "tokens": ["Nur", "um", "Lie\u00b7be", "wirst", "du", "wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sei getrost im bittern Sterben!", "tokens": ["Sei", "ge\u00b7trost", "im", "bit\u00b7tern", "Ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}