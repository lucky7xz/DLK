{"dta.poem.2737": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die vergr\u00fcnten Linden/  \n Bey Absterben Hn. P. von der L. den 14.  \n Septembr. 1679.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der W\u00e4lder Zier die hohen Linden/", "tokens": ["Der", "W\u00e4l\u00b7der", "Zier", "die", "ho\u00b7hen", "Lin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "ADJA", "NE", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Nymfen Lust- und Sommer-Hau\u00df/", "tokens": ["Der", "Nym\u00b7fen", "Lust", "und", "Som\u00b7mer\u00b7Hau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "TRUNC", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die sich so herrlich breiten aus", "tokens": ["Die", "sich", "so", "herr\u00b7lich", "brei\u00b7ten", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "ADV", "ADJD", "ADJA", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df man kan k\u00fchlen Schatten finden/", "tokens": ["Da\u00df", "man", "kan", "k\u00fch\u00b7len", "Schat\u00b7ten", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "ADJA", "NN", "VVINF", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Wenn alles fast f\u00fcr Hitze schmacht/", "tokens": ["Wenn", "al\u00b7les", "fast", "f\u00fcr", "Hit\u00b7ze", "schmacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So webt der Bl\u00e4tter gr\u00fcne Nacht", "tokens": ["So", "webt", "der", "Bl\u00e4t\u00b7ter", "gr\u00fc\u00b7ne", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den F\u00fcrhang/ drein die Westen spielen/", "tokens": ["Den", "F\u00fcr\u00b7hang", "/", "drein", "die", "Wes\u00b7ten", "spie\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df man wirds\u00fcss\u2019 Erfrischung f\u00fchlen.", "tokens": ["Da\u00df", "man", "wird\u00b7s\u00fcss'", "Er\u00b7fri\u00b7schung", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VAFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die sind den G\u00f6ttern l\u00e4ngst geweyhet", "tokens": ["Die", "sind", "den", "G\u00f6t\u00b7tern", "l\u00e4ngst", "ge\u00b7wey\u00b7het"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wurden Tempeln beygesetzt/", "tokens": ["Und", "wur\u00b7den", "Tem\u00b7peln", "bey\u00b7ge\u00b7setzt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Opffer-Vieh da abgemetzt/", "tokens": ["Das", "Opf\u00b7fer\u00b7Vieh", "da", "ab\u00b7ge\u00b7metzt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Seher hat da propheceyet/", "tokens": ["Der", "Se\u00b7her", "hat", "da", "pro\u00b7phe\u00b7ce\u00b7yet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVFIN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Kurtz: man lie\u00df gar des Weyrauchs-Flamm", "tokens": ["Kurtz", ":", "man", "lie\u00df", "gar", "des", "Wey\u00b7rauchs\u00b7Flamm"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$.", "PIS", "VVFIN", "ADV", "ART", "NN"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.6": {"text": "An dem bejahrtem Linden-Stamm/", "tokens": ["An", "dem", "be\u00b7jahr\u00b7tem", "Lin\u00b7den\u00b7Stamm", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bi\u00df an die blauen Wolcken steigen/", "tokens": ["Bi\u00df", "an", "die", "blau\u00b7en", "Wol\u00b7cken", "stei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Damit die Andacht zu bezeugen.", "tokens": ["Da\u00b7mit", "die", "An\u00b7dacht", "zu", "be\u00b7zeu\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Tichter wissen mehr zu sagen/", "tokens": ["Die", "Tich\u00b7ter", "wis\u00b7sen", "mehr", "zu", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie melden wie das sch\u00f6ne Weib", "tokens": ["Sie", "mel\u00b7den", "wie", "das", "sch\u00f6\u00b7ne", "Weib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Philyra/ nachdem ihr Leib", "tokens": ["Die", "Phi\u00b7ly\u00b7ra", "/", "nach\u00b7dem", "ihr", "Leib"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Pferd-Mensch Chiron hat getragen/", "tokens": ["Den", "Pferd\u00b7Mensch", "Chi\u00b7ron", "hat", "ge\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Verwandelt sey in diesen Baum/", "tokens": ["Ver\u00b7wan\u00b7delt", "sey", "in", "die\u00b7sen", "Baum", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil ihrem Bitten statt und Raum", "tokens": ["Weil", "ih\u00b7rem", "Bit\u00b7ten", "statt", "und", "Raum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PTKVZ", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der grosse Jupiter gegeben/", "tokens": ["Der", "gros\u00b7se", "Ju\u00b7pi\u00b7ter", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sie solt\u2019 in Zweigen k\u00fcnfftig leben.", "tokens": ["Sie", "solt'", "in", "Zwei\u00b7gen", "k\u00fcnff\u00b7tig", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Als nun der G\u00f6tzen-Dienst verschwunden/", "tokens": ["Als", "nun", "der", "G\u00f6t\u00b7zen\u00b7Dienst", "ver\u00b7schwun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hat doch der Linde Sch\u00e4tzbarkeit/", "tokens": ["Hat", "doch", "der", "Lin\u00b7de", "Sch\u00e4tz\u00b7bar\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NE", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil sie der Menschen Hertz erfreut/", "tokens": ["Weil", "sie", "der", "Men\u00b7schen", "Hertz", "er\u00b7freut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch immer hohes Lob gefunden:", "tokens": ["Noch", "im\u00b7mer", "ho\u00b7hes", "Lob", "ge\u00b7fun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie ist der F\u00fcrsten Taffel-Hau\u00df;", "tokens": ["Sie", "ist", "der", "F\u00fcrs\u00b7ten", "Taf\u00b7fel\u00b7Hau\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Dorff legt sein Gericht da aus:", "tokens": ["Das", "Dorff", "legt", "sein", "Ge\u00b7richt", "da", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Nymfen h\u00e4gen umb sie T\u00e4ntze", "tokens": ["Die", "Nym\u00b7fen", "h\u00e4\u00b7gen", "umb", "sie", "T\u00e4nt\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und flechten aus den Bl\u00e4ttern Kr\u00e4ntze.", "tokens": ["Und", "flech\u00b7ten", "aus", "den", "Bl\u00e4t\u00b7tern", "Kr\u00e4nt\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wird ihr laubicht Gipfelbl\u00fchen", "tokens": ["Und", "wird", "ihr", "lau\u00b7bicht", "Gip\u00b7fel\u00b7bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So steht sie gleichsam wie beschneyt;", "tokens": ["So", "steht", "sie", "gleich\u00b7sam", "wie", "be\u00b7schneyt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KOKOM", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es prangt ihr wei\u00df und gr\u00fcnes Kleid/", "tokens": ["Es", "prangt", "ihr", "wei\u00df", "und", "gr\u00fc\u00b7nes", "Kleid", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kan die Augen nach sich ziehen.", "tokens": ["Und", "kan", "die", "Au\u00b7gen", "nach", "sich", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Pomerantzen theures Oel/", "tokens": ["Der", "Po\u00b7me\u00b7rant\u00b7zen", "theu\u00b7res", "O\u00b7el", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und der Jesminen Geist und Seel/", "tokens": ["Und", "der", "Jes\u00b7mi\u00b7nen", "Geist", "und", "Seel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mag uns nicht so viel Lieblich keiten", "tokens": ["Mag", "uns", "nicht", "so", "viel", "Lieb\u00b7lich", "kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "ADV", "ADJD", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Als diese Bl\u00fcthe zubereiten.", "tokens": ["Als", "die\u00b7se", "Bl\u00fc\u00b7the", "zu\u00b7be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach aber/ wer sieht sonder Grauen/", "tokens": ["Ach", "a\u00b7ber", "/", "wer", "sieht", "son\u00b7der", "Grau\u00b7en", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$(", "PWS", "VVFIN", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn offt in unvermerckter Eil/", "tokens": ["Wenn", "offt", "in", "un\u00b7ver\u00b7merck\u00b7ter", "Eil", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein unversch\u00e4mt und k\u00fchnes Beil", "tokens": ["Ein", "un\u00b7ver\u00