{"dta.poem.10726": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "ArISTARCHUS,  \n SIVE DE CONTEMPTU LIN-  \n GU\u00c6 TEUTONIC \u00c6 .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Der liebe brunst balt frewde macht dem Hertzen/", "tokens": ["Der", "lie\u00b7be", "brunst", "balt", "frew\u00b7de", "macht", "dem", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bald lohnet sie mit wehmut vnd mit schmertzen/", "tokens": ["Bald", "loh\u00b7net", "sie", "mit", "weh\u00b7mut", "vnd", "mit", "schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es ist jhr glantz ein Schatten vnd ein schein/", "tokens": ["Es", "ist", "jhr", "glantz", "ein", "Schat\u00b7ten", "vnd", "ein", "schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ART", "NN", "KON", "ART", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vnd jhre lust ist bitter s\u00fcsse Pein.", "tokens": ["Vnd", "jhre", "lust", "ist", "bit\u00b7ter", "s\u00fcs\u00b7se", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relax