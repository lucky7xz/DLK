{"dta.poem.15846": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "51.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geh' unempfindlich nicht und unger\u00fchrt vorbei", "tokens": ["Geh'", "un\u00b7emp\u00b7find\u00b7lich", "nicht", "und", "un\u00b7ge\u00b7r\u00fchrt", "vor\u00b7bei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "PTKNEG", "KON", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vorm Sch\u00f6nen dieser Welt, alsobs nicht Gottes sei.", "tokens": ["Vorm", "Sch\u00f6\u00b7nen", "die\u00b7ser", "Welt", ",", "al\u00b7sobs", "nicht", "Got\u00b7tes", "sei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "NN", "$,", "KOUS", "PTKNEG", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zu schauen Blumenflor, zu h\u00f6ren Vogelchor,", "tokens": ["Zu", "schau\u00b7en", "Blu\u00b7men\u00b7flor", ",", "zu", "h\u00f6\u00b7ren", "Vo\u00b7ge\u00b7lchor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PTKZU", "VVINF", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat er das Auge dir erschlossen und das Ohr.", "tokens": ["Hat", "er", "das", "Au\u00b7ge", "dir", "er\u00b7schlos\u00b7sen", "und", "das", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PPER", "VVPP", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn du verstopfen willst das Ohr, das Auge schlie\u00dfen,", "tokens": ["Wenn", "du", "ver\u00b7stop\u00b7fen", "willst", "das", "Ohr", ",", "das", "Au\u00b7ge", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "ART", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kann Gottes Preis dir nicht ert\u00f6nen und ersprie\u00dfen.", "tokens": ["Kann", "Got\u00b7tes", "Preis", "dir", "nicht", "er\u00b7t\u00f6\u00b7nen", "und", "er\u00b7sprie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "NN", "PPER", "PTKNEG", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Viel Sch\u00f6nes hat die Welt, das, um von dir genossen", "tokens": ["Viel", "Sch\u00f6\u00b7nes", "hat", "die", "Welt", ",", "das", ",", "um", "von", "dir", "ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "$,", "PDS", "$,", "KOUI", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu werden, Gott erschuf, genie\u00df' es unverdrossen!", "tokens": ["Zu", "wer\u00b7den", ",", "Gott", "er\u00b7schuf", ",", "ge\u00b7ni\u00b7e\u00df'", "es", "un\u00b7ver\u00b7dros\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VAINF", "$,", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}