{"textgrid.poem.40316": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: In einer W\u00fcste lagen viele Menschen,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einer W\u00fcste lagen viele Menschen,", "tokens": ["In", "ei\u00b7ner", "W\u00fcs\u00b7te", "la\u00b7gen", "vie\u00b7le", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die fast verschmachteten; sie wimmerten.", "tokens": ["die", "fast", "ver\u00b7schmach\u00b7te\u00b7ten", ";", "sie", "wim\u00b7mer\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Ein sch\u00f6nes M\u00e4dchen nur,", "tokens": ["Ein", "sch\u00f6\u00b7nes", "M\u00e4d\u00b7chen", "nur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "mit hilflos braunen Augen,", "tokens": ["mit", "hil\u00b7flos", "brau\u00b7nen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "litt stumm den Durst; denn gieriger als der Durst", "tokens": ["litt", "stumm", "den", "Durst", ";", "denn", "gie\u00b7ri\u00b7ger", "als", "der", "Durst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$.", "KON", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "brannte ihr seliges Mitleid ...", "tokens": ["brann\u00b7te", "ihr", "se\u00b7li\u00b7ges", "Mit\u00b7leid", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Da trat, vom gl\u00fchenden Horizont herwachsend,", "tokens": ["Da", "trat", ",", "vom", "gl\u00fc\u00b7hen\u00b7den", "Ho\u00b7ri\u00b7zont", "her\u00b7wach\u00b7send", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "ein fremder Mann vor dieses Volk;", "tokens": ["ein", "frem\u00b7der", "Mann", "vor", "die\u00b7ses", "Volk", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "der hob den Zeigefinger ihnen dar.", "tokens": ["der", "hob", "den", "Zei\u00b7ge\u00b7fin\u00b7ger", "ih\u00b7nen", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Aus der gereckten, zitternden Spitze quoll", "tokens": ["Aus", "der", "ge\u00b7reck\u00b7ten", ",", "zit\u00b7tern\u00b7den", "Spit\u00b7ze", "quoll"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "ein gro\u00dfer Tropfen Blut, quoll, hing, und fiel,", "tokens": ["ein", "gro\u00b7\u00dfer", "Trop\u00b7fen", "Blut", ",", "quoll", ",", "hing", ",", "und", "fiel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "fiel in den Sand;", "tokens": ["fiel", "in", "den", "Sand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "verwundert sah das Volk den fremden Mann.", "tokens": ["ver\u00b7wun\u00b7dert", "sah", "das", "Volk", "den", "frem\u00b7den", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Der stand und stand, Tropfen auf Tropfen fiel", "tokens": ["Der", "stand", "und", "stand", ",", "Trop\u00b7fen", "auf", "Trop\u00b7fen", "fiel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "$,", "NN", "APPR", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "aus seinem Finger in den Sand;", "tokens": ["aus", "sei\u00b7nem", "Fin\u00b7ger", "in", "den", "Sand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "und immer, wenn die rote Quelle troff,", "tokens": ["und", "im\u00b7mer", ",", "wenn", "die", "ro\u00b7te", "Quel\u00b7le", "troff", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "erbleichte schauernd Er, sie aber staunten,", "tokens": ["er\u00b7bleich\u00b7te", "schau\u00b7ernd", "Er", ",", "sie", "a\u00b7ber", "staun\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "$,", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "und Einige \u00e4chzten: er verh\u00f6hnt uns ...", "tokens": ["und", "Ei\u00b7ni\u00b7ge", "\u00e4chz\u00b7ten", ":", "er", "ver\u00b7h\u00f6hnt", "uns", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVINF", "$.", "PPER", "VVFIN", "PPER", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Da schrie er laut mit seiner letzten Glut:", "tokens": ["Da", "schrie", "er", "laut", "mit", "sei\u00b7ner", "letz\u00b7ten", "Glut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "So kommt doch, trinkt! f\u00fcr Euch verblut'ich mich!", "tokens": ["So", "kommt", "doch", ",", "trinkt", "!", "f\u00fcr", "Euch", "ver\u00b7blut'\u00b7ich", "mich", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "VVFIN", "$.", "APPR", "PPER", "ADJD", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Doch jenes M\u00e4dchen sprach, inde\u00df er hinlosch:", "tokens": ["Doch", "je\u00b7nes", "M\u00e4d\u00b7chen", "sprach", ",", "in\u00b7de\u00df", "er", "hin\u00b7losch", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "$,", "ADV", "PPER", "ADJD", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.22": {"text": "Sie wollen Wasser ...", "tokens": ["Sie", "wol\u00b7len", "Was\u00b7ser", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}}}}}