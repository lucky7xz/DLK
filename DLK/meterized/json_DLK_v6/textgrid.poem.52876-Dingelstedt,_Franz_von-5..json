{"textgrid.poem.52876": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Schildwacht schreitet auf und ab", "tokens": ["Die", "Schild\u00b7wacht", "schrei\u00b7tet", "auf", "und", "ab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und pfeift sich ein Liedel unerm\u00fcdlich.", "tokens": ["Und", "pfeift", "sich", "ein", "Lie\u00b7del", "un\u00b7er\u00b7m\u00fcd\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hier ist das Gef\u00e4ngnis, schwarz wie ein Grab,", "tokens": ["Hier", "ist", "das", "Ge\u00b7f\u00e4ng\u00b7nis", ",", "schwarz", "wie", "ein", "Grab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Aber nicht so still, so friedlich.", "tokens": ["A\u00b7ber", "nicht", "so", "still", ",", "so", "fried\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Es rasselt hinter den Gittern schwer", "tokens": ["Es", "ras\u00b7selt", "hin\u00b7ter", "den", "Git\u00b7tern", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von eisernen Ketten und B\u00e4ndern,", "tokens": ["Von", "ei\u00b7ser\u00b7nen", "Ket\u00b7ten", "und", "B\u00e4n\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "St\u00f6hnen und \u00c4chzen zieht hin und her", "tokens": ["St\u00f6h\u00b7nen", "und", "\u00c4ch\u00b7zen", "zieht", "hin", "und", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "PTKVZ", "KON", "ADV"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und verhallt an den steinernen St\u00e4ndern.", "tokens": ["Und", "ver\u00b7hallt", "an", "den", "stei\u00b7ner\u00b7nen", "St\u00e4n\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "In jene Stangen packt eine Faust,", "tokens": ["In", "je\u00b7ne", "Stan\u00b7gen", "packt", "ei\u00b7ne", "Faust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der mag's noch nicht lange gewohnt sein!", "tokens": ["Der", "mag's", "noch", "nicht", "lan\u00b7ge", "ge\u00b7wohnt", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "PTKNEG", "ADV", "VVPP", "VAINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wie das wilde Gelock im Winde saust,", "tokens": ["Wie", "das", "wil\u00b7de", "Ge\u00b7lock", "im", "Win\u00b7de", "saust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wie die Augen blitzen im Mondschein!", "tokens": ["Wie", "die", "Au\u00b7gen", "blit\u00b7zen", "im", "Mond\u00b7schein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Herunter, Bursche! Sonst schrei' ich wach", "tokens": ["Her\u00b7un\u00b7ter", ",", "Bur\u00b7sche", "!", "Sonst", "schrei'", "ich", "wach"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$.", "ADV", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Schlie\u00dfer und seine Genossen,", "tokens": ["Den", "Schlie\u00b7\u00dfer", "und", "sei\u00b7ne", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Dann wirst Du an Dein dunkles Gemach", "tokens": ["Dann", "wirst", "Du", "an", "Dein", "dunk\u00b7les", "Ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Noch z\u00e4rtlicher angeschlossen.", "tokens": ["Noch", "z\u00e4rt\u00b7li\u00b7cher", "an\u00b7ge\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Fort, strecke Dich in Dein warmes Stroh,", "tokens": ["Fort", ",", "stre\u00b7cke", "Dich", "in", "Dein", "war\u00b7mes", "Stroh", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Versuch's wie die andren zu schlafen,", "tokens": ["Ver\u00b7such's", "wie", "die", "an\u00b7dren", "zu", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Was grinsest Du, was murrst Du so,", "tokens": ["Was", "grin\u00b7sest", "Du", ",", "was", "murrst", "Du", "so", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PWS", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bist Du mehr, als die anderen Sklaven?", "tokens": ["Bist", "Du", "mehr", ",", "als", "die", "an\u00b7de\u00b7ren", "Skla\u00b7ven", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$,", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbnicht besser, nicht schlechter als jene sind,", "tokens": ["\u00bb", "nicht", "bes\u00b7ser", ",", "nicht", "schlech\u00b7ter", "als", "je\u00b7ne", "sind", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "ADJD", "$,", "PTKNEG", "ADJD", "KOKOM", "PDS", "VAFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ein Verbrecher nach Euerer Sitte,", "tokens": ["Ein", "Ver\u00b7bre\u00b7cher", "nach", "Eu\u00b7e\u00b7rer", "Sit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Denke nur eben an Weib und Kind", "tokens": ["Den\u00b7ke", "nur", "e\u00b7ben", "an", "Weib", "und", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und an meines Vater H\u00fctte.", "tokens": ["Und", "an", "mei\u00b7nes", "Va\u00b7ter", "H\u00fct\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und streck' ich mich auf mein faules Stroh,", "tokens": ["Und", "streck'", "ich", "mich", "auf", "mein", "fau\u00b7les", "Stroh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dann von meinen \u00c4ckern tr\u00e4um' ich,", "tokens": ["Dann", "von", "mei\u00b7nen", "\u00c4\u00b7ckern", "tr\u00e4um'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die wogten von Halmen und \u00c4hren so,", "tokens": ["Die", "wog\u00b7ten", "von", "Hal\u00b7men", "und", "\u00c4h\u00b7ren", "so", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die waren so luftig, so r\u00e4umig.", "tokens": ["Die", "wa\u00b7ren", "so", "luf\u00b7tig", ",", "so", "r\u00e4u\u00b7mig", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Nun lieg' ich vielleicht auf meiner Saat,", "tokens": ["Nun", "lieg'", "ich", "viel\u00b7leicht", "auf", "mei\u00b7ner", "Saat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die ein anderer ausgedroschen\u00ab ...", "tokens": ["Die", "ein", "an\u00b7de\u00b7rer", "aus\u00b7ge\u00b7dro\u00b7schen", "\u00ab", "..."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ART", "ADJA", "VVIZU", "$(", "$("], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Still, Kamerad! da kommt der Soldat,", "tokens": ["Still", ",", "Ka\u00b7me\u00b7rad", "!", "da", "kommt", "der", "Sol\u00b7dat", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und meine Latern' ist erloschen.", "tokens": ["Und", "mei\u00b7ne", "La\u00b7tern'", "ist", "er\u00b7lo\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}