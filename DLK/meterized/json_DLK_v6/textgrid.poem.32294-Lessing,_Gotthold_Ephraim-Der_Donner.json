{"textgrid.poem.32294": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Der Donner", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es donnert! \u2013 Freunde, la\u00dft uns trinken!", "tokens": ["Es", "don\u00b7nert", "!", "\u2013", "Freun\u00b7de", ",", "la\u00dft", "uns", "trin\u00b7ken", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "NN", "$,", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Frevler und der Heuchler Heer", "tokens": ["Der", "Frev\u00b7ler", "und", "der", "Heuch\u00b7ler", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mag knechtisch auf die Kniee sinken.", "tokens": ["Mag", "knech\u00b7tisch", "auf", "die", "Kni\u00b7ee", "sin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es donnert! \u2013 Macht die Gl\u00e4ser leer!", "tokens": ["Es", "don\u00b7nert", "!", "\u2013", "Macht", "die", "Gl\u00e4\u00b7ser", "leer", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00dft N\u00fcchterne, la\u00dft Weiber zagen!", "tokens": ["La\u00dft", "N\u00fcch\u00b7ter\u00b7ne", ",", "la\u00dft", "Wei\u00b7ber", "za\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "VVIMP", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zeus ist gerecht, er straft das Meer:", "tokens": ["Zeus", "ist", "ge\u00b7recht", ",", "er", "straft", "das", "Meer", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sollt' er in seinen Nektar schlagen?", "tokens": ["Sollt'", "er", "in", "sei\u00b7nen", "Nek\u00b7tar", "schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}