{"textgrid.poem.39048": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Kirchenfest versammelt vor das Thor", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Kirchenfest versammelt vor das Thor", "tokens": ["Ein", "Kir\u00b7chen\u00b7fest", "ver\u00b7sam\u00b7melt", "vor", "das", "Thor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zum regen Gedr\u00e4nge die Bewohner der Stadt,", "tokens": ["Zum", "re\u00b7gen", "Ge\u00b7dr\u00e4n\u00b7ge", "die", "Be\u00b7woh\u00b7ner", "der", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da gl\u00e4nzt Atlas- und Seidenkleid", "tokens": ["Da", "gl\u00e4nzt", "At\u00b7las", "und", "Sei\u00b7den\u00b7kleid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Abendschimmer auf dem gr\u00fcnen Rasen.", "tokens": ["Im", "A\u00b7bend\u00b7schim\u00b7mer", "auf", "dem", "gr\u00fc\u00b7nen", "Ra\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Frohes Get\u00fcmmel und Kinderjauchzen,", "tokens": ["Fro\u00b7hes", "Ge\u00b7t\u00fcm\u00b7mel", "und", "Kin\u00b7der\u00b7jauch\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Und J\u00fcnglinge wandeln und suchen den Blick", "tokens": ["Und", "J\u00fcng\u00b7lin\u00b7ge", "wan\u00b7deln", "und", "su\u00b7chen", "den", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Der sch\u00f6neren Augen.", "tokens": ["Der", "sch\u00f6\u00b7ne\u00b7ren", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Ha! diese edle Gestalt in gr\u00fcner Seide,", "tokens": ["Ha", "!", "die\u00b7se", "ed\u00b7le", "Ge\u00b7stalt", "in", "gr\u00fc\u00b7ner", "Sei\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PDAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Wandelnd an der Seite des entz\u00fcckten Br\u00e4utigams,", "tokens": ["Wan\u00b7delnd", "an", "der", "Sei\u00b7te", "des", "ent\u00b7z\u00fcck\u00b7ten", "Br\u00e4u\u00b7ti\u00b7gams", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "ADJA", "NE", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Uebergl\u00e4nzt sie alle an Frische, Sch\u00f6nheit und Augenglanz,", "tokens": ["Ue\u00b7ber\u00b7gl\u00e4nzt", "sie", "al\u00b7le", "an", "Fri\u00b7sche", ",", "Sch\u00f6n\u00b7heit", "und", "Au\u00b7gen\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Wie sie im leichten Gespr\u00e4ch die vollen Lippen", "tokens": ["Wie", "sie", "im", "leich\u00b7ten", "Ge\u00b7spr\u00e4ch", "die", "vol\u00b7len", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Holdselig l\u00e4chelnd \u00f6ffnet,", "tokens": ["Hold\u00b7se\u00b7lig", "l\u00e4\u00b7chelnd", "\u00f6ff\u00b7net", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Spr\u00fchen blitzend durch das Corallenroth die Lichter der Perlenz\u00e4hnchen,", "tokens": ["Spr\u00fc\u00b7hen", "blit\u00b7zend", "durch", "das", "Co\u00b7ral\u00b7len\u00b7roth", "die", "Lich\u00b7ter", "der", "Per\u00b7len\u00b7z\u00e4hn\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.7": {"text": "Und alles an ihr, Geberde, Gang und Stimme", "tokens": ["Und", "al\u00b7les", "an", "ihr", ",", "Ge\u00b7ber\u00b7de", ",", "Gang", "und", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "APPR", "PPER", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Erklingt wie Musik und nimmt mein Herz gefangen,", "tokens": ["Er\u00b7klingt", "wie", "Mu\u00b7sik", "und", "nimmt", "mein", "Herz", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "NN", "KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+----+-+-+-", "measure": "dactylic.init"}, "line.9": {"text": "Da\u00df ich den Br\u00e4utigam beneidend fast ihn hasse.", "tokens": ["Da\u00df", "ich", "den", "Br\u00e4u\u00b7ti\u00b7gam", "be\u00b7nei\u00b7dend", "fast", "ihn", "has\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NE", "VVPP", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Da nimmt die Himmlische aus ihrem K\u00f6rbchen,", "tokens": ["Da", "nimmt", "die", "Himm\u00b7li\u00b7sche", "aus", "ih\u00b7rem", "K\u00f6rb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gro\u00dfe lombardische N\u00fcsse,", "tokens": ["Gro\u00b7\u00dfe", "lom\u00b7bar\u00b7di\u00b7sche", "N\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und bei\u00dft sie laut krachend hinter rother Lippengluth", "tokens": ["Und", "bei\u00dft", "sie", "laut", "kra\u00b7chend", "hin\u00b7ter", "ro\u00b7ther", "Lip\u00b7pen\u00b7gluth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Mit den Perlenz\u00e4hnchen auf.", "tokens": ["Mit", "den", "Per\u00b7len\u00b7z\u00e4hn\u00b7chen", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "O Br\u00e4utigam! \u00e4rmster der Menschen!", "tokens": ["O", "Br\u00e4u\u00b7ti\u00b7gam", "!", "\u00e4rms\u00b7ter", "der", "Men\u00b7schen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "ADJD", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "So rief ich entfliehend. \u2013", "tokens": ["So", "rief", "ich", "ent\u00b7flie\u00b7hend", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wohl h\u00f6rt man von Sirenen, Vampyren, Empusen,", "tokens": ["Wohl", "h\u00f6rt", "man", "von", "Si\u00b7re\u00b7nen", ",", "Vam\u00b7py\u00b7ren", ",", "Em\u00b7pu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+---+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und anderm tollen Zaubergespuk,", "tokens": ["Und", "an\u00b7derm", "tol\u00b7len", "Zau\u00b7ber\u00b7ge\u00b7spuk", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Das d\u00e4monisch sich der M\u00e4nnerherzen", "tokens": ["Das", "d\u00e4\u00b7mo\u00b7nisch", "sich", "der", "M\u00e4n\u00b7ner\u00b7her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "In t\u00e4uschender Gestalt bem\u00e4chtigt;", "tokens": ["In", "t\u00e4u\u00b7schen\u00b7der", "Ge\u00b7stalt", "be\u00b7m\u00e4ch\u00b7tigt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und ich war (furchtbar!) nahe der grausen Gefahr", "tokens": ["Und", "ich", "war", "(", "furcht\u00b7bar", "!", ")", "na\u00b7he", "der", "grau\u00b7sen", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "$(", "ADJD", "$.", "$(", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.8": {"text": "Beth\u00f6rt zu lieben eine Nu\u00dfknackerin!", "tokens": ["Be\u00b7th\u00f6rt", "zu", "lie\u00b7ben", "ei\u00b7ne", "Nu\u00df\u00b7knac\u00b7ke\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}