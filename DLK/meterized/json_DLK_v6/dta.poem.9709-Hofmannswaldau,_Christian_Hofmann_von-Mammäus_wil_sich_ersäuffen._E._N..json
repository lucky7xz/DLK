{"dta.poem.9709": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Mamm\u00e4us wil sich ers\u00e4uffen.  \n E. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mamm\u00e4us klaget stets/ und fluchet auf die liebe/", "tokens": ["Mam\u00b7m\u00e4\u00b7us", "kla\u00b7get", "stets", "/", "und", "flu\u00b7chet", "auf", "die", "lie\u00b7be", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$(", "KON", "VVFIN", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Indem sie allemahl sein ziel zur\u00fccke triebe.", "tokens": ["In\u00b7dem", "sie", "al\u00b7le\u00b7mahl", "sein", "ziel", "zu\u00b7r\u00fc\u00b7cke", "trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er klaget/ flucht/ und spricht: ich mu\u00df des todes seyn.", "tokens": ["Er", "kla\u00b7get", "/", "flucht", "/", "und", "spricht", ":", "ich", "mu\u00df", "des", "to\u00b7des", "seyn", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "VVFIN", "$(", "KON", "VVFIN", "$.", "PPER", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die martern mehren sich und dr\u00fccken mich mit hauffen.", "tokens": ["Die", "mar\u00b7tern", "meh\u00b7ren", "sich", "und", "dr\u00fc\u00b7cken", "mich", "mit", "hauf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "KON", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie werd ich ihrer lo\u00df? ich will ins wasser lauffen.", "tokens": ["Wie", "werd", "ich", "ih\u00b7rer", "lo\u00df", "?", "ich", "will", "ins", "was\u00b7ser", "lauf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPOSAT", "PTKVZ", "$.", "PPER", "VMFIN", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich dachte: gut/ so kommt ein stockfisch mehr hinein.", "tokens": ["Ich", "dach\u00b7te", ":", "gut", "/", "so", "kommt", "ein", "stock\u00b7fisch", "mehr", "hin\u00b7ein", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADJD", "$(", "ADV", "VVFIN", "ART", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}