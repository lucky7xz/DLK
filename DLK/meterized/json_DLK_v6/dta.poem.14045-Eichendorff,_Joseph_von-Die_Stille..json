{"dta.poem.14045": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Stille.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ich w\u00fcnscht', es w\u00e4re schon Morgen,", "tokens": ["Ich", "w\u00fcnscht'", ",", "es", "w\u00e4\u00b7re", "schon", "Mor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da fliegen zwei Lerchen auf,", "tokens": ["Da", "flie\u00b7gen", "zwei", "Ler\u00b7chen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die \u00fcberfliegen einander,", "tokens": ["Die", "\u00fc\u00b7berf\u00b7lie\u00b7gen", "ein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mein Herze folgt ihrem Lauf.", "tokens": ["Mein", "Her\u00b7ze", "folgt", "ih\u00b7rem", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}