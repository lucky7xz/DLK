{"textgrid.poem.59533": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, der in flammende Gebete", "tokens": ["Du", ",", "der", "in", "flam\u00b7men\u00b7de", "Ge\u00b7be\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Des Lebens h\u00f6chste Kraft gelegt", "tokens": ["Des", "Le\u00b7bens", "h\u00f6chs\u00b7te", "Kraft", "ge\u00b7legt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und aus des Busens tiefster St\u00e4tte", "tokens": ["Und", "aus", "des", "Bu\u00b7sens", "tiefs\u00b7ter", "St\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Herz in s\u00fc\u00dfer Sehnsucht regt,", "tokens": ["Das", "Herz", "in", "s\u00fc\u00b7\u00dfer", "Sehn\u00b7sucht", "regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du, aller Himmel h\u00f6chster Meister,", "tokens": ["Du", ",", "al\u00b7ler", "Him\u00b7mel", "h\u00f6chs\u00b7ter", "Meis\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du, alles Lebens h\u00f6chster Schein,", "tokens": ["Du", ",", "al\u00b7les", "Le\u00b7bens", "h\u00f6chs\u00b7ter", "Schein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Komm, f\u00fchre in das Land der Geister", "tokens": ["Komm", ",", "f\u00fch\u00b7re", "in", "das", "Land", "der", "Geis\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dein sehnend Kind zum Lichte ein.", "tokens": ["Dein", "seh\u00b7nend", "Kind", "zum", "Lich\u00b7te", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "CARD", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo Myriaden Sonnen kreisen,", "tokens": ["Wo", "My\u00b7ria\u00b7den", "Son\u00b7nen", "krei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Morgenr\u00f6ten Jubelklang", "tokens": ["Der", "Mor\u00b7gen\u00b7r\u00f6\u00b7ten", "Ju\u00b7bel\u00b7klang"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In tausendfach verschiednen Weisen", "tokens": ["In", "tau\u00b7send\u00b7fach", "ver\u00b7schied\u00b7nen", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ert\u00f6nt, ein heiliger Gesang,", "tokens": ["Er\u00b7t\u00f6nt", ",", "ein", "hei\u00b7li\u00b7ger", "Ge\u00b7sang", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo Millionen Heil'ge knien", "tokens": ["Wo", "Mil\u00b7lion\u00b7en", "Heil'\u00b7ge", "kni\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und schauen dir ins Angesicht,", "tokens": ["Und", "schau\u00b7en", "dir", "ins", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O Vater! Gott! la\u00df dort mich bl\u00fchen", "tokens": ["O", "Va\u00b7ter", "!", "Gott", "!", "la\u00df", "dort", "mich", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "NN", "$.", "VVIMP", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Am kleinsten Strahl von deinem Licht!", "tokens": ["Am", "kleins\u00b7ten", "Strahl", "von", "dei\u00b7nem", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn ach! zur kalten Erde wollen", "tokens": ["Denn", "ach", "!", "zur", "kal\u00b7ten", "Er\u00b7de", "wol\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "XY", "$.", "APPRART", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Himmelslichter nicht herab,", "tokens": ["Die", "Him\u00b7mels\u00b7lich\u00b7ter", "nicht", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ihre goldnen Lampen rollen", "tokens": ["Und", "ih\u00b7re", "gold\u00b7nen", "Lam\u00b7pen", "rol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gef\u00fchllos \u00fcber Sarg und Grab;", "tokens": ["Ge\u00b7f\u00fchl\u00b7los", "\u00fc\u00b7ber", "Sarg", "und", "Grab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Wechsel hier vom Leid zum Gl\u00fccke,", "tokens": ["Der", "Wech\u00b7sel", "hier", "vom", "Leid", "zum", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vom Gl\u00fcck zum Leide ist zu schwer:", "tokens": ["Vom", "Gl\u00fcck", "zum", "Lei\u00b7de", "ist", "zu", "schwer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "VAFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es bricht die zarte Geisterbr\u00fccke,", "tokens": ["Es", "bricht", "die", "zar\u00b7te", "Geis\u00b7ter\u00b7br\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und Paradiese bl\u00fchn nicht mehr.", "tokens": ["Und", "Pa\u00b7ra\u00b7die\u00b7se", "bl\u00fchn", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Drum Himmel steige! Sinke Erde", "tokens": ["Drum", "Him\u00b7mel", "stei\u00b7ge", "!", "Sin\u00b7ke", "Er\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "NN", "VVFIN", "$.", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und irdisch Leben unter mir!", "tokens": ["Und", "ir\u00b7disch", "Le\u00b7ben", "un\u00b7ter", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich ein wei\u00dfer Engel werde,", "tokens": ["Da\u00df", "ich", "ein", "wei\u00b7\u00dfer", "En\u00b7gel", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Steht, wei\u00dfe Engel, neben mir,", "tokens": ["Steht", ",", "wei\u00b7\u00dfe", "En\u00b7gel", ",", "ne\u00b7ben", "mir", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und helft im Glauben mir vollenden", "tokens": ["Und", "helft", "im", "Glau\u00b7ben", "mir", "voll\u00b7en\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Erde m\u00fchevollen Streit,", "tokens": ["Der", "Er\u00b7de", "m\u00fc\u00b7he\u00b7vol\u00b7len", "Streit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und traget mich auf reinen H\u00e4nden", "tokens": ["Und", "tra\u00b7get", "mich", "auf", "rei\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Empor ins Land der Seligkeit.", "tokens": ["Em\u00b7por", "ins", "Land", "der", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}