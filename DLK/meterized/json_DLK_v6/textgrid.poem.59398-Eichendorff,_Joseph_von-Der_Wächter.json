{"textgrid.poem.59398": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der W\u00e4chter", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "N\u00e4chtlich macht der Herr die Rund,", "tokens": ["N\u00e4cht\u00b7lich", "macht", "der", "Herr", "die", "Rund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sucht die Seinen unverdrossen,", "tokens": ["Sucht", "die", "Sei\u00b7nen", "un\u00b7ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PPOSS", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber \u00fcberall verschlossen", "tokens": ["A\u00b7ber", "\u00fc\u00b7be\u00b7rall", "ver\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trifft er T\u00fcr und Herzensgrund,", "tokens": ["Trifft", "er", "T\u00fcr", "und", "Her\u00b7zens\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und er wendet sich voll Trauer:", "tokens": ["Und", "er", "wen\u00b7det", "sich", "voll", "Trau\u00b7er", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Niemand ist, der mit mir wacht. \u2013", "tokens": ["Nie\u00b7mand", "ist", ",", "der", "mit", "mir", "wacht", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "VAFIN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur der Wald vernimmt's mit Schauer,", "tokens": ["Nur", "der", "Wald", "ver\u00b7nimmt's", "mit", "Schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Rauschet fromm die ganze Nacht.", "tokens": ["Rau\u00b7schet", "fromm", "die", "gan\u00b7ze", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Waldw\u00e4rts durch die Einsamkeit", "tokens": ["Wald\u00b7w\u00e4rts", "durch", "die", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rt ich \u00fcber Tal und Kl\u00fcften", "tokens": ["H\u00f6rt", "ich", "\u00fc\u00b7ber", "Tal", "und", "Kl\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Glocken in den stillen L\u00fcften,", "tokens": ["Glo\u00b7cken", "in", "den", "stil\u00b7len", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie aus fernem Morgen weit \u2013", "tokens": ["Wie", "aus", "fer\u00b7nem", "Mor\u00b7gen", "weit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "An die Tore will ich schlagen,", "tokens": ["An", "die", "To\u00b7re", "will", "ich", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "An Palast und H\u00fctten: Auf!", "tokens": ["An", "Pa\u00b7last", "und", "H\u00fct\u00b7ten", ":", "Auf", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$.", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Flammend schon die Gipfel ragen,", "tokens": ["Flam\u00b7mend", "schon", "die", "Gip\u00b7fel", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wachet auf, wacht auf, wacht auf!", "tokens": ["Wa\u00b7chet", "auf", ",", "wacht", "auf", ",", "wacht", "auf", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}