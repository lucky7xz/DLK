{"textgrid.poem.65385": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht auf die H\u00f6he will ich steigen,", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht auf die H\u00f6he will ich steigen,", "tokens": ["Nicht", "auf", "die", "H\u00f6\u00b7he", "will", "ich", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht in die Ferne will ich sehn.", "tokens": ["Nicht", "in", "die", "Fer\u00b7ne", "will", "ich", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie weit sich ihm die R\u00e4ume zeigen,", "tokens": ["Wie", "weit", "sich", "ihm", "die", "R\u00e4u\u00b7me", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PRF", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es bleibt mein Herz erschrocken stehn.", "tokens": ["Es", "bleibt", "mein", "Herz", "er\u00b7schro\u00b7cken", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo Berg' und Nebel blau verschwimmen,", "tokens": ["Wo", "Ber\u00b7g'", "und", "Ne\u00b7bel", "blau", "ver\u00b7schwim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "KON", "NN", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie fern von mir, von dir wie fern!", "tokens": ["Wie", "fern", "von", "mir", ",", "von", "dir", "wie", "fern", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "PPER", "$,", "APPR", "PPER", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So hoch die m\u00fcden Augen klimmen,", "tokens": ["So", "hoch", "die", "m\u00fc\u00b7den", "Au\u00b7gen", "klim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie reichen nicht an meinen Stern.", "tokens": ["Sie", "rei\u00b7chen", "nicht", "an", "mei\u00b7nen", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mit anderm Ma\u00dfe will ich messen", "tokens": ["Mit", "an\u00b7derm", "Ma\u00b7\u00dfe", "will", "ich", "mes\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der langen Trennung \u00f6den Raum;", "tokens": ["Der", "lan\u00b7gen", "Tren\u00b7nung", "\u00f6\u00b7den", "Raum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Meilen hat das Herz vergessen,", "tokens": ["Die", "Mei\u00b7len", "hat", "das", "Herz", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ruft nach dir in seinem Traum.", "tokens": ["Und", "ruft", "nach", "dir", "in", "sei\u00b7nem", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da sinken alle Berge nieder,", "tokens": ["Da", "sin\u00b7ken", "al\u00b7le", "Ber\u00b7ge", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die weiten Fl\u00e4chen ziehn sich ein;", "tokens": ["Die", "wei\u00b7ten", "Fl\u00e4\u00b7chen", "ziehn", "sich", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du k\u00f6mmst, du gehst, ich kehre wieder,", "tokens": ["Du", "k\u00f6mmst", ",", "du", "gehst", ",", "ich", "keh\u00b7re", "wie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und unser Pfad ist still und klein.", "tokens": ["Und", "un\u00b7ser", "Pfad", "ist", "still", "und", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}