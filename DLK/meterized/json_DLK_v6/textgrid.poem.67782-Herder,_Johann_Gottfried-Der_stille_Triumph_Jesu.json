{"textgrid.poem.67782": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der stille Triumph Jesu", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Br\u00fcder Jesu, kommt und singet", "tokens": ["Br\u00fc\u00b7der", "Je\u00b7su", ",", "kommt", "und", "sin\u00b7get"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$,", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eures K\u00f6nigs Reich!", "tokens": ["Eu\u00b7res", "K\u00f6\u00b7nigs", "Reich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Euer Lied und Euer Leben", "tokens": ["Eu\u00b7er", "Lied", "und", "Eu\u00b7er", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei dem Herren gleich!", "tokens": ["Sei", "dem", "Her\u00b7ren", "gleich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Von der Erd' hinauf gen Himmel", "tokens": ["Von", "der", "Erd'", "hin\u00b7auf", "gen", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00f6ne mein Gesang!", "tokens": ["T\u00f6\u00b7ne", "mein", "Ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Von der Erd' hinauf gen Himmel", "tokens": ["Von", "der", "Erd'", "hin\u00b7auf", "gen", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ging sein stiller Gang.", "tokens": ["Ging", "sein", "stil\u00b7ler", "Gang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Preiset, Himmel, preiset, Erden,", "tokens": ["Prei\u00b7set", ",", "Him\u00b7mel", ",", "prei\u00b7set", ",", "Er\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gottes Wunderrath!", "tokens": ["Got\u00b7tes", "Wun\u00b7der\u00b7rath", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Seine Lieb' und Allmacht wurden", "tokens": ["Sei\u00b7ne", "Lieb'", "und", "All\u00b7macht", "wur\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stille Menschenthat.", "tokens": ["Stil\u00b7le", "Men\u00b7schen\u00b7that", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Seine Lieb' und Allmacht gingen", "tokens": ["Sei\u00b7ne", "Lieb'", "und", "All\u00b7macht", "gin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tief verkannten Gang.", "tokens": ["Tief", "ver\u00b7kann\u00b7ten", "Gang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Von der Erd' hinauf gen Himmel", "tokens": ["Von", "der", "Erd'", "hin\u00b7auf", "gen", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sing' es mein Gesang!", "tokens": ["Sing'", "es", "mein", "Ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Ewig aus des Vaters Herzen", "tokens": ["E\u00b7wig", "aus", "des", "Va\u00b7ters", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Uns geborner Sohn,", "tokens": ["Uns", "ge\u00b7bor\u00b7ner", "Sohn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Aller Sch\u00f6pfung Licht und Leben,", "tokens": ["Al\u00b7ler", "Sch\u00f6p\u00b7fung", "Licht", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Uns der Liebe Thron,", "tokens": ["Uns", "der", "Lie\u00b7be", "Thron", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Kamst Du nieder, mitzuf\u00fchlen", "tokens": ["Kamst", "Du", "nie\u00b7der", ",", "mit\u00b7zu\u00b7f\u00fch\u00b7len"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "VVIZU"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unser Menschenherz,", "tokens": ["Un\u00b7ser", "Men\u00b7schen\u00b7herz", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Es zu heben, es zu leiten", "tokens": ["Es", "zu", "he\u00b7ben", ",", "es", "zu", "lei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Himmel-himmelw\u00e4rts!", "tokens": ["Him\u00b7mel\u00b7him\u00b7mel\u00b7w\u00e4rts", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}}, "stanza.7": {"line.1": {"text": "Oeffnet Euch, Ihr ew'gen Pforten!", "tokens": ["Oeff\u00b7net", "Euch", ",", "Ihr", "ew'\u00b7gen", "Pfor\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn es zeucht heran", "tokens": ["Denn", "es", "zeucht", "he\u00b7ran"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Blutgef\u00e4rbt der edle Sieger,", "tokens": ["Blut\u00b7ge\u00b7f\u00e4rbt", "der", "ed\u00b7le", "Sie\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der sein Volk gewann!", "tokens": ["Der", "sein", "Volk", "ge\u00b7wann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Alle seines Reiches Guten", "tokens": ["Al\u00b7le", "sei\u00b7nes", "Rei\u00b7ches", "Gu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Folgen still ihm nach;", "tokens": ["Fol\u00b7gen", "still", "ihm", "nach", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sie, wie er, im Dornenkranze,", "tokens": ["Sie", ",", "wie", "er", ",", "im", "Dor\u00b7nen\u00b7kran\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAV", "PPER", "$,", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Voll von sch\u00f6ner Schmach.", "tokens": ["Voll", "von", "sch\u00f6\u00b7ner", "Schmach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Wahrheit Gottes war ihr Leben,", "tokens": ["Wahr\u00b7heit", "Got\u00b7tes", "war", "ihr", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihrer Liebe Gluth;", "tokens": ["Ih\u00b7rer", "Lie\u00b7be", "Gluth", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Hoffnung Gottes war ihr Streben,", "tokens": ["Hoff\u00b7nung", "Got\u00b7tes", "war", "ihr", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "War ihr ewig Gut.", "tokens": ["War", "ihr", "e\u00b7wig", "Gut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Schaar der Lebenden und Todten,", "tokens": ["Schaar", "der", "Le\u00b7ben\u00b7den", "und", "Tod\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freue, freue Dich!", "tokens": ["Freu\u00b7e", ",", "freu\u00b7e", "Dich", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ob sein Samenkorn verweset,", "tokens": ["Ob", "sein", "Sa\u00b7men\u00b7korn", "ver\u00b7we\u00b7set", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bl\u00fcht es ewiglich.", "tokens": ["Bl\u00fcht", "es", "e\u00b7wig\u00b7lich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "Wo die Abendr\u00f6the leuchtet,", "tokens": ["Wo", "die", "A\u00b7bend\u00b7r\u00f6\u00b7the", "leuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird sein Reich einst gl\u00fchn,", "tokens": ["Wird", "sein", "Reich", "einst", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Tief im letzten Keim der Sch\u00f6pfung", "tokens": ["Tief", "im", "letz\u00b7ten", "Keim", "der", "Sch\u00f6p\u00b7fung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird sein Segen bl\u00fchn.", "tokens": ["Wird", "sein", "Se\u00b7gen", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "K\u00f6nig, la\u00df mich Deines Reiches", "tokens": ["K\u00f6\u00b7nig", ",", "la\u00df", "mich", "Dei\u00b7nes", "Rei\u00b7ches"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVIMP", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommen freudig sehn!", "tokens": ["Kom\u00b7men", "freu\u00b7dig", "sehn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "La\u00df mich lebend, la\u00df mich sterbend", "tokens": ["La\u00df", "mich", "le\u00b7bend", ",", "la\u00df", "mich", "ster\u00b7bend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJD", "$,", "VVIMP", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit Dein Reich erh\u00f6hn!", "tokens": ["Mit", "Dein", "Reich", "er\u00b7h\u00f6hn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}