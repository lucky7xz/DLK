{"dta.poem.17854": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "97.  Das ewge hat schl\u00e4chten verdrang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Man sieht fast alle Welt mit Juden spissen lauffen;", "tokens": ["Man", "sieht", "fast", "al\u00b7le", "Welt", "mit", "Ju\u00b7den", "spis\u00b7sen", "lauf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PIAT", "NN", "APPR", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und doch umbs Himmelreich so wenig Leute kauffen!", "tokens": ["Und", "doch", "umbs", "Him\u00b7mel\u00b7reich", "so", "we\u00b7nig", "Leu\u00b7te", "kauf\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}