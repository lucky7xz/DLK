{"dta.poem.1642": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vermischte  \n Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wer herrscht/ lebt immerdar in harten Dienstbarkeiten/", "tokens": ["Wer", "herrscht", "/", "lebt", "im\u00b7mer\u00b7dar", "in", "har\u00b7ten", "Dienst\u00b7bar\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verdacht/ Gefahr und Furcht sind seine Wacht und", "tokens": ["Ver\u00b7dacht", "/", "Ge\u00b7fahr", "und", "Furcht", "sind", "sei\u00b7ne", "Wacht", "und"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "NN", "KON", "NN", "VAFIN", "PPOSAT", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hutt/", "tokens": ["Hutt", "/"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Sein wollen pflegen Ehr und Wohlstand zu bestreiten/", "tokens": ["Sein", "wol\u00b7len", "pfle\u00b7gen", "Ehr", "und", "Wohl\u00b7stand", "zu", "be\u00b7strei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VMFIN", "VVINF", "NN", "KON", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und goldner Ketten Pracht umf\u00e4sselt seinen Mutt/", "tokens": ["Und", "gold\u00b7ner", "Ket\u00b7ten", "Pracht", "um\u00b7f\u00e4s\u00b7selt", "sei\u00b7nen", "Mutt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Schliest ins Gef\u00e4ngnis ein die Freyheit seiner Sinnen;", "tokens": ["Schliest", "ins", "Ge\u00b7f\u00e4ng\u00b7nis", "ein", "die", "Frey\u00b7heit", "sei\u00b7ner", "Sin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Der Richt-Spruch/ den er f\u00e4llt/ f\u00e4llt wieder auff sein Hau\u00df/", "tokens": ["Der", "Richt\u00b7Spruch", "/", "den", "er", "f\u00e4llt", "/", "f\u00e4llt", "wie\u00b7der", "auff", "sein", "Hau\u00df", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "VVFIN", "$(", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Mi\u00dfgunst und der Neyd durchhechelt sein Beginnen/", "tokens": ["Die", "Mi\u00df\u00b7gunst", "und", "der", "Neyd", "durch\u00b7he\u00b7chelt", "sein", "Be\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und stellen seiner Schuld und Unschuld M\u00e4ngel aus.", "tokens": ["Und", "stel\u00b7len", "sei\u00b7ner", "Schuld", "und", "Un\u00b7schuld", "M\u00e4n\u00b7gel", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "KON", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}