{"textgrid.poem.67421": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn von Schardt", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie bald, o Freund, wie bald ist es verschwunden,", "tokens": ["Wie", "bald", ",", "o", "Freund", ",", "wie", "bald", "ist", "es", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "FM", "NN", "$,", "PWAV", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das lange Jahr mit seinen kurzen Stunden!", "tokens": ["Das", "lan\u00b7ge", "Jahr", "mit", "sei\u00b7nen", "kur\u00b7zen", "Stun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was Dir einst Unlust, oft mir Sorge war,", "tokens": ["Was", "Dir", "einst", "Un\u00b7lust", ",", "oft", "mir", "Sor\u00b7ge", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "NN", "$,", "ADV", "PPER", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vor\u00fcber ist's, das kurze, lange Jahr.", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "ist's", ",", "das", "kur\u00b7ze", ",", "lan\u00b7ge", "Jahr", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch Eins, o Freund, Eins sei uns nicht vor\u00fcber:", "tokens": ["Doch", "Eins", ",", "o", "Freund", ",", "Eins", "sei", "uns", "nicht", "vor\u00b7\u00fc\u00b7ber", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "FM", "NN", "$,", "NN", "VAFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vereinte Pflicht macht Menschen Menschen lieber,", "tokens": ["Ver\u00b7ein\u00b7te", "Pflicht", "macht", "Men\u00b7schen", "Men\u00b7schen", "lie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vergangne M\u00fch wird im Andenken s\u00fc\u00df,", "tokens": ["Ver\u00b7gang\u00b7ne", "M\u00fch", "wird", "im", "An\u00b7den\u00b7ken", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Treue wird der Freundschaft Paradies.", "tokens": ["Und", "Treu\u00b7e", "wird", "der", "Freund\u00b7schaft", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dank also Dir f\u00fcr Deine Hilf' und Treue!", "tokens": ["Dank", "al\u00b7so", "Dir", "f\u00fcr", "Dei\u00b7ne", "Hil\u00b7f'", "und", "Treu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPER", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wirf nicht auf sie den Blick zur\u00fcck mit Reue!", "tokens": ["Wirf", "nicht", "auf", "sie", "den", "Blick", "zu\u00b7r\u00fcck", "mit", "Reu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPR", "PPER", "ART", "NN", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du h\u00f6rst nicht mehr der Kasten dumpfen Klang,", "tokens": ["Du", "h\u00f6rst", "nicht", "mehr", "der", "Kas\u00b7ten", "dum\u00b7pfen", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "H\u00f6r also noch mein reines: ", "tokens": ["H\u00f6r", "al\u00b7so", "noch", "mein", "rei\u00b7nes", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Einst sprech' auch ich: \u00bbHier sind die letzten Acten!\u00ab", "tokens": ["Einst", "sprech'", "auch", "ich", ":", "\u00bb", "Hier", "sind", "die", "letz\u00b7ten", "Ac\u00b7ten", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "$.", "$(", "ADV", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind sie mir Ruhmes- oder Leidespacten?", "tokens": ["Sind", "sie", "mir", "Ruh\u00b7mes", "o\u00b7der", "Lei\u00b7de\u00b7spac\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das wei\u00df ich nicht. Eins wei\u00df ich, und gewi\u00df:", "tokens": ["Das", "wei\u00df", "ich", "nicht", ".", "Eins", "wei\u00df", "ich", ",", "und", "ge\u00b7wi\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "$.", "NN", "VVFIN", "PPER", "$,", "KON", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vollbrachte M\u00fch ist Lebens-Paradies.", "tokens": ["Voll\u00b7brach\u00b7te", "M\u00fch", "ist", "Le\u00b7bens\u00b7Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}