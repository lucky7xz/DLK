{"dta.poem.625": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Williams III.   K\u00f6nigs in Gro\u00df-Britannien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Den Frantzen liesse ich Britanjens Waffen f\u00fchlen", "tokens": ["Den", "Frant\u00b7zen", "lies\u00b7se", "ich", "Bri\u00b7tan\u00b7jens", "Waf\u00b7fen", "f\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nachdem mein helden Muth mich auf den Thron gesetzt;", "tokens": ["Nach\u00b7dem", "mein", "hel\u00b7den", "Muth", "mich", "auf", "den", "Thron", "ge\u00b7setzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jacobus wolte zwar mit um die Beute spielen/", "tokens": ["Ja\u00b7co\u00b7bus", "wol\u00b7te", "zwar", "mit", "um", "die", "Beu\u00b7te", "spie\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADV", "APPR", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch wurde er mit nichts/ ich mit der Kron erg\u00f6tzt.", "tokens": ["Doch", "wur\u00b7de", "er", "mit", "nichts", "/", "ich", "mit", "der", "Kron", "er\u00b7g\u00f6tzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punc