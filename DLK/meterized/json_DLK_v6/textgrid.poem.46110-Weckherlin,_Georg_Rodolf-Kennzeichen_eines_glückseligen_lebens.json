{"textgrid.poem.46110": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Kennzeichen eines gl\u00fcckseligen lebens", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, wie gl\u00fcckselig ist das leben,", "tokens": ["Ach", ",", "wie", "gl\u00fcck\u00b7se\u00b7lig", "ist", "das", "le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADJD", "VAFIN", "PDS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "dem keines andern will gebeut,", "tokens": ["dem", "kei\u00b7nes", "an\u00b7dern", "will", "ge\u00b7beut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PIS", "VMFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "der ohn misgunst, neid oder streit", "tokens": ["der", "ohn", "mis\u00b7gunst", ",", "neid", "o\u00b7der", "streit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "$,", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "sicht andrer gl\u00fcck f\u00fcr\u00fcber schweben;", "tokens": ["sicht", "an\u00b7drer", "gl\u00fcck", "f\u00fc\u00b7r\u00fc\u00b7ber", "schwe\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der sein begird selbs recht regieret", "tokens": ["Der", "sein", "be\u00b7gird", "selbs", "recht", "re\u00b7gie\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "VAINF", "VAFIN", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und dessen from und teutscher mut", "tokens": ["und", "des\u00b7sen", "from", "und", "teut\u00b7scher", "mut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "ADJD", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ist sein bewehrter schutz und hut,", "tokens": ["ist", "sein", "be\u00b7wehr\u00b7ter", "schutz", "und", "hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "darunder sein herz triumfieret;", "tokens": ["da\u00b7run\u00b7der", "sein", "herz", "tri\u00b7um\u00b7fie\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Der kein geschrei noch lob begehret,", "tokens": ["Der", "kein", "ge\u00b7schrei", "noch", "lob", "be\u00b7ge\u00b7hret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJD", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "dem die warheit die gr\u00f6ste kunst,", "tokens": ["dem", "die", "war\u00b7heit", "die", "gr\u00f6s\u00b7te", "kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "den f\u00fcrsten oder p\u00f6fels gunst,", "tokens": ["den", "f\u00fcrs\u00b7ten", "o\u00b7der", "p\u00f6\u00b7fels", "gunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "den hofnung und forcht nicht beth\u00f6ret;", "tokens": ["den", "hof\u00b7nung", "und", "forcht", "nicht", "be\u00b7th\u00f6\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "PTKNEG", "VVFIN", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Der die fuchsschw\u00e4nzer fort l\u00e4\u00dft gehen,", "tokens": ["Der", "die", "fuchs\u00b7schw\u00e4n\u00b7zer", "fort", "l\u00e4\u00dft", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PTKVZ", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "sie speisend nicht von seinem gut,", "tokens": ["sie", "spei\u00b7send", "nicht", "von", "sei\u00b7nem", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKNEG", "APPR", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und dessen fehl, fall und armut", "tokens": ["und", "des\u00b7sen", "fehl", ",", "fall", "und", "ar\u00b7mut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PDS", "PTKVZ", "$,", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "kan seine h\u00e4sser nicht erh\u00f6hen;", "tokens": ["kan", "sei\u00b7ne", "h\u00e4s\u00b7ser", "nicht", "er\u00b7h\u00f6\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der selbs nichts wei\u00df, wie \u00fcbel schm\u00fcrzet", "tokens": ["Der", "selbs", "nichts", "wei\u00df", ",", "wie", "\u00fc\u00b7bel", "schm\u00fcr\u00b7zet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "PIS", "VVFIN", "$,", "PWAV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "des b\u00f6sen lob, des frommen fluch;", "tokens": ["des", "b\u00f6\u00b7sen", "lob", ",", "des", "from\u00b7men", "fluch", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "dem ein freind oder gutes buch", "tokens": ["dem", "ein", "freind", "o\u00b7der", "gu\u00b7tes", "buch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJD", "KON", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "die lange zeit schadlos verk\u00fcrzet;", "tokens": ["die", "lan\u00b7ge", "zeit", "schad\u00b7los", "ver\u00b7k\u00fcr\u00b7zet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und dessen mut f\u00fcr nichts sich scheuet,", "tokens": ["Und", "des\u00b7sen", "mut", "f\u00fcr", "nichts", "sich", "scheu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "APPR", "PIS", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "als allzeit fertig f\u00fcr den tod;", "tokens": ["als", "all\u00b7zeit", "fer\u00b7tig", "f\u00fcr", "den", "tod", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "der ernstlich fr\u00fch und spat zu got", "tokens": ["der", "ernst\u00b7lich", "fr\u00fch", "und", "spat", "zu", "got"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJD", "KON", "VVFIN", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "mehr um gnad, dan um g\u00fcter, schreiet.", "tokens": ["mehr", "um", "gnad", ",", "dan", "um", "g\u00fc\u00b7ter", ",", "schrei\u00b7et", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "ADV", "APPR", "NN", "$,", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Der mensch besorgt sich keines falles,", "tokens": ["Der", "mensch", "be\u00b7sorgt", "sich", "kei\u00b7nes", "fal\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "dieweil er frei, reich, gut und gro\u00df,", "tokens": ["die\u00b7weil", "er", "frei", ",", "reich", ",", "gut", "und", "gro\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "sein selbs herr; ob er wol landlos", "tokens": ["sein", "selbs", "herr", ";", "ob", "er", "wol", "land\u00b7los"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "KOUS", "PPER", "ADV", "ADJD"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "und habend nichts, hat er doch alles.", "tokens": ["und", "ha\u00b7bend", "nichts", ",", "hat", "er", "doch", "al\u00b7les", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "$,", "VAFIN", "PPER", "ADV", "PIS", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}