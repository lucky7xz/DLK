{"textgrid.poem.64825": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer nur erst recht vertraut ist mit des Lebens Kerne,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer nur erst recht vertraut ist mit des Lebens Kerne,", "tokens": ["Wer", "nur", "erst", "recht", "ver\u00b7traut", "ist", "mit", "des", "Le\u00b7bens", "Ker\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ADV", "ADJD", "VAFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der gie\u00dft sein Wohlthun aus auf Nahe wie auf Ferne \u2013", "tokens": ["Der", "gie\u00dft", "sein", "Wohl\u00b7thun", "aus", "auf", "Na\u00b7he", "wie", "auf", "Fer\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "APPR", "APPR", "NN", "KOKOM", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Auf jeglich' Menschenbild, gleichviel, ob sich sein Geist", "tokens": ["Auf", "jeg\u00b7lich'", "Men\u00b7schen\u00b7bild", ",", "gleich\u00b7viel", ",", "ob", "sich", "sein", "Geist"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "VVFIN", "$,", "KOUS", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als ebenb\u00fcrtig, ob als unterordnet weis't;", "tokens": ["Als", "e\u00b7ben\u00b7b\u00fcr\u00b7tig", ",", "ob", "als", "un\u00b7ter\u00b7ord\u00b7net", "weis't", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "$,", "KOUS", "KOKOM", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gleichviel, ob er mit ihm durch Lieb' und Treu' verbunden,", "tokens": ["Gleich\u00b7viel", ",", "ob", "er", "mit", "ihm", "durch", "Lieb'", "und", "Treu'", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "APPR", "PPER", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob er mit ihm sich nie verst\u00e4ndigt und gefunden;", "tokens": ["Ob", "er", "mit", "ihm", "sich", "nie", "ver\u00b7st\u00e4n\u00b7digt", "und", "ge\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "PRF", "ADV", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ob d\u00fcrr des Andern Herz, ob es an Bl\u00fcthen reich; \u2013", "tokens": ["Ob", "d\u00fcrr", "des", "An\u00b7dern", "Herz", ",", "ob", "es", "an", "Bl\u00fc\u00b7then", "reich", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "NE", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "APPR", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Milde Segen thaut \u2013 worauf? das gilt ihr gleich.", "tokens": ["Der", "Mil\u00b7de", "Se\u00b7gen", "thaut", "\u2013", "wo\u00b7rauf", "?", "das", "gilt", "ihr", "gleich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$(", "PWAV", "$.", "PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Dem Freunde thut er wohl, weil es ein Selbstbegl\u00fccken,", "tokens": ["Dem", "Freun\u00b7de", "thut", "er", "wohl", ",", "weil", "es", "ein", "Selbst\u00b7be\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Freundes Lebenspfad zu ebnen und zu schm\u00fccken;", "tokens": ["Des", "Freun\u00b7des", "Le\u00b7ben\u00b7spfad", "zu", "eb\u00b7nen", "und", "zu", "schm\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Weil im geliebten Aug' der helle Freudenglanz", "tokens": ["Weil", "im", "ge\u00b7lieb\u00b7ten", "Aug'", "der", "hel\u00b7le", "Freu\u00b7den\u00b7glanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Strahl, vor dem erbl\u00fcht des eignen Lebens Kranz.", "tokens": ["Ein", "Strahl", ",", "vor", "dem", "er\u00b7bl\u00fcht", "des", "eig\u00b7nen", "Le\u00b7bens", "Kranz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Dem Fremden thut er wohl, weil, wenn er hilft und lindert,", "tokens": ["Dem", "Frem\u00b7den", "thut", "er", "wohl", ",", "weil", ",", "wenn", "er", "hilft", "und", "lin\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "KOUS", "$,", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er seine Schuld gen ihn theilweise doch vermindert \u2013", "tokens": ["Er", "sei\u00b7ne", "Schuld", "gen", "ihn", "theil\u00b7wei\u00b7se", "doch", "ver\u00b7min\u00b7dert", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "APPR", "PPER", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Die gro\u00dfe Herzensschuld, die tief ihm ist bewu\u00dft:", "tokens": ["Die", "gro\u00b7\u00dfe", "Her\u00b7zens\u00b7schuld", ",", "die", "tief", "ihm", "ist", "be\u00b7wu\u00dft", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df f\u00fcr den Andern kalt und liebleer seine Brust.", "tokens": ["Da\u00df", "f\u00fcr", "den", "An\u00b7dern", "kalt", "und", "lieb\u00b7leer", "sei\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Durch Wohlthun will er nun sich dieser Schuld entled'gen", "tokens": ["Durch", "Wohl\u00b7thun", "will", "er", "nun", "sich", "die\u00b7ser", "Schuld", "ent\u00b7le\u00b7d'\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "PPER", "ADV", "PRF", "PDAT", "NN", "VVINF"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und f\u00fcr versagte Huld durch huld'ge That entsch\u00e4d'gen.", "tokens": ["Und", "f\u00fcr", "ver\u00b7sag\u00b7te", "Huld", "durch", "huld'\u00b7ge", "That", "ent\u00b7sch\u00e4d'\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Ihm scheint es doppelt Noth und doppelt heil'ge Pflicht,", "tokens": ["Ihm", "scheint", "es", "dop\u00b7pelt", "Noth", "und", "dop\u00b7pelt", "heil'\u00b7ge", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "NN", "KON", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Liebth\u00e4tig da zu sein, wo ihm die Lieb' gebricht.", "tokens": ["Liebt\u00b7h\u00e4\u00b7tig", "da", "zu", "sein", ",", "wo", "ihm", "die", "Lieb'", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PTKZU", "VAINF", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "So gegen Alle gleich, wirst du an seinen Werken,", "tokens": ["So", "ge\u00b7gen", "Al\u00b7le", "gleich", ",", "wirst", "du", "an", "sei\u00b7nen", "Wer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIS", "ADV", "$,", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer seinem Herzen fremd, wer theuer, niemals merken", "tokens": ["Wer", "sei\u00b7nem", "Her\u00b7zen", "fremd", ",", "wer", "theu\u00b7er", ",", "nie\u00b7mals", "mer\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "ADJD", "$,", "PWS", "ADJD", "$,", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}