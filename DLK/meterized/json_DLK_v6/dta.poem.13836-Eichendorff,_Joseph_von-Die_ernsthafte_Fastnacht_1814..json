{"dta.poem.13836": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die ernsthafte Fastnacht  1814.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Drauf sie einen Reiter schnelle", "tokens": ["Drauf", "sie", "ei\u00b7nen", "Rei\u00b7ter", "schnel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Senden, der so fr\u00f6hlich schaut,", "tokens": ["Sen\u00b7den", ",", "der", "so", "fr\u00f6h\u00b7lich", "schaut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der bl\u00e4\u00dft seinen Gru\u00df so helle,", "tokens": ["Der", "bl\u00e4\u00dft", "sei\u00b7nen", "Gru\u00df", "so", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "ADV", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wirbt da um die stolze Braut.", "tokens": ["Wirbt", "da", "um", "die", "stol\u00b7ze", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}