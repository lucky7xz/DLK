{"textgrid.poem.59668": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Himmelfahrt", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie prangt im Fr\u00fchlingskleide", "tokens": ["Wie", "prangt", "im", "Fr\u00fch\u00b7lings\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die gr\u00fcne, bunte Welt!", "tokens": ["Die", "gr\u00fc\u00b7ne", ",", "bun\u00b7te", "Welt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hat in Welt und Heide", "tokens": ["Und", "hat", "in", "Welt", "und", "Hei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "NN", "KON", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Musik und Lust bestellt:", "tokens": ["Mu\u00b7sik", "und", "Lust", "be\u00b7stellt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie klingt und spielt der Scherz", "tokens": ["Wie", "klingt", "und", "spielt", "der", "Scherz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In B\u00fcschen rings und B\u00e4umen", "tokens": ["In", "B\u00fc\u00b7schen", "rings", "und", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Von Edens Blumentr\u00e4umen", "tokens": ["Von", "E\u00b7dens", "Blu\u00b7men\u00b7tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den Klang in jedes Herz!", "tokens": ["Den", "Klang", "in", "je\u00b7des", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hinaus denn, meine Seele!", "tokens": ["Hin\u00b7aus", "denn", ",", "mei\u00b7ne", "See\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In voller Lust hinaus!", "tokens": ["In", "vol\u00b7ler", "Lust", "hin\u00b7aus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verk\u00fcnde, ruf, erz\u00e4hle", "tokens": ["Ver\u00b7k\u00fcn\u00b7de", ",", "ruf", ",", "er\u00b7z\u00e4h\u00b7le"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und kling und sing es aus!", "tokens": ["Und", "kling", "und", "sing", "es", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du bist von Lerchenart,", "tokens": ["Du", "bist", "von", "Ler\u00b7chen\u00b7art", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Nach oben will dein Leben:", "tokens": ["Nach", "o\u00b7ben", "will", "dein", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VMFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "La\u00df fliegen, klingen und schweben", "tokens": ["La\u00df", "flie\u00b7gen", ",", "klin\u00b7gen", "und", "schwe\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "VVINF", "$,", "VVFIN", "KON", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Die s\u00fc\u00dfe Himmelfahrt.", "tokens": ["Die", "s\u00fc\u00b7\u00dfe", "Him\u00b7mel\u00b7fahrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auf! L\u00fcfte deine Schwingen", "tokens": ["Auf", "!", "L\u00fcf\u00b7te", "dei\u00b7ne", "Schwin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["APPR", "$.", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum frohen Heimatort!", "tokens": ["Zum", "fro\u00b7hen", "Hei\u00b7ma\u00b7tort", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein Trachten, Sehnen, Ringen,", "tokens": ["Dein", "Trach\u00b7ten", ",", "Seh\u00b7nen", ",", "Rin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein Weg, dein Lauf ist dort \u2013", "tokens": ["Dein", "Weg", ",", "dein", "Lauf", "ist", "dort", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O flieg aus diesem Glanz", "tokens": ["O", "flieg", "aus", "die\u00b7sem", "Glanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der bunten Erdenlenze", "tokens": ["Der", "bun\u00b7ten", "Er\u00b7den\u00b7len\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ins Land der ew'gen Kr\u00e4nze!", "tokens": ["Ins", "Land", "der", "ew'\u00b7gen", "Kr\u00e4n\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dort ist dein Ziel, dein Kranz.", "tokens": ["Dort", "ist", "dein", "Ziel", ",", "dein", "Kranz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}