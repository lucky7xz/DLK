{"dta.poem.3380": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "An Ethelinde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wallt der Trennung Nebel endlich nieder?", "tokens": ["Wallt", "der", "Tren\u00b7nung", "Ne\u00b7bel", "end\u00b7lich", "nie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Strahlt des Wiedersehens goldner Tag?", "tokens": ["Strahlt", "des", "Wie\u00b7der\u00b7se\u00b7hens", "gold\u00b7ner", "Tag", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dich, du Langentbehrte, find' ich wieder;", "tokens": ["Dich", ",", "du", "Lan\u00b7gent\u00b7behr\u00b7te", ",", "find'", "ich", "wie\u00b7der", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dich, du Langgemisste \u2014 aber ach!", "tokens": ["Dich", ",", "du", "Lang\u00b7ge\u00b7miss\u00b7te", "a\u00b7ber", "ach", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "NN", "$(", "ADV", "ADV", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Ach, von deines Alfred Arm umschlungen,", "tokens": ["Ach", ",", "von", "dei\u00b7nes", "Al\u00b7fred", "Arm", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "PPOSAT", "NE", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Von des treuen Gatten Kuss durchdrungen,", "tokens": ["Von", "des", "treu\u00b7en", "Gat\u00b7ten", "Kuss", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Angesonnt von seines Auges Licht,", "tokens": ["An\u00b7ge\u00b7sonnt", "von", "sei\u00b7nes", "Au\u00b7ges", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Find' ich dich, du Fr\u00fchverlassne, nicht.", "tokens": ["Find'", "ich", "dich", ",", "du", "Fr\u00fch\u00b7ver\u00b7lass\u00b7ne", ",", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "PPER", "NN", "$,", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Heimgewallt zu jenen sch\u00f6nern Fluren,", "tokens": ["Heim\u00b7ge\u00b7wallt", "zu", "je\u00b7nen", "sch\u00f6\u00b7nern", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo ein ewiggr\u00fcner Fr\u00fchling bl\u00fcht;", "tokens": ["Wo", "ein", "e\u00b7wig\u00b7gr\u00fc\u00b7ner", "Fr\u00fch\u00b7ling", "bl\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo das Mark erhabnerer Naturen", "tokens": ["Wo", "das", "Mark", "er\u00b7hab\u00b7ne\u00b7rer", "Na\u00b7tu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ewge Kraft und ewge Gluth durchspr\u00fcht,", "tokens": ["Ew\u00b7ge", "Kraft", "und", "ew\u00b7ge", "Gluth", "durch\u00b7spr\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wo kein Siechender nach Heilung \u00e4chzet,", "tokens": ["Wo", "kein", "Sie\u00b7chen\u00b7der", "nach", "Hei\u00b7lung", "\u00e4ch\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wo kein Traurender nach Tr\u00f6stung lechzet,", "tokens": ["Wo", "kein", "Trau\u00b7ren\u00b7der", "nach", "Tr\u00f6s\u00b7tung", "lech\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Wo kein Gram die schweren Wimper n\u00e4sst,", "tokens": ["Wo", "kein", "Gram", "die", "schwe\u00b7ren", "Wim\u00b7per", "n\u00e4sst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Und kein Schmerz den Busen engt und presst;", "tokens": ["Und", "kein", "Schmerz", "den", "Bu\u00b7sen", "engt", "und", "presst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Wo kein Blendwerk unsre Augen blendet,", "tokens": ["Wo", "kein", "Blend\u00b7werk", "uns\u00b7re", "Au\u00b7gen", "blen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo kein Nebel unsern Blick umflirrt,", "tokens": ["Wo", "kein", "Ne\u00b7bel", "un\u00b7sern", "Blick", "um\u00b7flirrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo kein Jubel sich in Jammer endet,", "tokens": ["Wo", "kein", "Ju\u00b7bel", "sich", "in", "Jam\u00b7mer", "en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und den Wanderer kein Irrlicht irrt \u2014", "tokens": ["Und", "den", "Wan\u00b7de\u00b7rer", "kein", "Irr\u00b7licht", "irrt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PIAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Heimgewallt zu Edens selgen Matten", "tokens": ["Heim\u00b7ge\u00b7wallt", "zu", "E\u00b7dens", "sel\u00b7gen", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NE", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ist der Schatten deines trauten Gatten;", "tokens": ["Ist", "der", "Schat\u00b7ten", "dei\u00b7nes", "trau\u00b7ten", "Gat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "In der \u00dcberwinder helle Reihn", "tokens": ["In", "der", "\u00dc\u00b7berw\u00b7in\u00b7der", "hel\u00b7le", "Reihn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Trat der vielgepr\u00fcfte Dulder ein.", "tokens": ["Trat", "der", "viel\u00b7ge\u00b7pr\u00fcf\u00b7te", "Dul\u00b7der", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Schau, wie rauschet der Vollendung Palme", "tokens": ["Schau", ",", "wie", "rau\u00b7schet", "der", "Vol\u00b7len\u00b7dung", "Pal\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "VVFIN", "ART", "NN", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "In des Sch\u00f6nverkl\u00e4rten Strahlenhand!", "tokens": ["In", "des", "Sch\u00f6n\u00b7ver\u00b7kl\u00e4r\u00b7ten", "Strah\u00b7len\u00b7hand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Horch, wie jubeln des Triumphes Psalme,", "tokens": ["Horch", ",", "wie", "ju\u00b7beln", "des", "Tri\u00b7um\u00b7phes", "Psal\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "H\u00f6hnen alles Eitle, allen Tand!", "tokens": ["H\u00f6h\u00b7nen", "al\u00b7les", "Eit\u00b7le", ",", "al\u00b7len", "Tand", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Selig ist, den deine Seele liebte!", "tokens": ["Se\u00b7lig", "ist", ",", "den", "dei\u00b7ne", "See\u00b7le", "lieb\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Darum tr\u00f6ste dich, du Hochbetr\u00fcbte!", "tokens": ["Da\u00b7rum", "tr\u00f6s\u00b7te", "dich", ",", "du", "Hoch\u00b7be\u00b7tr\u00fcb\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "PPER", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "St\u00f6re nicht des Himmelb\u00fcrgers Gl\u00fcck \u2014", "tokens": ["St\u00f6\u00b7re", "nicht", "des", "Him\u00b7mel\u00b7b\u00fcr\u00b7gers", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Wende weg den grambew\u00f6lkten Blick;", "tokens": ["Wen\u00b7de", "weg", "den", "gram\u00b7be\u00b7w\u00f6lk\u00b7ten", "Blick", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Wend' ihn weg von jenen Regionen,", "tokens": ["Wend'", "ihn", "weg", "von", "je\u00b7nen", "Re\u00b7gi\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo der Vorwurf deiner Sehnsucht wallt,", "tokens": ["Wo", "der", "Vor\u00b7wurf", "dei\u00b7ner", "Sehn\u00b7sucht", "wallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Weg von jenen sonnenhellen Zonen,", "tokens": ["Weg", "von", "je\u00b7nen", "son\u00b7nen\u00b7hel\u00b7len", "Zo\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wo das englische Hosannah schallt \u2014", "tokens": ["Wo", "das", "eng\u00b7li\u00b7sche", "Ho\u00b7san\u00b7nah", "schallt"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Weg und hin zu den geliebten Deinen,", "tokens": ["Weg", "und", "hin", "zu", "den", "ge\u00b7lieb\u00b7ten", "Dei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Die f\u00fcr dich zum Himmel flehend weinen.", "tokens": ["Die", "f\u00fcr", "dich", "zum", "Him\u00b7mel", "fle\u00b7hend", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "APPRART", "NN", "VVPP", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Deine Julie flehet himmelan,", "tokens": ["Dei\u00b7ne", "Ju\u00b7lie", "fle\u00b7het", "him\u00b7me\u00b7lan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Himmelan dein Karl und dein Johann.", "tokens": ["Him\u00b7me\u00b7lan", "dein", "Karl", "und", "dein", "Jo\u00b7hann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NE", "KON", "PPOSAT", "NE", "$."], "meter": 