{"textgrid.poem.63168": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Der Totengr\u00e4ber", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich rede frisch von der Leber", "tokens": ["Ich", "re\u00b7de", "frisch", "von", "der", "Le\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Weg, zum Parlieren", "tokens": ["Weg", ",", "zum", "Par\u00b7lie\u00b7ren"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "APPRART", "NN"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Und Zieren", "tokens": ["Und", "Zie\u00b7ren"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Ist keine Zeit.", "tokens": ["Ist", "kei\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ein armer, wandernder, stellenloser Totengr\u00e4ber", "tokens": ["Ein", "ar\u00b7mer", ",", "wan\u00b7dern\u00b7der", ",", "stel\u00b7len\u00b7lo\u00b7ser", "To\u00b7ten\u00b7gr\u00e4\u00b7ber"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "PWAV", "$,", "ADJA", "NN"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Bittet um Arbeit.", "tokens": ["Bit\u00b7tet", "um", "Ar\u00b7beit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Habt ihr keinen Toten zu begraben?", "tokens": ["Habt", "ihr", "kei\u00b7nen", "To\u00b7ten", "zu", "be\u00b7gra\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Keine Leiche im Haus?", "tokens": ["Kei\u00b7ne", "Lei\u00b7che", "im", "Haus", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Ei der Daus!", "tokens": ["Ei", "der", "Daus", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Keine Mutter? Keine Tochter? Keinen Mann?", "tokens": ["Kei\u00b7ne", "Mut\u00b7ter", "?", "Kei\u00b7ne", "Toch\u00b7ter", "?", "Kei\u00b7nen", "Mann", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "PIAT", "NN", "$.", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.11": {"text": "Ich begrabe sie, so gut ichs kann.", "tokens": ["Ich", "be\u00b7gra\u00b7be", "sie", ",", "so", "gut", "ichs", "kann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADV", "ADJD", "PIS", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Bei mir ist jeder gut aufgehoben,", "tokens": ["Bei", "mir", "ist", "je\u00b7der", "gut", "auf\u00b7ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIS", "ADJD", "VVPP", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.13": {"text": "Das Werk wird seinen Sch\u00f6pfer loben.", "tokens": ["Das", "Werk", "wird", "sei\u00b7nen", "Sch\u00f6p\u00b7fer", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Ich trage die Schaufel stets bei mir", "tokens": ["Ich", "tra\u00b7ge", "die", "Schau\u00b7fel", "stets", "bei", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "PPER"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Und begrabe Sie auf Wunsch im Garten hier.", "tokens": ["Und", "be\u00b7gra\u00b7be", "Sie", "auf", "Wunsch", "im", "Gar\u00b7ten", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "APPRART", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.16": {"text": "Die Erde leicht und lau f\u00e4llt", "tokens": ["Die", "Er\u00b7de", "leicht", "und", "lau", "f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Auf Ihre Rippen", "tokens": ["Auf", "Ih\u00b7re", "Rip\u00b7pen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Wie Schnee.", "tokens": ["Wie", "Schnee", "."], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "Ein Grab ist schnell geschaufelt.", "tokens": ["Ein", "Grab", "ist", "schnell", "ge\u00b7schau\u00b7felt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Die Lippen", "tokens": ["Die", "Lip\u00b7pen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "L\u00e4cheln: Ade!", "tokens": ["L\u00e4\u00b7cheln", ":", "A\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Ich wandre immer hin und her,", "tokens": ["Ich", "wand\u00b7re", "im\u00b7mer", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ob ich nicht Arbeit f\u00e4nde.", "tokens": ["Ob", "ich", "nicht", "Ar\u00b7beit", "f\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein Herz ist leer, mein Beutel ist leer,", "tokens": ["Mein", "Herz", "ist", "leer", ",", "mein", "Beu\u00b7tel", "ist", "leer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und leer sind meine H\u00e4nde.", "tokens": ["Und", "leer", "sind", "mei\u00b7ne", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Denn wer mich sieht, der schl\u00e4gt von fern", "tokens": ["Denn", "wer", "mich", "sieht", ",", "der", "schl\u00e4gt", "von", "fern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "APPR", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um mich den Hasenhaken.", "tokens": ["Um", "mich", "den", "Ha\u00b7sen\u00b7ha\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die M\u00e4dchen schlafen und die Herrn", "tokens": ["Die", "M\u00e4d\u00b7chen", "schla\u00b7fen", "und", "die", "Herrn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht gern im Leichenlaken.", "tokens": ["Nicht", "gern", "im", "Lei\u00b7chen\u00b7la\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich bin ein verlorner Sohn. Ich frass die Treber", "tokens": ["Ich", "bin", "ein", "ver\u00b7lor\u00b7ner", "Sohn", ".", "Ich", "frass", "die", "Tre\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "+---+-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Der Fremde allzu lange Zeit.", "tokens": ["Der", "Frem\u00b7de", "all\u00b7zu", "lan\u00b7ge", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein armer, wandernder, stellenloser Totengr\u00e4ber", "tokens": ["Ein", "ar\u00b7mer", ",", "wan\u00b7dern\u00b7der", ",", "stel\u00b7len\u00b7lo\u00b7ser", "To\u00b7ten\u00b7gr\u00e4\u00b7ber"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "PWAV", "$,", "ADJA", "NN"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Bittet um Arbeit.", "tokens": ["Bit\u00b7tet", "um", "Ar\u00b7beit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}