, "meter": "-+-+-+", "measure": "iambic.tri"}, "line.96": {"text": "Die Eine nicht, so gro\u00df sie sey,", "tokens": ["Die", "Ei\u00b7ne", "nicht", ",", "so", "gro\u00df", "sie", "sey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "PTKNEG", "$,", "ADV", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.97": {"text": "Zerst\u00f6ren; ihr gewinnt", "tokens": ["Zer\u00b7st\u00f6\u00b7ren", ";", "ihr", "ge\u00b7winnt"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.98": {"text": "Inde\u00df, so wie Algier, dabei,", "tokens": ["In\u00b7de\u00df", ",", "so", "wie", "Al\u00b7gier", ",", "da\u00b7bei", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADV", "KOKOM", "NE", "$,", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.99": {"text": "Wenn von zehn Staaten, selten zwei", "tokens": ["Wenn", "von", "zehn", "Staa\u00b7ten", ",", "sel\u00b7ten", "zwei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "APPR", "CARD", "NN", "$,", "ADJD", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.100": {"text": "In Deutschland einig sind.", "tokens": ["In", "Deutschland", "ei\u00b7nig", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJD", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.101": {"text": "So ist's ein wahrer Markebrief,", "tokens": ["So", "ist's", "ein", "wah\u00b7rer", "Mar\u00b7ke\u00b7brief", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.102": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.103": {"text": "Weg ist, was aus dem Hafen lief,", "tokens": ["Weg", "ist", ",", "was", "aus", "dem", "Ha\u00b7fen", "lief", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.104": {"text": "Wenn sie die Ladung r\u00fchrt.", "tokens": ["Wenn", "sie", "die", "La\u00b7dung", "r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.105": {"text": "Mein kleines Schiff geh\u00f6ret zwar", "tokens": ["Mein", "klei\u00b7nes", "Schiff", "ge\u00b7h\u00f6\u00b7ret", "zwar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.106": {"text": "Zur Silberflotte nicht,", "tokens": ["Zur", "Sil\u00b7ber\u00b7flot\u00b7te", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.107": {"text": "Und dennoch l\u00e4\u00dft es der Corsar", "tokens": ["Und", "den\u00b7noch", "l\u00e4\u00dft", "es", "der", "Cor\u00b7sar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.108": {"text": "Nie ganz aus dem Gesicht'.", "tokens": ["Nie", "ganz", "aus", "dem", "Ge\u00b7sicht'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.109": {"text": "So kommt denn her und greift mich an!", "tokens": ["So", "kommt", "denn", "her", "und", "greift", "mich", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.110": {"text": "Ich halte sicher Stand.", "tokens": ["Ich", "hal\u00b7te", "si\u00b7cher", "Stand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.111": {"text": "Mein Brander, wenn ich sonst nichts kann,", "tokens": ["Mein", "Bran\u00b7der", ",", "wenn", "ich", "sonst", "nichts", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "ADV", "PIS", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.112": {"text": "Steckt wenigstens von allen dann", "tokens": ["Steckt", "we\u00b7nigs\u00b7tens", "von", "al\u00b7len", "dann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PIAT", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.113": {"text": "Ein Raubschiff erst in Brand.", "tokens": ["Ein", "Raub\u00b7schiff", "erst", "in", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.114": {"text": "Wenn's einem Theil' an Muth gebricht,", "tokens": ["Wenn's", "ei\u00b7nem", "Theil'", "an", "Muth", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.115": {"text": "Wird Sieg ein leichter Kauf;", "tokens": ["Wird", "Sieg", "ein", "leich\u00b7ter", "Kauf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.116": {"text": "Ich aber streiche sicher nicht,", "tokens": ["Ich", "a\u00b7ber", "strei\u00b7che", "si\u00b7cher", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.117": {"text": "Und fl\u00f6g' ich selbst mit auf.", "tokens": ["Und", "fl\u00f6g'", "ich", "selbst", "mit", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.118": {"text": "Allein genug Allegorie!", "tokens": ["Al\u00b7lein", "ge\u00b7nug", "Al\u00b7le\u00b7go\u00b7rie", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.119": {"text": "Was n\u00fctzt es, euch das Bild", "tokens": ["Was", "n\u00fctzt", "es", ",", "euch", "das", "Bild"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.120": {"text": "Der Wahrheit zeigen? Braucht ihr sie", "tokens": ["Der", "Wahr\u00b7heit", "zei\u00b7gen", "?", "Braucht", "ihr", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$.", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.121": {"text": "Doch selbst, \u2013 wie frech! \u2013 als Schild", "tokens": ["Doch", "selbst", ",", "\u2013", "wie", "frech", "!", "\u2013", "als", "Schild"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word"], "pos": ["KON", "ADV", "$,", "$(", "PWAV", "ADJD", "$.", "$(", "KOUS", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.122": {"text": "Der Schweden Banconote f\u00fchrt", "tokens": ["Der", "Schwe\u00b7den", "Ban\u00b7co\u00b7no\u00b7te", "f\u00fchrt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NE", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.123": {"text": "Die Warnung f\u00fcr den Hang,", "tokens": ["Die", "War\u00b7nung", "f\u00fcr", "den", "Hang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.124": {"text": "Der einen ", "tokens": ["Der", "ei\u00b7nen"], "token_info": ["word", "word"], "pos": ["ART", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.125": {"text": "Zum Gl\u00fcck' f\u00fcr euch, geht's zwar nicht an,", "tokens": ["Zum", "Gl\u00fcck", "f\u00fcr", "euch", ",", "geht's", "zwar", "nicht", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPER", "$,", "VVFIN", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.126": {"text": "Da\u00df ich den Ersten gleich,", "tokens": ["Da\u00df", "ich", "den", "Ers\u00b7ten", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.127": {"text": "Der wieder zum ", "tokens": ["Der", "wie\u00b7der", "zum"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADV", "APPRART"], "meter": "-+-+", "measure": "iambic.di"}, "line.128": {"text": "An mir wird, hangen lassen kann,", "tokens": ["An", "mir", "wird", ",", "han\u00b7gen", "las\u00b7sen", "kann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "$,", "VVINF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.129": {"text": "Doch zwingen kann ich euch,", "tokens": ["Doch", "zwin\u00b7gen", "kann", "ich", "euch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.130": {"text": "Da\u00df, wie ein Goldst\u00fcck ohne Rand,", "tokens": ["Da\u00df", ",", "wie", "ein", "Gold\u00b7st\u00fcck", "oh\u00b7ne", "Rand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.131": {"text": "Ihr, halb verst\u00fcmmelt, diesen Band", "tokens": ["Ihr", ",", "halb", "ver\u00b7st\u00fcm\u00b7melt", ",", "die\u00b7sen", "Band"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "ADJD", "VVPP", "$,", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.132": {"text": "Umher am Tr\u00f6del treibt;", "tokens": ["Um\u00b7her", "am", "Tr\u00f6\u00b7del", "treibt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.133": {"text": "Wo nicht: da\u00df ihr mit eigner Hand", "tokens": ["Wo", "nicht", ":", "da\u00df", "ihr", "mit", "eig\u00b7ner", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PTKNEG", "$.", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.134": {"text": "Euch euer Urtheil schreibt;", "tokens": ["Euch", "eu\u00b7er", "Ur\u00b7theil", "schreibt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.135": {"text": "Mit eigner Hand den Pranger setzt,", "tokens": ["Mit", "eig\u00b7ner", "Hand", "den", "Pran\u00b7ger", "setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.136": {"text": "Woran der erste Dieb, zersetzt", "tokens": ["Wo\u00b7ran", "der", "ers\u00b7te", "Dieb", ",", "zer\u00b7setzt"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.137": {"text": "Von Gei\u00dfeln, sterben soll;", "tokens": ["Von", "Gei\u00b7\u00dfeln", ",", "ster\u00b7ben", "soll", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.138": {"text": "Denn eure Freiheitsbriefe sind", "tokens": ["Denn", "eu\u00b7re", "Frei\u00b7heits\u00b7brie\u00b7fe", "sind"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.139": {"text": "Gerechter Rache, Spreu im Wind',", "tokens": ["Ge\u00b7rech\u00b7ter", "Ra\u00b7che", ",", "Spreu", "im", "Wind'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.140": {"text": "Und euer Ma\u00df ist voll!", "tokens": ["Und", "eu\u00b7er", "Ma\u00df", "ist", "voll", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}