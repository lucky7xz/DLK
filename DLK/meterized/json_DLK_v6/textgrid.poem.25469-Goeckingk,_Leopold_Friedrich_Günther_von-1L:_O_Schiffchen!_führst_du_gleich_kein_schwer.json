{"textgrid.poem.25469": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Schiffchen! f\u00fchrst du gleich kein schwer", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Schiffchen! f\u00fchrst du gleich kein schwer", "tokens": ["O", "Schiff\u00b7chen", "!", "f\u00fchrst", "du", "gleich", "kein", "schwer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "VVFIN", "PPER", "ADV", "PIAT", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gesch\u00fctz, an deinem Bord',", "tokens": ["Ge\u00b7sch\u00fctz", ",", "an", "dei\u00b7nem", "Bord'", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So setz' auf R\u00e4ubervollem Meer'", "tokens": ["So", "setz'", "auf", "R\u00e4u\u00b7ber\u00b7vol\u00b7lem", "Meer'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Fahrt doch ruhig fort.", "tokens": ["Die", "Fahrt", "doch", "ru\u00b7hig", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer oft, wie ich, schon Feinde schlug,", "tokens": ["Wer", "oft", ",", "wie", "ich", ",", "schon", "Fein\u00b7de", "schlug", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$,", "PWAV", "PPER", "$,", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem wird's zuletzt ein Tanz;", "tokens": ["Dem", "wird's", "zu\u00b7letzt", "ein", "Tanz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Drum segl' ich fort im vollen Flug',", "tokens": ["Drum", "segl'", "ich", "fort", "im", "vol\u00b7len", "Flug'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKVZ", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mir selbst Assecuranz genug,", "tokens": ["Mir", "selbst", "As\u00b7se\u00b7cu\u00b7ranz", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "ADV", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Ohn' all' Assecuranz.", "tokens": ["Ohn'", "all'", "As\u00b7se\u00b7cu\u00b7ranz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Ich lag noch still im Hafen hier,", "tokens": ["Ich", "lag", "noch", "still", "im", "Ha\u00b7fen", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Da forschte schon die Raubbegier:", "tokens": ["Da", "forschte", "schon", "die", "Raub\u00b7be\u00b7gier", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Wann? und wohin mein Lauf", "tokens": ["Wann", "?", "und", "wo\u00b7hin", "mein", "Lauf"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "KON", "PWAV", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Gerichtet sey? da pa\u00dften mir", "tokens": ["Ge\u00b7rich\u00b7tet", "sey", "?", "da", "pa\u00df\u00b7ten", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "$.", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Schon die Piraten auf.", "tokens": ["Schon", "die", "Pi\u00b7ra\u00b7ten", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Im Angesicht' des Hafens, macht", "tokens": ["Im", "An\u00b7ge\u00b7sicht'", "des", "Ha\u00b7fens", ",", "macht"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Das Raubgesindel auf mich Jagd,", "tokens": ["Das", "Raub\u00b7ge\u00b7sin\u00b7del", "auf", "mich", "Jagd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und folgt mir Strich vor Strich.", "tokens": ["Und", "folgt", "mir", "Strich", "vor", "Strich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Heran, Gesindel, hast du Muth!", "tokens": ["He\u00b7ran", ",", "Ge\u00b7sin\u00b7del", ",", "hast", "du", "Muth", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "NN", "$,", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Heran und entre! Ohne Blut", "tokens": ["He\u00b7ran", "und", "ent\u00b7re", "!", "Oh\u00b7ne", "Blut"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "KON", "VVFIN", "$.", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Nimmt freilich keiner mich.", "tokens": ["Nimmt", "frei\u00b7lich", "kei\u00b7ner", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Was soll das, Schurken! Warum steckt", "tokens": ["Was", "soll", "das", ",", "Schur\u00b7ken", "!", "Wa\u00b7rum", "steckt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWS", "VMFIN", "PDS", "$,", "NN", "$.", "PWAV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Ihr fremde Flaggen auf,", "tokens": ["Ihr", "frem\u00b7de", "Flag\u00b7gen", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Und haltet Euch am Bord' versteckt?", "tokens": ["Und", "hal\u00b7tet", "Euch", "am", "Bord'", "ver\u00b7steckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Wer achtet wohl darauf?", "tokens": ["Wer", "ach\u00b7tet", "wohl", "da\u00b7rauf", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Schw\u00e4rzt das Gesicht Euch wie ein Mohr,", "tokens": ["Schw\u00e4rzt", "das", "Ge\u00b7sicht", "Euch", "wie", "ein", "Mohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.26": {"text": "Nehmt noch so sch\u00f6ne Larven vor;", "tokens": ["Nehmt", "noch", "so", "sch\u00f6\u00b7ne", "Lar\u00b7ven", "vor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Man kennt Euch wohl, ihr Herrn!", "tokens": ["Man", "kennt", "Euch", "wohl", ",", "ihr", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": "Kauffahrer w\u00e4ret ihr? O Brut", "tokens": ["Kauf\u00b7fah\u00b7rer", "w\u00e4\u00b7ret", "ihr", "?", "O", "Brut"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$.", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Von ", "tokens": ["Von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.30": {"text": "Verr\u00e4th dich schon von fern.", "tokens": ["Ver\u00b7r\u00e4\u00b7th", "dich", "schon", "von", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.31": {"text": "Herab denn mit der Flagg', herab!", "tokens": ["Her\u00b7ab", "denn", "mit", "der", "Flag\u00b7g'", ",", "her\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.32": {"text": "Nehmt Eure plumpen Larven ab,", "tokens": ["Nehmt", "Eu\u00b7re", "plum\u00b7pen", "Lar\u00b7ven", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.34": {"text": "Und raubt, wie ", "tokens": ["Und", "raubt", ",", "wie"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.35": {"text": "Von ", "tokens": ["Von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.36": {"text": "Denn wer thut Widerstand?", "tokens": ["Denn", "wer", "thut", "Wi\u00b7der\u00b7stand", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "Des Eigners Schrein, wenn \u00fcber Bord", "tokens": ["Des", "Eig\u00b7ners", "Schrein", ",", "wenn", "\u00fc\u00b7ber", "Bord"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Ihr nach der Beute springt,", "tokens": ["Ihr", "nach", "der", "Beu\u00b7te", "springt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.39": {"text": "Ist alles; doch ihr segelt fort", "tokens": ["Ist", "al\u00b7les", ";", "doch", "ihr", "se\u00b7gelt", "fort"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "$.", "KON", "PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Mit Eurer Pris', und singt", "tokens": ["Mit", "Eu\u00b7rer", "Pris'", ",", "und", "singt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.41": {"text": "Und lacht, da\u00df ihr sie auf im Port',", "tokens": ["Und", "lacht", ",", "da\u00df", "ihr", "sie", "auf", "im", "Port'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PPER", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Und schnell an K\u00e4ufer bringt.", "tokens": ["Und", "schnell", "an", "K\u00e4u\u00b7fer", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.43": {"text": "Ein Schiff, das Sachsens Flagge f\u00fchrt,", "tokens": ["Ein", "Schiff", ",", "das", "Sach\u00b7sens", "Flag\u00b7ge", "f\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Bringt ihr zu ", "tokens": ["Bringt", "ihr", "zu"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKZU"], "meter": "+-+", "measure": "trochaic.di"}, "line.45": {"text": "Ist's umgekehrt: ei nun! was schiert", "tokens": ["Ist's", "um\u00b7ge\u00b7kehrt", ":", "ei", "nun", "!", "was", "schiert"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVPP", "$.", "NE", "ADV", "$.", "PWS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Euch das? Auf anderm Striche, f\u00fchrt", "tokens": ["Euch", "das", "?", "Auf", "an\u00b7derm", "Stri\u00b7che", ",", "f\u00fchrt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "PDS", "$.", "APPR", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Nach ", "tokens": ["Nach"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.48": {"text": "Sperrt man Euch beide Hafen gleich,", "tokens": ["Sperrt", "man", "Euch", "bei\u00b7de", "Ha\u00b7fen", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "PIAT", "NN", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.49": {"text": "Mit Eurem Raube nehmen Euch", "tokens": ["Mit", "Eu\u00b7rem", "Rau\u00b7be", "neh\u00b7men", "Euch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Noch zwanzig ", "tokens": ["Noch", "zwan\u00b7zig"], "token_info": ["word", "word"], "pos": ["ADV", "CARD"], "meter": "-+-", "measure": "amphibrach.single"}, "line.51": {"text": "Was ist es mehr? Ihr schiffet ", "tokens": ["Was", "ist", "es", "mehr", "?", "Ihr", "schif\u00b7fet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.52": {"text": "Die Waaren aus, statt ", "tokens": ["Die", "Waa\u00b7ren", "aus", ",", "statt"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "KOUI"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.53": {"text": "Wo nicht; so schickt von ", "tokens": ["Wo", "nicht", ";", "so", "schickt", "von"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PTKNEG", "$.", "ADV", "VVFIN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.54": {"text": "Sie einzeln, sicher, fort.", "tokens": ["Sie", "ein\u00b7zeln", ",", "si\u00b7cher", ",", "fort", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "ADJD", "$,", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.55": {"text": "Die M\u00e4kler, wenn sie gleich wie toll", "tokens": ["Die", "M\u00e4k\u00b7ler", ",", "wenn", "sie", "gleich", "wie", "toll"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ADV", "KOKOM", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.56": {"text": "Als R\u00e4uber Euch verschrein,", "tokens": ["Als", "R\u00e4u\u00b7ber", "Euch", "ver\u00b7schrein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.57": {"text": "Ersparen dennoch gerne Zoll,", "tokens": ["Er\u00b7spa\u00b7ren", "den\u00b7noch", "ger\u00b7ne", "Zoll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.58": {"text": "Und schleppen heimlich Frachten voll", "tokens": ["Und", "schlep\u00b7pen", "heim\u00b7lich", "Frach\u00b7ten", "voll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.59": {"text": "Von Eurem Raub' sich ein,", "tokens": ["Von", "Eu\u00b7rem", "Raub'", "sich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.60": {"text": "Denn ohne Hehler w\u00fcrden wohl", "tokens": ["Denn", "oh\u00b7ne", "Heh\u00b7ler", "w\u00fcr\u00b7den", "wohl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.61": {"text": "Der Stehler wenig seyn", "tokens": ["Der", "Steh\u00b7ler", "we\u00b7nig", "seyn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PIS", "VAINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.62": {"text": "Fahrt ihr \u2013 und was macht schneller reich? \u2013", "tokens": ["Fahrt", "ihr", "\u2013", "u