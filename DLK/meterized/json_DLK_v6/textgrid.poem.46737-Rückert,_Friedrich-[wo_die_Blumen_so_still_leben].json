{"textgrid.poem.46737": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wo die Blumen so still leben]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo die Blumen so still leben,", "tokens": ["Wo", "die", "Blu\u00b7men", "so", "still", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.2": {"text": "Leb' ich eben so still, eben", "tokens": ["Leb'", "ich", "e\u00b7ben", "so", "still", ",", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "ADJD", "$,", "ADV"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "So zufrieden wie sie.", "tokens": ["So", "zu\u00b7frie\u00b7den", "wie", "sie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "$."], "meter": "+-+-++", "measure": "unknown.measure.tetra"}, "line.4": {"text": "St\u00f6r' in unserem Stilleben", "tokens": ["St\u00f6r'", "in", "un\u00b7se\u00b7rem", "Stil\u00b7le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "NN"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Spur von Menschen uns nie!", "tokens": ["Spur", "von", "Men\u00b7schen", "uns", "nie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPER", "ADV", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Wie sie bl\u00fchenden Schein bilden", "tokens": ["Wie", "sie", "bl\u00fc\u00b7hen\u00b7den", "Schein", "bil\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJA", "NN", "VVINF"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Um mich, kann ich mir einbilden,", "tokens": ["Um", "mich", ",", "kann", "ich", "mir", "ein\u00b7bil\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "$,", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df nichts sei als ein Schein,", "tokens": ["Da\u00df", "nichts", "sei", "als", "ein", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VAFIN", "KOKOM", "ART", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Dr\u00e4nge zwischen den Scheinbilden", "tokens": ["Dr\u00e4n\u00b7ge", "zwi\u00b7schen", "den", "Schein\u00b7bil\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Sich nichts Wirkliches ein!", "tokens": ["Sich", "nichts", "Wirk\u00b7li\u00b7ches", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PIS", "ADJA", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Wo die Ranken so dicht schatten,", "tokens": ["Wo", "die", "Ran\u00b7ken", "so", "dicht", "schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "--+--++-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Kommen zu mir die Lichtschatten", "tokens": ["Kom\u00b7men", "zu", "mir", "die", "Licht\u00b7schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "ART", "NN"], "meter": "+--+-++-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Meiner Todten geschwebt;", "tokens": ["Mei\u00b7ner", "Tod\u00b7ten", "ge\u00b7schwebt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Sie sind lebend, sind nicht Schatten,", "tokens": ["Sie", "sind", "le\u00b7bend", ",", "sind", "nicht", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "VAFIN", "PTKNEG", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wo nichts anderes lebt.", "tokens": ["Wo", "nichts", "an\u00b7de\u00b7res", "lebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PIS", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}