{"textgrid.poem.59020": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Kranker mu\u00df dem Arzte ruhn:", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Kranker mu\u00df dem Arzte ruhn:", "tokens": ["Ein", "Kran\u00b7ker", "mu\u00df", "dem", "Arz\u00b7te", "ruhn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Komm her, la\u00df dich verbinden!", "tokens": ["Komm", "her", ",", "la\u00df", "dich", "ver\u00b7bin\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du hast mit Jesus nur zu tun,", "tokens": ["Du", "hast", "mit", "Je\u00b7sus", "nur", "zu", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NE", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und er mit deinen S\u00fcnden.", "tokens": ["Und", "er", "mit", "dei\u00b7nen", "S\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}