{"dta.poem.4072": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Eichb\u00e4ume .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Aus den G\u00e4rten komm' ich zu euch, ihr S\u00f6hne", "tokens": ["Aus", "den", "G\u00e4r\u00b7ten", "komm'", "ich", "zu", "euch", ",", "ihr", "S\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "APPR", "PPER", "$,", "PPOSAT", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "des Berges!", "tokens": ["des", "Ber\u00b7ges", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Aus den G\u00e4rten, da lebt die Natur, geduldig", "tokens": ["Aus", "den", "G\u00e4r\u00b7ten", ",", "da", "lebt", "die", "Na\u00b7tur", ",", "ge\u00b7dul\u00b7dig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,", "ADJD"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "und h\u00e4uslich,", "tokens": ["und", "h\u00e4us\u00b7lich", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Pflegend und wieder gepflegt, mit dem flei\u00dfigen", "tokens": ["Pfle\u00b7gend", "und", "wie\u00b7der", "ge\u00b7pflegt", ",", "mit", "dem", "flei\u00b7\u00dfi\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "KON", "ADV", "VVPP", "$,", "APPR", "ART", "NN"], "meter": "+--+--+--++-", "measure": "dactylic.tri.plus"}, "line.6": {"text": "Menschen zusammen.", "tokens": ["Men\u00b7schen", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Aber ihr, ihr Herrlichen! steht, wie ein Volk von", "tokens": ["A\u00b7ber", "ihr", ",", "ihr", "Herr\u00b7li\u00b7chen", "!", "steht", ",", "wie", "ein", "Volk", "von"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$.", "VVFIN", "$,", "PWAV", "ART", "NN", "APPR"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Titanen,", "tokens": ["Ti\u00b7ta\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "In der zahmeren Welt, und geh\u00f6rt nur euch und", "tokens": ["In", "der", "zah\u00b7me\u00b7ren", "Welt", ",", "und", "ge\u00b7h\u00f6rt", "nur", "euch", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "ADV", "PPER", "KON"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.10": {"text": "dem Himmel,", "tokens": ["dem", "Him\u00b7mel", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Der euch n\u00e4hrt' und erzog, und der Erde, die", "tokens": ["Der", "euch", "n\u00e4hrt'", "und", "er\u00b7zog", ",", "und", "der", "Er\u00b7de", ",", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "PPER", "VVFIN", "KON", "VVFIN", "$,", "KON", "ART", "NN", "$,", "PRELS"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.12": {"text": "euch geboren.", "tokens": ["euch", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Keiner von euch ist noch in der Menschen Schule", "tokens": ["Kei\u00b7ner", "von", "euch", "ist", "noch", "in", "der", "Men\u00b7schen", "Schu\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "APPR", "PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.14": {"text": "gegangen,", "tokens": ["ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Und ihr dr\u00e4ngt euch, fr\u00f6hlich und frei, aus kr\u00e4fti-", "tokens": ["Und", "ihr", "dr\u00e4ngt", "euch", ",", "fr\u00f6h\u00b7lich", "und", "frei", ",", "aus", "kr\u00e4f\u00b7ti"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$,", "APPR", "TRUNC"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "ger Wurzel", "tokens": ["ger", "Wur\u00b7zel"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Unter einander herauf und ergreift, wie der Adler,", "tokens": ["Un\u00b7ter", "ein\u00b7an\u00b7der", "her\u00b7auf", "und", "er\u00b7greift", ",", "wie", "der", "Ad\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "PTKVZ", "KON", "VVFIN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.18": {"text": "die Beute,", "tokens": ["die", "Beu\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Mit gewaltigem Arme den Raum, und gegen die", "tokens": ["Mit", "ge\u00b7wal\u00b7ti\u00b7gem", "Ar\u00b7me", "den", "Raum", ",", "und", "ge\u00b7gen", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,", "KON", "APPR", "ART"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.20": {"text": "Wolken", "tokens": ["Wol\u00b7ken"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.21": {"text": "Ist euch heiter und gro\u00df die sonnige Krone gerichtet.", "tokens": ["Ist", "euch", "hei\u00b7ter", "und", "gro\u00df", "die", "son\u00b7ni\u00b7ge", "Kro\u00b7ne", "ge\u00b7rich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "ADJD", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.22": {"text": "Eine Welt ist jeder von euch, wie die Sterne des", "tokens": ["Ei\u00b7ne", "Welt", "ist", "je\u00b7der", "von", "euch", ",", "wie", "die", "Ster\u00b7ne", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PIS", "APPR", "PPER", "$,", "PWAV", "ART", "NN", "ART"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.23": {"text": "Himmels", "tokens": ["Him\u00b7mels"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.24": {"text": "Lebt ihr, jeder ein Gott, in freiem Bunde zu-", "tokens": ["Lebt", "ihr", ",", "je\u00b7der", "ein", "Gott", ",", "in", "frei\u00b7em", "Bun\u00b7de", "zu"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PIS", "ART", "NN", "$,", "APPR", "ADJA", "NN", "TRUNC"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.25": {"text": "sammen.", "tokens": ["sam\u00b7men", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.26": {"text": "K\u00f6nnt' ich die Knechtschaft nur erdulden, ich nei-", "tokens": ["K\u00f6nnt'", "ich", "die", "Knecht\u00b7schaft", "nur", "er\u00b7dul\u00b7den", ",", "ich", "nei"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$,", "PPER", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.27": {"text": "dete nimmer", "tokens": ["de\u00b7te", "nim\u00b7mer"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.28": {"text": "Diesen Wald und schmiegte mich gern an's gesellige", "tokens": ["Die\u00b7sen", "Wald", "und", "schmieg\u00b7te", "mich", "gern", "an's", "ge\u00b7sel\u00b7li\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "KON", "VVFIN", "PPER", "ADV", "APPRART", "ADJA"], "meter": "+-+-+--++-+-+", "measure": "trochaic.septa.relaxed"}, "line.29": {"text": "Leben.", "tokens": ["Le\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.30": {"text": "Fesselte nur nicht mehr an's gesellige Leben das", "tokens": ["Fes\u00b7sel\u00b7te", "nur", "nicht", "mehr", "an's", "ge\u00b7sel\u00b7li\u00b7ge", "Le\u00b7ben", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PTKNEG", "ADV", "APPRART", "ADJA", "NN", "ART"], "meter": "+--+--+-+--+--", "measure": "dactylic.di.plus"}, "line.31": {"text": "Herz mich,", "tokens": ["Herz", "mich", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.32": {"text": "Das von Liebe nicht l\u00e4\u00dft, wie gern w\u00fcrd' ich unter", "tokens": ["Das", "von", "Lie\u00b7be", "nicht", "l\u00e4\u00dft", ",", "wie", "gern", "w\u00fcrd'", "ich", "un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "NN", "PTKNEG", "VVFIN", "$,", "PWAV", "ADV", "VAFIN", "PPER", "APPR"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.33": {"text": "euch wohnen!", "tokens": ["euch", "woh\u00b7nen", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}