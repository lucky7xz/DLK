{"dta.poem.13595": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Terzett .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Seh' fern am Horizont die Wetter steigen,", "tokens": ["Seh'", "fern", "am", "Ho\u00b7ri\u00b7zont", "die", "Wet\u00b7ter", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und durch die Wipfel, die sich leise neigen,", "tokens": ["Und", "durch", "die", "Wip\u00b7fel", ",", "die", "sich", "lei\u00b7se", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Rauscht droben schwellend ein gewaltig Lied,", "tokens": ["Rauscht", "dro\u00b7ben", "schwel\u00b7lend", "ein", "ge\u00b7wal\u00b7tig", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das ewig frisch mir durch die Seele zieht.", "tokens": ["Das", "e\u00b7wig", "frisch", "mir", "durch", "die", "See\u00b7le", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJD", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}