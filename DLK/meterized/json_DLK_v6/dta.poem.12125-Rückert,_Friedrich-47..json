{"dta.poem.12125": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "47.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer immer Sch\u00f6nes sieht, mu\u00df selber sch\u00f6n auch werden,", "tokens": ["Wer", "im\u00b7mer", "Sch\u00f6\u00b7nes", "sieht", ",", "mu\u00df", "sel\u00b7ber", "sch\u00f6n", "auch", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJA", "VVFIN", "$,", "VMFIN", "ADV", "ADJD", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An Seelenmienen sch\u00f6n und geistigen Geberden.", "tokens": ["An", "See\u00b7len\u00b7mie\u00b7nen", "sch\u00f6n", "und", "geis\u00b7ti\u00b7gen", "Ge\u00b7ber\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wo die Sch\u00f6nheit erst geworden innerlich,", "tokens": ["Und", "wo", "die", "Sch\u00f6n\u00b7heit", "erst", "ge\u00b7wor\u00b7den", "in\u00b7ner\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "VAPP", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da tritt sie auch hervor und zeigt im Aeu\u00dfern sich.", "tokens": ["Da", "tritt", "sie", "auch", "her\u00b7vor", "und", "zeigt", "im", "A\u00b7e\u00b7u\u00b7\u00dfern", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "KON", "VVFIN", "APPRART", "NN", "PRF", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Ein Engelmaler kann des eignen Leibes M\u00e4ngel", "tokens": ["Ein", "En\u00b7gel\u00b7ma\u00b7ler", "kann", "des", "eig\u00b7nen", "Lei\u00b7bes", "M\u00e4n\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht \u00fcberwinden, doch zeugt Kinder sch\u00f6n wie Engel.", "tokens": ["Nicht", "\u00fc\u00b7berw\u00b7in\u00b7den", ",", "doch", "zeugt", "Kin\u00b7der", "sch\u00f6n", "wie", "En\u00b7gel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "ADV", "VVFIN", "NN", "ADJD", "KOKOM", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}