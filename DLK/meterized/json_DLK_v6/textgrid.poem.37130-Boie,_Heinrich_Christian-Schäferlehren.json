{"textgrid.poem.37130": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Sch\u00e4ferlehren", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du hier in diesen Gr\u00fcnden", "tokens": ["Willst", "du", "hier", "in", "die\u00b7sen", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freude sonder Ekel finden,", "tokens": ["Freu\u00b7de", "son\u00b7der", "E\u00b7kel", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Freude sanft und wonniglich,", "tokens": ["Freu\u00b7de", "sanft", "und", "won\u00b7nig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00fc\u00dfer Freund! so h\u00f6re mich.", "tokens": ["S\u00fc\u00b7\u00dfer", "Freund", "!", "so", "h\u00f6\u00b7re", "mich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf dem saatbekr\u00e4nzten H\u00fcgel,", "tokens": ["Auf", "dem", "saat\u00b7be\u00b7kr\u00e4nz\u00b7ten", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An des Teiches klarem Spiegel,", "tokens": ["An", "des", "Tei\u00b7ches", "kla\u00b7rem", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf der Au, im Buchenwald", "tokens": ["Auf", "der", "Au", ",", "im", "Bu\u00b7chen\u00b7wald"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist ihr liebster Aufenthalt.", "tokens": ["Ist", "ihr", "liebs\u00b7ter", "Auf\u00b7ent\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In des Fr\u00fchlings Blumenkleide", "tokens": ["In", "des", "Fr\u00fch\u00b7lings", "Blu\u00b7men\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwebet leisen Tritts die Freude,", "tokens": ["Schwe\u00b7bet", "lei\u00b7sen", "Tritts", "die", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwebt sie selbst auf dieser Flur,", "tokens": ["Schwebt", "sie", "selbst", "auf", "die\u00b7ser", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Stadt ihr Schatten nur.", "tokens": ["In", "der", "Stadt", "ihr", "Schat\u00b7ten", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "F\u00fchlst du in der lauten Irre", "tokens": ["F\u00fchlst", "du", "in", "der", "lau\u00b7ten", "Ir\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses Baches, im Geschwirre", "tokens": ["Die\u00b7ses", "Ba\u00b7ches", ",", "im", "Ge\u00b7schwir\u00b7re"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "$,", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieser V\u00f6gel, in dem Kuss", "tokens": ["Die\u00b7ser", "V\u00f6\u00b7gel", ",", "in", "dem", "Kuss"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieses Wests, nicht ihren Gru\u00df?", "tokens": ["Die\u00b7ses", "Wests", ",", "nicht", "ih\u00b7ren", "Gru\u00df", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Bist du nicht dem Kr\u00e4uterwasen,", "tokens": ["Bist", "du", "nicht", "dem", "Kr\u00e4u\u00b7ter\u00b7wa\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht den L\u00e4mmern, die hier grasen,", "tokens": ["Nicht", "den", "L\u00e4m\u00b7mern", ",", "die", "hier", "gra\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht dem kleinsten Bl\u00fcmchen hold:", "tokens": ["Nicht", "dem", "kleins\u00b7ten", "Bl\u00fcm\u00b7chen", "hold", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Heim zur Stadt und kriech um Gold!", "tokens": ["Heim", "zur", "Stadt", "und", "kriech", "um", "Gold", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "KON", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Stille Freude fehlet nimmer,", "tokens": ["Stil\u00b7le", "Freu\u00b7de", "feh\u00b7let", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00e4uschend ist der lautern Schimmer,", "tokens": ["T\u00e4u\u00b7schend", "ist", "der", "lau\u00b7tern", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jede Leidenschaft ist Schmerz,", "tokens": ["Je\u00b7de", "Lei\u00b7den\u00b7schaft", "ist", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur die Liebe lohnt ein Herz.", "tokens": ["Nur", "die", "Lie\u00b7be", "lohnt", "ein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sie die Mutter alles Sch\u00f6nen,", "tokens": ["Sie", "die", "Mut\u00b7ter", "al\u00b7les", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00fc\u00dfe deine Freuden kr\u00f6nen;", "tokens": ["M\u00fc\u00b7\u00dfe", "dei\u00b7ne", "Freu\u00b7den", "kr\u00f6\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch eh sie die Myrte flicht,", "tokens": ["Doch", "eh", "sie", "die", "Myr\u00b7te", "flicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "H\u00f6re was die Weisheit spricht.", "tokens": ["H\u00f6\u00b7re", "was", "die", "Weis\u00b7heit", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Jene Rose lockt zum brechen:", "tokens": ["Je\u00b7ne", "Ro\u00b7se", "lockt", "zum", "bre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "APPRART", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00fcte dich! ihr Dorn kann stechen.", "tokens": ["H\u00fc\u00b7te", "dich", "!", "ihr", "Dorn", "kann", "ste\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jener Busch reizt deinen Sinn:", "tokens": ["Je\u00b7ner", "Busch", "reizt", "dei\u00b7nen", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fleuch! die Natter lauret drinn!", "tokens": ["Fleuch", "!", "die", "Nat\u00b7ter", "lau\u00b7ret", "drinn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Kann Sie Dorf und Flur verlachen,", "tokens": ["Kann", "Sie", "Dorf", "und", "Flur", "ver\u00b7la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird Sie dich nicht gl\u00fccklich machen.", "tokens": ["Wird", "Sie", "dich", "nicht", "gl\u00fcck\u00b7lich", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die der Schafe spotten kann,", "tokens": ["Die", "der", "Scha\u00b7fe", "spot\u00b7ten", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieht mit Spott den Sch\u00e4fer an.", "tokens": ["Sieht", "mit", "Spott", "den", "Sch\u00e4\u00b7fer", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Unschuld in der H\u00fctte bilde", "tokens": ["Un\u00b7schuld", "in", "der", "H\u00fct\u00b7te", "bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir ein M\u00e4dchen gut und milde.", "tokens": ["Dir", "ein", "M\u00e4d\u00b7chen", "gut", "und", "mil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ungesucht und ungesehn", "tokens": ["Un\u00b7ge\u00b7sucht", "und", "un\u00b7ge\u00b7sehn"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sey sie dir allein nur sch\u00f6n.", "tokens": ["Sey", "sie", "dir", "al\u00b7lein", "nur", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Seelenwort sey ihre Rede;", "tokens": ["See\u00b7len\u00b7wort", "sey", "ih\u00b7re", "Re\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00fcchtern blicke sie, nicht spr\u00f6de,", "tokens": ["Sch\u00fcch\u00b7tern", "bli\u00b7cke", "sie", ",", "nicht", "spr\u00f6\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht mir falscher Scham um sich,", "tokens": ["Nicht", "mir", "fal\u00b7scher", "Scham", "um", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "ADJA", "NN", "APPR", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ihr Herz erkenne dich!", "tokens": ["Und", "ihr", "Herz", "er\u00b7ken\u00b7ne", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Klugheit, deren Schein sie fliehe,", "tokens": ["Klug\u00b7heit", ",", "de\u00b7ren", "Schein", "sie", "flie\u00b7he", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Witz, um den sie sich nicht m\u00fche,", "tokens": ["Witz", ",", "um", "den", "sie", "sich", "nicht", "m\u00fc\u00b7he", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "ART", "PPER", "PRF", "PTKNEG", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sanftes Mitleid, das schon weint,", "tokens": ["Sanf\u00b7tes", "Mit\u00b7leid", ",", "das", "schon", "weint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn nur krank ihr L\u00e4mmchen scheint,", "tokens": ["Wenn", "nur", "krank", "ihr", "L\u00e4mm\u00b7chen", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Einfalt in Geschmack und Sitte,", "tokens": ["Ein\u00b7falt", "in", "Ge\u00b7schmack", "und", "Sit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Anmuth in dem kleinsten Schritte,", "tokens": ["An\u00b7muth", "in", "dem", "kleins\u00b7ten", "Schrit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wahl in Kleidung, Absicht nie,", "tokens": ["Wahl", "in", "Klei\u00b7dung", ",", "Ab\u00b7sicht", "nie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zier' und unterscheide sie!", "tokens": ["Zier'", "und", "un\u00b7ter\u00b7schei\u00b7de", "sie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Hast du solch ein Kind gefunden,", "tokens": ["Hast", "du", "solch", "ein", "Kind", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O so segne deine Stunden!", "tokens": ["O", "so", "seg\u00b7ne", "dei\u00b7ne", "Stun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Selig, giebt sie dir die Hand!", "tokens": ["Se\u00b7lig", ",", "giebt", "sie", "dir", "die", "Hand", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gold und Ueppigkeit sind Tand.", "tokens": ["Gold", "und", "Uep\u00b7pig\u00b7keit", "sind", "Tand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Deine Tage zu vers\u00fc\u00dfen,", "tokens": ["Dei\u00b7ne", "Ta\u00b7ge", "zu", "ver\u00b7s\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fchen Blumen, Quellen flie\u00dfen;", "tokens": ["Bl\u00fc\u00b7hen", "Blu\u00b7men", ",", "Quel\u00b7len", "flie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Arbeit macht dich froh und frisch,", "tokens": ["Ar\u00b7beit", "macht", "dich", "froh", "und", "frisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Milch und Brot w\u00fcrzt deinen Tisch.", "tokens": ["Milch", "und", "Brot", "w\u00fcrzt", "dei\u00b7nen", "Tisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Elend obenhin verg\u00fcldet", "tokens": ["E\u00b7lend", "o\u00b7ben\u00b7hin", "ver\u00b7g\u00fcl\u00b7det"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist was sonst der Stolz sich bildet.", "tokens": ["Ist", "was", "sonst", "der", "Stolz", "sich", "bil\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer noch w\u00fcnschet, ist nicht klug;", "tokens": ["Wer", "noch", "w\u00fcn\u00b7schet", ",", "ist", "nicht", "klug", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was du hast, ist dir 