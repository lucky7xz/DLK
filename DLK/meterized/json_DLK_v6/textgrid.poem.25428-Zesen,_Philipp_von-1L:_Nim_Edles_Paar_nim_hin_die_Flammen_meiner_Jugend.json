{"textgrid.poem.25428": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nim/ Edles Paar/ nim hin die Flammen meiner Jugend/", "genre": "verse", "period": "N.A.", "pub_year": 1654, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nim/ Edles Paar/ nim hin die Flammen meiner Jugend/", "tokens": ["Nim", "/", "Ed\u00b7les", "Paar", "/", "nim", "hin", "die", "Flam\u00b7men", "mei\u00b7ner", "Ju\u00b7gend", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ADJA", "NN", "$(", "VVIMP", "ADV", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "das blitzlen meiner bl\u00fcht/ den sporen hoher tugend/", "tokens": ["das", "blitz\u00b7len", "mei\u00b7ner", "bl\u00fcht", "/", "den", "spo\u00b7ren", "ho\u00b7her", "tu\u00b7gend", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "VVFIN", "$(", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "der mich trieb Himmel-an/ und ri\u00df den muntern Muht", "tokens": ["der", "mich", "trieb", "Him\u00b7mel\u00b7an", "/", "und", "ri\u00df", "den", "mun\u00b7tern", "Muht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "NE", "$(", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "aus staub und asche fort nach jener klaren gluht", "tokens": ["aus", "staub", "und", "asc\u00b7he", "fort", "nach", "je\u00b7ner", "kla\u00b7ren", "gluht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "KON", "ADJA", "PTKVZ", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "selbst aus und \u00fcber uns. Frau Fr\u00e4ue spielt hierinnen/", "tokens": ["selbst", "aus", "und", "\u00fc\u00b7ber", "uns", ".", "Frau", "Fr\u00e4u\u00b7e", "spielt", "hie\u00b7rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KON", "APPR", "PPER", "$.", "NN", "NN", "VVFIN", "PAV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "die feine Vene die/ die st\u00e4rkste der G\u00f6ttinnen/", "tokens": ["die", "fei\u00b7ne", "Ve\u00b7ne", "die", "/", "die", "st\u00e4rks\u00b7te", "der", "G\u00f6t\u00b7tin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "$(", "ART", "ADJA", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "der frohen fr\u00e4uerei und liebe stifterin/", "tokens": ["der", "fro\u00b7hen", "fr\u00e4u\u00b7e\u00b7rei", "und", "lie\u00b7be", "stif\u00b7te\u00b7rin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "die Rohm von uns geraubt. Die Vene/ die den sinn", "tokens": ["die", "Rohm", "von", "uns", "ge\u00b7raubt", ".", "Die", "Ve\u00b7ne", "/", "die", "den", "sinn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "VVPP", "$.", "ART", "NN", "$(", "ART", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "nat\u00fcrlich rege macht/ wil so ihr ziel erreichen.", "tokens": ["na\u00b7t\u00fcr\u00b7lich", "re\u00b7ge", "macht", "/", "wil", "so", "ihr", "ziel", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VVFIN", "$(", "VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Drauf sol die Geistliche durch-hin ein kreutze streichen", "tokens": ["Drauf", "sol", "die", "Geist\u00b7li\u00b7che", "durch\u00b7hin", "ein", "kreut\u00b7ze", "strei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "+--+--++-+-+-", "measure": "dactylic.di.plus"}, "line.11": {"text": "und t\u00e4mmen die natur. Wann dieses wird geschehn/", "tokens": ["und", "t\u00e4m\u00b7men", "die", "na\u00b7tur", ".", "Wann", "die\u00b7ses", "wird", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$.", "PWAV", "PDS", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "wird Vene selbsten sich durch sich gekreutzigt sehn.", "tokens": ["wird", "Ve\u00b7ne", "selbs\u00b7ten", "sich", "durch", "sich", "ge\u00b7kreut\u00b7zigt", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "VVFIN", "PRF", "APPR", "PRF", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So steigt durch staffeln auf ein Geist/ der feuer f\u00fchlet/", "tokens": ["So", "steigt", "durch", "staf\u00b7feln", "auf", "ein", "Geist", "/", "der", "feu\u00b7er", "f\u00fch\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "VVFIN", "APPR", "ART", "NN", "$(", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "und trift das augen-m\u00e4rk/ darnach er kl\u00fcglich zielet/", "tokens": ["und", "trift", "das", "au\u00b7gen\u00b7m\u00e4rk", "/", "dar\u00b7nach", "er", "kl\u00fcg\u00b7lich", "zie\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$(", "PAV", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "zur wahren Himmels-burg. So steig' ich auch gemach", "tokens": ["zur", "wah\u00b7ren", "Him\u00b7mels\u00b7burg", ".", "So", "steig'", "ich", "auch", "ge\u00b7mach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "nach meinem zwekke zu/ ans klahre sternen-dach.", "tokens": ["nach", "mei\u00b7nem", "zwek\u00b7ke", "zu", "/", "ans", "klah\u00b7re", "ster\u00b7nen\u00b7dach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "$(", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So schl\u00e4gt in heisser brunst die keusche liebes-flamme", "tokens": ["So", "schl\u00e4gt", "in", "heis\u00b7ser", "brunst", "die", "keu\u00b7sche", "lie\u00b7bes\u00b7flam\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "gespitzigt \u00fcber sich/ als jener gluhten Amme;", "tokens": ["ge\u00b7spit\u00b7zigt", "\u00fc\u00b7ber", "sich", "/", "als", "je\u00b7ner", "gluh\u00b7ten", "Am\u00b7me", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PRF", "$(", "KOUS", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "und wil von Eurer gunst indes sein angeblikt/", "tokens": ["und", "wil", "von", "Eu\u00b7rer", "gunst", "in\u00b7des", "sein", "an\u00b7ge\u00b7blikt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PPOSAT", "NN", "ADV", "PPOSAT", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "bis sie ink\u00fcnftig mehr/ ja himmels-strahlen schikt/", "tokens": ["bis", "sie", "in\u00b7k\u00fcnf\u00b7tig", "mehr", "/", "ja", "him\u00b7mels\u00b7strah\u00b7len", "schikt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "$(", "ADV", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "die \u00fcber-weltlich seind. ", "tokens": ["die", "\u00fc\u00b7ber\u00b7welt\u00b7lich", "seind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "zu prei\u00df und r\u00fchm sich l\u00e4sst so mancher Dichter h\u00f6ren/", "tokens": ["zu", "prei\u00df", "und", "r\u00fchm", "sich", "l\u00e4sst", "so", "man\u00b7cher", "Dich\u00b7ter", "h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "VVFIN", "PRF", "VVFIN", "ADV", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "weil ihre s\u00fc\u00dfe kunst Er unvergleichlich liebt/", "tokens": ["weil", "ih\u00b7re", "s\u00fc\u00b7\u00dfe", "kunst", "Er", "un\u00b7ver\u00b7gleich\u00b7lich", "liebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "und aller Wissenschaft so milde g\u00fcnste giebt;", "tokens": ["und", "al\u00b7ler", "Wis\u00b7sen\u00b7schaft", "so", "mil\u00b7de", "g\u00fcns\u00b7te", "giebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "ADJD", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "geruhet doch auch hier ein wenig gunst zu schenken", "tokens": ["ge\u00b7ru\u00b7het", "doch", "auch", "hier", "ein", "we\u00b7nig", "gunst", "zu", "schen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "ART", "PIAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "und ein geneugtes aug' auf diese Gluht zu lenken/", "tokens": ["und", "ein", "ge\u00b7neug\u00b7tes", "aug'", "auf", "die\u00b7se", "Gluht", "zu", "len\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "PDAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "die dan viel gl\u00fchender wird flammen/ als sie pfl\u00e4gt/", "tokens": ["die", "dan", "viel", "gl\u00fc\u00b7hen\u00b7der", "wird", "flam\u00b7men", "/", "als", "sie", "pfl\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "VAFIN", "VVINF", "$(", "KOUS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "wan sich durch eure gunst ihr himmels-blitz bew\u00e4gt.", "tokens": ["wan", "sich", "durch", "eu\u00b7re", "gunst", "ihr", "him\u00b7mels\u00b7blitz", "be\u00b7w\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.29": {"text": "bei seiner mu\u00df' ergetzt/ und h\u00e4lt in milden g\u00fcnsten", "tokens": ["bei", "sei\u00b7ner", "mu\u00df'", "er\u00b7getzt", "/", "und", "h\u00e4lt", "in", "mil\u00b7den", "g\u00fcns\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "VMFIN", "VVPP", "$(", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "der hohen Geister witz/ der wolle dieser Gluht", "tokens": ["der", "ho\u00b7hen", "Geis\u00b7ter", "witz", "/", "der", "wol\u00b7le", "die\u00b7ser", "Gluht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NE", "$(", "ART", "VMFIN", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "durch s\u00fc\u00dfes seiten-spiel auch geben seel und muht/", "tokens": ["durch", "s\u00fc\u00b7\u00dfes", "sei\u00b7ten\u00b7spiel", "auch", "ge\u00b7ben", "seel", "und", "muht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVINF", "ADJD", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "wie er dan l\u00f6blich pflegt. Dis sol mir sein ein zeuchen", "tokens": ["wie", "er", "dan", "l\u00f6b\u00b7lich", "pflegt", ".", "Dis", "sol", "mir", "sein", "ein", "zeu\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VVFIN", "$.", "PDS", "VMFIN", "PPER", "PPOSAT", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "der h\u00f6chsten Freundesgunst. und daf\u00fcr wil ich reichen", "tokens": ["der", "h\u00f6chs\u00b7ten", "Freun\u00b7des\u00b7gunst", ".", "und", "da\u00b7f\u00fcr", "wil", "ich", "rei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "KON", "PAV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "so manchen tr\u00e4uen dank/ so manches s\u00fc\u00dfe lied/", "tokens": ["so", "man\u00b7chen", "tr\u00e4u\u00b7en", "dank", "/", "so", "man\u00b7ches", "s\u00fc\u00b7\u00dfe", "lied", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "$(", "ADV", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "so manches zukker-wort/ das hin zur Nach-welt zieht/", "tokens": ["so", "man\u00b7ches", "zuk\u00b7ker\u00b7wort", "/", "das", "hin", "zur", "Nach\u00b7welt", "zieht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$(", "PDS", "ADV", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "und ewig bleiben sol mit ihrem hohen ruhme;", "tokens": ["und", "e\u00b7wig", "blei\u00b7ben", "sol", "mit", "ih\u00b7rem", "ho\u00b7hen", "ruh\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "VMFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "so lang in keuscher schahm die rohte rosen-bluhme", "tokens": ["so", "lang", "in", "keu\u00b7scher", "schahm", "die", "roh\u00b7te", "ro\u00b7sen\u00b7bluh\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "im liljen-bette gl\u00fcht; so lang ein Sommer gl\u00e4ntzt;", "tokens": ["im", "lil\u00b7jen\u00b7bet\u00b7te", "gl\u00fcht", ";", "so", "lang", "ein", "Som\u00b7mer", "gl\u00e4ntzt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$.", "ADV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "so lang es herbsten wird; so lang es wintert/ lentzt:", "tokens": ["so", "lang", "es", "herbs\u00b7ten", "wird", ";", "so", "lang", "es", "win\u00b7tert", "/", "lentzt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVINF", "VAFIN", "$.", "ADV", "ADJD", "PPER", "VVFIN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "ja so lange ich lebe", "tokens": ["ja", "so", "lan\u00b7ge", "ich", "le\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "PPER", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "meiner Herren", "tokens": ["mei\u00b7ner", "Her\u00b7ren"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "dienstwilligster", "tokens": ["dienst\u00b7wil\u00b7ligs\u00b7ter"], "token_info": ["word"], "pos": ["PDS"], "meter": "+-+-", "measure": "trochaic.di"}}}}}