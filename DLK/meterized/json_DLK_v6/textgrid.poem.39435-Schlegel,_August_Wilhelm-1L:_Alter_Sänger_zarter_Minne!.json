{"textgrid.poem.39435": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Alter S\u00e4nger zarter Minne!", "genre": "verse", "period": "N.A.", "pub_year": 1812, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alter S\u00e4nger zarter Minne!", "tokens": ["Al\u00b7ter", "S\u00e4n\u00b7ger", "zar\u00b7ter", "Min\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weibes Sch\u00f6ne, G\u00fct' und Zucht,", "tokens": ["Wei\u00b7bes", "Sch\u00f6\u00b7ne", ",", "G\u00fct'", "und", "Zucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aller Wonne Bl\u00fcth' und Frucht,", "tokens": ["Al\u00b7ler", "Won\u00b7ne", "Bl\u00fcth'", "und", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sp\u00e4htest du mit Meistersinne.", "tokens": ["Sp\u00e4h\u00b7test", "du", "mit", "Meis\u00b7ter\u00b7sin\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deines Spruches ward ich inne", "tokens": ["Dei\u00b7nes", "Spru\u00b7ches", "ward", "ich", "in\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tief in meines Herzens Grund:", "tokens": ["Tief", "in", "mei\u00b7nes", "Her\u00b7zens", "Grund", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "\u00bbweinende Augen haben s\u00fc\u00dfen Mund.\u00ab", "tokens": ["\u00bb", "wei\u00b7nen\u00b7de", "Au\u00b7gen", "ha\u00b7ben", "s\u00fc\u00b7\u00dfen", "Mund", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "VAFIN", "ADJA", "NN", "$.", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Von der Holden mu\u00dft' ich scheiden,", "tokens": ["Von", "der", "Hol\u00b7den", "mu\u00dft'", "ich", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mir neues Leben bot:", "tokens": ["Die", "mir", "neu\u00b7es", "Le\u00b7ben", "bot", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da erblich der Wangen Roth,", "tokens": ["Da", "er\u00b7blich", "der", "Wan\u00b7gen", "Roth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lust verkehrte sich in Leiden.", "tokens": ["Lust", "ver\u00b7kehr\u00b7te", "sich", "in", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch, um unser Weh zu weiden,", "tokens": ["Doch", ",", "um", "un\u00b7ser", "Weh", "zu", "wei\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schlo\u00dfen enger unsern Bund", "tokens": ["Schlo\u00b7\u00dfen", "en\u00b7ger", "un\u00b7sern", "Bund"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Weinende Augen und ein s\u00fc\u00dfer Mund.", "tokens": ["Wei\u00b7nen\u00b7de", "Au\u00b7gen", "und", "ein", "s\u00fc\u00b7\u00dfer", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Tr\u00fcb' umw\u00f6lkte sich mit Thr\u00e4nen,", "tokens": ["Tr\u00fcb'", "um\u00b7w\u00f6lk\u00b7te", "sich", "mit", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sonst so licht, ihr Himmelsblick,", "tokens": ["Sonst", "so", "licht", ",", "ihr", "Him\u00b7mels\u00b7blick", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil der Liebe hart Geschick", "tokens": ["Weil", "der", "Lie\u00b7be", "hart", "Ge\u00b7schick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Uns entri\u00df der Hoffnung W\u00e4hnen.", "tokens": ["Uns", "ent\u00b7ri\u00df", "der", "Hoff\u00b7nung", "W\u00e4h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da erbarmte sie mein Sehnen,", "tokens": ["Da", "er\u00b7barm\u00b7te", "sie", "mein", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Dem sie streng' oft widerstund:", "tokens": ["Dem", "sie", "streng'", "oft", "wi\u00b7der\u00b7stund", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Weinende Augen boten s\u00fc\u00dfen Mund.", "tokens": ["Wei\u00b7nen\u00b7de", "Au\u00b7gen", "bo\u00b7ten", "s\u00fc\u00b7\u00dfen", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Zwar verstummten jetzt die Worte,", "tokens": ["Zwar", "ver\u00b7stumm\u00b7ten", "jetzt", "die", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die sie lieblich sonst gekos't;", "tokens": ["Die", "sie", "lieb\u00b7lich", "sonst", "ge\u00b7kos't", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch es kam mir andrer Trost", "tokens": ["Doch", "es", "kam", "mir", "an\u00b7drer", "Trost"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus der Lippen Rosenpforte.", "tokens": ["Aus", "der", "Lip\u00b7pen", "Ro\u00b7sen\u00b7pfor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Meinem Gram zu Heil und Horte", "tokens": ["Mei\u00b7nem", "Gram", "zu", "Heil", "und", "Hor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "That mir mildes Gr\u00fc\u00dfen kund,", "tokens": ["That", "mir", "mil\u00b7des", "Gr\u00fc\u00b7\u00dfen", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Weinende Augen haben s\u00fc\u00dfen Mund.", "tokens": ["Wei\u00b7nen\u00b7de", "Au\u00b7gen", "ha\u00b7ben", "s\u00fc\u00b7\u00dfen", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Die sich treu und innig meinen,", "tokens": ["Die", "sich", "treu", "und", "in\u00b7nig", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trennet weder Land noch Meer.", "tokens": ["Tren\u00b7net", "we\u00b7der", "Land", "noch", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "NN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drum verzage nicht so sehr!", "tokens": ["Drum", "ver\u00b7za\u00b7ge", "nicht", "so", "sehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKNEG", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Einst ja wird der Tag erscheinen,", "tokens": ["Einst", "ja", "wird", "der", "Tag", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo ein seliges Vereinen", "tokens": ["Wo", "ein", "se\u00b7li\u00b7ges", "Ver\u00b7ei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Macht von allem Weh gesund", "tokens": ["Macht", "von", "al\u00b7lem", "Weh", "ge\u00b7sund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PIS", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Leuchtende Augen und den s\u00fc\u00dfen Mund.", "tokens": ["Leuch\u00b7ten\u00b7de", "Au\u00b7gen", "und", "den", "s\u00fc\u00b7\u00dfen", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}