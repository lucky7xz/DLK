{"textgrid.poem.30131": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "45.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Federn auff dem Hute tr\u00e4gt, der d\u00fcncket sich was seyn;", "tokens": ["Der", "Fe\u00b7dern", "auff", "dem", "Hu\u00b7te", "tr\u00e4gt", ",", "der", "d\u00fcn\u00b7cket", "sich", "was", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "PRF", "PIS", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Der Federn hinterm Ohre tr\u00e4gt, der d\u00fcncket sich kein Schwein;", "tokens": ["Der", "Fe\u00b7dern", "hin\u00b7term", "Oh\u00b7re", "tr\u00e4gt", ",", "der", "d\u00fcn\u00b7cket", "sich", "kein", "Schwein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "PRF", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Mit dem, der Hut und Ohr besteckt, k\u00fcmmt niemand \u00fcberein.", "tokens": ["Mit", "dem", ",", "der", "Hut", "und", "Ohr", "be\u00b7steckt", ",", "k\u00fcmmt", "nie\u00b7mand", "\u00fc\u00b7be\u00b7re\u00b7in", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "ART", "NN", "KON", "NN", "VVPP", "$,", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}}}}}