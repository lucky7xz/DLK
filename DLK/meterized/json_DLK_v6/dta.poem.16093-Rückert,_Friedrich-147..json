{"dta.poem.16093": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "147.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Sohn, gesteh ichs dir, da\u00df ich vergessen habe", "tokens": ["Mein", "Sohn", ",", "ge\u00b7steh", "ichs", "dir", ",", "da\u00df", "ich", "ver\u00b7ges\u00b7sen", "ha\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PIS", "PPER", "$,", "KOUS", "PPER", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gar manches nun als Greis, was ich gelernt als Knabe.", "tokens": ["Gar", "man\u00b7ches", "nun", "als", "Greis", ",", "was", "ich", "ge\u00b7lernt", "als", "Kna\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "KOUS", "NN", "$,", "PWS", "PPER", "VVPP", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht zur Entschuldigung gereicht dir das indessen;", "tokens": ["Nicht", "zur", "Ent\u00b7schul\u00b7di\u00b7gung", "ge\u00b7reicht", "dir", "das", "in\u00b7des\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "VVFIN", "PPER", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erst lernen mu\u00dft du's auch, eh du es darfst vergessen.", "tokens": ["Erst", "ler\u00b7nen", "mu\u00dft", "du's", "auch", ",", "eh", "du", "es", "darfst", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "po