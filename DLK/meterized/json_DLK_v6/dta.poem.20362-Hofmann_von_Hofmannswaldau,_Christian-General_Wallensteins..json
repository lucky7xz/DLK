{"dta.poem.20362": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "General Wallensteins.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hier liegt das grosse haupt/ so itzt wird ausgelacht;", "tokens": ["Hier", "liegt", "das", "gros\u00b7se", "haupt", "/", "so", "itzt", "wird", "aus\u00b7ge\u00b7lacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$(", "ADV", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Viel wissen mehr von mir/ als ich iemahls gedacht.", "tokens": ["Viel", "wis\u00b7sen", "mehr", "von", "mir", "/", "als", "ich", "ie\u00b7mahls", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PPER", "$(", "KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch wust ich/ da\u00df ein stein nicht leicht ein stern kan werden/", "tokens": ["Doch", "wust", "ich", "/", "da\u00df", "ein", "stein", "nicht", "leicht", "ein", "stern", "kan", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$(", "KOUS", "ART", "NN", "PTKNEG", "ADJD", "ART", "VVINF", "VMFIN", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein stein/ wie hoch er steigt/ f\u00e4llt endlich zu der erden.", "tokens": ["Ein", "stein", "/", "wie", "hoch", "er", "steigt", "/", "f\u00e4llt", "end\u00b7lich", "zu", "der", "er\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAV", "ADJD", "PPER", "VVFIN", "$(", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}