, "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Ich war sein Einziger, an den er alles wandte,", "tokens": ["Ich", "war", "sein", "Ein\u00b7zi\u00b7ger", ",", "an", "den", "er", "al\u00b7les", "wand\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "APPR", "PRELS", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.46": {"text": "bis da\u00df Apollo mich f\u00fcr seinen Freund erkante", "tokens": ["bis", "da\u00df", "A\u00b7pol\u00b7lo", "mich", "f\u00fcr", "sei\u00b7nen", "Freund", "er\u00b7kan\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "NE", "PRF", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "und nach zwei Kr\u00e4nzen mir den dritten flochte schon;", "tokens": ["und", "nach", "zwei", "Kr\u00e4n\u00b7zen", "mir", "den", "drit\u00b7ten", "floch\u00b7te", "schon", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "CARD", "NN", "PPER", "ART", "ADJA", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "nun aber ist mit mir ihm aller Trost darvon.", "tokens": ["nun", "a\u00b7ber", "ist", "mit", "mir", "ihm", "al\u00b7ler", "Trost", "dar\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "APPR", "PPER", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "So mu\u00df ich \u00fcber das auch dieses mich befahren,", "tokens": ["So", "mu\u00df", "ich", "\u00fc\u00b7ber", "das", "auch", "die\u00b7ses", "mich", "be\u00b7fah\u00b7ren", ","], "token_info": ["word", 