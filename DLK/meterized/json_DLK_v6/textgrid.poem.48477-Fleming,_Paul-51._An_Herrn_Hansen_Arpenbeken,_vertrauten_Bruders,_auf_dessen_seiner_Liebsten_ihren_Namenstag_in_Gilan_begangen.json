{"textgrid.poem.48477": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "51. An Herrn Hansen Arpenbeken, vertrauten Bruders, auf dessen seiner Liebsten ihren Namenstag in Gilan begangen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hilft Gott dir dieses Jahr, mein Bruder, \u00fcberleben,", "tokens": ["Hilft", "Gott", "dir", "die\u00b7ses", "Jahr", ",", "mein", "Bru\u00b7der", ",", "\u00fc\u00b7berl\u00b7e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "PDAT", "NN", "$,", "PPOSAT", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "so wirstu zweifelsfrei in gr\u00f6\u00dfrer Freude schweben,", "tokens": ["so", "wirs\u00b7tu", "zwei\u00b7fels\u00b7frei", "in", "gr\u00f6\u00df\u00b7rer", "Freu\u00b7de", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "CARD", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wenn du auf diesen Tag dein Sch\u00f6nstes von der Welt", "tokens": ["wenn", "du", "auf", "die\u00b7sen", "Tag", "dein", "Sch\u00f6ns\u00b7tes", "von", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "mit dir selbst binden wirst um dein und ihren Belt.", "tokens": ["mit", "dir", "selbst", "bin\u00b7den", "wirst", "um", "dein", "und", "ih\u00b7ren", "Belt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVINF", "VAFIN", "APPR", "PPOSAT", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn wirstu alles Leid der langen dreien Jahre,", "tokens": ["Denn", "wirs\u00b7tu", "al\u00b7les", "Leid", "der", "lan\u00b7gen", "drei\u00b7en", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "ART", "ADJA", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "die Persien uns verzehrt und Manchem graue Haare", "tokens": ["die", "Per\u00b7si\u00b7en", "uns", "ver\u00b7zehrt", "und", "Man\u00b7chem", "grau\u00b7e", "Haa\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVPP", "KON", "PIAT", "ADJA", "NN"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "bei frischer Jugend macht, auf einmal werden los,", "tokens": ["bei", "fri\u00b7scher", "Ju\u00b7gend", "macht", ",", "auf", "ein\u00b7mal", "wer\u00b7den", "los", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "APPR", "ADV", "VAINF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "wenn du gebunden liegst in deiner Liebsten Scho\u00df", "tokens": ["wenn", "du", "ge\u00b7bun\u00b7den", "liegst", "in", "dei\u00b7ner", "Liebs\u00b7ten", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "und in der deinen sie. Wie fr\u00f6lich wird sichs denken", "tokens": ["und", "in", "der", "dei\u00b7nen", "sie", ".", "Wie", "fr\u00f6\u00b7lich", "wird", "sichs", "den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "PPOSAT", "PPER", "$.", "PWAV", "ADJD", "VAFIN", "PIS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "an das, warum wir uns anitzt noch furchtsam kr\u00e4nken!", "tokens": ["an", "das", ",", "wa\u00b7rum", "wir", "uns", "a\u00b7nitzt", "noch", "furcht\u00b7sam", "kr\u00e4n\u00b7ken", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "$,", "PWAV", "PPER", "PRF", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie sicher wirstu denn erz\u00e4len von Gefahr;", "tokens": ["Wie", "si\u00b7cher", "wirs\u00b7tu", "denn", "er\u00b7z\u00e4\u00b7len", "von", "Ge\u00b7fahr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "VVINF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "es wird dir Zucker sein, was vormals Galle war.", "tokens": ["es", "wird", "dir", "Zu\u00b7cker", "sein", ",", "was", "vor\u00b7mals", "Gal\u00b7le", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "VAINF", "$,", "PRELS", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich zweifle sehr daran, da\u00df ich dann werde leben", "tokens": ["Ich", "zweif\u00b7le", "sehr", "da\u00b7ran", ",", "da\u00df", "ich", "dann", "wer\u00b7de", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "$,", "KOUS", "PPER", "ADV", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "und dir auch meinen Wundsch mit andern Freunden geben,", "tokens": ["und", "dir", "auch", "mei\u00b7nen", "Wund\u00b7sch", "mit", "an\u00b7dern", "Freun\u00b7den", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "weil dieser schwere Zug mich t\u00e4glich m\u00fcrber macht", "tokens": ["weil", "die\u00b7ser", "schwe\u00b7re", "Zug", "mich", "t\u00e4g\u00b7lich", "m\u00fcr\u00b7ber", "macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "PPER", "ADJD", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "und meinen st\u00e4rksten Teil schon l\u00e4ngst hat umgebracht.", "tokens": ["und", "mei\u00b7nen", "st\u00e4rks\u00b7ten", "Teil", "schon", "l\u00e4ngst", "hat", "um\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ADV", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Tut uns die Reise nach und sagts uns dann, ihr Br\u00fcder,", "tokens": ["Tut", "uns", "die", "Rei\u00b7se", "nach", "und", "sagts", "uns", "dann", ",", "ihr", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPR", "KON", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "wie stark ihr zoget aus, wie schwach ihr kommet wieder!", "tokens": ["wie", "stark", "ihr", "zo\u00b7get", "aus", ",", "wie", "schwach", "ihr", "kom\u00b7met", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVFIN", "PTKVZ", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wer reich und stark und alt zu werden ihm erkiest,", "tokens": ["Wer", "reich", "und", "stark", "und", "alt", "zu", "wer\u00b7den", "ihm", "er\u00b7kiest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "KON", "ADJD", "PTKZU", "VAINF", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "der lasse Reisen sein und bleibe, wo er ist!", "tokens": ["der", "las\u00b7se", "Rei\u00b7sen", "sein", "und", "blei\u00b7be", ",", "wo", "er", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "KON", "VVFIN", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Zu dem erschrecken mich die oft besorgten Posten,", "tokens": ["Zu", "dem", "er\u00b7schre\u00b7cken", "mich", "die", "oft", "be\u00b7sorg\u00b7ten", "Pos\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "PPER", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "was sich mein Vaterland anitzt noch lasse kosten,", "tokens": ["was", "sich", "mein", "Va\u00b7ter\u00b7land", "a\u00b7nitzt", "noch", "las\u00b7se", "kos\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "PPOSAT", "NN", "ADV", "ADV", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "um nur verderbt zu sein; es freue sich der Pein", "tokens": ["um", "nur", "ver\u00b7derbt", "zu", "sein", ";", "es", "freu\u00b7e", "sich", "der", "Pein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADV", "ADJD", "PTKZU", "VAINF", "$.", "PPER", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "und wolle noch nicht tot in seinem Tode sein", "tokens": ["und", "wol\u00b7le", "noch", "nicht", "tot", "in", "sei\u00b7nem", "To\u00b7de", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "PTKNEG", "ADJD", "APPR", "PPOSAT", "NN", "PPOSAT"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "So gro\u00dfe Lust ich vor mich hatte weg zu machen,", "tokens": ["So", "gro\u00b7\u00dfe", "Lust", "ich", "vor", "mich", "hat\u00b7te", "weg", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PPER", "APPR", "PPER", "VAFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "um des Gradiven Zorn von fernen zu verlachen:", "tokens": ["um", "des", "Gra\u00b7di\u00b7ven", "Zorn", "von", "fer\u00b7nen", "zu", "ver\u00b7la\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "APPR", "ADJA", "PTKZU", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.27": {"text": "Zeit, Eltern, Vaterland, Studiren, Ehr' und Gut,", "tokens": ["Zeit", ",", "El\u00b7tern", ",", "Va\u00b7ter\u00b7land", ",", "Stu\u00b7di\u00b7ren", ",", "Ehr'", "und", "Gut", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "das schlug ich Alles aus f\u00fcr diese Handvoll Blut,", "tokens": ["das", "schlug", "ich", "Al\u00b7les", "aus", "f\u00fcr", "die\u00b7se", "Hand\u00b7voll", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIS", "APPR", "APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "die mir doch hier verdirbt; ich dachte: La\u00df es fahren,", "tokens": ["die", "mir", "doch", "hier", "ver\u00b7dirbt", ";", "ich", "dach\u00b7te", ":", "La\u00df", "es", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVFIN", "$.", "PPER", "VVFIN", "$.", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "gib alles hin f\u00fcr dich! Kanst du dich nur ersparen,", "tokens": ["gib", "al\u00b7les", "hin", "f\u00fcr", "dich", "!", "Kanst", "du", "dich", "nur", "er\u00b7spa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIS", "ADV", "APPR", "PPER", "$.", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "so hastu satt an dir. Beschau inde\u00df die Welt", "tokens": ["so", "has\u00b7tu", "satt", "an", "dir", ".", "Be\u00b7schau", "in\u00b7de\u00df", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJD", "APPR", "PPER", "$.", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "und denke, da\u00df man viel von Vielgereisten h\u00e4lt!", "tokens": ["und", "den\u00b7ke", ",", "da\u00df", "man", "viel", "von", "Viel\u00b7ge\u00b7reis\u00b7ten", "h\u00e4lt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PIS", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "inzwischen wird es gut \u2013, so gro\u00df und gr\u00f6\u00dfer Grauen", "tokens": ["in\u00b7zwi\u00b7schen", "wird", "es", "gut", "\u2013", ",", "so", "gro\u00df", "und", "gr\u00f6\u00b7\u00dfer", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$(", "$,", "ADV", "ADJD", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "bef\u00e4llt mich itzund nun, da ich soll n\u00e4her schauen", "tokens": ["be\u00b7f\u00e4llt", "mich", "it\u00b7zund", "nun", ",", "da", "ich", "soll", "n\u00e4\u00b7her", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "KOUS", "PPER", "VMFIN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "mein durch f\u00fcnf ganze Jahr' entschlagnes Mei\u00dfner-Land,", "tokens": ["mein", "durch", "f\u00fcnf", "gan\u00b7ze", "Jahr'", "ent\u00b7schlag\u00b7nes", "Mei\u00df\u00b7ner\u00b7Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "APPR", "CARD", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "das von der Kriegesglut zu Pulver wird verbrant.", "tokens": ["das", "von", "der", "Krie\u00b7ges\u00b7glut", "zu", "Pul\u00b7ver", "wird", "ver\u00b7brant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ach, hast du noch nicht satt vom Schaden deiner Feinde?", "tokens": ["Ach", ",", "hast", "du", "noch", "nicht", "satt", "vom", "Scha\u00b7den", "dei\u00b7ner", "Fein\u00b7de", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Was, Landsman, schl\u00e4gst du dich denn nun mit deinem Freunde?", "tokens": ["Was", ",", "Lands\u00b7man", ",", "schl\u00e4gst", "du", "dich", "denn", "nun", "mit", "dei\u00b7nem", "Freun\u00b7de", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "NN", "$,", "VVFIN", "PPER", "PRF", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Verzeih mirs, hastu Recht! Ich aber kan mehr nicht", "tokens": ["Ver\u00b7zeih", "mirs", ",", "has\u00b7tu", "Recht", "!", "Ich", "a\u00b7ber", "kan", "mehr", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "NE", "$,", "VAFIN", "NN", "$.", "PPER", "ADV", "VMFIN", "ADV", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "als um dich sein betr\u00fcbt von wegen meiner Pflicht.", "tokens": ["als", "um", "dich", "sein", "be\u00b7tr\u00fcbt", "von", "we\u00