{"textgrid.poem.61536": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das soll dir nicht verhohlen sein,", "tokens": ["Das", "soll", "dir", "nicht", "ver\u00b7hoh\u00b7len", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ormuz und Ahriman hausen im Wein;", "tokens": ["Or\u00b7muz", "und", "Ah\u00b7ri\u00b7man", "hau\u00b7sen", "im", "Wein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Unter dem St\u00f6psel im Goldenen, Blanken", "tokens": ["Un\u00b7ter", "dem", "St\u00f6p\u00b7sel", "im", "Gol\u00b7de\u00b7nen", ",", "Blan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Schweben die freudigen Lichtgedanken;", "tokens": ["Schwe\u00b7ben", "die", "freu\u00b7di\u00b7gen", "Licht\u00b7ge\u00b7dan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Ahriman kauert am Boden der Flasche", "tokens": ["Ah\u00b7ri\u00b7man", "kau\u00b7ert", "am", "Bo\u00b7den", "der", "Fla\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Und lauert, da\u00df er dich erhasche.", "tokens": ["Und", "lau\u00b7ert", ",", "da\u00df", "er", "dich", "er\u00b7ha\u00b7sche", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}