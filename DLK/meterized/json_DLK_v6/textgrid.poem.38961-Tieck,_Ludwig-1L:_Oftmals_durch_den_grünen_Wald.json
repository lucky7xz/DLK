{"textgrid.poem.38961": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Oftmals durch den gr\u00fcnen Wald", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oftmals durch den gr\u00fcnen Wald", "tokens": ["Oft\u00b7mals", "durch", "den", "gr\u00fc\u00b7nen", "Wald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine liebe Stimme schallt,", "tokens": ["Ei\u00b7ne", "lie\u00b7be", "Stim\u00b7me", "schallt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meinen Nahmen ruft es,", "tokens": ["Mei\u00b7nen", "Nah\u00b7men", "ruft", "es", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ach! mich f\u00e4llt so pl\u00f6tzlich dann", "tokens": ["Ach", "!", "mich", "f\u00e4llt", "so", "pl\u00f6tz\u00b7lich", "dann"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "ADV", "ADJD", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Uebergro\u00dfe Freude an;", "tokens": ["Ue\u00b7ber\u00b7gro\u00b7\u00dfe", "Freu\u00b7de", "an", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist es die Geliebte?", "tokens": ["Ist", "es", "die", "Ge\u00b7lieb\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wieder glaub' ich sie zu sehn", "tokens": ["Wie\u00b7der", "glaub'", "ich", "sie", "zu", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor mir durch die B\u00fcsche gehn:", "tokens": ["Vor", "mir", "durch", "die", "B\u00fc\u00b7sche", "gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O mein Herz, wie treibt es!", "tokens": ["O", "mein", "Herz", ",", "wie", "treibt", "es", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "PWAV", "VVFIN", "PPER", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Aber dann verrauscht im Wind", "tokens": ["A\u00b7ber", "dann", "ver\u00b7rauscht", "im", "Wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das Gebilde so geschwind;", "tokens": ["Das", "Ge\u00b7bil\u00b7de", "so", "ge\u00b7schwind", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "M\u00fcde steh ich sinnend.", "tokens": ["M\u00fc\u00b7de", "steh", "ich", "sin\u00b7nend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wenn der Bach vom Felsen springt,", "tokens": ["Wenn", "der", "Bach", "vom", "Fel\u00b7sen", "springt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mein' ich da\u00df es mir gelingt,", "tokens": ["Mein'", "ich", "da\u00df", "es", "mir", "ge\u00b7lingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich bin nicht s\u00e4umig:", "tokens": ["Und", "ich", "bin", "nicht", "s\u00e4u\u00b7mig", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Stolz sieht mich der Felsen an,", "tokens": ["Stolz", "sieht", "mich", "der", "Fel\u00b7sen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ich schau ihn wieder an", "tokens": ["Und", "ich", "schau", "ihn", "wie\u00b7der", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eben auch nicht freundlich.", "tokens": ["E\u00b7ben", "auch", "nicht", "freund\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Blumen, die am Wege bl\u00fchn,", "tokens": ["Blu\u00b7men", ",", "die", "am", "We\u00b7ge", "bl\u00fchn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seh' ich ihren Nahmen ziehn,", "tokens": ["Seh'", "ich", "ih\u00b7ren", "Nah\u00b7men", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder Baum rauscht Lila:", "tokens": ["Je\u00b7der", "Baum", "rauscht", "Li\u00b7la", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "NE", "$."], "meter": "+-++-+", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Was habt ihr damit gethan?", "tokens": ["Was", "habt", "ihr", "da\u00b7mit", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PAV", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Bringt mich auf die rechte Bahn!", "tokens": ["Bringt", "mich", "auf", "die", "rech\u00b7te", "Bahn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Keine Kunst ist Necken.", "tokens": ["Kei\u00b7ne", "Kunst", "ist", "Ne\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Aber alles macht mich irr',", "tokens": ["A\u00b7ber", "al\u00b7les", "macht", "mich", "irr'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer dummer vom Gewirr,", "tokens": ["Im\u00b7mer", "dum\u00b7mer", "vom", "Ge\u00b7wirr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seh' ich kaum den Weg mehr;", "tokens": ["Seh'", "ich", "kaum", "den", "Weg", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Werd' ich aber vor ihr stehn,", "tokens": ["Werd'", "ich", "a\u00b7ber", "vor", "ihr", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Will ich um so klarer sehn,", "tokens": ["Will", "ich", "um", "so", "kla\u00b7rer", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Oder gar erblinden.", "tokens": ["O\u00b7der", "gar", "er\u00b7blin\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}