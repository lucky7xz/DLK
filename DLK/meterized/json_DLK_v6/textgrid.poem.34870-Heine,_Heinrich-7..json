{"textgrid.poem.34870": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist der alte Tambourmajor,", "tokens": ["Das", "ist", "der", "al\u00b7te", "Tam\u00b7bour\u00b7ma\u00b7jor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie ist er jetzt herunter!", "tokens": ["Wie", "ist", "er", "jetzt", "her\u00b7un\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zur Kaiserzeit stand er in Flor,", "tokens": ["Zur", "Kai\u00b7ser\u00b7zeit", "stand", "er", "in", "Flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Da war er gl\u00fccklich und munter.", "tokens": ["Da", "war", "er", "gl\u00fcck\u00b7lich", "und", "mun\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Er balancierte den gro\u00dfen Stock,", "tokens": ["Er", "ba\u00b7lan\u00b7cier\u00b7te", "den", "gro\u00b7\u00dfen", "Stock", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Mit lachendem Gesichte;", "tokens": ["Mit", "la\u00b7chen\u00b7dem", "Ge\u00b7sich\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die silbernen Tressen auf seinem Rock,", "tokens": ["Die", "sil\u00b7ber\u00b7nen", "Tres\u00b7sen", "auf", "sei\u00b7nem", "Rock", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die gl\u00e4nzten im Sonnenlichte.", "tokens": ["Die", "gl\u00e4nz\u00b7ten", "im", "Son\u00b7nen\u00b7lich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wenn er mit Trommelwirbelschall", "tokens": ["Wenn", "er", "mit", "Trom\u00b7mel\u00b7wir\u00b7bel\u00b7schall"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Einzog in St\u00e4dten und St\u00e4dtchen,", "tokens": ["Ein\u00b7zog", "in", "St\u00e4d\u00b7ten", "und", "St\u00e4dt\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da schlug das Herz im Widerhall", "tokens": ["Da", "schlug", "das", "Herz", "im", "Wi\u00b7der\u00b7hall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Weibern und den M\u00e4dchen.", "tokens": ["Den", "Wei\u00b7bern", "und", "den", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er kam und sah und siegte leicht", "tokens": ["Er", "kam", "und", "sah", "und", "sieg\u00b7te", "leicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl \u00fcber alle Sch\u00f6nen;", "tokens": ["Wohl", "\u00fc\u00b7ber", "al\u00b7le", "Sch\u00f6\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sein schwarzer Schnurrbart wurde feucht", "tokens": ["Sein", "schwar\u00b7zer", "Schnurr\u00b7bart", "wur\u00b7de", "feucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von deutschen Frauentr\u00e4nen.", "tokens": ["Von", "deut\u00b7schen", "Frau\u00b7en\u00b7tr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wir mu\u00dften es dulden! In jedem Land,", "tokens": ["Wir", "mu\u00df\u00b7ten", "es", "dul\u00b7den", "!", "In", "je\u00b7dem", "Land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$.", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wo die fremden Eroberer kamen,", "tokens": ["Wo", "die", "frem\u00b7den", "Er\u00b7o\u00b7be\u00b7rer", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Der Kaiser die Herren \u00fcberwand,", "tokens": ["Der", "Kai\u00b7ser", "die", "Her\u00b7ren", "\u00fc\u00b7ber\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Tambourmajor die Damen.", "tokens": ["Der", "Tam\u00b7bour\u00b7ma\u00b7jor", "die", "Da\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Wir haben lange getragen das Leid,", "tokens": ["Wir", "ha\u00b7ben", "lan\u00b7ge", "ge\u00b7tra\u00b7gen", "das", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Geduldig wie deutsche Eichen,", "tokens": ["Ge\u00b7dul\u00b7dig", "wie", "deut\u00b7sche", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bis endlich die hohe Obrigkeit", "tokens": ["Bis", "end\u00b7lich", "die", "ho\u00b7he", "Ob\u00b7rig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Uns gab das Befreiungszeichen.", "tokens": ["Uns", "gab", "das", "Be\u00b7frei\u00b7ungs\u00b7zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Wie in der Kampfbahn der Auerochs", "tokens": ["Wie", "in", "der", "Kampf\u00b7bahn", "der", "Au\u00b7e\u00b7rochs"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Erhuben wir unsere H\u00f6rner,", "tokens": ["Er\u00b7hu\u00b7ben", "wir", "un\u00b7se\u00b7re", "H\u00f6r\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Entledigten uns des fr\u00e4nkischen Jochs", "tokens": ["Ent\u00b7le\u00b7dig\u00b7ten", "uns", "des", "fr\u00e4n\u00b7ki\u00b7schen", "Jochs"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und sangen die Lieder von K\u00f6rner.", "tokens": ["Und", "san\u00b7gen", "die", "Lie\u00b7der", "von", "K\u00f6r\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NE", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Entsetzliche Verse! sie klangen ins Ohr", "tokens": ["Ent\u00b7setz\u00b7li\u00b7che", "Ver\u00b7se", "!", "sie", "klan\u00b7gen", "ins", "Ohr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Gar schauderhaft den Tyrannen!", "tokens": ["Gar", "schau\u00b7der\u00b7haft", "den", "Ty\u00b7ran\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Kaiser und der Tambourmajor,", "tokens": ["Der", "Kai\u00b7ser", "und", "der", "Tam\u00b7bour\u00b7ma\u00b7jor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie flohen erschrocken von dannen.", "tokens": ["Sie", "flo\u00b7hen", "er\u00b7schro\u00b7cken", "von", "dan\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "APPR", "ADV", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.9": {"line.1": {"text": "Sie ernteten beide den S\u00fcndenlohn", "tokens": ["Sie", "ern\u00b7te\u00b7ten", "bei\u00b7de", "den", "S\u00fcn\u00b7den\u00b7lohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und nahmen ein schlechtes Ende.", "tokens": ["Und", "nah\u00b7men", "ein", "schlech\u00b7tes", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es fiel der Kaiser Napoleon", "tokens": ["Es", "fiel", "der", "Kai\u00b7ser", "Na\u00b7po\u00b7le\u00b7on"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NE"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den Briten in die H\u00e4nde.", "tokens": ["Den", "Bri\u00b7ten", "in", "die", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wohl auf der Insel Sankt Helena,", "tokens": ["Wohl", "auf", "der", "In\u00b7sel", "Sankt", "He\u00b7le\u00b7na", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie marterten ihn gar sch\u00e4ndlich;", "tokens": ["Sie", "mar\u00b7ter\u00b7ten", "ihn", "gar", "sch\u00e4nd\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Am Magenkrebse starb er da", "tokens": ["Am", "Ma\u00b7gen\u00b7kreb\u00b7se", "starb", "er", "da"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach langen Leiden endlich.", "tokens": ["Nach", "lan\u00b7gen", "Lei\u00b7den", "end\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.11": {"line.1": {"text": "Der Tambourmajor, er ward entsetzt", "tokens": ["Der", "Tam\u00b7bour\u00b7ma\u00b7jor", ",", "er", "ward", "ent\u00b7setzt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NE", "$,", "PPER", "VAFIN", "VVPP"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gleichfalls von seiner Stelle.", "tokens": ["Gleich\u00b7falls", "von", "sei\u00b7ner", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Um nicht zu verhungern, dient er jetzt", "tokens": ["Um", "nicht", "zu", "ver\u00b7hun\u00b7gern", ",", "dient", "er", "jetzt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "PTKNEG", "PTKZU", "VVINF", "$,", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Als Hausknecht in unserm Hotele.", "tokens": ["Als", "Haus\u00b7knecht", "in", "un\u00b7serm", "Ho\u00b7te\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-++-+-+--", "measure": "unknown.measure.tetra"}}, "stanza.12": {"line.1": {"text": "Er heizt den Ofen, er fegt den Topf,", "tokens": ["Er", "heizt", "den", "O\u00b7fen", ",", "er", "fegt", "den", "Topf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mu\u00df Holz und Wasser schleppen.", "tokens": ["Mu\u00df", "Holz", "und", "Was\u00b7ser", "schlep\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit seinem wackelnd greisen Kopf", "tokens": ["Mit", "sei\u00b7nem", "wa\u00b7ckelnd", "grei\u00b7sen", "Kopf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Keucht er herauf die Treppen.", "tokens": ["Keucht", "er", "her\u00b7auf", "die", "Trep\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Wenn mich der Fritz besucht, so kann", "tokens": ["Wenn", "mich", "der", "Fritz", "be\u00b7sucht", ",", "so", "kann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NE", "VVPP", "$,", "ADV", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er nicht den Spa\u00df sich versagen,", "tokens": ["Er", "nicht", "den", "Spa\u00df", "sich", "ver\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Den drollig schlotternd langen Mann", "tokens": ["Den", "drol\u00b7lig", "schlot\u00b7ternd", "lan\u00b7gen", "Mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu nergeln und zu plagen.", "tokens": ["Zu", "ner\u00b7geln", "und", "zu", "pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.14": {"line.1": {"text": "\u00bbla\u00df ab mit Sp\u00f6ttelei'n, o Fritz!", "tokens": ["\u00bb", "la\u00df", "ab", "mit", "Sp\u00f6t\u00b7te\u00b7lei'n", ",", "o", "Fritz", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PTKVZ", "APPR", "NN", "$,", "FM", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es ziemt Germanias S\u00f6hnen", "tokens": ["Es", "ziemt", "Ger\u00b7ma\u00b7ni\u00b7as", "S\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wohl nimmermehr, mit schlechtem Witz", "tokens": ["Wohl", "nim\u00b7mer\u00b7mehr", ",", "mit", "schlech\u00b7tem", "Witz"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gefallene Gr\u00f6\u00dfe zu h\u00f6hnen.", "tokens": ["Ge\u00b7fal\u00b7le\u00b7ne", "Gr\u00f6\u00b7\u00dfe", "zu", "h\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.15": {"line.1": {"text": "Du solltest mit Piet\u00e4t, mich deucht,", "tokens": ["Du", "soll\u00b7test", "mit", "