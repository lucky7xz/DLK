{"textgrid.poem.57977": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "15. Die herrlichste Zeitk\u00fcrzung", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00e4llt hier die Zeit schon lang den Menschen dieser Zeit,", "tokens": ["F\u00e4llt", "hier", "die", "Zeit", "schon", "lang", "den", "Men\u00b7schen", "die\u00b7ser", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "ADJD", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie lang wird ihnen sein die lange Ewigkeit!", "tokens": ["Wie", "lang", "wird", "ih\u00b7nen", "sein", "die", "lan\u00b7ge", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "PPOSAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich, ob ich stille sitz', hab immer g'nug zu tun;", "tokens": ["Ich", ",", "ob", "ich", "stil\u00b7le", "sitz'", ",", "hab", "im\u00b7mer", "g'\u00b7nug", "zu", "tun", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "PPER", "VVFIN", "PTKVZ", "$,", "VAFIN", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die Ewigkeit scheint kurz dem, der in Gott kann ruhn.", "tokens": ["Die", "E\u00b7wig\u00b7keit", "scheint", "kurz", "dem", ",", "der", "in", "Gott", "kann", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "$,", "PRELS", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}