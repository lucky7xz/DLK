{"dta.poem.3113": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "158.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Lehre, wenn sie dir von Herzen widerstrebt,", "tokens": ["Die", "Leh\u00b7re", ",", "wenn", "sie", "dir", "von", "Her\u00b7zen", "wi\u00b7der\u00b7strebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du nur f\u00fchlest da\u00df sie dem im Herzen lebt,", "tokens": ["Wenn", "du", "nur", "f\u00fch\u00b7lest", "da\u00df", "sie", "dem", "im", "Her\u00b7zen", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "KOUS", "PPER", "ART", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der diese Lehre lehrt, mu\u00dft du sie gelten lassen,", "tokens": ["Der", "die\u00b7se", "Leh\u00b7re", "lehrt", ",", "mu\u00dft", "du", "sie", "gel\u00b7ten", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "VVFIN", "$,", "VMFIN", "PPER", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und suchen deinem Sinn sie irgend anzupassen.", "tokens": ["Und", "su\u00b7chen", "dei\u00b7nem", "Sinn", "sie", "ir\u00b7gend", "an\u00b7zu\u00b7pas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PPER", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Belebend \u00fcberall ist der Begeistrung Hauch,", "tokens": ["Be\u00b7le\u00b7bend", "\u00fc\u00b7be\u00b7rall", "ist", "der", "Be\u00b7geis\u00b7trung", "Hauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mag begeistern dich, wenn zu was anderm auch.", "tokens": ["Und", "mag", "be\u00b7geis\u00b7tern", "dich", ",", "wenn", "zu", "was", "an\u00b7derm", "auch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVINF", "PPER", "$,", "KOUS", "APPR", "PRELS", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}