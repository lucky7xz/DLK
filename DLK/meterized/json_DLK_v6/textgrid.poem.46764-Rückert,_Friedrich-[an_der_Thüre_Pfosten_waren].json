{"textgrid.poem.46764": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[an der Th\u00fcre Pfosten waren]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An der Th\u00fcre Pfosten waren", "tokens": ["An", "der", "Th\u00fc\u00b7re", "Pfos\u00b7ten", "wa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Angezeichnet eure Ma\u00dfe,", "tokens": ["An\u00b7ge\u00b7zeich\u00b7net", "eu\u00b7re", "Ma\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wir freuten uns zu sehen,", "tokens": ["Und", "wir", "freu\u00b7ten", "uns", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie in jedem halben Jahre", "tokens": ["Wie", "in", "je\u00b7dem", "hal\u00b7ben", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihr um eine halbe Spanne", "tokens": ["Ihr", "um", "ei\u00b7ne", "hal\u00b7be", "Span\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Oder eine ganze wuchset.", "tokens": ["O\u00b7der", "ei\u00b7ne", "gan\u00b7ze", "wuch\u00b7set", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Scheidend trat der Tod nun zwischen", "tokens": ["Schei\u00b7dend", "trat", "der", "Tod", "nun", "zwi\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unsre Freud' und euer Wachsen,", "tokens": ["Uns\u00b7re", "Freud'", "und", "eu\u00b7er", "Wach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und nicht werden an den Pfosten", "tokens": ["Und", "nicht", "wer\u00b7den", "an", "den", "Pfos\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "VAINF", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Eure Ma\u00dfe h\u00f6her steigen.", "tokens": ["Eu\u00b7re", "Ma\u00b7\u00dfe", "h\u00f6\u00b7her", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Aber gleichsam um auf eine", "tokens": ["A\u00b7ber", "gleich\u00b7sam", "um", "auf", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "APPR", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Art \u2013 ach eine schlimme Art \u2013 uns", "tokens": ["Art", "\u2013", "ach", "ei\u00b7ne", "schlim\u00b7me", "Art", "\u2013", "uns"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$(", "XY", "ART", "ADJA", "NN", "$(", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Zu entsch\u00e4d'gen, hat der W\u00fcrger", "tokens": ["Zu", "ent\u00b7sch\u00e4d'\u00b7gen", ",", "hat", "der", "W\u00fcr\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Euch nun auf die Sterbebettchen", "tokens": ["Euch", "nun", "auf", "die", "Ster\u00b7be\u00b7bett\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Hingestreckt um eine ganze", "tokens": ["Hin\u00b7ge\u00b7streckt", "um", "ei\u00b7ne", "gan\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Spanne l\u00e4nger als ihr lebtet.", "tokens": ["Span\u00b7ne", "l\u00e4n\u00b7ger", "als", "ihr", "leb\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KOKOM", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Soll das etwa daf\u00fcr gelten", "tokens": ["Soll", "das", "et\u00b7wa", "da\u00b7f\u00fcr", "gel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PDS", "ADV", "PAV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Euern Eltern, um ein ganzes", "tokens": ["Eu\u00b7ern", "El\u00b7tern", ",", "um", "ein", "gan\u00b7zes"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KOUI", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Oder halbes Jahr des Lebens", "tokens": ["O\u00b7der", "hal\u00b7bes", "Jahr", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Euch im Tode nachzumessen?", "tokens": ["Euch", "im", "To\u00b7de", "nach\u00b7zu\u00b7mes\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}