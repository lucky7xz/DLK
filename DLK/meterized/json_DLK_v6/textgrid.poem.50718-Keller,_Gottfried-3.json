{"textgrid.poem.50718": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "3", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Glase bl\u00fcht ein frischer Rosenstrau\u00df,", "tokens": ["Im", "Gla\u00b7se", "bl\u00fcht", "ein", "fri\u00b7scher", "Ro\u00b7sen\u00b7strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Daneben webt ein J\u00fcnglingsleben aus;", "tokens": ["Da\u00b7ne\u00b7ben", "webt", "ein", "J\u00fcng\u00b7lings\u00b7le\u00b7ben", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ins Zimmer bricht der volle Abendglanz \u2013", "tokens": ["Ins", "Zim\u00b7mer", "bricht", "der", "vol\u00b7le", "A\u00b7bend\u00b7glanz", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Welch sch\u00f6nes Bild f\u00fcr einen Totentanz!", "tokens": ["Welch", "sch\u00f6\u00b7nes", "Bild", "f\u00fcr", "ei\u00b7nen", "To\u00b7ten\u00b7tanz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Von rotem Golde trieft das Sommerland,", "tokens": ["Von", "ro\u00b7tem", "Gol\u00b7de", "trieft", "das", "Som\u00b7mer\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Reb am Fenster und die Kammerwand,", "tokens": ["Die", "Reb", "am", "Fens\u00b7ter", "und", "die", "Kam\u00b7mer\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Todeskranke und sein Linnentuch,", "tokens": ["Der", "To\u00b7des\u00b7kran\u00b7ke", "und", "sein", "Lin\u00b7nen\u00b7tuch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Rot bl\u00fcht das Pf\u00e4fflein betend aus dem Buch.", "tokens": ["Rot", "bl\u00fcht", "das", "Pf\u00e4f\u00b7flein", "be\u00b7tend", "aus", "dem", "Buch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du armer Schwarzer, sag, was willst du hier?", "tokens": ["Du", "ar\u00b7mer", "Schwar\u00b7zer", ",", "sag", ",", "was", "willst", "du", "hier", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "VVFIN", "$,", "PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sieh! nicht einmal die Blumen horchen dir.", "tokens": ["Sieh", "!", "nicht", "ein\u00b7mal", "die", "Blu\u00b7men", "hor\u00b7chen", "dir", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PTKNEG", "ADV", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nach Westen neigt sich alles insgesamt,", "tokens": ["Nach", "Wes\u00b7ten", "neigt", "sich", "al\u00b7les", "ins\u00b7ge\u00b7samt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Sonne \u00fcbt das heil'ge Totenamt.", "tokens": ["Die", "Son\u00b7ne", "\u00fcbt", "das", "heil'\u00b7ge", "To\u00b7ten\u00b7amt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie abendsch\u00f6n das Haupt des Kranken gl\u00fcht,", "tokens": ["Wie", "a\u00b7bend\u00b7sch\u00f6n", "das", "Haupt", "des", "Kran\u00b7ken", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df kaum man ahnt, wie wei\u00df der Tod drauf bl\u00fcht!", "tokens": ["Da\u00df", "kaum", "man", "ahnt", ",", "wie", "wei\u00df", "der", "Tod", "drauf", "bl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "VVFIN", "$,", "PWAV", "VVFIN", "ART", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sein Nachtmahlkelch ist lautres Sonnengold.", "tokens": ["Sein", "Nacht\u00b7mahl\u00b7kelch", "ist", "laut\u00b7res", "Son\u00b7nen\u00b7gold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie schl\u00fcrft er durstig diesen Liebessold!", "tokens": ["Wie", "schl\u00fcrft", "er", "durs\u00b7tig", "die\u00b7sen", "Lie\u00b7bes\u00b7sold", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und scheidend winkt der letzte Lebensstrahl,", "tokens": ["Und", "schei\u00b7dend", "winkt", "der", "letz\u00b7te", "Le\u00b7bens\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erkaltet und vergl\u00fcht sind Berg und Tal;", "tokens": ["Er\u00b7kal\u00b7tet", "und", "ver\u00b7gl\u00fcht", "sind", "Berg", "und", "Tal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Rosen sind geblieben frisch und rot,", "tokens": ["Die", "Ro\u00b7sen", "sind", "ge\u00b7blie\u00b7ben", "frisch", "und", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Jedoch das Menschenkind ist bleich und tot!", "tokens": ["Je\u00b7doch", "das", "Men\u00b7schen\u00b7kind", "ist", "bleich", "und", "tot", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}