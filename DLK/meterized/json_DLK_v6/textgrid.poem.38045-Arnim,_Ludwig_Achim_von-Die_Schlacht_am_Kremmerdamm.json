{"textgrid.poem.38045": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Die Schlacht am Kremmerdamm", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Barnim de fast l\u00fctke Mann,", "tokens": ["Als", "Bar\u00b7nim", "de", "fast", "l\u00fct\u00b7ke", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "ADV", "ADJA", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Averst im Kriege nich quade,", "tokens": ["A\u00b7verst", "im", "Krie\u00b7ge", "nich", "qua\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PTKNEG", "ADV", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Am langen Damme kam heran,", "tokens": ["Am", "lan\u00b7gen", "Dam\u00b7me", "kam", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ging he flietig tho Rade.", "tokens": ["Ging", "he", "flie\u00b7tig", "tho", "Ra\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "FM", "FM", "FM", "FM", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "He sprack: Dat is en garstig Lock,", "tokens": ["He", "sprack", ":", "Dat", "is", "en", "gars\u00b7tig", "Lock", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "FM", "FM", "FM", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da m\u00fctten wie nich dorchrieden,", "tokens": ["Da", "m\u00fct\u00b7ten", "wie", "nich", "dorch\u00b7rie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "KOKOM", "PTKNEG", "ADV", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Et m\u00f6gt uns kosten unsen Rock,", "tokens": ["Et", "m\u00f6gt", "uns", "kos\u00b7ten", "un\u00b7sen", "Rock", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie willen man hier bliven.", "tokens": ["Wie", "wil\u00b7len", "man", "hier", "bli\u00b7ven", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie willen schrieven ut de St\u00fcr,", "tokens": ["Wie", "wil\u00b7len", "schrie\u00b7ven", "ut", "de", "St\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJA", "FM", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "De uns de nich will geven,", "tokens": ["De", "uns", "de", "nich", "will", "ge\u00b7ven", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "NE", "PTKNEG", "VMFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den willen wie br\u00fcden mit det F\u00fcr,", "tokens": ["Den", "wil\u00b7len", "wie", "br\u00fc\u00b7den", "mit", "det", "F\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Un nah det Veh em streven.", "tokens": ["Un", "nah", "det", "Veh", "em", "stre\u00b7ven", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}}, "stanza.4": {"line.1": {"text": "Det Rath gefehl em allen wol,", "tokens": ["Det", "Rath", "ge\u00b7fehl", "em", "al\u00b7len", "wol", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "ADJA", "PIAT", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Se fingen an tho grawen,", "tokens": ["Se", "fin\u00b7gen", "an", "tho", "gra\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NE", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Se mackten in de Erden holl,", "tokens": ["Se", "mack\u00b7ten", "in", "de", "Er\u00b7den", "holl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NE", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Brachten det unnerst baven.", "tokens": ["Brach\u00b7ten", "det", "un\u00b7nerst", "ba\u00b7ven", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "ADV", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Marckgraf Ludwig de tappre Held,", "tokens": ["Marck\u00b7graf", "Lud\u00b7wig", "de", "tapp\u00b7re", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Heelt up den Kremmschen Huwen,", "tokens": ["Heelt", "up", "den", "Kremm\u00b7schen", "Hu\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Un dachte, dat sick da int Feld,", "tokens": ["Un", "dach\u00b7te", ",", "dat", "sick", "da", "int", "Feld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PRELS", "PRF", "ADV", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "De Pamern sch\u00f6llen truven.", "tokens": ["De", "Pa\u00b7mern", "sch\u00f6l\u00b7len", "tru\u00b7ven", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da averst kener kam herv\u00e4r,", "tokens": ["Da", "a\u00b7verst", "ke\u00b7ner", "kam", "her\u00b7v\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Liet he rupen sienen Peter,", "tokens": ["Liet", "he", "ru\u00b7pen", "sie\u00b7nen", "Pe\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PPER", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Un sprack: Krieg diene Trumpet her,", "tokens": ["Un", "sprack", ":", "Krieg", "die\u00b7ne", "Trum\u00b7pet", "her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "NN", "PDAT", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Ried hen, als en Trumpeter.", "tokens": ["Ried", "hen", ",", "als", "en", "Trum\u00b7pe\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "KOUS", "ADJA", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.7": {"line.1": {"text": "Det segge Hertog Barnim an,", "tokens": ["Det", "seg\u00b7ge", "Her\u00b7tog", "Bar\u00b7nim", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich hedde grot Verlangen,", "tokens": ["Ich", "hed\u00b7de", "grot", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Em as den Gast, un sienen Mann,", "tokens": ["Em", "as", "den", "Gast", ",", "un", "sie\u00b7nen", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "$,", "FM", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Felde tho emfangen.", "tokens": ["Im", "Fel\u00b7de", "tho", "em\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wo averst em det nich behagt,", "tokens": ["Wo", "a\u00b7verst", "em", "det", "nich", "be\u00b7hagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJA", "PDS", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So will ick em thospr\u00e4cken,", "tokens": ["So", "will", "ick", "em", "thos\u00b7pr\u00e4\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Un ock im Luge sien unverzagt,", "tokens": ["Un", "ock", "im", "Lu\u00b7ge", "sien", "un\u00b7ver\u00b7zagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPRART", "NN", "VVFIN", "ADJD", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "De Lanz mit em tho br\u00e4cken.", "tokens": ["De", "Lanz", "mit", "em", "tho", "br\u00e4\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "De Hertog sprack: He were da,", "tokens": ["De", "Her\u00b7tog", "sprack", ":", "He", "we\u00b7re", "da", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "$.", "NE", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Un lichtlich ock tho finnen,", "tokens": ["Un", "licht\u00b7lich", "ock", "tho", "fin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Det Sp\u00f6t det st\u00fcnde op de Wah,", "tokens": ["Det", "Sp\u00f6t", "det", "st\u00fcn\u00b7de", "op", "de", "Wah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Woll siehn, we werd gewinnen.", "tokens": ["Woll", "siehn", ",", "we", "werd", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "PWAV", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Drup ging et up den Damm hinab,", "tokens": ["Drup", "ging", "et", "up", "den", "Damm", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "NE", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "De was vull luter K\u00f6ppe,", "tokens": ["De", "was", "vull", "lu\u00b7ter", "K\u00f6p\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Et gaf da manchen harten Knap,", "tokens": ["Et", "gaf", "da", "man\u00b7chen", "har\u00b7ten", "Knap", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "De Schall ging in de Z\u00f6ppe.", "tokens": ["De", "Schall", "ging", "in", "de", "Z\u00f6p\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "De M\u00e4rcker kunnen nich bestahn,", "tokens": ["De", "M\u00e4r\u00b7cker", "kun\u00b7nen", "nich", "be\u00b7stahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VMFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "De Lug was ehr Verderven,", "tokens": ["De", "Lug", "was", "ehr", "Ver\u00b7der\u00b7ven", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PWS", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da mu\u00dfte mancher liggen gahn,", "tokens": ["Da", "mu\u00df\u00b7te", "man\u00b7cher", "lig\u00b7gen", "gahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Un ahne Wunne sterven.", "tokens": ["Un", "ah\u00b7ne", "Wun\u00b7ne", "ster\u00b7ven", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "VVINF", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}}, "stanza.12": {"line.1": {"text": "Drum weken se up d\u00fcsse Siet,", "tokens": ["Drum", "we\u00b7ken", "se", "up", "d\u00fcs\u00b7se", "Siet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Un menen da tho fechten;", "tokens": ["Un", "me\u00b7nen", "da", "tho", "fech\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "NE", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "De Pamer folgt im vullen Tritt,", "tokens": ["De", "Pa\u00b7mer", "folgt", "im", "vul\u00b7len", "Tritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schlog Heeren mit den Knechten.", "tokens": ["Schlog", "Hee\u00b7ren", "mit", "den", "Knech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Tho Cremmen ging em det nich an,", "tokens": ["Tho", "Crem\u00b7men", "ging", "em", "det", "nich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "XY", "PDS", "PTKNEG", "PTKVZ", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "He mu\u00dfte buten blieven,", "tokens": ["He", "mu\u00df\u00b7te", "bu\u00b7ten", "blie\u00b7ven", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Det Fotvolck stund da Mann v\u00f6r Mann,", "tokens": ["Det", "Fot\u00b7volck", "stund", "da", "Mann", "v\u00f6r", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ADV", "NN", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hulp em thor\u00fcgge driewen.", "tokens": ["Hulp", "em", "tho\u00b7r\u00fcg\u00b7ge", "drie\u00b7wen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.14": {"line.1": {"text": "Se schoten up de Strat hinut,", "tokens": ["Se", "scho\u00b7ten", "up", "de", "Strat", "hi\u00b7nut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "De men van Pamern krewelt,", "tokens": ["De", "men", "van", "Pa\u00b7mern", "kre\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "VVFIN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Un f\u00f6hlen em so up de Hut,", "tokens": ["Un", "f\u00f6h\u00b7len", "em", "so", "up", "de", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Det em det Harte wewelt.", "tokens": ["Det", "em", "det", "Har\u00b7te", "we\u00b7welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.15": {"line.1": {"text": "Det, sprack Schwerin, deit hier ken got,", "tokens": ["Det", ",", "sprack", "Schwe\u00b7rin", ",", "deit", "hier", "ken", "got", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "NN", "$,", "ADV", "ADV", "VVFIN", "NE", "$,"], "meter": "+--+-+-+", "measure":