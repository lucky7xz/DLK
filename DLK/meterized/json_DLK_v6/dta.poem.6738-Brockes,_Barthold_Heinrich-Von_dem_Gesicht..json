{"dta.poem.6738": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Apffel ist die Th\u00fcr', durch schneller Feder Schliessen", "tokens": ["Der", "Apf\u00b7fel", "ist", "die", "Th\u00fcr'", ",", "durch", "schnel\u00b7ler", "Fe\u00b7der", "Schlies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Oeffnen, f\u00fchrt das Aug\u2019, indem es bald den Ort,", "tokens": ["Und", "Oeff\u00b7nen", ",", "f\u00fchrt", "das", "Aug'", ",", "in\u00b7dem", "es", "bald", "den", "Ort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bald die Figur ver\u00e4ndert, immerfort", "tokens": ["Bald", "die", "Fi\u00b7gur", "ver\u00b7\u00e4n\u00b7dert", ",", "im\u00b7mer\u00b7fort"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ART", "NN", "VVPP", "$,", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Strahlen bey sich ein, die allezeit drauf schiessen.", "tokens": ["Die", "Strah\u00b7len", "bey", "sich", "ein", ",", "die", "al\u00b7le\u00b7zeit", "drauf", "schies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "PTKVZ", "$,", "PRELS", "ADV", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}