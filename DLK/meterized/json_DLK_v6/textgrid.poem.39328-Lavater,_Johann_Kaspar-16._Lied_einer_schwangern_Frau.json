{"textgrid.poem.39328": {"metadata": {"author": {"name": "Lavater, Johann Kaspar", "birth": "N.A.", "death": "N.A."}, "title": "16. Lied einer schwangern Frau", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unter meinem Herzen schwebt,", "tokens": ["Un\u00b7ter", "mei\u00b7nem", "Her\u00b7zen", "schwebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Athmet sanft ein junges Leben.", "tokens": ["Ath\u00b7met", "sanft", "ein", "jun\u00b7ges", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Du, der jedes Herz belebt,", "tokens": ["Du", ",", "der", "je\u00b7des", "Herz", "be\u00b7lebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Du vertrautest mir dies Leben.", "tokens": ["Du", "ver\u00b7trau\u00b7test", "mir", "dies", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PDS", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dein ist's, bester Vater, dein,", "tokens": ["Dein", "ist's", ",", "bes\u00b7ter", "Va\u00b7ter", ",", "dein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NE", "$,", "ADJA", "NN", "$,", "PPOSAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Vater aller, mehr als mein.", "tokens": ["Va\u00b7ter", "al\u00b7ler", ",", "mehr", "als", "mein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "$,", "PIAT", "KOKOM", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dein, der alles giebt und thut,", "tokens": ["Dein", ",", "der", "al\u00b7les", "giebt", "und", "thut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$,", "PRELS", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein ist alles Gro\u00df' und Kleine!", "tokens": ["Dein", "ist", "al\u00b7les", "Gro\u00df'", "und", "Klei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PIAT", "NN", "KON", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dein das zarte Fleisch und Blut!", "tokens": ["Dein", "das", "zar\u00b7te", "Fleisch", "und", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dein die werdenden Gebeine!", "tokens": ["Dein", "die", "wer\u00b7den\u00b7den", "Ge\u00b7bei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "An mein Herz, o Vater, band", "tokens": ["An", "mein", "Herz", ",", "o", "Va\u00b7ter", ",", "band"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "FM", "NN", "$,", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dies mein Herzchen deine Hand!", "tokens": ["Dies", "mein", "Herz\u00b7chen", "dei\u00b7ne", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dank sinkt dir die Seele zu!", "tokens": ["Dank", "sinkt", "dir", "die", "See\u00b7le", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wunderbarer, bester Geber!", "tokens": ["Wun\u00b7der\u00b7ba\u00b7rer", ",", "bes\u00b7ter", "Ge\u00b7ber", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Allenthalben wirkest du", "tokens": ["Al\u00b7len\u00b7thal\u00b7ben", "wir\u00b7kest", "du"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unerforschlicher Beleber!", "tokens": ["Un\u00b7er\u00b7for\u00b7schli\u00b7cher", "Be\u00b7le\u00b7ber", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Nimm, weil's noch mein Kind nicht kann,", "tokens": ["Nimm", ",", "weil's", "noch", "mein", "Kind", "nicht", "kann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ADV", "PPOSAT", "NN", "PTKNEG", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meinen Preis f\u00fcr seinen an.", "tokens": ["Mei\u00b7nen", "Preis", "f\u00fcr", "sei\u00b7nen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sch\u00f6pfer, Preis, da\u00df du es schufst!", "tokens": ["Sch\u00f6p\u00b7fer", ",", "Preis", ",", "da\u00df", "du", "es", "schufst", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dank, weil du es schon bewahrtest,", "tokens": ["Dank", ",", "weil", "du", "es", "schon", "be\u00b7wahr\u00b7test", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus der Nacht zum Licht es rufst,", "tokens": ["Aus", "der", "Nacht", "zum", "Licht", "es", "rufst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihm auch bald dich offenbarest,", "tokens": ["Ihm", "auch", "bald", "dich", "of\u00b7fen\u00b7ba\u00b7rest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df du deinen Geist ihm giebst,", "tokens": ["Da\u00df", "du", "dei\u00b7nen", "Geist", "ihm", "giebst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Du es mehr als Mutter liebst!", "tokens": ["Du", "es", "mehr", "als", "Mut\u00b7ter", "liebst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ADV", "KOUS", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Kann ich's sorglos tragen, ich?", "tokens": ["Kann", "ich's", "sorg\u00b7los", "tra\u00b7gen", ",", "ich", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PIS", "ADJD", "VVINF", "$,", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kann des Lieblings ich vergessen?", "tokens": ["Kann", "des", "Lieb\u00b7lings", "ich", "ver\u00b7ges\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch k\u00f6nnt' ich unm\u00fctterlich,", "tokens": ["Doch", "k\u00f6nnt'", "ich", "un\u00b7m\u00fct\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ich die Mutter, sein vergessen,", "tokens": ["Ich", "die", "Mut\u00b7ter", ",", "sein", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "PPOSAT", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Achten keiner Mutterpflicht,", "tokens": ["Ach\u00b7ten", "kei\u00b7ner", "Mut\u00b7ter\u00b7pflicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sein verg\u00e4\u00dfest du doch nicht!", "tokens": ["Sein", "ver\u00b7g\u00e4\u00b7\u00dfest", "du", "doch", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Freudig \u00fcbergeben dir", "tokens": ["Freu\u00b7dig", "\u00fc\u00b7ber\u00b7ge\u00b7ben", "dir"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sey, o Vater seiner Tage,", "tokens": ["Sey", ",", "o", "Va\u00b7ter", "sei\u00b7ner", "Ta\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "FM", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dir schon itzt geweiht von mir", "tokens": ["Dir", "schon", "itzt", "ge\u00b7weiht", "von", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "VVPP", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sey das Herzchen, das ich trage.", "tokens": ["Sey", "das", "Herz\u00b7chen", ",", "das", "ich", "tra\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deines Sohnes Eigenthum;", "tokens": ["Dei\u00b7nes", "Soh\u00b7nes", "Ei\u00b7gen\u00b7thum", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sey ihm Freude, sey ihm Ruhm!", "tokens": ["Sey", "ihm", "Freu\u00b7de", ",", "sey", "ihm", "Ruhm", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Jesus Christus! Kinder-Freund!", "tokens": ["Je\u00b7sus", "Chris\u00b7tus", "!", "Kin\u00b7der\u00b7Freund", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$.", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Israels getreuer H\u00fcter!", "tokens": ["Is\u00b7raels", "ge\u00b7treu\u00b7er", "H\u00fc\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn mein Mutterauge weint,", "tokens": ["Wenn", "mein", "Mut\u00b7ter\u00b7au\u00b7ge", "weint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00f6ster aller bangen M\u00fctter!", "tokens": ["Tr\u00f6s\u00b7ter", "al\u00b7ler", "ban\u00b7gen", "M\u00fct\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nimm, wo ich nicht helfen kann,", "tokens": ["Nimm", ",", "wo", "ich", "nicht", "hel\u00b7fen", "kann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meines Kindleins du dich an!", "tokens": ["Mei\u00b7nes", "Kin\u00b7dleins", "du", "dich", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Du, der allen alles ist!", "tokens": ["Du", ",", "der", "al\u00b7len", "al\u00b7les", "ist", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PIAT", "PIS", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Du, du Leben der Verlornen!", "tokens": ["Du", ",", "du", "Le\u00b7ben", "der", "Ver\u00b7lor\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Du, du Liebe! Jesus Christ!", "tokens": ["Du", ",", "du", "Lie\u00b7be", "!", "Je\u00b7sus", "Christ", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "NN", "$.", "NE", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00fcter der noch Ungebornen!", "tokens": ["H\u00fc\u00b7ter", "der", "noch", "Un\u00b7ge\u00b7bor\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "W\u00fcrd' es lebend oder todt,", "tokens": ["W\u00fcrd'", "es", "le\u00b7bend", "o\u00b7der", "todt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Du bist meines Kindes Gott!", "tokens": ["Du", "bist", "mei\u00b7nes", "Kin\u00b7des", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ach, wenn das nur sterblich w\u00e4r,", "tokens": ["Ach", ",", "wenn", "das", "nur", "sterb\u00b7lich", "w\u00e4r", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PDS", "ADV", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was durch mich geboren w\u00fcrde!", "tokens": ["Was", "durch", "mich", "ge\u00b7bo\u00b7ren", "w\u00fcr\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4rst du Jesus nicht, wie schwer", "tokens": ["W\u00e4rst", "du", "Je\u00b7sus", "nicht", ",", "wie", "schwer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "NE", "PTKNEG", "$,", "PWAV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4re mir des Lieblings B\u00fcrde!", "tokens": ["W\u00e4\u00b7re", "mir", "des", "Lieb\u00b7lings", "B\u00fcr\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber nun frohlockt mein Geist,", "tokens": ["A\u00b7ber", "nun", "froh\u00b7lockt", "mein", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der mein Kind unsterblich wei\u00dft.", "tokens": ["Der", "mein", "Kind", "uns\u00b7terb\u00b7lich", "wei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Du, du Gottes h\u00f6chster Sohn!", "tokens": ["Du", ",", "du", "Got\u00b7tes", "h\u00f6chs\u00b7ter", "Sohn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und Geborner doch vom Weibe!", "tokens": ["Und", "Ge\u00b7bor\u00b7ner", "doch", "vom", "Wei\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! ach freute deiner schon", "tokens": ["Ach", "!", "ach", "freu\u00b7te", "dei\u00b7ner", "schon"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "XY", "VVFIN", "PPOSAT", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich mein Kind in Mutterleibe!", "tokens": ["Sich", "mein", "Kind", "in", "Mut\u00b7ter\u00b7lei\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00fcpft' es, wie Johannes, wenn", "tokens": ["H\u00fcpft'", "es", ",", "wie", "Jo\u00b7han\u00b7nes", ",", "wenn"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "NE", "$,", "KOUS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich den Namen Jesus nenn'.", "tokens": ["Ich", "den", "Na\u00b7men", "Je\u00b7sus", "nenn'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}