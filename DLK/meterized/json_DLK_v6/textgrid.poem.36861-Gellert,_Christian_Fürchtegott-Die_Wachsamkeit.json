{"textgrid.poem.36861": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Die Wachsamkeit", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht, da\u00df ich's schon ergriffen h\u00e4tte;", "tokens": ["Nicht", ",", "da\u00df", "ich's", "schon", "er\u00b7grif\u00b7fen", "h\u00e4t\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "PIS", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die beste Tugend bleibt noch schwach;", "tokens": ["Die", "bes\u00b7te", "Tu\u00b7gend", "bleibt", "noch", "schwach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch, da\u00df ich meine Seele rette,", "tokens": ["Doch", ",", "da\u00df", "ich", "mei\u00b7ne", "See\u00b7le", "ret\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jag ich dem Kleinod eifrig nach.", "tokens": ["Jag", "ich", "dem", "Klei\u00b7nod", "eif\u00b7rig", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn Tugend ohne Wachsamkeit", "tokens": ["Denn", "Tu\u00b7gend", "oh\u00b7ne", "Wach\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verliert sich bald in Sicherheit.", "tokens": ["Ver\u00b7liert", "sich", "bald", "in", "Si\u00b7cher\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So lang ich hier im Leibe walle,", "tokens": ["So", "lang", "ich", "hier", "im", "Lei\u00b7be", "wal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bin ich ein Kind, das strauchelnd geht.", "tokens": ["Bin", "ich", "ein", "Kind", ",", "das", "strau\u00b7chelnd", "geht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der sehe zu, da\u00df er nicht falle,", "tokens": ["Der", "se\u00b7he", "zu", ",", "da\u00df", "er", "nicht", "fal\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der, wenn sein N\u00e4chster f\u00e4llt, noch steht.", "tokens": ["Der", ",", "wenn", "sein", "N\u00e4chs\u00b7ter", "f\u00e4llt", ",", "noch", "steht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auch die bek\u00e4mpfte b\u00f6se Lust", "tokens": ["Auch", "die", "be\u00b7k\u00e4mpf\u00b7te", "b\u00f6\u00b7se", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Stirbt niemals ganz in unsrer Brust.", "tokens": ["Stirbt", "nie\u00b7mals", "ganz", "in", "uns\u00b7rer", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nicht jede Besserung ist Tugend;", "tokens": ["Nicht", "je\u00b7de", "Bes\u00b7se\u00b7rung", "ist", "Tu\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Oft ist sie nur das Werk der Zeit.", "tokens": ["Oft", "ist", "sie", "nur", "das", "Werk", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die wilde Hitze roher Jugend", "tokens": ["Die", "wil\u00b7de", "Hit\u00b7ze", "ro\u00b7her", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wird mit den Jahren Sittsamkeit:", "tokens": ["Wird", "mit", "den", "Jah\u00b7ren", "Sitt\u00b7sam\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und was Natur und Zeit getan,", "tokens": ["Und", "was", "Na\u00b7tur", "und", "Zeit", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sieht unser Stolz f\u00fcr Tugend an.", "tokens": ["Sieht", "un\u00b7ser", "Stolz", "f\u00fcr", "Tu\u00b7gend", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Oft ist die \u00c4ndrung deiner Seelen", "tokens": ["Oft", "ist", "die", "\u00c4n\u00b7drung", "dei\u00b7ner", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Tausch der Triebe der Natur.", "tokens": ["Ein", "Tausch", "der", "Trie\u00b7be", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du f\u00fchlst, wie Stolz und Ruhmsucht qu\u00e4len,", "tokens": ["Du", "f\u00fchlst", ",", "wie", "Stolz", "und", "Ruhm\u00b7sucht", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und d\u00e4mpfst sie; doch du wechselst nur;", "tokens": ["Und", "d\u00e4mpfst", "sie", ";", "doch", "du", "wech\u00b7selst", "nur", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "KON", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Herz f\u00fchlt einen andern Reiz,", "tokens": ["Dein", "Herz", "f\u00fchlt", "ei\u00b7nen", "an\u00b7dern", "Reiz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Stolz wird Wollust, oder Geiz.", "tokens": ["Dein", "Stolz", "wird", "Wol\u00b7lust", ",", "o\u00b7der", "Geiz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Oft ist es Kunst und Eigenliebe,", "tokens": ["Oft", "ist", "es", "Kunst", "und", "Ei\u00b7gen\u00b7lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was andern strenge Tugend scheint.", "tokens": ["Was", "an\u00b7dern", "stren\u00b7ge", "Tu\u00b7gend", "scheint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Trieb des Neids, der Schm\u00e4hsucht Triebe", "tokens": ["Der", "Trieb", "des", "Neids", ",", "der", "Schm\u00e4h\u00b7sucht", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erweckten dir so manchen Feind;", "tokens": ["Er\u00b7weck\u00b7ten", "dir", "so", "man\u00b7chen", "Feind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du wirst behutsam, schr\u00e4nkst dich ein,", "tokens": ["Du", "wirst", "be\u00b7hut\u00b7sam", ",", "schr\u00e4nkst", "dich", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Fliehst nicht die Schm\u00e4hsucht, nur den Schein.", "tokens": ["Fliehst", "nicht", "die", "Schm\u00e4h\u00b7sucht", ",", "nur", "den", "Schein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "$,", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du denkst, weil Dinge dich nicht r\u00fchren,", "tokens": ["Du", "denkst", ",", "weil", "Din\u00b7ge", "dich", "nicht", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "NN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch die der andern Tugend f\u00e4llt:", "tokens": ["Durch", "die", "der", "an\u00b7dern", "Tu\u00b7gend", "f\u00e4llt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So werde nichts dein Herz verf\u00fchren;", "tokens": ["So", "wer\u00b7de", "nichts", "dein", "Herz", "ver\u00b7f\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch jedes Herz hat seine Welt.", "tokens": ["Doch", "je\u00b7des", "Herz", "hat", "sei\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den, welchen Stand und Gold nicht r\u00fchrt,", "tokens": ["Den", ",", "wel\u00b7chen", "Stand", "und", "Gold", "nicht", "r\u00fchrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAT", "NN", "KON", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hat oft ein Blick, ein Wort verf\u00fchrt.", "tokens": ["Hat", "oft", "ein", "Blick", ",", "ein", "Wort", "ver\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Oft schl\u00e4ft der Trieb in deinem Herzen.", "tokens": ["Oft", "schl\u00e4ft", "der", "Trieb", "in", "dei\u00b7nem", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du scheinst von Rachsucht dir befreit;", "tokens": ["Du", "scheinst", "von", "Rach\u00b7sucht", "dir", "be\u00b7freit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Itzt sollst du eine Schmach verschmerzen,", "tokens": ["Itzt", "sollst", "du", "ei\u00b7ne", "Schmach", "ver\u00b7schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sieh, dein Herz wallt auf und dr\u00e4ut,", "tokens": ["Und", "sieh", ",", "dein", "Herz", "wallt", "auf", "und", "dr\u00e4ut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und schilt so lieblos und so hart,", "tokens": ["Und", "schilt", "so", "lieb\u00b7los", "und", "so", "hart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als es zuerst gescholten ward.", "tokens": ["Als", "es", "zu\u00b7erst", "ge\u00b7schol\u00b7ten", "ward", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Oft denkt, wenn wir der Stille pflegen,", "tokens": ["Oft", "denkt", ",", "wenn", "wir", "der", "Stil\u00b7le", "pfle\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Herz im stillen tugendhaft.", "tokens": ["Das", "Herz", "im", "stil\u00b7len", "tu\u00b7gend\u00b7haft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kaum lachet uns die Welt entgegen:", "tokens": ["Kaum", "la\u00b7chet", "uns", "die", "Welt", "ent\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So regt sich unsre Leidenschaft.", "tokens": ["So", "regt", "sich", "uns\u00b7re", "Lei\u00b7den\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir werden im Ger\u00e4usche schwach,", "tokens": ["Wir", "wer\u00b7den", "im", "Ge\u00b7r\u00e4u\u00b7sche", "schwach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und geben endlich strafbar nach.", "tokens": ["Und", "ge\u00b7ben", "end\u00b7lich", "straf\u00b7bar", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Du opferst Gott die leichtern Triebe", "tokens": ["Du", "op\u00b7ferst", "Gott", "die", "leich\u00b7tern", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch einen strengen Lebenslauf;", "tokens": ["Durch", "ei\u00b7nen", "stren\u00b7gen", "Le\u00b7bens\u00b7lauf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch opferst du, will's seine Liebe,", "tokens": ["Doch", "op\u00b7ferst", "du", ",", "will's", "sei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm auch die liebste Neigung auf?", "tokens": ["Ihm", "auch", "die", "liebs\u00b7te", "Nei\u00b7gung", "auf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dies ist das Auge, dies der Fu\u00df,", "tokens": ["Dies", "ist", "das", "Au\u00b7ge", ",", "dies", "der", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PDS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die sich der Christ entrei\u00dfen mu\u00df.", "tokens": ["Die", "sich", "der", "Christ", "ent\u00b7rei\u00b7\u00dfen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Du fliehst, geneigt zu Ruh und Stille,", "tokens": ["Du", "fliehst", ",", "ge\u00b7neigt", "zu", "Ruh", "und", "Stil\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Welt, und liebst die Einsamkeit;", "tokens": ["Die", "Welt", ",", "und", "liebst", "die", "Ein\u00b7sam\u00b7keit", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch bist du, fordert's Gottes Wille,", "tokens": ["Doch", "bist", "du", ",", "for\u00b7dert's", "Got\u00b7tes", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "$,", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auch dieser zu entfliehn bereit?", "tokens": ["Auch", "die\u00b7ser", "zu", "ent\u00b7fliehn", "be\u00b7reit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "PTKZU", "VVINF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Herz ha\u00dft Habsucht, Neid und Zank;", "tokens": ["Dein", "Herz", "ha\u00dft", "Hab\u00b7sucht", ",", "Neid", "und", "Zank", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Flieht's Unmut auch und M\u00fc\u00dfiggang?", "tokens": ["Flieht's", "Un\u00b7mut", "auch", "und", "M\u00fc\u00b7\u00dfig\u00b7gang", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Du bist gerecht; denn auch bescheiden?", "tokens": ["Du", "bist", "ge\u00b7recht", ";", "denn", "auch", "be\u00b7schei\u00b7den", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$.", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Liebst M\u00e4\u00dfigkeit; denn auch Geduld?", "tokens": ["Liebst", "M\u00e4\u00b7\u00dfig\u00b7keit", ";", "denn", "auch", "Ge\u00b7duld", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "KON", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du dienest gern, wenn andre leiden;", "tokens": ["Du", "die\u00b7nest", "gern", ",", "wenn", "and\u00b7re", "lei\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vergibst du Feinden auch die Schuld?", "tokens": ["Ver\u00b7gibst", "du", "Fein\u00b7den", "auch", "die", "Schuld", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von allen Lastern sollst du rein,", "tokens": ["Von", "al\u00b7len", "Las\u00b7tern", "sollst", "du", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu aller Tugend willig sein.", "tokens": ["Zu", "al\u00b7ler", "Tu\u00b7gend", "wil\u00b7lig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Sei nicht vermessen! Wach und streite;", "tokens": ["Sei", "nicht", "ver\u00b7mes\u00b7sen", "!", "Wach", "und", "strei\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "VVINF", "$.", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denk nicht, da\u00df du schon gnug getan.", "tokens": ["Denk", "nicht", ",", "da\u00df", "du", "schon", "gnug", "ge\u00b7tan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Herz hat seine schwache Seite,", "tokens": ["Dein", "Herz", "hat", "sei\u00b7ne", "schwa\u00b7che", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die greift der Feind der Wohlfahrt an.", "tokens": ["Die", "greift", "der", "Feind", "der", "Wohl\u00b7fahrt", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Sicherheit droht dir den Fall;", "tokens": ["Die", "Si\u00b7cher\u00b7heit", "droht", "dir", "den", "Fall", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drum wache stets, wach \u00fcberall!", "tokens": ["Drum", "wa\u00b7che", "stets", ",", "wach", "\u00fc\u00b7be\u00b7rall", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}