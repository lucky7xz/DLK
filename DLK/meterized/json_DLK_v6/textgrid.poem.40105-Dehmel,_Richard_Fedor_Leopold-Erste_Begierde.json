{"textgrid.poem.40105": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Erste Begierde", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O da\u00df der Ku\u00df doch Ewig dauern m\u00f6chte,", "tokens": ["O", "da\u00df", "der", "Ku\u00df", "doch", "E\u00b7wig", "dau\u00b7ern", "m\u00f6ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ART", "NN", "ADV", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "den taumelnd auf die Lippen dir ich pre\u00dfte,", "tokens": ["den", "tau\u00b7melnd", "auf", "die", "Lip\u00b7pen", "dir", "ich", "pre\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "als Du zum Abschied botest mir die Rechte,", "tokens": ["als", "Du", "zum", "Ab\u00b7schied", "bo\u00b7test", "mir", "die", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "da\u00df starr wie Binsen stand der Schwarm der G\u00e4ste!", "tokens": ["da\u00df", "starr", "wie", "Bin\u00b7sen", "stand", "der", "Schwarm", "der", "G\u00e4s\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KOKOM", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nein, l\u00e4nger ", "tokens": ["Nein", ",", "l\u00e4n\u00b7ger"], "token_info": ["word", "punct", "word"], "pos": ["PTKANT", "$,", "ADJD"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "ich ", "tokens": ["ich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "die liebekranken Glieder n\u00e4chtens dehnen;", "tokens": ["die", "lie\u00b7be\u00b7kran\u00b7ken", "Glie\u00b7der", "n\u00e4ch\u00b7tens", "deh\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbo ", "tokens": ["\u00bb", "o"], "token_info": ["punct", "word"], "pos": ["$(", "FM"], "meter": "-", "measure": "single.down"}}, "stanza.3": {"line.1": {"text": "Oh, komm! noch f\u00fchlt dich zitternd jeder Sinn,", "tokens": ["Oh", ",", "komm", "!", "noch", "f\u00fchlt", "dich", "zit\u00b7ternd", "je\u00b7der", "Sinn", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "vom hei\u00dfen Duft berauscht aus deinem Kleide,", "tokens": ["vom", "hei\u00b7\u00dfen", "Duft", "be\u00b7rauscht", "aus", "dei\u00b7nem", "Klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "sieht wogend gl\u00fchn, du Flammenk\u00f6nigin,", "tokens": ["sieht", "wo\u00b7gend", "gl\u00fchn", ",", "du", "Flam\u00b7men\u00b7k\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVINF", "$,", "PPER", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "im Aschenflor um dich die Kupferseide.", "tokens": ["im", "A\u00b7schen\u00b7flor", "um", "dich", "die", "Kup\u00b7fer\u00b7sei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Gie\u00df ", "tokens": ["Gie\u00df"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "ich ", "tokens": ["ich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "vor dieses Feuerregens wilder Brut,", "tokens": ["vor", "die\u00b7ses", "Feu\u00b7er\u00b7re\u00b7gens", "wil\u00b7der", "Brut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "vor diesen Weh'n, die w\u00fchlend in mir brauen.", "tokens": ["vor", "die\u00b7sen", "Weh'n", ",", "die", "w\u00fch\u00b7lend", "in", "mir", "brau\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Es schie\u00dft die Saat aus ihrem dunklen Schoo\u00df,", "tokens": ["Es", "schie\u00dft", "die", "Saat", "aus", "ih\u00b7rem", "dunk\u00b7len", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "die lange schmachtend lag in spr\u00f6der H\u00fclle:", "tokens": ["die", "lan\u00b7ge", "schmach\u00b7tend", "lag", "in", "spr\u00f6\u00b7der", "H\u00fcl\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "ich will mich ", "tokens": ["ich", "will", "mich"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "aus meiner leeren Brunst zu Frucht und F\u00fclle!", "tokens": ["aus", "mei\u00b7ner", "lee\u00b7ren", "Brunst", "zu", "Frucht", "und", "F\u00fcl\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Matt werden will ich meiner schw\u00fclen Lust:", "tokens": ["Matt", "wer\u00b7den", "will", "ich", "mei\u00b7ner", "schw\u00fc\u00b7len", "Lust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAINF", "VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "oh ", "tokens": ["oh"], "token_info": ["word"], "pos": ["XY"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "die Furcht, die Sehnsucht dieser jungen Brust:", "tokens": ["die", "Furcht", ",", "die", "Sehn\u00b7sucht", "die\u00b7ser", "jun\u00b7gen", "Brust", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Auf Nelkend\u00fcften kommt die Nacht gezogen,", "tokens": ["Auf", "Nel\u00b7ken\u00b7d\u00fcf\u00b7ten", "kommt", "die", "Nacht", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "o k\u00e4mst auch Du so s\u00fc\u00df und so verstohlen!", "tokens": ["o", "k\u00e4mst", "auch", "Du", "so", "s\u00fc\u00df", "und", "so", "ver\u00b7stoh\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "VVFIN", "ADV", "PPER", "ADV", "ADJD", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "so mondeswei\u00df dich in die Sammetwogen,", "tokens": ["so", "mon\u00b7des\u00b7wei\u00df", "dich", "in", "die", "Sam\u00b7met\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "den Purpurflaum der schw\u00e4rzlichen Violen,", "tokens": ["den", "Pur\u00b7pur\u00b7flaum", "der", "schw\u00e4rz\u00b7li\u00b7chen", "Vi\u00b7o\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "die streun ich will, an diese Brust zu betten:", "tokens": ["die", "streun", "ich", "will", ",", "an", "die\u00b7se", "Brust", "zu", "bet\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PPER", "VMFIN", "$,", "APPR", "PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "da\u00df alle meine M\u00e4chte an des Weibes", "tokens": ["da\u00df", "al\u00b7le", "mei\u00b7ne", "M\u00e4ch\u00b7te", "an", "des", "Wei\u00b7bes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "enth\u00fcllten G\u00f6ttlichkeiten sich entketten,", "tokens": ["ent\u00b7h\u00fcll\u00b7ten", "G\u00f6tt\u00b7lich\u00b7kei\u00b7ten", "sich", "ent\u00b7ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "versink' ich \u2013 in den Teppich \u2013 Deines \u2013 Leibes!\u00ab", "tokens": ["ver\u00b7sink'", "ich", "\u2013", "in", "den", "Tep\u00b7pich", "\u2013", "Dei\u00b7nes", "\u2013", "Lei\u00b7bes", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$(", "APPR", "ART", "NN", "$(", "PPOSAT", "$(", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}