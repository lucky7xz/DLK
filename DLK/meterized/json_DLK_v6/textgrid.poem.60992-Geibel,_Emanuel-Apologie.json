{"textgrid.poem.60992": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Apologie", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df ich auch zur sch\u00f6nen Zeit des Fr\u00fchlings", "tokens": ["Da\u00df", "ich", "auch", "zur", "sch\u00f6\u00b7nen", "Zeit", "des", "Fr\u00fch\u00b7lings"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Morgens lange stets im Bette s\u00e4ume,", "tokens": ["Mor\u00b7gens", "lan\u00b7ge", "stets", "im", "Bet\u00b7te", "s\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Darum wollt ihr, Freunde, mich verklagen?", "tokens": ["Da\u00b7rum", "wollt", "ihr", ",", "Freun\u00b7de", ",", "mich", "ver\u00b7kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "$,", "NN", "$,", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Tut es immerhin! Euch hat beim Werden", "tokens": ["Tut", "es", "im\u00b7mer\u00b7hin", "!", "Euch", "hat", "beim", "Wer\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "$.", "PPER", "VAFIN", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Nicht die Muse freundlich angel\u00e4chelt,", "tokens": ["Nicht", "die", "Mu\u00b7se", "freund\u00b7lich", "an\u00b7ge\u00b7l\u00e4\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und mit Morpheus' lieblichem Geschlechte", "tokens": ["Und", "mit", "Mor\u00b7pheus'", "lieb\u00b7li\u00b7chem", "Ge\u00b7schlech\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Seid ihr ganz und gar in herbem Zwiespalt.", "tokens": ["Seid", "ihr", "ganz", "und", "gar", "in", "her\u00b7bem", "Zwies\u00b7palt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ADV", "KON", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Nicht die Wonne kennt ihr, auf dem Lager", "tokens": ["Nicht", "die", "Won\u00b7ne", "kennt", "ihr", ",", "auf", "dem", "La\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "VVFIN", "PPER", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Sich zu dehnen, wenn am offnen Fenster", "tokens": ["Sich", "zu", "deh\u00b7nen", ",", "wenn", "am", "off\u00b7nen", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "PTKZU", "VVINF", "$,", "KOUS", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Gr\u00fcnes Weinlaub schwankt im Sonnenschimmer", "tokens": ["Gr\u00fc\u00b7nes", "Wein\u00b7laub", "schwankt", "im", "Son\u00b7nen\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Und die Bl\u00fcten rot und wei\u00df hereinwehn.", "tokens": ["Und", "die", "Bl\u00fc\u00b7ten", "rot", "und", "wei\u00df", "her\u00b7ein\u00b7wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "KON", "VVFIN", "VVINF", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Drau\u00dfen in den Rosenb\u00fcschen fl\u00f6tet", "tokens": ["Drau\u00b7\u00dfen", "in", "den", "Ro\u00b7sen\u00b7b\u00fc\u00b7schen", "fl\u00f6\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Dann die Nachtigall, und wie die T\u00f6ne", "tokens": ["Dann", "die", "Nach\u00b7ti\u00b7gall", ",", "und", "wie", "die", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "KON", "PWAV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Lieblich sich durch meine Seele dehnen,", "tokens": ["Lieb\u00b7lich", "sich", "durch", "mei\u00b7ne", "See\u00b7le", "deh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Spinnt der Morgentraum in halbem Wachen", "tokens": ["Spinnt", "der", "Mor\u00b7gen\u00b7traum", "in", "hal\u00b7bem", "Wa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Sich noch fort und wird zu holden Liedern.", "tokens": ["Sich", "noch", "fort", "und", "wird", "zu", "hol\u00b7den", "Lie\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PTKVZ", "KON", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Trifft mir endlich dann der Strahl die Wimpern,", "tokens": ["Trifft", "mir", "end\u00b7lich", "dann", "der", "Strahl", "die", "Wim\u00b7pern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Spring' ich rasch empor, auf wei\u00dfe Bl\u00e4tter", "tokens": ["Spring'", "ich", "rasch", "em\u00b7por", ",", "auf", "wei\u00b7\u00dfe", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJD", "PTKVZ", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Die gereimten Tr\u00e4ume festzubannen.", "tokens": ["Die", "ge\u00b7reim\u00b7ten", "Tr\u00e4u\u00b7me", "fest\u00b7zu\u00b7ban\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Abends aber schleich' ich zur Geliebten,", "tokens": ["A\u00b7bends", "a\u00b7ber", "schleich'", "ich", "zur", "Ge\u00b7lieb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Und sie liest es, was in s\u00fc\u00dfer D\u00e4mmrung", "tokens": ["Und", "sie", "liest", "es", ",", "was", "in", "s\u00fc\u00b7\u00dfer", "D\u00e4mm\u00b7rung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Gr\u00fc\u00dfend durch des Freundes Brust gezogen,", "tokens": ["Gr\u00fc\u00b7\u00dfend", "durch", "des", "Freun\u00b7des", "Brust", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Und mit K\u00fcssen lohnt sie jede Zeile.", "tokens": ["Und", "mit", "K\u00fcs\u00b7sen", "lohnt", "sie", "je\u00b7de", "Zei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Sagt nun, ihr profanen Traumver\u00e4chter,", "tokens": ["Sagt", "nun", ",", "ihr", "pro\u00b7fa\u00b7nen", "Traum\u00b7ver\u00b7\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sagt nun, wollt ihr l\u00e4nger noch mich schelten?", "tokens": ["Sagt", "nun", ",", "wollt", "ihr", "l\u00e4n\u00b7ger", "noch", "mich", "schel\u00b7ten", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "VMFIN", "PPER", "ADJD", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}