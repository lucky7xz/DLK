{"textgrid.poem.46038": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer mich liebt, wird mein Wort halten;", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer mich liebt, wird mein Wort halten;", "tokens": ["Wer", "mich", "liebt", ",", "wird", "mein", "Wort", "hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil mein Wort ist Vaters Wort,", "tokens": ["Weil", "mein", "Wort", "ist", "Va\u00b7ters", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird des Vaters Liebe walten", "tokens": ["Wird", "des", "Va\u00b7ters", "Lie\u00b7be", "wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bei dem Liebenden hinfort.", "tokens": ["Bei", "dem", "Lie\u00b7ben\u00b7den", "hin\u00b7fort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und mit ihm ich bei euch wohne;", "tokens": ["Und", "mit", "ihm", "ich", "bei", "euch", "woh\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch den Tr\u00f6stenden, den Geist,", "tokens": ["Doch", "den", "Tr\u00f6s\u00b7ten\u00b7den", ",", "den", "Geist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schickt im Namen er vom Sohne,", "tokens": ["Schickt", "im", "Na\u00b7men", "er", "vom", "Soh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er euch Erkenntnis weist.", "tokens": ["Da\u00df", "er", "euch", "Er\u00b7kennt\u00b7nis", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Scheidend geb' ich euch den Frieden!", "tokens": ["Schei\u00b7dend", "geb'", "ich", "euch", "den", "Frie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Frieden, den die Welt nicht gibt;", "tokens": ["Frie\u00b7den", ",", "den", "die", "Welt", "nicht", "gibt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freut euch, wenn ich hingeschieden", "tokens": ["Freut", "euch", ",", "wenn", "ich", "hin\u00b7ge\u00b7schie\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu dem Vater, der mich liebt!", "tokens": ["Zu", "dem", "Va\u00b7ter", ",", "der", "mich", "liebt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenig red' ich mehr und gehe,", "tokens": ["We\u00b7nig", "red'", "ich", "mehr", "und", "ge\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn jetzt k\u00f6mmt der F\u00fcrst der Welt,", "tokens": ["Denn", "jetzt", "k\u00f6mmt", "der", "F\u00fcrst", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat an mir nichts. Es geschehe!", "tokens": ["Hat", "an", "mir", "nichts", ".", "Es", "ge\u00b7sche\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "PIS", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Weil dem Vater es gef\u00e4llt.", "tokens": ["Weil", "dem", "Va\u00b7ter", "es", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}