{"textgrid.poem.37388": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es war ein M\u00e4gdlein froh und keck,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war ein M\u00e4gdlein froh und keck,", "tokens": ["Es", "war", "ein", "M\u00e4gd\u00b7lein", "froh", "und", "keck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stets lacht ihr Rosenmund,", "tokens": ["Stets", "lacht", "ihr", "Ro\u00b7sen\u00b7mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr schien die Liebe Lebenszweck", "tokens": ["Ihr", "schien", "die", "Lie\u00b7be", "Le\u00b7bens\u00b7zweck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und alles andre Schund.", "tokens": ["Und", "al\u00b7les", "and\u00b7re", "Schund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie denkt an nichts als an Pl\u00e4sier,", "tokens": ["Sie", "denkt", "an", "nichts", "als", "an", "Pl\u00e4\u00b7sier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIS", "KOKOM", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seitdem die Mutter tot,", "tokens": ["Seit\u00b7dem", "die", "Mut\u00b7ter", "tot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie lacht und liebt, obgleich es ihr", "tokens": ["Sie", "lacht", "und", "liebt", ",", "ob\u00b7gleich", "es", "ihr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "KOUS", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Vater oft verbot.", "tokens": ["Der", "Va\u00b7ter", "oft", "ver\u00b7bot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Einst hat sie frech und unbedacht", "tokens": ["Einst", "hat", "sie", "frech", "und", "un\u00b7be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Schatz, der ihr gef\u00e4llt,", "tokens": ["Den", "Schatz", ",", "der", "ihr", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sich f\u00fcr die Zeit um Mitternacht", "tokens": ["Sich", "f\u00fcr", "die", "Zeit", "um", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Kirchhof hinbestellt.", "tokens": ["Zum", "Kirch\u00b7hof", "hin\u00b7be\u00b7stellt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und als sie kam zum Stelldichein,", "tokens": ["Und", "als", "sie", "kam", "zum", "Stell\u00b7dich\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O h\u00f6rt, was sich begab,", "tokens": ["O", "h\u00f6rt", ",", "was", "sich", "be\u00b7gab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PRELS", "PRF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da stand ein Geist im Mondenschein", "tokens": ["Da", "stand", "ein", "Geist", "im", "Mon\u00b7den\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf ihrer Mutter Grab.", "tokens": ["Auf", "ih\u00b7rer", "Mut\u00b7ter", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er steht so starr, er steht so stumm,", "tokens": ["Er", "steht", "so", "starr", ",", "er", "steht", "so", "stumm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er blickt so kummervoll.", "tokens": ["Er", "blickt", "so", "kum\u00b7mer\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das M\u00e4gdlein dreht sich schaudernd um", "tokens": ["Das", "M\u00e4gd\u00b7lein", "dreht", "sich", "schau\u00b7dernd", "um"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und rennt nach Haus wie toll.", "tokens": ["Und", "rennt", "nach", "Haus", "wie", "toll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KOKOM", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es wird, wer einen Geist gesehn,", "tokens": ["Es", "wird", ",", "wer", "ei\u00b7nen", "Geist", "ge\u00b7sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nie mehr des Lebens froh,", "tokens": ["Nie", "mehr", "des", "Le\u00b7bens", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er f\u00fchlt, es ist um ihn geschehn.", "tokens": ["Er", "f\u00fchlt", ",", "es", "ist", "um", "ihn", "ge\u00b7schehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem M\u00e4gdlein ging es so.", "tokens": ["Dem", "M\u00e4gd\u00b7lein", "ging", "es", "so", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sie welkt dahin, sie will und mag", "tokens": ["Sie", "welkt", "da\u00b7hin", ",", "sie", "will", "und", "mag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "$,", "PPER", "VMFIN", "KON", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht mehr zu Spiel und Tanz.", "tokens": ["Nicht", "mehr", "zu", "Spiel", "und", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man flocht ihr um Johannistag", "tokens": ["Man", "flocht", "ihr", "um", "Jo\u00b7han\u00b7nis\u00b7tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bereits den Totenkranz.", "tokens": ["Be\u00b7reits", "den", "To\u00b7ten\u00b7kranz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}