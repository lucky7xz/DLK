{"textgrid.poem.58715": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr, strafe mich nicht in deinem Zorn! Psalm 6, 2.", "tokens": ["Herr", ",", "stra\u00b7fe", "mich", "nicht", "in", "dei\u00b7nem", "Zorn", "!", "Psalm", "6", ",", "2."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "number", "punct", "ordinal"], "pos": ["NN", "$,", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$.", "XY", "CARD", "$,", "ADJA"], "meter": "-+--+-+-++", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Gedenke, Herr, an deine Barmherzigkeit! Psalm 25, 6", "tokens": ["Ge\u00b7den\u00b7ke", ",", "Herr", ",", "an", "dei\u00b7ne", "Barm\u00b7her\u00b7zig\u00b7keit", "!", "Psalm", "25", ",", "6"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "number", "punct", "number"], "pos": ["NN", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$.", "NN", "CARD", "$,", "CARD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Hab' ich denn, gro\u00dfer Gott, mir deinen Zorn erwecket,", "tokens": ["Hab'", "ich", "denn", ",", "gro\u00b7\u00dfer", "Gott", ",", "mir", "dei\u00b7nen", "Zorn", "er\u00b7we\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "ADJA", "NN", "$,", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der mich Unseligen so schrecket?", "tokens": ["Der", "mich", "Un\u00b7se\u00b7li\u00b7gen", "so", "schre\u00b7cket", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wie hab' ich d\u00fcrfen dir, mein Gott, mi\u00dff\u00e4llig sein?", "tokens": ["Wie", "hab'", "ich", "d\u00fcr\u00b7fen", "dir", ",", "mein", "Gott", ",", "mi\u00df\u00b7f\u00e4l\u00b7lig", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VMFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur dran zu denken macht mir Pein.", "tokens": ["Nur", "dran", "zu", "den\u00b7ken", "macht", "mir", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "PTKZU", "VVINF", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich hab' verdienet zwar, zu f\u00fchlen deine Blitzen,", "tokens": ["Ich", "hab'", "ver\u00b7die\u00b7net", "zwar", ",", "zu", "f\u00fch\u00b7len", "dei\u00b7ne", "Blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVFIN", "ADV", "$,", "PTKZU", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch denke, Herr, was w\u00fcrd' dir's n\u00fctzen!", "tokens": ["Doch", "den\u00b7ke", ",", "Herr", ",", "was", "w\u00fcrd'", "dir's", "n\u00fct\u00b7zen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "NN", "$,", "PWS", "VAFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bin ja nur ein armer Staub der Erd',", "tokens": ["Ich", "bin", "ja", "nur", "ein", "ar\u00b7mer", "Staub", "der", "Erd'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nicht einmal deines Eifers wert.", "tokens": ["Nicht", "ein\u00b7mal", "dei\u00b7nes", "Ei\u00b7fers", "wert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ein pures Nichts bin ich, willst du doch Straf' erzeigen?", "tokens": ["Ein", "pu\u00b7res", "Nichts", "bin", "ich", ",", "willst", "du", "doch", "Straf'", "er\u00b7zei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "$,", "VMFIN", "PPER", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wohlan, mein Herr, ich will mich beugen,", "tokens": ["Wo\u00b7hlan", ",", "mein", "Herr", ",", "ich", "will", "mich", "beu\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$,", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich stimm' in deinen Rechtsspruch ein,", "tokens": ["Ich", "stimm'", "in", "dei\u00b7nen", "Rechts\u00b7spruch", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dies treulos', dies undankbar' Herz soll mir ein Scheuel sein!", "tokens": ["Dies", "treu\u00b7los'", ",", "dies", "un\u00b7dank\u00b7ba\u00b7r'", "Herz", "soll", "mir", "ein", "Scheu\u00b7el", "sein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PDS", "KON", "NN", "VMFIN", "PPER", "ART", "NN", "VAINF", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.6": {"line.1": {"text": "La\u00df deinen Zorn, mein Herr und K\u00f6nig, sinken,", "tokens": ["La\u00df", "dei\u00b7nen", "Zorn", ",", "mein", "Herr", "und", "K\u00f6\u00b7nig", ",", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich bete deine Zuchtrut' an.", "tokens": ["Ich", "be\u00b7te", "dei\u00b7ne", "Zuchtrut'", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Tu was du willst, es soll mir s\u00fc\u00dfe d\u00fcnken,", "tokens": ["Tu", "was", "du", "willst", ",", "es", "soll", "mir", "s\u00fc\u00b7\u00dfe", "d\u00fcn\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWS", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vergi\u00df nur, was ich \u00dcbles hab' getan!", "tokens": ["Ver\u00b7gi\u00df", "nur", ",", "was", "ich", "\u00dcb\u00b7les", "hab'", "ge\u00b7tan", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PWS", "PPER", "NE", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mi\u00dffiel ich dir auch nur auf einen Augenblick,", "tokens": ["Mi\u00df\u00b7fiel", "ich", "dir", "auch", "nur", "auf", "ei\u00b7nen", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So bin ich's wert, da\u00df mich die Straf' sein harte dr\u00fcck'.", "tokens": ["So", "bin", "ich's", "wert", ",", "da\u00df", "mich", "die", "Straf'", "sein", "har\u00b7te", "dr\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}