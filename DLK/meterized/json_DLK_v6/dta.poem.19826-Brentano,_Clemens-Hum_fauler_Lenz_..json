{"dta.poem.19826": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Hum fauler Lenz .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es wollt eine Frau zu Weine gahn. Hum fauler               ", "tokens": ["Es", "wollt", "ei\u00b7ne", "Frau", "zu", "Wei\u00b7ne", "gahn", ".", "Hum", "fau\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "NN", "VVFIN", "$.", "NE", "NE"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Lenz.", "tokens": ["Lenz", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Und wollt den Mann nicht mit sich han. Ha ha ha.", "tokens": ["Und", "wollt", "den", "Mann", "nicht", "mit", "sich", "han", ".", "Ha", "ha", "ha", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PTKNEG", "APPR", "PRF", "VAFIN", "$.", "ITJ", "ITJ", "ITJ", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Du mu\u00dft zu Hause bleiben. Hum etc.", "tokens": ["Du", "mu\u00dft", "zu", "Hau\u00b7se", "blei\u00b7ben", ".", "Hum", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "abbreviation"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF", "$.", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sollst K\u00fch und K\u00e4lber treiben. Ha etc.", "tokens": ["Sollst", "K\u00fch", "und", "K\u00e4l\u00b7ber", "trei\u00b7ben", ".", "Ha", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "abbreviation"], "pos": ["VMFIN", "NN", "KON", "NN", "VVINF", "$.", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach Mann, was hast du dann gethan. Hum etc.", "tokens": ["Ach", "Mann", ",", "was", "hast", "du", "dann", "ge\u00b7than", ".", "Hum", "et\u00b7c."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "abbreviation"], "pos": ["ITJ", "NN", "$,", "PWS", "VAFIN", "PPER", "ADV", "VVPP", "$.", "FM.la", "FM.la"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Du hast den Rahm gefressen ab. Ha etc.", "tokens": ["Du", "hast", "den", "Rahm", "ge\u00b7fres\u00b7sen", "ab", ".", "Ha", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "abbreviation"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "PTKVZ", "$.", "FM.la", "FM.la"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Und hast die Molken lassen stahn. Hum etc.", "tokens": ["Und", "hast", "die", "Mol\u00b7ken", "las\u00b7sen", "stahn", ".", "Hum", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "abbreviation"], "pos": ["KON", "VAFIN", "ART", "NN", "VVINF", "VVFIN", "$.", "FM.la", "FM.la"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Daf\u00fcr mu\u00dft du jezt Pr\u00fcgel han. Ha etc.", "tokens": ["Da\u00b7f\u00fcr", "mu\u00dft", "du", "jezt", "Pr\u00fc\u00b7gel", "han", ".", "Ha", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "abbreviation"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "NN", "VAFIN", "$.", "FM.la", "FM.la"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.5": {"line.1": {"text": "Die Frau ergrif den Plaul. Hum etc.", "tokens": ["Die", "Frau", "er\u00b7grif", "den", "Plaul", ".", "Hum", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "abbreviation"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$.", "FM.la", "FM.la"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Und schlug den Mann aufs Maul. Ha etc.", "tokens": ["Und", "schlug", "den", "Mann", "aufs", "Maul", ".", "Ha", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "abbreviation"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$.", "FM.la", "FM.la"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der kroch zum H\u00fchnerloch hinaus. Hum etc.", "tokens": ["Der", "kroch", "zum", "H\u00fch\u00b7ner\u00b7loch", "hin\u00b7aus", ".", "Hum", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "abbreviation"], "pos": ["ART", "NN", "APPRART", "NN", "PTKVZ", "$.", "FM.la", "FM.la"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wohl in das n\u00e4chste Nachbarhaus. Ha etc.", "tokens": ["Wohl", "in", "das", "n\u00e4chs\u00b7te", "Nach\u00b7bar\u00b7haus", ".", "Ha", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "abbreviation"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$.", "FM.la", "FM.la"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "Ach Nachbar, ich mu\u00df klagen. Hum etc.", "tokens": ["Ach", "Nach\u00b7bar", ",", "ich", "mu\u00df", "kla\u00b7gen", ".", "Hum", "et\u00b7c."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "abbreviation"], "pos": ["ITJ", "NN", "$,", "PPER", "VMFIN", "VVINF", "$.", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Frau hat mich geschlagen. Ha etc.", "tokens": ["Mein", "Frau", "hat", "mich", "ge\u00b7schla\u00b7gen", ".", "Ha", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "abbreviation"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$.", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "So ist mir gestern auch geschehn. Hum etc.", "tokens": ["So", "ist", "mir", "ge\u00b7stern", "auch", "ge\u00b7schehn", ".", "Hum", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "abbreviation"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$.", "FM.la", "FM.la"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So will ich wieder heime gehn. Ha. etc.", "tokens": ["So", "will", "ich", "wie\u00b7der", "hei\u00b7me", "gehn", ".", "Ha", ".", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "abbreviation"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJA", "VVINF", "$.", "NE", "$.", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}