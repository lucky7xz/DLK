{"textgrid.poem.49321": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Art l\u00e4sset nicht von Art, die Katze l\u00e4\u00dft das Mausen,", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Art l\u00e4sset nicht von Art, die Katze l\u00e4\u00dft das Mausen,", "tokens": ["Art", "l\u00e4s\u00b7set", "nicht", "von", "Art", ",", "die", "Kat\u00b7ze", "l\u00e4\u00dft", "das", "Mau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "APPR", "NN", "$,", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Dieb das Stehlen nicht, die Affen wollen laufen,", "tokens": ["Der", "Dieb", "das", "Steh\u00b7len", "nicht", ",", "die", "Af\u00b7fen", "wol\u00b7len", "lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKNEG", "$,", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Garte bringt sein Kraut, der Hirte treibt f\u00fcrs Thor,", "tokens": ["Der", "Gar\u00b7te", "bringt", "sein", "Kraut", ",", "der", "Hir\u00b7te", "treibt", "f\u00fcrs", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was ehmals Wasser war, wird Wasser wie zuvor.", "tokens": ["Was", "eh\u00b7mals", "Was\u00b7ser", "war", ",", "wird", "Was\u00b7ser", "wie", "zu\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "VAFIN", "$,", "VAFIN", "NN", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Salz komt aus Wasser her, es quillet aus der Erden,", "tokens": ["Salz", "komt", "aus", "Was\u00b7ser", "her", ",", "es", "quil\u00b7let", "aus", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "PTKVZ", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kan auch mit schlechter M\u00fch' ein Wasser wiedrum werden,", "tokens": ["Kan", "auch", "mit", "schlech\u00b7ter", "M\u00fch'", "ein", "Was\u00b7ser", "wie\u00b7drum", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "ADJA", "NN", "ART", "NN", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und weil denn Eis und Schnee aus Wasser ist gemacht,", "tokens": ["Und", "weil", "denn", "Eis", "und", "Schnee", "aus", "Was\u00b7ser", "ist", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "NN", "KON", "NN", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So wird es auch sehr leicht ins Wasser wieder bracht.", "tokens": ["So", "wird", "es", "auch", "sehr", "leicht", "ins", "Was\u00b7ser", "wie\u00b7der", "bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Katz' h\u00e4lt zwar das Licht, wann Salomo wil essen;", "tokens": ["Die", "Katz'", "h\u00e4lt", "zwar", "das", "Licht", ",", "wann", "Sa\u00b7lo\u00b7mo", "wil", "es\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,", "PWAV", "NE", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Erblicket sie die Maus, so ist des Lichts vergessen,", "tokens": ["Er\u00b7bli\u00b7cket", "sie", "die", "Maus", ",", "so", "ist", "des", "Lichts", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und wann Marcolphus gleich noch eins so zornig w\u00e4r,", "tokens": ["Und", "wann", "Mar\u00b7col\u00b7phus", "gleich", "noch", "eins", "so", "zor\u00b7nig", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NE", "ADV", "ADV", "PIS", "ADV", "ADJD", "VAFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.12": {"text": "Bleibt doch mein Sprichwort wahr: Natur geht f\u00fcr die Lehr.", "tokens": ["Bleibt", "doch", "mein", "Sprich\u00b7wort", "wahr", ":", "Na\u00b7tur", "geht", "f\u00fcr", "die", "Lehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$.", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}