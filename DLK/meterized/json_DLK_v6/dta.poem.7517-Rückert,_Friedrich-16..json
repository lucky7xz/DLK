{"dta.poem.7517": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Baumeisterin Natur scheint f\u00fcr sich selbst zumeist", "tokens": ["Bau\u00b7meis\u00b7te\u00b7rin", "Na\u00b7tur", "scheint", "f\u00fcr", "sich", "selbst", "zu\u00b7meist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "APPR", "PRF", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu baun, und baut zuletzt doch Alles f\u00fcr den Geist.", "tokens": ["Zu", "baun", ",", "und", "baut", "zu\u00b7letzt", "doch", "Al\u00b7les", "f\u00fcr", "den", "Geist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KON", "VVFIN", "ADV", "ADV", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der schrankenlose Geist ist darum nur gefangen", "tokens": ["Der", "schran\u00b7ken\u00b7lo\u00b7se", "Geist", "ist", "da\u00b7rum", "nur", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PAV", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Schranken, um darin zur Freiheit zu gelangen.", "tokens": ["In", "Schran\u00b7ken", ",", "um", "da\u00b7rin", "zur", "Frei\u00b7heit", "zu", "ge\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "PAV", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein S\u00e4ugling ist der Geist, Natur ist seine Amme,", "tokens": ["Ein", "S\u00e4ug\u00b7ling", "ist", "der", "Geist", ",", "Na\u00b7tur", "ist", "sei\u00b7ne", "Am\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie n\u00e4hrt ihn, bis er f\u00fchlt, da\u00df er von ihr nicht stamme.", "tokens": ["Sie", "n\u00e4hrt", "ihn", ",", "bis", "er", "f\u00fchlt", ",", "da\u00df", "er", "von", "ihr", "nicht", "stam\u00b7me", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "APPR", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die dunkle Mutter will ihr Kind in Schlummer halten;", "tokens": ["Die", "dunk\u00b7le", "Mut\u00b7ter", "will", "ihr", "Kind", "in", "Schlum\u00b7mer", "hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von oben bricht ein Stral durch ihres Hauses Spalten.", "tokens": ["Von", "o\u00b7ben", "bricht", "ein", "Stral", "durch", "ih\u00b7res", "Hau\u00b7ses", "Spal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und wie der Schmetterling erwacht vom Puppentraum,", "tokens": ["Und", "wie", "der", "Schmet\u00b7ter\u00b7ling", "er\u00b7wacht", "vom", "Pup\u00b7pen\u00b7traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schwingt der Gedanke frei sich \u00fcber Zeit und Raum.", "tokens": ["Schwingt", "der", "Ge\u00b7dan\u00b7ke", "frei", "sich", "\u00fc\u00b7ber", "Zeit", "und", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "PRF", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.6": {"line.1": {"text": "Wie, wann die Frucht ist reif, von selbst die Kapsel springt,", "tokens": ["Wie", ",", "wann", "die", "Frucht", "ist", "reif", ",", "von", "selbst", "die", "Kap\u00b7sel", "springt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "ART", "NN", "VAFIN", "ADJD", "$,", "APPR", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und hin der Saame fliegt, von Himmelsluft beschwingt.", "tokens": ["Und", "hin", "der", "Saa\u00b7me", "fliegt", ",", "von", "Him\u00b7mel\u00b7sluft", "be\u00b7schwingt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wie der Br\u00fctmutter Huhn die Entenbrut entrann,", "tokens": ["Wie", "der", "Br\u00fct\u00b7mut\u00b7ter", "Huhn", "die", "En\u00b7ten\u00b7brut", "ent\u00b7rann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und auf die Flut sich wagt, wo sie nicht folgen kann.", "tokens": ["Und", "auf", "die", "Flut", "sich", "wagt", ",", "wo", "sie", "nicht", "fol\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PRF", "VVFIN", "$,", "PWAV", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}