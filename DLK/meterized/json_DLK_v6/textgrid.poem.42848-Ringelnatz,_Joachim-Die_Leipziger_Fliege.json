{"textgrid.poem.42848": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Die Leipziger Fliege", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob wohl die Fliegen Eier in uns legen,", "tokens": ["Ob", "wohl", "die", "Flie\u00b7gen", "Ei\u00b7er", "in", "uns", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn sie so lange auf uns sitzen bleiben", "tokens": ["Wenn", "sie", "so", "lan\u00b7ge", "auf", "uns", "sit\u00b7zen", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "PPER", "VVINF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wir sie, weil wir schlafen, nicht vertreiben?", "tokens": ["Und", "wir", "sie", ",", "weil", "wir", "schla\u00b7fen", ",", "nicht", "ver\u00b7trei\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Man sollte seinen K\u00f6rper viel mehr pflegen,", "tokens": ["Man", "soll\u00b7te", "sei\u00b7nen", "K\u00f6r\u00b7per", "viel", "mehr", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPOSAT", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Fliege, die mich darauf brachte,", "tokens": ["Die", "Flie\u00b7ge", ",", "die", "mich", "da\u00b7rauf", "brach\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als ich in meinem Mietslogis erwachte,", "tokens": ["Als", "ich", "in", "mei\u00b7nem", "Miets\u00b7lo\u00b7gis", "er\u00b7wach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "War eine greisenhafte und ergraute,", "tokens": ["War", "ei\u00b7ne", "grei\u00b7sen\u00b7haf\u00b7te", "und", "er\u00b7grau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da\u00df ich nur zaghaft mir getraute,", "tokens": ["Da\u00df", "ich", "nur", "zag\u00b7haft", "mir", "ge\u00b7trau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie wenigstens ein bi\u00dfchen totzuschlagen.", "tokens": ["Sie", "we\u00b7nigs\u00b7tens", "ein", "bi\u00df\u00b7chen", "tot\u00b7zu\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie sterben im November sowieso", "tokens": ["Sie", "ster\u00b7ben", "im", "No\u00b7vem\u00b7ber", "so\u00b7wi\u00b7e\u00b7so"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In Leipzig. (Sp\u00e4ter als wie anderswo.)", "tokens": ["In", "Leip\u00b7zig", ".", "(", "Sp\u00e4\u00b7ter", "als", "wie", "an\u00b7ders\u00b7wo", ".", ")"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "$.", "$(", "ADJD", "KOKOM", "KOKOM", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie k\u00f6nnen Sterbende doch oft noch plagen,", "tokens": ["Wie", "k\u00f6n\u00b7nen", "Ster\u00b7ben\u00b7de", "doch", "oft", "noch", "pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "NN", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Alter stimmt nicht immer mild.", "tokens": ["Das", "Al\u00b7ter", "stimmt", "nicht", "im\u00b7mer", "mild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie sind unheimlich dann und boshaft wild.", "tokens": ["Sie", "sind", "un\u00b7heim\u00b7lich", "dann", "und", "bos\u00b7haft", "wild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "KON", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Doch unter solcher feuchten Sumpfluft leiden", "tokens": ["Doch", "un\u00b7ter", "sol\u00b7cher", "feuch\u00b7ten", "Sum\u00b7pfluft", "lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Alle. Leipzig hat seinen Hustenreiz.", "tokens": ["Al\u00b7le", ".", "Leip\u00b7zig", "hat", "sei\u00b7nen", "Hus\u00b7ten\u00b7reiz", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "$.", "NE", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Man sollte im November Leipzig meiden,", "tokens": ["Man", "soll\u00b7te", "im", "No\u00b7vem\u00b7ber", "Leip\u00b7zig", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPRART", "NN", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach Frankreich reisen oder in die Schweiz.", "tokens": ["Nach", "Fran\u00b7kreich", "rei\u00b7sen", "o\u00b7der", "in", "die", "Schweiz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVINF", "KON", "APPR", "ART", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Die Fliege hat mir alle Lust genommen.", "tokens": ["Die", "Flie\u00b7ge", "hat", "mir", "al\u00b7le", "Lust", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich bin nicht wach und bin auch nicht im Schlaf.", "tokens": ["Ich", "bin", "nicht", "wach", "und", "bin", "auch", "nicht", "im", "Schlaf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "KON", "VAFIN", "ADV", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als m\u00fc\u00dfte ein Gewitter kommen.", "tokens": ["Als", "m\u00fc\u00df\u00b7te", "ein", "Ge\u00b7wit\u00b7ter", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ob wohl ein Blitz je eine Fliege traf?", "tokens": ["Ob", "wohl", "ein", "Blitz", "je", "ei\u00b7ne", "Flie\u00b7ge", "traf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}