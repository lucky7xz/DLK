{"textgrid.poem.36727": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie voll Unschuld du bist, du s\u00fc\u00df jungfr\u00e4uliches Antlitz,", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie voll Unschuld du bist, du s\u00fc\u00df jungfr\u00e4uliches Antlitz,", "tokens": ["Wie", "voll", "Un\u00b7schuld", "du", "bist", ",", "du", "s\u00fc\u00df", "jung\u00b7fr\u00e4u\u00b7li\u00b7ches", "Ant\u00b7litz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "PPER", "VAFIN", "$,", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "-+---+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So befangen, so sanft, kaum noch der Kindheit entbl\u00fcht.", "tokens": ["So", "be\u00b7fan\u00b7gen", ",", "so", "sanft", ",", "kaum", "noch", "der", "Kind\u00b7heit", "ent\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "ADJD", "$,", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Sch\u00fcchtern noch thust du, obwohl schon Mutter geworden, so bist du", "tokens": ["Sch\u00fcch\u00b7tern", "noch", "thust", "du", ",", "ob\u00b7wohl", "schon", "Mut\u00b7ter", "ge\u00b7wor\u00b7den", ",", "so", "bist", "du"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "PPER", "$,", "KOUS", "ADV", "NN", "VAPP", "$,", "ADV", "VAFIN", "PPER"], "meter": "+--+-++-+--+-+-+", "measure": "iambic.octa.plus.invert"}, "line.4": {"text": "Dir's nicht bewu\u00dft, und wei\u00dft selbst noch nicht, wie dir geschah.", "tokens": ["Dir's", "nicht", "be\u00b7wu\u00dft", ",", "und", "wei\u00dft", "selbst", "noch", "nicht", ",", "wie", "dir", "ge\u00b7schah", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADJD", "$,", "KON", "VVFIN", "ADV", "ADV", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}}}}}