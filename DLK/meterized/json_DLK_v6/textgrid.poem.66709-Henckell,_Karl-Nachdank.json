{"textgrid.poem.66709": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Nachdank", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es waren schwere, dunkele Zeiten ...", "tokens": ["Es", "wa\u00b7ren", "schwe\u00b7re", ",", "dun\u00b7ke\u00b7le", "Zei\u00b7ten", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Wogen schleuderten mich hinab", "tokens": ["Die", "Wo\u00b7gen", "schleu\u00b7der\u00b7ten", "mich", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.3": {"text": "In hoffnungverschlingenden Abgrund.", "tokens": ["In", "hoff\u00b7nung\u00b7ver\u00b7schlin\u00b7gen\u00b7den", "Ab\u00b7grund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Es waren leichte, leuchtende Tage ...", "tokens": ["Es", "wa\u00b7ren", "leich\u00b7te", ",", "leuch\u00b7ten\u00b7de", "Ta\u00b7ge", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Wellen wirbelten mich empor", "tokens": ["Die", "Wel\u00b7len", "wir\u00b7bel\u00b7ten", "mich", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Auf sonnengl\u00fccksch\u00e4umende Spitzen ...", "tokens": ["Auf", "son\u00b7nen\u00b7gl\u00fcck\u00b7sch\u00e4u\u00b7men\u00b7de", "Spit\u00b7zen", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Es kamen in dunkeln und leuchtenden Stunden", "tokens": ["Es", "ka\u00b7men", "in", "dun\u00b7keln", "und", "leuch\u00b7ten\u00b7den", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "KON", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Geflogen Zeichen freundlicher Huld,", "tokens": ["Ge\u00b7flo\u00b7gen", "Zei\u00b7chen", "freund\u00b7li\u00b7cher", "Huld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Zart fernhergr\u00fc\u00dfend Gedenken.", "tokens": ["Zart", "fern\u00b7her\u00b7gr\u00fc\u00b7\u00dfend", "Ge\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Versunken im Taumel der Finsternisse", "tokens": ["Ver\u00b7sun\u00b7ken", "im", "Tau\u00b7mel", "der", "Fins\u00b7ter\u00b7nis\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und Sonnenwirbel ist manch ein Dank \u2013", "tokens": ["Und", "Son\u00b7nen\u00b7wir\u00b7bel", "ist", "manch", "ein", "Dank", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PIAT", "ART", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nun denk' ich, nun dank' ich der Liebe.", "tokens": ["Nun", "denk'", "ich", ",", "nun", "dank'", "ich", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}}}}}