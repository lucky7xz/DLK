{"dta.poem.15018": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "FrAGMENTUM  \n der Vorrede zu den  \n  Tr \u2012 ll \u2012 rischen Fabeln,  \n Oder  \n  Schutz-Schrift  \n gegen den Verfasser der  \n  Critischen Dichtkunst.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1741", "urn": "urn:nbn:de:kobv:b4-200905198526", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der \u00e4rgerliche Geist, den viel Gelehrte treiben,", "tokens": ["Der", "\u00e4r\u00b7ger\u00b7li\u00b7che", "Geist", ",", "den", "viel", "Ge\u00b7lehr\u00b7te", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird von ihm lebenslang mit Ernst vermieden bleiben;", "tokens": ["Wird", "von", "ihm", "le\u00b7bens\u00b7lang", "mit", "Ernst", "ver\u00b7mie\u00b7den", "blei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "VVFIN", "APPR", "NE", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie mancher heist ein F\u00fcrst in Kunst und Wissenschaft,", "tokens": ["Wie", "man\u00b7cher", "heist", "ein", "F\u00fcrst", "in", "Kunst", "und", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und schreibt und zanckt sich doch mit andern p\u00f6belhaft.", "tokens": ["Und", "schreibt", "und", "zanckt", "sich", "doch", "mit", "an\u00b7dern", "p\u00f6\u00b7bel\u00b7haft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PRF", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer bey den K\u00fcnsten nicht die H\u00f6fligkeit studiret,", "tokens": ["Wer", "bey", "den", "K\u00fcns\u00b7ten", "nicht", "die", "H\u00f6f\u00b7lig\u00b7keit", "stu\u00b7di\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-++-+", "measure": "unknown.measure.septa"}, "line.6": {"text": "Scheint ihm wie eine Sau, mit g\u00fcldnem Band gezieret.", "tokens": ["Scheint", "ihm", "wie", "ei\u00b7ne", "Sau", ",", "mit", "g\u00fcld\u00b7nem", "Band", "ge\u00b7zie\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "ART", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}