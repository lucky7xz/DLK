{"textgrid.poem.26538": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie lieblich klingen deiner Dichtung Laute!", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie lieblich klingen deiner Dichtung Laute!", "tokens": ["Wie", "lieb\u00b7lich", "klin\u00b7gen", "dei\u00b7ner", "Dich\u00b7tung", "Lau\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und dennoch sind sie ungeh\u00f6rt verklungen;", "tokens": ["Und", "den\u00b7noch", "sind", "sie", "un\u00b7ge\u00b7h\u00f6rt", "ver\u00b7klun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von Allen, die da einst mit dir gesungen,", "tokens": ["Von", "Al\u00b7len", ",", "die", "da", "einst", "mit", "dir", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "ADV", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Warst du der Einz'ge, der den Himmel schaute.", "tokens": ["Warst", "du", "der", "Einz'\u00b7ge", ",", "der", "den", "Him\u00b7mel", "schau\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch von der D\u00e4mm'rung, die dich rings umgraute,", "tokens": ["Doch", "von", "der", "D\u00e4m\u00b7m'\u00b7rung", ",", "die", "dich", "rings", "um\u00b7grau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Ward auch zuletzt dein zarter Geist durchdrungen,", "tokens": ["Ward", "auch", "zu\u00b7letzt", "dein", "zar\u00b7ter", "Geist", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und eh' du v\u00f6llig dich an's Licht gerungen,", "tokens": ["Und", "eh'", "du", "v\u00f6l\u00b7lig", "dich", "an's", "Licht", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Versiegte leis' der Quell, der erst sich staute.", "tokens": ["Ver\u00b7sieg\u00b7te", "leis'", "der", "Quell", ",", "der", "erst", "sich", "stau\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,", "PRELS", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dir ward das unheilvollste Loos von allen:", "tokens": ["Dir", "ward", "das", "un\u00b7heil\u00b7volls\u00b7te", "Loos", "von", "al\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du sah'st dich, ach, f\u00fcr eine Zeit geboren,", "tokens": ["Du", "sah'st", "dich", ",", "ach", ",", "f\u00fcr", "ei\u00b7ne", "Zeit", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ITJ", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die nie gebaut an eig'nen Ruhmeshallen;", "tokens": ["Die", "nie", "ge\u00b7baut", "an", "eig'\u00b7nen", "Ruh\u00b7me\u00b7shal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die niemals sich ein hohes Ziel erkoren \u2013", "tokens": ["Die", "nie\u00b7mals", "sich", "ein", "ho\u00b7hes", "Ziel", "er\u00b7ko\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PRF", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wie sie mu\u00dfte in sich selbst zerfallen,", "tokens": ["Und", "wie", "sie", "mu\u00df\u00b7te", "in", "sich", "selbst", "zer\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VMFIN", "APPR", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So ging mit ihr ihr Bestes auch verloren.", "tokens": ["So", "ging", "mit", "ihr", "ihr", "Bes\u00b7tes", "auch", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}