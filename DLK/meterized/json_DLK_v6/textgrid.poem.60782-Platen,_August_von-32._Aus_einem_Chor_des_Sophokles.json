{"textgrid.poem.60782": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "32. Aus einem Chor des Sophokles", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht gezeugt sein, w\u00e4re das beste Schicksal,", "tokens": ["Nicht", "ge\u00b7zeugt", "sein", ",", "w\u00e4\u00b7re", "das", "bes\u00b7te", "Schick\u00b7sal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "VAINF", "$,", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Oder doch fr\u00fch sterben in zarter Kindheit:", "tokens": ["O\u00b7der", "doch", "fr\u00fch", "ster\u00b7ben", "in", "zar\u00b7ter", "Kind\u00b7heit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "W\u00e4chst zum J\u00fcngling einer empor, verfolgt ihn", "tokens": ["W\u00e4chst", "zum", "J\u00fcng\u00b7ling", "ei\u00b7ner", "em\u00b7por", ",", "ver\u00b7folgt", "ihn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "PTKVZ", "$,", "VVFIN", "PPER"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "\u00dcppige Torheit,", "tokens": ["\u00dcp\u00b7pi\u00b7ge", "Tor\u00b7heit", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "W\u00e4hrend Mi\u00dfgunst, Streit und Gefahr und Ha\u00df ihm", "tokens": ["W\u00e4h\u00b7rend", "Mi\u00df\u00b7gunst", ",", "Streit", "und", "Ge\u00b7fahr", "und", "Ha\u00df", "ihm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN", "KON", "NN", "PPER"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Qu\u00e4lend nahn; reift vollends hinan zum Greis er,", "tokens": ["Qu\u00e4\u00b7lend", "nahn", ";", "reift", "vol\u00b7lends", "hi\u00b7nan", "zum", "Greis", "er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "$.", "VVFIN", "ADV", "ADV", "APPRART", "NN", "PPER", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Jede Schmach mu\u00df dulden er dann, vereinzelt", "tokens": ["Je\u00b7de", "Schmach", "mu\u00df", "dul\u00b7den", "er", "dann", ",", "ver\u00b7ein\u00b7zelt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PIAT", "NN", "VMFIN", "VVINF", "PPER", "ADV", "$,", "VVFIN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Stehend und kraftlos.", "tokens": ["Ste\u00b7hend", "und", "kraft\u00b7los", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Stets umdroht uns Flutengedr\u00e4ng und schleudert", "tokens": ["Stets", "um\u00b7droht", "uns", "Flu\u00b7ten\u00b7ge\u00b7dr\u00e4ng", "und", "schleu\u00b7dert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "VVFIN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hart an steilabfallenden Klippenstrand uns,", "tokens": ["Hart", "an", "steil\u00b7ab\u00b7fal\u00b7len\u00b7den", "Klip\u00b7pen\u00b7strand", "uns", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Mag der S\u00fcd nun peitschen die Woge, mag sie", "tokens": ["Mag", "der", "S\u00fcd", "nun", "peit\u00b7schen", "die", "Wo\u00b7ge", ",", "mag", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "$,", "VMFIN", "PPER"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Schwellen der Nordsturm.", "tokens": ["Schwel\u00b7len", "der", "Nord\u00b7sturm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}