{"textgrid.poem.46603": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[es hat dir die Weihnachtszeit]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hat dir die Weihnachtszeit", "tokens": ["Es", "hat", "dir", "die", "Weih\u00b7nachts\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit andrer Pracht", "tokens": ["Mit", "an\u00b7drer", "Pracht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Auch eine Bauernhochzeit", "tokens": ["Auch", "ei\u00b7ne", "Bau\u00b7ern\u00b7hoch\u00b7zeit"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Spiel gebracht.", "tokens": ["Zum", "Spiel", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Gesellschaft bunt aus Pappe", "tokens": ["Ge\u00b7sell\u00b7schaft", "bunt", "aus", "Pap\u00b7pe"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf Holz geleimt,", "tokens": ["Auf", "Holz", "ge\u00b7leimt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Vom Strumpfe bis zur Kappe", "tokens": ["Vom", "Strump\u00b7fe", "bis", "zur", "Kap\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein Schmuck vers\u00e4umt.", "tokens": ["Kein", "Schmuck", "ver\u00b7s\u00e4umt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Mit Kn\u00f6pfen ihre Jacken", "tokens": ["Mit", "Kn\u00f6p\u00b7fen", "ih\u00b7re", "Ja\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Reich \u00fcbers\u00e4t,", "tokens": ["Reich", "\u00fc\u00b7bers\u00b7\u00e4t", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und ihre rothen Backen", "tokens": ["Und", "ih\u00b7re", "ro\u00b7then", "Ba\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Recht aufgebl\u00e4ht.", "tokens": ["Recht", "auf\u00b7ge\u00b7bl\u00e4ht", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Hier spielt man Ba\u00df und Geigen", "tokens": ["Hier", "spielt", "man", "Ba\u00df", "und", "Gei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Hackebrett,", "tokens": ["Und", "Hac\u00b7ke\u00b7brett", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Dort dreht man sich im Reigen", "tokens": ["Dort", "dreht", "man", "sich", "im", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Halb plump, halb nett.", "tokens": ["Halb", "plump", ",", "halb", "nett", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "ADJD", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Die meisten an den Tischen", "tokens": ["Die", "meis\u00b7ten", "an", "den", "Ti\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf Stuhl und Bank", "tokens": ["Auf", "Stuhl", "und", "Bank"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sind da, sich zu erfrischen", "tokens": ["Sind", "da", ",", "sich", "zu", "er\u00b7fri\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "$,", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Speis' und Trank.", "tokens": ["Mit", "Speis'", "und", "Trank", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Wer bringt zur Krankenkammer", "tokens": ["Wer", "bringt", "zur", "Kran\u00b7ken\u00b7kam\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Saus und Braus?", "tokens": ["Den", "Saus", "und", "Braus", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wie seltsam nimmt beim Jammer", "tokens": ["Wie", "selt\u00b7sam", "nimmt", "beim", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Lust sich aus!", "tokens": ["Die", "Lust", "sich", "aus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Doch mach' es dir noch heiter", "tokens": ["Doch", "mach'", "es", "dir", "noch", "hei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Lebensrest!", "tokens": ["Den", "Le\u00b7bens\u00b7rest", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+--", "measure": "dactylic.init"}, "line.3": {"text": "Du Armer siehst nichts weiter", "tokens": ["Du", "Ar\u00b7mer", "siehst", "nichts", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "VVFIN", "PIS", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vom Lebensfest.", "tokens": ["Vom", "Le\u00b7bens\u00b7fest", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Du hast, zum Tod gelagert,", "tokens": ["Du", "hast", ",", "zum", "Tod", "ge\u00b7la\u00b7gert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch halb erweckt,", "tokens": ["Noch", "halb", "er\u00b7weckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die H\u00e4ndchen abgemagert", "tokens": ["Die", "H\u00e4nd\u00b7chen", "ab\u00b7ge\u00b7ma\u00b7gert"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Danach gestreckt.", "tokens": ["Da\u00b7nach", "ge\u00b7streckt", "."], "token_info": ["word", "word", "punct"], "pos": ["PAV", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Du spielst noch, weil du spieltest", "tokens": ["Du", "spielst", "noch", ",", "weil", "du", "spiel\u00b7test"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein Lebenlang,", "tokens": ["Dein", "Le\u00b7ben\u00b7lang", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Doch ist's, als ob du hieltest", "tokens": ["Doch", "ist's", ",", "als", "ob", "du", "hiel\u00b7test"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NE", "$,", "KOKOM", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein Spiel mit Zwang.", "tokens": ["Dein", "Spiel", "mit", "Zwang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Entlocken dir die Possen", "tokens": ["Ent\u00b7lo\u00b7cken", "dir", "die", "Pos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht einen Ton?", "tokens": ["Nicht", "ei\u00b7nen", "Ton", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Du h\u00e4ltst den Mund verschlossen", "tokens": ["Du", "h\u00e4ltst", "den", "Mund", "ver\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Seit Tagen schon.", "tokens": ["Seit", "Ta\u00b7gen", "schon", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "Sie sehn mit starrem Lachen", "tokens": ["Sie", "sehn", "mit", "star\u00b7rem", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dir ins Gesicht,", "tokens": ["Dir", "ins", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Um lachen dich zu machen;", "tokens": ["Um", "la\u00b7chen", "dich", "zu", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du kannst es nicht.", "tokens": ["Du", "kannst", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "Und nun den Blick, den starren,", "tokens": ["Und", "nun", "den", "Blick", ",", "den", "star\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du zugethan,", "tokens": ["Du", "zu\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sehn lachend dich die Narren", "tokens": ["Sehn", "la\u00b7chend", "dich", "die", "Nar\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch immer an.", "tokens": ["Noch", "im\u00b7mer", "an", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.13": {"line.1": {"text": "Zum Trauern w\u00e4r' es doch Zeit", "tokens": ["Zum", "Trau\u00b7ern", "w\u00e4r'", "es", "doch", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADV", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Im Trauerhaus,", "tokens": ["Im", "Trau\u00b7er\u00b7haus", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Doch bleibt die Bauernhochzeit", "tokens": ["Doch", "bleibt", "die", "Bau\u00b7ern\u00b7hoch\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Beim Hochzeitschmaus.", "tokens": ["Beim", "Hoch\u00b7zeit\u00b7schmaus", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.14": {"line.1": {"text": "Ihr stummen Musikanten", "tokens": ["Ihr", "stum\u00b7men", "Mu\u00b7si\u00b7kan\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Spielt auf der Braut,", "tokens": ["Spielt", "auf", "der", "Braut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und la\u00dft die Anverwandten", "tokens": ["Und", "la\u00dft", "die", "An\u00b7ver\u00b7wand\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hier klagen laut.", "tokens": ["Hier", "kla\u00b7gen", "laut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}