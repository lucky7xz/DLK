{"textgrid.poem.31447": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nvr ruhe / meine Seel' / in Gottes tieffen wunden.", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nvr ruhe / meine Seel' / in Gottes tieffen wunden.", "tokens": ["Nvr", "ru\u00b7he", "/", "mei\u00b7ne", "Seel'", "/", "in", "Got\u00b7tes", "tief\u00b7fen", "wun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "PPOSAT", "NN", "$(", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Versenk all deine bitt' / in seinem Meer voll Blut;", "tokens": ["Ver\u00b7senk", "all", "dei\u00b7ne", "bitt'", "/", "in", "sei\u00b7nem", "Meer", "voll", "Blut", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "PPOSAT", "VVFIN", "$(", "APPR", "PPOSAT", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die kleinhett deiner sach' / in di\u00df Allwesend Gut.", "tokens": ["die", "klein\u00b7hett", "dei\u00b7ner", "sach'", "/", "in", "di\u00df", "A\u00b7llwe\u00b7send", "Gut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJD", "$(", "APPR", "PDS", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was das gesch\u00f6pfversagt / wird in dem Sch\u00f6pfer funden.", "tokens": ["Was", "das", "ge\u00b7sch\u00f6pf\u00b7ver\u00b7sagt", "/", "wird", "in", "dem", "Sch\u00f6p\u00b7fer", "fun\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "VVPP", "$(", "VAFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sihstu schon keinen trost in dieser Erden-Runden:", "tokens": ["Sih\u00b7stu", "schon", "kei\u00b7nen", "trost", "in", "die\u00b7ser", "Er\u00b7den\u00b7Run\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PIAT", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "wi\u00df / Gott ist gr\u00f6sser noch / als sie / dein Herz und muht.", "tokens": ["wi\u00df", "/", "Gott", "ist", "gr\u00f6s\u00b7ser", "noch", "/", "als", "sie", "/", "dein", "Herz", "und", "muht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "NN", "VAFIN", "ADJD", "ADV", "$(", "KOUS", "PPER", "$(", "PPOSAT", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die wunder er allzeit auf \u00fcber-Irdisch thut.", "tokens": ["Die", "wun\u00b7der", "er", "all\u00b7zeit", "auf", "\u00fc\u00b7ber\u00b7Ir\u00b7disch", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "APPR", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ihr wesen wird daraus / wann alles seyn verschwunden.", "tokens": ["Ihr", "we\u00b7sen", "wird", "da\u00b7raus", "/", "wann", "al\u00b7les", "seyn", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VAFIN", "PAV", "$(", "PWAV", "PIS", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wann Geist-erforschung schon sich weiter nicht erstreckt /", "tokens": ["Wann", "Geist\u00b7er\u00b7for\u00b7schung", "schon", "sich", "wei\u00b7ter", "nicht", "er\u00b7streckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "PRF", "ADV", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "so la\u00df die sinnen in des Glaubens s\u00fc\u00dfheit schlaffen:", "tokens": ["so", "la\u00df", "die", "sin\u00b7nen", "in", "des", "Glau\u00b7bens", "s\u00fc\u00df\u00b7heit", "schlaf\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "ADJA", "APPR", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "dann wird dir Gott daraus ergetzlichkeit erschaffen", "tokens": ["dann", "wird", "dir", "Gott", "da\u00b7raus", "er\u00b7getz\u00b7lich\u00b7keit", "er\u00b7schaf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "PAV", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "und angenehme Freud / wann er dich wider weckt.", "tokens": ["und", "an\u00b7ge\u00b7neh\u00b7me", "Freud", "/", "wann", "er", "dich", "wi\u00b7der", "weckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$(", "PWAV", "PPER", "PRF", "APPR", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Traustu dir h\u00f6her nicht durch Hoffnung aufzufliegen:", "tokens": ["Traus\u00b7tu", "dir", "h\u00f6\u00b7her", "nicht", "durch", "Hoff\u00b7nung", "auf\u00b7zu\u00b7flie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "|warheit |", "tokens": ["|wa\u00b7rheit", "|"], "token_info": ["word", "word"], "pos": ["NN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "so bleib' im tieffen grund der\u203aAllmacht\u2039 G0ttes", "tokens": ["so", "bleib'", "im", "tief\u00b7fen", "grund", "der", "\u203a", "All\u00b7macht", "\u2039", "G0\u00b7ttes"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$(", "NN"], "meter": "-+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.16": {"text": "|g\u00fcte | liegen.", "tokens": ["|g\u00fc\u00b7te", "|", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "$(", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}