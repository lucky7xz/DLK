{"textgrid.poem.41553": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am vierten Sonntage nach Pfingsten", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So ist aus deines heil'gen Buches Schein", "tokens": ["So", "ist", "aus", "dei\u00b7nes", "heil'\u00b7gen", "Bu\u00b7ches", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gefallen denn ein Strahl in meine Nacht,", "tokens": ["Ge\u00b7fal\u00b7len", "denn", "ein", "Strahl", "in", "mei\u00b7ne", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In meines Herzens modergrauen Schacht.", "tokens": ["In", "mei\u00b7nes", "Her\u00b7zens", "mo\u00b7der\u00b7grau\u00b7en", "Schacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du gabst ihn Herr, du hast mir selbst gebracht", "tokens": ["Du", "gabst", "ihn", "Herr", ",", "du", "hast", "mir", "selbst", "ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was ewig meiner Hoffnung Edelstein.", "tokens": ["Was", "e\u00b7wig", "mei\u00b7ner", "Hoff\u00b7nung", "E\u00b7del\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es ist zuviel, zuviel; ich fa\u00df es kaum:", "tokens": ["Es", "ist", "zu\u00b7viel", ",", "zu\u00b7viel", ";", "ich", "fa\u00df", "es", "kaum", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "$,", "PIS", "$.", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Um meine ganz versunkne Seele, weh,", "tokens": ["Um", "mei\u00b7ne", "ganz", "ver\u00b7sunk\u00b7ne", "See\u00b7le", ",", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADV", "ADJA", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So \u00f6d und aschig wie Gomorrhas See,", "tokens": ["So", "\u00f6d", "und", "asc\u00b7hig", "wie", "Go\u00b7morr\u00b7has", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "KOKOM", "NE", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Um sie soll Freude sein in deiner H\u00f6h'!", "tokens": ["Um", "sie", "soll", "Freu\u00b7de", "sein", "in", "dei\u00b7ner", "H\u00f6h'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VMFIN", "NN", "PPOSAT", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es ist zuviel, weh mir! es ist ein Traum!", "tokens": ["Es", "ist", "zu\u00b7viel", ",", "weh", "mir", "!", "es", "ist", "ein", "Traum", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "$,", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Kann wachsen denn, wie des Polypen Arm,", "tokens": ["Kann", "wach\u00b7sen", "denn", ",", "wie", "des", "Po\u00b7ly\u00b7pen", "Arm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "KON", "$,", "PWAV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus Tr\u00e4nen die verlorne Eigenschaft?", "tokens": ["Aus", "Tr\u00e4\u00b7nen", "die", "ver\u00b7lor\u00b7ne", "Ei\u00b7gen\u00b7schaft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zieht mit der Reue wieder ein die Kraft?", "tokens": ["Zieht", "mit", "der", "Reu\u00b7e", "wie\u00b7der", "ein", "die", "Kraft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "ART", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Ist es genug, wenn tot die Leidenschaft", "tokens": ["Ist", "es", "ge\u00b7nug", ",", "wenn", "tot", "die", "Lei\u00b7den\u00b7schaft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "$,", "KOUS", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zerfressen liegt wie von Insektenschwarm?", "tokens": ["Zer\u00b7fres\u00b7sen", "liegt", "wie", "von", "In\u00b7sek\u00b7ten\u00b7schwarm", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KOKOM", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ist es genug vor deiner Gnad' und Lieb',", "tokens": ["Ist", "es", "ge\u00b7nug", "vor", "dei\u00b7ner", "Gnad'", "und", "Lieb'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn \u00fcber das Geb\u00e4ude ausgebrannt", "tokens": ["Wenn", "\u00fc\u00b7ber", "das", "Ge\u00b7b\u00e4u\u00b7de", "aus\u00b7ge\u00b7brannt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sich sehnsuchtsvoll und betend streckt die Hand,", "tokens": ["Sich", "sehn\u00b7suchts\u00b7voll", "und", "be\u00b7tend", "streckt", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Hand, so alle \u00dcbel ausgesandt,", "tokens": ["Die", "Hand", ",", "so", "al\u00b7le", "\u00dc\u00b7bel", "aus\u00b7ge\u00b7sandt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Hand, der, ach, das brand'ge Zeichen blieb?", "tokens": ["Die", "Hand", ",", "der", ",", "ach", ",", "das", "bran\u00b7d'\u00b7ge", "Zei\u00b7chen", "blieb", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$,", "ITJ", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Und doch hast du ein heilig Wort gesandt", "tokens": ["Und", "doch", "hast", "du", "ein", "hei\u00b7lig", "Wort", "ge\u00b7sandt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "ADJD", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Uns bindend mit gewalt'ger Gnadenpflicht,", "tokens": ["Uns", "bin\u00b7dend", "mit", "ge\u00b7walt'\u00b7ger", "Gna\u00b7den\u00b7pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu glauben gegen eigenes Gericht,", "tokens": ["Zu", "glau\u00b7ben", "ge\u00b7gen", "ei\u00b7ge\u00b7nes", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was st\u00f6hnend aus des Herzens Kammern bricht", "tokens": ["Was", "st\u00f6h\u00b7nend", "aus", "des", "Her\u00b7zens", "Kam\u00b7mern", "bricht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "APPR", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und selber die Verwerfung sich erkannt.", "tokens": ["Und", "sel\u00b7ber", "die", "Ver\u00b7wer\u00b7fung", "sich", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Zu glauben ach wie s\u00fc\u00df und ach wie schwer!", "tokens": ["Zu", "glau\u00b7ben", "ach", "wie", "s\u00fc\u00df", "und", "ach", "wie", "schwer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "KOKOM", "ADJD", "KON", "ADV", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Weh! nicht auf meine S\u00fcnden darf ich schaun,", "tokens": ["Weh", "!", "nicht", "auf", "mei\u00b7ne", "S\u00fcn\u00b7den", "darf", "ich", "schaun", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PTKNEG", "APPR", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Soll nicht in ihrem Schlamme das Vertraun", "tokens": ["Soll", "nicht", "in", "ih\u00b7rem", "Schlam\u00b7me", "das", "Ver\u00b7traun"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ersticken, wie ein Wild in Sumpfes Graun,", "tokens": ["Er\u00b7sti\u00b7cken", ",", "wie", "ein", "Wild", "in", "Sump\u00b7fes", "Graun", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "APPR", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie ein Gev\u00f6gel ob dem Toten Meer.", "tokens": ["Wie", "ein", "Ge\u00b7v\u00f6\u00b7gel", "ob", "dem", "To\u00b7ten", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Was du gesprochen, Herr, wer meistert's k\u00fchn,", "tokens": ["Was", "du", "ge\u00b7spro\u00b7chen", ",", "Herr", ",", "wer", "meis\u00b7tert's", "k\u00fchn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "NN", "$,", "PWS", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bist gn\u00e4d'ger du als Menschensinn ermi\u00dft?", "tokens": ["Bist", "gn\u00e4d'\u00b7ger", "du", "als", "Men\u00b7schen\u00b7sinn", "er\u00b7mi\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So bist du Herr der Heiland und der Christ;", "tokens": ["So", "bist", "du", "Herr", "der", "Hei\u00b7land", "und", "der", "Christ", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ich, die nur ein armer Schatten ist,", "tokens": ["Und", "ich", ",", "die", "nur", "ein", "ar\u00b7mer", "Schat\u00b7ten", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PRELS", "ADV", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was darf ich anders tun als glaubend knien!", "tokens": ["Was", "darf", "ich", "an\u00b7ders", "tun", "als", "glau\u00b7bend", "kni\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "KOKOM", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}