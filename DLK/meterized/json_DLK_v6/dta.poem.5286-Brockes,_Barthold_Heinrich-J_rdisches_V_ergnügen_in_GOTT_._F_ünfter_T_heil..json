{"dta.poem.5286": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Knospen, so noch nicht ge\u00f6fnet, und, in nicht minden", "tokens": ["Die", "Knos\u00b7pen", ",", "so", "noch", "nicht", "ge\u00b7\u00f6f\u00b7net", ",", "und", ",", "in", "nicht", "min\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "PTKNEG", "VVPP", "$,", "KON", "$,", "APPR", "PTKNEG", "VVINF"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.2": {"text": "sch\u00f6nem Gr\u00fcnen", "tokens": ["sch\u00f6\u00b7nem", "Gr\u00fc\u00b7nen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Des Laubes, gleichfals g\u00fclden schienen,", "tokens": ["Des", "Lau\u00b7bes", ",", "gleich\u00b7fals", "g\u00fcl\u00b7den", "schie\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vermehrten noch des Busches Glantz,", "tokens": ["Ver\u00b7mehr\u00b7ten", "noch", "des", "Bu\u00b7sches", "Glantz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und was ihn vollends herrlich machte ist, da\u00df der Blumen", "tokens": ["Und", "was", "ihn", "vol\u00b7lends", "herr\u00b7lich", "mach\u00b7te", "ist", ",", "da\u00df", "der", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "ADV", "ADJD", "VVFIN", "VAFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Meng' ihn gantz,", "tokens": ["Meng'", "ihn", "gantz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Und mehr fast, als das Laub sie, deckte. Wir freuten uns,", "tokens": ["Und", "mehr", "fast", ",", "als", "das", "Laub", "sie", ",", "deck\u00b7te", ".", "Wir", "freu\u00b7ten", "uns", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$,", "KOUS", "ART", "NN", "PPER", "$,", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "$,"], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "von GOttes Wercken", "tokens": ["von", "Got\u00b7tes", "Wer\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "In dieser neuen Wunder-Blum\u2019 ein neues Wunder zu be-", "tokens": ["In", "die\u00b7ser", "neu\u00b7en", "Wun\u00b7der\u00b7Blum'", "ein", "neu\u00b7es", "Wun\u00b7der", "zu", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ART", "ADJA", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "mercken,", "tokens": ["mer\u00b7cken", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Und danckten ihm, da\u00df er, als Sch\u00f6pfer, in seiner Crea-", "tokens": ["Und", "danck\u00b7ten", "ihm", ",", "da\u00df", "er", ",", "als", "Sch\u00f6p\u00b7fer", ",", "in", "sei\u00b7ner", "Crea"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "$,", "KOUS", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "turen Pracht,", "tokens": ["tu\u00b7ren", "Pracht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Auf eine nie geseh\u2019ne Weise, uns abermahl sich sichtbar", "tokens": ["Auf", "ei\u00b7ne", "nie", "ge\u00b7seh'\u00b7ne", "Wei\u00b7se", ",", "uns", "a\u00b7ber\u00b7mahl", "sich", "sicht\u00b7bar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$,", "PPER", "ADV", "PRF", "ADJD"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "macht.", "tokens": ["macht", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}}}}}