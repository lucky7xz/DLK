{"textgrid.poem.59646": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Abschied von der Welt", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ade! Ich mu\u00df nun scheiden,", "tokens": ["A\u00b7de", "!", "Ich", "mu\u00df", "nun", "schei\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Ihr Freunde, gute Nacht!", "tokens": ["Ihr", "Freun\u00b7de", ",", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In Freuden und in Leiden", "tokens": ["In", "Freu\u00b7den", "und", "in", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gar schwer ist's mir gemacht,", "tokens": ["Gar", "schwer", "ist's", "mir", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In Kummer und in Tr\u00e4nen,", "tokens": ["In", "Kum\u00b7mer", "und", "in", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In Arbeit und in Not;", "tokens": ["In", "Ar\u00b7beit", "und", "in", "Not", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Drum ruft mein hei\u00dfes Sehnen:", "tokens": ["Drum", "ruft", "mein", "hei\u00b7\u00dfes", "Seh\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "O komm, mein Herr und Gott!", "tokens": ["O", "komm", ",", "mein", "Herr", "und", "Gott", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O komm und schleu\u00df dem Matten", "tokens": ["O", "komm", "und", "schleu\u00df", "dem", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die m\u00fcden Augen zu,", "tokens": ["Die", "m\u00fc\u00b7den", "Au\u00b7gen", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bett' ihm im k\u00fchlen Schatten", "tokens": ["Bett'", "ihm", "im", "k\u00fch\u00b7len", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die stille, sanfte Ruh',", "tokens": ["Die", "stil\u00b7le", ",", "sanf\u00b7te", "Ruh'", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bett' ihm im k\u00fchlen Grabe", "tokens": ["Bett'", "ihm", "im", "k\u00fch\u00b7len", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den letzten weichen Pf\u00fchl,", "tokens": ["Den", "letz\u00b7ten", "wei\u00b7chen", "Pf\u00fchl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die letzte Liebesgabe", "tokens": ["Die", "letz\u00b7te", "Lie\u00b7bes\u00b7ga\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vom ganzen Weltgew\u00fchl.", "tokens": ["Vom", "gan\u00b7zen", "Welt\u00b7ge\u00b7w\u00fchl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ade! Ihr sollt nicht weinen,", "tokens": ["A\u00b7de", "!", "Ihr", "sollt", "nicht", "wei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Ihr Freunde lieb und fromm,", "tokens": ["Ihr", "Freun\u00b7de", "lieb", "und", "fromm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Licht wird wieder scheinen,", "tokens": ["Das", "Licht", "wird", "wie\u00b7der", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das ruft dem Schl\u00e4fer: Komm!", "tokens": ["Das", "ruft", "dem", "Schl\u00e4\u00b7fer", ":", "Komm", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$.", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das klingt in seine Kammer:", "tokens": ["Das", "klingt", "in", "sei\u00b7ne", "Kam\u00b7mer", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Steh, Schl\u00e4fer, steh nun auf!", "tokens": ["Steh", ",", "Schl\u00e4\u00b7fer", ",", "steh", "nun", "auf", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Steh auf vom Erdenjammer!", "tokens": ["Steh", "auf", "vom", "Er\u00b7den\u00b7jam\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dein Himmel tut sich auf.", "tokens": ["Dein", "Him\u00b7mel", "tut", "sich", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ade! Ihr sollt nicht klagen,", "tokens": ["A\u00b7de", "!", "Ihr", "sollt", "nicht", "kla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Da\u00df nun ich hinnen mu\u00df,", "tokens": ["Da\u00df", "nun", "ich", "hin\u00b7nen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Nacht wird wieder tagen", "tokens": ["Die", "Nacht", "wird", "wie\u00b7der", "ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Freuden\u00fcberflu\u00df,", "tokens": ["Mit", "Freu\u00b7den\u00b7\u00fc\u00b7berf\u00b7lu\u00df", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der gro\u00dfe Held der Frommen", "tokens": ["Der", "gro\u00b7\u00dfe", "Held", "der", "From\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wird mit der Krone stehn,", "tokens": ["Wird", "mit", "der", "Kro\u00b7ne", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und Engel werden kommen", "tokens": ["Und", "En\u00b7gel", "wer\u00b7den", "kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und mich zu Gott erh\u00f6hn.", "tokens": ["Und", "mich", "zu", "Gott", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}