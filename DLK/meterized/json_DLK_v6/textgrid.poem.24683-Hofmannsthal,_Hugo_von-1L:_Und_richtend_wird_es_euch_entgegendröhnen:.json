{"textgrid.poem.24683": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und richtend wird es euch entgegendr\u00f6hnen:", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und richtend wird es euch entgegendr\u00f6hnen:", "tokens": ["Und", "rich\u00b7tend", "wird", "es", "euch", "ent\u00b7ge\u00b7gen\u00b7dr\u00f6h\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbverfluchte Schar von Gegenwartsver\u00e4chtern!", "tokens": ["\u00bb", "ver\u00b7fluch\u00b7te", "Schar", "von", "Ge\u00b7gen\u00b7warts\u00b7ver\u00b7\u00e4ch\u00b7tern", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gewandelt seid ihr ", "tokens": ["Ge\u00b7wan\u00b7delt", "seid", "ihr"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Den V\u00e4tern fremd und fremd den eignen S\u00f6hnen;", "tokens": ["Den", "V\u00e4\u00b7tern", "fremd", "und", "fremd", "den", "eig\u00b7nen", "S\u00f6h\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ihr schwanktet kl\u00e4glich zwischen den Verfechtern", "tokens": ["Ihr", "schwank\u00b7tet", "kl\u00e4g\u00b7lich", "zwi\u00b7schen", "den", "Ver\u00b7fech\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von neuen Farben, neuen eignen T\u00f6nen,", "tokens": ["Von", "neu\u00b7en", "Far\u00b7ben", ",", "neu\u00b7en", "eig\u00b7nen", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von neuem Zweifeln, Suchen, Lachen, St\u00f6hnen,", "tokens": ["Von", "neu\u00b7em", "Zwei\u00b7feln", ",", "Su\u00b7chen", ",", "La\u00b7chen", ",", "St\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und zwischen des Ererbten starren W\u00e4chtern.", "tokens": ["Und", "zwi\u00b7schen", "des", "Er\u00b7erb\u00b7ten", "star\u00b7ren", "W\u00e4ch\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "In Unverstehen seid ihr hingegangen", "tokens": ["In", "Un\u00b7ver\u00b7ste\u00b7hen", "seid", "ihr", "hin\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch aller St\u00fcrme heilig gro\u00dfes Grauen,", "tokens": ["Durch", "al\u00b7ler", "St\u00fcr\u00b7me", "hei\u00b7lig", "gro\u00b7\u00dfes", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch aller Farben gl\u00fchend starkes Prangen", "tokens": ["Durch", "al\u00b7ler", "Far\u00b7ben", "gl\u00fc\u00b7hend", "star\u00b7kes", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "In taubem H\u00f6ren und in blindem Schauen:", "tokens": ["In", "tau\u00b7bem", "H\u00f6\u00b7ren", "und", "in", "blin\u00b7dem", "Schau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "All Eines ist der Anfang und das Ende,", "tokens": ["All", "Ei\u00b7nes", "ist", "der", "An\u00b7fang", "und", "das", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIS", "VAFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wo du stehst, dort ist die Zeitenwende!\u00ab", "tokens": ["Und", "wo", "du", "stehst", ",", "dort", "ist", "die", "Zei\u00b7ten\u00b7wen\u00b7de", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}