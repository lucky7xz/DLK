{"textgrid.poem.47098": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel hat eine Thr\u00e4ne geweint,", "tokens": ["Der", "Him\u00b7mel", "hat", "ei\u00b7ne", "Thr\u00e4\u00b7ne", "ge\u00b7weint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die hat sich ins Meer zu verlieren gemeint.", "tokens": ["Die", "hat", "sich", "ins", "Meer", "zu", "ver\u00b7lie\u00b7ren", "ge\u00b7meint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PRF", "APPRART", "NN", "PTKZU", "VVINF", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die Muschel kam und schlo\u00df sie ein;", "tokens": ["Die", "Mu\u00b7schel", "kam", "und", "schlo\u00df", "sie", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du sollst nun meine Perle sein.", "tokens": ["Du", "sollst", "nun", "mei\u00b7ne", "Per\u00b7le", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du sollst nicht vor den Wogen zagen,", "tokens": ["Du", "sollst", "nicht", "vor", "den", "Wo\u00b7gen", "za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich will hindurch dich ruhig tragen.", "tokens": ["Ich", "will", "hin\u00b7durch", "dich", "ru\u00b7hig", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PAV", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "O du mein Schmerz, du meine Lust,", "tokens": ["O", "du", "mein", "Schmerz", ",", "du", "mei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "$,", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Du Himmelsthr\u00e4n' in meiner Brust!", "tokens": ["Du", "Him\u00b7mels\u00b7thr\u00e4n'", "in", "mei\u00b7ner", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Gib, Himmel, da\u00df ich in reinem Gem\u00fcte", "tokens": ["Gib", ",", "Him\u00b7mel", ",", "da\u00df", "ich", "in", "rei\u00b7nem", "Ge\u00b7m\u00fc\u00b7te"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "NN", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Den reinsten deiner Tropfen h\u00fcte!", "tokens": ["Den", "reins\u00b7ten", "dei\u00b7ner", "Trop\u00b7fen", "h\u00fc\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}