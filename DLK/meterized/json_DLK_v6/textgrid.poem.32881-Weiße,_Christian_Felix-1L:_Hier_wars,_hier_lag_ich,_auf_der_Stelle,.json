{"textgrid.poem.32881": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier wars, hier lag ich, auf der Stelle,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier wars, hier lag ich, auf der Stelle,", "tokens": ["Hier", "wars", ",", "hier", "lag", "ich", ",", "auf", "der", "Stel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In diesem veilchenvollen Gras;", "tokens": ["In", "die\u00b7sem", "veil\u00b7chen\u00b7vol\u00b7len", "Gras", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An diesem Baum, bey dieser Quelle,", "tokens": ["An", "die\u00b7sem", "Baum", ",", "bey", "die\u00b7ser", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da tr\u00e4umte mir vom jungen Licidas!", "tokens": ["Da", "tr\u00e4um\u00b7te", "mir", "vom", "jun\u00b7gen", "Li\u00b7ci\u00b7das", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der B\u00fcsche kleine S\u00e4nger f\u00fchlten", "tokens": ["Der", "B\u00fc\u00b7sche", "klei\u00b7ne", "S\u00e4n\u00b7ger", "f\u00fchl\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Lenz, und sangen Lieb und Lust!", "tokens": ["Den", "Lenz", ",", "und", "san\u00b7gen", "Lieb", "und", "Lust", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich lag, die freyen Zephyr spielten", "tokens": ["Ich", "lag", ",", "die", "frey\u00b7en", "Ze\u00b7phyr", "spiel\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Muthwillig mir um die halb offne Brust.", "tokens": ["Mut\u00b7hwil\u00b7lig", "mir", "um", "die", "halb", "off\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da kam er, o mit welchen Blicken!", "tokens": ["Da", "kam", "er", ",", "o", "mit", "wel\u00b7chen", "Bli\u00b7cken", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "FM", "APPR", "PWAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Beredter, als das, was er sprach:", "tokens": ["Be\u00b7red\u00b7ter", ",", "als", "das", ",", "was", "er", "sprach", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PDS", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Liebe Sehnsucht auszudr\u00fccken,", "tokens": ["Der", "Lie\u00b7be", "Sehn\u00b7sucht", "aus\u00b7zu\u00b7dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Versucht er oft, und stammelte nur Ach!", "tokens": ["Ver\u00b7sucht", "er", "oft", ",", "und", "stam\u00b7mel\u00b7te", "nur", "Ach", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KON", "VVFIN", "ADV", "ITJ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich seufzt ihm nach: wir seufzten beyde", "tokens": ["Ich", "seufzt", "ihm", "nach", ":", "wir", "seufz\u00b7ten", "bey\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Uns endlich unsern stillen Schmerz;", "tokens": ["Uns", "end\u00b7lich", "un\u00b7sern", "stil\u00b7len", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ich gestand ihm, welche Freude!", "tokens": ["Und", "ich", "ge\u00b7stand", "ihm", ",", "wel\u00b7che", "Freu\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "PWAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Voll Z\u00e4rtlichkeit, ihm, wie er mir, sein Herz.", "tokens": ["Voll", "Z\u00e4rt\u00b7lich\u00b7keit", ",", "ihm", ",", "wie", "er", "mir", ",", "sein", "Herz", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PPER", "$,", "PWAV", "PPER", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Frey k\u00fc\u00dft er mich, und welches Gl\u00fccke!", "tokens": ["Frey", "k\u00fc\u00dft", "er", "mich", ",", "und", "wel\u00b7ches", "Gl\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "$,", "KON", "PWAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich ward nicht einmal roth dabey.", "tokens": ["Ich", "ward", "nicht", "ein\u00b7mal", "roth", "da\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "ADJD", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich gab ihm jeden Ku\u00df zur\u00fccke;", "tokens": ["Ich", "gab", "ihm", "je\u00b7den", "Ku\u00df", "zu\u00b7r\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich, ich? und ward nicht einmal roth dabey!", "tokens": ["Ich", ",", "ich", "?", "und", "ward", "nicht", "ein\u00b7mal", "roth", "da\u00b7bey", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "$.", "KON", "VAFIN", "PTKNEG", "ADV", "ADJD", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Hier schlummr' ich ietzt von neuem wieder,", "tokens": ["Hier", "schlumm\u00b7r'", "ich", "ietzt", "von", "neu\u00b7em", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "O Traumgott! komm mit leisen Schritt,", "tokens": ["O", "Traum\u00b7gott", "!", "komm", "mit", "lei\u00b7sen", "Schritt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und zeige mir den Sch\u00e4fer wieder! \u2013 \u2013", "tokens": ["Und", "zei\u00b7ge", "mir", "den", "Sch\u00e4\u00b7fer", "wie\u00b7der", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NE", "ADV", "$.", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hast du noch mehr der Freuden, bring sie mit!", "tokens": ["Hast", "du", "noch", "mehr", "der", "Freu\u00b7den", ",", "bring", "sie", "mit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}