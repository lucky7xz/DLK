{"textgrid.poem.56794": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Jahrhunderttausende durchmi\u00dft mein Geist ...", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jahrhunderttausende durchmi\u00dft mein Geist ...", "tokens": ["Jahr\u00b7hun\u00b7dert\u00b7tau\u00b7sen\u00b7de", "durch\u00b7mi\u00dft", "mein", "Geist", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.2": {"text": "Verwandelt ist der Erde Angesicht,", "tokens": ["Ver\u00b7wan\u00b7delt", "ist", "der", "Er\u00b7de", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "der Menschheit letzte Horde tief vergreist.", "tokens": ["der", "Menschheit", "letz\u00b7te", "Hor\u00b7de", "tief", "ver\u00b7greist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Kaum bricht durch Wolken mehr das liebe Licht.", "tokens": ["Kaum", "bricht", "durch", "Wol\u00b7ken", "mehr", "das", "lie\u00b7be", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbwie alt sind wohl die Menschen?\u00ab fragt ein Kind", "tokens": ["\u00bb", "wie", "alt", "sind", "wohl", "die", "Men\u00b7schen", "?", "\u00ab", "fragt", "ein", "Kind"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "PWAV", "ADJD", "VAFIN", "ADV", "ART", "NN", "$.", "$(", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "den Vater. Und ich h\u00f6re, wie der spricht:", "tokens": ["den", "Va\u00b7ter", ".", "Und", "ich", "h\u00f6\u00b7re", ",", "wie", "der", "spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "KON", "PPER", "VVFIN", "$,", "PWAV", "ART", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbso alt, mein Liebling, als die Sterne sind!\u00ab", "tokens": ["\u00bb", "so", "alt", ",", "mein", "Lieb\u00b7ling", ",", "als", "die", "Ster\u00b7ne", "sind", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADJD", "$,", "PPOSAT", "NN", "$,", "KOUS", "ART", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbwas sind das, Sterne, Vater?\u00ab \u00bbSp\u00e4h einmal,", "tokens": ["\u00bb", "was", "sind", "das", ",", "Ster\u00b7ne", ",", "Va\u00b7ter", "?", "\u00ab", "\u00bb", "Sp\u00e4h", "ein\u00b7mal", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "PDS", "$,", "NN", "$,", "NN", "$.", "$(", "$(", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "wenn nachts im Nebel w\u00fchlt der wilde Wind.", "tokens": ["wenn", "nachts", "im", "Ne\u00b7bel", "w\u00fchlt", "der", "wil\u00b7de", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Vielleicht ersp\u00e4hst du einen stillen Strahl:", "tokens": ["Viel\u00b7leicht", "er\u00b7sp\u00e4hst", "du", "ei\u00b7nen", "stil\u00b7len", "Strahl", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der kommt von Welten, die unendlich fern;", "tokens": ["Der", "kommt", "von", "Wel\u00b7ten", ",", "die", "un\u00b7end\u00b7lich", "fern", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "$,", "PRELS", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "uralte Sagen r\u00fchmen ihre Zahl.\u00ab", "tokens": ["ur\u00b7al\u00b7te", "Sa\u00b7gen", "r\u00fch\u00b7men", "ih\u00b7re", "Zahl", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "\u00bbdoch Vater, sprich, wie alt ist solch ein Stern;", "tokens": ["\u00bb", "doch", "Va\u00b7ter", ",", "sprich", ",", "wie", "alt", "ist", "solch", "ein", "Stern", ";"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NN", "$,", "ADJD", "$,", "PWAV", "ADJD", "VAFIN", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "denn gleiches Alter gabst den Menschen du?\u00ab", "tokens": ["denn", "glei\u00b7ches", "Al\u00b7ter", "gabst", "den", "Men\u00b7schen", "du", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ART", "NN", "PPER", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbdas, kleiner Frager, w\u00fc\u00dft ich selber gern!", "tokens": ["\u00bb", "das", ",", "klei\u00b7ner", "Fra\u00b7ger", ",", "w\u00fc\u00dft", "ich", "sel\u00b7ber", "gern", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Sieh, Kind, z\u00e4hl' tausend Jahren tausend zu", "tokens": ["Sieh", ",", "Kind", ",", "z\u00e4hl'", "tau\u00b7send", "Jah\u00b7ren", "tau\u00b7send", "zu"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "CARD", "NN", "VVPP", "PTKZU"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und abertausend, z\u00e4hl' solang du magst, \u2013", "tokens": ["und", "a\u00b7bert\u00b7au\u00b7send", ",", "z\u00e4hl'", "so\u00b7lang", "du", "magst", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "$,", "VVFIN", "VMFIN", "PPER", "VMFIN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "dein Hirnchen k\u00e4me nimmermehr zur Ruh!", "tokens": ["dein", "Hirn\u00b7chen", "k\u00e4\u00b7me", "nim\u00b7mer\u00b7mehr", "zur", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Kein Mund wei\u00df Antwort dem, wonach du fragst:", "tokens": ["Kein", "Mund", "wei\u00df", "Ant\u00b7wort", "dem", ",", "wo\u00b7nach", "du", "fragst", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "NN", "ART", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denn keine Rechnung f\u00fchrt dahin zur\u00fcck,", "tokens": ["Denn", "kei\u00b7ne", "Rech\u00b7nung", "f\u00fchrt", "da\u00b7hin", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PAV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "daran neugierig du zu r\u00fchren wagst ...", "tokens": ["da\u00b7ran", "neu\u00b7gie\u00b7rig", "du", "zu", "r\u00fch\u00b7ren", "wagst", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "PPER", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Doch alter M\u00e4rchen wei\u00df ich manches St\u00fcck \u2013", "tokens": ["Doch", "al\u00b7ter", "M\u00e4r\u00b7chen", "wei\u00df", "ich", "man\u00b7ches", "St\u00fcck", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "noch mehr die Mutter! Willst du? geh hinein!", "tokens": ["noch", "mehr", "die", "Mut\u00b7ter", "!", "Willst", "du", "?", "geh", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$.", "VMFIN", "PPER", "$.", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "(oh Kinderherz mit deinem kurzen Gl\u00fcck!)\u00ab", "tokens": ["(", "oh", "Kin\u00b7der\u00b7herz", "mit", "dei\u00b7nem", "kur\u00b7zen", "Gl\u00fcck", "!", ")", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "FM", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Kaum ward es Tag, schon bricht die Nacht herein ...", "tokens": ["Kaum", "ward", "es", "Tag", ",", "schon", "bricht", "die", "Nacht", "her\u00b7ein", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Knabe l\u00e4uft nach einem plumpen Bau ...", "tokens": ["Der", "Kna\u00b7be", "l\u00e4uft", "nach", "ei\u00b7nem", "plum\u00b7pen", "Bau", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Aug' des Mannes glimmt ein stierer Schein ...", "tokens": ["Im", "Aug'", "des", "Man\u00b7nes", "glimmt", "ein", "stie\u00b7rer", "Schein", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Ein tiefes Graun verwehrt mir weitre Schau.", "tokens": ["Ein", "tie\u00b7fes", "Graun", "ver\u00b7wehrt", "mir", "weit\u00b7re", "Schau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}