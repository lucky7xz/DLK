{"textgrid.poem.66872": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hochragende Gladiolus,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hochragende Gladiolus,", "tokens": ["Hoch\u00b7ra\u00b7gen\u00b7de", "Gla\u00b7di\u00b7o\u00b7lus", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Rot wie ein r\u00f6mischer Kardinal,", "tokens": ["Rot", "wie", "ein", "r\u00f6\u00b7mi\u00b7scher", "Kar\u00b7di\u00b7nal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Wie steigt dein Farben\u00fcberflu\u00df", "tokens": ["Wie", "steigt", "dein", "Far\u00b7ben\u00b7\u00fc\u00b7berf\u00b7lu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Empor \u2013 ein Bl\u00fctenfeuerstrahl!", "tokens": ["Em\u00b7por", "\u2013", "ein", "Bl\u00fc\u00b7ten\u00b7feu\u00b7er\u00b7strahl", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$(", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So stolz, so gl\u00fchend und so k\u00fchn", "tokens": ["So", "stolz", ",", "so", "gl\u00fc\u00b7hend", "und", "so", "k\u00fchn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "M\u00f6cht' ich auch bl\u00fchn", "tokens": ["M\u00f6cht'", "ich", "auch", "bl\u00fchn"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "VVINF"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Nach so viel Qual", "tokens": ["Nach", "so", "viel", "Qual"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Nun noch einmal ...", "tokens": ["Nun", "noch", "ein\u00b7mal", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ich sp\u00fcr's, mich f\u00e4rbt der Sonne Ku\u00df.", "tokens": ["Ich", "sp\u00fcr's", ",", "mich", "f\u00e4rbt", "der", "Son\u00b7ne", "Ku\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Rasch rollt mein Blut,", "tokens": ["Rasch", "rollt", "mein", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Nun sch\u00fctzt mich gut,", "tokens": ["Nun", "sch\u00fctzt", "mich", "gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ihr Bl\u00e4tter, gegen Feindeswut,", "tokens": ["Ihr", "Bl\u00e4t\u00b7ter", ",", "ge\u00b7gen", "Fein\u00b7des\u00b7wut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Schwerterstahl", "tokens": ["Wie", "Schwer\u00b7ter\u00b7stahl"], "token_info": ["word", "word"], "pos": ["PWAV", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Scharf zugespitzt!", "tokens": ["Scharf", "zu\u00b7ge\u00b7spitzt", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "---+", "measure": "unknown.measure.single"}, "line.6": {"text": "Wer wei\u00df, wie's wettert noch und blitzt! ...", "tokens": ["Wer", "wei\u00df", ",", "wie's", "wet\u00b7tert", "noch", "und", "blitzt", "!", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "VVFIN", "ADV", "KON", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie ich dich liebend anschaun mu\u00df,", "tokens": ["Wie", "ich", "dich", "lie\u00b7bend", "an\u00b7schaun", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hochragende Gladiolus!", "tokens": ["Hoch\u00b7ra\u00b7gen\u00b7de", "Gla\u00b7di\u00b7o\u00b7lus", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}}}}}