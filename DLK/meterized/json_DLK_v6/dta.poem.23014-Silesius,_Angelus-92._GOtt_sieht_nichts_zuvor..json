{"dta.poem.23014": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "92. GOtt sieht nichts zuvor.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "* Gott siehet nichts zuvor: Drumb leugstu wen\u0303 du jhn", "tokens": ["*", "Gott", "sie\u00b7het", "nichts", "zu\u00b7vor", ":", "Drumb", "leugs\u00b7tu", "we\u00f1", "du", "jhn"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["XY", "NN", "VVFIN", "PIS", "ADV", "$.", "PAV", "VVFIN", "KOUS", "PPER", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit der Vorsehung mi\u00dft nach deinem bl\u00f6den Sinn.", "tokens": ["Mit", "der", "Vor\u00b7se\u00b7hung", "mi\u00dft", "nach", "dei\u00b7nem", "bl\u00f6\u00b7den", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "* In Gott ist kein vor oder darnach sehen? sondern", "tokens": ["*", "In", "Gott", "ist", "kein", "vor", "o\u00b7der", "dar\u00b7nach", "se\u00b7hen", "?", "son\u00b7dern"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["XY", "APPR", "NN", "VAFIN", "PIAT", "APPR", "KON", "PAV", "VVINF", "$.", "KON"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er siehet von Ewigkeit alles gegenwertig f\u00fcr jhm/", "tokens": ["Er", "sie\u00b7het", "von", "E\u00b7wig\u00b7keit", "al\u00b7les", "ge\u00b7gen\u00b7wer\u00b7tig", "f\u00fcr", "jhm", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PIS", "ADJD", "APPR", "PPER", "$("], "meter": "-+--+--+-+-+--+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "wie es geschiehet/ nicht wie es geschehen wirdt oder", "tokens": ["wie", "es", "ge\u00b7schie\u00b7het", "/", "nicht", "wie", "es", "ge\u00b7sche\u00b7hen", "wirdt", "o\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVPP", "$(", "PTKNEG", "PWAV", "PPER", "VVPP", "VAFIN", "KON"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.6": {"text": "geschehen ist.", "tokens": ["ge\u00b7sche\u00b7hen", "ist", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}