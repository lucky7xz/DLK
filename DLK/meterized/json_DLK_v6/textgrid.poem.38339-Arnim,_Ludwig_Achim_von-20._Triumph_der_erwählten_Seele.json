{"textgrid.poem.38339": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "20. Triumph der erw\u00e4hlten Seele", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Triumph, Triumph! Es kommt mit Pracht", "tokens": ["Tri\u00b7umph", ",", "Tri\u00b7umph", "!", "Es", "kommt", "mit", "Pracht"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$.", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Siegesf\u00fcrst heut aus der Schlacht;", "tokens": ["Der", "Sie\u00b7ges\u00b7f\u00fcrst", "heut", "aus", "der", "Schlacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer seines Reiches Unterthan,", "tokens": ["Wer", "sei\u00b7nes", "Rei\u00b7ches", "Un\u00b7ter\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schau heute sein Triumphfest an!", "tokens": ["Schau", "heu\u00b7te", "sein", "Tri\u00b7umph\u00b7fest", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Triumph! Triumph! Victoria!", "tokens": ["Tri\u00b7umph", "!", "Tri\u00b7umph", "!", "Vic\u00b7to\u00b7ria", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ewiges Hallelujah.", "tokens": ["Und", "e\u00b7wi\u00b7ges", "Hal\u00b7le\u00b7lu\u00b7jah", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Vor Freuden Thal, Berg, Wald erklingt,", "tokens": ["Vor", "Freu\u00b7den", "Thal", ",", "Berg", ",", "Wald", "er\u00b7klingt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "NN", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Erde sch\u00f6nes Blumwerk bringt,", "tokens": ["Die", "Er\u00b7de", "sch\u00f6\u00b7nes", "Blum\u00b7werk", "bringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Zierath, die Tapezerey", "tokens": ["Der", "Zie\u00b7rath", ",", "die", "Ta\u00b7pe\u00b7ze\u00b7rey"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Zeigt da\u00df ihr Sch\u00f6pfer Sieger sey.", "tokens": ["Zeigt", "da\u00df", "ihr", "Sch\u00f6p\u00b7fer", "Sie\u00b7ger", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "PPOSAT", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Triumph u.s.w.", "tokens": ["Tri\u00b7umph", "u.", "s.", "w."], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NN", "APPR", "VVIMP", "NN"], "meter": "--", "measure": "unknown.measure.zero"}}, "stanza.3": {"line.1": {"text": "Die Sonne sich aufs Sch\u00f6nste schm\u00fcckt,", "tokens": ["Die", "Son\u00b7ne", "sich", "aufs", "Sch\u00f6ns\u00b7te", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wieder durch das Blaue blickt;", "tokens": ["Und", "wie\u00b7der", "durch", "das", "Blau\u00b7e", "blickt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die vor pechschwarz im Trauerkleid", "tokens": ["Die", "vor", "pech\u00b7schwarz", "im", "Trau\u00b7er\u00b7kleid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NE", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beschaut den blutgen Todesstreit,", "tokens": ["Be\u00b7schaut", "den", "blut\u00b7gen", "To\u00b7des\u00b7streit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Triumph u.s.w.", "tokens": ["Tri\u00b7umph", "u.", "s.", "w."], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NN", "APPR", "VVIMP", "NN"], "meter": "--", "measure": "unknown.measure.zero"}}, "stanza.4": {"line.1": {"text": "Das stille Lamm jezt nicht mehr schweigt,", "tokens": ["Das", "stil\u00b7le", "Lamm", "jezt", "nicht", "mehr", "schweigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich muthig als ein L\u00f6w erzeigt;", "tokens": ["Sich", "mut\u00b7hig", "als", "ein", "L\u00f6w", "er\u00b7zeigt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein harter Fels ihn h\u00e4lt und zwingt,", "tokens": ["Kein", "har\u00b7ter", "Fels", "ihn", "h\u00e4lt", "und", "zwingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Grab, Siegel, Riegel vor ihm springt.", "tokens": ["Grab", ",", "Sie\u00b7gel", ",", "Rie\u00b7gel", "vor", "ihm", "springt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Triumph u.s.w.", "tokens": ["Tri\u00b7umph", "u.", "s.", "w."], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NN", "APPR", "VVIMP", "NN"], "meter": "--", "measure": "unknown.measure.zero"}}, "stanza.5": {"line.1": {"text": "Der andre Adam heut erwacht,", "tokens": ["Der", "and\u00b7re", "A\u00b7dam", "heut", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach seiner harten Todesnacht;", "tokens": ["Nach", "sei\u00b7ner", "har\u00b7ten", "To\u00b7des\u00b7nacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus seiner Seite er erbaut,", "tokens": ["Aus", "sei\u00b7ner", "Sei\u00b7te", "er", "er\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns seine theur erl\u00f6ste Braut.", "tokens": ["Uns", "sei\u00b7ne", "theur", "er\u00b7l\u00f6s\u00b7te", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Triumph u.s.w.", "tokens": ["Tri\u00b7umph", "u.", "s.", "w."], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NN", "APPR", "VVIMP", "NN"], "meter": "--", "measure": "unknown.measure.zero"}}, "stanza.6": {"line.1": {"text": "Wie Aarons Ruthe sch\u00f6n ausschlug,", "tokens": ["Wie", "Aa\u00b7rons", "Ru\u00b7the", "sch\u00f6n", "aus\u00b7schlug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NE", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Morgen bl\u00fcht und Mandeln trug;", "tokens": ["Am", "Mor\u00b7gen", "bl\u00fcht", "und", "Man\u00b7deln", "trug", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So tr\u00e4get Frucht der Seligkeit", "tokens": ["So", "tr\u00e4\u00b7get", "Frucht", "der", "Se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des hohen Priesters Leichnam heut.", "tokens": ["Des", "ho\u00b7hen", "Pries\u00b7ters", "Leich\u00b7nam", "heut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Triumph u.s.w.", "tokens": ["Tri\u00b7umph", "u.", "s.", "w."], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NN", "APPR", "VVIMP", "NN"], "meter": "--", "measure": "unknown.measure.zero"}}, "stanza.7": {"line.1": {"text": "Nun ist die Herrlichkeit erk\u00e4mpft,", "tokens": ["Nun", "ist", "die", "Herr\u00b7lich\u00b7keit", "er\u00b7k\u00e4mpft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der S\u00fcnden Pest und Gift ged\u00e4mpft;", "tokens": ["Der", "S\u00fcn\u00b7den", "Pest", "und", "Gift", "ge\u00b7d\u00e4mpft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der schweren Handschrift Fluch und Bann", "tokens": ["Der", "schwe\u00b7ren", "Hand\u00b7schrift", "Fluch", "und", "Bann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vertrit hier mein Erl\u00f6sersmann.", "tokens": ["Ver\u00b7trit", "hier", "mein", "Er\u00b7l\u00f6\u00b7sers\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Triumph u.s.w.", "tokens": ["Tri\u00b7umph", "u.", "s.", "w."], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NN", "APPR", "VVIMP", "NN"], "meter": "--", "measure": "unknown.measure.zero"}}, "stanza.8": {"line.1": {"text": "Du theure Seel bist ausgeb\u00fcrgt,", "tokens": ["Du", "theu\u00b7re", "Seel", "bist", "aus\u00b7ge\u00b7b\u00fcrgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der h\u00f6llische Tyrann erw\u00fcrgt,", "tokens": ["Der", "h\u00f6l\u00b7li\u00b7sche", "Ty\u00b7rann", "er\u00b7w\u00fcrgt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Raubschlo\u00df und geschworne Rott", "tokens": ["Sein", "Raub\u00b7schlo\u00df", "und", "ge\u00b7schwor\u00b7ne", "Rott"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist ganz zerst\u00f6rt, der Tod ein Spott.", "tokens": ["Ist", "ganz", "zer\u00b7st\u00f6rt", ",", "der", "Tod", "ein", "Spott", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Triumph u.s.w.", "tokens": ["Tri\u00b7umph", "u.", "s.", "w."], "token_info": ["word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NN", "APPR", "VVIMP", "NN"], "meter": "--", "measure": "unknown.measure.zero"}}, "stanza.9": {"line.1": {"text": "Herr Jesu, wahrer Siegesf\u00fcrst,", "tokens": ["Herr", "Je\u00b7su", ",", "wah\u00b7rer", "Sie\u00b7ges\u00b7f\u00fcrst", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir glauben, da\u00df du schenken wirst", "tokens": ["Wir", "glau\u00b7ben", ",", "da\u00df", "du", "schen\u00b7ken", "wirst"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVINF", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Uns deinen Frieden, den du bracht", "tokens": ["Uns", "dei\u00b7nen", "Frie\u00b7den", ",", "den", "du", "bracht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit aus dem Grab und aus der Schlacht.", "tokens": ["Mit", "aus", "dem", "Grab", "und", "aus", "der", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Triumph! Triumph! Victoria!", "tokens": ["Tri\u00b7umph", "!", "Tri\u00b7umph", "!", "Vic\u00b7to\u00b7ria", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ewiges Hallelujah.", "tokens": ["Und", "e\u00b7wi\u00b7ges", "Hal\u00b7le\u00b7lu\u00b7jah", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}