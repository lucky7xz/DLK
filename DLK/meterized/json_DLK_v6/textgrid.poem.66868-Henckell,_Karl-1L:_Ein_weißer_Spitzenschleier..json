{"textgrid.poem.66868": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein wei\u00dfer Spitzenschleier.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein wei\u00dfer Spitzenschleier.", "tokens": ["Ein", "wei\u00b7\u00dfer", "Spit\u00b7zen\u00b7schlei\u00b7er", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein schwarzer Schwalbenfrack.", "tokens": ["Ein", "schwar\u00b7zer", "Schwal\u00b7ben\u00b7frack", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Fabrikbesitzer Meyer", "tokens": ["Fab\u00b7rik\u00b7be\u00b7sit\u00b7zer", "Me\u00b7yer"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Und \u00bbJungfrau\u00ab Siegellack.", "tokens": ["Und", "\u00bb", "Jung\u00b7frau", "\u00ab", "Sie\u00b7gel\u00b7lack", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "$(", "NN", "$(", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gesang und Orgelt\u00f6ne,", "tokens": ["Ge\u00b7sang", "und", "Or\u00b7gel\u00b7t\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Goldprunkender Altar.", "tokens": ["Gold\u00b7prun\u00b7ken\u00b7der", "Al\u00b7tar", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mehr oder weniger sch\u00f6ne", "tokens": ["Mehr", "o\u00b7der", "we\u00b7ni\u00b7ger", "sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "KON", "PIAT", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Brautjungfern hinterm Paar.", "tokens": ["Braut\u00b7jung\u00b7fern", "hin\u00b7term", "Paar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Der w\u00fcrdige Schwarzrock predigt,", "tokens": ["Der", "w\u00fcr\u00b7di\u00b7ge", "Schwarz\u00b7rock", "pre\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Er predigt lind und lau,", "tokens": ["Er", "pre\u00b7digt", "lind", "und", "lau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Heilsakt wird erledigt,", "tokens": ["Der", "Heil\u00b7sakt", "wird", "er\u00b7le\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Herr Meyer hat 'ne Frau.", "tokens": ["Herr", "Me\u00b7yer", "hat", "'ne", "Frau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Von Siegell\u00e4ckchens Lippen", "tokens": ["Von", "Sie\u00b7gel\u00b7l\u00e4ck\u00b7chens", "Lip\u00b7pen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00c4therisch zittert's: \u00bbJa!\u00ab", "tokens": ["\u00c4\u00b7the\u00b7risch", "zit\u00b7tert's", ":", "\u00bb", "Ja", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["ADJD", "VVFIN", "$.", "$(", "PTKANT", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Dann mu\u00df sie Medoc nippen,", "tokens": ["Dann", "mu\u00df", "sie", "Me\u00b7doc", "nip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NE", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie ist der Ohnmacht nah.", "tokens": ["Sie", "ist", "der", "Ohn\u00b7macht", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie stehen auf vom Kissen", "tokens": ["Sie", "ste\u00b7hen", "auf", "vom", "Kis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und reichen sich den Arm,", "tokens": ["Und", "rei\u00b7chen", "sich", "den", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kircht\u00fcren aufgerissen,", "tokens": ["Kirch\u00b7t\u00fc\u00b7ren", "auf\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Formiert der Hochzeitsschwarm.", "tokens": ["For\u00b7miert", "der", "Hoch\u00b7zeits\u00b7schwarm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Die nassen Taschent\u00fccher", "tokens": ["Die", "nas\u00b7sen", "Ta\u00b7schen\u00b7t\u00fc\u00b7cher"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sind wieder beigesteckt;", "tokens": ["Sind", "wie\u00b7der", "bei\u00b7ge\u00b7steckt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Mann der Gottesb\u00fccher", "tokens": ["Der", "Mann", "der", "Got\u00b7tes\u00b7b\u00fc\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Spitzt sich den Mund auf Sekt.", "tokens": ["Spitzt", "sich", "den", "Mund", "auf", "Sekt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "Brautmarsch von Wagner. Paarig", "tokens": ["Braut\u00b7marsch", "von", "Wag\u00b7ner", ".", "Paa\u00b7rig"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "NE", "$.", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Hinaus und ins Kupee.", "tokens": ["Hin\u00b7aus", "und", "ins", "Ku\u00b7pee", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ein Dichter lockenhaarig", "tokens": ["Ein", "Dich\u00b7ter", "lo\u00b7cken\u00b7haa\u00b7rig"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schleicht seitw\u00e4rts seelenweh.", "tokens": ["Schleicht", "seit\u00b7w\u00e4rts", "see\u00b7len\u00b7weh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Er hat \u00bbSie\u00ab angedichtet,", "tokens": ["Er", "hat", "\u00bb", "Sie", "\u00ab", "an\u00b7ge\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "PPER", "$(", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er hat \u00bbSie\u00ab anget\u00f6nt,", "tokens": ["Er", "hat", "\u00bb", "Sie", "\u00ab", "an\u00b7ge\u00b7t\u00f6nt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "PPER", "$(", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Glauben ist vernichtet,", "tokens": ["Sein", "Glau\u00b7ben", "ist", "ver\u00b7nich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein Ideal verh\u00f6hnt.", "tokens": ["Sein", "I\u00b7deal", "ver\u00b7h\u00f6hnt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "\u00bbwir wollen Freunde bleiben\u00ab \u2013", "tokens": ["\u00bb", "wir", "wol\u00b7len", "Freun\u00b7de", "blei\u00b7ben", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VMFIN", "NN", "VVINF", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da steht's mit Veilchenduft \u2013", "tokens": ["Da", "steht's", "mit", "Veil\u00b7chen\u00b7duft", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbjedoch uns nicht mehr schreiben,", "tokens": ["\u00bb", "je\u00b7doch", "uns", "nicht", "mehr", "schrei\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Man lebt nicht von der Luft.\u00ab", "tokens": ["Man", "lebt", "nicht", "von", "der", "Luft", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "O wei\u00dfer Spitzenschleier!", "tokens": ["O", "wei\u00b7\u00dfer", "Spit\u00b7zen\u00b7schlei\u00b7er", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O schwarzer Schwalbenfrack!", "tokens": ["O", "schwar\u00b7zer", "Schwal\u00b7ben\u00b7frack", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Frau Fabrikantin Meyer,", "tokens": ["Frau", "Fab\u00b7ri\u00b7kan\u00b7tin", "Me\u00b7yer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Geborne Siegellack.", "tokens": ["Ge\u00b7bor\u00b7ne", "Sie\u00b7gel\u00b7lack", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}