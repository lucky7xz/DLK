{"textgrid.poem.50626": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Almosen", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es reicht der Armuth Hand das beste Bettelbrot;", "tokens": ["Es", "reicht", "der", "Ar\u00b7muth", "Hand", "das", "bes\u00b7te", "Bet\u00b7tel\u00b7brot", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer selbst in Drangsal, hilft am r\u00fchmlichsten aus Noth,", "tokens": ["Wer", "selbst", "in", "Dran\u00b7gsal", ",", "hilft", "am", "r\u00fchm\u00b7lichs\u00b7ten", "aus", "Noth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NN", "$,", "VVFIN", "APPRART", "ADJA", "APPR", "NN", "$,"], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Und der wird meist von Gott geliebt,", "tokens": ["Und", "der", "wird", "meist", "von", "Gott", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VAFIN", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der, weil er Andern giebt, die H\u00e4nde selbst l\u00e4\u00dft leer;", "tokens": ["Der", ",", "weil", "er", "An\u00b7dern", "giebt", ",", "die", "H\u00e4n\u00b7de", "selbst", "l\u00e4\u00dft", "leer", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "ADJA", "VVFIN", "$,", "ART", "NN", "ADV", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn Der, der wenig giebt von Wenigem, giebt mehr,", "tokens": ["Denn", "Der", ",", "der", "we\u00b7nig", "giebt", "von", "We\u00b7ni\u00b7gem", ",", "giebt", "mehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "PIS", "VVFIN", "APPR", "NE", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Als Der, der viel von Vielem giebt.", "tokens": ["Als", "Der", ",", "der", "viel", "von", "Vie\u00b7lem", "giebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "PRELS", "ADV", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}