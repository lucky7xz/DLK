{"dta.poem.11079": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Callisten, als sie ihm ein sauer  \n gesichte machte.  \n * *  B.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin als wie ein schiffer-nachen,", "tokens": ["Ich", "bin", "als", "wie", "ein", "schif\u00b7fer\u00b7nach\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOUS", "KOKOM", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An den der sturm die wilden fluthen schmei\u00dft.", "tokens": ["An", "den", "der", "sturm", "die", "wil\u00b7den", "flut\u00b7hen", "schmei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der liebe macht bemeiftert meinen geist,", "tokens": ["Der", "lie\u00b7be", "macht", "be\u00b7meif\u00b7tert", "mei\u00b7nen", "geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und will mich ihr zum sclaven machen.", "tokens": ["Und", "will", "mich", "ihr", "zum", "scla\u00b7ven", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich schiff\u2019 in einer see,", "tokens": ["Ich", "schiff'", "in", "ei\u00b7ner", "see", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die wollust und begierd aufschwellen;", "tokens": ["Die", "wol\u00b7lust", "und", "be\u00b7gierd", "auf\u00b7schwel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Was wunder, wenn ich in den wellen,", "tokens": ["Was", "wun\u00b7der", ",", "wenn", "ich", "in", "den", "wel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "KOUS", "PPER", "APPR", "ART", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Eh ichs vermein\u2019, in tausend dr\u00fcmmer geh.", "tokens": ["Eh", "ichs", "ver\u00b7mein'", ",", "in", "tau\u00b7send", "dr\u00fcm\u00b7mer", "geh."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "APPR", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die klugheit l\u00e4\u00dft das ruder fallen,", "tokens": ["Die", "klug\u00b7heit", "l\u00e4\u00dft", "das", "ru\u00b7der", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein ancker sinckt, mein mast zersplittert sich,", "tokens": ["Mein", "an\u00b7cker", "sinckt", ",", "mein", "mast", "zer\u00b7split\u00b7tert", "sich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des himmels grimm geht eintzig \u00fcber mich,", "tokens": ["Des", "him\u00b7mels", "grimm", "geht", "eint\u00b7zig", "\u00fc\u00b7ber", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich h\u00f6re seine donner knallen.", "tokens": ["Ich", "h\u00f6\u00b7re", "sei\u00b7ne", "don\u00b7ner", "knal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Jm hertzen trag ich tag;", "tokens": ["Jm", "hert\u00b7zen", "trag", "ich", "tag", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Von aussen seh ich schwartze n\u00e4chte.", "tokens": ["Von", "aus\u00b7sen", "seh", "ich", "schwart\u00b7ze", "n\u00e4ch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ach! da\u00df Calliste doch ged\u00e4chte,", "tokens": ["Ach", "!", "da\u00df", "Cal\u00b7lis\u00b7te", "doch", "ge\u00b7d\u00e4ch\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "NE", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df tag und nacht sich nicht verschwistern mag.", "tokens": ["Da\u00df", "tag", "und", "nacht", "sich", "nicht", "ver\u00b7schwis\u00b7tern", "mag."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "NN", "KON", "NN", "PRF", "PTKNEG", "VVINF", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Verzeih, wo ich zu unbescheiden!", "tokens": ["Ver\u00b7zeih", ",", "wo", "ich", "zu", "un\u00b7be\u00b7schei\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein antlitz ists, so diese nacht gebiehrt.", "tokens": ["Dein", "ant\u00b7litz", "ists", ",", "so", "die\u00b7se", "nacht", "ge\u00b7biehrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "ADV", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer sonnen in den augen f\u00fchrt,", "tokens": ["Wer", "son\u00b7nen", "in", "den", "au\u00b7gen", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df keinen nebel um sich leiden.", "tokens": ["Mu\u00df", "kei\u00b7nen", "ne\u00b7bel", "um", "sich", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Verschleu\u00df dich l\u00e4nger nicht!", "tokens": ["Ver\u00b7schleu\u00df", "dich", "l\u00e4n\u00b7ger", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Gesetzt, da\u00df ich vor dich nicht tauge,", "tokens": ["Ge\u00b7setzt", ",", "da\u00df", "ich", "vor", "dich", "nicht", "tau\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPER", "APPR", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Lehnt doch des adlers stumpffes auge", "tokens": ["Lehnt", "doch", "des", "ad\u00b7lers", "stumpf\u00b7fes", "au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Selbst seine krafft vom grossen sonnen-licht.", "tokens": ["Selbst", "sei\u00b7ne", "krafft", "vom", "gros\u00b7sen", "son\u00b7nen\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wiewohl, ich w\u00fcnsche keine sonne.", "tokens": ["Wie\u00b7wohl", ",", "ich", "w\u00fcn\u00b7sche", "kei\u00b7ne", "son\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Soll meine fahrt bey tr\u00fcb- und tunckler nacht", "tokens": ["Soll", "mei\u00b7ne", "fahrt", "bey", "tr\u00fcb", "und", "tunck\u00b7ler", "nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "VVFIN", "APPR", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durch deinen schlu\u00df, Calliste! seyn vollbracht,", "tokens": ["Durch", "dei\u00b7nen", "schlu\u00df", ",", "Cal\u00b7lis\u00b7te", "!", "seyn", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NE", "$.", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So g\u00f6nne mir zum minsten diese wonne:", "tokens": ["So", "g\u00f6n\u00b7ne", "mir", "zum", "mins\u00b7ten", "die\u00b7se", "won\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df beiner br\u00fcste schein,", "tokens": ["Da\u00df", "bei\u00b7ner", "br\u00fcs\u00b7te", "schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die wie zwey stern\u2019 auf deinen anmuths-h\u00f6hen", "tokens": ["Die", "wie", "zwey", "stern'", "auf", "dei\u00b7nen", "an\u00b7muths\u00b7h\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "KOKOM", "CARD", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Bald in die h\u00f6h, bald wieder abwarts gehen,", "tokens": ["Bald", "in", "die", "h\u00f6h", ",", "bald", "wie\u00b7der", "ab\u00b7warts", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJD", "$,", "ADV", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mir mein compa\u00df, mein leitstern m\u00f6ge seyn!", "tokens": ["Mir", "mein", "com\u00b7pa\u00df", ",", "mein", "leits\u00b7tern", "m\u00f6\u00b7ge", "seyn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "PPOSAT", "VVINF", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}