{"textgrid.poem.59318": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Trauriger Winter", "genre": "verse", "period": "N.A.", "pub_year": 1808, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ziehen Nebel, falbe Bl\u00e4tter fallen,", "tokens": ["Nun", "zie\u00b7hen", "Ne\u00b7bel", ",", "fal\u00b7be", "Bl\u00e4t\u00b7ter", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00d6d alle Stellen, die uns oft entz\u00fccket!", "tokens": ["\u00d6d", "al\u00b7le", "Stel\u00b7len", ",", "die", "uns", "oft", "ent\u00b7z\u00fc\u00b7cket", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und noch einmal tief' R\u00fchrung uns begl\u00fccket,", "tokens": ["Und", "noch", "ein\u00b7mal", "tie\u00b7f'", "R\u00fch\u00b7rung", "uns", "be\u00b7gl\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Wie aus der Flucht die Abschiedslieder schallen.", "tokens": ["Wie", "aus", "der", "Flucht", "die", "Ab\u00b7schieds\u00b7lie\u00b7der", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wohl manchem bl\u00fcht aus solchem Tod Gefallen:", "tokens": ["Wohl", "man\u00b7chem", "bl\u00fcht", "aus", "sol\u00b7chem", "Tod", "Ge\u00b7fal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df er nun eng ans bl\u00fchnde Herz gedr\u00fccket,", "tokens": ["Da\u00df", "er", "nun", "eng", "ans", "bl\u00fchn\u00b7de", "Herz", "ge\u00b7dr\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von roten Lippen holdre Str\u00e4u\u00dfe pfl\u00fccket", "tokens": ["Von", "ro\u00b7ten", "Lip\u00b7pen", "hold\u00b7re", "Str\u00e4u\u00b7\u00dfe", "pfl\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als Lenz je beut mit W\u00e4ldern, Wiesen allen.", "tokens": ["Als", "Lenz", "je", "beut", "mit", "W\u00e4l\u00b7dern", ",", "Wie\u00b7sen", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "VVFIN", "APPR", "NN", "$,", "PWAV", "PIS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mir sagte niemals ihrer Augen Bl\u00e4ue:", "tokens": ["Mir", "sag\u00b7te", "nie\u00b7mals", "ih\u00b7rer", "Au\u00b7gen", "Bl\u00e4u\u00b7e", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbruh auch aus! Willst du ewig sinnen?\u00ab", "tokens": ["\u00bb", "ruh", "auch", "aus", "!", "Willst", "du", "e\u00b7wig", "sin\u00b7nen", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "ADV", "PTKVZ", "$.", "VMFIN", "PPER", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und einsam sah ich so den Sommer fahren.", "tokens": ["Und", "ein\u00b7sam", "sah", "ich", "so", "den", "Som\u00b7mer", "fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So will ich tief des Lenzes Bl\u00fcte wahren,", "tokens": ["So", "will", "ich", "tief", "des", "Len\u00b7zes", "Bl\u00fc\u00b7te", "wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und mit Erinnern zaubrisch mich umspinnen,", "tokens": ["Und", "mit", "E\u00b7rin\u00b7nern", "zaub\u00b7risch", "mich", "um\u00b7spin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis ich nach langem Traum erwach im Maie.", "tokens": ["Bis", "ich", "nach", "lan\u00b7gem", "Traum", "er\u00b7wach", "im", "Mai\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}