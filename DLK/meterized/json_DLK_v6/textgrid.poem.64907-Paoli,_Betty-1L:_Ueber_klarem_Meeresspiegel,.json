{"textgrid.poem.64907": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ueber klarem Meeresspiegel,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ueber klarem Meeresspiegel,", "tokens": ["Ue\u00b7ber", "kla\u00b7rem", "Mee\u00b7res\u00b7spie\u00b7gel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch der L\u00fcfte freies Reich,", "tokens": ["Durch", "der", "L\u00fcf\u00b7te", "frei\u00b7es", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zieh'n mit kr\u00e4ftig k\u00fchnem Fl\u00fcgel", "tokens": ["Zieh'n", "mit", "kr\u00e4f\u00b7tig", "k\u00fch\u00b7nem", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wanderv\u00f6gel liederreich.", "tokens": ["Wan\u00b7der\u00b7v\u00f6\u00b7gel", "lie\u00b7der\u00b7reich", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fern weg lenket ihr Gel\u00fcste", "tokens": ["Fern", "weg", "len\u00b7ket", "ihr", "Ge\u00b7l\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ihren Flug von Baum und Flur", "tokens": ["Ih\u00b7ren", "Flug", "von", "Baum", "und", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "NE", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die Menschen an der K\u00fcste", "tokens": ["Und", "die", "Men\u00b7schen", "an", "der", "K\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kennen ihre Stimme nur.", "tokens": ["Ken\u00b7nen", "ih\u00b7re", "Stim\u00b7me", "nur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo Lavinen niedergleiten,", "tokens": ["Wo", "La\u00b7vi\u00b7nen", "nie\u00b7derg\u00b7lei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wurden von gewalt'ger Hand", "tokens": ["Wur\u00b7den", "von", "ge\u00b7walt'\u00b7ger", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einer Aeolsharfe Saiten", "tokens": ["Ei\u00b7ner", "A\u00b7e\u00b7ols\u00b7har\u00b7fe", "Sai\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "K\u00fchn von Berg zu Berg gespannt.", "tokens": ["K\u00fchn", "von", "Berg", "zu", "Berg", "ge\u00b7spannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Klingen nur beim Mondenstrahle", "tokens": ["Klin\u00b7gen", "nur", "beim", "Mon\u00b7dens\u00b7trah\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mystisch wie ein Geistesschwur,", "tokens": ["Mys\u00b7tisch", "wie", "ein", "Geis\u00b7tes\u00b7schwur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch die Menschen tief im Thale", "tokens": ["Doch", "die", "Men\u00b7schen", "tief", "im", "Tha\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kennen ihre T\u00f6ne nur.", "tokens": ["Ken\u00b7nen", "ih\u00b7re", "T\u00f6\u00b7ne", "nur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und so dringet meine Stimme", "tokens": ["Und", "so", "drin\u00b7get", "mei\u00b7ne", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu dem trostlos \u00f6den Strand,", "tokens": ["Zu", "dem", "trost\u00b7los", "\u00f6\u00b7den", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "D'ran ich von des Schicksals Grimme", "tokens": ["D'\u00b7ran", "ich", "von", "des", "Schick\u00b7sals", "Grim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Andromeda gebannt!", "tokens": ["Wie", "And\u00b7ro\u00b7me\u00b7da", "ge\u00b7bannt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch die N\u00e4chte meiner Schmerzen", "tokens": ["Durch", "die", "N\u00e4ch\u00b7te", "mei\u00b7ner", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hallt vermittelnd dein Gesang;", "tokens": ["Hallt", "ver\u00b7mit\u00b7telnd", "dein", "Ge\u00b7sang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch du selbst bist meinem Herzen", "tokens": ["Doch", "du", "selbst", "bist", "mei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nur ein Lied, ein Ton, ein Klang!", "tokens": ["Nur", "ein", "Lied", ",", "ein", "Ton", ",", "ein", "Klang", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}