{"textgrid.poem.52923": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Umsonst! Es nimmt das reine Element", "tokens": ["Um\u00b7sonst", "!", "Es", "nimmt", "das", "rei\u00b7ne", "E\u00b7le\u00b7ment"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Leib nicht auf, der sich mit Schuld beladen,", "tokens": ["Den", "Leib", "nicht", "auf", ",", "der", "sich", "mit", "Schuld", "be\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "PTKVZ", "$,", "PRELS", "PRF", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Mal, das mir auf Stirn und Achseln brennt,", "tokens": ["Das", "Mal", ",", "das", "mir", "auf", "Stirn", "und", "Ach\u00b7seln", "brennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "W\u00e4scht keine ab der kosenden Najaden.", "tokens": ["W\u00e4scht", "kei\u00b7ne", "ab", "der", "ko\u00b7sen\u00b7den", "Na\u00b7ja\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zu ihrem Sklaven pr\u00e4gte mich die Welt,", "tokens": ["Zu", "ih\u00b7rem", "Skla\u00b7ven", "pr\u00e4g\u00b7te", "mich", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich naschte von der Frucht der Hesperiden,", "tokens": ["Ich", "naschte", "von", "der", "Frucht", "der", "Hes\u00b7pe\u00b7ri\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nun scheucht mich's fort, wo's eben noch mich h\u00e4lt,", "tokens": ["Nun", "scheucht", "mich's", "fort", ",", "wo's", "e\u00b7ben", "noch", "mich", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKVZ", "$,", "PWAV", "ADV", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Selbst Meer und Eiland geben keinen Frieden.", "tokens": ["Selbst", "Meer", "und", "Ei\u00b7land", "ge\u00b7ben", "kei\u00b7nen", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gern h\u00e4tt' ich meinen Stab hier eingepflanzt,", "tokens": ["Gern", "h\u00e4tt'", "ich", "mei\u00b7nen", "Stab", "hier", "ein\u00b7ge\u00b7pflanzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu sehen, ob der d\u00fcrre gr\u00fcnt' und tr\u00fcge,", "tokens": ["Zu", "se\u00b7hen", ",", "ob", "der", "d\u00fcr\u00b7re", "gr\u00fcnt'", "und", "tr\u00fc\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im roten Wasser lustig mitgetanzt", "tokens": ["Im", "ro\u00b7ten", "Was\u00b7ser", "lus\u00b7tig", "mit\u00b7ge\u00b7tanzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und mich zur Ruh' gesetzt und zur Gen\u00fcge.", "tokens": ["Und", "mich", "zur", "Ruh'", "ge\u00b7setzt", "und", "zur", "Ge\u00b7n\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VVPP", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es soll nicht sein, die Welle st\u00f6\u00dft mich aus,", "tokens": ["Es", "soll", "nicht", "sein", ",", "die", "Wel\u00b7le", "st\u00f6\u00dft", "mich", "aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VAINF", "$,", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Felsen will den Gleitenden nicht tragen, \u2013", "tokens": ["Der", "Fel\u00b7sen", "will", "den", "Glei\u00b7ten\u00b7den", "nicht", "tra\u00b7gen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So leb denn wohl, du r\u00e4uchrig Fischerhaus,", "tokens": ["So", "leb", "denn", "wohl", ",", "du", "r\u00e4uch\u00b7rig", "Fi\u00b7scher\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "$,", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das mich geborgen hat in stillen Tagen!", "tokens": ["Das", "mich", "ge\u00b7bor\u00b7gen", "hat", "in", "stil\u00b7len", "Ta\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVPP", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Leb wohl, der Helga gr\u00fcn-rot-wei\u00dfes Land,", "tokens": ["Leb", "wohl", ",", "der", "Hel\u00b7ga", "gr\u00fcn\u00b7rot\u00b7wei\u00b7\u00dfes", "Land", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "ART", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gott sch\u00fctze dich, und englische Gesetze!", "tokens": ["Gott", "sch\u00fct\u00b7ze", "dich", ",", "und", "eng\u00b7li\u00b7sche", "Ge\u00b7set\u00b7ze", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df nie der Seehund mangle deinem Strand,", "tokens": ["Da\u00df", "nie", "der", "See\u00b7hund", "mang\u00b7le", "dei\u00b7nem", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nie Schell- und Stockfisch deiner S\u00f6hne Netze!", "tokens": ["Nie", "Schell", "und", "Stock\u00b7fisch", "dei\u00b7ner", "S\u00f6h\u00b7ne", "Net\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "TRUNC", "KON", "NN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Reich mir noch eins den Mund zum Kusse her,", "tokens": ["Reich", "mir", "noch", "eins", "den", "Mund", "zum", "Kus\u00b7se", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PIS", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Sch\u00f6n-\u00c4nnchen, morgen k\u00fc\u00dft er andre Jungen;", "tokens": ["Sch\u00f6n\u00b7\u00c4nn\u00b7chen", ",", "mor\u00b7gen", "k\u00fc\u00dft", "er", "and\u00b7re", "Jun\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann denk an mich, wenn nicht das weite Meer,", "tokens": ["Dann", "denk", "an", "mich", ",", "wenn", "nicht", "das", "wei\u00b7te", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "$,", "KOUS", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das r\u00e4chende, zur Heimkehr mich verschlungen!", "tokens": ["Das", "r\u00e4\u00b7chen\u00b7de", ",", "zur", "Heim\u00b7kehr", "mich", "ver\u00b7schlun\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}