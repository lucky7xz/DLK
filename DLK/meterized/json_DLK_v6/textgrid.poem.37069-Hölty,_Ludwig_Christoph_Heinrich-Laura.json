{"textgrid.poem.37069": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Laura", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bald wird des Grabes Ruhe mich decken, bald", "tokens": ["Bald", "wird", "des", "Gra\u00b7bes", "Ru\u00b7he", "mich", "de\u00b7cken", ",", "bald"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "PPER", "VVINF", "$,", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Umschweb ich Lauren, Ahnungen sagens mir,", "tokens": ["Um\u00b7schweb", "ich", "Lau\u00b7ren", ",", "Ah\u00b7nun\u00b7gen", "sa\u00b7gens", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "NN", "$,", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Sterbeglocke schalt mirs, n\u00e4chtlich", "tokens": ["Die", "Ster\u00b7be\u00b7glo\u00b7cke", "schalt", "mirs", ",", "n\u00e4cht\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "NE", "$,", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6r ich ihr Schallen, und Engel rufen:", "tokens": ["H\u00f6r", "ich", "ihr", "Schal\u00b7len", ",", "und", "En\u00b7gel", "ru\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPOSAT", "NN", "$,", "KON", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Du sollst getr\u00f6stet werden, du Weinender,", "tokens": ["Du", "sollst", "ge\u00b7tr\u00f6s\u00b7tet", "wer\u00b7den", ",", "du", "Wei\u00b7nen\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Um Lauren schweben, bis sie das Paradies", "tokens": ["Um", "Lau\u00b7ren", "schwe\u00b7ben", ",", "bis", "sie", "das", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "NN", "VVINF", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit dir bewohnet. \u2013 Todesstunde,", "tokens": ["Mit", "dir", "be\u00b7woh\u00b7net", ".", "\u2013", "To\u00b7dess\u00b7tun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$.", "$(", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fl\u00fcgle die Schritte, du Menschenfreundin,", "tokens": ["Fl\u00fcg\u00b7le", "die", "Schrit\u00b7te", ",", "du", "Men\u00b7schen\u00b7freun\u00b7din", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PPER", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Du Bothin Gottes! Wonne mir, Wonne mir,", "tokens": ["Du", "Bo\u00b7thin", "Got\u00b7tes", "!", "Won\u00b7ne", "mir", ",", "Won\u00b7ne", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "NN", "$.", "NN", "PPER", "$,", "PWAV", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich str\u00f6me, kommst du, kniend, wo Laura kniet,", "tokens": ["Ich", "str\u00f6\u00b7me", ",", "kommst", "du", ",", "kni\u00b7end", ",", "wo", "Lau\u00b7ra", "kniet", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,", "VVPP", "$,", "PWAV", "NE", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Anbetung \u00fcber sie, und Andacht,", "tokens": ["An\u00b7be\u00b7tung", "\u00fc\u00b7ber", "sie", ",", "und", "An\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "KON", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wann sie vom Kelche des Bundes trinket.", "tokens": ["Wann", "sie", "vom", "Kel\u00b7che", "des", "Bun\u00b7des", "trin\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Und s\u00fc\u00dfre Schauer, Schauer der Seraphim", "tokens": ["Und", "s\u00fc\u00df\u00b7re", "Schau\u00b7er", ",", "Schau\u00b7er", "der", "Se\u00b7ra\u00b7phim"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "NE", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Am Throne Gottes, t\u00f6net sie Prei\u00dfgesang,", "tokens": ["Am", "Thro\u00b7ne", "Got\u00b7tes", ",", "t\u00f6\u00b7net", "sie", "Prei\u00df\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$,", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vom Mayenfr\u00fchroth angel\u00e4chelt,", "tokens": ["Vom", "Ma\u00b7yen\u00b7fr\u00fch\u00b7roth", "an\u00b7ge\u00b7l\u00e4\u00b7chelt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus dem begeisterten vollen Herzen.", "tokens": ["Aus", "dem", "be\u00b7geis\u00b7ter\u00b7ten", "vol\u00b7len", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Ich folg', im Mondenschimmer, der Denkerin", "tokens": ["Ich", "fol\u00b7g'", ",", "im", "Mon\u00b7den\u00b7schim\u00b7mer", ",", "der", "Den\u00b7ke\u00b7rin"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPRART", "NN", "$,", "ART", "NN"], "meter": "-+--+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Durch deine K\u00fchlung, duftende Fr\u00fchlingsnacht,", "tokens": ["Durch", "dei\u00b7ne", "K\u00fch\u00b7lung", ",", "duf\u00b7ten\u00b7de", "Fr\u00fch\u00b7lings\u00b7nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und decke, wann ihr Auge sinket,", "tokens": ["Und", "de\u00b7cke", ",", "wann", "ihr", "Au\u00b7ge", "sin\u00b7ket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie mit verbreitetem Fl\u00fcgel; wehe", "tokens": ["Sie", "mit", "ver\u00b7brei\u00b7te\u00b7tem", "Fl\u00fc\u00b7gel", ";", "we\u00b7he"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "APPR", "ADJA", "NN", "$.", "ADJD"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Den Morgenschlummer, wehe den frommen Traum", "tokens": ["Den", "Mor\u00b7gen\u00b7schlum\u00b7mer", ",", "we\u00b7he", "den", "from\u00b7men", "Traum"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von ihrer Stirn, und f\u00fchre die Wachende", "tokens": ["Von", "ih\u00b7rer", "Stirn", ",", "und", "f\u00fch\u00b7re", "die", "Wa\u00b7chen\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Zum Garten, sich der Mayenbl\u00fcthen,", "tokens": ["Zum", "Gar\u00b7ten", ",", "sich", "der", "Ma\u00b7yen\u00b7bl\u00fc\u00b7then", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich des Gezwitschers umher, zu freuen.", "tokens": ["Sich", "des", "Ge\u00b7zwit\u00b7schers", "um\u00b7her", ",", "zu", "freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "PTKVZ", "$,", "PTKZU", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Sie dankt mir, o Gedanke voll Seeligkeit!", "tokens": ["Sie", "dankt", "mir", ",", "o", "Ge\u00b7dan\u00b7ke", "voll", "See\u00b7lig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "FM", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dereinst die hohen Christengef\u00fchle, dankt", "tokens": ["De\u00b7reinst", "die", "ho\u00b7hen", "Chris\u00b7ten\u00b7ge\u00b7f\u00fch\u00b7le", ",", "dankt"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mir einst am Throne des Erl\u00f6sers", "tokens": ["Mir", "einst", "am", "Thro\u00b7ne", "des", "Er\u00b7l\u00f6\u00b7sers"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jede vergo\u00dfene Christenthr\u00e4ne.", "tokens": ["Je\u00b7de", "ver\u00b7go\u00b7\u00dfe\u00b7ne", "Chris\u00b7ten\u00b7thr\u00e4\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}