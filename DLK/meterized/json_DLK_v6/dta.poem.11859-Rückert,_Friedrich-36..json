{"dta.poem.11859": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein jeder ist sich selbst der n\u00e4chste. Zeugen sind", "tokens": ["Ein", "je\u00b7der", "ist", "sich", "selbst", "der", "n\u00e4chs\u00b7te", ".", "Zeu\u00b7gen", "sind"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PIS", "VAFIN", "PRF", "ADV", "ART", "ADJA", "$.", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von dieser Lehr' am Baum die Bl\u00e4tter, liebes Kind.", "tokens": ["Von", "die\u00b7ser", "Lehr'", "am", "Baum", "die", "Bl\u00e4t\u00b7ter", ",", "lie\u00b7bes", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPRART", "NN", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die saugen oben ein begierig allen Regen,", "tokens": ["Die", "sau\u00b7gen", "o\u00b7ben", "ein", "be\u00b7gie\u00b7rig", "al\u00b7len", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "ADJD", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df nichts zum Boden kommt, der trocken bleibt deswegen.", "tokens": ["Da\u00df", "nichts", "zum", "Bo\u00b7den", "kommt", ",", "der", "tro\u00b7cken", "bleibt", "des\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "NN", "VVFIN", "$,", "PRELS", "ADJD", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wann aber sie sich satt getrunken, sch\u00fctteln sie", "tokens": ["Wann", "a\u00b7ber", "sie", "sich", "satt", "ge\u00b7trun\u00b7ken", ",", "sch\u00fct\u00b7teln", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADV", "PPER", "PRF", "ADJD", "VVPP", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Ueberflu\u00df zur Erd', und nun kommts auch an die.", "tokens": ["Den", "Ue\u00b7berf\u00b7lu\u00df", "zur", "Erd'", ",", "und", "nun", "kommts", "auch", "an", "die", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "KON", "ADV", "VVFIN", "ADV", "APPR", "ART", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Was oft sie vom Palast hinaus zum Fenster sch\u00fctten,", "tokens": ["Was", "oft", "sie", "vom", "Pa\u00b7last", "hin\u00b7aus", "zum", "Fens\u00b7ter", "sch\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "APPRART", "NN", "APZR", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O k\u00e4m' es ebenso auch an die armen H\u00fctten!", "tokens": ["O", "k\u00e4m'", "es", "e\u00b7ben\u00b7so", "auch", "an", "die", "ar\u00b7men", "H\u00fct\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}