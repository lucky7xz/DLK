{"textgrid.poem.34110": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf Manchen", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freilich thut es dir noth, zu schaffen, ich glaub' es, doch, leider!", "tokens": ["Frei\u00b7lich", "thut", "es", "dir", "noth", ",", "zu", "schaf\u00b7fen", ",", "ich", "glaub'", "es", ",", "doch", ",", "lei\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "NN", "$,", "PTKZU", "VVINF", "$,", "PPER", "VVFIN", "PPER", "$,", "ADV", "$,", "ADV", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Thut es der Welt nicht noth, da\u00df sie besizt, was du schaffst.", "tokens": ["Thut", "es", "der", "Welt", "nicht", "noth", ",", "da\u00df", "sie", "be\u00b7sizt", ",", "was", "du", "schaffst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}}}}