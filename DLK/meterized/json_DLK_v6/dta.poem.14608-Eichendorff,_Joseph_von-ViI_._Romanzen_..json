{"dta.poem.14608": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "ViI .   Romanzen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Aus schweren Tr\u00e4umen", "tokens": ["Aus", "schwe\u00b7ren", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Fuhr ich oft auf und sah durch Tannenwipfel", "tokens": ["Fuhr", "ich", "oft", "auf", "und", "sah", "durch", "Tan\u00b7nen\u00b7wip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Mond zieh'n \u00fcber'n stillen Grund und sang", "tokens": ["Den", "Mond", "zieh'n", "\u00fc\u00b7ber'n", "stil\u00b7len", "Grund", "und", "sang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vor Bangigkeit und schlummert' wieder ein. \u2014", "tokens": ["Vor", "Ban\u00b7gig\u00b7keit", "und", "schlum\u00b7mert'", "wie\u00b7der", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "VVFIN", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ja, Menschenstimme, hell aus frommer Brust!", "tokens": ["Ja", ",", "Men\u00b7schen\u00b7stim\u00b7me", ",", "hell", "aus", "from\u00b7mer", "Brust", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du bist doch die gewaltigste, und triffst", "tokens": ["Du", "bist", "doch", "die", "ge\u00b7wal\u00b7tigs\u00b7te", ",", "und", "triffst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den rechten Grundton, der verworren anklingt", "tokens": ["Den", "rech\u00b7ten", "Grund\u00b7ton", ",", "der", "ver\u00b7wor\u00b7ren", "an\u00b7klingt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "In all' den tausend Stimmen der Natur! \u2014", "tokens": ["In", "all'", "den", "tau\u00b7send", "Stim\u00b7men", "der", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIS", "ART", "CARD", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}