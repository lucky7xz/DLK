{"textgrid.poem.41952": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nahe dich mir, Elegie, leichth\u00fcpfende Grazientochter,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nahe dich mir, Elegie, leichth\u00fcpfende Grazientochter,", "tokens": ["Na\u00b7he", "dich", "mir", ",", "E\u00b7le\u00b7gie", ",", "leicht\u00b7h\u00fcp\u00b7fen\u00b7de", "Gra\u00b7zi\u00b7en\u00b7toch\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Freundin zarten Gef\u00fchls, nahe dich mir, Elegie!", "tokens": ["Freun\u00b7din", "zar\u00b7ten", "Ge\u00b7f\u00fchls", ",", "na\u00b7he", "dich", "mir", ",", "E\u00b7le\u00b7gie", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "VVFIN", "PPER", "PPER", "$,", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Nicht vom d\u00fcsteren Schleier verh\u00fcllt, den herrisch der Volkswahn", "tokens": ["Nicht", "vom", "d\u00fcs\u00b7te\u00b7ren", "Schlei\u00b7er", "ver\u00b7h\u00fcllt", ",", "den", "her\u00b7risch", "der", "Volks\u00b7wahn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPRART", "ADJA", "NN", "VVPP", "$,", "ART", "NN", "ART", "NN"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Einst um die Stirn dir wob, nicht mit ger\u00f6thetem Blick.", "tokens": ["Einst", "um", "die", "Stirn", "dir", "wob", ",", "nicht", "mit", "ge\u00b7r\u00f6\u00b7the\u00b7tem", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPER", "VVFIN", "$,", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.5": {"text": "Kummer entnervt das Gef\u00fchl, und feind den verzagenden Thr\u00e4nen", "tokens": ["Kum\u00b7mer", "ent\u00b7nervt", "das", "Ge\u00b7f\u00fchl", ",", "und", "feind", "den", "ver\u00b7za\u00b7gen\u00b7den", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "KON", "NN", "ART", "ADJA", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Schwinden die Musen; umsonst weinet im Pontus Ovid.", "tokens": ["Schwin\u00b7den", "die", "Mu\u00b7sen", ";", "um\u00b7sonst", "wei\u00b7net", "im", "Pon\u00b7tus", "O\u00b7vid", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "ADV", "VVFIN", "APPRART", "NE", "NE", "$."], "meter": "+--+--++--+-+-", "measure": "dactylic.di.plus"}, "line.7": {"text": "Nein, ein fl\u00fcchtiges Kind erscheine mir, schlank und gelenksam;", "tokens": ["Nein", ",", "ein", "fl\u00fcch\u00b7ti\u00b7ges", "Kind", "er\u00b7schei\u00b7ne", "mir", ",", "schlank", "und", "ge\u00b7lenk\u00b7sam", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "ADJA", "NN", "VVFIN", "PPER", "$,", "VVFIN", "KON", "ADJD", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Tanzend schwebe dein Fu\u00df \u00fcber die Blumen der Flur;", "tokens": ["Tan\u00b7zend", "schwe\u00b7be", "dein", "Fu\u00df", "\u00fc\u00b7ber", "die", "Blu\u00b7men", "der", "Flur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Scherzend necke die Hand mit duftendem Bl\u00fcthengest\u00f6ber", "tokens": ["Scher\u00b7zend", "ne\u00b7cke", "die", "Hand", "mit", "duf\u00b7ten\u00b7dem", "Bl\u00fc\u00b7then\u00b7ge\u00b7st\u00f6\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+----+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Amorn, welcher erz\u00fcrnt mit dem Geschosse dir droht.", "tokens": ["A\u00b7morn", ",", "wel\u00b7cher", "er\u00b7z\u00fcrnt", "mit", "dem", "Ge\u00b7schos\u00b7se", "dir", "droht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADJD", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "L\u00e4chelnd folg' Idalia dir, doch wind' um der G\u00f6ttin", "tokens": ["L\u00e4\u00b7chelnd", "fol\u00b7g'", "I\u00b7da\u00b7lia", "dir", ",", "doch", "wind'", "um", "der", "G\u00f6t\u00b7tin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "NE", "PPER", "$,", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Ueppigen Reiz das Gewand z\u00fcchtiger Grazie sich.", "tokens": ["Uep\u00b7pi\u00b7gen", "Reiz", "das", "Ge\u00b7wand", "z\u00fcch\u00b7ti\u00b7ger", "Gra\u00b7zie", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "ADJA", "NN", "PRF", "$."], "meter": "+--+--++--+-+", "measure": "dactylic.di.plus"}, "line.13": {"text": "Singe von Lust und Liebe mir vor, doch nimmer verfolge", "tokens": ["Sin\u00b7ge", "von", "Lust", "und", "Lie\u00b7be", "mir", "vor", ",", "doch", "nim\u00b7mer", "ver\u00b7fol\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN", "PPER", "PTKVZ", "$,", "ADV", "ADV", "VVFIN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Reue die Lust, nie sey Kummer der Liebe Geno\u00df.", "tokens": ["Reu\u00b7e", "die", "Lust", ",", "nie", "sey", "Kum\u00b7mer", "der", "Lie\u00b7be", "Ge\u00b7no\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADV", "VAFIN", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.15": {"text": "Spiele geziemen dir nur, nicht Leidenschaften; behutsam", "tokens": ["Spie\u00b7le", "ge\u00b7zie\u00b7men", "dir", "nur", ",", "nicht", "Lei\u00b7den\u00b7schaf\u00b7ten", ";", "be\u00b7hut\u00b7sam"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "VVPP", "PPER", "ADV", "$,", "PTKNEG", "NN", "$.", "ADJD"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Nahe dem Kelch, sonst sinkt welkend die Blume dahin.", "tokens": ["Na\u00b7he", "dem", "Kelch", ",", "sonst", "sinkt", "wel\u00b7kend", "die", "Blu\u00b7me", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADV", "VVFIN", "ADJD", "ART", "NN", "PAV", "$."], "meter": "+--+-++--+--+", "measure": "iambic.hexa.invert"}, "line.17": {"text": "Weinst du, so weine nur Thr\u00e4nen der List, wie die schlaue Kokette,", "tokens": ["Weinst", "du", ",", "so", "wei\u00b7ne", "nur", "Thr\u00e4\u00b7nen", "der", "List", ",", "wie", "die", "schlau\u00b7e", "Ko\u00b7ket\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VVFIN", "ADV", "NN", "ART", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Und die Verzweiflung selbst schm\u00fccke der Hoffnung Gewand.", "tokens": ["Und", "die", "Ver\u00b7zwei\u00b7flung", "selbst", "schm\u00fc\u00b7cke", "der", "Hoff\u00b7nung", "Ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.19": {"text": "Thr\u00e4nen erweichen den menschlichen Sinn und Thr\u00e4nen den Orkus,", "tokens": ["Thr\u00e4\u00b7nen", "er\u00b7wei\u00b7chen", "den", "menschli\u00b7chen", "Sinn", "und", "Thr\u00e4\u00b7nen", "den", "Or\u00b7kus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.20": {"text": "Waffne mit Thr\u00e4nen dich nur, magisch besiegst du die Welt;", "tokens": ["Waff\u00b7ne", "mit", "Thr\u00e4\u00b7nen", "dich", "nur", ",", "ma\u00b7gisch", "be\u00b7siegst", "du", "die", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPER", "ADV", "$,", "ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-+--+-+--+", "measure": "iambic.hexa.invert"}, "line.21": {"text": "Lust ist der Sterblichen Wunsch und Lust das Leben der G\u00f6tter,", "tokens": ["Lust", "ist", "der", "Sterb\u00b7li\u00b7chen", "Wunsch", "und", "Lust", "das", "Le\u00b7ben", "der", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "KON", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Hauche nur Lust, und schnell \u00f6ffnet sich jegliches Herz.", "tokens": ["Hau\u00b7che", "nur", "Lust", ",", "und", "schnell", "\u00f6ff\u00b7net", "sich", "jeg\u00b7li\u00b7ches", "Herz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "$,", "KON", "ADJD", "VVFIN", "PRF", "ADJA", "NN", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}, "line.23": {"text": "Jeglichen locke mit schmeichelnder Kunst und Jedem gefalle.", "tokens": ["Jeg\u00b7li\u00b7chen", "lo\u00b7cke", "mit", "schmei\u00b7cheln\u00b7der", "Kunst", "und", "Je\u00b7dem", "ge\u00b7fal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN", "KON", "PIAT", "NN", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.24": {"text": "Schilt die Menge dich auch buhlerisch, gilt ihr Gesetz?", "tokens": ["Schilt", "die", "Men\u00b7ge", "dich", "auch", "buh\u00b7le\u00b7risch", ",", "gilt", "ihr", "Ge\u00b7setz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADV", "ADJD", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.25": {"text": "Richten mag sie, was nach dem Gebrauch, was n\u00fctzlich und recht sey,", "tokens": ["Rich\u00b7ten", "mag", "sie", ",", "was", "nach", "dem", "Ge\u00b7brauch", ",", "was", "n\u00fctz\u00b7lich", "und", "recht", "sey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "PRELS", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.26": {"text": "Aber das Sch\u00f6ne begreift einzig das sch\u00f6ne Gem\u00fcth.", "tokens": ["A\u00b7ber", "das", "Sch\u00f6\u00b7ne", "be\u00b7greift", "ein\u00b7zig", "das", "sch\u00f6\u00b7ne", "Ge\u00b7m\u00fcth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.27": {"text": "Ha, du nahst! Ich f\u00fchle das Wehn ambrosischer D\u00fcfte", "tokens": ["Ha", ",", "du", "nahst", "!", "Ich", "f\u00fch\u00b7le", "das", "Wehn", "am\u00b7bro\u00b7si\u00b7scher", "D\u00fcf\u00b7te"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.28": {"text": "Amor, rufe den Lenz, baue mir Lauben im Hain!", "tokens": ["A\u00b7mor", ",", "ru\u00b7fe", "den", "Lenz", ",", "bau\u00b7e", "mir", "Lau\u00b7ben", "im", "Hain", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "NN", "APPRART", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.29": {"text": "Folge mir nach und wohne bei mir, du Holde; melodisch", "tokens": ["Fol\u00b7ge", "mir", "nach", "und", "woh\u00b7ne", "bei", "mir", ",", "du", "Hol\u00b7de", ";", "me\u00b7lo\u00b7disch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "PPER", "APPR", "KON", "VVFIN", "APPR", "PPER", "$,", "PPER", "NN", "$.", "ADJD"], "meter": "+--+-+--+-+-+-+", "measure": "iambic.septa.invert"}, "line.30": {"text": "T\u00f6nt mir die Laute; wohlan, hauche mir Lieder in's Herz!", "tokens": ["T\u00f6nt", "mir", "die", "Lau\u00b7te", ";", "wo\u00b7hlan", ",", "hau\u00b7che", "mir", "Lie\u00b7der", "in's", "Herz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "ADV", "$,", "VVFIN", "PPER", "NN", "APPRART", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}}}}}