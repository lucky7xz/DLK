{"textgrid.poem.59762": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer sich des Muts erk\u00fchnt zu singen und zu klagen", "tokens": ["Wer", "sich", "des", "Muts", "er\u00b7k\u00fchnt", "zu", "sin\u00b7gen", "und", "zu", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PRF", "ART", "NN", "VVPP", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Weh, o Vaterland, dein Weh, o Menschenherz,", "tokens": ["Dein", "Weh", ",", "o", "Va\u00b7ter\u00b7land", ",", "dein", "Weh", ",", "o", "Men\u00b7schen\u00b7herz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer die Lawine w\u00e4lzt der Schicksalsr\u00e4tselfragen,", "tokens": ["Wer", "die", "La\u00b7wi\u00b7ne", "w\u00e4lzt", "der", "Schick\u00b7sals\u00b7r\u00e4t\u00b7sel\u00b7fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bald fliegend himmelauf, bald st\u00fcrzend h\u00f6llenw\u00e4rts,", "tokens": ["Bald", "flie\u00b7gend", "him\u00b7me\u00b7lauf", ",", "bald", "st\u00fcr\u00b7zend", "h\u00f6l\u00b7len\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$,", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der horche nimmer auf, wo Sp\u00e4ne von Philistern,", "tokens": ["Der", "hor\u00b7che", "nim\u00b7mer", "auf", ",", "wo", "Sp\u00e4\u00b7ne", "von", "Phi\u00b7lis\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "PTKVZ", "$,", "PWAV", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit schalem Spott bespritzt, durch Himmelsflammen knistern.", "tokens": ["Mit", "scha\u00b7lem", "Spott", "be\u00b7spritzt", ",", "durch", "Him\u00b7mels\u00b7flam\u00b7men", "knis\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}