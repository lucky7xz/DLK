{"textgrid.poem.34438": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: O jene Tage sind so fern,", "genre": "verse", "period": "N.A.", "pub_year": 1886, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O jene Tage sind so fern,", "tokens": ["O", "je\u00b7ne", "Ta\u00b7ge", "sind", "so", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "da einst Genuss des Menschen Loos.", "tokens": ["da", "einst", "Ge\u00b7nuss", "des", "Men\u00b7schen", "Loos", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Zeit ist hell vom Morgenstern,", "tokens": ["Die", "Zeit", "ist", "hell", "vom", "Mor\u00b7gens\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "doch sonnenlos.", "tokens": ["doch", "son\u00b7nen\u00b7los", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "In dieser Nacht, im Sternenschein", "tokens": ["In", "die\u00b7ser", "Nacht", ",", "im", "Ster\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "schl\u00e4ft rings das Volk auf feuchter Streu,", "tokens": ["schl\u00e4ft", "rings", "das", "Volk", "auf", "feuch\u00b7ter", "Streu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "stets von der Tr\u00e4ume bunten Reihn", "tokens": ["stets", "von", "der", "Tr\u00e4u\u00b7me", "bun\u00b7ten", "Reihn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "genarrt aufs neu.", "tokens": ["ge\u00b7narrt", "aufs", "neu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Kein Zagen tritt an sie heran,", "tokens": ["Kein", "Za\u00b7gen", "tritt", "an", "sie", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "was ihnen tr\u00e4umt, scheint ihnen wahr:", "tokens": ["was", "ih\u00b7nen", "tr\u00e4umt", ",", "scheint", "ih\u00b7nen", "wahr", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "vergessen dieser Schatten Bann,", "tokens": ["ver\u00b7ges\u00b7sen", "die\u00b7ser", "Schat\u00b7ten", "Bann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PDAT", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "fern die Gefahr.", "tokens": ["fern", "die", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Doch die vom Fels im Sternenstrahl", "tokens": ["Doch", "die", "vom", "Fels", "im", "Ster\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "gen Osten wenden ihr Gesicht,", "tokens": ["gen", "Os\u00b7ten", "wen\u00b7den", "ihr", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "sie f\u00fchlen dieses Dunkels Qual,", "tokens": ["sie", "f\u00fch\u00b7len", "die\u00b7ses", "Dun\u00b7kels", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "sie tr\u00e4umen nicht.", "tokens": ["sie", "tr\u00e4u\u00b7men", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Die grossen Augen, hoffnungsk\u00fchn,", "tokens": ["Die", "gros\u00b7sen", "Au\u00b7gen", ",", "hoff\u00b7nungs\u00b7k\u00fchn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "erflehn die Stunde, da es tagt \u2013", "tokens": ["er\u00b7flehn", "die", "Stun\u00b7de", ",", "da", "es", "tagt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die grossen Augen bangend gl\u00fchn", "tokens": ["die", "gros\u00b7sen", "Au\u00b7gen", "ban\u00b7gend", "gl\u00fchn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "durch tiefe Nacht.", "tokens": ["durch", "tie\u00b7fe", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Und ihre Schwerter, blank und klar,", "tokens": ["Und", "ih\u00b7re", "Schwer\u00b7ter", ",", "blank", "und", "klar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PTKVZ", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "funkeln im Sternenlicht \u2013", "tokens": ["fun\u00b7keln", "im", "Ster\u00b7nen\u00b7licht", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "sie kennen dieser Nacht Gefahr,", "tokens": ["sie", "ken\u00b7nen", "die\u00b7ser", "Nacht", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "sie tr\u00e4umen nicht!", "tokens": ["sie", "tr\u00e4u\u00b7men", "nicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Erwacht vom Traume bin auch ich", "tokens": ["Er\u00b7wacht", "vom", "Trau\u00b7me", "bin", "auch", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "VAFIN", "ADV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und sch\u00e4me mich der langen Ruh:", "tokens": ["und", "sch\u00e4\u00b7me", "mich", "der", "lan\u00b7gen", "Ruh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ob manches sch\u00f6ne Bild entwich \u2013", "tokens": ["ob", "man\u00b7ches", "sch\u00f6\u00b7ne", "Bild", "ent\u00b7wich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "noch leuchtest du,", "tokens": ["noch", "leuch\u00b7test", "du", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "du Hoffnung einer lichtren Welt,", "tokens": ["du", "Hoff\u00b7nung", "ei\u00b7ner", "licht\u00b7ren", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "du Sternbild, das im Osten flammt", "tokens": ["du", "Stern\u00b7bild", ",", "das", "im", "Os\u00b7ten", "flammt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und jedes Menschen Herz erhellt,", "tokens": ["und", "je\u00b7des", "Men\u00b7schen", "Herz", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "das gottentstammt!", "tokens": ["das", "got\u00b7tent\u00b7stammt", "!"], "token_info": ["word", "word", "punct"], "pos": ["PDS", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}