{"dta.poem.14459": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Lieder .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wolken, Str\u00f6me, Schiffe, alle", "tokens": ["Wol\u00b7ken", ",", "Str\u00f6\u00b7me", ",", "Schif\u00b7fe", ",", "al\u00b7le"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Segeln in die Pracht hinein \u2014", "tokens": ["Se\u00b7geln", "in", "die", "Pracht", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keines kehrt zur\u00fcck von allen", "tokens": ["Kei\u00b7nes", "kehrt", "zu\u00b7r\u00fcck", "von", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PTKVZ", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich stehe so allein.", "tokens": ["Und", "ich", "ste\u00b7he", "so", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}