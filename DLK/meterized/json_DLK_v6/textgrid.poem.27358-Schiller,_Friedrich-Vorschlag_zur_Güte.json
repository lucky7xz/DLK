{"textgrid.poem.27358": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Vorschlag zur G\u00fcte", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Teilt euch wie Br\u00fcder! Es sind der W\u00fcrste gerade zwei Dutzend,", "tokens": ["Teilt", "euch", "wie", "Br\u00fc\u00b7der", "!", "Es", "sind", "der", "W\u00fcrs\u00b7te", "ge\u00b7ra\u00b7de", "zwei", "Dut\u00b7zend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "ADV", "CARD", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Und wer Astyanax sang, nehme noch diese von mir.", "tokens": ["Und", "wer", "As\u00b7ty\u00b7a\u00b7nax", "sang", ",", "neh\u00b7me", "noch", "die\u00b7se", "von", "mir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "NE", "VVFIN", "$,", "VVFIN", "ADV", "PDAT", "APPR", "PPER", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}}}}