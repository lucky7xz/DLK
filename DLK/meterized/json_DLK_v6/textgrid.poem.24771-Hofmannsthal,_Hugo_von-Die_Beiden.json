{"textgrid.poem.24771": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "Die Beiden", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie trug den Becher in der Hand \u2013", "tokens": ["Sie", "trug", "den", "Be\u00b7cher", "in", "der", "Hand", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Kinn und Mund glich seinem Rand \u2013,", "tokens": ["Ihr", "Kinn", "und", "Mund", "glich", "sei\u00b7nem", "Rand", "\u2013", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So leicht und sicher war ihr Gang,", "tokens": ["So", "leicht", "und", "si\u00b7cher", "war", "ihr", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Tropfen aus dem Becher sprang.", "tokens": ["Kein", "Trop\u00b7fen", "aus", "dem", "Be\u00b7cher", "sprang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So leicht und fest war seine Hand:", "tokens": ["So", "leicht", "und", "fest", "war", "sei\u00b7ne", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er ritt auf einem jungen Pferde,", "tokens": ["Er", "ritt", "auf", "ei\u00b7nem", "jun\u00b7gen", "Pfer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und mit nachl\u00e4ssiger Geb\u00e4rde", "tokens": ["Und", "mit", "nach\u00b7l\u00e4s\u00b7si\u00b7ger", "Ge\u00b7b\u00e4r\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erzwang er, da\u00df es zitternd stand.", "tokens": ["Er\u00b7zwang", "er", ",", "da\u00df", "es", "zit\u00b7ternd", "stand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Jedoch, wenn er aus ihrer Hand", "tokens": ["Je\u00b7doch", ",", "wenn", "er", "aus", "ih\u00b7rer", "Hand"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den leichten Becher nehmen sollte,", "tokens": ["Den", "leich\u00b7ten", "Be\u00b7cher", "neh\u00b7men", "soll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So war es beiden allzu schwer:", "tokens": ["So", "war", "es", "bei\u00b7den", "all\u00b7zu", "schwer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn beide bebten sie so sehr,", "tokens": ["Denn", "bei\u00b7de", "beb\u00b7ten", "sie", "so", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df keine Hand die andre fand", "tokens": ["Da\u00df", "kei\u00b7ne", "Hand", "die", "and\u00b7re", "fand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "ART", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und dunkler Wein am Boden rollte.", "tokens": ["Und", "dunk\u00b7ler", "Wein", "am", "Bo\u00b7den", "roll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}