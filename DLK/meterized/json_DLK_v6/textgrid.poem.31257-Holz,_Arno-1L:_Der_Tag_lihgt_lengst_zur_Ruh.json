trochaic.tetra"}, "line.7": {"text": "Denn man sucht sich niemahls satt", "tokens": ["Denn", "man", "sucht", "sich", "nie\u00b7mahls", "satt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "PRF", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "darnach/ wa\u00df man s\u00e4lbst nicht hat!", "tokens": ["dar\u00b7nach", "/", "wa\u00df", "man", "s\u00e4lbst", "nicht", "hat", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$(", "KOUS", "PIS", "ADV", "PTKNEG", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Gleich so ruffstu und mit Lachen/", "tokens": ["Gleich", "so", "ruffs\u00b7tu", "und", "mit", "La\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "KON", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "m\u00e4nckstu dich in andrer Sachen?", "tokens": ["m\u00e4ncks\u00b7tu", "dich", "in", "an\u00b7drer", "Sa\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Nicht so f\u00e4st und \u00fcmmerzu/", "tokens": ["Nicht", "so", "f\u00e4st", "und", "\u00fcm\u00b7mer\u00b7zu", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "KON", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "du verlihbter Rakker du!", "tokens": ["du", "ver\u00b7lihb\u00b7ter", "Rak\u00b7ker", "du", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "O zokker-s\u00fcsse Noht/", "tokens": ["O", "zok\u00b7ker\u00b7s\u00fcs\u00b7se", "Noht", "/"], 