{"textgrid.poem.31257": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Tag lihgt lengst zur Ruh/", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Tag lihgt lengst zur Ruh/", "tokens": ["Der", "Tag", "lihgt", "lengst", "zur", "Ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "und lescht die letzten Lichtgens au\u00df.", "tokens": ["und", "lescht", "die", "letz\u00b7ten", "Licht\u00b7gens", "au\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Zeisgens/ Amstelgens und Truscheln", "tokens": ["Die", "Zeis\u00b7gens", "/", "A\u00b7mstel\u00b7gens", "und", "Tru\u00b7scheln"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "NE", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "schon still in ihren Nestgens kuscheln.", "tokens": ["schon", "still", "in", "ih\u00b7ren", "Nest\u00b7gens", "ku\u00b7scheln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Dorillgen/ ist es dir genehm/", "tokens": ["Do\u00b7rill\u00b7gen", "/", "ist", "es", "dir", "ge\u00b7nehm", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VAFIN", "PPER", "PPER", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "so mach ich es mir itzt beqwem/", "tokens": ["so", "mach", "ich", "es", "mir", "itzt", "be\u00b7qwem", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PPER", "ADV", "ADJA", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "weil da\u00df, wa\u00df mich dir so verb\u00fcndet/", "tokens": ["weil", "da\u00df", ",", "wa\u00df", "mich", "dir", "so", "ver\u00b7b\u00fcn\u00b7det", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "$,", "KOUS", "PPER", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "dein H\u00e4ndgen auch im F\u00fcnstern f\u00fcndet.", "tokens": ["dein", "H\u00e4nd\u00b7gen", "auch", "im", "F\u00fcns\u00b7tern", "f\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Itzt acht ich nichts for deine K\u00fcsse", "tokens": ["Itzt", "acht", "ich", "nichts", "for", "dei\u00b7ne", "K\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "CARD", "PPER", "PIS", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "gantz Indjens bundte P\u00e4rlen-Fl\u00fcsse/", "tokens": ["gantz", "Ind\u00b7jens", "bund\u00b7te", "P\u00e4r\u00b7len\u00b7F\u00b7l\u00fcs\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "fast b\u00fcn ich mir s\u00e4lbst entrukkt/", "tokens": ["fast", "b\u00fcn", "ich", "mir", "s\u00e4lbst", "ent\u00b7rukkt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "VVFIN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "wenn dein Mund auff meinem zukkt!", "tokens": ["wenn", "dein", "Mund", "auff", "mei\u00b7nem", "zukkt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Blanck besilbert steht die Th\u00fcr/", "tokens": ["Blanck", "be\u00b7sil\u00b7bert", "steht", "die", "Th\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "kukk/ schon bricht der Mond herf\u00fcr!", "tokens": ["kukk", "/", "schon", "bricht", "der", "Mond", "her\u00b7f\u00fcr", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kom/ wir schwimmen/ ich und du/", "tokens": ["Kom", "/", "wir", "schwim\u00b7men", "/", "ich", "und", "du", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "PPER", "VVFIN", "$(", "PPER", "KON", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "auff die Zokker-Insuln zu!", "tokens": ["auff", "die", "Zok\u00b7ker\u00b7In\u00b7suln", "zu", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Im nahen Pusch brohbt Filomele", "tokens": ["Im", "na\u00b7hen", "Pusch", "brohbt", "Fi\u00b7lo\u00b7me\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die P\u00e4rlen- und Korallen-Kehle;", "tokens": ["die", "P\u00e4r\u00b7len", "und", "Ko\u00b7ral\u00b7len\u00b7Keh\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Frau ", "tokens": ["Frau"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "der Sternen-P\u00f6fel dantzt!", "tokens": ["der", "Ster\u00b7nen\u00b7P\u00f6\u00b7fel", "dantzt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Deines Leibs bezihrter Bau", "tokens": ["Dei\u00b7nes", "Leibs", "be\u00b7zihr\u00b7ter", "Bau"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "gleicht ", "tokens": ["gleicht"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Rohsen und Rubinen streiten", "tokens": ["Roh\u00b7sen", "und", "Ru\u00b7bi\u00b7nen", "strei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "sich \u00fcmb ihn von allen Seiten.", "tokens": ["sich", "\u00fcmb", "ihn", "von", "al\u00b7len", "Sei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Seine wohl-geformte L\u00e4nge", "tokens": ["Sei\u00b7ne", "wohl\u00b7ge\u00b7form\u00b7te", "L\u00e4n\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "bringt mich seelig ins Gedr\u00e4nge/", "tokens": ["bringt", "mich", "see\u00b7lig", "ins", "Ge\u00b7dr\u00e4n\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "kaum drukkstu die Augen zu/", "tokens": ["kaum", "drukks\u00b7tu", "die", "Au\u00b7gen", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "ART", "NN", "PTKZU", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "wenn ich wa\u00df Verbohtnes dhu!", "tokens": ["wenn", "ich", "wa\u00df", "Ver\u00b7boht\u00b7nes", "dhu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "FM", "FM", "FM", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "G\u00f6nne/ da\u00df ich noch erwehne", "tokens": ["G\u00f6n\u00b7ne", "/", "da\u00df", "ich", "noch", "er\u00b7weh\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$(", "KOUS", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "jene Alabaster-Schwehne/", "tokens": ["je\u00b7ne", "A\u00b7la\u00b7bas\u00b7ter\u00b7Schweh\u00b7ne", "/"], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "die auff deinem Marmol-Meer", "tokens": ["die", "auff", "dei\u00b7nem", "Mar\u00b7mol\u00b7Meer"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "langsam schauckeln hin und hehr;", "tokens": ["lang\u00b7sam", "schauc\u00b7keln", "hin", "und", "hehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PTKVZ", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "for mich sind dihse zween Narzissen", "tokens": ["for", "mich", "sind", "dih\u00b7se", "zween", "Nar\u00b7zis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VAFIN", "PDS", "VVFIN", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "die aller-sch\u00f6nsten Schl\u00e4kker-Bissen!", "tokens": ["die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Schl\u00e4k\u00b7ker\u00b7Bis\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr Schwestern wohl gepaart", "tokens": ["Ihr", "Schwes\u00b7tern", "wohl", "ge\u00b7paart"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "ohn alle jede Kanten/", "tokens": ["ohn", "al\u00b7le", "je\u00b7de", "Kan\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PIAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "ihr habt so r\u00e4cht die Ahrt", "tokens": ["ihr", "habt", "so", "r\u00e4cht", "die", "Ahrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "der f\u00e4sten Adamanten \u2013", "tokens": ["der", "f\u00e4s\u00b7ten", "A\u00b7da\u00b7man\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "ich mu\u00df euch/ \u00fcmb euch zu geniessen/", "tokens": ["ich", "mu\u00df", "euch", "/", "\u00fcmb", "euch", "zu", "ge\u00b7nies\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$(", "VVIMP", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "in die verlihbten H\u00e4nde schliessen!", "tokens": ["in", "die", "ver\u00b7lihb\u00b7ten", "H\u00e4n\u00b7de", "schlies\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Deine mehr al\u00df g\u00f6ldne Lokken/", "tokens": ["Dei\u00b7ne", "mehr", "al\u00df", "g\u00f6ld\u00b7ne", "Lok\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PIAT", "KOKOM", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "deine zahrt-gekr\u00fcllte Flokken/", "tokens": ["dei\u00b7ne", "zahr\u00b7tge\u00b7kr\u00fcll\u00b7te", "Flok\u00b7ken", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "deine Wollen-weiche Hand/", "tokens": ["dei\u00b7ne", "Wol\u00b7len\u00b7wei\u00b7che", "Hand", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "deine Wollust-runde Augen/", "tokens": ["dei\u00b7ne", "Wol\u00b7lust\u00b7run\u00b7de", "Au\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "die mich gleichsahm in sich saugen/", "tokens": ["die", "mich", "gleich\u00b7sahm", "in", "sich", "sau\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "PRF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "alles k\u00fc\u00df ich dir touchant!", "tokens": ["al\u00b7les", "k\u00fc\u00df", "ich", "dir", "tou\u00b7ch\u00b7ant", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": 