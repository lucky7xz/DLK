{"dta.poem.19483": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Schnitterlied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Wir schnitten die Saaten, wir Buben und Dirnen,", "tokens": ["Wir", "schnit\u00b7ten", "die", "Saa\u00b7ten", ",", "wir", "Bu\u00b7ben", "und", "Dir\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Mit nackenden Armen und triefenden Stirnen,", "tokens": ["Mit", "na\u00b7cken\u00b7den", "Ar\u00b7men", "und", "trie\u00b7fen\u00b7den", "Stir\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Von steigenden dunkeln Gewittern bedroht \u2014", "tokens": ["Von", "stei\u00b7gen\u00b7den", "dun\u00b7keln", "Ge\u00b7wit\u00b7tern", "be\u00b7droht"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "VVPP", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Gerettet das Korn! Und nicht Einer der darbe!", "tokens": ["Ge\u00b7ret\u00b7tet", "das", "Korn", "!", "Und", "nicht", "Ei\u00b7ner", "der", "dar\u00b7be", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "$.", "KON", "PTKNEG", "PIS", "ART", "ADJA", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Von Aehre zu Garbe", "tokens": ["Von", "A\u00b7eh\u00b7re", "zu", "Gar\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Ist Raum f\u00fcr den Tod \u2014", "tokens": ["Ist", "Raum", "f\u00fcr", "den", "Tod"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "ART", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Wie schwellen die Lippen des Lebens so rot!", "tokens": ["Wie", "schwel\u00b7len", "die", "Lip\u00b7pen", "des", "Le\u00b7bens", "so", "rot", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Hoch thronet ihr Sch\u00f6nen auf g\u00fcldenen Sitzen", "tokens": ["Hoch", "thro\u00b7net", "ihr", "Sch\u00f6\u00b7nen", "auf", "g\u00fcl\u00b7de\u00b7nen", "Sit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "In strotzenden Garben umflimmert von Blitzen \u2014", "tokens": ["In", "strot\u00b7zen\u00b7den", "Gar\u00b7ben", "um\u00b7flim\u00b7mert", "von", "Blit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "APPR", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Nicht Eine die darbe! Wir bringen das Brot!", "tokens": ["Nicht", "Ei\u00b7ne", "die", "dar\u00b7be", "!", "Wir", "brin\u00b7gen", "das", "Brot", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ART", "ADJA", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Zum Reigen! Zum Tanze! Zur tosenden Runde!", "tokens": ["Zum", "Rei\u00b7gen", "!", "Zum", "Tan\u00b7ze", "!", "Zur", "to\u00b7sen\u00b7den", "Run\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "APPRART", "NN", "$.", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Von Becher zu Munde", "tokens": ["Von", "Be\u00b7cher", "zu", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ist Raum f\u00fcr den Tod \u2014", "tokens": ["Ist", "Raum", "f\u00fcr", "den", "Tod"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "ART", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Wie schwellen die Lippen des Lebens so rot!", "tokens": ["Wie", "schwel\u00b7len", "die", "Lip\u00b7pen", "des", "Le\u00b7bens", "so", "rot", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}