{"textgrid.poem.67239": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hygea, G\u00f6ttinn, es str\u00f6mt", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hygea, G\u00f6ttinn, es str\u00f6mt", "tokens": ["Hy\u00b7gea", ",", "G\u00f6t\u00b7tinn", ",", "es", "str\u00f6mt"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "PPER", "VVFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Von dir aus Heilung und Kraft,", "tokens": ["Von", "dir", "aus", "Hei\u00b7lung", "und", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und Leben haucht", "tokens": ["Und", "Le\u00b7ben", "haucht"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "In die Adern der Natur,", "tokens": ["In", "die", "A\u00b7dern", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ihre pochende Pulse schlagen,", "tokens": ["Da\u00df", "ih\u00b7re", "po\u00b7chen\u00b7de", "Pul\u00b7se", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Da\u00df schwillt ihr alls\u00e4ugender Busen,", "tokens": ["Da\u00df", "schwillt", "ihr", "all\u00b7s\u00e4u\u00b7gen\u00b7der", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Dein Odem, Beseelende!", "tokens": ["Dein", "O\u00b7dem", ",", "Be\u00b7see\u00b7len\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Schaue, G\u00f6ttinn, herab, es knieen", "tokens": ["Schau\u00b7e", ",", "G\u00f6t\u00b7tinn", ",", "her\u00b7ab", ",", "es", "kni\u00b7e\u00b7en"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "ADV", "$,", "PPER", "VVFIN"], "meter": "+-+--+-+--", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Der Flehenden tausendmahl tausend an deinem Altar,", "tokens": ["Der", "Fle\u00b7hen\u00b7den", "tau\u00b7send\u00b7mahl", "tau\u00b7send", "an", "dei\u00b7nem", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.3": {"text": "Bebende H\u00e4nd' erheben sie dir", "tokens": ["Be\u00b7ben\u00b7de", "H\u00e4nd'", "er\u00b7he\u00b7ben", "sie", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PPER"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und es steigt empor in der Weihrauchwolk' ihr Gebet.", "tokens": ["Und", "es", "steigt", "em\u00b7por", "in", "der", "Weih\u00b7rauch\u00b7wol\u00b7k'", "ihr", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "--+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Aus den Tausenden der Gel\u00fcbde sondre du,", "tokens": ["Aus", "den", "Tau\u00b7sen\u00b7den", "der", "Ge\u00b7l\u00fcb\u00b7de", "sond\u00b7re", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Himmelstochter, mein Flehen, das nicht", "tokens": ["Him\u00b7mel\u00b7stoch\u00b7ter", ",", "mein", "Fle\u00b7hen", ",", "das", "nicht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "PRELS", "PTKNEG"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Eigener Schmerz entpre\u00dft sterbenden Lippen.", "tokens": ["Ei\u00b7ge\u00b7ner", "Schmerz", "ent\u00b7pre\u00dft", "ster\u00b7ben\u00b7den", "Lip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Keine Genesungsmaale von mir", "tokens": ["Kei\u00b7ne", "Ge\u00b7ne\u00b7sungs\u00b7maa\u00b7le", "von", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "PPER"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Harren dein an deines Tempels Wand;", "tokens": ["Har\u00b7ren", "dein", "an", "dei\u00b7nes", "Tem\u00b7pels", "Wand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Statt der Gaben nimm von mir, o Retterinn,", "tokens": ["Statt", "der", "Ga\u00b7ben", "nimm", "von", "mir", ",", "o", "Ret\u00b7te\u00b7rinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "APPR", "PPER", "$,", "FM", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Die bangen Thr\u00e4nen, die mein Aug' und mein blutend Herz", "tokens": ["Die", "ban\u00b7gen", "Thr\u00e4\u00b7nen", ",", "die", "mein", "Aug'", "und", "mein", "blu\u00b7tend", "Herz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "KON", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Weinen auf Emilia's bleiche Hand!", "tokens": ["Wei\u00b7nen", "auf", "E\u00b7mi\u00b7lia's", "blei\u00b7che", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Ach, Emilia, sieh', Emilia liegt", "tokens": ["Ach", ",", "E\u00b7mi\u00b7lia", ",", "sieh'", ",", "E\u00b7mi\u00b7lia", "liegt"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "NE", "$,", "VVFIN", "$,", "NE", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kraftlos und leidend! Es beugt sich welk", "tokens": ["Kraft\u00b7los", "und", "lei\u00b7dend", "!", "Es", "beugt", "sich", "welk"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "$.", "PPER", "VVFIN", "PRF", "ADJD"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Herab die Rose der Wang', es umw\u00f6lkt sich, ach!", "tokens": ["Her\u00b7ab", "die", "Ro\u00b7se", "der", "Wang'", ",", "es", "um\u00b7w\u00f6lkt", "sich", ",", "ach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$,", "PPER", "VVFIN", "PRF", "$,", "ITJ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "In Emilia's Auge die Heitre der Himmelsbl\u00e4ue.", "tokens": ["In", "E\u00b7mi\u00b7lia's", "Au\u00b7ge", "die", "Heit\u00b7re", "der", "Him\u00b7mels\u00b7bl\u00e4u\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}}, "stanza.6": {"line.1": {"text": "Hygea, G\u00f6ttinn, es keimt", "tokens": ["Hy\u00b7gea", ",", "G\u00f6t\u00b7tinn", ",", "es", "keimt"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "PPER", "VVFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Da, wo du wallst \u00fcber der Gebirge Scheitel", "tokens": ["Da", ",", "wo", "du", "wallst", "\u00fc\u00b7ber", "der", "Ge\u00b7bir\u00b7ge", "Schei\u00b7tel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PPER", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Und durch den Schoo\u00df des gr\u00fcnenden Thals,", "tokens": ["Und", "durch", "den", "Schoo\u00df", "des", "gr\u00fc\u00b7nen\u00b7den", "Thals", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Labsal unter deinem Fu\u00df empor und Heil!", "tokens": ["Lab\u00b7sal", "un\u00b7ter", "dei\u00b7nem", "Fu\u00df", "em\u00b7por", "und", "Heil", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PTKVZ", "KON", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Deine Locken athmen, wenn du vor\u00fcberschwebst,", "tokens": ["Dei\u00b7ne", "Lo\u00b7cken", "ath\u00b7men", ",", "wenn", "du", "vor\u00b7\u00fc\u00b7bersc\u00b7hwebst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Lebenshauche mit der Narde Duft,", "tokens": ["Le\u00b7bens\u00b7hau\u00b7che", "mit", "der", "Nar\u00b7de", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und da, wo die L\u00fcfte theilt dein Saffranmantel", "tokens": ["Und", "da", ",", "wo", "die", "L\u00fcf\u00b7te", "theilt", "dein", "Saf\u00b7fran\u00b7man\u00b7tel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Gaukeln heilende W\u00fcrzger\u00fcche dir nach!", "tokens": ["Gau\u00b7keln", "hei\u00b7len\u00b7de", "W\u00fcrz\u00b7ge\u00b7r\u00fc\u00b7che", "dir", "nach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Helferinn, du bist reich", "tokens": ["Hel\u00b7fe\u00b7rinn", ",", "du", "bist", "reich"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADJD"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "An Rettung! Aber dein Balsamkelch ist er", "tokens": ["An", "Ret\u00b7tung", "!", "A\u00b7ber", "dein", "Bal\u00b7sam\u00b7kelch", "ist", "er"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$.", "KON", "PPOSAT", "NN", "VAFIN", "PPER"], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ach, ausgegossen! \u2013 so komm, ich beschw\u00f6re dich, komm,", "tokens": ["Ach", ",", "aus\u00b7ge\u00b7gos\u00b7sen", "!", "\u2013", "so", "komm", ",", "ich", "be\u00b7schw\u00f6\u00b7re", "dich", ",", "komm", ","], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "VVPP", "$.", "$(", "ADV", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$,", "VVFIN", "$,"], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Raube von mir der St\u00e4rke F\u00fclle,", "tokens": ["Rau\u00b7be", "von", "mir", "der", "St\u00e4r\u00b7ke", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Die aus der Gesundheit \u00fcbersch\u00e4umendem Becher,", "tokens": ["Die", "aus", "der", "Ge\u00b7sund\u00b7heit", "\u00fc\u00b7bersc\u00b7h\u00e4u\u00b7men\u00b7dem", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Tobend mir in den Adern fleu\u00dft", "tokens": ["To\u00b7bend", "mir", "in", "den", "A\u00b7dern", "fleu\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Und in dem Nervenarm schwellend mir zuckt!", "tokens": ["Und", "in", "dem", "Ner\u00b7ve\u00b7narm", "schwel\u00b7lend", "mir", "zuckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "PPER", "VVFIN", "$."], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Nimm was dein ist von mir und tr\u00e4ufle Genesung", "tokens": ["Nimm", "was", "dein", "ist", "von", "mir", "und", "tr\u00e4uf\u00b7le", "Ge\u00b7ne\u00b7sung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PWS", "PPOSAT", "VAFIN", "APPR", "PPER", "KON", "ADJA", "NN"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "In die lechzenden Lippen Emilia's, da\u00df sie wieder", "tokens": ["In", "die", "lech\u00b7zen\u00b7den", "Lip\u00b7pen", "E\u00b7mi\u00b7lia's", ",", "da\u00df", "sie", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "$,", "KOUS", "PPER", "ADV"], "meter": "--+--+---+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Gl\u00fch'n, und da\u00df wieder, wie von Morgenthau erquickt,", "tokens": ["Gl\u00fch'n", ",", "und", "da\u00df", "wie\u00b7der", ",", "wie", "von", "Mor\u00b7gent\u00b7hau", "er\u00b7quickt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "KOUS", "ADV", "$,", "PWAV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bl\u00fche die Rose der Wang', und Emilia's Auge", "tokens": ["Bl\u00fc\u00b7he", "die", "Ro\u00b7se", "der", "Wang'", ",", "und", "E\u00b7mi\u00b7lia's", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "NN", "$,", "KON", "NE", "NN"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Gl\u00e4nze wieder im Schmuck des Sonnenhimmels.", "tokens": ["Gl\u00e4n\u00b7ze", "wie\u00b7der", "im", "Schmuck", "des", "Son\u00b7nen\u00b7him\u00b7mels", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-++-+-+-+-", "measure": "unknown.measure.hexa"}}, "stanza.9": {"line.1": {"text": "H\u00f6re, G\u00f6ttinn, ach! mein Flehn und das Flehen Aller,", "tokens": ["H\u00f6\u00b7re", ",", "G\u00f6t\u00b7tinn", ",", "ach", "!", "mein", "Flehn", "und", "das", "Fle\u00b7hen", "Al\u00b7ler", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "ITJ", "$.", "PPOSAT", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Aller Guten, die All' Emilia lieben!", "tokens": ["Al\u00b7ler", "Gu\u00b7ten", ",", "die", "All'", "E\u00b7mi\u00b7lia", "lie\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PIAT", "NN", "VVINF", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "Gieb mir, o Tochter Gottes, mit der Genesung", "tokens": ["Gieb", "mir", ",", "o", "Toch\u00b7ter", "Got\u00b7tes", ",", "mit", "der", "Ge\u00b7ne\u00b7sung"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "FM", "NN", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Meiner Leidenden, wieder Trost und Ruhe!", "tokens": ["Mei\u00b7ner", "Lei\u00b7den\u00b7den", ",", "wie\u00b7der", "Trost", "und", "Ru\u00b7he", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "NN", "KON", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ach! sie flohen von mir, und es schleicht indessen,", "tokens": ["Ach", "!", "sie", "flo\u00b7hen", "von", "mir", ",", "und", "es", "s