{"textgrid.poem.60972": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "1L: O F\u00fcrst, aus einem Stamm von Weisen,", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O F\u00fcrst, aus einem Stamm von Weisen,", "tokens": ["O", "F\u00fcrst", ",", "aus", "ei\u00b7nem", "Stamm", "von", "Wei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den alle mild und edel preisen", "tokens": ["Den", "al\u00b7le", "mild", "und", "e\u00b7del", "prei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "ADJD", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vereint und laut:", "tokens": ["Ver\u00b7eint", "und", "laut", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ist mir's verg\u00f6nnt, ein Wort zu wagen,", "tokens": ["Ist", "mir's", "ver\u00b7g\u00f6nnt", ",", "ein", "Wort", "zu", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "VVPP", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Obwohl ich dich in meinen Tagen", "tokens": ["Ob\u00b7wohl", "ich", "dich", "in", "mei\u00b7nen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von Angesichte nie geschaut?", "tokens": ["Von", "An\u00b7ge\u00b7sich\u00b7te", "nie", "ge\u00b7schaut", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwar werd ich deine Gunst verlieren,", "tokens": ["Zwar", "werd", "ich", "dei\u00b7ne", "Gunst", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wofern sie je, dies Haupt zu zieren,", "tokens": ["Wo\u00b7fern", "sie", "je", ",", "dies", "Haupt", "zu", "zie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "PDS", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mir ward zuteil:", "tokens": ["Mir", "ward", "zu\u00b7teil", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Du neigtest einst dich meinen Scherzen,", "tokens": ["Du", "neig\u00b7test", "einst", "dich", "mei\u00b7nen", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bringe jetzt ein Lied der Schmerzen,", "tokens": ["Ich", "brin\u00b7ge", "jetzt", "ein", "Lied", "der", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch such ich nicht mein eigen Heil.", "tokens": ["Doch", "such", "ich", "nicht", "mein", "ei\u00b7gen", "Heil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich flehe f\u00fcr das Volk der Leiden,", "tokens": ["Ich", "fle\u00b7he", "f\u00fcr", "das", "Volk", "der", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das aus der Heimat auszuscheiden", "tokens": ["Das", "aus", "der", "Hei\u00b7mat", "aus\u00b7zu\u00b7schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gedr\u00e4ngt die Zeit;", "tokens": ["Ge\u00b7dr\u00e4ngt", "die", "Zeit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ich flehe f\u00fcr umsonst Ermannte,", "tokens": ["Ich", "fle\u00b7he", "f\u00fcr", "um\u00b7sonst", "Er\u00b7mann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr fl\u00fcchtige Helden und verbannte", "tokens": ["F\u00fcr", "fl\u00fcch\u00b7ti\u00b7ge", "Hel\u00b7den", "und", "ver\u00b7bann\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "VVFIN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Um einen Funken Menschlichkeit.", "tokens": ["Um", "ei\u00b7nen", "Fun\u00b7ken", "Menschlich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie sein der Rache nicht verfallen!", "tokens": ["Sie", "sein", "der", "Ra\u00b7che", "nicht", "ver\u00b7fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schon ist das Herz im Busen Allen", "tokens": ["Schon", "ist", "das", "Herz", "im", "Bu\u00b7sen", "Al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Genug beschwert,", "tokens": ["Ge\u00b7nug", "be\u00b7schwert", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ums Vaterland genug bek\u00fcmmert:", "tokens": ["Ums", "Va\u00b7ter\u00b7land", "ge\u00b7nug", "be\u00b7k\u00fcm\u00b7mert", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vom Henker werde nicht zertr\u00fcmmert", "tokens": ["Vom", "Hen\u00b7ker", "wer\u00b7de", "nicht", "zer\u00b7tr\u00fcm\u00b7mert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PTKNEG", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr edles und ber\u00fchmtes Schwert!", "tokens": ["Ihr", "ed\u00b7les", "und", "be\u00b7r\u00fchm\u00b7tes", "Schwert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie auch des Gegners Groll sich steigert,", "tokens": ["Wie", "auch", "des", "Geg\u00b7ners", "Groll", "sich", "stei\u00b7gert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Werd ihnen kein Asyl verweigert,", "tokens": ["Werd", "ih\u00b7nen", "kein", "A\u00b7syl", "ver\u00b7wei\u00b7gert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Kein Trost im Schmerz!", "tokens": ["Kein", "Trost", "im", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und wo ein Gast sich eingefunden,", "tokens": ["Und", "wo", "ein", "Gast", "sich", "ein\u00b7ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Betr\u00e4ufle Balsam seine Wunden,", "tokens": ["Be\u00b7tr\u00e4uf\u00b7le", "Bal\u00b7sam", "sei\u00b7ne", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So lange schl\u00e4gt ein deutsches Herz!", "tokens": ["So", "lan\u00b7ge", "schl\u00e4gt", "ein", "deut\u00b7sches", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und k\u00f6nnten F\u00fcrsten dies verneinen,", "tokens": ["Und", "k\u00f6nn\u00b7ten", "F\u00fcrs\u00b7ten", "dies", "ver\u00b7nei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "PDS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So m\u00f6cht ein Phalaris erscheinen,", "tokens": ["So", "m\u00f6cht", "ein", "Pha\u00b7la\u00b7ris", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von Scham entbl\u00f6\u00dft,", "tokens": ["Von", "Scham", "ent\u00b7bl\u00f6\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Der die, die seinen Schutz erk\u00fcren,", "tokens": ["Der", "die", ",", "die", "sei\u00b7nen", "Schutz", "er\u00b7k\u00fc\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die seine H\u00f6lle helfen sch\u00fcren,", "tokens": ["Die", "sei\u00b7ne", "H\u00f6l\u00b7le", "hel\u00b7fen", "sch\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In ihren eignen Ofen st\u00f6\u00dft!", "tokens": ["In", "ih\u00b7ren", "eig\u00b7nen", "O\u00b7fen", "st\u00f6\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie mancher w\u00e4hnt den Feind zersplittert,", "tokens": ["Wie", "man\u00b7cher", "w\u00e4hnt", "den", "Feind", "zer\u00b7split\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Indes die Nemesis umwittert", "tokens": ["In\u00b7des", "die", "Ne\u00b7me\u00b7sis", "um\u00b7wit\u00b7tert"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Siegers Zelt.", "tokens": ["Des", "Sie\u00b7gers", "Zelt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Triumphe sind wie Niederlagen,", "tokens": ["Tri\u00b7um\u00b7phe", "sind", "wie", "Nie\u00b7der\u00b7la\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KOKOM", "NN", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Wenn ihre Frucht besteht in Klagen,", "tokens": ["Wenn", "ih\u00b7re", "Frucht", "be\u00b7steht", "in", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im grenzenlosen Ha\u00df der Welt.", "tokens": ["Im", "gren\u00b7zen\u00b7lo\u00b7sen", "Ha\u00df", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und sei's, und soll die Welt es Glauben,", "tokens": ["Und", "sei's", ",", "und", "soll", "die", "Welt", "es", "Glau\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "KON", "VMFIN", "ART", "NN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der M\u00e4chtige darf sich k\u00fchn erlauben", "tokens": ["Der", "M\u00e4ch\u00b7ti\u00b7ge", "darf", "sich", "k\u00fchn", "er\u00b7lau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PRF", "ADJD", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Jedwede Tat:", "tokens": ["Jed\u00b7we\u00b7de", "Tat", ":"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Er wetze hunderttausend Klingen", "tokens": ["Er", "wet\u00b7ze", "hun\u00b7dert\u00b7tau\u00b7send", "Klin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und lasse sein Tedeum singen", "tokens": ["Und", "las\u00b7se", "sein", "Te\u00b7de\u00b7um", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Vom Volke, das er niedertrat!", "tokens": ["Vom", "Vol\u00b7ke", ",", "das", "er", "nie\u00b7der\u00b7trat", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Nur borg er nicht den Schein des Rechtes,", "tokens": ["Nur", "borg", "er", "nicht", "den", "Schein", "des", "Rech\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er flehe nicht zu Gott f\u00fcr Schlechtes", "tokens": ["Er", "fle\u00b7he", "nicht", "zu", "Gott", "f\u00fcr", "Schlech\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Um Schutz und Wehr;", "tokens": ["Um", "Schutz", "und", "Wehr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Er trage frei das offne Laster,", "tokens": ["Er", "tra\u00b7ge", "frei", "das", "off\u00b7ne", "Las\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und seine Stirn von Alabaster", "tokens": ["Und", "sei\u00b7ne", "Stirn", "von", "A\u00b7la\u00b7bas\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Beflecke keine R\u00f6te mehr!", "tokens": ["Be\u00b7fle\u00b7cke", "kei\u00b7ne", "R\u00f6\u00b7te", "mehr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Nur r\u00fchm er nicht sich und erdichte", "tokens": ["Nur", "r\u00fchm", "er", "nicht", "sich", "und", "er\u00b7dich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "PRF", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein g\u00f6ttlich Recht! Es ruft Geschichte", "tokens": ["Ein", "g\u00f6tt\u00b7lich", "Recht", "!", "Es", "ruft", "Ge\u00b7schich\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "$.", "PPER", "VVFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr lautes Nein.", "tokens": ["Ihr", "lau\u00b7tes", "Nein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Wie manche, deren Gr\u00e4ber sprechen,", "tokens": ["Wie", "man\u00b7che", ",", "de\u00b7ren", "Gr\u00e4\u00b7ber", "spre\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "PRELAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Erlangten Kronen durch Verbrechen!", "tokens": ["Er\u00b7lang\u00b7ten", "Kro\u00b7nen", "durch", "Ver\u00b7bre\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kann ein Verbrechen g\u00f6ttlich sein?", "tokens": ["Kann", "ein", "Ver\u00b7bre\u00b7chen", "g\u00f6tt\u00b7lich", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Manch Reich entstand durch Schwert und Flamme,", "tokens": ["Manch", "Reich", "ent\u00b7stand", "durch", "Schwert", "und", "Flam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es ist von manchem hohen Stamme", "tokens": ["Es", "ist", "von", "man\u00b7chem", "ho\u00b7hen", "Stam\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wurzel faul.", "tokens": ["Die", "Wur\u00b7zel", "faul", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und seit es K\u00f6nige hat gegeben,", "tokens": ["Und", "seit", "es", "K\u00f6\u00b7ni\u00b7ge", "hat", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "So rief sie nur das Volk ins Leben", "tokens": ["So", "rief", "sie", "nur", "das", "Volk", "ins", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Seit jenem ersten K\u00f6nig Saul!", "tokens": ["Seit", "je\u00b7nem", "ers\u00b7ten", "K\u00f6\u00b7nig", "Saul", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Nur um des Volkes Wunsch zu stillen,", "tokens": ["Nur", "um", "des", "Vol\u00b7kes", "Wunsch", "zu", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hat ihn gesalbt mit Widerwillen", "tokens": ["Hat", "ihn", "ge\u00b7salbt", "mit", "Wi\u00b7der\u00b7wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Herrn Prophet.", "tokens": ["Des", "Herrn", "Pro\u00b7phet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Oh, m\u00f6chten F\u00fcrsten stets empfinden,", "tokens": ["Oh", ",", "m\u00f6ch\u00b7ten", "F\u00fcrs\u00b7ten", "stets", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df Erdentage schnell verschwinden", "tokens": ["Da\u00df", "Er\u00b7den\u00b7ta\u00b7ge", "schnell", "ver\u00b7schwin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und nur des Namens Ruhm besteht!", "tokens": ["Und", "nur", "des", "Na\u00b7mens", "Ruhm", "be\u00b7steht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}