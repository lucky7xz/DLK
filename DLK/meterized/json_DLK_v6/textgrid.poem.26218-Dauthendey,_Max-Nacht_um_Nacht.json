{"textgrid.poem.26218": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Nacht um Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mond zieht hinterm Schiff einher,", "tokens": ["Der", "Mond", "zieht", "hin\u00b7term", "Schiff", "ein\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er wird des Abends Herr im Meer,", "tokens": ["Er", "wird", "des", "A\u00b7bends", "Herr", "im", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Begleitet Nacht um Nacht die Fahrt.", "tokens": ["Be\u00b7glei\u00b7tet", "Nacht", "um", "Nacht", "die", "Fahrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich hab' ihm forschend nachgestarrt,", "tokens": ["Ich", "hab'", "ihm", "for\u00b7schend", "nach\u00b7ge\u00b7starrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich fragte ihn: \u00bbWohin so sp\u00e4t?\u00ab", "tokens": ["Ich", "frag\u00b7te", "ihn", ":", "\u00bb", "Wo\u00b7hin", "so", "sp\u00e4t", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "$(", "PWAV", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u2013 Auch er wei\u00df nicht, wohin es geht.", "tokens": ["\u2013", "Auch", "er", "wei\u00df", "nicht", ",", "wo\u00b7hin", "es", "geht", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}