{"dta.poem.19353": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "86.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie wunderbarer Trieb Ameisenmillionen", "tokens": ["Wie", "wun\u00b7der\u00b7ba\u00b7rer", "Trieb", "A\u00b7mei\u00b7sen\u00b7mil\u00b7li\u00b7o\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beseelt, die einen Bau, den alle baun, bewohnen,", "tokens": ["Be\u00b7seelt", ",", "die", "ei\u00b7nen", "Bau", ",", "den", "al\u00b7le", "baun", ",", "be\u00b7woh\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "PRELS", "ART", "NN", "$,", "PRELS", "PIS", "VVINF", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In Ordnung ohne Bruch, in Eintracht ohne St\u00f6rung,", "tokens": ["In", "Ord\u00b7nung", "oh\u00b7ne", "Bruch", ",", "in", "Ein\u00b7tracht", "oh\u00b7ne", "St\u00f6\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ohn' Obrigkeit und Spruch, ohn' Aufruhr und Emp\u00f6rung;", "tokens": ["Ohn'", "Ob\u00b7rig\u00b7keit", "und", "Spruch", ",", "ohn'", "Auf\u00b7ruhr", "und", "Em\u00b7p\u00f6\u00b7rung", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Als regte ganz den Staat gemeinschaftlicher Rath,", "tokens": ["Als", "reg\u00b7te", "ganz", "den", "Staat", "ge\u00b7mein\u00b7schaft\u00b7li\u00b7cher", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ganz nur ihn bewegt gemeinschaftliche That.", "tokens": ["Da", "ganz", "nur", "ihn", "be\u00b7wegt", "ge\u00b7mein\u00b7schaft\u00b7li\u00b7che", "That", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Mensch, hinter der Natur wie stehst du weit zur\u00fcck!", "tokens": ["Mensch", ",", "hin\u00b7ter", "der", "Na\u00b7tur", "wie", "stehst", "du", "weit", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "KOKOM", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann wirst du aus dir selbst entfalten solch ein Gl\u00fcck?", "tokens": ["Wann", "wirst", "du", "aus", "dir", "selbst", "ent\u00b7fal\u00b7ten", "solch", "ein", "Gl\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "APPR", "PPER", "ADV", "VVFIN", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wann wie ein h\u00f6herer Naturgeist dich durchdringt", "tokens": ["Wann", "wie", "ein", "h\u00f6\u00b7he\u00b7rer", "Na\u00b7tur\u00b7geist", "dich", "durch\u00b7dringt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "KOKOM", "ART", "ADJA", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die g\u00f6ttliche Vernunft, und G\u00f6ttliches vollbringt;", "tokens": ["Die", "g\u00f6tt\u00b7li\u00b7che", "Ver\u00b7nunft", ",", "und", "G\u00f6tt\u00b7li\u00b7ches", "voll\u00b7bringt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Da\u00df, wie Ameisen jetzt, einst Menschenmillionen,", "tokens": ["Da\u00df", ",", "wie", "A\u00b7mei\u00b7sen", "jetzt", ",", "einst", "Men\u00b7schen\u00b7mil\u00b7li\u00b7o\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "NN", "ADV", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von gleichem Trieb beseelt, beisammen also wohnen,", "tokens": ["Von", "glei\u00b7chem", "Trieb", "be\u00b7seelt", ",", "bei\u00b7sam\u00b7men", "al\u00b7so", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "In Ordnung ohne Bruch, in Eintracht ohne St\u00f6rung,", "tokens": ["In", "Ord\u00b7nung", "oh\u00b7ne", "Bruch", ",", "in", "Ein\u00b7tracht", "oh\u00b7ne", "St\u00f6\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ohn' Obrigkeit und Spruch, ohn' Aufruhr noch Emp\u00f6rung.", "tokens": ["Ohn'", "Ob\u00b7rig\u00b7keit", "und", "Spruch", ",", "ohn'", "Auf\u00b7ruhr", "noch", "Em\u00b7p\u00f6\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}