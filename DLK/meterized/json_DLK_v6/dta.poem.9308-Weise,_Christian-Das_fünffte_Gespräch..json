{"dta.poem.9308": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das f\u00fcnffte Gespr\u00e4ch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df einen in der stadt", "tokens": ["Ich", "wei\u00df", "ei\u00b7nen", "in", "der", "stadt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der sechs paar sch\u00f6ne messer hat", "tokens": ["Der", "sechs", "paar", "sch\u00f6\u00b7ne", "mes\u00b7ser", "hat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "CARD", "PIAT", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In unterschiednen scheiden/", "tokens": ["In", "un\u00b7ter\u00b7schied\u00b7nen", "schei\u00b7den", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dieselben wil er k\u00fcnfftig leihn", "tokens": ["Die\u00b7sel\u00b7ben", "wil", "er", "k\u00fcnff\u00b7tig", "leihn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn jemand wil Trenchante seyn", "tokens": ["Wenn", "je\u00b7mand", "wil", "Tren\u00b7chan\u00b7te", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VMFIN", "NN", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bey ihren hochzeit freuden.", "tokens": ["Bey", "ih\u00b7ren", "hoch\u00b7zeit", "freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "2. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Damit verdient man ehr und danck", "tokens": ["Da\u00b7mit", "ver\u00b7dient", "man", "ehr", "und", "danck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PIS", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur an den jungfern-tische/", "tokens": ["Nur", "an", "den", "jung\u00b7fern\u00b7ti\u00b7sche", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die leutgen essen selten viel/", "tokens": ["Die", "leut\u00b7gen", "es\u00b7sen", "sel\u00b7ten", "viel", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und welche was verschlingen wil/", "tokens": ["Und", "wel\u00b7che", "was", "ver\u00b7schlin\u00b7gen", "wil", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "PWS", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die thut es unterm wische.", "tokens": ["Die", "thut", "es", "un\u00b7term", "wi\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}