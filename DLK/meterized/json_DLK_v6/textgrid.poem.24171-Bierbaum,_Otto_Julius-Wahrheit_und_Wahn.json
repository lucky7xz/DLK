{"textgrid.poem.24171": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Wahrheit und Wahn", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo w\u00e4chst die Wahrheit?", "tokens": ["Wo", "w\u00e4chst", "die", "Wahr\u00b7heit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Hinter dem Zaun, im Feld,", "tokens": ["Hin\u00b7ter", "dem", "Zaun", ",", "im", "Feld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Tief in der Wiese, im Wald:", "tokens": ["Tief", "in", "der", "Wie\u00b7se", ",", "im", "Wald", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,", "APPRART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Ueberall, \u00fcberall.", "tokens": ["Ue\u00b7be\u00b7rall", ",", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Aber \u00fcber sie her", "tokens": ["A\u00b7ber", "\u00fc\u00b7ber", "sie", "her"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "APZR"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Weht seine Halme der Wahn,", "tokens": ["Weht", "sei\u00b7ne", "Hal\u00b7me", "der", "Wahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Die Brotfrucht.", "tokens": ["Die", "Brot\u00b7frucht", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Willst du ihn ernten, den n\u00e4hrenden, greif", "tokens": ["Willst", "du", "ihn", "ern\u00b7ten", ",", "den", "n\u00e4h\u00b7ren\u00b7den", ",", "greif"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "PPER", "VVFIN", "$,", "ART", "ADJA", "$,", "ADJD"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Zu Sichel und Sense.", "tokens": ["Zu", "Si\u00b7chel", "und", "Sen\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Aber die Wahrheit zu pfl\u00fccken gen\u00fcgt", "tokens": ["A\u00b7ber", "die", "Wahr\u00b7heit", "zu", "pfl\u00fc\u00b7cken", "ge\u00b7n\u00fcgt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PTKZU", "VVINF", "VVFIN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Die Hand eines Kindes.", "tokens": ["Die", "Hand", "ei\u00b7nes", "Kin\u00b7des", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Drum siehst du sie \u00f6fter im Strau\u00dfe des Dichters,", "tokens": ["Drum", "siehst", "du", "sie", "\u00f6f\u00b7ter", "im", "Strau\u00b7\u00dfe", "des", "Dich\u00b7ters", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Als in den Tennen der klugen Leute.", "tokens": ["Als", "in", "den", "Ten\u00b7nen", "der", "klu\u00b7gen", "Leu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}