{"dta.poem.6833": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das alle T\u00f6ne rein und deutlich wieder giebet,", "tokens": ["Das", "al\u00b7le", "T\u00f6\u00b7ne", "rein", "und", "deut\u00b7lich", "wie\u00b7der", "gie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADJD", "KON", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die eine kluge Hand auf selben ausge\u00fcbet;", "tokens": ["Die", "ei\u00b7ne", "klu\u00b7ge", "Hand", "auf", "sel\u00b7ben", "aus\u00b7ge\u00b7\u00fc\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allein, was n\u00fctzet doch der sch\u00f6nen T\u00f6ne Klang?", "tokens": ["Al\u00b7lein", ",", "was", "n\u00fct\u00b7zet", "doch", "der", "sch\u00f6\u00b7nen", "T\u00f6\u00b7ne", "Klang", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "VVFIN", "ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn diese Wunder-Kunst nicht einen H\u00f6rer findet,", "tokens": ["Wenn", "die\u00b7se", "Wun\u00b7der\u00b7Kunst", "nicht", "ei\u00b7nen", "H\u00f6\u00b7rer", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der den bezaubernden Gesang", "tokens": ["Der", "den", "be\u00b7zau\u00b7bern\u00b7den", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "H\u00f6rt und beurtheilt und empfindet.", "tokens": ["H\u00f6rt", "und", "beurt\u00b7heilt", "und", "emp\u00b7fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dies ist die Seele nun, die dies Bewegen sp\u00fcrt,", "tokens": ["Dies", "ist", "die", "See\u00b7le", "nun", ",", "die", "dies", "Be\u00b7we\u00b7gen", "sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "$,", "PRELS", "PDS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sie \u00fcberleget sie, und wird durch sie ger\u00fchrt.", "tokens": ["Sie", "\u00fc\u00b7ber\u00b7le\u00b7get", "sie", ",", "und", "wird", "durch", "sie", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}