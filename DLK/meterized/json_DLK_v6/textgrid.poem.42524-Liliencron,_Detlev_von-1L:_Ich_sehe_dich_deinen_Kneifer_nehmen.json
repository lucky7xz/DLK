{"textgrid.poem.42524": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich sehe dich deinen Kneifer nehmen", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sehe dich deinen Kneifer nehmen", "tokens": ["Ich", "se\u00b7he", "dich", "dei\u00b7nen", "Knei\u00b7fer", "neh\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und auf die Nase dir bequemen;", "tokens": ["Und", "auf", "die", "Na\u00b7se", "dir", "be\u00b7que\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du suchst die Schere, schon liegt sie zur Hand,", "tokens": ["Du", "suchst", "die", "Sche\u00b7re", ",", "schon", "liegt", "sie", "zur", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und l\u00f6st vom Pakete Siegel und Band.", "tokens": ["Und", "l\u00f6st", "vom", "Pa\u00b7ke\u00b7te", "Sie\u00b7gel", "und", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Was ist denn das? Gedichte? Potz Blitz!", "tokens": ["Was", "ist", "denn", "das", "?", "Ge\u00b7dich\u00b7te", "?", "Potz", "Blitz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "PDS", "$.", "NN", "$.", "NN", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Gedichte von meinem Freunde Fritz.", "tokens": ["Ge\u00b7dich\u00b7te", "von", "mei\u00b7nem", "Freun\u00b7de", "Fritz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ei, ei, auch der ein Sonntagsj\u00e4ger,", "tokens": ["Ei", ",", "ei", ",", "auch", "der", "ein", "Sonn\u00b7tags\u00b7j\u00e4\u00b7ger", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$,", "ADV", "ART", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Lyraklimprer und Silbens\u00e4ger,", "tokens": ["Ein", "Ly\u00b7rak\u00b7limp\u00b7rer", "und", "Sil\u00b7ben\u00b7s\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Ein Mondscheinmeckrer, Gitarrenwimmrer,", "tokens": ["Ein", "Mond\u00b7schein\u00b7meck\u00b7rer", ",", "Gi\u00b7tar\u00b7ren\u00b7wimm\u00b7rer", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Ein Jambenbr\u00fcller und Stanzenzimmrer,", "tokens": ["Ein", "Jam\u00b7ben\u00b7br\u00fcl\u00b7ler", "und", "Stan\u00b7zen\u00b7zimm\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Hymnenheuler, Odenschnaufer,", "tokens": ["Hym\u00b7nen\u00b7heu\u00b7ler", ",", "O\u00b7den\u00b7schnau\u00b7fer", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Daktylenwirbler und Knittelversraufer.", "tokens": ["Dak\u00b7ty\u00b7len\u00b7wirb\u00b7ler", "und", "Knit\u00b7tel\u00b7ver\u00b7srau\u00b7fer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Dein feiner Spott liegt mir im Ohr;", "tokens": ["Dein", "fei\u00b7ner", "Spott", "liegt", "mir", "im", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du wei\u00dft, ich f\u00fcrcht mich ein wenig davor.", "tokens": ["Du", "wei\u00dft", ",", "ich", "f\u00fcrcht", "mich", "ein", "we\u00b7nig", "da\u00b7vor", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ART", "PIS", "PAV", "$."], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und doch, du Treuer, wie h\u00f6r ich ihn gern,", "tokens": ["Und", "doch", ",", "du", "Treu\u00b7er", ",", "wie", "h\u00f6r", "ich", "ihn", "gern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPER", "NN", "$,", "PWAV", "VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wir denken ja beide \u00fcber den Stern,", "tokens": ["Wir", "den\u00b7ken", "ja", "bei\u00b7de", "\u00fc\u00b7ber", "den", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der sich Erde nennt, fast immer gleich;", "tokens": ["Der", "sich", "Er\u00b7de", "nennt", ",", "fast", "im\u00b7mer", "gleich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "NN", "VVFIN", "$,", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Nicht wahr, auch \u00fcber das Himmelreich.", "tokens": ["Nicht", "wahr", ",", "auch", "\u00fc\u00b7ber", "das", "Him\u00b7mel\u00b7reich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Und nun, du klappst mein Buch schon zu,", "tokens": ["Und", "nun", ",", "du", "klappst", "mein", "Buch", "schon", "zu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schn\u00fcrst es ein zur ewigen Ruh,", "tokens": ["Und", "schn\u00fcrst", "es", "ein", "zur", "e\u00b7wi\u00b7gen", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Schleudersts hinauf auf den h\u00f6chsten Schrank,", "tokens": ["Schleu\u00b7dersts", "hin\u00b7auf", "auf", "den", "h\u00f6chs\u00b7ten", "Schrank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und das ist all f\u00fcr mich dein Dank?", "tokens": ["Und", "das", "ist", "all", "f\u00fcr", "mich", "dein", "Dank", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PIAT", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da ruht es aus auf deinen Befehl,", "tokens": ["Da", "ruht", "es", "aus", "auf", "dei\u00b7nen", "Be\u00b7fehl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Just zwischen Mozart und Marc Aurel,", "tokens": ["Just", "zwi\u00b7schen", "Mo\u00b7zart", "und", "Marc", "Au\u00b7rel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "KON", "NE", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Die h\u00e4upterbestaubt dort oben stehn;", "tokens": ["Die", "h\u00e4up\u00b7ter\u00b7be\u00b7staubt", "dort", "o\u00b7ben", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So wird es auch meinem B\u00fcchlein ergehn?", "tokens": ["So", "wird", "es", "auch", "mei\u00b7nem", "B\u00fcch\u00b7lein", "er\u00b7gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Dann murmelst du, der Klemmer f\u00e4llt:", "tokens": ["Dann", "mur\u00b7melst", "du", ",", "der", "Klem\u00b7mer", "f\u00e4llt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da hat mich der Gute sch\u00f6n geprellt,", "tokens": ["Da", "hat", "mich", "der", "Gu\u00b7te", "sch\u00f6n", "ge\u00b7prellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Es ist denn doch wirklich nachgrade zu arg,", "tokens": ["Es", "ist", "denn", "doch", "wirk\u00b7lich", "nach\u00b7gra\u00b7de", "zu", "arg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Der Deutsche verselt selbst im Sarg.", "tokens": ["Der", "Deut\u00b7sche", "ver\u00b7selt", "selbst", "im", "Sarg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "Ich bestimme, schmiert er f\u00fcrder Gedichte,", "tokens": ["Ich", "be\u00b7stim\u00b7me", ",", "schmiert", "er", "f\u00fcr\u00b7der", "Ge\u00b7dich\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Wir stellen ihn gleich vor die Schwurgerichte.", "tokens": ["Wir", "stel\u00b7len", "ihn", "gleich", "vor", "die", "Schwur\u00b7ge\u00b7rich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}