{"dta.poem.20051": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht im Thale der s\u00fc\u00dfen Heimat,", "tokens": ["Nicht", "im", "Tha\u00b7le", "der", "s\u00fc\u00b7\u00dfen", "Hei\u00b7mat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Beym Gemurmel der Silberquelle \u2014", "tokens": ["Beym", "Ge\u00b7mur\u00b7mel", "der", "Sil\u00b7ber\u00b7quel\u00b7le"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Bleich getragen aus dem Schlachtfeld", "tokens": ["Bleich", "ge\u00b7tra\u00b7gen", "aus", "dem", "Schlacht\u00b7feld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denk' ich dein, du s\u00fc\u00dfes Leben!", "tokens": ["Denk'", "ich", "dein", ",", "du", "s\u00fc\u00b7\u00dfes", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "All' die Freunde sind gefallen,", "tokens": ["All'", "die", "Freun\u00b7de", "sind", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sollt' ich weilen hier der eine?", "tokens": ["Sollt'", "ich", "wei\u00b7len", "hier", "der", "ei\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ADV", "ART", "ART", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nein! schon naht der bleiche Bote,", "tokens": ["Nein", "!", "schon", "naht", "der", "blei\u00b7che", "Bo\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der mich leitet zur s\u00fc\u00dfen Heimat.", "tokens": ["Der", "mich", "lei\u00b7tet", "zur", "s\u00fc\u00b7\u00dfen", "Hei\u00b7mat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Flecht' in's Haar den Kranz der Hochzeit,", "tokens": ["Flecht'", "in's", "Haar", "den", "Kranz", "der", "Hoch\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Halt bereit die Brautgewande", "tokens": ["Halt", "be\u00b7reit", "die", "Braut\u00b7ge\u00b7wan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ADJD", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die wollen, duft'gen Schaalen:", "tokens": ["Und", "die", "wol\u00b7len", ",", "duft'\u00b7gen", "Schaa\u00b7len", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "VMFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn wir kehren alle wieder", "tokens": ["Denn", "wir", "keh\u00b7ren", "al\u00b7le", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PIS", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In das Thal der s\u00fc\u00dfen Heimath.", "tokens": ["In", "das", "Thal", "der", "s\u00fc\u00b7\u00dfen", "Hei\u00b7math", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}