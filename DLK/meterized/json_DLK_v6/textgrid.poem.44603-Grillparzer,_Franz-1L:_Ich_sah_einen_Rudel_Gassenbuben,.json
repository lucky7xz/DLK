{"textgrid.poem.44603": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich sah einen Rudel Gassenbuben,", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah einen Rudel Gassenbuben,", "tokens": ["Ich", "sah", "ei\u00b7nen", "Ru\u00b7del", "Gas\u00b7sen\u00b7bu\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie kaum entschl\u00fcpft aus des Lehrers Stuben,", "tokens": ["Wie", "kaum", "ent\u00b7schl\u00fcpft", "aus", "des", "Leh\u00b7rers", "Stu\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVPP", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die warfen sich mit Ballen von Schnee", "tokens": ["Die", "war\u00b7fen", "sich", "mit", "Bal\u00b7len", "von", "Schnee"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und lachten, tats einem im Fallen weh.", "tokens": ["Und", "lach\u00b7ten", ",", "tats", "ei\u00b7nem", "im", "Fal\u00b7len", "weh", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "ART", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Sie waren mit Ekelnamen nicht faul", "tokens": ["Sie", "wa\u00b7ren", "mit", "E\u00b7kel\u00b7na\u00b7men", "nicht", "faul"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "PTKNEG", "ADJD"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und streckten die Zunge aus ihrem Maul.", "tokens": ["Und", "streck\u00b7ten", "die", "Zun\u00b7ge", "aus", "ih\u00b7rem", "Maul", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "\u00bbei\u00ab, dacht ich in meinem Sinne, \u00bbei,", "tokens": ["\u00bb", "ei", "\u00ab", ",", "dacht", "ich", "in", "mei\u00b7nem", "Sin\u00b7ne", ",", "\u00bb", "ei", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["$(", "XY", "$(", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,", "$(", "ITJ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Und so was duldet die Polizei?\u00ab", "tokens": ["Und", "so", "was", "dul\u00b7det", "die", "Po\u00b7li\u00b7zei", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "PWS", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Da gewahrt ich Gold in ihren Haaren", "tokens": ["Da", "ge\u00b7wahrt", "ich", "Gold", "in", "ih\u00b7ren", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Und sah erst, da\u00df es K\u00f6nige waren.", "tokens": ["Und", "sah", "erst", ",", "da\u00df", "es", "K\u00f6\u00b7ni\u00b7ge", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "KOUS", "PPER", "NN", "VAFIN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}