{"textgrid.poem.24893": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nacht war's; es hallte von dem Schellenklingen", "tokens": ["Nacht", "wa\u00b7r's", ";", "es", "hall\u00b7te", "von", "dem", "Schel\u00b7len\u00b7klin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Des Maultierzugs die Schlucht der Alpujarren;", "tokens": ["Des", "Maul\u00b7ti\u00b7er\u00b7zugs", "die", "Schlucht", "der", "Al\u00b7pu\u00b7jar\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die kahlen Felsenh\u00e4upter sahn wir starren,", "tokens": ["Die", "kah\u00b7len", "Fel\u00b7sen\u00b7h\u00e4up\u00b7ter", "sahn", "wir", "star\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die um die Stirn den Gletscherturban schlingen.", "tokens": ["Die", "um", "die", "Stirn", "den", "Glet\u00b7scher\u00b7tur\u00b7ban", "schlin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der F\u00fchrer ritt voran durch wildgezackte", "tokens": ["Der", "F\u00fch\u00b7rer", "ritt", "vo\u00b7ran", "durch", "wild\u00b7ge\u00b7zack\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Steinklippen, und auf sturmzernagtem Pfade", "tokens": ["Stein\u00b7klip\u00b7pen", ",", "und", "auf", "sturm\u00b7zer\u00b7nag\u00b7tem", "Pfa\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "+---+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Zum Klange seiner maurischen Ballade", "tokens": ["Zum", "Klan\u00b7ge", "sei\u00b7ner", "mau\u00b7ri\u00b7schen", "Bal\u00b7la\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bewegte langsam sich der Zug im Takte.", "tokens": ["Be\u00b7weg\u00b7te", "lang\u00b7sam", "sich", "der", "Zug", "im", "Tak\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PRF", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da stieg am Himmelsrand die ew'ge Leuchte,", "tokens": ["Da", "stieg", "am", "Him\u00b7mels\u00b7rand", "die", "ew'\u00b7ge", "Leuch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Vega lag vor uns im Morgenstrahle", "tokens": ["Die", "Ve\u00b7ga", "lag", "vor", "uns", "im", "Mor\u00b7gen\u00b7strah\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und dampfte aufw\u00e4rts, eine Opferschale", "tokens": ["Und", "dampf\u00b7te", "auf\u00b7w\u00e4rts", ",", "ei\u00b7ne", "Op\u00b7fer\u00b7scha\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Voll Weihrauch und voll klarer Himmelsfeuchte.", "tokens": ["Voll", "Weih\u00b7rauch", "und", "voll", "kla\u00b7rer", "Him\u00b7mels\u00b7feuch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Im Fr\u00fchglanz strahlten der Nevada Gipfel,", "tokens": ["Im", "Fr\u00fch\u00b7glanz", "strahl\u00b7ten", "der", "Ne\u00b7va\u00b7da", "Gip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie goldne Kuppeld\u00e4cher von Moscheen;", "tokens": ["Wie", "gold\u00b7ne", "Kup\u00b7pel\u00b7d\u00e4\u00b7cher", "von", "Mo\u00b7scheen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "And\u00e4chtig neigten in des Ostes Wehen,", "tokens": ["An\u00b7d\u00e4ch\u00b7tig", "neig\u00b7ten", "in", "des", "Os\u00b7tes", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gleich Betenden, die Palmen ihre Wipfel.", "tokens": ["Gleich", "Be\u00b7ten\u00b7den", ",", "die", "Pal\u00b7men", "ih\u00b7re", "Wip\u00b7fel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Vor uns von ihrem Teppich gr\u00fcner Saaten,", "tokens": ["Vor", "uns", "von", "ih\u00b7rem", "Tep\u00b7pich", "gr\u00fc\u00b7ner", "Saa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus Myrtendickicht und Orangenb\u00e4umen,", "tokens": ["Aus", "Myr\u00b7ten\u00b7di\u00b7ckicht", "und", "O\u00b7ran\u00b7gen\u00b7b\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Hob sich, ein Bild von Edens Wonnetr\u00e4umen,", "tokens": ["Hob", "sich", ",", "ein", "Bild", "von", "E\u00b7dens", "Won\u00b7net\u00b7r\u00e4u\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "ART", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Wunderstadt, die Schwester der Granaten.", "tokens": ["Die", "Wun\u00b7der\u00b7stadt", ",", "die", "Schwes\u00b7ter", "der", "Gra\u00b7na\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wir aber sanken auf die Stirn und riefen:", "tokens": ["Wir", "a\u00b7ber", "san\u00b7ken", "auf", "die", "Stirn", "und", "rie\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sei Allah, da\u00df wir dich erschaun, gepriesen!", "tokens": ["Sei", "Al\u00b7lah", ",", "da\u00df", "wir", "dich", "er\u00b7schaun", ",", "ge\u00b7prie\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "KOUS", "PPER", "PRF", "VVINF", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O Houri aus Muhammeds Paradiesen!", "tokens": ["O", "Hou\u00b7ri", "aus", "Mu\u00b7ham\u00b7meds", "Pa\u00b7ra\u00b7die\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O Perle in dem Kronschmuck der Kalifen!", "tokens": ["O", "Per\u00b7le", "in", "dem", "Kron\u00b7schmuck", "der", "Ka\u00b7li\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}