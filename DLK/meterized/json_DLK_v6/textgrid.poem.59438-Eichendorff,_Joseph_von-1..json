{"textgrid.poem.59438": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1828, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unten endlos nichts als Wasser,", "tokens": ["Un\u00b7ten", "end\u00b7los", "nichts", "als", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Droben Himmel still und weit,", "tokens": ["Dro\u00b7ben", "Him\u00b7mel", "still", "und", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKVZ", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur das G\u00f6tterland, das blasse,", "tokens": ["Nur", "das", "G\u00f6t\u00b7ter\u00b7land", ",", "das", "blas\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lag in Meereseinsamkeit,", "tokens": ["Lag", "in", "Mee\u00b7re\u00b7sein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo auf farbenlosen Matten", "tokens": ["Wo", "auf", "far\u00b7ben\u00b7lo\u00b7sen", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gipfel wie in Tr\u00e4umen stehn,", "tokens": ["Gip\u00b7fel", "wie", "in", "Tr\u00e4u\u00b7men", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und Gestalten ohne Schatten", "tokens": ["Und", "Ge\u00b7stal\u00b7ten", "oh\u00b7ne", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ewig lautlos sich ergehn.", "tokens": ["E\u00b7wig", "laut\u00b7los", "sich", "er\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwischen grauen Wolkenschweifen,", "tokens": ["Zwi\u00b7schen", "grau\u00b7en", "Wol\u00b7ken\u00b7schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die verschlafen Berg und Flut", "tokens": ["Die", "ver\u00b7schla\u00b7fen", "Berg", "und", "Flut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit den langen Schleiern streifen,", "tokens": ["Mit", "den", "lan\u00b7gen", "Schlei\u00b7ern", "strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoch der G\u00f6ttervater ruht.", "tokens": ["Hoch", "der", "G\u00f6t\u00b7ter\u00b7va\u00b7ter", "ruht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Heut zu fischen ihn gel\u00fcstet,", "tokens": ["Heut", "zu", "fi\u00b7schen", "ihn", "ge\u00b7l\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und vom zack'gen Felsenhang", "tokens": ["Und", "vom", "zack'\u00b7gen", "Fel\u00b7sen\u00b7hang"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In des Meeres gr\u00fcne W\u00fcste", "tokens": ["In", "des", "Mee\u00b7res", "gr\u00fc\u00b7ne", "W\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Senket er die Schnur zum Fang.", "tokens": ["Sen\u00b7ket", "er", "die", "Schnur", "zum", "Fang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sinnend sitzt er, und es flattern", "tokens": ["Sin\u00b7nend", "sitzt", "er", ",", "und", "es", "flat\u00b7tern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "KON", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bart und Haar im Sturme weit,", "tokens": ["Bart", "und", "Haar", "im", "Stur\u00b7me", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPRART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Zeit wird ihm so lange", "tokens": ["Und", "die", "Zeit", "wird", "ihm", "so", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der stillen Ewigkeit.", "tokens": ["In", "der", "stil\u00b7len", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da f\u00fchlt er die Angel zucken:", "tokens": ["Da", "f\u00fchlt", "er", "die", "An\u00b7gel", "zu\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "\u00bbei, das ist ein schwerer Fisch!\u00ab", "tokens": ["\u00bb", "ei", ",", "das", "ist", "ein", "schwe\u00b7rer", "Fisch", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Freudig f\u00e4ngt er an zu rucken,", "tokens": ["Freu\u00b7dig", "f\u00e4ngt", "er", "an", "zu", "ru\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Stemmt sich, zieht und windet frisch.", "tokens": ["Stemmt", "sich", ",", "zieht", "und", "win\u00b7det", "frisch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "VVFIN", "KON", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sieh, da hebt er Felsenspitzen", "tokens": ["Sieh", ",", "da", "hebt", "er", "Fel\u00b7sen\u00b7spit\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Langsam aus der Wasser Grund,", "tokens": ["Lang\u00b7sam", "aus", "der", "Was\u00b7ser", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und erschrocken aus den Ritzen", "tokens": ["Und", "er\u00b7schro\u00b7cken", "aus", "den", "Rit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schie\u00dfen schupp'ge Schlangen bunt;", "tokens": ["Schie\u00b7\u00dfen", "schupp'\u00b7ge", "Schlan\u00b7gen", "bunt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ringelnd' Unget\u00fcm' der Tiefen,", "tokens": ["Rin\u00b7gelnd'", "Un\u00b7ge\u00b7t\u00fcm'", "der", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die im \u00f6den Wogenhaus", "tokens": ["Die", "im", "\u00f6\u00b7den", "Wo\u00b7gen\u00b7haus"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In der gr\u00fcnen D\u00e4mmrung schliefen,", "tokens": ["In", "der", "gr\u00fc\u00b7nen", "D\u00e4mm\u00b7rung", "schlie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "St\u00fcrzen sich ins Meer hinaus.", "tokens": ["St\u00fcr\u00b7zen", "sich", "ins", "Meer", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch der Vater hebt aufs neue,", "tokens": ["Doch", "der", "Va\u00b7ter", "hebt", "aufs", "neu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und Gebirge, Tal und Strand", "tokens": ["Und", "Ge\u00b7bir\u00b7ge", ",", "Tal", "und", "Strand"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Taucht allm\u00e4hlich auf ins Freie;", "tokens": ["Taucht", "all\u00b7m\u00e4h\u00b7lich", "auf", "ins", "Frei\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es gr\u00fcnt das junge Land,", "tokens": ["Und", "es", "gr\u00fcnt", "das", "jun\u00b7ge", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Irrend farb'ge Lichter schweifen", "tokens": ["Ir\u00b7rend", "fa\u00b7rb'\u00b7ge", "Lich\u00b7ter", "schwei\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Und von Blumen gl\u00e4nzt die Flur,", "tokens": ["Und", "von", "Blu\u00b7men", "gl\u00e4nzt", "die", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Wo des Vaters Blick' sie streifen \u2013", "tokens": ["Wo", "des", "Va\u00b7ters", "Blick'", "sie", "strei\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da zerrei\u00dft die Angelschnur.", "tokens": ["Da", "zer\u00b7rei\u00dft", "die", "An\u00b7gel\u00b7schnur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie 'ne liebliche Sirene", "tokens": ["Wie", "'ne", "lieb\u00b7li\u00b7che", "Si\u00b7re\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Halb nun \u00fcberm Wellenglanz,", "tokens": ["Halb", "nun", "\u00fc\u00b7berm", "Wel\u00b7len\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Staunend ob der eignen Sch\u00f6ne,", "tokens": ["Stau\u00b7nend", "ob", "der", "eig\u00b7nen", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwebt es mit dem Bl\u00fctenkranz,", "tokens": ["Schwebt", "es", "mit", "dem", "Bl\u00fc\u00b7ten\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bei der L\u00fcfte lindem F\u00e4cheln", "tokens": ["Bei", "der", "L\u00fcf\u00b7te", "lin\u00b7dem", "F\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich im Meer, das rosig brennt,", "tokens": ["Sich", "im", "Meer", ",", "das", "ro\u00b7sig", "brennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Spiegelnd mit versch\u00e4mtem L\u00e4cheln \u2013", "tokens": ["Spie\u00b7gelnd", "mit", "ver\u00b7sch\u00e4m\u00b7tem", "L\u00e4\u00b7cheln", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Erde sie der Vater nennt.", "tokens": ["Er\u00b7de", "sie", "der", "Va\u00b7ter", "nennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}