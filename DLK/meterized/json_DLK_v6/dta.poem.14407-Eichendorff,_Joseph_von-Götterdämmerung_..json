{"dta.poem.14407": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "G\u00f6tterd\u00e4mmerung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Sie selbst mu\u00df sinnend stehen", "tokens": ["Sie", "selbst", "mu\u00df", "sin\u00b7nend", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So bleich im Fr\u00fchlingsschein,", "tokens": ["So", "bleich", "im", "Fr\u00fch\u00b7lings\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Augen untergehen,", "tokens": ["Die", "Au\u00b7gen", "un\u00b7ter\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der sch\u00f6ne Leib wird Stein. \u2014", "tokens": ["Der", "sch\u00f6\u00b7ne", "Leib", "wird", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}