{"textgrid.poem.39740": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "2. Am andern Morgen", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ueber Nacht das Thal beschneit,", "tokens": ["Ue\u00b7ber", "Nacht", "das", "Thal", "be\u00b7schneit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber Nacht ward's Winterszeit!", "tokens": ["Ue\u00b7ber", "Nacht", "ward's", "Win\u00b7ter\u00b7szeit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schneewei\u00df bl\u00fchn alle B\u00e4ume,", "tokens": ["Schnee\u00b7wei\u00df", "bl\u00fchn", "al\u00b7le", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Das sind mir Bl\u00fctentr\u00e4ume!", "tokens": ["Das", "sind", "mir", "Bl\u00fc\u00b7ten\u00b7tr\u00e4u\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}