{"dta.poem.20162": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Lieb-erfreute  \n  FJLAMON .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1666", "urn": "urn:nbn:de:kobv:b4-20424-1", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch-Historischer Lustgarten. Frankfurt (Main), 1666."}, "poem": {"stanza.1": {"line.1": {"text": "Ach weh und aber weh? die ihr so junges Leben", "tokens": ["Ach", "weh", "und", "a\u00b7ber", "weh", "?", "die", "ihr", "so", "jun\u00b7ges", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "KON", "ADV", "PTKVZ", "$.", "PRELS", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Liebes eiteln Lust so sch\u00e4ndlich \u00fcbergeben!", "tokens": ["Der", "Lie\u00b7bes", "ei\u00b7teln", "Lust", "so", "sch\u00e4nd\u00b7lich", "\u00fc\u00b7ber\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die von Cupido sich von dem nichts werthen Kind", "tokens": ["Die", "von", "Cu\u00b7pi\u00b7do", "sich", "von", "dem", "nichts", "wert\u00b7hen", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NE", "PRF", "APPR", "ART", "PIS", "VVFIN", "NN"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Erschrekken lassen balt/ Ach blind und \u00fcber blindt", "tokens": ["Er\u00b7schrek\u00b7ken", "las\u00b7sen", "balt", "/", "Ach", "blind", "und", "\u00fc\u00b7ber", "blindt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "VVFIN", "$(", "ITJ", "ADJD", "KON", "APPR", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sind die/ so nach der Lied\u2019 und Venus Luften laussen/", "tokens": ["Sind", "die", "/", "so", "nach", "der", "Lied'", "und", "Ve\u00b7nus", "Luf\u00b7ten", "laus\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "$(", "ADV", "APPR", "ART", "NN", "KON", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weil sie auf ihren Hal\u00df viel Ungl\u00fck selber tauffen.", "tokens": ["Weil", "sie", "auf", "ih\u00b7ren", "Hal\u00df", "viel", "Un\u00b7gl\u00fck", "sel\u00b7ber", "tauf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Exempel hat man gnug. Doch liebe wer da wil", "tokens": ["Ex\u00b7em\u00b7pel", "hat", "man", "gnug", ".", "Doch", "lie\u00b7be", "wer", "da", "wil"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PIS", "ADV", "$.", "KON", "VVFIN", "PWS", "ADV", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das nichtig\u2019 Affenspiel/ sitz\u2019 ich doch in der Still/", "tokens": ["Das", "nich\u00b7tig'", "Af\u00b7fen\u00b7spiel", "/", "sitz'", "ich", "doch", "in", "der", "Still", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$(", "VVIMP", "PPER", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Und seh\u2019 in Wunder an/ wie hier auf dieser Erden", "tokens": ["Und", "seh'", "in", "Wun\u00b7der", "an", "/", "wie", "hier", "auf", "die\u00b7ser", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "PTKVZ", "$(", "KOKOM", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Viel Edle Cavaliers au\u00df Herren Sclaven werden.", "tokens": ["Viel", "Ed\u00b7le", "Ca\u00b7va\u00b7liers", "au\u00df", "Her\u00b7ren", "Scla\u00b7ven", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPR", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}