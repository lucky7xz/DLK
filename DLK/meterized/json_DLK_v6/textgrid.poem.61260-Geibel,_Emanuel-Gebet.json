{"textgrid.poem.61260": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Gebet", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr, den ich tief im Herzen trage, sei du mit mir!", "tokens": ["Herr", ",", "den", "ich", "tief", "im", "Her\u00b7zen", "tra\u00b7ge", ",", "sei", "du", "mit", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$,", "VAFIN", "PPER", "APPR", "PPER", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Du Gnadenhort in Gl\u00fcck und Plage, sei du mit mir!", "tokens": ["Du", "Gna\u00b7den\u00b7hort", "in", "Gl\u00fcck", "und", "Pla\u00b7ge", ",", "sei", "du", "mit", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "KON", "NN", "$,", "VAFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Im Brand des Sommers, der dem Manne die Wange br\u00e4unt,", "tokens": ["Im", "Brand", "des", "Som\u00b7mers", ",", "der", "dem", "Man\u00b7ne", "die", "Wan\u00b7ge", "br\u00e4unt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wie in der Jugend Rosenhage, sei du mit mir;", "tokens": ["Wie", "in", "der", "Ju\u00b7gend", "Ro\u00b7sen\u00b7ha\u00b7ge", ",", "sei", "du", "mit", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "$,", "VAFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.5": {"text": "Beh\u00fcte mich am Born der Freude vor \u00dcbermut,", "tokens": ["Be\u00b7h\u00fc\u00b7te", "mich", "am", "Born", "der", "Freu\u00b7de", "vor", "\u00dc\u00b7ber\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Und wenn ich an mir selbst verzage, sei du mit mir!", "tokens": ["Und", "wenn", "ich", "an", "mir", "selbst", "ver\u00b7za\u00b7ge", ",", "sei", "du", "mit", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPER", "ADV", "VVFIN", "$,", "VAFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.7": {"text": "Gib deinen Geist zu meinem Liede, da\u00df rein es sei,", "tokens": ["Gib", "dei\u00b7nen", "Geist", "zu", "mei\u00b7nem", "Lie\u00b7de", ",", "da\u00df", "rein", "es", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,", "KOUS", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Und da\u00df kein Wort mich einst verklage, sei du mit mir!", "tokens": ["Und", "da\u00df", "kein", "Wort", "mich", "einst", "ver\u00b7kla\u00b7ge", ",", "sei", "du", "mit", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIAT", "NN", "PPER", "ADV", "VVFIN", "$,", "VAFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.9": {"text": "Dein Segen ist wie Tau der Reben; nichts kann ich selbst,", "tokens": ["Dein", "Se\u00b7gen", "ist", "wie", "Tau", "der", "Re\u00b7ben", ";", "nichts", "kann", "ich", "selbst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "NN", "ART", "NN", "$.", "PIS", "VMFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Doch da\u00df ich k\u00fchn das H\u00f6chste wage, sei du mit mir!", "tokens": ["Doch", "da\u00df", "ich", "k\u00fchn", "das", "H\u00f6chs\u00b7te", "wa\u00b7ge", ",", "sei", "du", "mit", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "ART", "ADJA", "VVFIN", "$,", "VAFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.11": {"text": "O du mein Trost, du meine St\u00e4rke, mein Sonnenlicht,", "tokens": ["O", "du", "mein", "Trost", ",", "du", "mei\u00b7ne", "St\u00e4r\u00b7ke", ",", "mein", "Son\u00b7nen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "$,", "PPER", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Bis an das Ende meiner Tage sei du mit mir!", "tokens": ["Bis", "an", "das", "En\u00b7de", "mei\u00b7ner", "Ta\u00b7ge", "sei", "du", "mit", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "PPOSAT", "NN", "VAFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}}}}