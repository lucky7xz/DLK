{"textgrid.poem.41064": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich habe dich gesehen,", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe dich gesehen,", "tokens": ["Ich", "ha\u00b7be", "dich", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein herzlich's Kind, so treu, so gut,", "tokens": ["Ein", "herz\u00b7lich's", "Kind", ",", "so", "treu", ",", "so", "gut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein warmes Herz, ein muntres Blut,", "tokens": ["Ein", "war\u00b7mes", "Herz", ",", "ein", "mun\u00b7tres", "Blut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie's V\u00f6glein in den H\u00f6hen.", "tokens": ["Wie's", "V\u00f6\u00b7glein", "in", "den", "H\u00f6\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich habe dich gesehen,", "tokens": ["Ich", "ha\u00b7be", "dich", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bild der Geduld im Todeskampf,", "tokens": ["Bild", "der", "Ge\u00b7duld", "im", "To\u00b7des\u00b7kampf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Das lichte Auge brach im Krampf,", "tokens": ["Das", "lich\u00b7te", "Au\u00b7ge", "brach", "im", "Krampf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Haupt durchzuckten Wehen.", "tokens": ["Das", "Haupt", "durch\u00b7zuck\u00b7ten", "We\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich habe dich gesehen,", "tokens": ["Ich", "ha\u00b7be", "dich", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Tot, lilienwei\u00df und lilienmild,", "tokens": ["Tot", ",", "li\u00b7li\u00b7en\u00b7wei\u00df", "und", "li\u00b7li\u00b7en\u00b7mild", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Ganz eines sel'gen Engels Bild,", "tokens": ["Ganz", "ei\u00b7nes", "sel'\u00b7gen", "En\u00b7gels", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich blieb anbetend stehen.", "tokens": ["Ich", "blieb", "an\u00b7be\u00b7tend", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O m\u00f6cht' ich wiedersehen", "tokens": ["O", "m\u00f6cht'", "ich", "wie\u00b7der\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dich, Engel, wenn mein Auge bricht,", "tokens": ["Dich", ",", "En\u00b7gel", ",", "wenn", "mein", "Au\u00b7ge", "bricht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Herschwebend aus des Himmels Licht,", "tokens": ["Her\u00b7schwe\u00b7bend", "aus", "des", "Him\u00b7mels", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Tod mir beizustehen! \u2013", "tokens": ["Im", "Tod", "mir", "bei\u00b7zu\u00b7ste\u00b7hen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}