{"textgrid.poem.29722": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "137.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Laxa hat ein sch\u00f6nes Fleisch, eines von dem weissen;", "tokens": ["La\u00b7xa", "hat", "ein", "sch\u00f6\u00b7nes", "Fleisch", ",", "ei\u00b7nes", "von", "dem", "weis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "APPR", "ART", "VVINF", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Doch man saget, da\u00df ihr drauff offte sitzen Schmeissen.", "tokens": ["Doch", "man", "sa\u00b7get", ",", "da\u00df", "ihr", "drauff", "off\u00b7te", "sit\u00b7zen", "Schmeis\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "KOUS", "PPER", "PAV", "ADJA", "ADJA", "NN", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}}}}