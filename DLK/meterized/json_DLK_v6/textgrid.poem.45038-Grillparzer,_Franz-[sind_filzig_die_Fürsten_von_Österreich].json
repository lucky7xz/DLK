{"textgrid.poem.45038": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[sind filzig die F\u00fcrsten von \u00d6sterreich]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sind filzig die F\u00fcrsten von \u00d6sterreich,", "tokens": ["Sind", "fil\u00b7zig", "die", "F\u00fcrs\u00b7ten", "von", "\u00d6s\u00b7ter\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Braucht ihr euch deshalb nicht zu gr\u00e4men;", "tokens": ["Braucht", "ihr", "euch", "des\u00b7halb", "nicht", "zu", "gr\u00e4\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PAV", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die guten Herren geben zwar nichts,", "tokens": ["Die", "gu\u00b7ten", "Her\u00b7ren", "ge\u00b7ben", "zwar", "nichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PIS", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Doch lassen sie alles sich nehmen.", "tokens": ["Doch", "las\u00b7sen", "sie", "al\u00b7les", "sich", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "PRF", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}