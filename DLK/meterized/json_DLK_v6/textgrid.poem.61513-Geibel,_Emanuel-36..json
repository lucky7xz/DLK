{"textgrid.poem.61513": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zweck? Das Kunstwerk hat nur einen,", "tokens": ["Zweck", "?", "Das", "Kunst\u00b7werk", "hat", "nur", "ei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VAFIN", "ADV", "ART", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Still im eignen Glanz zu ruhn;", "tokens": ["Still", "im", "eig\u00b7nen", "Glanz", "zu", "ruhn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber durch ihr blo\u00df Erscheinen", "tokens": ["A\u00b7ber", "durch", "ihr", "blo\u00df", "Er\u00b7schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mag die Sch\u00f6nheit Wunder tun.", "tokens": ["Mag", "die", "Sch\u00f6n\u00b7heit", "Wun\u00b7der", "tun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}