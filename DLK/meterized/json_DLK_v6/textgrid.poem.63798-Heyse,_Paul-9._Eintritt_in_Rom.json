{"textgrid.poem.63798": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "9. Eintritt in Rom", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dicht vor Ponte molle begr\u00fc\u00dft den nordischen Wandrer", "tokens": ["Dicht", "vor", "Pon\u00b7te", "mol\u00b7le", "be\u00b7gr\u00fc\u00dft", "den", "nor\u00b7di\u00b7schen", "Wand\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "NE", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Rechts der T\u00e4ufer und links Christus, zur Taufe geneigt.", "tokens": ["Rechts", "der", "T\u00e4u\u00b7fer", "und", "links", "Chris\u00b7tus", ",", "zur", "Tau\u00b7fe", "ge\u00b7neigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ADV", "NE", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "\u00dcber die Breite des Wegs spr\u00fcht hier die Gnade, zum Zeichen,", "tokens": ["\u00dc\u00b7ber", "die", "Brei\u00b7te", "des", "Wegs", "spr\u00fcht", "hier", "die", "Gna\u00b7de", ",", "zum", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "+--+--++--+--+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Da\u00df ein Tropfe des Heils auch die Verstocktesten trifft.", "tokens": ["Da\u00df", "ein", "Trop\u00b7fe", "des", "Heils", "auch", "die", "Ver\u00b7stock\u00b7tes\u00b7ten", "trifft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Aber der Teufel erfand das Dampfro\u00df. Heiden und Juden", "tokens": ["A\u00b7ber", "der", "Teu\u00b7fel", "er\u00b7fand", "das", "Dam\u00b7pfro\u00df", ".", "Hei\u00b7den", "und", "Ju\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$.", "NN", "KON", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Schleichen sich heillos jetzt hinten herum in die Stadt.", "tokens": ["Schlei\u00b7chen", "sich", "heil\u00b7los", "jetzt", "hin\u00b7ten", "he\u00b7rum", "in", "die", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ADV", "ADV", "APZR", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+--++-+", "measure": "trochaic.septa.relaxed"}}}}}