{"dta.poem.17829": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "72.  Die Fanlheit \u00fcberkomt nicht den  \n Himmel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Ach Fauler reg dich doch! wie bleibstu immer liegen?", "tokens": ["Ach", "Fau\u00b7ler", "reg", "dich", "doch", "!", "wie", "bleibs\u00b7tu", "im\u00b7mer", "lie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VVFIN", "PPER", "ADV", "$.", "PWAV", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcrwahr der Him\u0304el wird dir nicht ins Maul einfliegen.", "tokens": ["F\u00fcr\u00b7wahr", "der", "Him\u0304el", "wird", "dir", "nicht", "ins", "Maul", "ein\u00b7flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAFIN", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}}}}