{"dta.poem.3205": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "56.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie eine lange Nacht die Feldwacht auf dem Posten", "tokens": ["Wie", "ei\u00b7ne", "lan\u00b7ge", "Nacht", "die", "Feld\u00b7wacht", "auf", "dem", "Pos\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ausharret mit Geduld, bis roth es wird im Osten;", "tokens": ["Aus\u00b7har\u00b7ret", "mit", "Ge\u00b7duld", ",", "bis", "roth", "es", "wird", "im", "Os\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "KOUS", "ADJD", "PPER", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So vierzehn Tage hab' ich harrend hingebracht,", "tokens": ["So", "vier\u00b7zehn", "Ta\u00b7ge", "hab'", "ich", "har\u00b7rend", "hin\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die alle waren mir nur eine lange Nacht.", "tokens": ["Die", "al\u00b7le", "wa\u00b7ren", "mir", "nur", "ei\u00b7ne", "lan\u00b7ge", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nun ist, ich danke Gott, auch diese Nacht vor\u00fcber,", "tokens": ["Nun", "ist", ",", "ich", "dan\u00b7ke", "Gott", ",", "auch", "die\u00b7se", "Nacht", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PPER", "VVFIN", "NN", "$,", "ADV", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch reicht ihr Schatten weit noch in den Tag her\u00fcber.", "tokens": ["Doch", "reicht", "ihr", "Schat\u00b7ten", "weit", "noch", "in", "den", "Tag", "her\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADJD", "ADV", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ach da\u00df gemenget sind, wem sollen wir es klagen,", "tokens": ["Ach", "da\u00df", "ge\u00b7men\u00b7get", "sind", ",", "wem", "sol\u00b7len", "wir", "es", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "VVPP", "VAFIN", "$,", "PWS", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So lange N\u00e4chte zu so kurzen Lebenstagen!", "tokens": ["So", "lan\u00b7ge", "N\u00e4ch\u00b7te", "zu", "so", "kur\u00b7zen", "Le\u00b7bens\u00b7ta\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}