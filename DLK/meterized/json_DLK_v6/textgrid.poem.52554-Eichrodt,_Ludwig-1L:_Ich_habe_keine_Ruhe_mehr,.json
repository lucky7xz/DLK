{"textgrid.poem.52554": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich habe keine Ruhe mehr,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe keine Ruhe mehr,", "tokens": ["Ich", "ha\u00b7be", "kei\u00b7ne", "Ru\u00b7he", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es treibt mich hin zu dir,", "tokens": ["Es", "treibt", "mich", "hin", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O da\u00df ich Liebste bei dir w\u00e4r,", "tokens": ["O", "da\u00df", "ich", "Liebs\u00b7te", "bei", "dir", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "NN", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O da\u00df du w\u00e4rst bei mir!", "tokens": ["O", "da\u00df", "du", "w\u00e4rst", "bei", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nun hab ich lange ausgeharrt", "tokens": ["Nun", "hab", "ich", "lan\u00b7ge", "aus\u00b7ge\u00b7harrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im widrigen Geschick,", "tokens": ["Im", "wid\u00b7ri\u00b7gen", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und l\u00e4nger bleib und l\u00e4nger wart", "tokens": ["Und", "l\u00e4n\u00b7ger", "bleib", "und", "l\u00e4n\u00b7ger", "wart"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich keinen Augenblick.", "tokens": ["Ich", "kei\u00b7nen", "Au\u00b7gen\u00b7blick", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich komme schneller als der Wind,", "tokens": ["Ich", "kom\u00b7me", "schnel\u00b7ler", "als", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Rastloser Qual bewu\u00dft,", "tokens": ["Rast\u00b7lo\u00b7ser", "Qual", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und bis ich wieder Ruhe find,", "tokens": ["Und", "bis", "ich", "wie\u00b7der", "Ru\u00b7he", "find", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lieg ich an deiner Brust.", "tokens": ["Lieg", "ich", "an", "dei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}