{"dta.poem.12963": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Das vierte capitel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Njmm dich genau in acht! man mu\u00df nicht alles glauben,", "tokens": ["Njmm", "dich", "ge\u00b7nau", "in", "acht", "!", "man", "mu\u00df", "nicht", "al\u00b7les", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPR", "CARD", "$.", "PIS", "VMFIN", "PTKNEG", "PIS", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Was der gemeine ruff von unserm n\u00e4chsten spricht.", "tokens": ["Was", "der", "ge\u00b7mei\u00b7ne", "ruff", "von", "un\u00b7serm", "n\u00e4chs\u00b7ten", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die spinne setzt ihr gifft auch auf die reinsten trauben.", "tokens": ["Die", "spin\u00b7ne", "setzt", "ihr", "gifft", "auch", "auf", "die", "reins\u00b7ten", "trau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer kennt den leichten sinn des frechen volckes nicht?", "tokens": ["Wer", "kennt", "den", "leich\u00b7ten", "sinn", "des", "fre\u00b7chen", "vol\u00b7ckes", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum siehe dich wohl vor, und denck an GOttes willen,", "tokens": ["Drum", "sie\u00b7he", "dich", "wohl", "vor", ",", "und", "denck", "an", "Got\u00b7tes", "wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "KON", "VVIMP", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der dich zur lieb ermahnt, und nicht zum richter macht:", "tokens": ["Der", "dich", "zur", "lieb", "er\u00b7mahnt", ",", "und", "nicht", "zum", "rich\u00b7ter", "macht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "ADJD", "VVFIN", "$,", "KON", "PTKNEG", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn ob die wenigsten gleich sein gebot erf\u00fcllen,", "tokens": ["Denn", "ob", "die", "we\u00b7nigs\u00b7ten", "gleich", "sein", "ge\u00b7bot", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "VVFIN", "ADV", "VAINF", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So gieb doch, weil du kanst, der schwachheit gute nacht.", "tokens": ["So", "gieb", "doch", ",", "weil", "du", "kanst", ",", "der", "schwach\u00b7heit", "gu\u00b7te", "nacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "$,", "KOUS", "PPER", "VMFIN", "$,", "PRELS", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wo das nur schwachheit ist, wenn eine b\u00f6se sage", "tokens": ["Wo", "das", "nur", "schwach\u00b7heit", "ist", ",", "wenn", "ei\u00b7ne", "b\u00f6\u00b7se", "sa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PDS", "ADV", "ADJD", "VAFIN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Vom n\u00e4chsten, mehr bey uns, als eine gute gilt.", "tokens": ["Vom", "n\u00e4chs\u00b7ten", ",", "mehr", "bey", "uns", ",", "als", "ei\u00b7ne", "gu\u00b7te", "gilt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADV", "APPR", "PPER", "$,", "KOUS", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Leg alles, was du h\u00f6rst, bedachtsam auf die wage,", "tokens": ["Leg", "al\u00b7les", ",", "was", "du", "h\u00f6rst", ",", "be\u00b7dacht\u00b7sam", "auf", "die", "wa\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,", "ADJD", "APPR", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und wisse, da\u00df man offt die unschuld selber schilt.", "tokens": ["Und", "wis\u00b7se", ",", "da\u00df", "man", "offt", "die", "un\u00b7schuld", "sel\u00b7ber", "schilt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PIS", "ADV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Du must dem bruder nicht sein wort zu poltzen drehen.", "tokens": ["Du", "must", "dem", "bru\u00b7der", "nicht", "sein", "wort", "zu", "polt\u00b7zen", "dre\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "PPOSAT", "NN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der Liebe mantel deckt viel solche fehler zu.", "tokens": ["Der", "Lie\u00b7be", "man\u00b7tel", "deckt", "viel", "sol\u00b7che", "feh\u00b7ler", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Was jenem wiederfuhr, kan auch von dir geschehen,", "tokens": ["Was", "je\u00b7nem", "wie\u00b7der\u00b7fuhr", ",", "kan", "auch", "von", "dir", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "VVFIN", "$,", "VMFIN", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der angebshrne fall l\u00e4st uns doch wenig ruh.", "tokens": ["Der", "an\u00b7gebshr\u00b7ne", "fall", "l\u00e4st", "uns", "doch", "we\u00b7nig", "ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.17": {"text": "Die beste klugheit ist: Bed\u00e4chtig seyn und leben,", "tokens": ["Die", "bes\u00b7te", "klug\u00b7heit", "ist", ":", "Be\u00b7d\u00e4ch\u00b7tig", "seyn", "und", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$.", "ADJD", "VAINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und niemals allzusteif auf seinem kopffe stehn.", "tokens": ["Und", "nie\u00b7mals", "all\u00b7zu\u00b7steif", "auf", "sei\u00b7nem", "kopf\u00b7fe", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Man mu\u00df den leuten nicht durchgehends glauben geben,", "tokens": ["Man", "mu\u00df", "den", "leu\u00b7ten", "nicht", "durch\u00b7ge\u00b7hends", "glau\u00b7ben", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "PTKNEG", "ADV", "VVINF", "VVINF", "$,"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.20": {"text": "Noch mit den zeitungen durch alle stra\u00dfen gehn.", "tokens": ["Noch", "mit", "den", "zei\u00b7tun\u00b7gen", "durch", "al\u00b7le", "stra\u00b7\u00dfen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "++-+---+-+-+", "measure": "unknown.measure.hexa"}, "line.21": {"text": "Bek\u00fcmmre dich vielmehr um dich und deine s\u00fcnden,", "tokens": ["Be\u00b7k\u00fcmm\u00b7re", "dich", "viel\u00b7mehr", "um", "dich", "und", "dei\u00b7ne", "s\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "KON", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Als um des nachbars thun, weil du dir n\u00e4her bist;", "tokens": ["Als", "um", "des", "nach\u00b7bars", "thun", ",", "weil", "du", "dir", "n\u00e4\u00b7her", "bist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVINF", "$,", "KOUS", "PPER", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wiewol du wirst vor dich dein hertze nicht ergr\u00fcnden,", "tokens": ["Wie\u00b7wol", "du", "wirst", "vor", "dich", "dein", "hert\u00b7ze", "nicht", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "APPR", "PPER", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Des menschen eigenlieb\u2019 ist voll verborgner list.", "tokens": ["Des", "men\u00b7schen", "ei\u00b7gen\u00b7lieb'", "ist", "voll", "ver\u00b7borg\u00b7ner", "list", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.25": {"text": "Darum vertrau dich GOtt, und einem weisen freunde,", "tokens": ["Da\u00b7rum", "ver\u00b7trau", "dich", "Gott", ",", "und", "ei\u00b7nem", "wei\u00b7sen", "freun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "NN", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der ein gel\u00e4utert hertz und viel erfahrung hat.", "tokens": ["Der", "ein", "ge\u00b7l\u00e4u\u00b7tert", "hertz", "und", "viel", "er\u00b7fah\u00b7rung", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "VVPP", "NN", "KON", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Fleisch, welt und eigensinn fleuch, als die \u00e4rgsten feinde,", "tokens": ["Fleisch", ",", "welt", "und", "ei\u00b7gen\u00b7sinn", "fleuch", ",", "als", "die", "\u00e4rgs\u00b7ten", "fein\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "KON", "ADV", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und suche nirgends sonst, als nur bey frommen rath.", "tokens": ["Und", "su\u00b7che", "nir\u00b7gends", "sonst", ",", "als", "nur", "bey", "from\u00b7men", "rath", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$,", "KOUS", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wer also thut und lebt, der wandelt klug und sicher.", "tokens": ["Wer", "al\u00b7so", "thut", "und", "lebt", ",", "der", "wan\u00b7delt", "klug", "und", "si\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "KON", "VVFIN", "$,", "PRELS", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "O seele! wandle doch den wahren tugend-lauf.", "tokens": ["O", "see\u00b7le", "!", "wand\u00b7le", "doch", "den", "wah\u00b7ren", "tu\u00b7gen\u00b7dlauf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ein GOtt-gela\u00dfner geist bedarff nicht erst viel b\u00fccher,", "tokens": ["Ein", "Got\u00b7tge\u00b7la\u00df\u00b7ner", "geist", "be\u00b7darff", "nicht", "erst", "viel", "b\u00fc\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "PTKNEG", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Denn GOtt thut ihm das buch vollkommner weisheit auf.", "tokens": ["Denn", "Gott", "thut", "ihm", "das", "buch", "voll\u00b7komm\u00b7ner", "weis\u00b7heit", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}