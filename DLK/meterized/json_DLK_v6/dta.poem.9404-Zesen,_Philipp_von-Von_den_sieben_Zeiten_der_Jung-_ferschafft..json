{"dta.poem.9404": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Von den sieben Zeiten der Jung-  \n ferschafft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jugend", "tokens": ["Ju\u00b7gend"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "und tugend", "tokens": ["und", "tu\u00b7gend"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "steht artig beysam\u0303en/", "tokens": ["steht", "ar\u00b7tig", "bey\u00b7sam\u0303en", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVPP", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "jugend", "tokens": ["ju\u00b7gend"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "und tugend", "tokens": ["und", "tu\u00b7gend"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "erweitert das feld", "tokens": ["er\u00b7wei\u00b7tert", "das", "feld"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "und bringet auch geld:", "tokens": ["und", "brin\u00b7get", "auch", "geld", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "der Tugenden Flammen", "tokens": ["der", "Tu\u00b7gen\u00b7den", "Flam\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Sollen die Jungfern erwecken geschwind", "tokens": ["Sol\u00b7len", "die", "Jung\u00b7fern", "er\u00b7we\u00b7cken", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "VVINF", "VVPP"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.10": {"text": "Wenn sich das v\u00f6rderste f\u00fcnffte Jahr findt:", "tokens": ["Wenn", "sich", "das", "v\u00f6r\u00b7ders\u00b7te", "f\u00fcnff\u00b7te", "Jahr", "findt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Kommen noch andere f\u00fcnffe geschlichen/", "tokens": ["Kom\u00b7men", "noch", "an\u00b7de\u00b7re", "f\u00fcnf\u00b7fe", "ge\u00b7schli\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIS", "VVFIN", "VVPP", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.12": {"text": "Soll Sie sich schicken zur nahrung und k\u00fcchen", "tokens": ["Soll", "Sie", "sich", "schi\u00b7cken", "zur", "nah\u00b7rung", "und", "k\u00fc\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "ADJD", "APPRART", "NN", "KON", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "k\u00f6m\u0303t es zum dritten mahl wider herrein/", "tokens": ["k\u00f6m\u0303t", "es", "zum", "drit\u00b7ten", "mahl", "wi\u00b7der", "her\u00b7rein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "ADV", "APPR", "ADV", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.14": {"text": "dann will die Rose gebrochen schon seyn/", "tokens": ["dann", "will", "die", "Ro\u00b7se", "ge\u00b7bro\u00b7chen", "schon", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVPP", "ADV", "VAINF", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "dann seyn die lustigsten zeiten/", "tokens": ["dann", "seyn", "die", "lus\u00b7tigs\u00b7ten", "zei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "da mann soll Kr\u00e4ntze bereiten", "tokens": ["da", "mann", "soll", "Kr\u00e4nt\u00b7ze", "be\u00b7rei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VMFIN", "NN", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "da mann die Blumen eintr\u00e4gt", "tokens": ["da", "mann", "die", "Blu\u00b7men", "ein\u00b7tr\u00e4gt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ART", "NN", "VVFIN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.18": {"text": "die mann bi\u00dfhero geh\u00e4gt:", "tokens": ["die", "mann", "bi\u00df\u00b7he\u00b7ro", "ge\u00b7h\u00e4gt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Wenn aber das vierde", "tokens": ["Wenn", "a\u00b7ber", "das", "vier\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "mit voller begierde", "tokens": ["mit", "vol\u00b7ler", "be\u00b7gier\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.21": {"text": "sich zeiget/ als dann", "tokens": ["sich", "zei\u00b7get", "/", "als", "dann"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PRF", "VVFIN", "$(", "KOKOM", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.22": {"text": "sie schone der Man\u0303", "tokens": ["sie", "scho\u00b7ne", "der", "Ma\u00f1"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.23": {"text": "l\u00e4sst ruhen in armen/", "tokens": ["l\u00e4sst", "ru\u00b7hen", "in", "ar\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.24": {"text": "und st\u00fcndlich erwarmen/", "tokens": ["und", "st\u00fcnd\u00b7lich", "er\u00b7war\u00b7men", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.25": {"text": "k\u00f6m\u0303t es zum f\u00fcnften mal an;", "tokens": ["k\u00f6m\u0303t", "es", "zum", "f\u00fcnf\u00b7ten", "mal", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "ADV", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.26": {"text": "ist es fast \u00fcmb Sie gethan:", "tokens": ["ist", "es", "fast", "\u00fcmb", "Sie", "ge\u00b7than", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "k\u00f6m\u0303t es zum sechstenmal wider gegangen/", "tokens": ["k\u00f6m\u0303t", "es", "zum", "sechs\u00b7ten\u00b7mal", "wi\u00b7der", "ge\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADV", "APPR", "VVPP", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.28": {"text": "k\u00f6m\u0303t denn kein Witwer/ so ist es geschehn/", "tokens": ["k\u00f6m\u0303t", "denn", "kein", "Wit\u00b7wer", "/", "so", "ist", "es", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "$(", "ADV", "VAFIN", "PPER", "VVPP", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.29": {"text": "Alles in allem; ist sch\u00e4ndlich versehn;", "tokens": ["Al\u00b7les", "in", "al\u00b7lem", ";", "ist", "sch\u00e4nd\u00b7lich", "ver\u00b7sehn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PIS", "$.", "VAFIN", "ADJD", "VVINF", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.30": {"text": "Dann wird zu wasser Jhr gantzes verlangen.", "tokens": ["Dann", "wird", "zu", "was\u00b7ser", "Ihr", "gant\u00b7zes", "ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PRELS", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}