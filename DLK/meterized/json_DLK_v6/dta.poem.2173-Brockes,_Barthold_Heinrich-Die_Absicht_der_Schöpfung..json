{"dta.poem.2173": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Absicht der Sch\u00f6pfung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das wundervolle Buch der lehrenden Natur", "tokens": ["Das", "wun\u00b7der\u00b7vol\u00b7le", "Buch", "der", "leh\u00b7ren\u00b7den", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zeigt uns ein liebend- weis- allm\u00e4chtges Wunder-", "tokens": ["Zeigt", "uns", "ein", "lie\u00b7ben\u00b7d", "weis", "all\u00b7m\u00e4cht\u00b7ges", "Wun\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "TRUNC", "TRUNC", "ADJA", "TRUNC"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "wesen,", "tokens": ["we\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Und giebt es unserm Geist auf jedem Blatt zu lesen.", "tokens": ["Und", "giebt", "es", "un\u00b7serm", "Geist", "auf", "je\u00b7dem", "Blatt", "zu", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Von seinem Daseyn zeigt sich \u00fcberall die Spur,", "tokens": ["Von", "sei\u00b7nem", "Da\u00b7seyn", "zeigt", "sich", "\u00fc\u00b7be\u00b7rall", "die", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PRF", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den Seelen pr\u00e4get er von seinem wahren Seyn", "tokens": ["Den", "See\u00b7len", "pr\u00e4\u00b7get", "er", "von", "sei\u00b7nem", "wah\u00b7ren", "Seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den deutlichsten Begriff in dem Begriff selbst ein,", "tokens": ["Den", "deut\u00b7lichs\u00b7ten", "Be\u00b7griff", "in", "dem", "Be\u00b7griff", "selbst", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.8": {"text": "Da\u00df er, durch seine G\u00fct, Erbarmung, Huld und Lieben", "tokens": ["Da\u00df", "er", ",", "durch", "sei\u00b7ne", "G\u00fct", ",", "Er\u00b7bar\u00b7mung", ",", "Huld", "und", "Lie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "KON", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nur ganz allein getrieben,", "tokens": ["Nur", "ganz", "al\u00b7lein", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Gesch\u00f6pf\u2019 hervorgebracht, um ihnen wohlzuthun.", "tokens": ["Ge\u00b7sch\u00f6pf'", "her\u00b7vor\u00b7ge\u00b7bracht", ",", "um", "ih\u00b7nen", "wohl\u00b7zu\u00b7thun", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "KOUI", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Auf Liebe kann allein der wahre Grund beruhn,", "tokens": ["Auf", "Lie\u00b7be", "kann", "al\u00b7lein", "der", "wah\u00b7re", "Grund", "be\u00b7ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df etwas worden ist. Die Gottheit brauchte nicht,", "tokens": ["Da\u00df", "et\u00b7was", "wor\u00b7den", "ist", ".", "Die", "Got\u00b7theit", "brauch\u00b7te", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VAPP", "VAFIN", "$.", "ART", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Als aller Seligkeit selbst\u00e4ndig\u2019 ewge F\u00fclle,", "tokens": ["Als", "al\u00b7ler", "Se\u00b7lig\u00b7keit", "selb\u00b7st\u00e4n\u00b7dig'", "ew\u00b7ge", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Noch seliger zu seyn. Die\u00df zeiget uns das Licht", "tokens": ["Noch", "se\u00b7li\u00b7ger", "zu", "seyn", ".", "Die\u00df", "zei\u00b7get", "uns", "das", "Licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PTKZU", "VAINF", "$.", "PDS", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der denkenden Vernunft, auch, da\u00df des Sch\u00f6pfers Wille", "tokens": ["Der", "den\u00b7ken\u00b7den", "Ver\u00b7nunft", ",", "auch", ",", "da\u00df", "des", "Sch\u00f6p\u00b7fers", "Wil\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wohl nicht gewesen sey, Gesch\u00f6pfe zu erschaffen", "tokens": ["Wohl", "nicht", "ge\u00b7we\u00b7sen", "sey", ",", "Ge\u00b7sch\u00f6p\u00b7fe", "zu", "er\u00b7schaf\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "VAPP", "VAFIN", "$,", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Zu dieser Absicht blo\u00df, die Fehler zu bestrafen,", "tokens": ["Zu", "die\u00b7ser", "Ab\u00b7sicht", "blo\u00df", ",", "die", "Feh\u00b7ler", "zu", "be\u00b7stra\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und folglich, da\u00df er unser Wesen", "tokens": ["Und", "folg\u00b7lich", ",", "da\u00df", "er", "un\u00b7ser", "We\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Zu einem Gegenwurf von seiner Streng\u2019 erlesen,", "tokens": ["Zu", "ei\u00b7nem", "Ge\u00b7gen\u00b7wurf", "von", "sei\u00b7ner", "Streng'", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Gerechtigkeit genannt. So viel befinden wir,", "tokens": ["Ge\u00b7rech\u00b7tig\u00b7keit", "ge\u00b7nannt", ".", "So", "viel", "be\u00b7fin\u00b7den", "wir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$.", "ADV", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Da\u00df Gott in unsern Geist selbst den Begriff gesenket,", "tokens": ["Da\u00df", "Gott", "in", "un\u00b7sern", "Geist", "selbst", "den", "Be\u00b7griff", "ge\u00b7sen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da\u00df man an solchen Satz ohn Abscheu nicht gedenket.", "tokens": ["Da\u00df", "man", "an", "sol\u00b7chen", "Satz", "ohn", "Ab\u00b7scheu", "nicht", "ge\u00b7den\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PIAT", "NN", "APPR", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}