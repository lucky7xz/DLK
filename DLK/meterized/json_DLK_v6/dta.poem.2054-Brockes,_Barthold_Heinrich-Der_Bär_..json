{"dta.poem.2054": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der B\u00e4r .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus den dickverwachs\u2019nen H\u00f6len gehet dieses wilde", "tokens": ["Aus", "den", "dick\u00b7ver\u00b7wachs'\u00b7nen", "H\u00f6\u00b7len", "ge\u00b7het", "die\u00b7ses", "wil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PDAT", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Thier,", "tokens": ["Thier", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "In dem ganzen langen Winter, schlaf- und s\u00e4ugend,", "tokens": ["In", "dem", "gan\u00b7zen", "lan\u00b7gen", "Win\u00b7ter", ",", "schlaf", "und", "s\u00e4u\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,", "TRUNC", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "nicht herf\u00fcr.", "tokens": ["nicht", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Ist die\u00df nicht bewundernswerth,", "tokens": ["Ist", "die\u00df", "nicht", "be\u00b7wun\u00b7derns\u00b7werth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df sich solch gefr\u00e4\u00dfig Thier sonder Speise dennoch", "tokens": ["Da\u00df", "sich", "solch", "ge\u00b7fr\u00e4\u00b7\u00dfig", "Thier", "son\u00b7der", "Spei\u00b7se", "den\u00b7noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PIAT", "ADJA", "NN", "KON", "NN", "ADV"], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "n\u00e4hrt,", "tokens": ["n\u00e4hrt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Und dadurch, da er nicht raubet, sondern unaufh\u00f6rlich", "tokens": ["Und", "da\u00b7durch", ",", "da", "er", "nicht", "rau\u00b7bet", ",", "son\u00b7dern", "un\u00b7auf\u00b7h\u00f6r\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PAV", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,", "KON", "ADJD"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.9": {"text": "ruht,", "tokens": ["ruht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Fast die halbe Zeit des Lebens, so viel minder Schaden", "tokens": ["Fast", "die", "hal\u00b7be", "Zeit", "des", "Le\u00b7bens", ",", "so", "viel", "min\u00b7der", "Scha\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN", "$,", "ADV", "ADV", "ADV", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.11": {"text": "thut?", "tokens": ["thut", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.12": {"text": "Seine Gall\u2019, sein Fett, und Auge brauchet man in Arze-", "tokens": ["Sei\u00b7ne", "Gall'", ",", "sein", "Fett", ",", "und", "Au\u00b7ge", "brau\u00b7chet", "man", "in", "Ar\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "KON", "NN", "VVFIN", "PIS", "APPR", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.13": {"text": "neyen,", "tokens": ["ne\u00b7yen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.14": {"text": "Seine Pfoten und sein Kopf n\u00fctzen uns auf Gastereyen;", "tokens": ["Sei\u00b7ne", "Pfo\u00b7ten", "und", "sein", "Kopf", "n\u00fct\u00b7zen", "uns", "auf", "Gas\u00b7te\u00b7re\u00b7yen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "--+--+-+-+-+--+", "measure": "anapaest.di.plus"}, "line.15": {"text": "Auch der Jungen Fleisch ist e\u00dfbar. Dienet also auch", "tokens": ["Auch", "der", "Jun\u00b7gen", "Fleisch", "ist", "e\u00df\u00b7bar", ".", "Die\u00b7net", "al\u00b7so", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VAFIN", "ADJD", "$.", "VVFIN", "ADV", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.16": {"text": "der B\u00e4r,", "tokens": ["der", "B\u00e4r", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.17": {"text": "Seiner Wildheit ungeachtet, uns zum Nutzen, Gott", "tokens": ["Sei\u00b7ner", "Wild\u00b7heit", "un\u00b7ge\u00b7ach\u00b7tet", ",", "uns", "zum", "Nut\u00b7zen", ",", "Gott"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "APPO", "$,", "PPER", "APPRART", "NN", "$,", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.18": {"text": "zur Ehr.", "tokens": ["zur", "Ehr", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}}}}