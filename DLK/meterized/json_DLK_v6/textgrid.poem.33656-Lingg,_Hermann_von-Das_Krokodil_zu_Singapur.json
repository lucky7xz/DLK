{"textgrid.poem.33656": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Das Krokodil zu Singapur", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im heil'gen Teich zu Singapur", "tokens": ["Im", "heil'\u00b7gen", "Teich", "zu", "Sin\u00b7ga\u00b7pur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da liegt ein altes Krokodil", "tokens": ["Da", "liegt", "ein", "al\u00b7tes", "Kro\u00b7ko\u00b7dil"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von \u00e4u\u00dferst gr\u00e4mlicher Natur", "tokens": ["Von", "\u00e4u\u00b7\u00dferst", "gr\u00e4m\u00b7li\u00b7cher", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kaut an einem Lotosstil.", "tokens": ["Und", "kaut", "an", "ei\u00b7nem", "Lo\u00b7toss\u00b7til", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es ist ganz alt und v\u00f6llig blind,", "tokens": ["Es", "ist", "ganz", "alt", "und", "v\u00f6l\u00b7lig", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wenn es einmal friert des Nachts,", "tokens": ["Und", "wenn", "es", "ein\u00b7mal", "friert", "des", "Nachts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "ART", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So weint es wie ein kleines Kind,", "tokens": ["So", "weint", "es", "wie", "ein", "klei\u00b7nes", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch wenn ein sch\u00f6ner Tag ist, lacht's.", "tokens": ["Doch", "wenn", "ein", "sch\u00f6\u00b7ner", "Tag", "ist", ",", "lacht'", "s."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "abbreviation"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VAFIN", "$,", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}