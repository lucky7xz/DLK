{"dta.poem.1347": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Thau-Wetter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Der Winter weicht/ der Schnee zuweicht/ die Rinnen thr\u00e4-", "tokens": ["Der", "Win\u00b7ter", "weicht", "/", "der", "Schnee", "zu\u00b7weicht", "/", "die", "Rin\u00b7nen", "thr\u00e4"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "NN", "VVFIN", "$(", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "nen/", "tokens": ["nen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Der hart-gefrorne Strom/ das Ei\u00df/ beginnt zu g\u00e4hnen:", "tokens": ["Der", "har\u00b7tge\u00b7fror\u00b7ne", "Strom", "/", "das", "Ei\u00df", "/", "be\u00b7ginnt", "zu", "g\u00e4h\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "$(", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach la\u00df/ o harter Mensch/ auch die verstockten Sinnen", "tokens": ["Ach", "la\u00df", "/", "o", "har\u00b7ter", "Mensch", "/", "auch", "die", "ver\u00b7stock\u00b7ten", "Sin\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "$(", "FM", "ADJA", "NN", "$(", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit weicher Busse Thr\u00e4nen rinnen!", "tokens": ["Mit", "wei\u00b7cher", "Bus\u00b7se", "Thr\u00e4\u00b7nen", "rin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Man sp\u00fcrt die warme Lufft der lauhen Mittags-Winde/", "tokens": ["Man", "sp\u00fcrt", "die", "war\u00b7me", "Lufft", "der", "lau\u00b7hen", "Mit\u00b7tags\u00b7Win\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie rauschen in der H\u00f6h/ und streichen durch die Gr\u00fcnde.", "tokens": ["Sie", "rau\u00b7schen", "in", "der", "H\u00f6h", "/", "und", "strei\u00b7chen", "durch", "die", "Gr\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$(", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Zorn des H\u00f6chsten raucht; ach la\u00df ihn in dich dringen/", "tokens": ["Der", "Zorn", "des", "H\u00f6chs\u00b7ten", "raucht", ";", "ach", "la\u00df", "ihn", "in", "dich", "drin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$.", "XY", "VVIMP", "PPER", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und dein befel\u00dftes Hertze zwingen!", "tokens": ["Und", "dein", "be\u00b7fel\u00df\u00b7tes", "Hert\u00b7ze", "zwin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die Erde schwimmt voll Koth/ du voller Missethaten:", "tokens": ["Die", "Er\u00b7de", "schwimmt", "voll", "Koth", "/", "du", "vol\u00b7ler", "Mis\u00b7se\u00b7tha\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "NN", "$(", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ach saubere dich bald/ dieweil dir noch zu rathen!", "tokens": ["Ach", "sau\u00b7be\u00b7re", "dich", "bald", "/", "die\u00b7weil", "dir", "noch", "zu", "ra\u00b7then", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "ADV", "$(", "KOUS", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bereite Geist und Sinn/ auff da\u00df du kanst im Lentzen", "tokens": ["Be\u00b7rei\u00b7te", "Geist", "und", "Sinn", "/", "auff", "da\u00df", "du", "kanst", "im", "Lent\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN", "$(", "APPR", "KOUS", "PPER", "VMFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "F\u00fcr GOTT mit neuer Zierde gl\u00e4ntzen.", "tokens": ["F\u00fcr", "GoTT", "mit", "neu\u00b7er", "Zier\u00b7de", "gl\u00e4nt\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}