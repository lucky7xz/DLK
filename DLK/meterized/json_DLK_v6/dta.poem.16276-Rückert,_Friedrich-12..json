{"dta.poem.16276": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du den lauten Streit vom P\u00f6bel stillen willst,", "tokens": ["Wenn", "du", "den", "lau\u00b7ten", "Streit", "vom", "P\u00f6\u00b7bel", "stil\u00b7len", "willst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich sage dir, wie du am sichersten ihn stillst.", "tokens": ["Ich", "sa\u00b7ge", "dir", ",", "wie", "du", "am", "si\u00b7chers\u00b7ten", "ihn", "stillst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "APPRART", "ADJA", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Erst la\u00df die Streitenden recht an einander toben,", "tokens": ["Erst", "la\u00df", "die", "Strei\u00b7ten\u00b7den", "recht", "an", "ein\u00b7an\u00b7der", "to\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "ADJD", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis sich zur Heiserkeit die Wuth hat ausgeschnoben.", "tokens": ["Bis", "sich", "zur", "Hei\u00b7ser\u00b7keit", "die", "Wuth", "hat", "aus\u00b7ge\u00b7schno\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPRART", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "K\u00f6nnen sie nicht mehr schrein, dann werden sie dich h\u00f6ren,", "tokens": ["K\u00f6n\u00b7nen", "sie", "nicht", "mehr", "schrein", ",", "dann", "wer\u00b7den", "sie", "dich", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$,", "ADV", "VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Dann schlage Frieden vor, sie werden ihn beschw\u00f6ren.", "tokens": ["Dann", "schla\u00b7ge", "Frie\u00b7den", "vor", ",", "sie", "wer\u00b7den", "ihn", "be\u00b7schw\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKVZ", "$,", "PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}