{"textgrid.poem.52880": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott, einen Strahl aus deinen Wolken sende", "tokens": ["Gott", ",", "ei\u00b7nen", "Strahl", "aus", "dei\u00b7nen", "Wol\u00b7ken", "sen\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf dieser Vorstadt schmerzenreiches Dach!", "tokens": ["Auf", "die\u00b7ser", "Vor\u00b7stadt", "schmer\u00b7zen\u00b7rei\u00b7ches", "Dach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hier ringt ein Mensch mit seinem schweren Ende,", "tokens": ["Hier", "ringt", "ein", "Mensch", "mit", "sei\u00b7nem", "schwe\u00b7ren", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sei gn\u00e4dig, hilf der armen Seele nach!", "tokens": ["Sei", "gn\u00e4\u00b7dig", ",", "hilf", "der", "ar\u00b7men", "See\u00b7le", "nach", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "VVIMP", "ART", "ADJA", "NN", "APPR", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zieh aus der Kinder fesselndem Gewimmer,", "tokens": ["Zieh", "aus", "der", "Kin\u00b7der", "fes\u00b7seln\u00b7dem", "Ge\u00b7wim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Zieh aus des Weibs Umschlingung ihn zu dir.", "tokens": ["Zieh", "aus", "des", "Weibs", "Um\u00b7schlin\u00b7gung", "ihn", "zu", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "PPER", "APPR", "PPER", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Herr, s\u00e4ume nicht! Er duldet ja noch immer,", "tokens": ["Herr", ",", "s\u00e4u\u00b7me", "nicht", "!", "Er", "dul\u00b7det", "ja", "noch", "im\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKNEG", "$.", "PPER", "VVFIN", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Herr, schl\u00e4fst du auch? O wache, Herr, mit mir!", "tokens": ["Herr", ",", "schl\u00e4fst", "du", "auch", "?", "O", "wa\u00b7che", ",", "Herr", ",", "mit", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "$.", "NE", "ADJA", "$,", "NN", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Am niedren Fenster schleich ich sacht vor\u00fcber,", "tokens": ["Am", "nied\u00b7ren", "Fens\u00b7ter", "schleich", "ich", "sacht", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch glimmt der Lampe Docht, wer l\u00f6scht sie aus?", "tokens": ["Noch", "glimmt", "der", "Lam\u00b7pe", "Docht", ",", "wer", "l\u00f6scht", "sie", "aus", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,", "PWS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie schimmert durch die Laden, st\u00fcndlich tr\u00fcber,", "tokens": ["Sie", "schim\u00b7mert", "durch", "die", "La\u00b7den", ",", "st\u00fcnd\u00b7lich", "tr\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und K\u00e4uzlein flattern um das Sterbehaus.", "tokens": ["Und", "K\u00e4uz\u00b7lein", "flat\u00b7tern", "um", "das", "Ster\u00b7be\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Hu! Fort von dieser schauervollen Schwelle,", "tokens": ["Hu", "!", "Fort", "von", "die\u00b7ser", "schau\u00b7er\u00b7vol\u00b7len", "Schwel\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hier tut ein Andrer W\u00e4chterdienst als ich.", "tokens": ["Hier", "tut", "ein", "A\u00b7ndrer", "W\u00e4ch\u00b7ter\u00b7dienst", "als", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "KOUS", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dort lagert er, der schreckliche Geselle,", "tokens": ["Dort", "la\u00b7gert", "er", ",", "der", "schreck\u00b7li\u00b7che", "Ge\u00b7sel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und kauert lauernd vor die T\u00fcre sich.", "tokens": ["Und", "kau\u00b7ert", "lau\u00b7ernd", "vor", "die", "T\u00fc\u00b7re", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Er malt ein Kreuz, ein wei\u00dfes, an die Schalter,", "tokens": ["Er", "malt", "ein", "Kreuz", ",", "ein", "wei\u00b7\u00dfes", ",", "an", "die", "Schal\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er winkt, er klopft ... O W\u00fcrger, halte an!", "tokens": ["Er", "winkt", ",", "er", "klopft", "...", "O", "W\u00fcr\u00b7ger", ",", "hal\u00b7te", "an", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$(", "NE", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es ist geschehn. Hab Dank, du alter, kalter", "tokens": ["Es", "ist", "ge\u00b7schehn", ".", "Hab", "Dank", ",", "du", "al\u00b7ter", ",", "kal\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "NN", "NN", "$,", "PPER", "ADJA", "$,", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nachtw\u00e4chtersmann, du hast dein Werk getan!", "tokens": ["Nacht\u00b7w\u00e4ch\u00b7ters\u00b7mann", ",", "du", "hast", "dein", "Werk", "ge\u00b7tan", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}