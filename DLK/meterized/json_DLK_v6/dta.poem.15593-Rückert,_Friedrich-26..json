{"dta.poem.15593": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit Unrecht r\u00fchmst du dich, in freiem Haus zu walten,", "tokens": ["Mit", "Un\u00b7recht", "r\u00fchmst", "du", "dich", ",", "in", "frei\u00b7em", "Haus", "zu", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PRF", "$,", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du die drinnen mu\u00dft mit Zwang zur\u00fcck behalten.", "tokens": ["Wenn", "du", "die", "drin\u00b7nen", "mu\u00dft", "mit", "Zwang", "zu\u00b7r\u00fcck", "be\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VMFIN", "APPR", "NN", "PTKVZ", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Den, der freiwillig nicht will bleiben, la\u00df ihn ziehn;", "tokens": ["Den", ",", "der", "frei\u00b7wil\u00b7lig", "nicht", "will", "blei\u00b7ben", ",", "la\u00df", "ihn", "ziehn", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADJD", "PTKNEG", "VMFIN", "VVINF", "$,", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sonst wird dein freies Haus zum Zwangstall nur f\u00fcr ihn.", "tokens": ["Sonst", "wird", "dein", "frei\u00b7es", "Haus", "zum", "Zwang\u00b7stall", "nur", "f\u00fcr", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du sprichst: Er \u00fcbernahm in diesem Hause Pflichten,", "tokens": ["Du", "sprichst", ":", "Er", "\u00fc\u00b7bern\u00b7ahm", "in", "die\u00b7sem", "Hau\u00b7se", "Pflich\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und eh' er abziehn darf, mu\u00df er die erst verrichten.", "tokens": ["Und", "eh'", "er", "ab\u00b7ziehn", "darf", ",", "mu\u00df", "er", "die", "erst", "ver\u00b7rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVINF", "VMFIN", "$,", "VMFIN", "PPER", "ART", "ADV", "VVINF", "$."], "meter": "-+-+--+-++-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Nein! Pflichten hat er nur, solang er bleibt im Haus;", "tokens": ["Nein", "!", "Pflich\u00b7ten", "hat", "er", "nur", ",", "so\u00b7lang", "er", "bleibt", "im", "Haus", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "NN", "VAFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sobald er ausziehn will, ist die Verpflichtung aus.", "tokens": ["So\u00b7bald", "er", "aus\u00b7ziehn", "will", ",", "ist", "die", "Ver\u00b7pflich\u00b7tung", "aus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$,", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}