{"dta.poem.21075": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Erwin an Ellwina.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sahe sie in ihrer Sch\u00f6nheit Strahle,               ", "tokens": ["Ich", "sa\u00b7he", "sie", "in", "ih\u00b7rer", "Sch\u00f6n\u00b7heit", "Strah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich sah die sch\u00f6nste Blume unsrer Flur,", "tokens": ["Ich", "sah", "die", "sch\u00f6ns\u00b7te", "Blu\u00b7me", "uns\u00b7rer", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dich sah ich, Lieblingin, im seebesp\u00fchlten Thale,", "tokens": ["Dich", "sah", "ich", ",", "Lieb\u00b7lin\u00b7gin", ",", "im", "see\u00b7be\u00b7sp\u00fchl\u00b7ten", "Tha\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Im hehren Tempel der Natur.", "tokens": ["Im", "heh\u00b7ren", "Tem\u00b7pel", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Im hellen Reihen der Anbeterinnen", "tokens": ["Im", "hel\u00b7len", "Rei\u00b7hen", "der", "An\u00b7be\u00b7te\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sah ich dich sitzen sonder Schmuck und Zier;", "tokens": ["Sah", "ich", "dich", "sit\u00b7zen", "son\u00b7der", "Schmuck", "und", "Zier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "VVINF", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und st\u00fcrmisch schlug mein Herz. Es schwindelte", "tokens": ["Und", "st\u00fcr\u00b7misch", "schlug", "mein", "Herz", ".", "Es", "schwin\u00b7del\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "den Sinnen,", "tokens": ["den", "Sin\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Und meine Kraft versagte mir.", "tokens": ["Und", "mei\u00b7ne", "Kraft", "ver\u00b7sag\u00b7te", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nur sch\u00fcchtern wagt' ich es, den ", "tokens": ["Nur", "sch\u00fcch\u00b7tern", "wagt'", "ich", "es", ",", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVINF", "VVFIN", "PPER", "PPER", "$,", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu preisen, sch\u00fcchtern nur die ewige,", "tokens": ["Zu", "prei\u00b7sen", ",", "sch\u00fcch\u00b7tern", "nur", "die", "e\u00b7wi\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "VVFIN", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die g\u00f6ttliche Natur. In meinem Innern brannten", "tokens": ["Die", "g\u00f6tt\u00b7li\u00b7che", "Na\u00b7tur", ".", "In", "mei\u00b7nem", "In\u00b7nern", "brann\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Flammen f\u00fcr die Sterbliche.", "tokens": ["Die", "Flam\u00b7men", "f\u00fcr", "die", "Sterb\u00b7li\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.4": {"line.1": {"text": "Und ist nicht Liebe heilig, wie die Tugend?", "tokens": ["Und", "ist", "nicht", "Lie\u00b7be", "hei\u00b7lig", ",", "wie", "die", "Tu\u00b7gend", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "NN", "ADJD", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist sie nicht s\u00fcss, wie die Unsterblichkeit?", "tokens": ["Ist", "sie", "nicht", "s\u00fcss", ",", "wie", "die", "U\u00b7nsterb\u00b7lich\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "$,", "PWAV", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Was frommet ohne sie der G\u00f6tter ewge Jugend,", "tokens": ["Was", "from\u00b7met", "oh\u00b7ne", "sie", "der", "G\u00f6t\u00b7ter", "ew\u00b7ge", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was aller Himmel Seligkeit?", "tokens": ["Was", "al\u00b7ler", "Him\u00b7mel", "Se\u00b7lig\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "F\u00fcr Einen Tag, verlebt an deiner Seiten,", "tokens": ["F\u00fcr", "Ei\u00b7nen", "Tag", ",", "ver\u00b7lebt", "an", "dei\u00b7ner", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr Eine Nacht, durchwacht an deiner Brust,", "tokens": ["F\u00fcr", "Ei\u00b7ne", "Nacht", ",", "durch\u00b7wacht", "an", "dei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "W\u00e4r' eine Welt mir feil voll schaler Seligkeiten,", "tokens": ["W\u00e4r'", "ei\u00b7ne", "Welt", "mir", "feil", "voll", "scha\u00b7ler", "Se\u00b7lig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "ADJD", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und eines Himmels matte Lust.", "tokens": ["Und", "ei\u00b7nes", "Him\u00b7mels", "mat\u00b7te", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "F\u00fcr Einen Kranz, von deiner Hand gewunden,", "tokens": ["F\u00fcr", "Ei\u00b7nen", "Kranz", ",", "von", "dei\u00b7ner", "Hand", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr Eine Schleife, deiner Liebe Pfand,", "tokens": ["F\u00fcr", "Ei\u00b7ne", "Schlei\u00b7fe", ",", "dei\u00b7ner", "Lie\u00b7be", "Pfand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "G\u00e4b' ich den Lorbeer hin, den mir in sch\u00f6nern", "tokens": ["G\u00e4b'", "ich", "den", "Lor\u00b7beer", "hin", ",", "den", "mir", "in", "sch\u00f6\u00b7nern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "PTKVZ", "$,", "PRELS", "PPER", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Stunden", "tokens": ["Stun\u00b7den"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Die Muse um die Schl\u00e4fe wand.", "tokens": ["Die", "Mu\u00b7se", "um", "die", "Schl\u00e4\u00b7fe", "wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wer liebt, wie ich, die Dichter und die Weisen!", "tokens": ["Wer", "liebt", ",", "wie", "ich", ",", "die", "Dich\u00b7ter", "und", "die", "Wei\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "PPER", "$,", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch ach, f\u00fcr Eine Zeile deiner Hand", "tokens": ["Doch", "ach", ",", "f\u00fcr", "Ei\u00b7ne", "Zei\u00b7le", "dei\u00b7ner", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "XY", "$,", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vertauscht' ich ohne Schmerz die B\u00fccher meiner", "tokens": ["Ver\u00b7tauscht'", "ich", "oh\u00b7ne", "Schmerz", "die", "B\u00fc\u00b7cher", "mei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weisen,", "tokens": ["Wei\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Und aller Hochgelahrtheit Tand.", "tokens": ["Und", "al\u00b7ler", "Hoch\u00b7ge\u00b7lahr\u00b7theit", "Tand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Dein trauter Gruss durchstr\u00f6mt mich mit Ent-", "tokens": ["Dein", "trau\u00b7ter", "Gruss", "durch\u00b7str\u00f6mt", "mich", "mit", "Ent"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "APPR", "TRUNC"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "z\u00fccken,", "tokens": ["z\u00fc\u00b7cken", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Wie keines Dichters lieblichster Gesang.", "tokens": ["Wie", "kei\u00b7nes", "Dich\u00b7ters", "lieb\u00b7lichs\u00b7ter", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dein Lob, dein s\u00fcsses Lob, dein lohnend H\u00e4nde-", "tokens": ["Dein", "Lob", ",", "dein", "s\u00fcs\u00b7ses", "Lob", ",", "dein", "loh\u00b7nend", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "dr\u00fccken", "tokens": ["dr\u00fc\u00b7cken"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Entflammt mich, wie der Nachwelt Dank.", "tokens": ["Ent\u00b7flammt", "mich", ",", "wie", "der", "Nach\u00b7welt", "Dank", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Dein Wink, dein Nick, o wunderbares Wesen,", "tokens": ["Dein", "Wink", ",", "dein", "Nick", ",", "o", "wun\u00b7der\u00b7ba\u00b7res", "We\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein L\u00e4cheln, dein bedeutungsreicher Blick,", "tokens": ["Dein", "L\u00e4\u00b7cheln", ",", "dein", "be\u00b7deu\u00b7tungs\u00b7rei\u00b7cher", "Blick", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Macht den Verzweifelnden von jeder Qual genesen,", "tokens": ["Macht", "den", "Ver\u00b7zwei\u00b7feln\u00b7den", "von", "je\u00b7der", "Qual", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Und ruft vom Orkus ihn zur\u00fcck.", "tokens": ["Und", "ruft", "vom", "Or\u00b7kus", "ihn", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "O d\u00fcrft' ich einmal nur dich fest umstricken!", "tokens": ["O", "d\u00fcrft'", "ich", "ein\u00b7mal", "nur", "dich", "fest", "um\u00b7stri\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADV", "ADV", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "D\u00fcrft' ich nur einmal von der Sehnsucht Drang", "tokens": ["D\u00fcrft'", "ich", "nur", "ein\u00b7mal", "von", "der", "Sehn\u00b7sucht", "Drang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dahingerissen dich an meinen Busen dr\u00fccken", "tokens": ["Da\u00b7hin\u00b7ge\u00b7ris\u00b7sen", "dich", "an", "mei\u00b7nen", "Bu\u00b7sen", "dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PRF", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zwei selige Minuten lang!", "tokens": ["Zwei", "se\u00b7li\u00b7ge", "Mi\u00b7nu\u00b7ten", "lang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}