{"textgrid.poem.60395": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Singt ihr in eurem Freudenliede:", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Singt ihr in eurem Freudenliede:", "tokens": ["Singt", "ihr", "in", "eu\u00b7rem", "Freu\u00b7den\u00b7lie\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der heitre Mond am Himmel lacht,", "tokens": ["Der", "heit\u00b7re", "Mond", "am", "Him\u00b7mel", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ihm entstrahlt ein s\u00fc\u00dfer Friede \u2013", "tokens": ["Und", "ihm", "ent\u00b7strahlt", "ein", "s\u00fc\u00b7\u00dfer", "Frie\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So habt ihr nie den Mond bedacht.", "tokens": ["So", "habt", "ihr", "nie", "den", "Mond", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Seht ihr ihn dort her\u00fcberschweben,", "tokens": ["Seht", "ihr", "ihn", "dort", "her\u00b7\u00fc\u00b7bersc\u00b7hwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bleich, ohne Wasser, ohne Luft;", "tokens": ["Bleich", ",", "oh\u00b7ne", "Was\u00b7ser", ",", "oh\u00b7ne", "Luft", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUI", "NN", "$,", "KOUI", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er zieht mit ausgestorbnem Leben,", "tokens": ["Er", "zieht", "mit", "aus\u00b7ge\u00b7storb\u00b7nem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Totengr\u00e4ber samt der Gruft.", "tokens": ["Ein", "To\u00b7ten\u00b7gr\u00e4\u00b7ber", "samt", "der", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dort dringt der Mond mit seinem Schimmer", "tokens": ["Dort", "dringt", "der", "Mond", "mit", "sei\u00b7nem", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Still dem Nachtwandler ins Gemach", "tokens": ["Still", "dem", "Nacht\u00b7wand\u00b7ler", "ins", "Ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "APPRART", "NN"], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Und winkt und lockt aus Bett und Zimmer,", "tokens": ["Und", "winkt", "und", "lockt", "aus", "Bett", "und", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Schl\u00e4fer folgt ihm auf das Dach", "tokens": ["Der", "Schl\u00e4\u00b7fer", "folgt", "ihm", "auf", "das", "Dach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und huscht, geschlo\u00dfner Augenlider,", "tokens": ["Und", "huscht", ",", "ge\u00b7schlo\u00df\u00b7ner", "Au\u00b7gen\u00b7li\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hin, her, des Daches steilsten Bug,", "tokens": ["Hin", ",", "her", ",", "des", "Da\u00b7ches", "steils\u00b7ten", "Bug", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKVZ", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Als hielte geistiges Gefieder", "tokens": ["Als", "hiel\u00b7te", "geis\u00b7ti\u00b7ges", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Enthoben ihn dem Erdenzug.", "tokens": ["Ent\u00b7ho\u00b7ben", "ihn", "dem", "Er\u00b7den\u00b7zug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Mond zieht traurig durch die Sph\u00e4ren,", "tokens": ["Der", "Mond", "zieht", "trau\u00b7rig", "durch", "die", "Sph\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denn all die Seinen ruhn im Grab;", "tokens": ["Denn", "all", "die", "Sei\u00b7nen", "ruhn", "im", "Grab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ART", "PPOSS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum wischt er sich die hellen Z\u00e4hren", "tokens": ["Drum", "wischt", "er", "sich", "die", "hel\u00b7len", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bei Nacht an unsern Blumen ab.", "tokens": ["Bei", "Nacht", "an", "un\u00b7sern", "Blu\u00b7men", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Darum durchschleicht er Fenster, T\u00fcren,", "tokens": ["Da\u00b7rum", "durch\u00b7schleicht", "er", "Fens\u00b7ter", ",", "T\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Diebessohlen leis und lind,", "tokens": ["Auf", "Die\u00b7bes\u00b7soh\u00b7len", "leis", "und", "lind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Erde heimlich zu entf\u00fchren", "tokens": ["Der", "Er\u00b7de", "heim\u00b7lich", "zu", "ent\u00b7f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Schlafe dies und jenes Kind.", "tokens": ["Im", "Schla\u00b7fe", "dies", "und", "je\u00b7nes", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDS", "KON", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Den Schl\u00e4fern um den Leib zu schlingen", "tokens": ["Den", "Schl\u00e4\u00b7fern", "um", "den", "Leib", "zu", "schlin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sucht er sein feines Silbernetz", "tokens": ["Sucht", "er", "sein", "fei\u00b7nes", "Sil\u00b7ber\u00b7netz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sie zu sich hinaufzuschwingen;", "tokens": ["Und", "sie", "zu", "sich", "hin\u00b7auf\u00b7zu\u00b7schwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch seine F\u00e4den rei\u00dfen stets.", "tokens": ["Doch", "sei\u00b7ne", "F\u00e4\u00b7den", "rei\u00b7\u00dfen", "stets", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und ewig wird es ihm mi\u00dfgl\u00fccken,", "tokens": ["Und", "e\u00b7wig", "wird", "es", "ihm", "mi\u00df\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu stehlen sich ein Spielgesind,", "tokens": ["Zu", "steh\u00b7len", "sich", "ein", "Spiel\u00b7ge\u00b7sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In seine W\u00fcste zu entr\u00fccken", "tokens": ["In", "sei\u00b7ne", "W\u00fcs\u00b7te", "zu", "ent\u00b7r\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein lebenswarmes Erdenkind.", "tokens": ["Ein", "le\u00b7bens\u00b7war\u00b7mes", "Er\u00b7den\u00b7kind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Der Mond wohl auch die Schlummerlosen", "tokens": ["Der", "Mond", "wohl", "auch", "die", "Schlum\u00b7mer\u00b7lo\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Erde zu entlocken sucht;", "tokens": ["Der", "Er\u00b7de", "zu", "ent\u00b7lo\u00b7cken", "sucht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er will mit schw\u00e4rmerischem Kosen", "tokens": ["Er", "will", "mit", "schw\u00e4r\u00b7me\u00b7ri\u00b7schem", "Ko\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bereden sie zu fr\u00fcher Flucht.", "tokens": ["Be\u00b7re\u00b7den", "sie", "zu", "fr\u00fc\u00b7her", "Flucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKA", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Oft wenn ich ging durch Wald und Wiesen,", "tokens": ["Oft", "wenn", "ich", "ging", "durch", "Wald", "und", "Wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Log mir der Mondenschein so lang,", "tokens": ["Log", "mir", "der", "Mon\u00b7den\u00b7schein", "so", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sei auf Erden nur verwiesen,", "tokens": ["Ich", "sei", "auf", "Er\u00b7den", "nur", "ver\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis ich hinweg mich sehnte bang.", "tokens": ["Bis", "ich", "hin\u00b7weg", "mich", "sehn\u00b7te", "bang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Weil er uns nicht vermag zu stehlen,", "tokens": ["Weil", "er", "uns", "nicht", "ver\u00b7mag", "zu", "steh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht wachend, nicht in Schlafesruh,", "tokens": ["Nicht", "wa\u00b7chend", ",", "nicht", "in", "Schla\u00b7fes\u00b7ruh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schickt er mit Blicken, stieren, scheelen,", "tokens": ["Schickt", "er", "mit", "Bli\u00b7cken", ",", "stie\u00b7ren", ",", "schee\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Der Erde Todesw\u00fcnsche zu.", "tokens": ["Der", "Er\u00b7de", "To\u00b7des\u00b7w\u00fcn\u00b7sche", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Als Knabe schon konnt ich nicht schauen", "tokens": ["Als", "Kna\u00b7be", "schon", "konnt", "ich", "nicht", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Zum stillen blassen Mond empor,", "tokens": ["Zum", "stil\u00b7len", "blas\u00b7sen", "Mond", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df nicht ein wunderliches Grauen", "tokens": ["Da\u00df", "nicht", "ein", "wun\u00b7der\u00b7li\u00b7ches", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir heimlich das Gebein durchfror.", "tokens": ["Mir", "heim\u00b7lich", "das", "Ge\u00b7bein", "durch\u00b7fror", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Nirgends, auf Wald und Feld und Stra\u00dfen,", "tokens": ["Nir\u00b7gends", ",", "auf", "Wald", "und", "Feld", "und", "Stra\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Frohlockt so hell des Mondes Licht,", "tokens": ["Froh\u00b7lockt", "so", "hell", "des", "Mon\u00b7des", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie auf dem Kirchhof, wo verlassen", "tokens": ["Wie", "auf", "dem", "Kirch\u00b7hof", ",", "wo", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "$,", "PWAV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein armes Herz vor Leide bricht.", "tokens": ["Ein", "ar\u00b7mes", "Herz", "vor", "Lei\u00b7de", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Ja, Gr\u00e4ber sind f\u00fcr ihn die Stelle,", "tokens": ["Ja", ",", "Gr\u00e4\u00b7ber", "sind", "f\u00fcr", "ihn", "die", "Stel\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "VAFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und an Ruinen Dorngestr\u00e4uch;", "tokens": ["Und", "an", "Ru\u00b7i\u00b7nen", "Dorn\u00b7ge\u00b7str\u00e4uch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Doch vor des Mondes schlim