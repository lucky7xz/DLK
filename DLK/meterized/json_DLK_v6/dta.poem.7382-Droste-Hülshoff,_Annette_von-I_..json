{"dta.poem.7382": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "I .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Der Kapit\u00e4n steht an der Spiere,", "tokens": ["Der", "Ka\u00b7pi\u00b7t\u00e4n", "steht", "an", "der", "Spie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Fernrohr in gebr\u00e4unter Hand,", "tokens": ["Das", "Fern\u00b7rohr", "in", "ge\u00b7br\u00e4un\u00b7ter", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem schwarzgelockten Passagiere", "tokens": ["Dem", "schwarz\u00b7ge\u00b7lock\u00b7ten", "Pas\u00b7sa\u00b7gie\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hat er den R\u00fccken zugewandt.", "tokens": ["Hat", "er", "den", "R\u00fc\u00b7cken", "zu\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nach einem Wolkenstreif in Sinnen", "tokens": ["Nach", "ei\u00b7nem", "Wol\u00b7ken\u00b7streif", "in", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Beiden wie zwei Pfeiler sehn,", "tokens": ["Die", "Bei\u00b7den", "wie", "zwei", "Pfei\u00b7ler", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "KOKOM", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Fremde spricht: \u201ewas braut da drinnen?\u201c", "tokens": ["Der", "Frem\u00b7de", "spricht", ":", "\u201e", "was", "braut", "da", "drin\u00b7nen", "?", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PWS", "VVFIN", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "\u201eder Teufel,\u201c brummt der Kapit\u00e4n.", "tokens": ["\u201e", "der", "Teu\u00b7fel", ",", "\u201c", "brummt", "der", "Ka\u00b7pi\u00b7t\u00e4n", "."], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "$(", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da hebt von morschen Balkens Tr\u00fcmmer", "tokens": ["Da", "hebt", "von", "mor\u00b7schen", "Bal\u00b7kens", "Tr\u00fcm\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Kranker seine feuchte Stirn,", "tokens": ["Ein", "Kran\u00b7ker", "sei\u00b7ne", "feuch\u00b7te", "Stirn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Aethers Blau, der See Geflimmer,", "tokens": ["Des", "A\u00b7e\u00b7thers", "Blau", ",", "der", "See", "Ge\u00b7flim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ach, Alles qu\u00e4lt sein fiebernd Hirn!", "tokens": ["Ach", ",", "Al\u00b7les", "qu\u00e4lt", "sein", "fie\u00b7bernd", "Hirn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIS", "VVFIN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er l\u00e4\u00dft die Blicke, schwer und d\u00fcster,", "tokens": ["Er", "l\u00e4\u00dft", "die", "Bli\u00b7cke", ",", "schwer", "und", "d\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Entl\u00e4ngs dem harten Pf\u00fchle gehn,", "tokens": ["Ent\u00b7l\u00e4ngs", "dem", "har\u00b7ten", "Pf\u00fch\u00b7le", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die eingegrabnen Worte liest er:", "tokens": ["Die", "ein\u00b7ge\u00b7grab\u00b7nen", "Wor\u00b7te", "liest", "er", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "\u201e", "tokens": ["\u201e"], "token_info": ["punct"], "pos": ["$("]}}, "stanza.3": {"line.1": {"text": "Die Wolke steigt, zur Mittagsstunde", "tokens": ["Die", "Wol\u00b7ke", "steigt", ",", "zur", "Mit\u00b7tags\u00b7stun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Schiff \u00e4chzt auf der Wellen H\u00f6hn,", "tokens": ["Das", "Schiff", "\u00e4chzt", "auf", "der", "Wel\u00b7len", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gezisch, Geheul aus w\u00fcstem Grunde,", "tokens": ["Ge\u00b7zisch", ",", "Ge\u00b7heul", "aus", "w\u00fcs\u00b7tem", "Grun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Bohlen weichen mit Gest\u00f6hn.", "tokens": ["Die", "Boh\u00b7len", "wei\u00b7chen", "mit", "Ge\u00b7st\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u201ejesus, Marie! wir sind verloren!\u201c", "tokens": ["\u201e", "je\u00b7sus", ",", "Ma\u00b7rie", "!", "wir", "sind", "ver\u00b7lo\u00b7ren", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NE", "$,", "NE", "$.", "PPER", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vom Mast geschleudert der Matros',", "tokens": ["Vom", "Mast", "ge\u00b7schleu\u00b7dert", "der", "Ma\u00b7tros'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein dumpfer Krach in Aller Ohren,", "tokens": ["Ein", "dum\u00b7pfer", "Krach", "in", "Al\u00b7ler", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und langsam l\u00f6st der Bau sich los.", "tokens": ["Und", "lang\u00b7sam", "l\u00f6st", "der", "Bau", "sich", "los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Noch liegt der Kranke am Verdecke,", "tokens": ["Noch", "liegt", "der", "Kran\u00b7ke", "am", "Ver\u00b7de\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Um seinen Balken fest geklemmt,", "tokens": ["Um", "sei\u00b7nen", "Bal\u00b7ken", "fest", "ge\u00b7klemmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da k\u00f6mmt die Fluth, und eine Strecke", "tokens": ["Da", "k\u00f6mmt", "die", "Fluth", ",", "und", "ei\u00b7ne", "Stre\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wird er in's w\u00fcste Meer geschwemmt.", "tokens": ["Wird", "er", "in's", "w\u00fcs\u00b7te", "Meer", "ge\u00b7schwemmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was nicht gel\u00e4ng' der Kr\u00e4fte Sporne,", "tokens": ["Was", "nicht", "ge\u00b7l\u00e4ng'", "der", "Kr\u00e4f\u00b7te", "Spor\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das leistet ihm der starre Krampf,", "tokens": ["Das", "leis\u00b7tet", "ihm", "der", "star\u00b7re", "Krampf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wie ein Narwall mit dem Horne", "tokens": ["Und", "wie", "ein", "Nar\u00b7wall", "mit", "dem", "Hor\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Schie\u00dft fort er durch der Wellen Dampf.", "tokens": ["Schie\u00dft", "fort", "er", "durch", "der", "Wel\u00b7len", "Dampf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie lange so? er wei\u00df es nimmer,", "tokens": ["Wie", "lan\u00b7ge", "so", "?", "er", "wei\u00df", "es", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "$.", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dann trifft ein Stral des Auges Ball,", "tokens": ["Dann", "trifft", "ein", "Stral", "des", "Au\u00b7ges", "Ball", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und langsam schwimmt er mit der Tr\u00fcmmer", "tokens": ["Und", "lang\u00b7sam", "schwimmt", "er", "mit", "der", "Tr\u00fcm\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf \u00f6dem glitzerndem Kristall.", "tokens": ["Auf", "\u00f6\u00b7dem", "glit\u00b7zern\u00b7dem", "Kris\u00b7tall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+++", "measure": "unknown.measure.penta"}, "line.5": {"text": "Das Schiff! \u2014 die Mannschaft! \u2014 sie versanken.", "tokens": ["Das", "Schiff", "!", "die", "Mann\u00b7schaft", "!", "sie", "ver\u00b7san\u00b7ken", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "ART", "NN", "$.", "$(", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Doch nein, dort auf der Wasserbahn,", "tokens": ["Doch", "nein", ",", "dort", "auf", "der", "Was\u00b7ser\u00b7bahn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dort sieht den Passagier er schwanken", "tokens": ["Dort", "sieht", "den", "Pas\u00b7sa\u00b7gier", "er", "schwan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In einer Kiste morschem Kahn.", "tokens": ["In", "ei\u00b7ner", "Kis\u00b7te", "mor\u00b7schem", "Kahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Armselge Lade! sie wird sinken,", "tokens": ["Arm\u00b7sel\u00b7ge", "La\u00b7de", "!", "sie", "wird", "sin\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VAFIN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Er strengt die heisre Stimme an:", "tokens": ["Er", "strengt", "die", "heis\u00b7re", "Stim\u00b7me", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201enur grade! Freund, du dr\u00fcckst zur Linken!\u201c", "tokens": ["\u201e", "nur", "gra\u00b7de", "!", "Freund", ",", "du", "dr\u00fcckst", "zur", "Lin\u00b7ken", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADV", "$.", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und immer n\u00e4her schwankt's heran,", "tokens": ["Und", "im\u00b7mer", "n\u00e4\u00b7her", "schwankt's", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und immer n\u00e4her treibt die Tr\u00fcmmer,", "tokens": ["Und", "im\u00b7mer", "n\u00e4\u00b7her", "treibt", "die", "Tr\u00fcm\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie ein verwehtes M\u00f6vennest;", "tokens": ["Wie", "ein", "ver\u00b7weh\u00b7tes", "M\u00f6\u00b7ven\u00b7nest", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.8": {"text": "\u201emich d\u00fcnkt ich sehe Land im West!\u201c", "tokens": ["\u201e", "mich", "d\u00fcnkt", "ich", "se\u00b7he", "Land", "im", "West", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "VVFIN", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nun r\u00fchren sich der F\u00e4hren Ende,", "tokens": ["Nun", "r\u00fch\u00b7ren", "sich", "der", "F\u00e4h\u00b7ren", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er sieht des fremden Auges Blitz,", "tokens": ["Er", "sieht", "des", "frem\u00b7den", "Au\u00b7ges", "Blitz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da pl\u00f6tzlich f\u00fchlt er starke H\u00e4nde,", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "f\u00fchlt", "er", "star\u00b7ke", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fchlt w\u00fcthend sich gezerrt vom Sitz.", "tokens": ["F\u00fchlt", "w\u00fct\u00b7hend", "sich", "ge\u00b7zerrt", "vom", "Sitz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PRF", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u201ebarmherzigkeit! ich kann nicht k\u00e4mpfen.\u201c", "tokens": ["\u201e", "barm\u00b7her\u00b7zig\u00b7keit", "!", "ich", "kann", "nicht", "k\u00e4mp\u00b7fen", ".", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$.", "PPER", "VMFIN", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er klammert dort, er klemmt sich hier;", "tokens": ["Er", "klam\u00b7mert", "dort", ",", "er", "klemmt", "sich", "hier", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "PRF", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein heisrer Schrei, den Wellen d\u00e4mpfen,", "tokens": ["Ein", "heis\u00b7rer", "Schrei", ",", "den", "Wel\u00b7len", "d\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Am Balken schwimmt der Passagier.", "tokens": ["Am", "Bal\u00b7ken", "schwimmt", "der", "Pas\u00b7sa\u00b7gier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Dann hat er kr\u00e4ftig sich geschwungen,", "tokens": ["Dann", "hat", "er", "kr\u00e4f\u00b7tig", "sich", "ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und schaukelt durch das \u00f6de Blau,", "tokens": ["Und", "schau\u00b7kelt", "durch", "das", "\u00f6\u00b7de", "Blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er sieht das Land wie D\u00e4mmerungen", "tokens": ["Er", "sieht", "das", "Land", "wie", "D\u00e4m\u00b7me\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOKOM", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Enttauchen und zergehn in Grau.", "tokens": ["Ent\u00b7tau\u00b7chen", "und", "zer\u00b7gehn", "in", "Grau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch lange ist er so geschwommen,", "tokens": ["Noch", "lan\u00b7ge", "ist", "er", "so", "ge\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Umflattert von der M\u00f6ve Schrei,", "tokens": ["Um\u00b7flat\u00b7tert", "von", "der", "M\u00f6\u00b7ve", "Schrei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dann hat ein Schiff ihn aufgenommen,", "tokens": ["Dann", "hat", "ein", "Schiff", "ihn", "auf\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Viktoria! nun ist er frei!", "tokens": ["Vik\u00b7to\u00b7ria", "!", "nun", "ist", "er", "frei", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}