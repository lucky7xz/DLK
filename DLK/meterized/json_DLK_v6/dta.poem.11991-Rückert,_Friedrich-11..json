{"dta.poem.11991": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn dich ein Uebel trifft, so denk: es ist ein kleines,", "tokens": ["Wenn", "dich", "ein", "Ue\u00b7bel", "trifft", ",", "so", "denk", ":", "es", "ist", "ein", "klei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "$.", "PPER", "VAFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Opfer das du bringst f\u00fcr Gro\u00dfes Allgemeines.", "tokens": ["Das", "Op\u00b7fer", "das", "du", "bringst", "f\u00fcr", "Gro\u00b7\u00dfes", "All\u00b7ge\u00b7mei\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn so gewoben ist der Welt Zusammenhang,", "tokens": ["Denn", "so", "ge\u00b7wo\u00b7ben", "ist", "der", "Welt", "Zu\u00b7sam\u00b7men\u00b7hang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geordnet so des Tongewirrs Zusammenklang.", "tokens": ["Ge\u00b7ord\u00b7net", "so", "des", "Ton\u00b7ge\u00b7wirrs", "Zu\u00b7sam\u00b7men\u00b7klang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Webe w\u00e4chst nur, wo der Faden wird geschlagen;", "tokens": ["Die", "We\u00b7be", "w\u00e4chst", "nur", ",", "wo", "der", "Fa\u00b7den", "wird", "ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PWAV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der volle Wollaut schwillt, wo einzle Fl\u00f6ten klagen.", "tokens": ["Der", "vol\u00b7le", "Wol\u00b7laut", "schwillt", ",", "wo", "einz\u00b7le", "Fl\u00f6\u00b7ten", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "PWAV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Heil, wenn ein Faden nur, ein Fl\u00f6tenton du bist", "tokens": ["Heil", ",", "wenn", "ein", "Fa\u00b7den", "nur", ",", "ein", "Fl\u00f6\u00b7ten\u00b7ton", "du", "bist"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "ADV", "$,", "ART", "NN", "PPER", "VAFIN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Im gro\u00dfen Harmoniegeweb das ewig ist.", "tokens": ["Im", "gro\u00b7\u00dfen", "Har\u00b7mo\u00b7nie\u00b7ge\u00b7web", "das", "e\u00b7wig", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}