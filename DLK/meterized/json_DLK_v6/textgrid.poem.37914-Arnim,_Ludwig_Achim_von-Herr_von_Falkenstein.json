{"textgrid.poem.37914": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Herr von Falkenstein", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es reit der Herr von Falkenstein,", "tokens": ["Es", "reit", "der", "Herr", "von", "Fal\u00b7ken\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl \u00fcber ein' breite Haide.", "tokens": ["Wohl", "\u00fc\u00b7ber", "ein'", "brei\u00b7te", "Hai\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Was sieht er an dem Wege stehn?", "tokens": ["Was", "sieht", "er", "an", "dem", "We\u00b7ge", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein M\u00e4del mit weissem Kleide.", "tokens": ["Ein", "M\u00e4\u00b7del", "mit", "weis\u00b7sem", "Klei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbwohin, wohinaus du sch\u00f6ne Magd?", "tokens": ["\u00bb", "wo\u00b7hin", ",", "wo\u00b7hi\u00b7naus", "du", "sch\u00f6\u00b7ne", "Magd", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$,", "PWAV", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was machet ihr hier alleine?", "tokens": ["Was", "ma\u00b7chet", "ihr", "hier", "al\u00b7lei\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wollt ihr die Nacht mein Schlafbule seyn,", "tokens": ["Wollt", "ihr", "die", "Nacht", "mein", "Schlaf\u00b7bu\u00b7le", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "So reitet ihr mit mir heime.\u00ab", "tokens": ["So", "rei\u00b7tet", "ihr", "mit", "mir", "hei\u00b7me", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bbmit euch heimreiten, das thu' ich nicht,", "tokens": ["\u00bb", "mit", "euch", "heim\u00b7rei\u00b7ten", ",", "das", "thu'", "ich", "nicht", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-++--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Kann euch doch nicht erkennen.\u00ab", "tokens": ["Kann", "euch", "doch", "nicht", "er\u00b7ken\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbich bin der Herr von Falkenstein,", "tokens": ["\u00bb", "ich", "bin", "der", "Herr", "von", "Fal\u00b7ken\u00b7stein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und thu mich selber nennen.\u00ab", "tokens": ["Und", "thu", "mich", "sel\u00b7ber", "nen\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbseyd ihr der Herr von Falkenstein,", "tokens": ["\u00bb", "seyd", "ihr", "der", "Herr", "von", "Fal\u00b7ken\u00b7stein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Derselbe edle Herre,", "tokens": ["Der\u00b7sel\u00b7be", "ed\u00b7le", "Her\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So will ich euch bitten um'n Gefang'n mein,", "tokens": ["So", "will", "ich", "euch", "bit\u00b7ten", "um'n", "Ge\u00b7fang'n", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVFIN", "APPR", "NN", "PPOSAT", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den will ich haben zur Ehe.\u00ab \u2013", "tokens": ["Den", "will", "ich", "ha\u00b7ben", "zur", "E\u00b7he", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "VMFIN", "PPER", "VAFIN", "APPRART", "NN", "$.", "$(", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbden Gefangnen mein, den geb ich dir nicht,", "tokens": ["\u00bb", "den", "Ge\u00b7fang\u00b7nen", "mein", ",", "den", "geb", "ich", "dir", "nicht", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "PPOSAT", "$,", "ART", "VVFIN", "PPER", "PPER", "PTKNEG", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Im Thurn mu\u00df er vertrauren.", "tokens": ["Im", "Thurn", "mu\u00df", "er", "ver\u00b7trau\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu Falkenstein steht ein tiefer Thurn,", "tokens": ["Zu", "Fal\u00b7ken\u00b7stein", "steht", "ein", "tie\u00b7fer", "Thurn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wohl zwischen zwo hohen Mauren.\u00ab \u2013", "tokens": ["Wohl", "zwi\u00b7schen", "zwo", "ho\u00b7hen", "Mau\u00b7ren", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "APPR", "CARD", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbsteht zu Falkenstein ein tiefer Thurn,", "tokens": ["\u00bb", "steht", "zu", "Fal\u00b7ken\u00b7stein", "ein", "tie\u00b7fer", "Thurn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wohl zwischen zwei hohen Mauren,", "tokens": ["Wohl", "zwi\u00b7schen", "zwei", "ho\u00b7hen", "Mau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So will ich an den Mauren stehn,", "tokens": ["So", "will", "ich", "an", "den", "Mau\u00b7ren", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und will ihm helfen trauren.\u00ab \u2013", "tokens": ["Und", "will", "ihm", "hel\u00b7fen", "trau\u00b7ren", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVINF", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sie ging den Thurm wohl um und wieder um:", "tokens": ["Sie", "ging", "den", "Thurm", "wohl", "um", "und", "wie\u00b7der", "um", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "KON", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbfeinslieb, bist du darinnen?", "tokens": ["\u00bb", "feins\u00b7lieb", ",", "bist", "du", "da\u00b7rin\u00b7nen", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn ich dich nicht sehen kann,", "tokens": ["Und", "wenn", "ich", "dich", "nicht", "se\u00b7hen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So komm ich von meinen Sinnen.\u00ab", "tokens": ["So", "komm", "ich", "von", "mei\u00b7nen", "Sin\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Sie ging den Thurm wohl um und wieder um,", "tokens": ["Sie", "ging", "den", "Thurm", "wohl", "um", "und", "wie\u00b7der", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "KON", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Thurm wollt sie auf schlie\u00dfen:", "tokens": ["Den", "Thurm", "wollt", "sie", "auf", "schlie\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "APPR", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbund wenn die Nacht ein Jahr lang w\u00e4r;", "tokens": ["\u00bb", "und", "wenn", "die", "Nacht", "ein", "Jahr", "lang", "w\u00e4r", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "KOUS", "ART", "NN", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Keine Stund th\u00e4t mich verdrie\u00dfen!", "tokens": ["Kei\u00b7ne", "Stund", "th\u00e4t", "mich", "ver\u00b7drie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ei d\u00fcrft ich scharfe Messer tragen,", "tokens": ["Ei", "d\u00fcrft", "ich", "schar\u00b7fe", "Mes\u00b7ser", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie unsers Herrn sein Knechte,", "tokens": ["Wie", "un\u00b7sers", "Herrn", "sein", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich th\u00e4t mit'm Herrn von Falkenstein,", "tokens": ["Ich", "th\u00e4t", "mit'm", "Herrn", "von", "Fal\u00b7ken\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um meinen Herzliebsten fechten!\u00ab \u2013", "tokens": ["Um", "mei\u00b7nen", "Herz\u00b7liebs\u00b7ten", "fech\u00b7ten", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVINF", "$.", "$(", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "\u00bbmit einer Jungfrau fecht ich nicht,", "tokens": ["\u00bb", "mit", "ei\u00b7ner", "Jung\u00b7frau", "fecht", "ich", "nicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das w\u00e4r mir immer ein Schande!", "tokens": ["Das", "w\u00e4r", "mir", "im\u00b7mer", "ein", "Schan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich will dir deinen Gefangnen geben;", "tokens": ["Ich", "will", "dir", "dei\u00b7nen", "Ge\u00b7fang\u00b7nen", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zieh mit ihm aus dem Lande!\u00ab \u2013", "tokens": ["Zieh", "mit", "ihm", "aus", "dem", "Lan\u00b7de", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "ART", "NN", "$.", "$(", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.11": {"line.1": {"text": "\u00bbwohl aus dem Lande, da zieh ich nicht,", "tokens": ["\u00bb", "wohl", "aus", "dem", "Lan\u00b7de", ",", "da", "zieh", "ich", "nicht", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Hab niemand was gestohlen:", "tokens": ["Hab", "nie\u00b7mand", "was", "ge\u00b7stoh\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "PWS", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn ich was hab liegen lahn,", "tokens": ["Und", "wenn", "ich", "was", "hab", "lie\u00b7gen", "lahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "VAFIN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So darf ichs wieder holen.\u00ab", "tokens": ["So", "darf", "ichs", "wie\u00b7der", "ho\u00b7len", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}