{"textgrid.poem.41541": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am zweiten Sonntage nach Ostern", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein guter Hirt l\u00e4\u00dft seine Schafe nimmer!", "tokens": ["Ein", "gu\u00b7ter", "Hirt", "l\u00e4\u00dft", "sei\u00b7ne", "Scha\u00b7fe", "nim\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O wehe, Hirt! den ein verk\u00fcmmert Lamm", "tokens": ["O", "we\u00b7he", ",", "Hirt", "!", "den", "ein", "ver\u00b7k\u00fcm\u00b7mert", "Lamm"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "$,", "NN", "$.", "ART", "ART", "VVPP", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Einst klagend nennen wird mit Angstgewimmer,", "tokens": ["Einst", "kla\u00b7gend", "nen\u00b7nen", "wird", "mit", "Angst\u00b7ge\u00b7wim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein blutend wundes, eins voll Wust und Schlamm.", "tokens": ["Ein", "blu\u00b7tend", "wun\u00b7des", ",", "eins", "voll", "Wust", "und", "Schlamm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "$,", "PIS", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was willst du sagen? Schweig!", "tokens": ["Was", "willst", "du", "sa\u00b7gen", "?", "Schweig", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$.", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Dein Wort ist tot, der Stirne Zeichen Kains gleich.", "tokens": ["Dein", "Wort", "ist", "tot", ",", "der", "Stir\u00b7ne", "Zei\u00b7chen", "Kains", "gleich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "NN", "NE", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Weh, F\u00fcrsten euch! die ihr des Volkes Seelen", "tokens": ["Weh", ",", "F\u00fcrs\u00b7ten", "euch", "!", "die", "ihr", "des", "Vol\u00b7kes", "See\u00b7len"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "PPER", "$.", "PRELS", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gen Vorteil w\u00e4gt und irdisches Gedeihn.", "tokens": ["Gen", "Vor\u00b7teil", "w\u00e4gt", "und", "ir\u00b7di\u00b7sches", "Ge\u00b7deihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Weh, Eltern! denen Kindes gl\u00e4nzend Fehlen", "tokens": ["Weh", ",", "El\u00b7tern", "!", "de\u00b7nen", "Kin\u00b7des", "gl\u00e4n\u00b7zend", "Feh\u00b7len"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$.", "PDS", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weit lieber ist als Einfalt sonder Schein.", "tokens": ["Weit", "lie\u00b7ber", "ist", "als", "Ein\u00b7falt", "son\u00b7der", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VAFIN", "KOKOM", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ihr warbt euch das Gericht;", "tokens": ["Ihr", "warbt", "euch", "das", "Ge\u00b7richt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sprecht nicht von Ehre! eure kennt man dr\u00fcben nicht!", "tokens": ["Sprecht", "nicht", "von", "Eh\u00b7re", "!", "eu\u00b7re", "kennt", "man", "dr\u00fc\u00b7ben", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "NN", "$.", "PPOSAT", "VVFIN", "PIS", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Hausv\u00e4ter, wehe! die ein dienend Wesen", "tokens": ["Haus\u00b7v\u00e4\u00b7ter", ",", "we\u00b7he", "!", "die", "ein", "die\u00b7nend", "We\u00b7sen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "$.", "ART", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur an sich nahmen wie gedingten Leib;", "tokens": ["Nur", "an", "sich", "nah\u00b7men", "wie", "ge\u00b7ding\u00b7ten", "Leib", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "VVFIN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Unw\u00fcrdig seid zu Hirten ihr erlesen", "tokens": ["Un\u00b7w\u00fcr\u00b7dig", "seid", "zu", "Hir\u00b7ten", "ihr", "er\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "APPR", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Freundlosem Manne, unberatnem Weib.", "tokens": ["Freund\u00b7lo\u00b7sem", "Man\u00b7ne", ",", "un\u00b7be\u00b7rat\u00b7nem", "Weib", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Habt ihr gewu\u00dft und schwiegt;", "tokens": ["Habt", "ihr", "ge\u00b7wu\u00dft", "und", "schwiegt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Seht, jeder Flecken brandig an der Hand euch liegt!", "tokens": ["Seht", ",", "je\u00b7der", "Fle\u00b7cken", "bran\u00b7dig", "an", "der", "Hand", "euch", "liegt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PIAT", "NN", "ADJD", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und wehe, wehe allen! deren H\u00e4nden", "tokens": ["Und", "we\u00b7he", ",", "we\u00b7he", "al\u00b7len", "!", "de\u00b7ren", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "$,", "ADJD", "PIAT", "$.", "PRELAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ward anvertraut ein \u00fcberschwenglich Gut.", "tokens": ["Ward", "an\u00b7ver\u00b7traut", "ein", "\u00fc\u00b7bersc\u00b7hweng\u00b7lich", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Weh, Lehrer euch! die Herzen, leicht zu wenden,", "tokens": ["Weh", ",", "Leh\u00b7rer", "euch", "!", "die", "Her\u00b7zen", ",", "leicht", "zu", "wen\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "PPER", "$.", "ART", "NN", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vergiftet habt mit Hohn und \u00dcbermut.", "tokens": ["Ver\u00b7gif\u00b7tet", "habt", "mit", "Hohn", "und", "\u00dc\u00b7ber\u00b7mut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Pfund' euch vorgestreckt,", "tokens": ["Die", "Pfund'", "euch", "vor\u00b7ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Nicht wohl vergrubt ihr sie, habt sie mit Rost befleckt.", "tokens": ["Nicht", "wohl", "ver\u00b7grubt", "ihr", "sie", ",", "habt", "sie", "mit", "Rost", "be\u00b7fleckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "PPER", "PPER", "$,", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Doch bist du frei? darfst du so k\u00fchn denn sprechen", "tokens": ["Doch", "bist", "du", "frei", "?", "darfst", "du", "so", "k\u00fchn", "denn", "spre\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PTKVZ", "$.", "VMFIN", "PPER", "ADV", "ADJD", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Bannwort \u00fcber tausend Menschen aus?", "tokens": ["Das", "Bann\u00b7wort", "\u00fc\u00b7ber", "tau\u00b7send", "Men\u00b7schen", "aus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wem Kron' und Macht, wem Haus und Hof gebrechen,", "tokens": ["Wem", "Kron'", "und", "Macht", ",", "wem", "Haus", "und", "Hof", "ge\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "KON", "NN", "$,", "PWS", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schlie\u00dft ihn die Pflicht von ihren Schranken aus?", "tokens": ["Schlie\u00dft", "ihn", "die", "Pflicht", "von", "ih\u00b7ren", "Schran\u00b7ken", "aus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Denk nach! schwer ist die Frag';", "tokens": ["Denk", "nach", "!", "schwer", "ist", "die", "Frag'", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$.", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Um dein' und fremde Seele gilt's, denk nach!", "tokens": ["Um", "dein'", "und", "frem\u00b7de", "See\u00b7le", "gilt's", ",", "denk", "nach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "KON", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wenn Kinderohr an deinen Lippen h\u00e4nget,", "tokens": ["Wenn", "Kin\u00b7der\u00b7ohr", "an", "dei\u00b7nen", "Lip\u00b7pen", "h\u00e4n\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn Kinderblick in deinen Augen liest,", "tokens": ["Wenn", "Kin\u00b7der\u00b7blick", "in", "dei\u00b7nen", "Au\u00b7gen", "liest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn jedes kecke Wort, das vor sich dr\u00e4nget,", "tokens": ["Wenn", "je\u00b7des", "ke\u00b7cke", "Wort", ",", "das", "vor", "sich", "dr\u00e4n\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "$,", "PRELS", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie gl\u00fchend Blei in zarte Ohren flie\u00dft:", "tokens": ["Wie", "gl\u00fc\u00b7hend", "Blei", "in", "zar\u00b7te", "Oh\u00b7ren", "flie\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bist du dann nicht der Hirt?", "tokens": ["Bist", "du", "dann", "nicht", "der", "Hirt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ist dein die Schuld nicht, wenn das arme Lamm verirrt?", "tokens": ["Ist", "dein", "die", "Schuld", "nicht", ",", "wenn", "das", "ar\u00b7me", "Lamm", "ver\u00b7irrt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ART", "NN", "PTKNEG", "$,", "KOUS", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Und wenn ein schwach Gem\u00fct, ein stumpfes Sinnen,", "tokens": ["Und", "wenn", "ein", "schwach", "Ge\u00b7m\u00fct", ",", "ein", "stump\u00b7fes", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJD", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Neugierig horcht auf jedes Wort von dir,", "tokens": ["Neu\u00b7gie\u00b7rig", "horcht", "auf", "je\u00b7des", "Wort", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "PIAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um alles m\u00f6chte Gleichheit sich gewinnen,", "tokens": ["Um", "al\u00b7les", "m\u00f6ch\u00b7te", "Gleich\u00b7heit", "sich", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aufzeichnet jede Miene mit Begier:", "tokens": ["Auf\u00b7zeich\u00b7net", "je\u00b7de", "Mie\u00b7ne", "mit", "Be\u00b7gier", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O, spricht nicht dies Gesicht:", "tokens": ["O", ",", "spricht", "nicht", "dies", "Ge\u00b7sicht", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PTKNEG", "PDS", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ich acht' auf dich, bei Gott verdirb mich nicht?", "tokens": ["Ich", "acht'", "auf", "dich", ",", "bei", "Gott", "ver\u00b7dirb", "mich", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "APPR", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Hast du mir Herr an diesem Tag erschlossen,", "tokens": ["Hast", "du", "mir", "Herr", "an", "die\u00b7sem", "Tag", "er\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "NN", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wem nie so ernst zuvor ich nachgedacht,", "tokens": ["Wem", "nie", "so", "ernst", "zu\u00b7vor", "ich", "nach\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ADJD", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So knie ich denn in Flehen hingegossen:", "tokens": ["So", "knie", "ich", "denn", "in", "Fle\u00b7hen", "hin\u00b7ge\u00b7gos\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hier ist der Wille, gib mir nun die Macht!", "tokens": ["Hier", "ist", "der", "Wil\u00b7le", ",", "gib", "mir", "nun", "die", "Macht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "VVIMP", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Sinn so rasch und leicht \u2013", "tokens": ["Der", "Sinn", "so", "rasch", "und", "leicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Leg deine schwere Hand auf ihn, bi