{"textgrid.poem.45199": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Die liberalen Journalisten", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wen scheltet ihr reaktion\u00e4r?", "tokens": ["Wen", "schel\u00b7tet", "ihr", "re\u00b7ak\u00b7ti\u00b7o\u00b7n\u00e4r", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ihr seid zumeist in unsern Mauern,", "tokens": ["Ihr", "seid", "zu\u00b7meist", "in", "un\u00b7sern", "Mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da jeden, der nicht verstandesleer,", "tokens": ["Da", "je\u00b7den", ",", "der", "nicht", "ver\u00b7stan\u00b7des\u00b7leer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "$,", "PRELS", "PTKNEG", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den fr\u00fchern Zustand ihr macht bedauern.", "tokens": ["Den", "fr\u00fc\u00b7hern", "Zu\u00b7stand", "ihr", "macht", "be\u00b7dau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}