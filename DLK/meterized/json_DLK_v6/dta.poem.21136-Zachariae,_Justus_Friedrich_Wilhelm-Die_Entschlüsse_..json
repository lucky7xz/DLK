{"dta.poem.21136": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Entschl\u00fcsse .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Alzindor bittet mich zum Weine,", "tokens": ["Al\u00b7zin\u00b7dor", "bit\u00b7tet", "mich", "zum", "Wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Wein ist gut. Ob ich erscheine?", "tokens": ["Sein", "Wein", "ist", "gut", ".", "Ob", "ich", "er\u00b7schei\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$.", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das kan wohl geschehn!", "tokens": ["Das", "kan", "wohl", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Doch denket er mich zu bestechen,", "tokens": ["Doch", "den\u00b7ket", "er", "mich", "zu", "be\u00b7ste\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Von seiner Narrheit nicht zu sprechen?", "tokens": ["Von", "sei\u00b7ner", "Nar\u00b7rheit", "nicht", "zu", "spre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das will ich doch sehn!", "tokens": ["Das", "will", "ich", "doch", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "++--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Die Vettern sagen: Bleib zu Hause,", "tokens": ["Die", "Vet\u00b7tern", "sa\u00b7gen", ":", "Bleib", "zu", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und laufe nicht zu jedem Schmause!", "tokens": ["Und", "lau\u00b7fe", "nicht", "zu", "je\u00b7dem", "Schmau\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das kan wohl geschehn!", "tokens": ["Das", "kan", "wohl", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Doch denken mich die klugen Herren", "tokens": ["Doch", "den\u00b7ken", "mich", "die", "klu\u00b7gen", "Her\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie einen H\u00e4nfling einzusperren?", "tokens": ["Wie", "ei\u00b7nen", "H\u00e4nf\u00b7ling", "ein\u00b7zu\u00b7sper\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das will ich doch sehn!", "tokens": ["Das", "will", "ich", "doch", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "++--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Man soll nicht in Pasquillen singen,", "tokens": ["Man", "soll", "nicht", "in", "Pas\u00b7quil\u00b7len", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PTKNEG", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Den und Die in Verse bringen.", "tokens": ["Und", "Den", "und", "Die", "in", "Ver\u00b7se", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "KON", "ART", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das kan wohl geschehn!", "tokens": ["Das", "kan", "wohl", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Allein denkt man mich scheu zu machen,", "tokens": ["Al\u00b7lein", "denkt", "man", "mich", "scheu", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Die Narren gar nicht auszulachen?", "tokens": ["Die", "Nar\u00b7ren", "gar", "nicht", "aus\u00b7zu\u00b7la\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das will ich doch sehn!", "tokens": ["Das", "will", "ich", "doch", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "++--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Mein Vormund spricht: Er will schon lieben?", "tokens": ["Mein", "Vor\u00b7mund", "spricht", ":", "Er", "will", "schon", "lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das k\u00f6nnt er immer noch verschieben!", "tokens": ["Das", "k\u00f6nnt", "er", "im\u00b7mer", "noch", "ver\u00b7schie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das kan wohl geschehn!", "tokens": ["Das", "kan", "wohl", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ja, ja; noch weicht dem Wein die Liebe;", "tokens": ["Ja", ",", "ja", ";", "noch", "weicht", "dem", "Wein", "die", "Lie\u00b7be", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "$.", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch stets verschm\u00e4ht ich ihre Triebe!", "tokens": ["Doch", "stets", "ver\u00b7schm\u00e4ht", "ich", "ih\u00b7re", "Trie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das will ich doch sehn!", "tokens": ["Das", "will", "ich", "doch", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "++--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Da\u00df ich nach meines Doctors Lehre", "tokens": ["Da\u00df", "ich", "nach", "mei\u00b7nes", "Doc\u00b7tors", "Leh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jm Fieber allen Wein verschw\u00f6re;", "tokens": ["Jm", "Fie\u00b7ber", "al\u00b7len", "Wein", "ver\u00b7schw\u00f6\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das kan wohl geschehn!", "tokens": ["Das", "kan", "wohl", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Doch wenn das Fieber mich verlassen,", "tokens": ["Doch", "wenn", "das", "Fie\u00b7ber", "mich", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sollt ich den Wein noch immer hassen?", "tokens": ["Sollt", "ich", "den", "Wein", "noch", "im\u00b7mer", "has\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das will ich doch sehn!", "tokens": ["Das", "will", "ich", "doch", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "++--+", "measure": "iambic.tri.chol"}}}}}