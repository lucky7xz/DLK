{"textgrid.poem.25271": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Horch, durch gr\u00fcblerische F\u00f6hren", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Horch, durch gr\u00fcblerische F\u00f6hren", "tokens": ["Horch", ",", "durch", "gr\u00fcb\u00b7ler\u00b7i\u00b7sche", "F\u00f6h\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Woget st\u00f6hnendes Verst\u00f6ren", "tokens": ["Wo\u00b7get", "st\u00f6h\u00b7nen\u00b7des", "Ver\u00b7st\u00f6\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Herbstlich rauhes Nachtgebraus.", "tokens": ["Herbst\u00b7lich", "rau\u00b7hes", "Nacht\u00b7ge\u00b7braus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die reckenhaften Eichen", "tokens": ["Und", "die", "re\u00b7cken\u00b7haf\u00b7ten", "Ei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Toben, weil die Wipfel bleichen,", "tokens": ["To\u00b7ben", ",", "weil", "die", "Wip\u00b7fel", "blei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schaurig trostlos ihren Kummer aus.", "tokens": ["Schau\u00b7rig", "trost\u00b7los", "ih\u00b7ren", "Kum\u00b7mer", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Droben, wo durch Wolkenhader", "tokens": ["Dro\u00b7ben", ",", "wo", "durch", "Wol\u00b7ken\u00b7ha\u00b7der"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00e4ulich wallt des Mondes Duft,", "tokens": ["Bl\u00e4u\u00b7lich", "wallt", "des", "Mon\u00b7des", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rudert durch die barsche Luft", "tokens": ["Ru\u00b7dert", "durch", "die", "bar\u00b7sche", "Luft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wilder G\u00e4nse Keilgeschwader.", "tokens": ["Wil\u00b7der", "G\u00e4n\u00b7se", "Keil\u00b7ge\u00b7schwa\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihrer Sehnsucht dunkel Raunen", "tokens": ["Ih\u00b7rer", "Sehn\u00b7sucht", "dun\u00b7kel", "Rau\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lenkt empor mein stummes Staunen", "tokens": ["Lenkt", "em\u00b7por", "mein", "stum\u00b7mes", "Stau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und erweckt die k\u00fchlen Schauer", "tokens": ["Und", "er\u00b7weckt", "die", "k\u00fch\u00b7len", "Schau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unvergessner Abschiedstrauer.", "tokens": ["Un\u00b7ver\u00b7gess\u00b7ner", "Ab\u00b7schieds\u00b7trau\u00b7er", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dein gedenk ich, armer Freund!", "tokens": ["Dein", "ge\u00b7denk", "ich", ",", "ar\u00b7mer", "Freund", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vom vertrauten Heim betrogen,", "tokens": ["Vom", "ver\u00b7trau\u00b7ten", "Heim", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bist du fr\u00f6stelnd fortgezogen", "tokens": ["Bist", "du", "fr\u00f6s\u00b7telnd", "fort\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit der Wildgans s\u00fcdenw\u00e4rts.", "tokens": ["Mit", "der", "Wild\u00b7gans", "s\u00fc\u00b7den\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Drau\u00dfen suchst du, grambegleitet,", "tokens": ["Drau\u00b7\u00dfen", "suchst", "du", ",", "gram\u00b7be\u00b7glei\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was allein das eigne Herz,", "tokens": ["Was", "al\u00b7lein", "das", "eig\u00b7ne", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie der Baum sein Harz, bereitet.", "tokens": ["Wie", "der", "Baum", "sein", "Harz", ",", "be\u00b7rei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Fahret wohl! Ich bleibe hausen,", "tokens": ["Fah\u00b7ret", "wohl", "!", "Ich", "blei\u00b7be", "hau\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "PPER", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die F\u00f6hren m\u00fcrrisch brausen", "tokens": ["Wo", "die", "F\u00f6h\u00b7ren", "m\u00fcr\u00b7risch", "brau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit mir selbst allein; verschweige", "tokens": ["Mit", "mir", "selbst", "al\u00b7lein", ";", "ver\u00b7schwei\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPER", "ADV", "ADV", "$.", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meiner Sehnsucht Schrei und neige", "tokens": ["Mei\u00b7ner", "Sehn\u00b7sucht", "Schrei", "und", "nei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zum Gebet mein Haupt: Wohlan,", "tokens": ["Zum", "Ge\u00b7bet", "mein", "Haupt", ":", "Wo\u00b7hlan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$.", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Sei nun stark, Einsiedel! Zeige,", "tokens": ["Sei", "nun", "stark", ",", "Ein\u00b7sie\u00b7del", "!", "Zei\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "NN", "$.", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Was die eigne Kraft noch kann!", "tokens": ["Was", "die", "eig\u00b7ne", "Kraft", "noch", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "ADV", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Tiefste Andacht weihe dich,", "tokens": ["Tiefs\u00b7te", "An\u00b7dacht", "wei\u00b7he", "dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und zur \u00d6de, Zaubrer, sprich", "tokens": ["Und", "zur", "\u00d6\u00b7de", ",", "Zaub\u00b7rer", ",", "sprich"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "APPRART", "NN", "$,", "NN", "$,", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Jenes Wort, das Berggestein", "tokens": ["Je\u00b7nes", "Wort", ",", "das", "Berg\u00b7ge\u00b7stein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Spaltet: \u00bbSesam, tue dich auf!\u00ab/", "tokens": ["Spal\u00b7tet", ":", "\u00bb", "Se\u00b7sam", ",", "tue", "dich", "auf", "!", "\u00ab", "/"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "$.", "$(", "NE", "$,", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Dann hinein, getrost hinein!", "tokens": ["Dann", "hin\u00b7ein", ",", "ge\u00b7trost", "hin\u00b7ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Hinter rauher Felsenwand", "tokens": ["Hin\u00b7ter", "rau\u00b7her", "Fel\u00b7sen\u00b7wand"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "L\u00e4chelt dir ein S\u00fcdenland/", "tokens": ["L\u00e4\u00b7chelt", "dir", "ein", "S\u00fc\u00b7den\u00b7land", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Dein ersehntes Friedenland.", "tokens": ["Dein", "er\u00b7sehn\u00b7tes", "Frie\u00b7den\u00b7land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}