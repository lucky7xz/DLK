{"dta.poem.8756": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxXIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Du bist nur um so mehr geplagt:", "tokens": ["Du", "bist", "nur", "um", "so", "mehr", "ge\u00b7plagt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O la\u00df die Tage mit dir schalten,", "tokens": ["O", "la\u00df", "die", "Ta\u00b7ge", "mit", "dir", "schal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und thun, was ihnen wohlbehagt!", "tokens": ["Und", "thun", ",", "was", "ih\u00b7nen", "wohl\u00b7be\u00b7hagt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Soll dir das Leben stets gefallen,", "tokens": ["Soll", "dir", "das", "Le\u00b7ben", "stets", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das nie auf Dauer sich verstand,", "tokens": ["Das", "nie", "auf", "Dau\u00b7er", "sich", "ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So la\u00df das Sch\u00f6nste wieder fallen,", "tokens": ["So", "la\u00df", "das", "Sch\u00f6ns\u00b7te", "wie\u00b7der", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und schlie\u00dfe nicht zu fest die Hand!", "tokens": ["Und", "schlie\u00b7\u00dfe", "nicht", "zu", "fest", "die", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PTKA", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Verm\u00f6cht' ich doch gelind zu tr\u00e4ufen", "tokens": ["Ver\u00b7m\u00f6cht'", "ich", "doch", "ge\u00b7lind", "zu", "tr\u00e4u\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In deine Brust, wenn Schmerz und Muth", "tokens": ["In", "dei\u00b7ne", "Brust", ",", "wenn", "Schmerz", "und", "Muth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie oft vergeblich \u00fcberh\u00e4ufen,", "tokens": ["Sie", "oft", "ver\u00b7geb\u00b7lich", "\u00fc\u00b7berh\u00b7\u00e4u\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur wen'ge Tropfen leichtes Blut!", "tokens": ["Nur", "wen'\u00b7ge", "Trop\u00b7fen", "leich\u00b7tes", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O suche ruhig zu verschlafen", "tokens": ["O", "su\u00b7che", "ru\u00b7hig", "zu", "ver\u00b7schla\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In jeder Nacht des Tages Pein,", "tokens": ["In", "je\u00b7der", "Nacht", "des", "Ta\u00b7ges", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Denn wer verm\u00f6chte Gott zu strafen,", "tokens": ["Denn", "wer", "ver\u00b7m\u00f6ch\u00b7te", "Gott", "zu", "stra\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der uns verdammte, Mensch zu seyn?", "tokens": ["Der", "uns", "ver\u00b7damm\u00b7te", ",", "Mensch", "zu", "seyn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}