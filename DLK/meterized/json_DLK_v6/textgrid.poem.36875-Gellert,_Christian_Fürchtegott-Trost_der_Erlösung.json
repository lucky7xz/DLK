{"textgrid.poem.36875": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Trost der Erl\u00f6sung", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gedanke, der uns Leben gibt,", "tokens": ["Ge\u00b7dan\u00b7ke", ",", "der", "uns", "Le\u00b7ben", "gibt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Welch Herz vermag dich auszudenken!", "tokens": ["Welch", "Herz", "ver\u00b7mag", "dich", "aus\u00b7zu\u00b7den\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbalso hat Gott die Welt geliebt,", "tokens": ["\u00bb", "al\u00b7so", "hat", "Gott", "die", "Welt", "ge\u00b7liebt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Uns seinen Sohn zu schenken!\u00ab", "tokens": ["Uns", "sei\u00b7nen", "Sohn", "zu", "schen\u00b7ken", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hoch \u00fcber die Vernunft erh\u00f6ht,", "tokens": ["Hoch", "\u00fc\u00b7ber", "die", "Ver\u00b7nunft", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Umringt mit heilgen Finsternissen,", "tokens": ["Um\u00b7ringt", "mit", "heil\u00b7gen", "Fins\u00b7ter\u00b7nis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcllst du mein Herz mit Majest\u00e4t,", "tokens": ["F\u00fcllst", "du", "mein", "Herz", "mit", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und stillest mein Gewissen.", "tokens": ["Und", "stil\u00b7lest", "mein", "Ge\u00b7wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich kann der Sonne Wunder nicht,", "tokens": ["Ich", "kann", "der", "Son\u00b7ne", "Wun\u00b7der", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch ihren Lauf und Bau ergr\u00fcnden;", "tokens": ["Noch", "ih\u00b7ren", "Lauf", "und", "Bau", "er\u00b7gr\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und doch kann ich der Sonne Licht", "tokens": ["Und", "doch", "kann", "ich", "der", "Son\u00b7ne", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihre W\u00e4rm empfinden.", "tokens": ["Und", "ih\u00b7re", "W\u00e4rm", "emp\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So kann mein Geist den hohen Rat", "tokens": ["So", "kann", "mein", "Geist", "den", "ho\u00b7hen", "Rat"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Opfers Jesu nicht ergr\u00fcnden;", "tokens": ["Des", "Op\u00b7fers", "Je\u00b7su", "nicht", "er\u00b7gr\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Allein das G\u00f6ttliche der Tat,", "tokens": ["Al\u00b7lein", "das", "G\u00f6tt\u00b7li\u00b7che", "der", "Tat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das kann mein Herz empfinden.", "tokens": ["Das", "kann", "mein", "Herz", "emp\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nimm mir den Trost, da\u00df Jesus Christ", "tokens": ["Nimm", "mir", "den", "Trost", ",", "da\u00df", "Je\u00b7sus", "Christ"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "KOUS", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Kreuz nicht meine Schuld getragen,", "tokens": ["Am", "Kreuz", "nicht", "mei\u00b7ne", "Schuld", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht Gott und mein Erl\u00f6ser ist:", "tokens": ["Nicht", "Gott", "und", "mein", "Er\u00b7l\u00f6\u00b7ser", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So werd ich angstvoll zagen.", "tokens": ["So", "werd", "ich", "angst\u00b7voll", "za\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ist Christi Wort nicht Gottes Sinn:", "tokens": ["Ist", "Chris\u00b7ti", "Wort", "nicht", "Got\u00b7tes", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NN", "PTKNEG", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So werd ich ewig irren m\u00fcssen,", "tokens": ["So", "werd", "ich", "e\u00b7wig", "ir\u00b7ren", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wer Gott ist, und was ich bin,", "tokens": ["Und", "wer", "Gott", "ist", ",", "und", "was", "ich", "bin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "NN", "VAFIN", "$,", "KON", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und werden soll, nicht wissen.", "tokens": ["Und", "wer\u00b7den", "soll", ",", "nicht", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAINF", "VMFIN", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Nein, diesen Trost der Christenheit", "tokens": ["Nein", ",", "die\u00b7sen", "Trost", "der", "Chris\u00b7ten\u00b7heit"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PDAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Soll mir kein frecher Sp\u00f6tter rauben;", "tokens": ["Soll", "mir", "kein", "fre\u00b7cher", "Sp\u00f6t\u00b7ter", "rau\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich f\u00fchle seine G\u00f6ttlichkeit,", "tokens": ["Ich", "f\u00fch\u00b7le", "sei\u00b7ne", "G\u00f6tt\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und halte fest am Glauben.", "tokens": ["Und", "hal\u00b7te", "fest", "am", "Glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Des Sohnes Gottes Eigentum,", "tokens": ["Des", "Soh\u00b7nes", "Got\u00b7tes", "Ei\u00b7gen\u00b7tum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch ihn des ewgen Lebens Erbe,", "tokens": ["Durch", "ihn", "des", "ew\u00b7gen", "Le\u00b7bens", "Er\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dies bin ich; und das ist mein Ruhm,", "tokens": ["Dies", "bin", "ich", ";", "und", "das", "ist", "mein", "Ruhm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$.", "KON", "PDS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf den ich leb und sterbe.", "tokens": ["Auf", "den", "ich", "leb", "und", "ster\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Er gibt mir seinen Geist, das Pfand,", "tokens": ["Er", "gibt", "mir", "sei\u00b7nen", "Geist", ",", "das", "Pfand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Daran wir seine Liebe merken,", "tokens": ["Da\u00b7ran", "wir", "sei\u00b7ne", "Lie\u00b7be", "mer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und bildet uns durch seine Hand", "tokens": ["Und", "bil\u00b7det", "uns", "durch", "sei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu allen guten Werken.", "tokens": ["Zu", "al\u00b7len", "gu\u00b7ten", "Wer\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "So lang ich seinen Willen gern", "tokens": ["So", "lang", "ich", "sei\u00b7nen", "Wil\u00b7len", "gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit einem reinen Herzen tue;", "tokens": ["Mit", "ei\u00b7nem", "rei\u00b7nen", "Her\u00b7zen", "tue", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So f\u00fchl ich eine Kraft des Herrn,", "tokens": ["So", "f\u00fchl", "ich", "ei\u00b7ne", "Kraft", "des", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schmecke Fried und Ruhe.", "tokens": ["Und", "schme\u00b7cke", "Fried", "und", "Ru\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Und wenn mich meine S\u00fcnde kr\u00e4nkt,", "tokens": ["Und", "wenn", "mich", "mei\u00b7ne", "S\u00fcn\u00b7de", "kr\u00e4nkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ich zu seinem Kreuze trete;", "tokens": ["Und", "ich", "zu", "sei\u00b7nem", "Kreu\u00b7ze", "tre\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So wei\u00df ich, da\u00df er mein gedenkt,", "tokens": ["So", "wei\u00df", "ich", ",", "da\u00df", "er", "mein", "ge\u00b7denkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und tut, worum ich bete.", "tokens": ["Und", "tut", ",", "wo\u00b7rum", "ich", "be\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ich wei\u00df, da\u00df mein Erl\u00f6ser lebt,", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "mein", "Er\u00b7l\u00f6\u00b7ser", "lebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich, erwecket aus der Erde,", "tokens": ["Da\u00df", "ich", ",", "er\u00b7we\u00b7cket", "aus", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn er sich zum Gericht erhebt,", "tokens": ["Wenn", "er", "sich", "zum", "Ge\u00b7richt", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Im Fleisch ihn schauen werde.", "tokens": ["Im", "Fleisch", "ihn", "schau\u00b7en", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Kann unsre Lieb im Glauben hier", "tokens": ["Kann", "uns\u00b7re", "Lieb", "im", "Glau\u00b7ben", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr den, der uns geliebt, erkalten?", "tokens": ["F\u00fcr", "den", ",", "der", "uns", "ge\u00b7liebt", ",", "er\u00b7kal\u00b7ten", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PPER", "VVPP", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dies ist die Lieb, o Gott! zu dir,", "tokens": ["Dies", "ist", "die", "Lieb", ",", "o", "Gott", "!", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "FM", "NN", "$.", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Wort von Herzen halten.", "tokens": ["Dein", "Wort", "von", "Her\u00b7zen", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Erf\u00fcll mein Herz mit Dankbarkeit,", "tokens": ["Er\u00b7f\u00fcll", "mein", "Herz", "mit", "Dank\u00b7bar\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So oft ich deinen Namen nenne,", "tokens": ["So", "oft", "ich", "dei\u00b7nen", "Na\u00b7men", "nen\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und hilf, da\u00df ich dich allezeit", "tokens": ["Und", "hilf", ",", "da\u00df", "ich", "dich", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KO