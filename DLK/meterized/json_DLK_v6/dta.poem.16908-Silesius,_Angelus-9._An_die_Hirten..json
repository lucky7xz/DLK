{"dta.poem.16908": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "9.  An die Hirten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.71", "nl:0.28"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Gieb Antwort liebes Volk/ was hastu doch gesungen", "tokens": ["Gieb", "Ant\u00b7wort", "lie\u00b7bes", "Volk", "/", "was", "has\u00b7tu", "doch", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "NN", "ADJA", "NN", "$(", "PWS", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als du inn Stall eingingst mit den erbebten Zungen/", "tokens": ["Als", "du", "inn", "Stall", "ein\u00b7gingst", "mit", "den", "er\u00b7beb\u00b7ten", "Zun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und GOtt ein Kind gesehn? Da\u00df auch mein Jesulein", "tokens": ["Und", "Gott", "ein", "Kind", "ge\u00b7sehn", "?", "Da\u00df", "auch", "mein", "Je\u00b7su\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "ART", "NN", "VVPP", "$.", "KOUS", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit einem Hirten Lied von mir gepreist kan seyn.", "tokens": ["Mit", "ei\u00b7nem", "Hir\u00b7ten", "Lied", "von", "mir", "ge\u00b7preist", "kan", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "APPR", "PPER", "VVPP", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}