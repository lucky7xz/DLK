{"textgrid.poem.43666": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Frau Spinne spinnt im Sonnenschein", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frau Spinne spinnt im Sonnenschein", "tokens": ["Frau", "Spin\u00b7ne", "spinnt", "im", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und singt dazu ein Liedelein:", "tokens": ["Und", "singt", "da\u00b7zu", "ein", "Lie\u00b7de\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Di da didallala.", "tokens": ["Di", "da", "di\u00b7dal\u00b7la\u00b7la", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie spinnt so zart, sie spinnt so fein,", "tokens": ["Sie", "spinnt", "so", "zart", ",", "sie", "spinnt", "so", "fein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und denkt: Wer will mein Meister sein?", "tokens": ["Und", "denkt", ":", "Wer", "will", "mein", "Meis\u00b7ter", "sein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PWS", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Di da didallala.", "tokens": ["Di", "da", "di\u00b7dal\u00b7la\u00b7la", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da hangt ihr H\u00e4uslein blank und rein,", "tokens": ["Da", "hangt", "ihr", "H\u00e4us\u00b7lein", "blank", "und", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Frau Spinne setzt sich mitten drein.", "tokens": ["Frau", "Spin\u00b7ne", "setzt", "sich", "mit\u00b7ten", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PRF", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Di da didallala.", "tokens": ["Di", "da", "di\u00b7dal\u00b7la\u00b7la", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbnun sitz' ich hier so ganz allein \u2013", "tokens": ["\u00bb", "nun", "sitz'", "ich", "hier", "so", "ganz", "al\u00b7lein", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "ADV", "ADV", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat keiner Lust, mein Gast zu sein?\u00ab", "tokens": ["Hat", "kei\u00b7ner", "Lust", ",", "mein", "Gast", "zu", "sein", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$,", "PPOSAT", "NN", "PTKZU", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Di da didallala.", "tokens": ["Di", "da", "di\u00b7dal\u00b7la\u00b7la", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Gleich kommt ein lustig M\u00fcckelein:", "tokens": ["Gleich", "kommt", "ein", "lus\u00b7tig", "M\u00fc\u00b7ckel\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbsei mir willkommen, Schwesterlein!\u00ab", "tokens": ["\u00bb", "sei", "mir", "will\u00b7kom\u00b7men", ",", "Schwes\u00b7ter\u00b7lein", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "PPER", "ADJD", "$,", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Di da didallala.", "tokens": ["Di", "da", "di\u00b7dal\u00b7la\u00b7la", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Frau Spinn' umarmt's und spinnt es ein,", "tokens": ["Frau", "Spinn'", "um\u00b7armt's", "und", "spinnt", "es", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nichts hilft dem M\u00fcckelein sein Schrei'n.", "tokens": ["Nichts", "hilft", "dem", "M\u00fc\u00b7ckel\u00b7ein", "sein", "Schrei'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PIS", "VVFIN", "ART", "NN", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Di da didallala.", "tokens": ["Di", "da", "di\u00b7dal\u00b7la\u00b7la", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und sie verschmaust es klimperklein", "tokens": ["Und", "sie", "ver\u00b7schmaust", "es", "klim\u00b7per\u00b7klein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und singt dazu ein Liedelein:", "tokens": ["Und", "singt", "da\u00b7zu", "ein", "Lie\u00b7de\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Di da didallala.", "tokens": ["Di", "da", "di\u00b7dal\u00b7la\u00b7la", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}