{"dta.poem.18330": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Ein dunkles Blatt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "In Nacht und Frost will ich verenden \u2014", "tokens": ["In", "Nacht", "und", "Frost", "will", "ich", "ver\u00b7en\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Oft scheint das Leben mir nicht werth,", "tokens": ["Oft", "scheint", "das", "Le\u00b7ben", "mir", "nicht", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur einen Vers dran zu verschwenden.", "tokens": ["Nur", "ei\u00b7nen", "Vers", "dran", "zu", "ver\u00b7schwen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr aber fragt mich nicht, warum?", "tokens": ["Ihr", "a\u00b7ber", "fragt", "mich", "nicht", ",", "wa\u00b7rum", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht Liebe mehr ist's, die ich suche!", "tokens": ["Nicht", "Lie\u00b7be", "mehr", "ist's", ",", "die", "ich", "su\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wei\u00df, die Welt dreht sich rundum,", "tokens": ["Ich", "wei\u00df", ",", "die", "Welt", "dreht", "sich", "run\u00b7dum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Auch wenn ich lachend sie verfluche!", "tokens": ["Auch", "wenn", "ich", "la\u00b7chend", "sie", "ver\u00b7flu\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}