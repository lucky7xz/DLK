{"textgrid.poem.45206": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[ihr tut, wie die deutschen Studenten tun]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr tut, wie die deutschen Studenten tun,", "tokens": ["Ihr", "tut", ",", "wie", "die", "deut\u00b7schen", "Stu\u00b7den\u00b7ten", "tun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es geht schon aufs Kommerschieren,", "tokens": ["Es", "geht", "schon", "aufs", "Kom\u00b7mer\u00b7schie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ihr seid ihnen \u00e4hnlich in allem nun,", "tokens": ["Ihr", "seid", "ih\u00b7nen", "\u00e4hn\u00b7lich", "in", "al\u00b7lem", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "APPR", "PIS", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "In allem, bis aufs Studieren.", "tokens": ["In", "al\u00b7lem", ",", "bis", "aufs", "Stu\u00b7die\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "KOUS", "APPRART", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}}}}