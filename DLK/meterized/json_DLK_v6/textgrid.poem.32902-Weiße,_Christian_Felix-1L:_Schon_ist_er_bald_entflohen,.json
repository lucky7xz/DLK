{"textgrid.poem.32902": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schon ist er bald entflohen,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon ist er bald entflohen,", "tokens": ["Schon", "ist", "er", "bald", "ent\u00b7flo\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Winter, meine Lust!", "tokens": ["Der", "Win\u00b7ter", ",", "mei\u00b7ne", "Lust", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die sanften Weste drohen", "tokens": ["Die", "sanf\u00b7ten", "Wes\u00b7te", "dro\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir schrecklichen Verlust!", "tokens": ["Mir", "schreck\u00b7li\u00b7chen", "Ver\u00b7lust", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Umsonst bl\u00fcht mir Betr\u00fcbten", "tokens": ["Um\u00b7sonst", "bl\u00fcht", "mir", "Be\u00b7tr\u00fcb\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Die neugebohrne Welt:", "tokens": ["Die", "neu\u00b7ge\u00b7bohr\u00b7ne", "Welt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Krieg ruft den Geliebten", "tokens": ["Der", "Krieg", "ruft", "den", "Ge\u00b7lieb\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Von mir ins rauhe Feld.", "tokens": ["Von", "mir", "ins", "rau\u00b7he", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "In jeder Blum entschlie\u00dfet", "tokens": ["In", "je\u00b7der", "Blum", "ent\u00b7schlie\u00b7\u00dfet"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich mir ein neuer Schmerz,", "tokens": ["Sich", "mir", "ein", "neu\u00b7er", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Zephyr, der sie k\u00fcsset,", "tokens": ["Der", "Ze\u00b7phyr", ",", "der", "sie", "k\u00fcs\u00b7set", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Haucht Wehmuth in mein Herz:", "tokens": ["Haucht", "Weh\u00b7muth", "in", "mein", "Herz", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Landschaft bunte Scenen,", "tokens": ["Der", "Land\u00b7schaft", "bun\u00b7te", "Sce\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die blumenreiche Au,", "tokens": ["Die", "blu\u00b7men\u00b7rei\u00b7che", "Au", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sehn meiner bangen Thr\u00e4nen", "tokens": ["Sehn", "mei\u00b7ner", "ban\u00b7gen", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mehr als den Morgenthau.", "tokens": ["Mehr", "als", "den", "Mor\u00b7gent\u00b7hau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Umsonst singt jede Kehle", "tokens": ["Um\u00b7sonst", "singt", "je\u00b7de", "Keh\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Fr\u00fchling froh bem\u00fcht:", "tokens": ["Den", "Fr\u00fch\u00b7ling", "froh", "be\u00b7m\u00fcht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir singt selbst Philomele", "tokens": ["Mir", "singt", "selbst", "Phi\u00b7lo\u00b7me\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein banges Klagelied.", "tokens": ["Ein", "ban\u00b7ges", "Kla\u00b7ge\u00b7lied", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Des Leidens Melodien", "tokens": ["Des", "Lei\u00b7dens", "Me\u00b7lo\u00b7dien"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "H\u00f6r ich im freyen Bach:", "tokens": ["H\u00f6r", "ich", "im", "frey\u00b7en", "Bach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es rei\u00dft der Nord im Fliehen", "tokens": ["Es", "rei\u00dft", "der", "Nord", "im", "Flie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein ganzes Gl\u00fccke nach.", "tokens": ["Mein", "gan\u00b7zes", "Gl\u00fc\u00b7cke", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O steig noch nicht hernieder", "tokens": ["O", "steig", "noch", "nicht", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "PTKNEG", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du Lenz, der Erde Lust!", "tokens": ["Du", "Lenz", ",", "der", "Er\u00b7de", "Lust", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir bringst du Blumen wieder,", "tokens": ["Mir", "bringst", "du", "Blu\u00b7men", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch Gram in meine Brust.", "tokens": ["Doch", "Gram", "in", "mei\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dich w\u00fcnscht die Welt: die Freuden", "tokens": ["Dich", "w\u00fcnscht", "die", "Welt", ":", "die", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Liebe bringst du ihr:", "tokens": ["Der", "Lie\u00b7be", "bringst", "du", "ihr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sollt ich sie nicht beneiden?", "tokens": ["Sollt", "ich", "sie", "nicht", "be\u00b7nei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die meinen raubst du mir!", "tokens": ["Die", "mei\u00b7nen", "raubst", "du", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}