{"textgrid.poem.25896": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[ich sehe den Berg, den breit schwebenden]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sehe den Berg, den breit schwebenden.", "tokens": ["Ich", "se\u00b7he", "den", "Berg", ",", "den", "breit", "schwe\u00b7ben\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er zieht an den Himmel edel,", "tokens": ["Er", "zieht", "an", "den", "Him\u00b7mel", "e\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Einfach und sich selbst genug.", "tokens": ["Ein\u00b7fach", "und", "sich", "selbst", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PRF", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Das starke purpurne Meer,", "tokens": ["Das", "star\u00b7ke", "pur\u00b7pur\u00b7ne", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Die blaue erhabene Wolke kommen zu ihm,", "tokens": ["Die", "blau\u00b7e", "er\u00b7ha\u00b7be\u00b7ne", "Wol\u00b7ke", "kom\u00b7men", "zu", "ihm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Sein Schnee ist wei\u00dfer als der Schnee.", "tokens": ["Sein", "Schnee", "ist", "wei\u00b7\u00dfer", "als", "der", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur der Reife nahe dem Edlen!", "tokens": ["Nur", "der", "Rei\u00b7fe", "na\u00b7he", "dem", "Ed\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Jahrtausende begegnen dir auf dem Wege,", "tokens": ["Jahr\u00b7tau\u00b7sen\u00b7de", "be\u00b7geg\u00b7nen", "dir", "auf", "dem", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und dem Leidlosen alles Leid,", "tokens": ["Und", "dem", "Leid\u00b7lo\u00b7sen", "al\u00b7les", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}, "line.7": {"text": "Den starken Wissenden pr\u00fcfen starke Tote.", "tokens": ["Den", "star\u00b7ken", "Wis\u00b7sen\u00b7den", "pr\u00fc\u00b7fen", "star\u00b7ke", "To\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Sorgengeruch des Verbrannten begleitet dich,", "tokens": ["Sor\u00b7gen\u00b7ge\u00b7ruch", "des", "Ver\u00b7brann\u00b7ten", "be\u00b7glei\u00b7tet", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Urasche, und ihre Dunkelheit vergi\u00dft du nie.", "tokens": ["U\u00b7rasc\u00b7he", ",", "und", "ih\u00b7re", "Dun\u00b7kel\u00b7heit", "ver\u00b7gi\u00dft", "du", "nie", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.4": {"line.1": {"text": "Blumen und Fr\u00fcchte stehen am Fu\u00df,", "tokens": ["Blu\u00b7men", "und", "Fr\u00fcch\u00b7te", "ste\u00b7hen", "am", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Gr\u00fcn, aber dunkelgrundig.", "tokens": ["Gr\u00fcn", ",", "a\u00b7ber", "dun\u00b7kel\u00b7grun\u00b7dig", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Du lachst und f\u00fcrchtest die schwelgenden G\u00e4rten.", "tokens": ["Du", "lachst", "und", "f\u00fcrch\u00b7test", "die", "schwel\u00b7gen\u00b7den", "G\u00e4r\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und reiche, d\u00fcstere D\u00f6rfer,", "tokens": ["Und", "rei\u00b7che", ",", "d\u00fcs\u00b7te\u00b7re", "D\u00f6r\u00b7fer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ihr Reichtum ist \u00e4rmer als alle Armut.", "tokens": ["Ihr", "Reich\u00b7tum", "ist", "\u00e4r\u00b7mer", "als", "al\u00b7le", "Ar\u00b7mut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KOKOM", "PIAT", "NN", "$."], "meter": "-+--+-++-+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Der Tag lebt dort nur am Himmel,", "tokens": ["Der", "Tag", "lebt", "dort", "nur", "am", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auf Erden Mauern und H\u00e4user zeigen hartn\u00e4ckige Nacht.", "tokens": ["Auf", "Er\u00b7den", "Mau\u00b7ern", "und", "H\u00e4u\u00b7ser", "zei\u00b7gen", "hart\u00b7n\u00e4\u00b7cki\u00b7ge", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Und furchtbar lachen dort alle Menschen,", "tokens": ["Und", "furcht\u00b7bar", "la\u00b7chen", "dort", "al\u00b7le", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie verlachen grimmig sich selbst.", "tokens": ["Sie", "ver\u00b7la\u00b7chen", "grim\u00b7mig", "sich", "selbst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PRF", "ADV", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Dann auf den finstern Feldern,", "tokens": ["Dann", "auf", "den", "fins\u00b7tern", "Fel\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Sonne verachten,", "tokens": ["Die", "Son\u00b7ne", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Mu\u00dft du schattenlos gehen;", "tokens": ["Mu\u00dft", "du", "schat\u00b7ten\u00b7los", "ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Dir schwinden Knochen und Schwere", "tokens": ["Dir", "schwin\u00b7den", "Kno\u00b7chen", "und", "Schwe\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Auf der gewichtlosen Kohle.", "tokens": ["Auf", "der", "ge\u00b7wicht\u00b7lo\u00b7sen", "Koh\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.7": {"line.1": {"text": "Oben wartet die unergr\u00fcndliche H\u00f6he, schmucklos.", "tokens": ["O\u00b7ben", "war\u00b7tet", "die", "un\u00b7er\u00b7gr\u00fcnd\u00b7li\u00b7che", "H\u00f6\u00b7he", ",", "schmuck\u00b7los", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "ADJD", "$."], "meter": "+-+--+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Du verwirfst Geh\u00f6r und Gesicht.", "tokens": ["Du", "ver\u00b7wirfst", "Ge\u00b7h\u00f6r", "und", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dein Lachen geht unter, spurlos.", "tokens": ["Dein", "La\u00b7chen", "geht", "un\u00b7ter", ",", "spur\u00b7los", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "$,", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Dann wirst du sehend,", "tokens": ["Dann", "wirst", "du", "se\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Das wirst du erfahren,", "tokens": ["Das", "wirst", "du", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Niemandem willst du dich nennen,", "tokens": ["Nie\u00b7man\u00b7dem", "willst", "du", "dich", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Ob auch den Boden die Fl\u00fcche der Bettler ersch\u00fcttern.", "tokens": ["Ob", "auch", "den", "Bo\u00b7den", "die", "Fl\u00fc\u00b7che", "der", "Bett\u00b7ler", "er\u00b7sch\u00fct\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}}}}}