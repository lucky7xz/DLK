{"dta.poem.13123": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201enichts bleibet unerw\u00e4hnt \u201e, so fieng sie pl\u00f6zlich an;", "tokens": ["\u201e", "nichts", "blei\u00b7bet", "un\u00b7er\u00b7w\u00e4hnt", "\u201e", ",", "so", "fi\u00b7eng", "sie", "pl\u00f6z\u00b7lich", "an", ";"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VVFIN", "ADJD", "$(", "$,", "ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "(jhr Arm hielt eine Schlang und einen stolzen Hahn)", "tokens": ["(", "jhr", "Arm", "hielt", "ei\u00b7ne", "Schlang", "und", "ei\u00b7nen", "stol\u00b7zen", "Hahn", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "ART", "NN", "KON", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eso red\u2019 ich auch von mir. Wer kann es unrecht nennen,", "tokens": ["\u201e", "so", "red'", "ich", "auch", "von", "mir", ".", "Wer", "kann", "es", "un\u00b7recht", "nen\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$.", "PWS", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eauch meinen Wirckungen ein kleines Lob zu g\u00f6nnen?", "tokens": ["\u201e", "auch", "mei\u00b7nen", "Wir\u00b7ckun\u00b7gen", "ein", "klei\u00b7nes", "Lob", "zu", "g\u00f6n\u00b7nen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPOSAT", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "595\u201dFast jede r\u00fchmte sich, was ihrer Tugend Kraft", "tokens": ["\"", "Fast", "je\u00b7de", "r\u00fchm\u00b7te", "sich", ",", "was", "ih\u00b7rer", "Tu\u00b7gend", "Kraft"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "PIAT", "VVFIN", "PRF", "$,", "PRELS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eder K\u00f6niginn vor Hilff und Nuzbarkeit geschafft;", "tokens": ["\u201e", "der", "K\u00f6\u00b7ni\u00b7ginn", "vor", "Hilff", "und", "Nuz\u00b7bar\u00b7keit", "ge\u00b7schafft", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eso trifft der Rang mich auch ein wenig anzuzeigen,", "tokens": ["\u201e", "so", "trifft", "der", "Rang", "mich", "auch", "ein", "we\u00b7nig", "an\u00b7zu\u00b7zei\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "NN", "PPER", "ADV", "ART", "PIS", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ewie weit die Tugenden, wann ich entfernt bin, steigen.", "tokens": ["\u201e", "wie", "weit", "die", "Tu\u00b7gen\u00b7den", ",", "wann", "ich", "ent\u00b7fernt", "bin", ",", "stei\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PWAV", "ADJD", "ART", "NN", "$,", "PWAV", "PPER", "VVPP", "VAFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}