{"dta.poem.23400": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Guter Rath.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Willst du frei sein von Beschwerden,", "tokens": ["Willst", "du", "frei", "sein", "von", "Be\u00b7schwer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VAINF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Arme Seele, hier auf Erden,", "tokens": ["Ar\u00b7me", "See\u00b7le", ",", "hier", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf! nach M\u00fcnchen mu\u00dft du ziehn:", "tokens": ["Auf", "!", "nach", "M\u00fcn\u00b7chen", "mu\u00dft", "du", "ziehn", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "APPR", "NE", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort steht jede Kunst in Bl\u00fcthe,", "tokens": ["Dort", "steht", "je\u00b7de", "Kunst", "in", "Bl\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dort wird jeglichem Gem\u00fcthe", "tokens": ["Dort", "wird", "jeg\u00b7li\u00b7chem", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Irgend noch ein Heil verliehn.", "tokens": ["Ir\u00b7gend", "noch", "ein", "Heil", "ver\u00b7liehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bei des Herrn dem\u00fcth'gen Dienern,", "tokens": ["Bei", "des", "Herrn", "de\u00b7m\u00fcth'\u00b7gen", "Die\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei den frommen Capuzinern,", "tokens": ["Bei", "den", "from\u00b7men", "Ca\u00b7pu\u00b7zi\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Arme Seele, nimm Quartier!", "tokens": ["Ar\u00b7me", "See\u00b7le", ",", "nimm", "Quar\u00b7tier", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVIMP", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihnen kann man ganz vertrauen,", "tokens": ["Ih\u00b7nen", "kann", "man", "ganz", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn die heil'gen V\u00e4ter brauen", "tokens": ["Denn", "die", "heil'\u00b7gen", "V\u00e4\u00b7ter", "brau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch das allerbeste Bier.", "tokens": ["Doch", "das", "al\u00b7ler\u00b7bes\u00b7te", "Bier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}