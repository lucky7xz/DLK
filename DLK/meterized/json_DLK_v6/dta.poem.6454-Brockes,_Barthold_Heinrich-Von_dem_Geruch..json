{"dta.poem.6454": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geruch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Mensch wird offt ein Ding sehr strenge riechend finden;", "tokens": ["Ein", "Mensch", "wird", "offt", "ein", "Ding", "sehr", "stren\u00b7ge", "rie\u00b7chend", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "ADV", "VVFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der nahe bey ihm steht, wird nichts davon empfinden.", "tokens": ["Der", "na\u00b7he", "bey", "ihm", "steht", ",", "wird", "nichts", "da\u00b7von", "emp\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "VVFIN", "$,", "VAFIN", "PIS", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man findet manch Gehirn von solcher Z\u00e4rtlichkeit,", "tokens": ["Man", "fin\u00b7det", "manch", "Ge\u00b7hirn", "von", "sol\u00b7cher", "Z\u00e4rt\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIAT", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df es fast nichts, was riecht, vertragen kan,", "tokens": ["Da\u00df", "es", "fast", "nichts", ",", "was", "riecht", ",", "ver\u00b7tra\u00b7gen", "kan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "$,", "PWS", "VVFIN", "$,", "VVFIN", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der kleinen C\u00f6rperchen Bewegung ficht ihn an,", "tokens": ["Der", "klei\u00b7nen", "C\u00f6r\u00b7per\u00b7chen", "Be\u00b7we\u00b7gung", "ficht", "ihn", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Fast mehr, als die Beschaffenheit", "tokens": ["Fast", "mehr", ",", "als", "die", "Be\u00b7schaf\u00b7fen\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von gr\u00f6bern D\u00fcfften ihn verletzt.", "tokens": ["Von", "gr\u00f6\u00b7bern", "D\u00fcff\u00b7ten", "ihn", "ver\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es zeiget ja fast t\u00e4glich sich,", "tokens": ["Es", "zei\u00b7get", "ja", "fast", "t\u00e4g\u00b7lich", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wie offt ein Mensch so wunderlich", "tokens": ["Wie", "offt", "ein", "Mensch", "so", "wun\u00b7der\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Durch den Geruch gantz aus sich selbst gesetzet,", "tokens": ["Durch", "den", "Ge\u00b7ruch", "gantz", "aus", "sich", "selbst", "ge\u00b7set\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "APPR", "PRF", "ADV", "VVPP", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.11": {"text": "Und schnell gekr\u00e4ncket wird; Hat jemand die Natur,", "tokens": ["Und", "schnell", "ge\u00b7kr\u00e4n\u00b7cket", "wird", ";", "Hat", "je\u00b7mand", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "VAFIN", "$.", "VAFIN", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So braucht es einer Blum in einem Garten nur.", "tokens": ["So", "braucht", "es", "ei\u00b7ner", "Blum", "in", "ei\u00b7nem", "Gar\u00b7ten", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NE", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der kan nicht den Geruch von einer Ros\u2019 ertragen,", "tokens": ["Der", "kan", "nicht", "den", "Ge\u00b7ruch", "von", "ei\u00b7ner", "Ros'", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PTKNEG", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Den andern kan der Dufft der Jelsomin verjagen.", "tokens": ["Den", "an\u00b7dern", "kan", "der", "Dufft", "der", "Jel\u00b7so\u00b7min", "ver\u00b7ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}