{"textgrid.poem.26536": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer da zu fr\u00fch die Gunst der Welt erfahren", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer da zu fr\u00fch die Gunst der Welt erfahren", "tokens": ["Wer", "da", "zu", "fr\u00fch", "die", "Gunst", "der", "Welt", "er\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "PTKA", "ADJD", "ART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ihres Beifalls \u00dcbermaa\u00df errungen,", "tokens": ["Und", "ih\u00b7res", "Bei\u00b7falls", "\u00dc\u00b7berm\u00b7aa\u00df", "er\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der wird sofort, von Hochmuth rasch durchdrungen,", "tokens": ["Der", "wird", "so\u00b7fort", ",", "von", "Hoch\u00b7muth", "rasch", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$,", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die menschliche Gemeinheit offenbaren.", "tokens": ["Die", "menschli\u00b7che", "Ge\u00b7mein\u00b7heit", "of\u00b7fen\u00b7ba\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Schon auf dem Gipfel wird er sich gewahren,", "tokens": ["Schon", "auf", "dem", "Gip\u00b7fel", "wird", "er", "sich", "ge\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gewappnet, wie dem Haupt des Zeus entsprungen;", "tokens": ["Ge\u00b7wapp\u00b7net", ",", "wie", "dem", "Haupt", "des", "Zeus", "ent\u00b7sprun\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "ART", "NN", "ART", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verachten wird er dreist der Wahrheit Zungen,", "tokens": ["Ver\u00b7ach\u00b7ten", "wird", "er", "dreist", "der", "Wahr\u00b7heit", "Zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ungn\u00e4digsein \u2013 auch gegen Schmeichlerschaaren.", "tokens": ["Un\u00b7gn\u00e4\u00b7digs\u00b7ein", "\u2013", "auch", "ge\u00b7gen", "Schmeich\u00b7ler\u00b7schaa\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Er ", "tokens": ["Er"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Vermag ihm keine Demuth einzufl\u00f6\u00dfen:", "tokens": ["Ver\u00b7mag", "ihm", "kei\u00b7ne", "De\u00b7muth", "ein\u00b7zu\u00b7fl\u00f6\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df er sie tr\u00e4gt, soll ", "tokens": ["Da\u00df", "er", "sie", "tr\u00e4gt", ",", "soll"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "VMFIN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Blickt doch nur hin nach eu'ren Ruhmesgr\u00f6\u00dfen,", "tokens": ["Blickt", "doch", "nur", "hin", "nach", "eu'\u00b7ren", "Ruh\u00b7mes\u00b7gr\u00f6\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie sie da rings als schn\u00f6de G\u00f6tzen thronen,", "tokens": ["Wie", "sie", "da", "rings", "als", "schn\u00f6\u00b7de", "G\u00f6t\u00b7zen", "thro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "KOUS", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum Dank euch weisend ihre Hinterbl\u00f6\u00dfen.", "tokens": ["Zum", "Dank", "euch", "wei\u00b7send", "ih\u00b7re", "Hin\u00b7ter\u00b7bl\u00f6\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}