{"textgrid.poem.55010": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "N\u00e4he des Geliebten", "genre": "verse", "period": "N.A.", "pub_year": 1795, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich denke dein, wenn mir der Sonne Schimmer", "tokens": ["Ich", "den\u00b7ke", "dein", ",", "wenn", "mir", "der", "Son\u00b7ne", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "$,", "KOUS", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Meere strahlt;", "tokens": ["Vom", "Mee\u00b7re", "strahlt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ich denke dein, wenn sich des Mondes Flimmer", "tokens": ["Ich", "den\u00b7ke", "dein", ",", "wenn", "sich", "des", "Mon\u00b7des", "Flim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "$,", "KOUS", "PRF", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Quellen malt.", "tokens": ["In", "Quel\u00b7len", "malt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ich sehe dich, wenn auf dem fernen Wege", "tokens": ["Ich", "se\u00b7he", "dich", ",", "wenn", "auf", "dem", "fer\u00b7nen", "We\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Staub sich hebt;", "tokens": ["Der", "Staub", "sich", "hebt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "In tiefer Nacht, wenn auf dem schmalen Stege", "tokens": ["In", "tie\u00b7fer", "Nacht", ",", "wenn", "auf", "dem", "schma\u00b7len", "Ste\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Wandrer bebt.", "tokens": ["Der", "Wand\u00b7rer", "bebt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ich h\u00f6re dich, wenn dort mit dumpfem Rauschen", "tokens": ["Ich", "h\u00f6\u00b7re", "dich", ",", "wenn", "dort", "mit", "dum\u00b7pfem", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Welle steigt.", "tokens": ["Die", "Wel\u00b7le", "steigt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Im stillen Haine geh ich oft zu lauschen,", "tokens": ["Im", "stil\u00b7len", "Hai\u00b7ne", "geh", "ich", "oft", "zu", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn alles schweigt.", "tokens": ["Wenn", "al\u00b7les", "schweigt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ich bin bei dir, du seist auch noch so ferne,", "tokens": ["Ich", "bin", "bei", "dir", ",", "du", "seist", "auch", "noch", "so", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$,", "PPER", "VAFIN", "ADV", "ADV", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du bist mir nah!", "tokens": ["Du", "bist", "mir", "nah", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die Sonne sinkt, bald leuchten mir die Sterne.", "tokens": ["Die", "Son\u00b7ne", "sinkt", ",", "bald", "leuch\u00b7ten", "mir", "die", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O w\u00e4rst du da!", "tokens": ["O", "w\u00e4rst", "du", "da", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}