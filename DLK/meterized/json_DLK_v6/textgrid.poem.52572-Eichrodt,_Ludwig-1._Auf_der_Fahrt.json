{"textgrid.poem.52572": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1. Auf der Fahrt", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war eine Zeit, da liebt ich sie,", "tokens": ["Es", "war", "ei\u00b7ne", "Zeit", ",", "da", "liebt", "ich", "sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die herrliche Zeit, ich vergesse sie nie,", "tokens": ["Die", "herr\u00b7li\u00b7che", "Zeit", ",", "ich", "ver\u00b7ges\u00b7se", "sie", "nie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ihr gl\u00fcckliches Herz zu erfreuen,", "tokens": ["Ihr", "gl\u00fcck\u00b7li\u00b7ches", "Herz", "zu", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wie war ichs bem\u00fchet in Treuen!", "tokens": ["Wie", "war", "ichs", "be\u00b7m\u00fc\u00b7het", "in", "Treu\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PIS", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Und h\u00e4tt ich die Sonne, den Mond und die Stern", "tokens": ["Und", "h\u00e4tt", "ich", "die", "Son\u00b7ne", ",", "den", "Mond", "und", "die", "Stern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Damalen besessen, ich h\u00e4tte sie gern", "tokens": ["Da\u00b7ma\u00b7len", "be\u00b7ses\u00b7sen", ",", "ich", "h\u00e4t\u00b7te", "sie", "gern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "$,", "PPER", "VAFIN", "PPER", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Dran geben mit B\u00e4ndern und Schrauben", "tokens": ["Dran", "ge\u00b7ben", "mit", "B\u00e4n\u00b7dern", "und", "Schrau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVINF", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "F\u00fcr meinen unseligen Glauben.", "tokens": ["F\u00fcr", "mei\u00b7nen", "un\u00b7se\u00b7li\u00b7gen", "Glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da r\u00fcckten die fremden Reiter ein,", "tokens": ["Da", "r\u00fcck\u00b7ten", "die", "frem\u00b7den", "Rei\u00b7ter", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da lie\u00df mein Schatz mich stehen allein,", "tokens": ["Da", "lie\u00df", "mein", "Schatz", "mich", "ste\u00b7hen", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und den Wanderstock nahm ich wieder", "tokens": ["Und", "den", "Wan\u00b7der\u00b7stock", "nahm", "ich", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und sung verrufene Lieder.", "tokens": ["Und", "sung", "ver\u00b7ru\u00b7fe\u00b7ne", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Ach, seit ich nimmermehr denk an sie,", "tokens": ["Ach", ",", "seit", "ich", "nim\u00b7mer\u00b7mehr", "denk", "an", "sie", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADV", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ist doch die beste Melodie", "tokens": ["Ist", "doch", "die", "bes\u00b7te", "Me\u00b7lo\u00b7die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus meiner Seele verschwunden,", "tokens": ["Aus", "mei\u00b7ner", "See\u00b7le", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und ich habe sie nimmer gefunden.", "tokens": ["Und", "ich", "ha\u00b7be", "sie", "nim\u00b7mer", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}}}}