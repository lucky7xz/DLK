{"textgrid.poem.38278": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Das St. Hubertuslied", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im gr\u00fcnen Wald bin ich gewesen,", "tokens": ["Im", "gr\u00fc\u00b7nen", "Wald", "bin", "ich", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "PPER", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sah ich es ein Hirschelein stehn;", "tokens": ["Sah", "ich", "es", "ein", "Hir\u00b7sche\u00b7lein", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Das Hirschlein, das wollt ich erschiessen,", "tokens": ["Das", "Hirsc\u00b7hlein", ",", "das", "wollt", "ich", "er\u00b7schies\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VMFIN", "PPER", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "O Wunder, was hab ich gesehn.", "tokens": ["O", "Wun\u00b7der", ",", "was", "hab", "ich", "ge\u00b7sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Es thut mir die Flinte versagen,", "tokens": ["Es", "thut", "mir", "die", "Flin\u00b7te", "ver\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ein Kreutz thut das Hirschelein tragen;", "tokens": ["Ein", "Kreutz", "thut", "das", "Hir\u00b7sche\u00b7lein", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Stolzierend auf seinem Gewicht,", "tokens": ["Stol\u00b7zie\u00b7rend", "auf", "sei\u00b7nem", "Ge\u00b7wicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Die Gnade zum S\u00fcnder wohl spricht.", "tokens": ["Die", "Gna\u00b7de", "zum", "S\u00fcn\u00b7der", "wohl", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Da th\u00e4t ich zur Erden hinsinken,", "tokens": ["Da", "th\u00e4t", "ich", "zur", "Er\u00b7den", "hin\u00b7sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wohl auf meine bogene Knie;", "tokens": ["Wohl", "auf", "mei\u00b7ne", "bo\u00b7ge\u00b7ne", "Knie", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Th\u00e4t mir es entgegen blinken,", "tokens": ["Th\u00e4t", "mir", "es", "ent\u00b7ge\u00b7gen", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein silbernes Kreuzlein schneewei\u00df.", "tokens": ["Ein", "sil\u00b7ber\u00b7nes", "Kreuz\u00b7lein", "schnee\u00b7wei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Jezt thu ich kein Hirschlein mehr schiessen,", "tokens": ["Jezt", "thu", "ich", "kein", "Hirsc\u00b7hlein", "mehr", "schies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Will lieber in's Kloster mich schlie\u00dfen;", "tokens": ["Will", "lie\u00b7ber", "in's", "Klos\u00b7ter", "mich", "schlie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Dem gr\u00fcnen Wald sag ich gut Nacht,", "tokens": ["Dem", "gr\u00fc\u00b7nen", "Wald", "sag", "ich", "gut", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Gnade hat alles gemacht!", "tokens": ["Die", "Gna\u00b7de", "hat", "al\u00b7les", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}