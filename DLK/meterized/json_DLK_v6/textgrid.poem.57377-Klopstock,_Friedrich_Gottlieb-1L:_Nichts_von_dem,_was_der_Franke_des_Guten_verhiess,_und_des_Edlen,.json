{"textgrid.poem.57377": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nichts von dem, was der Franke des Guten verhiess, und des Edlen,", "genre": "verse", "period": "N.A.", "pub_year": 1795, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nichts von dem, was der Franke des Guten verhiess, und des Edlen,", "tokens": ["Nichts", "von", "dem", ",", "was", "der", "Fran\u00b7ke", "des", "Gu\u00b7ten", "ver\u00b7hiess", ",", "und", "des", "Ed\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVFIN", "$,", "KON", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Nichts von Allem diesen geschah;", "tokens": ["Nichts", "von", "Al\u00b7lem", "die\u00b7sen", "ge\u00b7schah", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PIS", "PDAT", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wie es auch mit entz\u00fcckendem Ton die Beredtsamkeit aussprach,", "tokens": ["Wie", "es", "auch", "mit", "ent\u00b7z\u00fc\u00b7cken\u00b7dem", "Ton", "die", "Be\u00b7redt\u00b7sam\u00b7keit", "aus\u00b7sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+--+-+-+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Und die Begeistrung es hob.", "tokens": ["Und", "die", "Be\u00b7geis\u00b7trung", "es", "hob", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Aber alles geschah, was je die st\u00e4rksten der Worte", "tokens": ["A\u00b7ber", "al\u00b7les", "ge\u00b7schah", ",", "was", "je", "die", "st\u00e4rks\u00b7ten", "der", "Wor\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "$,", "PRELS", "ADV", "ART", "VVFIN", "ART", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Schreckliches nanten, oder was nie", "tokens": ["Schreck\u00b7li\u00b7ches", "nan\u00b7ten", ",", "o\u00b7der", "was", "nie"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "VVFIN", "$,", "KON", "PWS", "ADV"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Selbst der Sprachen redendste nicht au nennen verm\u00f6chte,", "tokens": ["Selbst", "der", "Spra\u00b7chen", "re\u00b7dends\u00b7te", "nicht", "au", "nen\u00b7nen", "ver\u00b7m\u00f6ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKNEG", "NE", "VVINF", "VVFIN", "$,"], "meter": "+-+--+--+---+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Alles, alles dieses geschah!", "tokens": ["Al\u00b7les", ",", "al\u00b7les", "die\u00b7ses", "ge\u00b7schah", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "PDAT", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Und je schw\u00e4rzer es war, je grausender, ungeheurer,", "tokens": ["Und", "je", "schw\u00e4r\u00b7zer", "es", "war", ",", "je", "grau\u00b7sen\u00b7der", ",", "un\u00b7ge\u00b7heu\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PPER", "VAFIN", "$,", "ADV", "ADJD", "$,", "ADJA", "$,"], "meter": "--+--+-+--+-+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Desto \u00f6fter geschah's.", "tokens": ["Des\u00b7to", "\u00f6f\u00b7ter", "ge\u00b7scha\u00b7h'", "s."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Ha was w\u00e4hlest du dir, dich zu tr\u00f6sten? blutige Thr\u00e4nen?", "tokens": ["Ha", "was", "w\u00e4h\u00b7lest", "du", "dir", ",", "dich", "zu", "tr\u00f6s\u00b7ten", "?", "blu\u00b7ti\u00b7ge", "Thr\u00e4\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VVFIN", "PPER", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Oder der Franken ewigen Hass?", "tokens": ["O\u00b7der", "der", "Fran\u00b7ken", "e\u00b7wi\u00b7gen", "Hass", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "\u00bbnein, die Thr\u00e4ne nicht, und nicht den Hass. Ich verachte", "tokens": ["\u00bb", "nein", ",", "die", "Thr\u00e4\u00b7ne", "nicht", ",", "und", "nicht", "den", "Hass", ".", "Ich", "ver\u00b7ach\u00b7te"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "PTKANT", "$,", "ART", "NN", "PTKNEG", "$,", "KON", "PTKNEG", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Jeden, der rasen die Rasenden liess.\u00ab", "tokens": ["Je\u00b7den", ",", "der", "ra\u00b7sen", "die", "Ra\u00b7sen\u00b7den", "liess", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "$,", "PRELS", "VVFIN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.15": {"text": "Aber fluchest du nicht den Rasenden? \u00bbWer zum Steine", "tokens": ["A\u00b7ber", "flu\u00b7chest", "du", "nicht", "den", "Ra\u00b7sen\u00b7den", "?", "\u00bb", "Wer", "zum", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$.", "$(", "PWS", "APPRART", "NN"], "meter": "+-+-+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "Wurde, verstumt.\u00ab", "tokens": ["Wur\u00b7de", ",", "ver\u00b7stumt", ".", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "punct"], "pos": ["VAFIN", "$,", "VVPP", "$.", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "H\u00e4tt' ich euch nur nicht ger\u00fchrt, ihr Saiten, die von der vertilgten", "tokens": ["H\u00e4tt'", "ich", "euch", "nur", "nicht", "ge\u00b7r\u00fchrt", ",", "ihr", "Sai\u00b7ten", ",", "die", "von", "der", "ver\u00b7tilg\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "PTKNEG", "VVPP", "$,", "PPOSAT", "NN", "$,", "PRELS", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-+--++-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.18": {"text": "Freyheit sangen, und gleich", "tokens": ["Frey\u00b7heit", "san\u00b7gen", ",", "und", "gleich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "KON", "ADV"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.19": {"text": "T\u00f6nten dem ernsten klagenden Bach, der mit der Zipresse", "tokens": ["T\u00f6n\u00b7ten", "dem", "erns\u00b7ten", "kla\u00b7gen\u00b7den", "Bach", ",", "der", "mit", "der", "Zip\u00b7res\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+--+-+--+-+-+-+", "measure": "iambic.septa.invert"}, "line.20": {"text": "Neben Begrabenen rauscht.", "tokens": ["Ne\u00b7ben", "Be\u00b7gra\u00b7be\u00b7nen", "rauscht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.21": {"text": "Denn ihr strebtet umsonst den tiefgetrofnen zu heilen;", "tokens": ["Denn", "ihr", "streb\u00b7tet", "um\u00b7sonst", "den", "tief\u00b7ge\u00b7trof\u00b7nen", "zu", "hei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.22": {"text": "Risset die Wunde nur auf.", "tokens": ["Ris\u00b7set", "die", "Wun\u00b7de", "nur", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.23": {"text": "Wer an dem Fr\u00fchlingsmorgen der neugeborenen Freyheit", "tokens": ["Wer", "an", "dem", "Fr\u00fch\u00b7lings\u00b7mor\u00b7gen", "der", "neu\u00b7ge\u00b7bo\u00b7re\u00b7nen", "Frey\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Meine Freuden empfand,", "tokens": ["Mei\u00b7ne", "Freu\u00b7den", "emp\u00b7fand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.25": {"text": "Der allein, und kein anderer f\u00fchlt den innigen Schmerz auch,", "tokens": ["Der", "al\u00b7lein", ",", "und", "kein", "an\u00b7de\u00b7rer", "f\u00fchlt", "den", "in\u00b7ni\u00b7gen", "Schmerz", "auch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "KON", "PIAT", "PIS", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Welcher jetzo die Seele mir tr\u00fcbt.", "tokens": ["Wel\u00b7cher", "jet\u00b7zo", "die", "See\u00b7le", "mir", "tr\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.27": {"text": "O verg\u00e4ss' ich auf immer! Denn Linderung wird mir, so lang mich", "tokens": ["O", "ver\u00b7g\u00e4ss'", "ich", "auf", "im\u00b7mer", "!", "Denn", "Lin\u00b7de\u00b7rung", "wird", "mir", ",", "so", "lang", "mich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ADV", "$.", "KON", "NN", "VAFIN", "PPER", "$,", "ADV", "ADJD", "PPER"], "meter": "+-+--+--+--+-++-", "measure": "trochaic.septa.relaxed"}, "line.28": {"text": "K\u00fchlet ein Trunk aus Lethe gesch\u00f6pft.", "tokens": ["K\u00fch\u00b7let", "ein", "Trunk", "aus", "Le\u00b7the", "ge\u00b7sch\u00f6pft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NE", "VVPP", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}}}}