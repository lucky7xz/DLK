{"textgrid.poem.53605": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Absage", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch einmal? Ich d\u00e4chte, wir h\u00e4tten jetzt Frieden?", "tokens": ["Noch", "ein\u00b7mal", "?", "Ich", "d\u00e4ch\u00b7te", ",", "wir", "h\u00e4t\u00b7ten", "jetzt", "Frie\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "\u00dcber Gesetze wird friedlich entschieden . . .", "tokens": ["\u00dc\u00b7ber", "Ge\u00b7set\u00b7ze", "wird", "fried\u00b7lich", "ent\u00b7schie\u00b7den", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "NN", "VAFIN", "ADJD", "VVPP", "$.", "$.", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Ein Stra\u00dfensturm auf ein Parlament", "tokens": ["Ein", "Stra\u00b7\u00dfen\u00b7sturm", "auf", "ein", "Par\u00b7la\u00b7ment"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "ist kein Argument.", "tokens": ["ist", "kein", "Ar\u00b7gu\u00b7ment", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Diese Matrosen sind keine Matrosen.", "tokens": ["Die\u00b7se", "Mat\u00b7ro\u00b7sen", "sind", "kei\u00b7ne", "Mat\u00b7ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "+-++--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Dazwischen Schw\u00e4rme von Arbeitslosen.", "tokens": ["Da\u00b7zwi\u00b7schen", "Schw\u00e4r\u00b7me", "von", "Ar\u00b7beits\u00b7lo\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "APPR", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "K\u00e4mpfer. Banausen. Neugierige. Mob.", "tokens": ["K\u00e4mp\u00b7fer", ".", "Ba\u00b7nau\u00b7sen", ".", "Neu\u00b7gie\u00b7ri\u00b7ge", ".", "Mob", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "NN", "$.", "NN", "$.", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Nun aber stop \u2013!", "tokens": ["Nun", "a\u00b7ber", "stop", "\u2013", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "ADJD", "$(", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Das Parlament ist ein Spiegel des Landes.", "tokens": ["Das", "Par\u00b7la\u00b7ment", "ist", "ein", "Spie\u00b7gel", "des", "Lan\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da sitzen Vertreter jeden Standes.", "tokens": ["Da", "sit\u00b7zen", "Ver\u00b7tre\u00b7ter", "je\u00b7den", "Stan\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PIAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Will euch die Politik verdrie\u00dfen \u2013:", "tokens": ["Will", "euch", "die", "Po\u00b7li\u00b7tik", "ver\u00b7drie\u00b7\u00dfen", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$(", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00e4hlen! Nicht schie\u00dfen!", "tokens": ["W\u00e4h\u00b7len", "!", "Nicht", "schie\u00b7\u00dfen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "PTKNEG", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Eine Gasse der Freiheit \u2013 nicht eine Gosse!", "tokens": ["Ei\u00b7ne", "Gas\u00b7se", "der", "Frei\u00b7heit", "\u2013", "nicht", "ei\u00b7ne", "Gos\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "PTKNEG", "ART", "NN", "$."], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "R\u00fcckt ab von jenem L\u00e4rmmachertrosse!", "tokens": ["R\u00fcckt", "ab", "von", "je\u00b7nem", "L\u00e4rm\u00b7ma\u00b7cher\u00b7tros\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wir brauchen Ruhezeit. So wird das nie", "tokens": ["Wir", "brau\u00b7chen", "Ru\u00b7he\u00b7zeit", ".", "So", "wird", "das", "nie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$.", "ADV", "VAFIN", "PDS", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "eine Demokratie \u2013!", "tokens": ["ei\u00b7ne", "De\u00b7mo\u00b7kra\u00b7tie", "\u2013", "!"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ART", "NN", "$(", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}