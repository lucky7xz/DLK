{"textgrid.poem.35779": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Sorge, die mich niederdr\u00fcckt,", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Sorge, die mich niederdr\u00fcckt,", "tokens": ["O", "Sor\u00b7ge", ",", "die", "mich", "nie\u00b7der\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Sorge, weiche fern:", "tokens": ["O", "Sor\u00b7ge", ",", "wei\u00b7che", "fern", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Vater, der die Bl\u00fcmlein schm\u00fcckt,", "tokens": ["Mein", "Va\u00b7ter", ",", "der", "die", "Bl\u00fcm\u00b7lein", "schm\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der kleidet mich auch gern.", "tokens": ["Der", "klei\u00b7det", "mich", "auch", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bin ich auch traurig und verwais't,", "tokens": ["Bin", "ich", "auch", "trau\u00b7rig", "und", "ver\u00b7wais't", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist Tisch und Kammer leer:", "tokens": ["Ist", "Tisch", "und", "Kam\u00b7mer", "leer", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Vater, der die V\u00f6glein speist,", "tokens": ["Mein", "Va\u00b7ter", ",", "der", "die", "V\u00f6\u00b7glein", "speist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der l\u00e4\u00dft mich nimmermehr.", "tokens": ["Der", "l\u00e4\u00dft", "mich", "nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was ist denn noch, das mich betr\u00fcbt", "tokens": ["Was", "ist", "denn", "noch", ",", "das", "mich", "be\u00b7tr\u00fcbt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Diesseits der stillen Gruft?", "tokens": ["Dies\u00b7seits", "der", "stil\u00b7len", "Gruft", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wei\u00df, da\u00df mich der Vater liebt", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "mich", "der", "Va\u00b7ter", "liebt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und einst hin\u00fcber ruft.", "tokens": ["Und", "einst", "hin\u00b7\u00fc\u00b7ber", "ruft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dort werd' ich meinen Heiland sehn,", "tokens": ["Dort", "werd'", "ich", "mei\u00b7nen", "Hei\u00b7land", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Thron der Gnade knien;", "tokens": ["Am", "Thron", "der", "Gna\u00b7de", "kni\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dort werd' ich mehr als hier verstehn,", "tokens": ["Dort", "werd'", "ich", "mehr", "als", "hier", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "KOKOM", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dort werd' ich frischer bl\u00fchn.", "tokens": ["Dort", "werd'", "ich", "fri\u00b7scher", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ein St\u00fcndlein noch, dann ist er aus,", "tokens": ["Ein", "St\u00fcnd\u00b7lein", "noch", ",", "dann", "ist", "er", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ADV", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Traum, der Leben hei\u00dft;", "tokens": ["Der", "Traum", ",", "der", "Le\u00b7ben", "hei\u00dft", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann schwingt sich in sein ewig Haus", "tokens": ["Dann", "schwingt", "sich", "in", "sein", "e\u00b7wig", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Gott-vers\u00f6hnte Geist.", "tokens": ["Der", "Got\u00b7tver\u00b7s\u00f6hn\u00b7te", "Geist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dann hab' ich, was kein Ohr geh\u00f6rt,", "tokens": ["Dann", "hab'", "ich", ",", "was", "kein", "Ohr", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Auge je gesehn.", "tokens": ["Kein", "Au\u00b7ge", "je", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dort werd' ich selig und verkl\u00e4rt", "tokens": ["Dort", "werd'", "ich", "se\u00b7lig", "und", "ver\u00b7kl\u00e4rt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit heil'gen Engeln gehn.", "tokens": ["Mit", "heil'\u00b7gen", "En\u00b7geln", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wohlauf, mein Herz, und sei vergn\u00fcgt", "tokens": ["Wohl\u00b7auf", ",", "mein", "Herz", ",", "und", "sei", "ver\u00b7gn\u00fcgt"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$,", "KON", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schwing' dich himmelan!", "tokens": ["Und", "schwing'", "dich", "him\u00b7me\u00b7lan", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie Gott, der Herr, dein Leben f\u00fcgt,", "tokens": ["Wie", "Gott", ",", "der", "Herr", ",", "dein", "Le\u00b7ben", "f\u00fcgt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "ART", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So ist es wohlgethan.", "tokens": ["So", "ist", "es", "wohl\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}