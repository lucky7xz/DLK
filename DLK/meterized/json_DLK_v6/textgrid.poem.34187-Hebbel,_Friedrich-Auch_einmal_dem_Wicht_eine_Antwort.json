{"textgrid.poem.34187": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auch einmal dem Wicht eine Antwort", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein erb\u00e4rmlicher Wicht, der meinen Angelo gestern", "tokens": ["Ein", "er\u00b7b\u00e4rm\u00b7li\u00b7cher", "Wicht", ",", "der", "mei\u00b7nen", "An\u00b7ge\u00b7lo", "ge\u00b7stern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVINF"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Hoch bis zum Himmel erhob, heute mit F\u00fc\u00dfen ihn tritt,", "tokens": ["Hoch", "bis", "zum", "Him\u00b7mel", "er\u00b7hob", ",", "heu\u00b7te", "mit", "F\u00fc\u00b7\u00dfen", "ihn", "tritt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "APPRART", "NN", "VVFIN", "$,", "ADV", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Tadelt mich, da\u00df ich nicht schl\u00e4frig im Zimmer sitze und br\u00fcte,", "tokens": ["Ta\u00b7delt", "mich", ",", "da\u00df", "ich", "nicht", "schl\u00e4f\u00b7rig", "im", "Zim\u00b7mer", "sit\u00b7ze", "und", "br\u00fc\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "ADJD", "APPRART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Sondern die freie Natur suche, wie Kinder die Brust.", "tokens": ["Son\u00b7dern", "die", "frei\u00b7e", "Na\u00b7tur", "su\u00b7che", ",", "wie", "Kin\u00b7der", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,", "PWAV", "NN", "ART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Freund, das find' ich doch ", "tokens": ["Freund", ",", "das", "find'", "ich", "doch"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PDS", "VVFIN", "PPER", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Ja, ich schweife herum, ganz, wie der alte Homer,", "tokens": ["Ja", ",", "ich", "schwei\u00b7fe", "he\u00b7rum", ",", "ganz", ",", "wie", "der", "al\u00b7te", "Ho\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PTKVZ", "$,", "ADV", "$,", "PWAV", "ART", "ADJA", "NE", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Mein ist das erste der Veilchen und mein die letzte der Astern,", "tokens": ["Mein", "ist", "das", "ers\u00b7te", "der", "Veil\u00b7chen", "und", "mein", "die", "letz\u00b7te", "der", "As\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "ADJA", "ART", "NN", "KON", "PPOSAT", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Regen sogar und Sturm halten mich selten zu Haus!", "tokens": ["Re\u00b7gen", "so\u00b7gar", "und", "Sturm", "hal\u00b7ten", "mich", "sel\u00b7ten", "zu", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "NN", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}, "line.9": {"text": "Aber, wo h\u00f6rtest du denn, da\u00df Mauern und W\u00e4nde den Dichtern", "tokens": ["A\u00b7ber", ",", "wo", "h\u00f6r\u00b7test", "du", "denn", ",", "da\u00df", "Mau\u00b7ern", "und", "W\u00e4n\u00b7de", "den", "Dich\u00b7tern"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "NN", "KON", "NN", "ART", "NN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Je als Musen gedient, oder der Druckergesell?", "tokens": ["Je", "als", "Mu\u00b7sen", "ge\u00b7dient", ",", "o\u00b7der", "der", "Dru\u00b7cker\u00b7ge\u00b7sell", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "VVPP", "$,", "KON", "ART", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Niemals sa\u00dfen sie noch geb\u00fcckt vor hungrigen Bogen,", "tokens": ["Nie\u00b7mals", "sa\u00b7\u00dfen", "sie", "noch", "ge\u00b7b\u00fcckt", "vor", "hung\u00b7ri\u00b7gen", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Aufgekrempelt den Arm, wie es dem Weber geb\u00fchrt!", "tokens": ["Auf\u00b7ge\u00b7krem\u00b7pelt", "den", "Arm", ",", "wie", "es", "dem", "We\u00b7ber", "ge\u00b7b\u00fchrt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Nein, sie lauschten den Wellen, sie horchten dem Brausen des Windes,", "tokens": ["Nein", ",", "sie", "lauschten", "den", "Wel\u00b7len", ",", "sie", "horch\u00b7ten", "dem", "Brau\u00b7sen", "des", "Win\u00b7des", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Und ein Lilienblatt reichte als T\u00e4felchen aus.", "tokens": ["Und", "ein", "Li\u00b7li\u00b7en\u00b7blatt", "reich\u00b7te", "als", "T\u00e4\u00b7fel\u00b7chen", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KOUS", "NN", "PTKVZ", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}