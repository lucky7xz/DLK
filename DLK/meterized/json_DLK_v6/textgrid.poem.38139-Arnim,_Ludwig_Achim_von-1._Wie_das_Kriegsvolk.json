{"textgrid.poem.38139": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1. Wie das Kriegsvolk", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Georg von Freundsberg, von gro\u00dfer St\u00e4rk,", "tokens": ["Ge\u00b7org", "von", "Freunds\u00b7berg", ",", "von", "gro\u00b7\u00dfer", "St\u00e4rk", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein theurer Held, behielt das Feld,", "tokens": ["Ein", "theu\u00b7rer", "Held", ",", "be\u00b7hielt", "das", "Feld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Streit und Fehd, den Feind besteht,", "tokens": ["In", "Streit", "und", "Fehd", ",", "den", "Feind", "be\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In aller Schlacht er Gott zulegt die Ehr und Macht.", "tokens": ["In", "al\u00b7ler", "Schlacht", "er", "Gott", "zu\u00b7legt", "die", "Ehr", "und", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPER", "NN", "VVFIN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er \u00fcberwand mit eigner Hand", "tokens": ["Er", "\u00fc\u00b7ber\u00b7wand", "mit", "eig\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Venedisch Pracht, der Schweizer Macht,", "tokens": ["Ve\u00b7ne\u00b7disch", "Pracht", ",", "der", "Schwei\u00b7zer", "Macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Franz\u00f6sisch Schaar legt nieder gar,", "tokens": ["Fran\u00b7z\u00f6\u00b7sisch", "Schaar", "legt", "nie\u00b7der", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PTKVZ", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit grosser Schlacht den p\u00e4bstschen Bund zu Schanden macht.", "tokens": ["Mit", "gros\u00b7ser", "Schlacht", "den", "p\u00e4bst\u00b7schen", "Bund", "zu", "Schan\u00b7den", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Kaiser Ehr, macht er stets mehr,", "tokens": ["Der", "Kai\u00b7ser", "Ehr", ",", "macht", "er", "stets", "mehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Ihr Land und Leut besch\u00fczt allzeit,", "tokens": ["Ihr", "Land", "und", "Leut", "be\u00b7sch\u00fczt", "all\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit gro\u00dfer Gefahr er sieghaft war,", "tokens": ["Mit", "gro\u00b7\u00dfer", "Ge\u00b7fahr", "er", "sieg\u00b7haft", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ganz ehrenreich, man findt nicht bald, der ihm sey gleich.", "tokens": ["Ganz", "eh\u00b7ren\u00b7reich", ",", "man", "findt", "nicht", "bald", ",", "der", "ihm", "sey", "gleich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PIS", "VVFIN", "PTKNEG", "ADV", "$,", "PRELS", "PPER", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}