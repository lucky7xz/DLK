{"textgrid.poem.27468": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Verfehlter Beruf", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Konnte denn die Nadel dich nicht, nicht der Hobel ern\u00e4hren,", "tokens": ["Konn\u00b7te", "denn", "die", "Na\u00b7del", "dich", "nicht", ",", "nicht", "der", "Ho\u00b7bel", "er\u00b7n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "PPER", "PTKNEG", "$,", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Da\u00df du mit Metaphysik stiehlst ein abscheuliches Brot?", "tokens": ["Da\u00df", "du", "mit", "Me\u00b7ta\u00b7phy\u00b7sik", "stiehlst", "ein", "ab\u00b7scheu\u00b7li\u00b7ches", "Brot", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+---+", "measure": "unknown.measure.hexa"}}, "stanza.2": {"line.1": {"text": "Was mich bewegt, das Kleine mit Spott und mit Ernst zu verfolgen?", "tokens": ["Was", "mich", "be\u00b7wegt", ",", "das", "Klei\u00b7ne", "mit", "Spott", "und", "mit", "Ernst", "zu", "ver\u00b7fol\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "ART", "ADJA", "APPR", "NN", "KON", "APPR", "NE", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Weil es das Kleine nur ist, welches das Gro\u00dfe verdr\u00e4ngt.", "tokens": ["Weil", "es", "das", "Klei\u00b7ne", "nur", "ist", ",", "wel\u00b7ches", "das", "Gro\u00b7\u00dfe", "ver\u00b7dr\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "ADV", "VAFIN", "$,", "PRELS", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}}}}