{"dta.poem.21358": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die K\u00f6nigstochter seufzte tief:", "tokens": ["Die", "K\u00f6\u00b7nig\u00b7stoch\u00b7ter", "seufz\u00b7te", "tief", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn das meine Mutter w\u00fc\u00dfte!", "tokens": ["Wenn", "das", "mei\u00b7ne", "Mut\u00b7ter", "w\u00fc\u00df\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Pferdekopf herunter rief:", "tokens": ["Der", "Pfer\u00b7de\u00b7kopf", "her\u00b7un\u00b7ter", "rief", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APZR", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Herze brechen m\u00fc\u00dfte!", "tokens": ["Ihr", "Her\u00b7ze", "bre\u00b7chen", "m\u00fc\u00df\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mit stockendem Athem horchte ich hin,", "tokens": ["Mit", "sto\u00b7cken\u00b7dem", "A\u00b7them", "horch\u00b7te", "ich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wenn die Alte ernster und leiser", "tokens": ["Wenn", "die", "Al\u00b7te", "erns\u00b7ter", "und", "lei\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "KON", "ADJA"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zu sprechen begann und vom Rothbart sprach,", "tokens": ["Zu", "spre\u00b7chen", "be\u00b7gann", "und", "vom", "Roth\u00b7bart", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "KON", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Von unserem heimlichen Kaiser.", "tokens": ["Von", "un\u00b7se\u00b7rem", "heim\u00b7li\u00b7chen", "Kai\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Sie hat mir versichert, er sey nicht todt,", "tokens": ["Sie", "hat", "mir", "ver\u00b7si\u00b7chert", ",", "er", "sey", "nicht", "todt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie da glauben die Gelehrten,", "tokens": ["Wie", "da", "glau\u00b7ben", "die", "Ge\u00b7lehr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Er hause versteckt in einem Berg", "tokens": ["Er", "hau\u00b7se", "ver\u00b7steckt", "in", "ei\u00b7nem", "Berg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit seinen Waffengef\u00e4hrten.", "tokens": ["Mit", "sei\u00b7nen", "Waf\u00b7fen\u00b7ge\u00b7f\u00e4hr\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Kiffh\u00e4user ist der Berg genannt,", "tokens": ["Kiff\u00b7h\u00e4u\u00b7ser", "ist", "der", "Berg", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und drinnen ist eine H\u00f6hle;", "tokens": ["Und", "drin\u00b7nen", "ist", "ei\u00b7ne", "H\u00f6h\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Ampeln erhellen so geisterhaft", "tokens": ["Die", "Am\u00b7peln", "er\u00b7hel\u00b7len", "so", "geis\u00b7ter\u00b7haft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die hochgew\u00f6lbten S\u00e4\u00e4le.", "tokens": ["Die", "hoch\u00b7ge\u00b7w\u00f6lb\u00b7ten", "S\u00e4\u00b7\u00e4\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}