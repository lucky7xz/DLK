{"textgrid.poem.39405": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einsam in der Felsenh\u00f6hle,", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einsam in der Felsenh\u00f6hle,", "tokens": ["Ein\u00b7sam", "in", "der", "Fel\u00b7sen\u00b7h\u00f6h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tiefen Ernst in keuscher Seele,", "tokens": ["Tie\u00b7fen", "Ernst", "in", "keu\u00b7scher", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wohnte Ph\u00f6bus Priesterin.", "tokens": ["Wohn\u00b7te", "Ph\u00f6\u00b7bus", "Pries\u00b7te\u00b7rin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Oft, in stiller N\u00e4chte H\u00fcllen,", "tokens": ["Oft", ",", "in", "stil\u00b7ler", "N\u00e4ch\u00b7te", "H\u00fcl\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nahte sich der Gott Sibyllen,", "tokens": ["Nah\u00b7te", "sich", "der", "Gott", "Si\u00b7byl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu erleuchten ihren Sinn.", "tokens": ["Zu", "er\u00b7leuch\u00b7ten", "ih\u00b7ren", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Staunend fiel sie vor ihm nieder,", "tokens": ["Stau\u00b7nend", "fiel", "sie", "vor", "ihm", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihr erschauerten die Glieder,", "tokens": ["Ihr", "er\u00b7schau\u00b7er\u00b7ten", "die", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Die der hohe Gast durchdrang.", "tokens": ["Die", "der", "ho\u00b7he", "Gast", "durch\u00b7drang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sie \u00f6ffnete die Lippen,", "tokens": ["Und", "sie", "\u00f6ff\u00b7ne\u00b7te", "die", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und es schollen rings die Klippen", "tokens": ["Und", "es", "schol\u00b7len", "rings", "die", "Klip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Von prophetischem Gesang.", "tokens": ["Von", "pro\u00b7phe\u00b7ti\u00b7schem", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf geweihte Palmenbl\u00e4tter", "tokens": ["Auf", "ge\u00b7weih\u00b7te", "Pal\u00b7men\u00b7bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Grub sie dann den Spruch der G\u00f6tter,", "tokens": ["Grub", "sie", "dann", "den", "Spruch", "der", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von Apoll ihr offenbart.", "tokens": ["Von", "A\u00b7poll", "ihr", "of\u00b7fen\u00b7bart", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPER", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Vieler Menschen S\u00f6hne kamen,", "tokens": ["Vie\u00b7ler", "Men\u00b7schen", "S\u00f6h\u00b7ne", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Fragten, lasen, und vernahmen,", "tokens": ["Frag\u00b7ten", ",", "la\u00b7sen", ",", "und", "ver\u00b7nah\u00b7men", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was der Zukunft Schoo\u00df bewahrt.", "tokens": ["Was", "der", "Zu\u00b7kunft", "Schoo\u00df", "be\u00b7wahrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber \u00f6fters fuhr der Fl\u00fcgel", "tokens": ["A\u00b7ber", "\u00f6f\u00b7ters", "fuhr", "der", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eines Sturmwinds, trotz dem Riegel", "tokens": ["Ei\u00b7nes", "Sturm\u00b7winds", ",", "trotz", "dem", "Rie\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihrer Pforte, durch die Gruft,", "tokens": ["Ih\u00b7rer", "Pfor\u00b7te", ",", "durch", "die", "Gruft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, und ri\u00df die leichten Bl\u00e4tter", "tokens": ["Ach", ",", "und", "ri\u00df", "die", "leich\u00b7ten", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ohne Schutz und ohne Retter", "tokens": ["Oh\u00b7ne", "Schutz", "und", "oh\u00b7ne", "Ret\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sausend in die \u00f6de Luft.", "tokens": ["Sau\u00b7send", "in", "die", "\u00f6\u00b7de", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Prophetin, unbek\u00fcmmert", "tokens": ["Die", "Pro\u00b7phe\u00b7tin", ",", "un\u00b7be\u00b7k\u00fcm\u00b7mert"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um ihr Werk, vom Sturm zertr\u00fcmmert,", "tokens": ["Um", "ihr", "Werk", ",", "vom", "Sturm", "zer\u00b7tr\u00fcm\u00b7mert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Haschte keines je zur\u00fcck.", "tokens": ["Haschte", "kei\u00b7nes", "je", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wer von ihr in bangen N\u00f6then", "tokens": ["Wer", "von", "ihr", "in", "ban\u00b7gen", "N\u00f6\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Trost gehofft und Trost gebeten,", "tokens": ["Trost", "ge\u00b7hofft", "und", "Trost", "ge\u00b7be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fluchte dann auf sein Geschick. ", "tokens": ["Fluch\u00b7te", "dann", "auf", "sein", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Weisheit l\u00e4\u00dft mit sich nicht scherzen;", "tokens": ["Weis\u00b7heit", "l\u00e4\u00dft", "mit", "sich", "nicht", "scher\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PRF", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Menschen, haltet fest im Herzen", "tokens": ["Men\u00b7schen", ",", "hal\u00b7tet", "fest", "im", "Her\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Orakel der Vernunft.", "tokens": ["Die", "O\u00b7ra\u00b7kel", "der", "Ver\u00b7nunft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weh, wenn vor der L\u00fcste Toben", "tokens": ["Weh", ",", "wenn", "vor", "der", "L\u00fcs\u00b7te", "To\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ma\u00df und Ordnung weggestoben!", "tokens": ["Ma\u00df", "und", "Ord\u00b7nung", "weg\u00b7ge\u00b7sto\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hoffet keine Wiederkunft.", "tokens": ["Hof\u00b7fet", "kei\u00b7ne", "Wie\u00b7der\u00b7kunft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}