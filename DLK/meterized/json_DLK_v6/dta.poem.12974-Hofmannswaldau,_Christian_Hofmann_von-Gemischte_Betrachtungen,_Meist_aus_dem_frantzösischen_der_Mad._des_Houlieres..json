{"dta.poem.12974": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Gemischte Betrachtungen,  \n Meist aus dem frantz\u00f6sischen der   Mad.  \n des Houlieres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Klagst du, o mensch! den tod, wenn er zum hertzen drin-", "tokens": ["Klagst", "du", ",", "o", "mensch", "!", "den", "tod", ",", "wenn", "er", "zum", "hert\u00b7zen", "drin"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "FM", "FM", "$.", "ART", "NN", "$,", "KOUS", "PPER", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "get,", "tokens": ["get", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Der \u00fcbereilung an, so kennft du ihn noch nicht:", "tokens": ["Der", "\u00fc\u00b7be\u00b7rei\u00b7lung", "an", ",", "so", "kennft", "du", "ihn", "noch", "nicht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er kommt mit dir zur welt; Das allererste licht", "tokens": ["Er", "kommt", "mit", "dir", "zur", "welt", ";", "Das", "al\u00b7le\u00b7rers\u00b7te", "licht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Schleppt seinen schatten nach: Sein arm, der alles zwinget,", "tokens": ["Schleppt", "sei\u00b7nen", "schat\u00b7ten", "nach", ":", "Sein", "arm", ",", "der", "al\u00b7les", "zwin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "VVFIN", "PTKVZ", "$.", "PPOSAT", "ADJD", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Macht, da\u00df dein hochmuth ihm best\u00e4ndig zinsen mu\u00df.", "tokens": ["Macht", ",", "da\u00df", "dein", "hoch\u00b7muth", "ihm", "be\u00b7st\u00e4n\u00b7dig", "zin\u00b7sen", "mu\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPOSAT", "NN", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Der mensch hat, eh er stirbt, zu sterben angefangen:", "tokens": ["Der", "mensch", "hat", ",", "eh", "er", "stirbt", ",", "zu", "ster\u00b7ben", "an\u00b7ge\u00b7fan\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der letzte stos, wodurch wir in die grufft gelangen,", "tokens": ["Der", "letz\u00b7te", "stos", ",", "wo\u00b7durch", "wir", "in", "die", "grufft", "ge\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ist wahrlich nicht der tod; es ist nur sein beschlu\u00df.", "tokens": ["Ist", "wahr\u00b7lich", "nicht", "der", "tod", ";", "es", "ist", "nur", "sein", "be\u00b7schlu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ART", "NN", "$.", "PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}