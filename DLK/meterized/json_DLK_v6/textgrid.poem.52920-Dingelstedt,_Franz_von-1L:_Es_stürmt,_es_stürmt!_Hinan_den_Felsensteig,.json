{"textgrid.poem.52920": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es st\u00fcrmt, es st\u00fcrmt! Hinan den Felsensteig,", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es st\u00fcrmt, es st\u00fcrmt! Hinan den Felsensteig,", "tokens": ["Es", "st\u00fcrmt", ",", "es", "st\u00fcrmt", "!", "Hi\u00b7nan", "den", "Fel\u00b7sen\u00b7steig", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$.", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Blick in die Nacht, du L\u00e4sterer, und neige", "tokens": ["Blick", "in", "die", "Nacht", ",", "du", "L\u00e4s\u00b7te\u00b7rer", ",", "und", "nei\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PPER", "NN", "$,", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zur Erde dich, vor Freud' und Schrecken bleich, \u2013", "tokens": ["Zur", "Er\u00b7de", "dich", ",", "vor", "Freud'", "und", "Schre\u00b7cken", "bleich", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "PPER", "$,", "APPR", "NN", "KON", "NN", "ADJD", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das ist das Meer! Nun sieh und beb' und schweige!", "tokens": ["Das", "ist", "das", "Meer", "!", "Nun", "sieh", "und", "beb'", "und", "schwei\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$.", "ADV", "VVFIN", "KON", "VVFIN", "KON", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie weit wirft es die Wellen-Kronen fort,", "tokens": ["Wie", "weit", "wirft", "es", "die", "Wel\u00b7len\u00b7Kro\u00b7nen", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie r\u00fcttelt's an der morschen Felsenkammer!", "tokens": ["Wie", "r\u00fct\u00b7telt's", "an", "der", "mor\u00b7schen", "Fel\u00b7sen\u00b7kam\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es \u00e4chzt das Schifflein selbst im sichren Port", "tokens": ["Es", "\u00e4chzt", "das", "Schif\u00b7flein", "selbst", "im", "sich\u00b7ren", "Port"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und h\u00e4lt sich fester an des Ankers Klammer.", "tokens": ["Und", "h\u00e4lt", "sich", "fes\u00b7ter", "an", "des", "An\u00b7kers", "Klam\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ist's eine Woge, die gen Himmel rennt,", "tokens": ["Ist's", "ei\u00b7ne", "Wo\u00b7ge", ",", "die", "gen", "Him\u00b7mel", "rennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist's eine Wolke, die zum Meere regnet?", "tokens": ["Ist's", "ei\u00b7ne", "Wol\u00b7ke", ",", "die", "zum", "Mee\u00b7re", "reg\u00b7net", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du wei\u00dft es nicht; es haben ungetrennt", "tokens": ["Du", "wei\u00dft", "es", "nicht", ";", "es", "ha\u00b7ben", "un\u00b7ge\u00b7trennt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sich Meer und Himmel br\u00fcderlich begegnet.", "tokens": ["Sich", "Meer", "und", "Him\u00b7mel", "br\u00fc\u00b7der\u00b7lich", "be\u00b7geg\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "KON", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Zermalmt es nicht, entfesselt' Riesenpaar,", "tokens": ["Zer\u00b7malmt", "es", "nicht", ",", "ent\u00b7fes\u00b7selt'", "Rie\u00b7sen\u00b7paar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Kindlein, das in Euren Armen zittert,", "tokens": ["Das", "Kin\u00b7dlein", ",", "das", "in", "Eu\u00b7ren", "Ar\u00b7men", "zit\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "La\u00dft stehn die H\u00fctten, die so manches Jahr", "tokens": ["La\u00dft", "stehn", "die", "H\u00fct\u00b7ten", ",", "die", "so", "man\u00b7ches", "Jahr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In Eurem Grimme furchtsam sind verwittert!", "tokens": ["In", "Eu\u00b7rem", "Grim\u00b7me", "furcht\u00b7sam", "sind", "ver\u00b7wit\u00b7tert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der Leuchtturm schwankt, die Glocke dr\u00f6hnt im Turm,", "tokens": ["Der", "Leucht\u00b7turm", "schwankt", ",", "die", "Glo\u00b7cke", "dr\u00f6hnt", "im", "Turm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Insel sch\u00fcttert, \u2013 Herr, es geht zu Ende!", "tokens": ["Die", "In\u00b7sel", "sch\u00fct\u00b7tert", ",", "\u2013", "Herr", ",", "es", "geht", "zu", "En\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "$(", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sieh her, mein Volk, das ist Dein Meer im Sturm,", "tokens": ["Sieh", "her", ",", "mein", "Volk", ",", "das", "ist", "Dein", "Meer", "im", "Sturm", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nun hebe betend die gebundnen H\u00e4nde!", "tokens": ["Nun", "he\u00b7be", "be\u00b7tend", "die", "ge\u00b7bund\u00b7nen", "H\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}