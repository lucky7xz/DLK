{"textgrid.poem.65244": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Est Est!", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hart an dem Bolsener See,", "tokens": ["Hart", "an", "dem", "Bol\u00b7se\u00b7ner", "See", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Auf des Flaschenberges H\u00f6h',", "tokens": ["Auf", "des", "Fla\u00b7schen\u00b7ber\u00b7ges", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Steht ein kleiner Leichenstein", "tokens": ["Steht", "ein", "klei\u00b7ner", "Lei\u00b7chen\u00b7stein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit der kurzen Inschrift drein:", "tokens": ["Mit", "der", "kur\u00b7zen", "In\u00b7schrift", "drein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Unter diesem Monument,", "tokens": ["Un\u00b7ter", "die\u00b7sem", "Mo\u00b7nu\u00b7ment", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Welches keinen Namen nennt,", "tokens": ["Wel\u00b7ches", "kei\u00b7nen", "Na\u00b7men", "nennt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruht ein Herr von deutschem Blut,", "tokens": ["Ruht", "ein", "Herr", "von", "deut\u00b7schem", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deutschem Schlund und deutschem Muth,", "tokens": ["Deut\u00b7schem", "Schlund", "und", "deut\u00b7schem", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der hier starb den sch\u00f6nsten Tod \u2013", "tokens": ["Der", "hier", "starb", "den", "sch\u00f6ns\u00b7ten", "Tod", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Seine Schuld vergeb' ihm Gott!", "tokens": ["Sei\u00b7ne", "Schuld", "ver\u00b7geb'", "ihm", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Als er reist' im welschen Land,", "tokens": ["Als", "er", "reist'", "im", "wel\u00b7schen", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vielen schlechten Wein er fand,", "tokens": ["Vie\u00b7len", "schlech\u00b7ten", "Wein", "er", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welcher leicht, wie Wasser, wog", "tokens": ["Wel\u00b7cher", "leicht", ",", "wie", "Was\u00b7ser", ",", "wog"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWAT", "ADJD", "$,", "PWAV", "NN", "$,", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Lippen schief ihm zog;", "tokens": ["Und", "die", "Lip\u00b7pen", "schief", "ihm", "zog", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Und er rief: Ich halt's nicht aus!", "tokens": ["Und", "er", "rief", ":", "Ich", "halt's", "nicht", "aus", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Lieber Knappe, reit' voraus!", "tokens": ["Lie\u00b7ber", "Knap\u00b7pe", ",", "reit'", "vo\u00b7raus", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sprich in jedem Wirthshaus ein,", "tokens": ["Sprich", "in", "je\u00b7dem", "Wirths\u00b7haus", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und probire jeden Wein;", "tokens": ["Und", "pro\u00b7bi\u00b7re", "je\u00b7den", "Wein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo er dir zum besten schmeckt,", "tokens": ["Wo", "er", "dir", "zum", "bes\u00b7ten", "schmeckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei f\u00fcr mich der Tisch gedeckt,", "tokens": ["Sei", "f\u00fcr", "mich", "der", "Tisch", "ge\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und damit ich find' das Nest,", "tokens": ["Und", "da\u00b7mit", "ich", "find'", "das", "Nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Schreib' an's Thor mir an ein ", "tokens": ["Schreib'", "an's", "Thor", "mir", "an", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "PPER", "APPR", "ART"], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Und der Knappe ritt voran,", "tokens": ["Und", "der", "Knap\u00b7pe", "ritt", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Hielt vor jedem Schenkhaus an,", "tokens": ["Hielt", "vor", "je\u00b7dem", "Schenk\u00b7haus", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trank ein Glas von jedem Wein,", "tokens": ["Trank", "ein", "Glas", "von", "je\u00b7dem", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "War der gut, so kehrt' er ein,", "tokens": ["War", "der", "gut", ",", "so", "kehrt'", "er", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "War der schlecht, so sprengt' er fort,", "tokens": ["War", "der", "schlecht", ",", "so", "sprengt'", "er", "fort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis er fand den rechten Ort.", "tokens": ["Bis", "er", "fand", "den", "rech\u00b7ten", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Also kam er nach der Stadt,", "tokens": ["Al\u00b7so", "kam", "er", "nach", "der", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die den Muskateller hat,", "tokens": ["Die", "den", "Mus\u00b7ka\u00b7tel\u00b7ler", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der im ganzen welschen Land", "tokens": ["Der", "im", "gan\u00b7zen", "wel\u00b7schen", "Land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "ADJA", "PWAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr den besten wird genannt;", "tokens": ["F\u00fcr", "den", "bes\u00b7ten", "wird", "ge\u00b7nannt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als von diesem trank der Knecht,", "tokens": ["Als", "von", "die\u00b7sem", "trank", "der", "Knecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PDAT", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "D\u00fcnkt' ", "tokens": ["D\u00fcnkt'"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}}, "stanza.7": {"line.1": {"text": "Und mit feuerrothem Stift,", "tokens": ["Und", "mit", "feu\u00b7er\u00b7ro\u00b7them", "Stift", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mit riesengro\u00dfer Schrift,", "tokens": ["Und", "mit", "rie\u00b7sen\u00b7gro\u00b7\u00dfer", "Schrift", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Malt er nach des Weins Geb\u00fchr", "tokens": ["Malt", "er", "nach", "des", "Weins", "Ge\u00b7b\u00fchr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ja, nach anderem Bericht", "tokens": ["Ja", ",", "nach", "an\u00b7de\u00b7rem", "Be\u00b7richt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fehlt die dritte Silbe nicht.", "tokens": ["Fehlt", "die", "drit\u00b7te", "Sil\u00b7be", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Der Herr Ritter kam, sah, trank,", "tokens": ["Der", "Herr", "Rit\u00b7ter", "kam", ",", "sah", ",", "trank", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis er todt zu Boden sank.", "tokens": ["Bis", "er", "todt", "zu", "Bo\u00b7den", "sank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schenke, Schenkin, Kellner, Knapp'", "tokens": ["Schen\u00b7ke", ",", "Schen\u00b7kin", ",", "Kell\u00b7ner", ",", "Knapp'"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gruben ihm ein sch\u00f6nes Grab,", "tokens": ["Gru\u00b7ben", "ihm", "ein", "sch\u00f6\u00b7nes", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hart an dem Bolsener See,", "tokens": ["Hart", "an", "dem", "Bol\u00b7se\u00b7ner", "See", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Auf des Flaschenberges H\u00f6h'.", "tokens": ["Auf", "des", "Fla\u00b7schen\u00b7ber\u00b7ges", "H\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Und sein Knapp', der Kostewein,", "tokens": ["Und", "sein", "Knapp'", ",", "der", "Kos\u00b7te\u00b7wein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Setzt' ihm einen Leichenstein,", "tokens": ["Setzt'", "ihm", "ei\u00b7nen", "Lei\u00b7chen\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne Wappen, Stern und Hut,", "tokens": ["Oh\u00b7ne", "Wap\u00b7pen", ",", "Stern", "und", "Hut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit der Inschrift kurz und gut:", "tokens": ["Mit", "der", "In\u00b7schrift", "kurz", "und", "gut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Als ich nach dem Berge kam,", "tokens": ["Als", "ich", "nach", "dem", "Ber\u00b7ge", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine Flasch' ich zu mir nahm,", "tokens": ["Ei\u00b7ne", "Fla\u00b7sch'", "ich", "zu", "mir", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und die zweite trug ich fort", "tokens": ["Und", "die", "zwei\u00b7te", "trug", "ich", "fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "VVFIN", "PPER", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach dem weltber\u00fchmten Ort,", "tokens": ["Nach", "dem", "welt\u00b7be\u00b7r\u00fchm\u00b7ten", "Ort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo der deutsche Ritter liegt,", "tokens": ["Wo", "der", "deut\u00b7sche", "Rit\u00b7ter", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der vom ", "tokens": ["Der", "vom"], "token_info": ["word", "word"], "pos": ["ART", "APPRART"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.11": {"line.1": {"text": "Selig preis' ich deine Ruh',", "tokens": ["Se\u00b7lig", "preis'", "ich", "dei\u00b7ne", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Alter, guter Freiherr du,", "tokens": ["Al\u00b7ter", ",", "gu\u00b7ter", "Frei\u00b7herr", "du", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der du hier gefallen bist", "tokens": ["Der", "du", "hier", "ge\u00b7fal\u00b7len", "bist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "VVPP", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von dem Trank, der doppelt ", "tokens": ["Von", "dem", "Trank", ",", "der", "dop\u00b7pelt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Doppelt ist in Kraft und Gluth,", "tokens": ["Dop\u00b7pelt", "ist", "in", "Kraft", "und", "Gluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "NN", "KON", "NN", "$,"], "m