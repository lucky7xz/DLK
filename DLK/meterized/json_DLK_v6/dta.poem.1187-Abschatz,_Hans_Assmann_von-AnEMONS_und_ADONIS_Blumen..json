{"dta.poem.1187": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "AnEMONS  \n und  \n  ADONIS  \n Blumen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Nach aller meiner Pein/ nach aller meiner Noth/", "tokens": ["Nach", "al\u00b7ler", "mei\u00b7ner", "Pein", "/", "nach", "al\u00b7ler", "mei\u00b7ner", "Noth", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$(", "APPR", "PIAT", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dadurch ich nur verbittert deine Sinnen/", "tokens": ["Da\u00b7durch", "ich", "nur", "ver\u00b7bit\u00b7tert", "dei\u00b7ne", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hab ich gelernt die Kunst dich zu gewinnen/", "tokens": ["Hab", "ich", "ge\u00b7lernt", "die", "Kunst", "dich", "zu", "ge\u00b7win\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "ART", "NN", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Fillis/ ich geh\u2019 in Tod.", "tokens": ["Fil\u00b7lis", "/", "ich", "geh'", "in", "Tod", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Fillis/ thu ich zuviel/ wenn ich mich untersteh/", "tokens": ["Fil\u00b7lis", "/", "thu", "ich", "zu\u00b7viel", "/", "wenn", "ich", "mich", "un\u00b7ter\u00b7steh", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVFIN", "PPER", "PIS", "$(", "KOUS", "PPER", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich dir recht gethan/ f\u00fcr aller Welt zu sagen:", "tokens": ["Da\u00df", "ich", "dir", "recht", "ge\u00b7than", "/", "f\u00fcr", "al\u00b7ler", "Welt", "zu", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVPP", "$(", "APPR", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Augenblick kan mich und dich vertragen:", "tokens": ["Ein", "Au\u00b7gen\u00b7blick", "kan", "mich", "und", "dich", "ver\u00b7tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich geh in Tod: Ade!", "tokens": ["Ich", "geh", "in", "Tod", ":", "A\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$.", "NN", "$."], "meter": "-+-++-", "measure": "unknown.measure.tri"}}}}}