{"textgrid.poem.58842": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "61. Glaubenswandel", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du mu\u00dft Gott folgen ohn' Verstehen,", "tokens": ["Du", "mu\u00dft", "Gott", "fol\u00b7gen", "ohn'", "Ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VVINF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht immer f\u00fchlen, schmecken, sehen.", "tokens": ["Nicht", "im\u00b7mer", "f\u00fch\u00b7len", ",", "schme\u00b7cken", ",", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "$,", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wer glaubet, der gibt Gott die Ehr';", "tokens": ["Wer", "glau\u00b7bet", ",", "der", "gibt", "Gott", "die", "Ehr'", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PRELS", "VVFIN", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gef\u00e4llst du ihm, was willst du mehr?", "tokens": ["Ge\u00b7f\u00e4llst", "du", "ihm", ",", "was", "willst", "du", "mehr", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}