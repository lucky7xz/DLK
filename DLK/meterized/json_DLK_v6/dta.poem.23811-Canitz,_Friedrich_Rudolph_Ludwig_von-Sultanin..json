{"dta.poem.23811": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Sultanin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Man zittert nun nicht mehr vor Ketten und vor Banden/", "tokens": ["Man", "zit\u00b7tert", "nun", "nicht", "mehr", "vor", "Ket\u00b7ten", "und", "vor", "Ban\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PTKNEG", "ADV", "APPR", "NN", "KON", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist in der Barbarey ein solches Bild vorhanden/", "tokens": ["Ist", "in", "der", "Bar\u00b7ba\u00b7rey", "ein", "sol\u00b7ches", "Bild", "vor\u00b7han\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "PIAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So wird dort mit der Zeit an Fesseln Mangel seyn/", "tokens": ["So", "wird", "dort", "mit", "der", "Zeit", "an", "Fes\u00b7seln", "Man\u00b7gel", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "ART", "NN", "APPR", "NN", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn wer nur sehen darf stellt sich zum Sclaven ein.", "tokens": ["Denn", "wer", "nur", "se\u00b7hen", "darf", "stellt", "sich", "zum", "Scla\u00b7ven", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVINF", "VMFIN", "VVFIN", "PRF", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}