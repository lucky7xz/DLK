{"textgrid.poem.43983": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: So wist einmahl, ich bin verliebt,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So wist einmahl, ich bin verliebt,", "tokens": ["So", "wist", "ein\u00b7mahl", ",", "ich", "bin", "ver\u00b7liebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und zwar in so ein Kind,", "tokens": ["Und", "zwar", "in", "so", "ein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das mir erst Lust zu leben giebt,", "tokens": ["Das", "mir", "erst", "Lust", "zu", "le\u00b7ben", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So schwer die Zeiten sind.", "tokens": ["So", "schwer", "die", "Zei\u00b7ten", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sein Ku\u00df ist meiner Seelen Kraft", "tokens": ["Sein", "Ku\u00df", "ist", "mei\u00b7ner", "See\u00b7len", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und hat an s\u00fc\u00dfer Glut", "tokens": ["Und", "hat", "an", "s\u00fc\u00b7\u00dfer", "Glut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Fast aller Sch\u00f6nen Eigenschaft,", "tokens": ["Fast", "al\u00b7ler", "Sch\u00f6\u00b7nen", "Ei\u00b7gen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nur nicht den Wanckelmuth.", "tokens": ["Nur", "nicht", "den", "Wan\u00b7ckel\u00b7muth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es schw\u00e4cht mir weder Geist noch Leib,", "tokens": ["Es", "schw\u00e4cht", "mir", "we\u00b7der", "Geist", "noch", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das denen sonst geschieht,", "tokens": ["Das", "de\u00b7nen", "sonst", "ge\u00b7schieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Amors stiller Zeitvertreib", "tokens": ["Die", "A\u00b7mors", "stil\u00b7ler", "Zeit\u00b7ver\u00b7treib"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NE", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am Narrenseile zieht;", "tokens": ["Am", "Nar\u00b7ren\u00b7sei\u00b7le", "zieht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es redet mir in Lust und Leid", "tokens": ["Es", "re\u00b7det", "mir", "in", "Lust", "und", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So klug als freundlich ein", "tokens": ["So", "klug", "als", "freund\u00b7lich", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "ADJD", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und l\u00e4st mich in der nechsten Zeit", "tokens": ["Und", "l\u00e4st", "mich", "in", "der", "nechs\u00b7ten", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des Unsterns Meister seyn.", "tokens": ["Des", "Uns\u00b7terns", "Meis\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Weicht, Eltern, G\u00f6nner, Gl\u00fcck und Freund,", "tokens": ["Weicht", ",", "El\u00b7tern", ",", "G\u00f6n\u00b7ner", ",", "Gl\u00fcck", "und", "Freund", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weicht, sag ich, immerhin,", "tokens": ["Weicht", ",", "sag", "ich", ",", "im\u00b7mer\u00b7hin", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr habt es nie so treu gemeint,", "tokens": ["Ihr", "habt", "es", "nie", "so", "treu", "ge\u00b7meint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als ich euch jezt noch bin;", "tokens": ["Als", "ich", "euch", "jezt", "noch", "bin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Inde\u00dfen, da euch vor mir graut,", "tokens": ["In\u00b7de\u00b7\u00dfen", ",", "da", "euch", "vor", "mir", "graut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So lern ich euch verschm\u00e4hn", "tokens": ["So", "lern", "ich", "euch", "ver\u00b7schm\u00e4hn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und dencke, mit der neuen Braut", "tokens": ["Und", "den\u00b7cke", ",", "mit", "der", "neu\u00b7en", "Braut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mich be\u00dfer zu versehn.", "tokens": ["Mich", "be\u00b7\u00dfer", "zu", "ver\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ach Hofnung, ach du Engelsbild", "tokens": ["Ach", "Hof\u00b7nung", ",", "ach", "du", "En\u00b7gels\u00b7bild"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "NN", "$,", "XY", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und meiner G\u00fcter Rest,", "tokens": ["Und", "mei\u00b7ner", "G\u00fc\u00b7ter", "Rest", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach, komm und k\u00fc\u00df und bleib mein Schild,", "tokens": ["Ach", ",", "komm", "und", "k\u00fc\u00df", "und", "bleib", "mein", "Schild", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "KON", "ADJD", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da alles schl\u00e4gt und pre\u00dft.", "tokens": ["Da", "al\u00b7les", "schl\u00e4gt", "und", "pre\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Komm, flicht uns unsern Hochzeitschmuck", "tokens": ["Komm", ",", "flicht", "uns", "un\u00b7sern", "Hoch\u00b7zeit\u00b7schmuck"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von deinem Wintergr\u00fcn!", "tokens": ["Von", "dei\u00b7nem", "Win\u00b7ter\u00b7gr\u00fcn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Tod, sonst nichts, ist starck genug,", "tokens": ["Der", "Tod", ",", "sonst", "nichts", ",", "ist", "starck", "ge\u00b7nug", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PIS", "$,", "VAFIN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ihn wieder aufzuziehn.", "tokens": ["Ihn", "wie\u00b7der", "auf\u00b7zu\u00b7ziehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}