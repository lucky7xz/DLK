{"textgrid.poem.42520": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Lebenskampf", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie kann das Leben anders sich verknoten,", "tokens": ["Wie", "kann", "das", "Le\u00b7ben", "an\u00b7ders", "sich", "ver\u00b7kno\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als eine Welt des Kampfes und der Schmerzen.", "tokens": ["Als", "ei\u00b7ne", "Welt", "des", "Kamp\u00b7fes", "und", "der", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn Fr\u00fchlingsschein und Sommerfarben lohten,", "tokens": ["Wenn", "Fr\u00fch\u00b7lings\u00b7schein", "und", "Som\u00b7mer\u00b7far\u00b7ben", "loh\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es wird sich bald der blaue Himmel schw\u00e4rzen.", "tokens": ["Es", "wird", "sich", "bald", "der", "blau\u00b7e", "Him\u00b7mel", "schw\u00e4r\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und ob von Rittern oder von Heloten:", "tokens": ["Und", "ob", "von", "Rit\u00b7tern", "o\u00b7der", "von", "He\u00b7lo\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ringsum der Feind, dein Dasein auszumerzen.", "tokens": ["Ring\u00b7sum", "der", "Feind", ",", "dein", "Da\u00b7sein", "aus\u00b7zu\u00b7mer\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PPOSAT", "NN", "VVIZU", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Getrost! Spartaner, nur dreihundert, boten", "tokens": ["Ge\u00b7trost", "!", "Spar\u00b7ta\u00b7ner", ",", "nur", "drei\u00b7hun\u00b7dert", ",", "bo\u00b7ten"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$.", "NN", "$,", "ADV", "CARD", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Viel tausend Pfeilen ihre Griechenherzen.", "tokens": ["Viel", "tau\u00b7send", "Pfei\u00b7len", "ih\u00b7re", "Grie\u00b7chen\u00b7her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}