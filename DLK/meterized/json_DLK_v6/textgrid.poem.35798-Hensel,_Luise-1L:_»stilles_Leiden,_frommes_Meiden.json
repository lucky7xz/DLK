{"textgrid.poem.35798": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbstilles Leiden, frommes Meiden", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbstilles Leiden, frommes Meiden", "tokens": ["\u00bb", "stil\u00b7les", "Lei\u00b7den", ",", "from\u00b7mes", "Mei\u00b7den"], "token_info": ["punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aller Erdenpracht und Lust,", "tokens": ["Al\u00b7ler", "Er\u00b7den\u00b7pracht", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heilig Lieben, stetes Ueben", "tokens": ["Hei\u00b7lig", "Lie\u00b7ben", ",", "ste\u00b7tes", "Ue\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei fortan dir nur bewu\u00dft.", "tokens": ["Sei", "for\u00b7tan", "dir", "nur", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Weinend b\u00fc\u00dfen ", "tokens": ["Wei\u00b7nend", "b\u00fc\u00b7\u00dfen"], "token_info": ["word", "word"], "pos": ["ADJD", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Der der S\u00fcnderin vergab,", "tokens": ["Der", "der", "S\u00fcn\u00b7de\u00b7rin", "ver\u00b7gab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heilig Sehnen, fromme Thr\u00e4nen", "tokens": ["Hei\u00b7lig", "Seh\u00b7nen", ",", "from\u00b7me", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Waschen deine Flecken ab.", "tokens": ["Wa\u00b7schen", "dei\u00b7ne", "Fle\u00b7cken", "ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Beten, Leiden, muthig Streiten", "tokens": ["Be\u00b7ten", ",", "Lei\u00b7den", ",", "mut\u00b7hig", "Strei\u00b7ten"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gegen Teufel, Welt und Lust,", "tokens": ["Ge\u00b7gen", "Teu\u00b7fel", ",", "Welt", "und", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese Waffen nur verschaffen", "tokens": ["Die\u00b7se", "Waf\u00b7fen", "nur", "ver\u00b7schaf\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieg und Ruhe deiner Brust.\u00ab \u2013", "tokens": ["Sieg", "und", "Ru\u00b7he", "dei\u00b7ner", "Brust", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "KON", "NN", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Hab' Erbarmen mit der Armen,", "tokens": ["Hab'", "Er\u00b7bar\u00b7men", "mit", "der", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab', o Herr! mit mir Geduld!", "tokens": ["Hab'", ",", "o", "Herr", "!", "mit", "mir", "Ge\u00b7duld", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$.", "APPR", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach nicht richte \u2013 nein, vernichte,", "tokens": ["Ach", "nicht", "rich\u00b7te", "\u2013", "nein", ",", "ver\u00b7nich\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ITJ", "PTKNEG", "VVFIN", "$(", "PTKANT", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vater! meine gro\u00dfe Schuld.", "tokens": ["Va\u00b7ter", "!", "mei\u00b7ne", "gro\u00b7\u00dfe", "Schuld", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sieh im Herzen meine Schmerzen;", "tokens": ["Sieh", "im", "Her\u00b7zen", "mei\u00b7ne", "Schmer\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sende Deines Geistes Licht;", "tokens": ["Sen\u00b7de", "Dei\u00b7nes", "Geis\u00b7tes", "Licht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stetes Weinen soll mich reinen:", "tokens": ["Ste\u00b7tes", "Wei\u00b7nen", "soll", "mich", "rei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df mich sehn Dein Angesicht!", "tokens": ["La\u00df", "mich", "sehn", "Dein", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sieh, ich habe Dir zur Gabe", "tokens": ["Sieh", ",", "ich", "ha\u00b7be", "Dir", "zur", "Ga\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VAFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ganz geopfert dieses Herz;", "tokens": ["Ganz", "ge\u00b7o\u00b7pfert", "die\u00b7ses", "Herz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich werde von der Erde", "tokens": ["Und", "ich", "wer\u00b7de", "von", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selig schweben himmelw\u00e4rts.", "tokens": ["Se\u00b7lig", "schwe\u00b7ben", "him\u00b7mel\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.7": {"line.1": {"text": "Mit Dir ringend, Dich bezwingend", "tokens": ["Mit", "Dir", "rin\u00b7gend", ",", "Dich", "be\u00b7zwin\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "VVPP", "$,", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Halt' ich, wie einst Jacob, Dich.", "tokens": ["Halt'", "ich", ",", "wie", "einst", "Ja\u00b7cob", ",", "Dich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PWAV", "ADV", "NE", "$,", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimmst in Deinen Himmel mich.", "tokens": ["Nimmst", "in", "Dei\u00b7nen", "Him\u00b7mel", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}