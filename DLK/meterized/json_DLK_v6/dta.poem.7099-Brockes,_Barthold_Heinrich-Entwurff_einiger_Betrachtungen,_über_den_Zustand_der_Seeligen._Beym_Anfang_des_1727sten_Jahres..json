{"dta.poem.7099": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df solche Lust nun sonder Ende w\u00e4hren,", "tokens": ["Da\u00df", "sol\u00b7che", "Lust", "nun", "son\u00b7der", "En\u00b7de", "w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ja gar in Ewigkeit sich immer noch vermehren,", "tokens": ["Ja", "gar", "in", "E\u00b7wig\u00b7keit", "sich", "im\u00b7mer", "noch", "ver\u00b7meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "APPR", "NN", "PRF", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und gr\u00f6sser werden kann, auch gr\u00f6sser werden wird,", "tokens": ["Und", "gr\u00f6s\u00b7ser", "wer\u00b7den", "kann", ",", "auch", "gr\u00f6s\u00b7ser", "wer\u00b7den", "wird", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAINF", "VMFIN", "$,", "ADV", "ADJD", "VAINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil GOTT unendlich ist im Wollen und im K\u00f6nnen:", "tokens": ["Weil", "GoTT", "un\u00b7end\u00b7lich", "ist", "im", "Wol\u00b7len", "und", "im", "K\u00f6n\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADJD", "VAFIN", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ist wol ein Inbegriff zu nennen", "tokens": ["Ist", "wol", "ein", "In\u00b7be\u00b7griff", "zu", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von jener Seeligkeit.", "tokens": ["Von", "je\u00b7ner", "See\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}