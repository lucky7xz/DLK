{"textgrid.poem.65868": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "142", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein S\u00fcndigen ist Lieb', und Ha\u00df dein Tugendstreben,", "tokens": ["Mein", "S\u00fcn\u00b7di\u00b7gen", "ist", "Lieb'", ",", "und", "Ha\u00df", "dein", "Tu\u00b7gend\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "KON", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ha\u00df meiner S\u00fcnd', in s\u00fcnd'ger Lieb' ern\u00e4hrt.", "tokens": ["Ha\u00df", "mei\u00b7ner", "S\u00fcnd'", ",", "in", "s\u00fcn\u00b7d'\u00b7ger", "Lieb'", "er\u00b7n\u00e4hrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "O nimm mein Tun, und stelle deins darneben,", "tokens": ["O", "nimm", "mein", "Tun", ",", "und", "stel\u00b7le", "deins", "dar\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "NE", "PAV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So findest du es nimmer scheltenswert.", "tokens": ["So", "fin\u00b7dest", "du", "es", "nim\u00b7mer", "schel\u00b7tens\u00b7wert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Der, seinen Scharlachschmuck entheiligend", "tokens": ["Der", ",", "sei\u00b7nen", "Schar\u00b7lach\u00b7schmuck", "en\u00b7thei\u00b7li\u00b7gend"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So oft als meiner, falschen Liebesbund", "tokens": ["So", "oft", "als", "mei\u00b7ner", ",", "fal\u00b7schen", "Lie\u00b7bes\u00b7bund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "KOKOM", "PPOSAT", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Besiegelt, fremden Bettes Schwur getrennt.", "tokens": ["Be\u00b7sie\u00b7gelt", ",", "frem\u00b7den", "Bet\u00b7tes", "Schwur", "ge\u00b7trennt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJA", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "La\u00df mich mit Fug dich hegen, wie du ", "tokens": ["La\u00df", "mich", "mit", "Fug", "dich", "he\u00b7gen", ",", "wie", "du"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "NN", "PPER", "VVFIN", "$,", "PWAV", "PPER"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "An\u00e4ugelst, die du liebst, wie ich dich hege;", "tokens": ["An\u00b7\u00e4u\u00b7gelst", ",", "die", "du", "liebst", ",", "wie", "ich", "dich", "he\u00b7ge", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.11": {"text": "Pflanz' in dein Herz Erbarmen, da\u00df es bl\u00fch',", "tokens": ["Pflanz'", "in", "dein", "Herz", "Er\u00b7bar\u00b7men", ",", "da\u00df", "es", "bl\u00fch'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und dir dein Mitleid Mitleid ernten m\u00f6ge!", "tokens": ["Und", "dir", "dein", "Mit\u00b7leid", "Mit\u00b7leid", "ern\u00b7ten", "m\u00f6\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Willst du genie\u00dfen, wo du nie erh\u00f6rt,", "tokens": ["Willst", "du", "ge\u00b7nie\u00b7\u00dfen", ",", "wo", "du", "nie", "er\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Bleib' es, nach eignem Beispiel, dir verwehrt.", "tokens": ["Bleib'", "es", ",", "nach", "eig\u00b7nem", "Bei\u00b7spiel", ",", "dir", "ver\u00b7wehrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}