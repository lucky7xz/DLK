{"textgrid.poem.49177": {"metadata": {"author": {"name": "Lautensack, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Magd", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist ganz Magd, ganz Dienerin. Von Antlitz", "tokens": ["Du", "bist", "ganz", "Magd", ",", "ganz", "Die\u00b7ne\u00b7rin", ".", "Von", "Ant\u00b7litz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$,", "ADV", "NN", "$.", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "gemei\u00dfelt nicht so sehr wie Meine Herrin;", "tokens": ["ge\u00b7mei\u00b7\u00dfelt", "nicht", "so", "sehr", "wie", "Mei\u00b7ne", "Her\u00b7rin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PTKNEG", "ADV", "ADV", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "und auch an Leib nicht allsosehr verfertigt", "tokens": ["und", "auch", "an", "Leib", "nicht", "all\u00b7so\u00b7sehr", "ver\u00b7fer\u00b7tigt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN", "PTKNEG", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "von K\u00fcnstlerhand, so Elfenbein und Silber", "tokens": ["von", "K\u00fcnst\u00b7ler\u00b7hand", ",", "so", "El\u00b7fen\u00b7bein", "und", "Sil\u00b7ber"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "und Mond.", "tokens": ["und", "Mond", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Dein Haar ist Dienerinnenhaar; wie Herbst", "tokens": ["Dein", "Haar", "ist", "Die\u00b7ne\u00b7rin\u00b7nen\u00b7haar", ";", "wie", "Herbst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$.", "PWAV", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und \u00fcberreife Trauben sind die Br\u00fcste;", "tokens": ["und", "\u00fc\u00b7berr\u00b7ei\u00b7fe", "Trau\u00b7ben", "sind", "die", "Br\u00fcs\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "dein Knie zu wohlig, und dein Scho\u00df zu weh,", "tokens": ["dein", "Knie", "zu", "woh\u00b7lig", ",", "und", "dein", "Scho\u00df", "zu", "weh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKA", "ADJD", "$,", "KON", "PPOSAT", "NN", "PTKZU", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "zu fern das Lachen, gleichwie Tr\u00e4nen um", "tokens": ["zu", "fern", "das", "La\u00b7chen", ",", "gleich\u00b7wie", "Tr\u00e4\u00b7nen", "um"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKA", "ADJD", "ART", "NN", "$,", "KON", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nichts n\u00e4her ...", "tokens": ["Nichts", "n\u00e4\u00b7her", "..."], "token_info": ["word", "word", "punct"], "pos": ["PIS", "ADJD", "$("], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Mich sticht seit sieben Tagen der Geruch", "tokens": ["Mich", "sticht", "seit", "sie\u00b7ben", "Ta\u00b7gen", "der", "Ge\u00b7ruch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "CARD", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "von dir! von deinem Leibe und Gewand", "tokens": ["von", "dir", "!", "von", "dei\u00b7nem", "Lei\u00b7be", "und", "Ge\u00b7wand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$.", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und Bett! wie einen Hund ein Feindsgeruch! ...", "tokens": ["und", "Bett", "!", "wie", "ei\u00b7nen", "Hund", "ein", "Feinds\u00b7ge\u00b7ruch", "!", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "$.", "PWAV", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dein Atem hungert f\u00fchllos, und dein Blut", "tokens": ["Dein", "A\u00b7tem", "hun\u00b7gert", "f\u00fchl\u00b7los", ",", "und", "dein", "Blut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "verzeiht hilflos und tatlos, stumpf, wie tot,", "tokens": ["ver\u00b7zeiht", "hil\u00b7flos", "und", "tat\u00b7los", ",", "stumpf", ",", "wie", "tot", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "$,", "ADJD", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "alles Getane, eh es noch getan ...", "tokens": ["al\u00b7les", "Ge\u00b7ta\u00b7ne", ",", "eh", "es", "noch", "ge\u00b7tan", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}, "line.8": {"text": "Liegts nicht wie Biblisch Land vor uns?", "tokens": ["Liegts", "nicht", "wie", "Bib\u00b7lisch", "Land", "vor", "uns", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "KOKOM", "NN", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Du solltest", "tokens": ["Du", "soll\u00b7test"], "token_info": ["word", "word"], "pos": ["PPER", "VMFIN"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "hoch auf dem hohen, widerwillgen, stolzen", "tokens": ["hoch", "auf", "dem", "ho\u00b7hen", ",", "wi\u00b7der\u00b7will\u00b7gen", ",", "stol\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "$,", "VVFIN", "$,", "VVINF"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "vom HERRN verschlossnen Scho\u00dfe meiner Herrin", "tokens": ["vom", "HeRRN", "ver\u00b7schloss\u00b7nen", "Scho\u00b7\u00dfe", "mei\u00b7ner", "Her\u00b7rin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Mir einen Sohn geb\u00e4ren \u2013 \u2013 \u2013 \u2013", "tokens": ["Mir", "ei\u00b7nen", "Sohn", "ge\u00b7b\u00e4\u00b7ren", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$(", "$(", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}