{"dta.poem.3591": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Schnelle Entwickelung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein junger Mann nahm sich ein Weib,               ", "tokens": ["Ein", "jun\u00b7ger", "Mann", "nahm", "sich", "ein", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Holdselig und gar fein von Leib.", "tokens": ["Hold\u00b7se\u00b7lig", "und", "gar", "fein", "von", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADV", "ADJD", "APPR", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Dem Weib er \u00fcbersah gar viel,", "tokens": ["Dem", "Weib", "er", "\u00fc\u00b7ber\u00b7sah", "gar", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schwieg ihr in allen Dingen still.", "tokens": ["Schwieg", "ihr", "in", "al\u00b7len", "Din\u00b7gen", "still", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Also gewinnt das Weib den Mann,", "tokens": ["Al\u00b7so", "ge\u00b7winnt", "das", "Weib", "den", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Da\u00df er nicht mehr zu Wein gehn kann.", "tokens": ["Da\u00df", "er", "nicht", "mehr", "zu", "Wein", "gehn", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mu\u00df der Gesellen auch ablassen,", "tokens": ["Mu\u00df", "der", "Ge\u00b7sel\u00b7len", "auch", "ab\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Darf nur mit ihr allein noch spassen,", "tokens": ["Darf", "nur", "mit", "ihr", "al\u00b7lein", "noch", "spas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch einsmal seht, da gieng er aus,", "tokens": ["Doch", "eins\u00b7mal", "seht", ",", "da", "gieng", "er", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kam ohngef\u00e4hr vors Schenkwirthshaus.", "tokens": ["Kam", "ohn\u00b7ge\u00b7f\u00e4hr", "vors", "Schenk\u00b7wirths\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Gesellen sein darinnen sassen,", "tokens": ["Ge\u00b7sel\u00b7len", "sein", "da\u00b7rin\u00b7nen", "sas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Recht fr\u00f6hlich tranken, sangen, assen.", "tokens": ["Recht", "fr\u00f6h\u00b7lich", "tran\u00b7ken", ",", "san\u00b7gen", ",", "as\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "$,", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Sie th\u00e4ten ihm gar balde winken,", "tokens": ["Sie", "th\u00e4\u00b7ten", "ihm", "gar", "bal\u00b7de", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der ein stand auf, bot ihm zu trinken.", "tokens": ["Der", "ein", "stand", "auf", ",", "bot", "ihm", "zu", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "VVFIN", "PTKVZ", "$,", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Er sch\u00fcttelte den Kopf und lachte,", "tokens": ["Er", "sch\u00fct\u00b7tel\u00b7te", "den", "Kopf", "und", "lach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Leute grosse Augen machten.", "tokens": ["Die", "Leu\u00b7te", "gros\u00b7se", "Au\u00b7gen", "mach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Der ein f\u00fchrt ihn hinein geschwind,", "tokens": ["Der", "ein", "f\u00fchrt", "ihn", "hin\u00b7ein", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er sizt bei ihnen wie ein Kind.", "tokens": ["Er", "sizt", "bei", "ih\u00b7nen", "wie", "ein", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Es war sein Herz ihm noch so schwer,", "tokens": ["Es", "war", "sein", "Herz", "ihm", "noch", "so", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hub an zu seufzen gar zu sehr.", "tokens": ["Hub", "an", "zu", "seuf\u00b7zen", "gar", "zu", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APZR", "PTKZU", "VVINF", "ADV", "PTKA", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Wie er ans Heimweh nur gedacht,", "tokens": ["Wie", "er", "ans", "Heim\u00b7weh", "nur", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Frau Gesundheit ward gebracht.", "tokens": ["Der", "Frau", "Ge\u00b7sund\u00b7heit", "ward", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Er tranks hinein, er trank es aus,", "tokens": ["Er", "tranks", "hin\u00b7ein", ",", "er", "trank", "es", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dachte gar nicht mehr nach Haus", "tokens": ["Und", "dach\u00b7te", "gar", "nicht", "mehr", "nach", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Sein Glas, das r\u00fcckt er immer vor,", "tokens": ["Sein", "Glas", ",", "das", "r\u00fcckt", "er", "im\u00b7mer", "vor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und war der lauteste im Chor.", "tokens": ["Und", "war", "der", "lau\u00b7tes\u00b7te", "im", "Chor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Doch die Gesellen giengen eben,", "tokens": ["Doch", "die", "Ge\u00b7sel\u00b7len", "gien\u00b7gen", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zwei mu\u00dften ihn nach Hause heben.", "tokens": ["Zwei", "mu\u00df\u00b7ten", "ihn", "nach", "Hau\u00b7se", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Recht mit Gewalt sie mu\u00dften schleppen,", "tokens": ["Recht", "mit", "Ge\u00b7walt", "sie", "mu\u00df\u00b7ten", "schlep\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er st\u00fcrzt hinauf die schmalen Treppen.", "tokens": ["Er", "st\u00fcrzt", "hin\u00b7auf", "die", "schma\u00b7len", "Trep\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Das Weib mit Angst kam angegangen,", "tokens": ["Das", "Weib", "mit", "Angst", "kam", "an\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Ungl\u00fcck meint sie, w\u00e4r ergangen.", "tokens": ["Ein", "Un\u00b7gl\u00fcck", "meint", "sie", ",", "w\u00e4r", "er\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Sie hat die ganze Nacht gewacht,", "tokens": ["Sie", "hat", "die", "gan\u00b7ze", "Nacht", "ge\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und im Gebet an ihn gedacht.", "tokens": ["Und", "im", "Ge\u00b7bet", "an", "ihn", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "Da ist er hart sie angegangen,", "tokens": ["Da", "ist", "er", "hart", "sie", "an\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Schl\u00e4gen hat er sie empfangen.", "tokens": ["Mit", "Schl\u00e4\u00b7gen", "hat", "er", "sie", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.19": {"line.1": {"text": "Was ist f\u00fcr Lehr daraus geflossen,", "tokens": ["Was", "ist", "f\u00fcr", "Lehr", "da\u00b7raus", "ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht jede Eh ist im Himmel geschossen.", "tokens": ["Nicht", "je\u00b7de", "Eh", "ist", "im", "Him\u00b7mel", "ge\u00b7schos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}