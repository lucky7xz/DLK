{"textgrid.poem.53783": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Suomi-Finnland", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie hat, o Deutschland, alles, was verworfen,", "tokens": ["Wie", "hat", ",", "o", "Deutschland", ",", "al\u00b7les", ",", "was", "ver\u00b7wor\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "$,", "FM", "NE", "$,", "PIS", "$,", "PRELS", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sich aufget\u00fcrmt auf deinem Golgatha;", "tokens": ["Sich", "auf\u00b7ge\u00b7t\u00fcrmt", "auf", "dei\u00b7nem", "Gol\u00b7ga\u00b7tha", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "PPOSAT", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Jetzt, wo der Riese endlich umgeworfen,", "tokens": ["Jetzt", ",", "wo", "der", "Rie\u00b7se", "end\u00b7lich", "um\u00b7ge\u00b7wor\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sind vielgesch\u00e4ftig auch die Zwerge da.", "tokens": ["Sind", "viel\u00b7ge\u00b7sch\u00e4f\u00b7tig", "auch", "die", "Zwer\u00b7ge", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O Deutschland, deine Schmach wird bald sich wenden . . .", "tokens": ["O", "Deutschland", ",", "dei\u00b7ne", "Schmach", "wird", "bald", "sich", "wen\u00b7den", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "NE", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "PRF", "VVINF", "$.", "$.", "$."], "meter": "+---+-+-+-", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Vom Berg der Zeiten blickt sie in die Lande", "tokens": ["Vom", "Berg", "der", "Zei\u00b7ten", "blickt", "sie", "in", "die", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Henker, der sein schmachvoll Werk bestellt . . .", "tokens": ["Zum", "Hen\u00b7ker", ",", "der", "sein", "schmach\u00b7voll", "Werk", "be\u00b7stellt", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPOSAT", "ADJD", "NN", "VVPP", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}