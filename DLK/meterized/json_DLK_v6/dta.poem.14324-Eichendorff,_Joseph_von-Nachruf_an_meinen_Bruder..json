{"dta.poem.14324": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Nachruf an meinen Bruder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "So singt, wie Syrenen,", "tokens": ["So", "singt", ",", "wie", "Sy\u00b7re\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "NE", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "Von hellblauen, sch\u00f6nen", "tokens": ["Von", "hell\u00b7blau\u00b7en", ",", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPR", "VVINF", "$,", "ADJA"], "meter": "-++-+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Vergangenen Zeiten,", "tokens": ["Ver\u00b7gan\u00b7ge\u00b7nen", "Zei\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der Abend von weitem,", "tokens": ["Der", "A\u00b7bend", "von", "wei\u00b7tem", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Versinkt dann im T\u00f6nen,", "tokens": ["Ver\u00b7sinkt", "dann", "im", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Erst Busen dann Mund,", "tokens": ["Erst", "Bu\u00b7sen", "dann", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Im bl\u00fchenden Grund.", "tokens": ["Im", "bl\u00fc\u00b7hen\u00b7den", "Grund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "O schweiget Syrenen!", "tokens": ["O", "schwei\u00b7get", "Sy\u00b7re\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}