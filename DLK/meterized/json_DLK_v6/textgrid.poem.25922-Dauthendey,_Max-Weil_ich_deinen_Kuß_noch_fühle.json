{"textgrid.poem.25922": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Weil ich deinen Ku\u00df noch f\u00fchle", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schw\u00fcle geht im Herzen um,", "tokens": ["Schw\u00fc\u00b7le", "geht", "im", "Her\u00b7zen", "um", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil ich deinen Ku\u00df noch f\u00fchle.", "tokens": ["Weil", "ich", "dei\u00b7nen", "Ku\u00df", "noch", "f\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Geh' ums Leben heut herum,", "tokens": ["Geh'", "ums", "Le\u00b7ben", "heut", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00f6cht' kein W\u00f6rtlein von mir geben,", "tokens": ["M\u00f6cht'", "kein", "W\u00f6rt\u00b7lein", "von", "mir", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur das Herz m\u00f6cht' mir entschweben,", "tokens": ["Nur", "das", "Herz", "m\u00f6cht'", "mir", "ent\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lippen blieben gerne stumm.", "tokens": ["Lip\u00b7pen", "blie\u00b7ben", "ger\u00b7ne", "stumm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Tragen von der Liebesstund", "tokens": ["Tra\u00b7gen", "von", "der", "Lie\u00b7bess\u00b7tund"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Noch die s\u00fc\u00dfe Bl\u00fcte und", "tokens": ["Noch", "die", "s\u00fc\u00b7\u00dfe", "Bl\u00fc\u00b7te", "und"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "KON"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Alle Glieder sagen warm:", "tokens": ["Al\u00b7le", "Glie\u00b7der", "sa\u00b7gen", "warm", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Arm macht niemand je mich wieder.", "tokens": ["Arm", "macht", "nie\u00b7mand", "je", "mich", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "ADV", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}