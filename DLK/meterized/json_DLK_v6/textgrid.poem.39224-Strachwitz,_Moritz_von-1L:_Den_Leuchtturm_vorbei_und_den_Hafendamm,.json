{"textgrid.poem.39224": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Den Leuchtturm vorbei und den Hafendamm,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Leuchtturm vorbei und den Hafendamm,", "tokens": ["Den", "Leucht\u00b7turm", "vor\u00b7bei", "und", "den", "Ha\u00b7fen\u00b7damm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O Herrin im S\u00fcden, Ade!", "tokens": ["O", "Her\u00b7rin", "im", "S\u00fc\u00b7den", ",", "A\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "APPRART", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hochspringend \u00fcber den Wellenkamm", "tokens": ["Hoch\u00b7sprin\u00b7gend", "\u00fc\u00b7ber", "den", "Wel\u00b7len\u00b7kamm"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Erhebt sich das meerdurchschweifende,", "tokens": ["Er\u00b7hebt", "sich", "das", "meer\u00b7durch\u00b7schwei\u00b7fen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Schwarzbusige, weitausgreifende,", "tokens": ["Schwarz\u00b7bu\u00b7si\u00b7ge", ",", "weit\u00b7aus\u00b7grei\u00b7fen\u00b7de", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Dampfschnaubende Ro\u00df der See.", "tokens": ["Dampf\u00b7schnau\u00b7ben\u00b7de", "Ro\u00df", "der", "See", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Und zwischen dem Ufer hinterw\u00e4rts,", "tokens": ["Und", "zwi\u00b7schen", "dem", "U\u00b7fer", "hin\u00b7ter\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie wird das Meer so breit!", "tokens": ["Wie", "wird", "das", "Meer", "so", "breit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es st\u00fcrzt sich das Schiff wie ein J\u00fcnglingsherz", "tokens": ["Es", "st\u00fcrzt", "sich", "das", "Schiff", "wie", "ein", "J\u00fcng\u00b7lings\u00b7herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "KOKOM", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Hinaus in das Unerme\u00dfliche;", "tokens": ["Hin\u00b7aus", "in", "das", "Un\u00b7er\u00b7me\u00df\u00b7li\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.5": {"text": "O Herrin, Unverge\u00dfliche,", "tokens": ["O", "Her\u00b7rin", ",", "Un\u00b7ver\u00b7ge\u00df\u00b7li\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Wie bin ich von Dir so weit!", "tokens": ["Wie", "bin", "ich", "von", "Dir", "so", "weit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "APPR", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "O Herrin im S\u00fcd, \u00fcber Nordlandsflut,", "tokens": ["O", "Her\u00b7rin", "im", "S\u00fcd", ",", "\u00fc\u00b7ber", "Nord\u00b7lands\u00b7flut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "APPRART", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wie ein Nordlicht zucke mein Lied", "tokens": ["Wie", "ein", "Nord\u00b7licht", "zu\u00b7cke", "mein", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und zaubere mit Morganas Glut", "tokens": ["Und", "zau\u00b7be\u00b7re", "mit", "Mor\u00b7ga\u00b7nas", "Glut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NE", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Urpl\u00f6tzlich vor die Tr\u00e4umende", "tokens": ["Ur\u00b7pl\u00f6tz\u00b7lich", "vor", "die", "Tr\u00e4u\u00b7men\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Das Meer, das hochaufb\u00e4umende,", "tokens": ["Das", "Meer", ",", "das", "hoch\u00b7auf\u00b7b\u00e4u\u00b7men\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Und den, der dr\u00fcber zieht!", "tokens": ["Und", "den", ",", "der", "dr\u00fc\u00b7ber", "zieht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "PAV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}