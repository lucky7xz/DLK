{"textgrid.poem.50831": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Am Vorderrhein", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ist er rauschend ausgezogen,", "tokens": ["Wie", "ist", "er", "rau\u00b7schend", "aus\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der junge Held, aus Kluft und Stein!", "tokens": ["Der", "jun\u00b7ge", "Held", ",", "aus", "Kluft", "und", "Stein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie hat er gierig eingesogen", "tokens": ["Wie", "hat", "er", "gie\u00b7rig", "ein\u00b7ge\u00b7so\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Milch der Freiheit, frisch und rein!", "tokens": ["Die", "Milch", "der", "Frei\u00b7heit", ",", "frisch", "und", "rein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun wallt der Gl\u00fcckliche hinnieder,", "tokens": ["Nun", "wallt", "der", "Gl\u00fcck\u00b7li\u00b7che", "hin\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hin in mein zweites Heimatland:", "tokens": ["Hin", "in", "mein", "zwei\u00b7tes", "Hei\u00b7mat\u00b7land", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O gr\u00fc\u00df mir alle deutschen Br\u00fcder,", "tokens": ["O", "gr\u00fc\u00df", "mir", "al\u00b7le", "deut\u00b7schen", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die herrlichen, l\u00e4ngs deinem Strand!", "tokens": ["Die", "herr\u00b7li\u00b7chen", ",", "l\u00e4ngs", "dei\u00b7nem", "Strand", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und gr\u00fc\u00df mir alle deutschen Frauen", "tokens": ["Und", "gr\u00fc\u00df", "mir", "al\u00b7le", "deut\u00b7schen", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit deinem besten Ritterbrauch,", "tokens": ["Mit", "dei\u00b7nem", "bes\u00b7ten", "Rit\u00b7ter\u00b7brauch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn du wirst die Dome schauen,", "tokens": ["Und", "wenn", "du", "wirst", "die", "Do\u00b7me", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die lieben K\u00e4uze, gr\u00fc\u00df sie auch!", "tokens": ["Die", "lie\u00b7ben", "K\u00e4u\u00b7ze", ",", "gr\u00fc\u00df", "sie", "auch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sonst w\u00fc\u00dft ich niemand mehr zu gr\u00fc\u00dfen", "tokens": ["Sonst", "w\u00fc\u00dft", "ich", "nie\u00b7mand", "mehr", "zu", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als etwa noch die Lorelei", "tokens": ["Als", "et\u00b7wa", "noch", "die", "Lo\u00b7re\u00b7lei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und deiner Reben freudig Sprie\u00dfen! \u2013", "tokens": ["Und", "dei\u00b7ner", "Re\u00b7ben", "freu\u00b7dig", "Sprie\u00b7\u00dfen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Es taucht ein Aar ins Wolkenlose", "tokens": ["Es", "taucht", "ein", "Aar", "ins", "Wol\u00b7ken\u00b7lo\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hoch \u00fcber mir im Sonnenschein.", "tokens": ["Hoch", "\u00fc\u00b7ber", "mir", "im", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich werfe eine Alpenrose", "tokens": ["Ich", "wer\u00b7fe", "ei\u00b7ne", "Al\u00b7pen\u00b7ro\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tief unten in den wilden Rhein!", "tokens": ["Tief", "un\u00b7ten", "in", "den", "wil\u00b7den", "Rhein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fchr nieder sie, f\u00fchr sie zu Tale,", "tokens": ["F\u00fchr", "nie\u00b7der", "sie", ",", "f\u00fchr", "sie", "zu", "Ta\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "PPER", "$,", "VVFIN", "PPER", "APPR", "NE", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Du sch\u00f6ner Strom, bis an das Meer,", "tokens": ["Du", "sch\u00f6\u00b7ner", "Strom", ",", "bis", "an", "das", "Meer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "KOUS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und weis dem Volk im Eichensaale,", "tokens": ["Und", "weis", "dem", "Volk", "im", "Ei\u00b7chen\u00b7saa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dem harrenden, dies Zeichen schwer!", "tokens": ["Dem", "har\u00b7ren\u00b7den", ",", "dies", "Zei\u00b7chen", "schwer", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PDS", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}