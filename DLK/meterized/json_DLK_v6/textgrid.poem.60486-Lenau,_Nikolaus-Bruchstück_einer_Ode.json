{"textgrid.poem.60486": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Bruchst\u00fcck einer Ode", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Stadt Get\u00f6s ward banges Gemurmel, und", "tokens": ["Der", "Stadt", "Ge\u00b7t\u00f6s", "ward", "ban\u00b7ges", "Ge\u00b7mur\u00b7mel", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJA", "NN", "$,", "KON"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Es schlagen mattern Schlages die Pulse nun", "tokens": ["Es", "schla\u00b7gen", "mat\u00b7tern", "Schla\u00b7ges", "die", "Pul\u00b7se", "nun"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des vielgesch\u00e4ftgen Lebens, \u2013 schrecklich", "tokens": ["Des", "viel\u00b7ge\u00b7sch\u00e4ft\u00b7gen", "Le\u00b7bens", ",", "\u2013", "schreck\u00b7lich"], "token_info": ["word", "word", "word", "punct", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "$(", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hallt noch im Ohre das Donnerwort ihr. \u2013", "tokens": ["Hallt", "noch", "im", "Oh\u00b7re", "das", "Don\u00b7ner\u00b7wort", "ihr", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "PPER", "$.", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Klagt, Glocken, klagt! Der Herrliche sank! mit ihm", "tokens": ["Klagt", ",", "Glo\u00b7cken", ",", "klagt", "!", "Der", "Herr\u00b7li\u00b7che", "sank", "!", "mit", "ihm"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "$.", "ART", "NN", "VVFIN", "$.", "APPR", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das gro\u00dfe Werk des Br\u00fcdervereins der streng", "tokens": ["Das", "gro\u00b7\u00dfe", "Werk", "des", "Br\u00fc\u00b7der\u00b7ver\u00b7eins", "der", "streng"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ART", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Geschiedenen V\u00f6lker! \u2013 H\u00fclle tiefer,", "tokens": ["Ge\u00b7schie\u00b7de\u00b7nen", "V\u00f6l\u00b7ker", "!", "\u2013", "H\u00fcl\u00b7le", "tie\u00b7fer", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "$(", "NN", "ADJD", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Tiefer, o Seele, dich ein in Wehmut!", "tokens": ["Tie\u00b7fer", ",", "o", "See\u00b7le", ",", "dich", "ein", "in", "Weh\u00b7mut", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "FM", "NN", "$,", "PRF", "ART", "APPR", "NN", "$."], "meter": "+--+-++-+-", "measure": "iambic.penta.invert"}}}}}