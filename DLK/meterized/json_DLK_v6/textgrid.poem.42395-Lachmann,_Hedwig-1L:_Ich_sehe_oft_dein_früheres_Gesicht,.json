{"textgrid.poem.42395": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich sehe oft dein fr\u00fcheres Gesicht,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sehe oft dein fr\u00fcheres Gesicht,", "tokens": ["Ich", "se\u00b7he", "oft", "dein", "fr\u00fc\u00b7he\u00b7res", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Geliebter, und es gl\u00fcckt mir, einen raschen", "tokens": ["Ge\u00b7lieb\u00b7ter", ",", "und", "es", "gl\u00fcckt", "mir", ",", "ei\u00b7nen", "ra\u00b7schen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "KON", "PPER", "VVFIN", "PPER", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verlornen Ausdruck, der wie Sonnenlicht", "tokens": ["Ver\u00b7lor\u00b7nen", "Aus\u00b7druck", ",", "der", "wie", "Son\u00b7nen\u00b7licht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "KOKOM", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Vorbeihuscht, im Erinnern zu erhaschen.", "tokens": ["Vor\u00b7bei\u00b7huscht", ",", "im", "E\u00b7rin\u00b7nern", "zu", "er\u00b7ha\u00b7schen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Und manchmal \u00fcberkommt mich wie ein Bangen", "tokens": ["Und", "manch\u00b7mal", "\u00fc\u00b7ber\u00b7kommt", "mich", "wie", "ein", "Ban\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach meinen Kindern, da sie noch ganz klein", "tokens": ["Nach", "mei\u00b7nen", "Kin\u00b7dern", ",", "da", "sie", "noch", "ganz", "klein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und hilflos waren, und der erste Schein", "tokens": ["Und", "hil\u00b7flos", "wa\u00b7ren", ",", "und", "der", "ers\u00b7te", "Schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von z\u00e4rtlichem Verstehen ihre Wangen", "tokens": ["Von", "z\u00e4rt\u00b7li\u00b7chem", "Ver\u00b7ste\u00b7hen", "ih\u00b7re", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sanft \u00fcberflog, und das Nachmirverlangen", "tokens": ["Sanft", "\u00fc\u00b7berf\u00b7log", ",", "und", "das", "Nach\u00b7mir\u00b7ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "KON", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und die Befriedigung, bei mir zu sein.", "tokens": ["Und", "die", "Be\u00b7frie\u00b7di\u00b7gung", ",", "bei", "mir", "zu", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPR", "PPER", "PTKZU", "VAINF", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Und meines Vaters Bild enttaucht den Gr\u00fcnden", "tokens": ["Und", "mei\u00b7nes", "Va\u00b7ters", "Bild", "ent\u00b7taucht", "den", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Ewigkeit, so wie ich ihn gekannt;", "tokens": ["Der", "E\u00b7wig\u00b7keit", ",", "so", "wie", "ich", "ihn", "ge\u00b7kannt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "KOKOM", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und ob dar\u00fcber Jahre auch vergingen,", "tokens": ["Und", "ob", "da\u00b7r\u00fc\u00b7ber", "Jah\u00b7re", "auch", "ver\u00b7gin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PAV", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bin ich oft j\u00e4h in seinen Kreis gebannt,", "tokens": ["Bin", "ich", "oft", "j\u00e4h", "in", "sei\u00b7nen", "Kreis", "ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und seh mit mildem Feuer sich entz\u00fcnden", "tokens": ["Und", "seh", "mit", "mil\u00b7dem", "Feu\u00b7er", "sich", "ent\u00b7z\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Blicke, die so an den Fernen hingen.", "tokens": ["Die", "Bli\u00b7cke", ",", "die", "so", "an", "den", "Fer\u00b7nen", "hin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und seh mich selbst, ein Wesen, das mir glich,", "tokens": ["Und", "seh", "mich", "selbst", ",", "ein", "We\u00b7sen", ",", "das", "mir", "glich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zuerst als Kind, dann jung erbl\u00fcht und allen", "tokens": ["Zu\u00b7erst", "als", "Kind", ",", "dann", "jung", "er\u00b7bl\u00fcht", "und", "al\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "NN", "$,", "ADV", "ADJD", "VVPP", "KON", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verk\u00fcndigungen zugewandt und offen,", "tokens": ["Ver\u00b7k\u00fcn\u00b7di\u00b7gun\u00b7gen", "zu\u00b7ge\u00b7wandt", "und", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vertrauen, Gl\u00fcck begehren, zagen, hoffen \u2013", "tokens": ["Ver\u00b7trau\u00b7en", ",", "Gl\u00fcck", "be\u00b7geh\u00b7ren", ",", "za\u00b7gen", ",", "hof\u00b7fen", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "VVINF", "$,", "ADJA", "$,", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und dann dies alles von mir abgefallen,", "tokens": ["Und", "dann", "dies", "al\u00b7les", "von", "mir", "ab\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PDS", "PIS", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vertraut und fremd und immer doch noch ich.", "tokens": ["Ver\u00b7traut", "und", "fremd", "und", "im\u00b7mer", "doch", "noch", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "KON", "ADV", "ADV", "ADV", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}