{"textgrid.poem.26593": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich habe geliebt", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe geliebt", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7liebt"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Wie Dichter lieben,", "tokens": ["Wie", "Dich\u00b7ter", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und ob ich auch hohes Gl\u00fcck genossen \u2013", "tokens": ["Und", "ob", "ich", "auch", "ho\u00b7hes", "Gl\u00fcck", "ge\u00b7nos\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADJA", "NN", "VVPP", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mehr noch hab' ich gelitten.", "tokens": ["Mehr", "noch", "hab'", "ich", "ge\u00b7lit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Jetzt, da mein Herz steht", "tokens": ["Jetzt", ",", "da", "mein", "Herz", "steht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPOSAT", "NN", "VVFIN"], "meter": "---++", "measure": "unknown.measure.di"}, "line.2": {"text": "In der Sonnenwende der Liebe,", "tokens": ["In", "der", "Son\u00b7nen\u00b7wen\u00b7de", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Erfa\u00dft mich seltsame Wehmuth.", "tokens": ["Er\u00b7fa\u00dft", "mich", "selt\u00b7sa\u00b7me", "Weh\u00b7muth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Empf\u00e4nglich noch f\u00fcr der Sch\u00f6nheit Zauber,", "tokens": ["Emp\u00b7f\u00e4ng\u00b7lich", "noch", "f\u00fcr", "der", "Sch\u00f6n\u00b7heit", "Zau\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und mit gesch\u00e4rftem Aug'", "tokens": ["Und", "mit", "ge\u00b7sch\u00e4rf\u00b7tem", "Aug'"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ersp\u00e4hend den feinsten Reiz,", "tokens": ["Er\u00b7sp\u00e4\u00b7hend", "den", "feins\u00b7ten", "Reiz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Sp\u00fcr' ich auch noch", "tokens": ["Sp\u00fcr'", "ich", "auch", "noch"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Unverbraucht", "tokens": ["Un\u00b7ver\u00b7braucht"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Des J\u00fcnglings Gluth", "tokens": ["Des", "J\u00fcng\u00b7lings", "Gluth"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "In ernst und kraftvoll gereifter Mannesseele.", "tokens": ["In", "ernst", "und", "kraft\u00b7voll", "ge\u00b7reif\u00b7ter", "Man\u00b7nes\u00b7see\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Aber zugleich schon", "tokens": ["A\u00b7ber", "zu\u00b7gleich", "schon"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "F\u00fchl' ich mich angeweht", "tokens": ["F\u00fchl'", "ich", "mich", "an\u00b7ge\u00b7weht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PRF", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Von leisen, mahnungsvollen Schauern", "tokens": ["Von", "lei\u00b7sen", ",", "mah\u00b7nungs\u00b7vol\u00b7len", "Schau\u00b7ern"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "VVINF", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Nahenden Alters,", "tokens": ["Na\u00b7hen\u00b7den", "Al\u00b7ters", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Und jener trostlosen Zeit,", "tokens": ["Und", "je\u00b7ner", "trost\u00b7lo\u00b7sen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.16": {"text": "Wo Eros oft noch", "tokens": ["Wo", "E\u00b7ros", "oft", "noch"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NE", "ADV", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.17": {"text": "Den sch\u00e4rfsten seiner Pfeile versendet,", "tokens": ["Den", "sch\u00e4rfs\u00b7ten", "sei\u00b7ner", "Pfei\u00b7le", "ver\u00b7sen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "W\u00e4hrend abgewandt steht", "tokens": ["W\u00e4h\u00b7rend", "ab\u00b7ge\u00b7wandt", "steht"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJD", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "Die g\u00f6ttliche Mutter.", "tokens": ["Die", "g\u00f6tt\u00b7li\u00b7che", "Mut\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}