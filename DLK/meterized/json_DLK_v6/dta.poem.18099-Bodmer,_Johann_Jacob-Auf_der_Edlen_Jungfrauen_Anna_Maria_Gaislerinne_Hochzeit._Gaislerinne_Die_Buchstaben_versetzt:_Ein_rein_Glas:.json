{"dta.poem.18099": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Auf der Edlen Jungfrauen Anna Maria  \n Gaislerinne Hochzeit.  \n Gaislerinne  \n Die Buchstaben versetzt:  \n Ein rein Glas:", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Freylich, freylich ist ein Glas,", "tokens": ["Frey\u00b7lich", ",", "frey\u00b7lich", "ist", "ein", "Glas", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Edle Jungfrau, alles das,", "tokens": ["Ed\u00b7le", "Jung\u00b7frau", ",", "al\u00b7les", "das", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PIS", "PDS", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was in eurer besten Zier,", "tokens": ["Was", "in", "eu\u00b7rer", "bes\u00b7ten", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als die Sonne, leuchtet f\u00fcr.", "tokens": ["Als", "die", "Son\u00b7ne", ",", "leuch\u00b7tet", "f\u00fcr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "VVFIN", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schaut, wie sch\u00f6n die Sternen all", "tokens": ["Schaut", ",", "wie", "sch\u00f6n", "die", "Ster\u00b7nen", "all"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ADJD", "ART", "NN", "PIAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Leuchten aus des Himmels Saal,", "tokens": ["Leuch\u00b7ten", "aus", "des", "Him\u00b7mels", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie der Mond sein bleiches Haar", "tokens": ["Wie", "der", "Mond", "sein", "blei\u00b7ches", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ausgebreitet gantz und gar,", "tokens": ["Aus\u00b7ge\u00b7brei\u00b7tet", "gantz", "und", "gar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "KON", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wie die grosse weite Welt", "tokens": ["Wie", "die", "gros\u00b7se", "wei\u00b7te", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Schl\u00e4frig in die Bette f\u00e4llt,", "tokens": ["Schl\u00e4f\u00b7rig", "in", "die", "Bet\u00b7te", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Wie die Wasser stehen still,", "tokens": ["Wie", "die", "Was\u00b7ser", "ste\u00b7hen", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Wie sich nichts bewegen will.", "tokens": ["Wie", "sich", "nichts", "be\u00b7we\u00b7gen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PIS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Eh der V\u00f6gel Lobgesang", "tokens": ["Eh", "der", "V\u00f6\u00b7gel", "Lob\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Wiedert\u00f6nt mit hellem Klang,", "tokens": ["Wie\u00b7der\u00b7t\u00f6nt", "mit", "hel\u00b7lem", "Klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Eh der lichte Venus-Stern", "tokens": ["Eh", "der", "lich\u00b7te", "Ve\u00b7nus\u00b7S\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.16": {"text": "Sich l\u00e4\u00dft sehen weit und fern,", "tokens": ["Sich", "l\u00e4\u00dft", "se\u00b7hen", "weit", "und", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Eh die sch\u00f6ne Morgenr\u00f6th", "tokens": ["Eh", "die", "sch\u00f6\u00b7ne", "Mor\u00b7gen\u00b7r\u00f6th"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Aus dem s\u00fcssen Schlaf aufsteht,", "tokens": ["Aus", "dem", "s\u00fcs\u00b7sen", "Schlaf", "auf\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Und entdecket ihren Schein,", "tokens": ["Und", "ent\u00b7de\u00b7cket", "ih\u00b7ren", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Wird das Glas zubr\u00f6chen seyn.", "tokens": ["Wird", "das", "Glas", "zu\u00b7br\u00f6\u00b7chen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}