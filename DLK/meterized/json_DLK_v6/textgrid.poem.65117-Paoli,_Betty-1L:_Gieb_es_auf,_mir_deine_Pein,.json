{"textgrid.poem.65117": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gieb es auf, mir deine Pein,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gieb es auf, mir deine Pein,", "tokens": ["Gieb", "es", "auf", ",", "mir", "dei\u00b7ne", "Pein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Stolzen Sinnes, zu verhehlen!", "tokens": ["Stol\u00b7zen", "Sin\u00b7nes", ",", "zu", "ver\u00b7heh\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Andre t\u00e4uschen mag der Schein,", "tokens": ["And\u00b7re", "t\u00e4u\u00b7schen", "mag", "der", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "VMFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch nicht schmerzverwandte Seelen!", "tokens": ["Doch", "nicht", "schmerz\u00b7ver\u00b7wand\u00b7te", "See\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Diese sind, ob auch ihr Bund", "tokens": ["Die\u00b7se", "sind", ",", "ob", "auch", "ihr", "Bund"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "$,", "KOUS", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fremdem Aug' nicht sichtbar scheine,", "tokens": ["Frem\u00b7dem", "Aug'", "nicht", "sicht\u00b7bar", "schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf dem weiten Erdenrund", "tokens": ["Auf", "dem", "wei\u00b7ten", "Er\u00b7den\u00b7rund"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine mystische Gemeine.", "tokens": ["Ei\u00b7ne", "mys\u00b7ti\u00b7sche", "Ge\u00b7mei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wer an seines Gl\u00fcckes Bahr'", "tokens": ["Wer", "an", "sei\u00b7nes", "Gl\u00fc\u00b7ckes", "Bahr'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hielt die ernste Todtenwache,", "tokens": ["Hielt", "die", "erns\u00b7te", "Tod\u00b7ten\u00b7wa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Z\u00e4hlt zu der geweihten Schaar,", "tokens": ["Z\u00e4hlt", "zu", "der", "ge\u00b7weih\u00b7ten", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und versteht des Schmerzens Sprache.", "tokens": ["Und", "ver\u00b7steht", "des", "Schmer\u00b7zens", "Spra\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und die Br\u00fcder kennen sich", "tokens": ["Und", "die", "Br\u00fc\u00b7der", "ken\u00b7nen", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "An geheimen Ordenszeichen,", "tokens": ["An", "ge\u00b7hei\u00b7men", "Or\u00b7dens\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn sie, wie jetzt du und ich,", "tokens": ["Wenn", "sie", ",", "wie", "jetzt", "du", "und", "ich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "ADV", "PPER", "KON", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Still bewegt die Hand sich reichen.", "tokens": ["Still", "be\u00b7wegt", "die", "Hand", "sich", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}