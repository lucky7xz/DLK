"VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zur Strafe werdet ihr die Zeit erleben m\u00fcssen/", "tokens": ["Zur", "Stra\u00b7fe", "wer\u00b7det", "ihr", "die", "Zeit", "er\u00b7le\u00b7ben", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df sieben sich umsonst um einen Mann gerissen.", "tokens": ["Da\u00df", "sie\u00b7ben", "sich", "um\u00b7sonst", "um", "ei\u00b7nen", "Mann", "ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}