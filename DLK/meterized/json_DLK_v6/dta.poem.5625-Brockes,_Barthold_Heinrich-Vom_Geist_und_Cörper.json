{"dta.poem.5625": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom Geist und C\u00f6rper", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn auch ein Jupiter, durch Morpheus, Phantasey'n,", "tokens": ["Wenn", "auch", "ein", "Ju\u00b7pi\u00b7ter", ",", "durch", "Mor\u00b7pheus", ",", "Phan\u00b7ta\u00b7sey'n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$,", "APPR", "NE", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich zu betr\u00fcgen, h\u00e4tt\u2019 aus nichts hervorgezogen;", "tokens": ["Mich", "zu", "be\u00b7tr\u00fc\u00b7gen", ",", "h\u00e4tt'", "aus", "nichts", "her\u00b7vor\u00b7ge\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VAFIN", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bin ich jedennoch nicht betrogen,", "tokens": ["Bin", "ich", "je\u00b7den\u00b7noch", "nicht", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wann ich nur ", "tokens": ["Wann", "ich", "nur"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Ja ihr Pyrrhonier, verstockte Sceptiei,", "tokens": ["Ja", "ihr", "Pyrr\u00b7ho\u00b7nier", ",", "ver\u00b7stock\u00b7te", "Scep\u00b7ti\u00b7ei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Und ihr gleichg\u00fcltigen Academisten!", "tokens": ["Und", "ihr", "gleich\u00b7g\u00fcl\u00b7ti\u00b7gen", "A\u00b7ca\u00b7de\u00b7mis\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Wahrheit findet sich. der Geist beweget sie.", "tokens": ["Die", "Wahr\u00b7heit", "fin\u00b7det", "sich", ".", "der", "Geist", "be\u00b7we\u00b7get", "sie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$.", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Lasst Phantaseyen! Lasst Chymeren!", "tokens": ["Lasst", "Phan\u00b7ta\u00b7se\u00b7yen", "!", "Lasst", "Chy\u00b7me\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$.", "VVFIN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Sich zeigen unserm Geist bey gantzen Heeren", "tokens": ["Sich", "zei\u00b7gen", "un\u00b7serm", "Geist", "bey", "gant\u00b7zen", "Hee\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "In solcher ungeformt-und w\u00fcsten Seltsamkeit,", "tokens": ["In", "sol\u00b7cher", "un\u00b7ge\u00b7form\u00b7t\u00b7und", "w\u00fcs\u00b7ten", "Selt\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "In solcher grilligen Beschaffenheit,", "tokens": ["In", "sol\u00b7cher", "gril\u00b7li\u00b7gen", "Be\u00b7schaf\u00b7fen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Als nie gewesen sind und nimmer kommen k\u00f6nnen,", "tokens": ["Als", "nie", "ge\u00b7we\u00b7sen", "sind", "und", "nim\u00b7mer", "kom\u00b7men", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VAPP", "VAFIN", "KON", "ADV", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Man kan mich doch davor nicht unempfindlich nennen.", "tokens": ["Man", "kan", "mich", "doch", "da\u00b7vor", "nicht", "un\u00b7emp\u00b7find\u00b7lich", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "PAV", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mein Zweifel geht so weit: ich zweifele daran,", "tokens": ["Mein", "Zwei\u00b7fel", "geht", "so", "weit", ":", "ich", "zwei\u00b7fe\u00b7le", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "$.", "PPER", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ob etwas ausser mir. Ich zweifle gar dabey,", "tokens": ["Ob", "et\u00b7was", "aus\u00b7ser", "mir", ".", "Ich", "zweif\u00b7le", "gar", "da\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPER", "$.", "PPER", "VVFIN", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ob auch mein C\u00f6rper w\u00fcrcklich sey.", "tokens": ["Ob", "auch", "mein", "C\u00f6r\u00b7per", "w\u00fcrck\u00b7lich", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}