{"dta.poem.6047": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von ihren h\u00f6chsten H\u00f6h\u2019n den Durchgang. Hiermit gatten", "tokens": ["Von", "ih\u00b7ren", "h\u00f6chs\u00b7ten", "H\u00f6h'n", "den", "Durch\u00b7gang", ".", "Hier\u00b7mit", "gat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ART", "NN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die kleinen C\u00f6rper sich, so gleichsam canaliret,", "tokens": ["Die", "klei\u00b7nen", "C\u00f6r\u00b7per", "sich", ",", "so", "gleich\u00b7sam", "ca\u00b7na\u00b7li\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "$,", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und zwischen dreyen K\u00fcgelchen formiret.", "tokens": ["Und", "zwi\u00b7schen", "drey\u00b7en", "K\u00fc\u00b7gel\u00b7chen", "for\u00b7mi\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Es findet ihre grosse Menge", "tokens": ["Es", "fin\u00b7det", "ih\u00b7re", "gros\u00b7se", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zu ihrem Lauff bequeme G\u00e4nge.", "tokens": ["Zu", "ih\u00b7rem", "Lauff", "be\u00b7que\u00b7me", "G\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von allen Seiten hergedr\u00fccket, dringen sie", "tokens": ["Von", "al\u00b7len", "Sei\u00b7ten", "her\u00b7ge\u00b7dr\u00fc\u00b7cket", ",", "drin\u00b7gen", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In beyde Angeln ein, mit grosser Schnelligkeit,", "tokens": ["In", "bey\u00b7de", "An\u00b7geln", "ein", ",", "mit", "gros\u00b7ser", "Schnel\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und ist dadurch die Erde jederzeit,", "tokens": ["Und", "ist", "da\u00b7durch", "die", "Er\u00b7de", "je\u00b7der\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PAV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "In Jhrer Mitten,", "tokens": ["In", "Ih\u00b7rer", "Mit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Von ihnen gleichsam recht durchzogen und durchschnitten.", "tokens": ["Von", "ih\u00b7nen", "gleich\u00b7sam", "recht", "durch\u00b7zo\u00b7gen", "und", "durch\u00b7schnit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "ADJD", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Hierdurch erhalten sie die Erd\u2019 an ihrem Ort,", "tokens": ["Hier\u00b7durch", "er\u00b7hal\u00b7ten", "sie", "die", "Erd'", "an", "ih\u00b7rem", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So, da\u00df in diesem Stand, das Rund von unsrer Welt,", "tokens": ["So", ",", "da\u00df", "in", "die\u00b7sem", "Stand", ",", "das", "Rund", "von", "uns\u00b7rer", "Welt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "APPR", "PDAT", "NN", "$,", "PRELS", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ohn allen Anstand fort und fort", "tokens": ["Ohn", "al\u00b7len", "An\u00b7stand", "fort", "und", "fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Den immer gleichen Lauff beh\u00e4lt.", "tokens": ["Den", "im\u00b7mer", "glei\u00b7chen", "Lauff", "be\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}