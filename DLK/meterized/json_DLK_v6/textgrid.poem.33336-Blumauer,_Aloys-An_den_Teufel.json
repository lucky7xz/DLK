{"textgrid.poem.33336": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "An den Teufel", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man will dir, Armer, jetzt den Abschied geben,", "tokens": ["Man", "will", "dir", ",", "Ar\u00b7mer", ",", "jetzt", "den", "Ab\u00b7schied", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "$,", "NN", "$,", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und l\u00e4ugnet deine Macht:", "tokens": ["Und", "l\u00e4ug\u00b7net", "dei\u00b7ne", "Macht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man f\u00fchrt bei Tag ein teufelloses Leben,", "tokens": ["Man", "f\u00fchrt", "bei", "Tag", "ein", "teu\u00b7fel\u00b7lo\u00b7ses", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und scheut dich nur bei Nacht.", "tokens": ["Und", "scheut", "dich", "nur", "bei", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dir geht's, wie einem K\u00f6nig auf der B\u00fchne,", "tokens": ["Dir", "geht's", ",", "wie", "ei\u00b7nem", "K\u00f6\u00b7nig", "auf", "der", "B\u00fch\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Man spottet deiner keck,", "tokens": ["Man", "spot\u00b7tet", "dei\u00b7ner", "keck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Seit Jahren schon schreibt wider dich die k\u00fchne", "tokens": ["Seit", "Jah\u00b7ren", "schon", "schreibt", "wi\u00b7der", "dich", "die", "k\u00fch\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVFIN", "APPR", "PPER", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Berliner Bibliothek.", "tokens": ["Ber\u00b7li\u00b7ner", "Bib\u00b7lio\u00b7thek", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Ein jeder Knabe neckt dich zum Vergn\u00fcgen,", "tokens": ["Ein", "je\u00b7der", "Kna\u00b7be", "neckt", "dich", "zum", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und d\u00fcnkt dabei sich klug,", "tokens": ["Und", "d\u00fcnkt", "da\u00b7bei", "sich", "klug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "PRF", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du gleichst dem L\u00f6wen, den in letzten Z\u00fcgen", "tokens": ["Du", "gleichst", "dem", "L\u00f6\u00b7wen", ",", "den", "in", "letz\u00b7ten", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Sogar der Esel schlug.", "tokens": ["So\u00b7gar", "der", "E\u00b7sel", "schlug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch all' die Neckerei ist \u2013 wo nicht S\u00fcnde \u2013", "tokens": ["Doch", "all'", "die", "Ne\u00b7cke\u00b7rei", "ist", "\u2013", "wo", "nicht", "S\u00fcn\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "NN", "VAFIN", "$(", "PWAV", "PTKNEG", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch ohne Consequenz,", "tokens": ["Doch", "oh\u00b7ne", "Con\u00b7se\u00b7quenz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir haben leider! noch zu viele Gr\u00fcnde", "tokens": ["Wir", "ha\u00b7ben", "lei\u00b7der", "!", "noch", "zu", "vie\u00b7le", "Gr\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$.", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von deiner Existenz.", "tokens": ["Von", "dei\u00b7ner", "E\u00b7xis\u00b7tenz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die schwarzen Buben treiben ja noch immer", "tokens": ["Die", "schwar\u00b7zen", "Bu\u00b7ben", "trei\u00b7ben", "ja", "noch", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADV", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit Menschen ihren Hohn;", "tokens": ["Mit", "Men\u00b7schen", "ih\u00b7ren", "Hohn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur thun sie, was sie sonst ver\u00fcbten, nimmer", "tokens": ["Nur", "thun", "sie", ",", "was", "sie", "sonst", "ver\u00b7\u00fcb\u00b7ten", ",", "nim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,", "ADV"], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.4": {"text": "In eigener Person.", "tokens": ["In", "ei\u00b7ge\u00b7ner", "Per\u00b7son", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+---+", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Sie d\u00fcrfen keinen Hexentanz zwar halten;", "tokens": ["Sie", "d\u00fcr\u00b7fen", "kei\u00b7nen", "He\u00b7xen\u00b7tanz", "zwar", "hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch sieht man sie das nun", "tokens": ["Doch", "sieht", "man", "sie", "das", "nun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "PPER", "PDS", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In m\u00e4nnlichen und weiblichen Gestalten", "tokens": ["In", "m\u00e4nn\u00b7li\u00b7chen", "und", "weib\u00b7li\u00b7chen", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf der Redoute thun.", "tokens": ["Auf", "der", "Re\u00b7dou\u00b7te", "thun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "Die wilde Jagd mit gr\u00e4\u00dflichem Gebelle", "tokens": ["Die", "wil\u00b7de", "Jagd", "mit", "gr\u00e4\u00df\u00b7li\u00b7chem", "Ge\u00b7bel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hast du zwar eingestellt;", "tokens": ["Hast", "du", "zwar", "ein\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch machest du, da\u00df sie an deiner Stelle", "tokens": ["Doch", "ma\u00b7chest", "du", ",", "da\u00df", "sie", "an", "dei\u00b7ner", "Stel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Der Junker selber h\u00e4lt.", "tokens": ["Der", "Jun\u00b7ker", "sel\u00b7ber", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und schreckst du gleich die fromme Dame nimmer", "tokens": ["Und", "schreckst", "du", "gleich", "die", "from\u00b7me", "Da\u00b7me", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In schwarzer Liverei,", "tokens": ["In", "schwar\u00b7zer", "Li\u00b7ve\u00b7rei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So kommst du Nachts doch oft noch in ihr Zimmer", "tokens": ["So", "kommst", "du", "Nachts", "doch", "oft", "noch", "in", "ihr", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als L\u00e4ufer und Lakei.", "tokens": ["Als", "L\u00e4u\u00b7fer", "und", "La\u00b7kei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$."], "meter": "-+-+++", "measure": "unknown.measure.tetra"}}, "stanza.9": {"line.1": {"text": "Zwar l\u00e4ssest du, gleich allen b\u00f6sen Geistern,", "tokens": ["Zwar", "l\u00e4s\u00b7sest", "du", ",", "gleich", "al\u00b7len", "b\u00f6\u00b7sen", "Geis\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht mehr Gestank zum Hohn;", "tokens": ["Nicht", "mehr", "Ge\u00b7stank", "zum", "Hohn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch riecht man dich in unsern kleinen Meistern", "tokens": ["Doch", "riecht", "man", "dich", "in", "un\u00b7sern", "klei\u00b7nen", "Meis\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf f\u00fcnfzig Schritte schon.", "tokens": ["Auf", "f\u00fcnf\u00b7zig", "Schrit\u00b7te", "schon", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Dein H\u00f6rnerdiadem hat, wie ich h\u00f6re,", "tokens": ["Dein", "H\u00f6r\u00b7ner\u00b7di\u00b7a\u00b7dem", "hat", ",", "wie", "ich", "h\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Zwar noch kein Mensch geseh'n;", "tokens": ["Zwar", "noch", "kein", "Mensch", "ge\u00b7seh'n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch uns're M\u00e4nner rechnen sich's zur Ehre,", "tokens": ["Doch", "un\u00b7s'\u00b7re", "M\u00e4n\u00b7ner", "rech\u00b7nen", "sich's", "zur", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PIS", "APPRART", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "In diesem Schmuck zu gehn.", "tokens": ["In", "die\u00b7sem", "Schmuck", "zu", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Du gehst nicht mehr, um Jungen zu verstricken,", "tokens": ["Du", "gehst", "nicht", "mehr", ",", "um", "Jun\u00b7gen", "zu", "ver\u00b7stri\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$,", "KOUI", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als Weib zu ihnen hin;", "tokens": ["Als", "Weib", "zu", "ih\u00b7nen", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch f\u00e4hrst du jetzt, sie sich'rer zu ber\u00fccken,", "tokens": ["Doch", "f\u00e4hrst", "du", "jetzt", ",", "sie", "sich'\u00b7rer", "zu", "be\u00b7r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PPER", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In eine Kupplerin.", "tokens": ["In", "ei\u00b7ne", "Kupp\u00b7le\u00b7rin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Der schwarze Bock, der oft zu Lustgelagen", "tokens": ["Der", "schwar\u00b7ze", "Bock", ",", "der", "oft", "zu", "Lust\u00b7ge\u00b7la\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "$,", "PRELS", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So manche Hexe trug,", "tokens": ["So", "man\u00b7che", "He\u00b7xe", "trug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist abgeschaft, doch gibts in unsern Tagen", "tokens": ["Ist", "ab\u00b7ge\u00b7schaft", ",", "doch", "gibts", "in", "un\u00b7sern", "Ta\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "$,", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch graue B\u00f6cke g'nug.", "tokens": ["Noch", "grau\u00b7e", "B\u00f6\u00b7cke", "g'\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.13": {"line.1": {"text": "Aus Spiegeln guckt die Sch\u00f6nen zu bestrafen,", "tokens": ["Aus", "Spie\u00b7geln", "guckt", "die", "Sch\u00f6\u00b7nen", "zu", "be\u00b7stra\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zwar jetzt kein Satan mehr:", "tokens": ["Zwar", "jetzt", "kein", "Sa\u00b7tan", "mehr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch sind gar viele, die sich d'rin begaffen,", "tokens": ["Doch", "sind", "gar", "vie\u00b7le", ",", "die", "sich", "d'\u00b7rin", "be\u00b7ga\u00b7ffen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PIS", "$,", "PRELS", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Noch h\u00e4\u00dflicher, als er.", "tokens": ["Noch", "h\u00e4\u00df\u00b7li\u00b7cher", ",", "als", "er", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.14": {"line.1": {"text": "Du wagst dich zwar nicht mehr in Menschenlieber,", "tokens": ["Du", "wagst", "dich", "zwar", "nicht", "mehr", "in", "Men\u00b7schen\u00b7lie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seit Ga\u00dfner dir gedroht;", "tokens": ["Seit",