{"textgrid.poem.25576": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Verirret auf der Jagd, von seinen Leuten", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verirret auf der Jagd, von seinen Leuten", "tokens": ["Ver\u00b7ir\u00b7ret", "auf", "der", "Jagd", ",", "von", "sei\u00b7nen", "Leu\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ganz abgekommen, traf der F\u00fcrst zum Gl\u00fcck'", "tokens": ["Ganz", "ab\u00b7ge\u00b7kom\u00b7men", ",", "traf", "der", "F\u00fcrst", "zum", "Gl\u00fcck"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "$,", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch einen Bauer an, und lie\u00df von ihm zur\u00fcck", "tokens": ["Noch", "ei\u00b7nen", "Bau\u00b7er", "an", ",", "und", "lie\u00df", "von", "ihm", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sich durch den Wald bis an das Feld geleiten.", "tokens": ["Sich", "durch", "den", "Wald", "bis", "an", "das", "Feld", "ge\u00b7lei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "APPR", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der F\u00fcrst, der ihn bald die\u00df, bald das", "tokens": ["Der", "F\u00fcrst", ",", "der", "ihn", "bald", "die\u00df", ",", "bald", "das"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "PDS", "$,", "ADV", "ART"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Von seinem Dorf' und seinem Amtmann' fragte,", "tokens": ["Von", "sei\u00b7nem", "Dor\u00b7f'", "und", "sei\u00b7nem", "Amt\u00b7mann'", "frag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Wollt' endlich auch noch h\u00f6ren, was", "tokens": ["Wollt'", "end\u00b7lich", "auch", "noch", "h\u00f6\u00b7ren", ",", "was"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VMFIN", "ADV", "ADV", "ADV", "VVINF", "$,", "PWS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wohl Kasper von ihm selber sagte?", "tokens": ["Wohl", "Kas\u00b7per", "von", "ihm", "sel\u00b7ber", "sag\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "I! sprach der Bauer, der ist zwar", "tokens": ["I", "!", "sprach", "der", "Bau\u00b7er", ",", "der", "ist", "zwar"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["XY", "$.", "VVFIN", "ART", "NN", "$,", "PRELS", "VAFIN", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Noch gut genug, doch an der F\u00fcrstin f\u00e4nde", "tokens": ["Noch", "gut", "ge\u00b7nug", ",", "doch", "an", "der", "F\u00fcrs\u00b7tin", "f\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "$,", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Der Teufel selbst kein gutes Haar;", "tokens": ["Der", "Teu\u00b7fel", "selbst", "kein", "gu\u00b7tes", "Haar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Bei der hat, wie man h\u00f6rt, das Fodern gar kein Ende.", "tokens": ["Bei", "der", "hat", ",", "wie", "man", "h\u00f6rt", ",", "das", "Fo\u00b7dern", "gar", "kein", "En\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VAFIN", "$,", "PWAV", "PIS", "VVFIN", "$,", "ART", "NN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Das Urtheil war zum Gl\u00fcck' nicht wahr,", "tokens": ["Das", "Ur\u00b7theil", "war", "zum", "Gl\u00fcck", "nicht", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Drum l\u00e4chelte der F\u00fcrst, und dr\u00fcckte", "tokens": ["Drum", "l\u00e4\u00b7chel\u00b7te", "der", "F\u00fcrst", ",", "und", "dr\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Dem Kritiker ein Goldst\u00fcck in die Hand,", "tokens": ["Dem", "Kri\u00b7ti\u00b7ker", "ein", "Gold\u00b7st\u00fcck", "in", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Als er von fern das n\u00e4chste Dorf erblickte,", "tokens": ["Als", "er", "von", "fern", "das", "n\u00e4chs\u00b7te", "Dorf", "er\u00b7blick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Wohin der Weg von selbst sich fand.", "tokens": ["Wo\u00b7hin", "der", "Weg", "von", "selbst", "sich", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Nach ein paar Tagen aber schickte", "tokens": ["Nach", "ein", "paar", "Ta\u00b7gen", "a\u00b7ber", "schick\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PIAT", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Der F\u00fcrst nach Kaspern. Dieser kam.", "tokens": ["Der", "F\u00fcrst", "nach", "Kas\u00b7pern", ".", "Die\u00b7ser", "kam", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$.", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Als er ins Zimmer trat, erstickte", "tokens": ["Als", "er", "ins", "Zim\u00b7mer", "trat", ",", "er\u00b7stick\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Sein Herz beinah vor Furcht und Scham.", "tokens": ["Sein", "Herz", "bei\u00b7nah", "vor", "Furcht", "und", "Scham", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Doch Kaspers Angst verlor sich nach gerade,", "tokens": ["Doch", "Kas\u00b7pers", "Angst", "ver\u00b7lor", "sich", "nach", "ge\u00b7ra\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VVFIN", "PRF", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Denn wie es schien, kannt' ihn der F\u00fcrst nicht mehr,", "tokens": ["Denn", "wie", "es", "schien", ",", "kannt'", "ihn", "der", "F\u00fcrst", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Auch gab er mit gewohnter Gnade", "tokens": ["Auch", "gab", "er", "mit", "ge\u00b7wohn\u00b7ter", "Gna\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Dem allem, was Hans Kasper sprach, Geh\u00f6r.", "tokens": ["Dem", "al\u00b7lem", ",", "was", "Hans", "Kas\u00b7per", "sprach", ",", "Ge\u00b7h\u00f6r", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIS", "$,", "PRELS", "NE", "NE", "VVFIN", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Jetzt trat die F\u00fcrstin auch herein;", "tokens": ["Jetzt", "trat", "die", "F\u00fcrs\u00b7tin", "auch", "her\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "\u00bbgut, da\u00df du kommst! denn eben fiel mir ein,", "tokens": ["\u00bb", "gut", ",", "da\u00df", "du", "kommst", "!", "denn", "e\u00b7ben", "fiel", "mir", "ein", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$.", "KON", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Was ich dich gestern wollte fragen:", "tokens": ["Was", "ich", "dich", "ge\u00b7stern", "woll\u00b7te", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PRF", "VVINF", "VMFIN", "VVINF", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.29": {"text": "Ob du schon weist, wie ungemein", "tokens": ["Ob", "du", "schon", "weist", ",", "wie", "un\u00b7ge\u00b7mein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,", "PWAV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Viel Gut's von dir die Leute sagen?\u00ab", "tokens": ["Viel", "Gut's", "von", "dir", "die", "Leu\u00b7te", "sa\u00b7gen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "APPR", "PPER", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Nun? \u2013 \u00bbEi! kein gutes Haar soll an dir seyn!", "tokens": ["Nun", "?", "\u2013", "\u00bb", "Ei", "!", "kein", "gu\u00b7tes", "Haar", "soll", "an", "dir", "seyn", "!"], "token_info": ["word", "punct", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "$(", "$(", "NN", "$.", "PIAT", "ADJA", "NN", "VMFIN", "APPR", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Man sagt, du k\u00f6nntest nichts, als Fodern und Verschenken!\u00ab", "tokens": ["Man", "sagt", ",", "du", "k\u00f6nn\u00b7test", "nichts", ",", "als", "Fo\u00b7dern", "und", "Ver\u00b7schen\u00b7ken", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "VVFIN", "$,", "PPER", "VMFIN", "PIS", "$,", "KOUS", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und wer hat das gesagt? den la\u00df doch heut noch henken!", "tokens": ["Und", "wer", "hat", "das", "ge\u00b7sagt", "?", "den", "la\u00df", "doch", "heut", "noch", "hen\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "PDS", "VVPP", "$.", "ART", "VVIMP", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "\u00bbhier steht er selbst. \u2013 \u2013 Es f\u00e4llt dir doch noch ein,", "tokens": ["\u00bb", "hier", "steht", "er", "selbst", ".", "\u2013", "\u2013", "Es", "f\u00e4llt", "dir", "doch", "noch", "ein", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "$.", "$(", "$(", "PPER", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Hans! da\u00df du j\u00fcngst so was im Wald' bei ", "tokens": ["Hans", "!", "da\u00df", "du", "j\u00fcngst", "so", "was", "im", "Wald'", "bei"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "KOUS", "PPER", "ADV", "ADV", "PRELS", "APPRART", "NN", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Zu einem Reiter sprachst?\u00ab", "tokens": ["Zu", "ei\u00b7nem", "Rei\u00b7ter", "sprachst", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "Ja! doch wie konnt' ich denken,", "tokens": ["Ja", "!", "doch", "wie", "konnt'", "ich", "den\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "KON", "PWAV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Der Schelm w\u00fcrd' ein Verr\u00e4ther seyn,", "tokens": ["Der", "Schelm", "w\u00fcrd'", "ein", "Ver\u00b7r\u00e4\u00b7ther", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Zu dem ich's sagte?", "tokens": ["Zu", "dem", "ich's", "sag\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.40": {"text": "Ha! der Schelm ist dein!", "tokens": ["Ha", "!", "der", "Schelm", "ist", "dein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "VAFIN", "PPOSAT", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.41": {"text": "Fiel die Gemahlin lachend ein,", "tokens": ["Fiel", "die", "Ge\u00b7mah\u00b7lin", "la\u00b7chend", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.42": {"text": "Nun magst du ihm die Strafe schenken.", "tokens": ["Nun", "magst", "du", "ihm", "die", "Stra\u00b7fe", "schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}