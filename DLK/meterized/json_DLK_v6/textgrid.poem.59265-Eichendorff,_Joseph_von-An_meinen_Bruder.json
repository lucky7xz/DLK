{"textgrid.poem.59265": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An meinen Bruder", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was Gro\u00dfes sich begeben,", "tokens": ["Was", "Gro\u00b7\u00dfes", "sich", "be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der K\u00f6n'ge Herrlichkeit,", "tokens": ["Der", "K\u00f6n'\u00b7ge", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du sahst's mit freud'gem Beben,", "tokens": ["Du", "sahst's", "mit", "freu\u00b7d'\u00b7gem", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dir war's verg\u00f6nnt, zu leben", "tokens": ["Dir", "wa\u00b7r's", "ver\u00b7g\u00f6nnt", ",", "zu", "le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In dieser Wunderzeit.", "tokens": ["In", "die\u00b7ser", "Wun\u00b7der\u00b7zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und \u00fcber diese Wogen", "tokens": ["Und", "\u00fc\u00b7ber", "die\u00b7se", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kam hoch ein himmlisch Bild", "tokens": ["Kam", "hoch", "ein", "himm\u00b7lisch", "Bild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "ART", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durchs stille Blau gezogen,", "tokens": ["Durchs", "stil\u00b7le", "Blau", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Traf mit dem Zauberbogen", "tokens": ["Traf", "mit", "dem", "Zau\u00b7ber\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Dein Herz so fest und mild.", "tokens": ["Dein", "Herz", "so", "fest", "und", "mild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O wunderbares Grauen,", "tokens": ["O", "wun\u00b7der\u00b7ba\u00b7res", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zur selben Stund den Herrn", "tokens": ["Zur", "sel\u00b7ben", "Stund", "den", "Herrn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Wetterleuchten schauen,", "tokens": ["Im", "Wet\u00b7ter\u00b7leuch\u00b7ten", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und \u00fcber den stummen Gauen", "tokens": ["Und", "\u00fc\u00b7ber", "den", "stum\u00b7men", "Gau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Schuldloser Liebe Stern!", "tokens": ["Schuld\u00b7lo\u00b7ser", "Lie\u00b7be", "Stern", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Und hat nun ausgerungen", "tokens": ["Und", "hat", "nun", "aus\u00b7ge\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Deutschland siegeswund:", "tokens": ["Mein", "Deutschland", "sie\u00b7ges\u00b7wund", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Was damals Lieb gesungen,", "tokens": ["Was", "da\u00b7mals", "Lieb", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was Schwerter dir geklungen,", "tokens": ["Was", "Schwer\u00b7ter", "dir", "ge\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Klingt fort im Herzensgrund.", "tokens": ["Klingt", "fort", "im", "Her\u00b7zens\u00b7grund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "La\u00df bilden die Gewalten!", "tokens": ["La\u00df", "bil\u00b7den", "die", "Ge\u00b7wal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was davon himmlisch war,", "tokens": ["Was", "da\u00b7von", "himm\u00b7lisch", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PAV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kann nimmermehr veralten,", "tokens": ["Kann", "nim\u00b7mer\u00b7mehr", "ver\u00b7al\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wird in der Brust gestalten", "tokens": ["Wird", "in", "der", "Brust", "ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sich manches stille Jahr.", "tokens": ["Sich", "man\u00b7ches", "stil\u00b7le", "Jahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Fesseln m\u00fcssen springen,", "tokens": ["Die", "Fes\u00b7seln", "m\u00fcs\u00b7sen", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ja, endlich macht sich's frei,", "tokens": ["Ja", ",", "end\u00b7lich", "macht", "sich's", "frei", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Gro\u00dfes wird gelingen", "tokens": ["Und", "Gro\u00b7\u00dfes", "wird", "ge\u00b7lin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch Taten oder Singen,", "tokens": ["Durch", "Ta\u00b7ten", "o\u00b7der", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vor Gott ist's einerlei.", "tokens": ["Vor", "Gott", "ist's", "ei\u00b7ner\u00b7lei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}