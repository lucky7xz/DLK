{"textgrid.poem.46450": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: So sitze ich auf dem Sofa im Dunkel", "genre": "verse", "period": "N.A.", "pub_year": 1913, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So sitze ich auf dem Sofa im Dunkel", "tokens": ["So", "sit\u00b7ze", "ich", "auf", "dem", "So\u00b7fa", "im", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Und sinne \u2013", "tokens": ["Und", "sin\u00b7ne", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Langsam verschwellen, verengen sich die W\u00e4nde \u2013", "tokens": ["Lang\u00b7sam", "ver\u00b7schwel\u00b7len", ",", "ver\u00b7en\u00b7gen", "sich", "die", "W\u00e4n\u00b7de", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "VVFIN", "PRF", "ART", "NN", "$("], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Die Decke sinkt \u2013", "tokens": ["Die", "De\u00b7cke", "sinkt", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die lackierte T\u00fcr in der Ecke blinkt", "tokens": ["Die", "la\u00b7ckier\u00b7te", "T\u00fcr", "in", "der", "E\u00b7cke", "blinkt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sonderbar \u2013 \u2013", "tokens": ["Son\u00b7der\u00b7bar", "\u2013", "\u2013"], "token_info": ["word", "punct", "punct"], "pos": ["NN", "$(", "$("], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Mir ist, als wenn wer vor der T\u00fcr st\u00e4nde \u2013", "tokens": ["Mir", "ist", ",", "als", "wenn", "wer", "vor", "der", "T\u00fcr", "st\u00e4n\u00b7de", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "PWS", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Als wenn scheue H\u00e4nde", "tokens": ["Als", "wenn", "scheu\u00b7e", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Klopfen wollen \u2013 \u2013?", "tokens": ["Klop\u00b7fen", "wol\u00b7len", "\u2013", "\u2013", "?"], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["NN", "VMFIN", "$(", "$(", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Der Fu\u00dfboden schwankt \u2013", "tokens": ["Der", "Fu\u00df\u00b7bo\u00b7den", "schwankt", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Die T\u00fcr wankt \u2013 \u2013", "tokens": ["Die", "T\u00fcr", "wankt", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Die T\u00fcr! \u2013 Die T\u00fcr ist dunkel aufgequollen:", "tokens": ["Die", "T\u00fcr", "!", "\u2013", "Die", "T\u00fcr", "ist", "dun\u00b7kel", "auf\u00b7ge\u00b7quol\u00b7len", ":"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "ART", "NN", "VAFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da! Du kommst herein!", "tokens": ["Da", "!", "Du", "kommst", "her\u00b7ein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Geliebte!", "tokens": ["Ge\u00b7lieb\u00b7te", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Geliebte von Einst \u2013", "tokens": ["Ge\u00b7lieb\u00b7te", "von", "Einst", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Ich kann nicht aufstehn!", "tokens": ["Ich", "kann", "nicht", "auf\u00b7stehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Ich kann nicht schrei'n!", "tokens": ["Ich", "kann", "nicht", "schrei'n", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ich kann nichts mehr sehn! \u2013", "tokens": ["Ich", "kann", "nichts", "mehr", "sehn", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PIS", "ADV", "VVINF", "$.", "$("], "meter": "+---+", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Die W\u00e4nde st\u00fcrzen dunkel ein \u2013", "tokens": ["Die", "W\u00e4n\u00b7de", "st\u00fcr\u00b7zen", "dun\u00b7kel", "ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}