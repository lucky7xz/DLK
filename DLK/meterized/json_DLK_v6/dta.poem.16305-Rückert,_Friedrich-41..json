{"dta.poem.16305": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "41.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Komm her und la\u00df uns in den heil'gen Fluten baden,", "tokens": ["Komm", "her", "und", "la\u00df", "uns", "in", "den", "heil'\u00b7gen", "Flu\u00b7ten", "ba\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVIMP", "PPER", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die mit dem Silberblick zur Reinigkeit uns laden.", "tokens": ["Die", "mit", "dem", "Sil\u00b7berb\u00b7lick", "zur", "Rei\u00b7nig\u00b7keit", "uns", "la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Sonne breitet aus des Stralenmantels F\u00fcllen,", "tokens": ["Die", "Son\u00b7ne", "brei\u00b7tet", "aus", "des", "Stra\u00b7len\u00b7man\u00b7tels", "F\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um in ein sch\u00f6nres Kleid als ird'sches dich zu h\u00fcllen.", "tokens": ["Um", "in", "ein", "sch\u00f6n\u00b7res", "Kleid", "als", "ird'\u00b7sches", "dich", "zu", "h\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "ADJA", "NN", "KOUS", "ADJA", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein lindes Badetuch reicht dir die Morgenluft,", "tokens": ["Ein", "lin\u00b7des", "Ba\u00b7de\u00b7tuch", "reicht", "dir", "die", "Mor\u00b7gen\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das dich mit Wohlgeruch abtrocknet und mit Duft.", "tokens": ["Das", "dich", "mit", "Wohl\u00b7ge\u00b7ruch", "ab\u00b7trock\u00b7net", "und", "mit", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "NN", "VVFIN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das Wasser selber wallt ein G\u00fcrtel von Kristallen,", "tokens": ["Das", "Was\u00b7ser", "sel\u00b7ber", "wallt", "ein", "G\u00fcr\u00b7tel", "von", "Kris\u00b7tal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der dir um die Gestalt sich schmiegt mit Wohlgefallen.", "tokens": ["Der", "dir", "um", "die", "Ge\u00b7stalt", "sich", "schmiegt", "mit", "Wohl\u00b7ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "PRF", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und auf dem Grunde ruht, geschmeidigt von der Flut,", "tokens": ["Und", "auf", "dem", "Grun\u00b7de", "ruht", ",", "ge\u00b7schmei\u00b7digt", "von", "der", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Erde, die dir weich Sandalendienste thut.", "tokens": ["Die", "Er\u00b7de", ",", "die", "dir", "weich", "San\u00b7da\u00b7len\u00b7diens\u00b7te", "thut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "So tauche rein dich ein in jedes Element,", "tokens": ["So", "tau\u00b7che", "rein", "dich", "ein", "in", "je\u00b7des", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PRF", "ART", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sei von dem, der ist in jedem, ungetrennt.", "tokens": ["Und", "sei", "von", "dem", ",", "der", "ist", "in", "je\u00b7dem", ",", "un\u00b7ge\u00b7trennt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ART", "$,", "PRELS", "VAFIN", "APPR", "PIS", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die Flut, die ewig tr\u00e4uft von seinen Augenlieden,", "tokens": ["Die", "Flut", ",", "die", "e\u00b7wig", "tr\u00e4uft", "von", "sei\u00b7nen", "Au\u00b7gen\u00b7lie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat er zum Labequell dem Erdendurst beschieden.", "tokens": ["Hat", "er", "zum", "La\u00b7be\u00b7quell", "dem", "Er\u00b7dend\u00b7urst", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}