{"dta.poem.10950": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Eine zu  \n Altenbruch gego\u00dfne Glocke.  \n Auf Ersuchen des dortigen Kirchspiels.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So oft, in meinem weiten Munde, die harte Zunge schallt\nund klingt,", "tokens": ["So", "oft", ",", "in", "mei\u00b7nem", "wei\u00b7ten", "Mun\u00b7de", ",", "die", "har\u00b7te", "Zun\u00b7ge", "schallt", "und", "klingt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "La\u00dft euer Herz vom Erzt erweichen, und denkt an der Posaunen", "tokens": ["La\u00dft", "eu\u00b7er", "Herz", "vom", "Erzt", "er\u00b7wei\u00b7chen", ",", "und", "denkt", "an", "der", "Po\u00b7sau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$,", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Der, bey dem letzten Donnerknall,", "tokens": ["Der", ",", "bey", "dem", "letz\u00b7ten", "Don\u00b7ner\u00b7knall", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Euch vor des H\u00f6chsten Thron zu ruffen, auch durch", "tokens": ["Euch", "vor", "des", "H\u00f6chs\u00b7ten", "Thron", "zu", "ruf\u00b7fen", ",", "auch", "durch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,", "ADV", "APPR"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "die tiefen Gr\u00e4ber dringt.", "tokens": ["die", "tie\u00b7fen", "Gr\u00e4\u00b7ber", "dringt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Es lie\u00df das Kirchspiel ", "tokens": ["Es", "lie\u00df", "das", "Kirch\u00b7spiel"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Und, siebzehn hundert sechs und drey\u00dfig, zum Werkzeug eurer", "tokens": ["Und", ",", "sieb\u00b7zehn", "hun\u00b7dert", "sechs", "und", "drey\u00b7\u00dfig", ",", "zum", "Werk\u00b7zeug", "eu\u00b7rer"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "CARD", "CARD", "CARD", "KON", "ADJD", "$,", "APPRART", "NN", "PPOSAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}}}}