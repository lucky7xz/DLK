{"textgrid.poem.50488": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "12. Auf Critorax", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Der seinem F\u00fcrsten dienlich ist,", "tokens": ["Der", "sei\u00b7nem", "F\u00fcrs\u00b7ten", "dien\u00b7lich", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch sich dabey auch nicht vergisst.", "tokens": ["Doch", "sich", "da\u00b7bey", "auch", "nicht", "ver\u00b7gisst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PAV", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er ist ein Mann, der leichtlich nicht,", "tokens": ["Er", "ist", "ein", "Mann", ",", "der", "leicht\u00b7lich", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was er ", "tokens": ["Was", "er"], "token_info": ["word", "word"], "pos": ["PWS", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Der von sich frey zu sprechen pflegt,", "tokens": ["Der", "von", "sich", "frey", "zu", "spre\u00b7chen", "pflegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PRF", "ADJD", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und was ", "tokens": ["Und", "was"], "token_info": ["word", "word"], "pos": ["KON", "PWS"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Allein denn sind es solche Leut',", "tokens": ["Al\u00b7lein", "denn", "sind", "es", "sol\u00b7che", "Leut'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die er auf keine Weise scheut:", "tokens": ["Die", "er", "auf", "kei\u00b7ne", "Wei\u00b7se", "scheut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So dass ein Tropf, der ihn nicht kennt;", "tokens": ["So", "dass", "ein", "Tropf", ",", "der", "ihn", "nicht", "kennt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Das, ", "tokens": ["Das", ","], "token_info": ["word", "punct"], "pos": ["PDS", "$,"], "meter": "-", "measure": "single.down"}, "line.12": {"text": "Und unerfahren in der Welt,", "tokens": ["Und", "un\u00b7er\u00b7fah\u00b7ren", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Vor ", "tokens": ["Vor"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.14": {"text": "Das was er spricht, ist hell und klar,", "tokens": ["Das", "was", "er", "spricht", ",", "ist", "hell", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}