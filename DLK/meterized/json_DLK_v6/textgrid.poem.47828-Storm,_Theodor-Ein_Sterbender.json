: "iambic.penta"}, "line.33": {"text": "Sie will nicht gehen in den Dienst des Todes;", "tokens": ["Sie", "will", "nicht", "ge\u00b7hen", "in", "den", "Dienst", "des", "To\u00b7des", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Er aber zwingt sie, denn sein Wille soll", "tokens": ["Er", "a\u00b7ber", "zwingt", "sie", ",", "denn", "sein", "Wil\u00b7le", "soll"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$,", "KON", "PPOSAT", "NN", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "So weit noch reichen, als er es vermag.", "tokens": ["So", "weit", "noch", "rei\u00b7chen", ",", "als", "er", "es", "ver\u00b7mag", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVINF", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Wanduhr mi\u00dft mit hartem Pendelschlag,", "tokens": ["Die", "Wan\u00b7duhr", "mi\u00dft", "mit", "har\u00b7tem", "Pen\u00b7del\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als dr\u00e4nge sie, die fliehenden Sekunden;", "tokens": ["Als", "dr\u00e4n\u00b7ge", "sie", ",", "die", "flie\u00b7hen\u00b7den", "Se\u00b7kun\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sein Auge dunkelt; ungesehen naht,