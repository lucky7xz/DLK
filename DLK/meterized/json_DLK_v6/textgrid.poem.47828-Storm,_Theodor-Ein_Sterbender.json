{"textgrid.poem.47828": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Ein Sterbender", "genre": "verse", "period": "N.A.", "pub_year": 1863, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Fenster sitzt er, alt, gebrochnen Leibes,", "tokens": ["Am", "Fens\u00b7ter", "sitzt", "er", ",", "alt", ",", "ge\u00b7broch\u00b7nen", "Lei\u00b7bes", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und trommelt m\u00fc\u00dfig an die feuchten Scheiben;", "tokens": ["Und", "trom\u00b7melt", "m\u00fc\u00b7\u00dfig", "an", "die", "feuch\u00b7ten", "Schei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Grau ist der Wintertag und grau sein Haar.", "tokens": ["Grau", "ist", "der", "Win\u00b7ter\u00b7tag", "und", "grau", "sein", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Mitunter auch besieht er aufmerksam", "tokens": ["Mi\u00b7tun\u00b7ter", "auch", "be\u00b7sieht", "er", "auf\u00b7merk\u00b7sam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der Adern H\u00fcpfen auf der welken Hand.", "tokens": ["Der", "A\u00b7dern", "H\u00fcp\u00b7fen", "auf", "der", "wel\u00b7ken", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Es geht zu Ende; ratlos irrt sein Aug", "tokens": ["Es", "geht", "zu", "En\u00b7de", ";", "rat\u00b7los", "irrt", "sein", "Aug"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$.", "ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Von Tisch zu Tisch, drauf Schriftwerk aller Art,", "tokens": ["Von", "Tisch", "zu", "Tisch", ",", "drauf", "Schrift\u00b7werk", "al\u00b7ler", "Art", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,", "PAV", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sein harrend, hoch und h\u00f6her sich get\u00fcrmt.", "tokens": ["Sein", "har\u00b7rend", ",", "hoch", "und", "h\u00f6\u00b7her", "sich", "ge\u00b7t\u00fcrmt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVPP", "$,", "ADJD", "KON", "ADJD", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Vergebens! Was er t\u00e4glich sonst bezwang,", "tokens": ["Ver\u00b7ge\u00b7bens", "!", "Was", "er", "t\u00e4g\u00b7lich", "sonst", "be\u00b7zwang", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PWS", "PPER", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Es ward ein Berg; er kommt nicht mehr hin\u00fcber.", "tokens": ["Es", "ward", "ein", "Berg", ";", "er", "kommt", "nicht", "mehr", "hin\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "PPER", "VVFIN", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und dennoch, wenn auch tr\u00fcbe, l\u00e4chelt er", "tokens": ["Und", "den\u00b7noch", ",", "wenn", "auch", "tr\u00fc\u00b7be", ",", "l\u00e4\u00b7chelt", "er"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$,", "KOUS", "ADV", "ADJA", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und sucht wie sonst noch mit sich selbst zu scherzen;", "tokens": ["Und", "sucht", "wie", "sonst", "noch", "mit", "sich", "selbst", "zu", "scher\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ADV", "ADV", "APPR", "PRF", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Ein Aktensto\u00df, in t\u00fccht'gen Stein gehauen,", "tokens": ["Ein", "Ak\u00b7ten\u00b7sto\u00df", ",", "in", "t\u00fccht'\u00b7gen", "Stein", "ge\u00b7hau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Es d\u00fcnket ihn kein \u00fcbel Epitaph.", "tokens": ["Es", "d\u00fcn\u00b7ket", "ihn", "kein", "\u00fc\u00b7bel", "E\u00b7pi\u00b7taph", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Doch streng aufs neue schlie\u00dfet sich sein Mund;", "tokens": ["Doch", "streng", "aufs", "neu\u00b7e", "schlie\u00b7\u00dfet", "sich", "sein", "Mund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Er kehrt sich ab, und wieder mit den grellen", "tokens": ["Er", "kehrt", "sich", "ab", ",", "und", "wie\u00b7der", "mit", "den", "grel\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$,", "KON", "ADV", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Pupillen starrt er in die \u00f6de Luft", "tokens": ["Pu\u00b7pil\u00b7len", "starrt", "er", "in", "die", "\u00f6\u00b7de", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und trommelt weiter an die Fensterscheiben.", "tokens": ["Und", "trom\u00b7melt", "wei\u00b7ter", "an", "die", "Fens\u00b7ter\u00b7schei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da wird es pl\u00f6tzlich hell; ein bleicher Strahl", "tokens": ["Da", "wird", "es", "pl\u00f6tz\u00b7lich", "hell", ";", "ein", "blei\u00b7cher", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADJD", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Wintersonne leuchtet ins Gemach", "tokens": ["Der", "Win\u00b7ter\u00b7son\u00b7ne", "leuch\u00b7tet", "ins", "Ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und auf ein Bild gen\u00fcber an der Wand.", "tokens": ["Und", "auf", "ein", "Bild", "ge\u00b7n\u00fc\u00b7ber", "an", "der", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und aus dem Rahmen tritt ein M\u00e4dchenkopf,", "tokens": ["Und", "aus", "dem", "Rah\u00b7men", "tritt", "ein", "M\u00e4d\u00b7chen\u00b7kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Darauf wie Fr\u00fchtau noch die Jugend liegt;", "tokens": ["Da\u00b7rauf", "wie", "Fr\u00fch\u00b7tau", "noch", "die", "Ju\u00b7gend", "liegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOKOM", "NE", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Aus gro\u00dfen, hold erstaunten Augen spr\u00fcht", "tokens": ["Aus", "gro\u00b7\u00dfen", ",", "hold", "er\u00b7staun\u00b7ten", "Au\u00b7gen", "spr\u00fcht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJD", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Verhei\u00dfung aller Erdenseligkeit.", "tokens": ["Ver\u00b7hei\u00b7\u00dfung", "al\u00b7ler", "Er\u00b7den\u00b7se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Er kennt das Wort auf diesen roten Lippen,", "tokens": ["Er", "kennt", "das", "Wort", "auf", "die\u00b7sen", "ro\u00b7ten", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Er nur allein. Erinnrung fa\u00dft ihn an;", "tokens": ["Er", "nur", "al\u00b7lein", ".", "E\u00b7rinn\u00b7rung", "fa\u00dft", "ihn", "an", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "$.", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Fata Morgana steigen auf bet\u00f6rend;", "tokens": ["Fa\u00b7ta", "Mor\u00b7ga\u00b7na", "stei\u00b7gen", "auf", "be\u00b7t\u00f6\u00b7rend", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "VVPP", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Lau wird die Luft \u2013 wie hold die D\u00fcfte wehen!", "tokens": ["Lau", "wird", "die", "Luft", "\u2013", "wie", "hold", "die", "D\u00fcf\u00b7te", "we\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$(", "PWAV", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.12": {"text": "Mit Rosen ist der Garten \u00fcbersch\u00fcttet,", "tokens": ["Mit", "Ro\u00b7sen", "ist", "der", "Gar\u00b7ten", "\u00fc\u00b7ber\u00b7sch\u00fct\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Auf allen B\u00fcschen liegt der Sonnenschein.", "tokens": ["Auf", "al\u00b7len", "B\u00fc\u00b7schen", "liegt", "der", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Die Bienen summen; und ein M\u00e4dchenlachen", "tokens": ["Die", "Bie\u00b7nen", "sum\u00b7men", ";", "und", "ein", "M\u00e4d\u00b7chen\u00b7la\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$.", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Fliegt s\u00fc\u00df und silbern durch den Sommertag.", "tokens": ["Fliegt", "s\u00fc\u00df", "und", "sil\u00b7bern", "durch", "den", "Som\u00b7mer\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Sein Ohr ist trunken. \u00bbOh, nur einmal noch!\u00ab", "tokens": ["Sein", "Ohr", "ist", "trun\u00b7ken", ".", "\u00bb", "Oh", ",", "nur", "ein\u00b7mal", "noch", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$.", "$(", "ITJ", "$,", "ADV", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Er lauscht umsonst, und seufzend sinkt sein Haupt.", "tokens": ["Er", "lauscht", "um\u00b7sonst", ",", "und", "seuf\u00b7zend", "sinkt", "sein", "Haupt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "\u00bbdu starbst \u2013 Wo bist du? \u2013 Gibt es eine Stelle", "tokens": ["\u00bb", "du", "starbst", "\u2013", "Wo", "bist", "du", "?", "\u2013", "Gibt", "es", "ei\u00b7ne", "Stel\u00b7le"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "$(", "PWAV", "VAFIN", "PPER", "$.", "$(", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Noch irgendwo im Weltraum, wo du bist? \u2013", "tokens": ["Noch", "ir\u00b7gend\u00b7wo", "im", "Welt\u00b7raum", ",", "wo", "du", "bist", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$,", "PWAV", "PPER", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Denn da\u00df du mein gewesen, da\u00df das Weib", "tokens": ["Denn", "da\u00df", "du", "mein", "ge\u00b7we\u00b7sen", ",", "da\u00df", "das", "Weib"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "VAPP", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Dem Manne gab der unbekannte Gott \u2013", "tokens": ["Dem", "Man\u00b7ne", "gab", "der", "un\u00b7be\u00b7kann\u00b7te", "Gott", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Ach dieser unergr\u00fcndlich s\u00fc\u00dfe Trunk,", "tokens": ["Ach", "die\u00b7ser", "un\u00b7er\u00b7gr\u00fcnd\u00b7lich", "s\u00fc\u00b7\u00dfe", "Trunk", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PDAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Und s\u00fc\u00dfer stets, je l\u00e4nger du ihn trinkst,", "tokens": ["Und", "s\u00fc\u00b7\u00dfer", "stets", ",", "je", "l\u00e4n\u00b7ger", "du", "ihn", "trinkst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$,", "ADV", "ADJD", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Er l\u00e4\u00dft mich zweifeln an Unsterblichkeit;", "tokens": ["Er", "l\u00e4\u00dft", "mich", "zwei\u00b7feln", "an", "U\u00b7nsterb\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Denn alle Bitternis und Not des Lebens", "tokens": ["Denn", "al\u00b7le", "Bit\u00b7ter\u00b7nis", "und", "Not", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "KON", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Vergilt er tausendfach; und dr\u00fcberhin", "tokens": ["Ver\u00b7gilt", "er", "tau\u00b7send\u00b7fach", ";", "und", "dr\u00fc\u00b7berh\u00b7in"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "$.", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Zu hoffen, zu verlangen wei\u00df ich nichts!\u00ab", "tokens": ["Zu", "hof\u00b7fen", ",", "zu", "ver\u00b7lan\u00b7gen", "wei\u00df", "ich", "nichts", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "VVFIN", "PPER", "PIS", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "In leere Luft ausstreckt er seine Arme:", "tokens": ["In", "lee\u00b7re", "Luft", "aus\u00b7streckt", "er", "sei\u00b7ne", "Ar\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "\u00bbhier diese R\u00e4ume, wo du einst gelebt,", "tokens": ["