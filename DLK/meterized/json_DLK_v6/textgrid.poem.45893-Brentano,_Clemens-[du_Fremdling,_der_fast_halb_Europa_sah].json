{"textgrid.poem.45893": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[du Fremdling, der fast halb Europa sah]", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Fremdling, der fast halb Europa sah,", "tokens": ["Du", "Fremd\u00b7ling", ",", "der", "fast", "halb", "Eu\u00b7ro\u00b7pa", "sah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "ADV", "ADJD", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kein' Albus, nein Karbatschen sollst du kriegen,", "tokens": ["Kein'", "Al\u00b7bus", ",", "nein", "Kar\u00b7bat\u00b7schen", "sollst", "du", "krie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PTKANT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was gestern dir mit Unrecht nicht geschah,", "tokens": ["Was", "ge\u00b7stern", "dir", "mit", "Un\u00b7recht", "nicht", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "APPR", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Dem Schicksal sollst du heute unterliegen,", "tokens": ["Dem", "Schick\u00b7sal", "sollst", "du", "heu\u00b7te", "un\u00b7ter\u00b7lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "K\u00f6mmst du La Rochens Enkelsohn zu nah,", "tokens": ["K\u00f6mmst", "du", "La", "Ro\u00b7chens", "En\u00b7kel\u00b7sohn", "zu", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NE", "NE", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So wirst du gleich die Trepp' hinunter fliegen.", "tokens": ["So", "wirst", "du", "gleich", "die", "Trepp'", "hin\u00b7un\u00b7ter", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}