{"dta.poem.3044": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "90.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer immer auf der Hut, sich zu vertheidigen,", "tokens": ["Wer", "im\u00b7mer", "auf", "der", "Hut", ",", "sich", "zu", "ver\u00b7thei\u00b7di\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ART", "NN", "$,", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht reizen darf den Feind und nicht beleidigen;", "tokens": ["Nicht", "rei\u00b7zen", "darf", "den", "Feind", "und", "nicht", "be\u00b7lei\u00b7di\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "ART", "NN", "KON", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der hat wohl schlimmen Stand und \u00fcblen Feldwachposten,", "tokens": ["Der", "hat", "wohl", "schlim\u00b7men", "Stand", "und", "\u00fcb\u00b7len", "Feld\u00b7wach\u00b7pos\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wobei er wenig Ruh und s\u00fc\u00dfen Schlaf wird kosten.", "tokens": ["Wo\u00b7bei", "er", "we\u00b7nig", "Ruh", "und", "s\u00fc\u00b7\u00dfen", "Schlaf", "wird", "kos\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "KON", "ADJA", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er m\u00f6chte w\u00fcnschen, wenn er d\u00fcrfte, kurze Dauer", "tokens": ["Er", "m\u00f6ch\u00b7te", "w\u00fcn\u00b7schen", ",", "wenn", "er", "d\u00fcrf\u00b7te", ",", "kur\u00b7ze", "Dau\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "KOUS", "PPER", "VMFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Kampfentscheidung statt der langgespannten Lauer.", "tokens": ["Der", "Kamp\u00b7fent\u00b7schei\u00b7dung", "statt", "der", "lang\u00b7ge\u00b7spann\u00b7ten", "Lau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So ist des Menschen Stand gen\u00fcber dem Geschick,", "tokens": ["So", "ist", "des", "Men\u00b7schen", "Stand", "ge\u00b7n\u00fc\u00b7ber", "dem", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor dem er sicher ist nicht einen Augenblick.", "tokens": ["Vor", "dem", "er", "si\u00b7cher", "ist", "nicht", "ei\u00b7nen", "Au\u00b7gen\u00b7blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "VAFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Angreifen darf er nicht, und nicht zur\u00fcck sich ziehn,", "tokens": ["An\u00b7grei\u00b7fen", "darf", "er", "nicht", ",", "und", "nicht", "zu\u00b7r\u00fcck", "sich", "ziehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "$,", "KON", "PTKNEG", "PTKVZ", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur stets gew\u00e4rtig seyn, da\u00df an der Feind greif' ihn.", "tokens": ["Nur", "stets", "ge\u00b7w\u00e4r\u00b7tig", "seyn", ",", "da\u00df", "an", "der", "Feind", "greif'", "ihn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VAINF", "$,", "KOUS", "APPR", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}