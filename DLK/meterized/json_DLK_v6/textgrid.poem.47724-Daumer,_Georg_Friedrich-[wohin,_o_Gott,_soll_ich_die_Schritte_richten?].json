{"textgrid.poem.47724": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wohin, o Gott, soll ich die Schritte richten?]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohin, o Gott, soll ich die Schritte richten?", "tokens": ["Wo\u00b7hin", ",", "o", "Gott", ",", "soll", "ich", "die", "Schrit\u00b7te", "rich\u00b7ten", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "FM", "NN", "$,", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Womit soll ich mein krankes Herz beschwichten?", "tokens": ["Wo\u00b7mit", "soll", "ich", "mein", "kran\u00b7kes", "Herz", "be\u00b7schwich\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es giebt nur einen Weg; ich mu\u00df ihn gehen,", "tokens": ["Es", "giebt", "nur", "ei\u00b7nen", "Weg", ";", "ich", "mu\u00df", "ihn", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis sterbend alle meine Pulse stehen.", "tokens": ["Bis", "ster\u00b7bend", "al\u00b7le", "mei\u00b7ne", "Pul\u00b7se", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "PIAT", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}