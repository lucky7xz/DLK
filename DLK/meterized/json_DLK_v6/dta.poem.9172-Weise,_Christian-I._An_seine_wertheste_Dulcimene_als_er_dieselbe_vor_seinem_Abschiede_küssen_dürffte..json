{"dta.poem.9172": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "I.  \n An seine wertheste Dulcimene/ als er dieselbe vor  \n seinem Abschiede k\u00fcssen d\u00fcrffte.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Dulcimene soll ich k\u00fcssen/", "tokens": ["Dul\u00b7ci\u00b7me\u00b7ne", "soll", "ich", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oder bin ich viel zu schlecht", "tokens": ["O\u00b7der", "bin", "ich", "viel", "zu", "schlecht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PTKA", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Reiner lippen art zu wissen?", "tokens": ["Rei\u00b7ner", "lip\u00b7pen", "art", "zu", "wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gib mir doch vor di\u00dfmahl recht:", "tokens": ["Gib", "mir", "doch", "vor", "di\u00df\u00b7mahl", "recht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ein subtiler ku\u00df/ mein liecht/", "tokens": ["Ein", "sub\u00b7ti\u00b7ler", "ku\u00df", "/", "mein", "liecht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PPOSAT", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Nimmt dir deine farbe nicht.", "tokens": ["Nimmt", "dir", "dei\u00b7ne", "far\u00b7be", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Rosen bleiben dennoch rosen/", "tokens": ["Ro\u00b7sen", "blei\u00b7ben", "den\u00b7noch", "ro\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob der biene gleich beliebt", "tokens": ["Ob", "der", "bie\u00b7ne", "gleich", "be\u00b7liebt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jhren bl\u00e4ttern liebzukosen:", "tokens": ["Ih\u00b7ren", "bl\u00e4t\u00b7tern", "lieb\u00b7zu\u00b7ko\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Drum sey du auch unbetr\u00fcbt/", "tokens": ["Drum", "sey", "du", "auch", "un\u00b7be\u00b7tr\u00fcbt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "ADJD", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Denn du solst mein rosen schein/", "tokens": ["Denn", "du", "solst", "mein", "ro\u00b7sen", "schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich die fromme biene seyn.", "tokens": ["Ich", "die", "from\u00b7me", "bie\u00b7ne", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Dulcimene bist du b\u00f6se/", "tokens": ["Dul\u00b7ci\u00b7me\u00b7ne", "bist", "du", "b\u00f6\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich endlich meinen sinn", "tokens": ["Da\u00df", "ich", "end\u00b7lich", "mei\u00b7nen", "sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von der bl\u00f6den furcht erl\u00f6se/", "tokens": ["Von", "der", "bl\u00f6\u00b7den", "furcht", "er\u00b7l\u00f6\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ein bi\u00dfgen k\u00fchner bin?", "tokens": ["Und", "ein", "bi\u00df\u00b7gen", "k\u00fch\u00b7ner", "bin", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach nimm doch den freyen lauff", "tokens": ["Ach", "nimm", "doch", "den", "frey\u00b7en", "lauff"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVIMP", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Meiner lust nicht \u00fcbel auff.", "tokens": ["Mei\u00b7ner", "lust", "nicht", "\u00fc\u00b7bel", "auff", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "4. La\u00df mich nur/ wer wei\u00df wie lange", "tokens": ["La\u00df", "mich", "nur", "/", "wer", "wei\u00df", "wie", "lan\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "$(", "PWS", "VVFIN", "KOKOM", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du und ich einander sehn.", "tokens": ["Du", "und", "ich", "ein\u00b7an\u00b7der", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meinem hertzen ist schon bange", "tokens": ["Mei\u00b7nem", "hert\u00b7zen", "ist", "schon", "ban\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn es kan gar bald geschehn/", "tokens": ["Denn", "es", "kan", "gar", "bald", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "ADV", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df mein ungewisser fu\u00df", "tokens": ["Da\u00df", "mein", "un\u00b7ge\u00b7wis\u00b7ser", "fu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJD", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Frembder leitung folgen mu\u00df.", "tokens": ["Fremb\u00b7der", "lei\u00b7tung", "fol\u00b7gen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Jtzt bin ich noch bey den linden/", "tokens": ["Jtzt", "bin", "ich", "noch", "bey", "den", "lin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch es ist des gl\u00fcckes-spiel/", "tokens": ["Doch", "es", "ist", "des", "gl\u00fc\u00b7ckes\u00b7spiel", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welches mich von dir entbinden", "tokens": ["Wel\u00b7ches", "mich", "von", "dir", "ent\u00b7bin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht mit macht entreissen wil/", "tokens": ["Nicht", "mit", "macht", "en\u00b7treis\u00b7sen", "wil", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "VVFIN", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und so schick ich mit bedacht", "tokens": ["Und", "so", "schick", "ich", "mit", "be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mich zur letzten guten nacht.", "tokens": ["Mich", "zur", "letz\u00b7ten", "gu\u00b7ten", "nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Was ich unterdessen lebe", "tokens": ["Was", "ich", "un\u00b7ter\u00b7des\u00b7sen", "le\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVINF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leb ich dir und meiner lust/", "tokens": ["Leb", "ich", "dir", "und", "mei\u00b7ner", "lust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "KON", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bi\u00df ich mich darein ergebe", "tokens": ["Bi\u00df", "ich", "mich", "da\u00b7rein", "er\u00b7ge\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "PAV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df du mich gesegnen must/", "tokens": ["Da\u00df", "du", "mich", "ge\u00b7seg\u00b7nen", "must", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wer wei\u00df ob morgen nicht", "tokens": ["Und", "wer", "wei\u00df", "ob", "mor\u00b7gen", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "KOUS", "ADV", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir mein abschieds urtheil spricht.", "tokens": ["Mir", "mein", "ab\u00b7schieds", "ur\u00b7theil", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "7. Nun dieweil wir stets beysammen", "tokens": ["Nun", "die\u00b7weil", "wir", "stets", "bey\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In ergebner treu gelebt/", "tokens": ["In", "er\u00b7geb\u00b7ner", "treu", "ge\u00b7lebt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJD", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ingleichen freundschaffts-flammen", "tokens": ["Und", "in\u00b7glei\u00b7chen", "freund\u00b7schaffts\u00b7flam\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach ergetzlichkeit gestrebt/", "tokens": ["Nach", "er\u00b7getz\u00b7lich\u00b7keit", "ge\u00b7strebt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So gedencke da\u00df ein ku\u00df", "tokens": ["So", "ge\u00b7den\u00b7cke", "da\u00df", "ein", "ku\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KOUS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Unsern spa\u00df versiegeln mu\u00df.", "tokens": ["Un\u00b7sern", "spa\u00df", "ver\u00b7sie\u00b7geln", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "8. Halt mein liebgen halte stille/", "tokens": ["Halt", "mein", "lieb\u00b7gen", "hal\u00b7te", "stil\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "VVFIN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses ist mein erstes mal", "tokens": ["Die\u00b7ses", "ist", "mein", "ers\u00b7tes", "mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich meinen wunsch erf\u00fclle/", "tokens": ["Da\u00df", "ich", "mei\u00b7nen", "wunsch", "er\u00b7f\u00fcl\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und vielleicht die letzte zahl/", "tokens": ["Und", "viel\u00b7leicht", "die", "letz\u00b7te", "zahl", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Von der angenehmen last", "tokens": ["Von", "der", "an\u00b7ge\u00b7neh\u00b7men", "last"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die du mir bereitet hast.", "tokens": ["Die", "du", "mir", "be\u00b7rei\u00b7tet", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "9. Bleib mir nur dabey gewogen/", "tokens": ["Bleib", "mir", "nur", "da\u00b7bey", "ge\u00b7wo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PAV", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und gleich wie der freundschaffts-bund", "tokens": ["Und", "gleich", "wie", "der", "freund\u00b7schaffts\u00b7bund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOKOM", "ART", "NN"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Dich noch niemals hat betrogen/", "tokens": ["Dich", "noch", "nie\u00b7mals", "hat", "be\u00b7tro\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Also gl\u00e4ube/ da\u00df mein mund/", "tokens": ["Al\u00b7so", "gl\u00e4u\u00b7be", "/", "da\u00df", "mein", "mund", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Welcher dich so furchtsam k\u00fcst/", "tokens": ["Wel\u00b7cher", "dich", "so", "furcht\u00b7sam", "k\u00fcst", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bl\u00f6de/ doch best\u00e4ndig ist.", "tokens": ["Bl\u00f6\u00b7de", "/", "doch", "be\u00b7st\u00e4n\u00b7dig", "ist", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "10. Wilst du meiner bald vergessen/", "tokens": ["Wilst", "du", "mei\u00b7ner", "bald", "ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "ADV", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So vergi\u00df mich immerhin/", "tokens": ["So", "ver\u00b7gi\u00df", "mich", "im\u00b7mer\u00b7hin", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn weil ich am leib entsessen/", "tokens": ["Denn", "weil", "ich", "am", "leib", "ent\u00b7ses\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPRART", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An der seele bey dir bin/", "tokens": ["An", "der", "see\u00b7le", "bey", "dir", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPER", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denck ich doppelt/ halb vor mich/", "tokens": ["Denck", "ich", "dop\u00b7pelt", "/", "halb", "vor", "mich", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "$(", "ADJD", "APPR", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und/ mein leben/ halb vor dich.", "tokens": ["Und", "/", "mein", "le\u00b7ben", "/", "halb", "vor", "dich", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PPOSAT", "VVINF", "$(", "ADJD", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}