{"textgrid.poem.39561": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zu guten Mu\u00df-Almanachen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu guten Mu\u00df-Almanachen,", "tokens": ["Zu", "gu\u00b7ten", "Mu\u00df\u00b7Al\u00b7ma\u00b7nach\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mu\u00df man haben dreierlei Sachen.", "tokens": ["Mu\u00df", "man", "ha\u00b7ben", "drei\u00b7er\u00b7lei", "Sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Deutschheit, Romantik und Melancholei", "tokens": ["Deutschheit", ",", "Ro\u00b7man\u00b7tik", "und", "Me\u00b7lan\u00b7cho\u00b7lei"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "R\u00fchre zu Brei;", "tokens": ["R\u00fch\u00b7re", "zu", "Brei", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Thu' das Kinderp\u00e4ppchen", "tokens": ["Thu'", "das", "Kin\u00b7der\u00b7p\u00e4pp\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "In ein fein saubres L\u00e4ppchen;", "tokens": ["In", "ein", "fein", "sau\u00b7bres", "L\u00e4pp\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Schn\u00fcrchen herum!", "tokens": ["Schn\u00fcr\u00b7chen", "he\u00b7rum", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Dann la\u00df zutschen das Publicum.", "tokens": ["Dann", "la\u00df", "zut\u00b7schen", "das", "Pub\u00b7li\u00b7cum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}