{"dta.poem.19588": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der Berg der Seligkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Bergesr\u00fccken still besonnt,", "tokens": ["Ein", "Ber\u00b7ge\u00b7sr\u00fc\u00b7cken", "still", "be\u00b7sonnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Allum der duft'ge Horizont \u2014", "tokens": ["Al\u00b7lum", "der", "duft'\u00b7ge", "Ho\u00b7ri\u00b7zont"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier sa\u00df der Christ und rings im Kreis", "tokens": ["Hier", "sa\u00df", "der", "Christ", "und", "rings", "im", "Kreis"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Galil\u00e4er stufenweis", "tokens": ["Die", "Ga\u00b7li\u00b7l\u00e4\u00b7er", "stu\u00b7fen\u00b7weis"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gelagert auf den steilen Triften \u2014", "tokens": ["Ge\u00b7la\u00b7gert", "auf", "den", "stei\u00b7len", "Trif\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Meister lobt' der Lilie Kleid,", "tokens": ["Der", "Meis\u00b7ter", "lobt'", "der", "Li\u00b7lie", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hie\u00df g\u00f6ttlich Werk das Friedestiften", "tokens": ["Hie\u00df", "g\u00f6tt\u00b7lich", "Werk", "das", "Frie\u00b7des\u00b7tif\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und r\u00fchmte die Barmherzigkeit.", "tokens": ["Und", "r\u00fchm\u00b7te", "die", "Barm\u00b7her\u00b7zig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Er lie\u00df die Segensschwingen breiten", "tokens": ["Er", "lie\u00df", "die", "Se\u00b7gens\u00b7schwin\u00b7gen", "brei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "All seines Reiches Seligkeiten.", "tokens": ["All", "sei\u00b7nes", "Rei\u00b7ches", "Se\u00b7lig\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Dann ist er sacht hinabgegangen ...", "tokens": ["Dann", "ist", "er", "sacht", "hin\u00b7ab\u00b7ge\u00b7gan\u00b7gen", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und hat am Kreuzesstamm gehangen.", "tokens": ["Und", "hat", "am", "Kreu\u00b7zes\u00b7stamm", "ge\u00b7han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Am Berg der Seligkeiten irrten", "tokens": ["Am", "Berg", "der", "Se\u00b7lig\u00b7kei\u00b7ten", "irr\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Hirtin Stapfen und des Hirten,", "tokens": ["Der", "Hir\u00b7tin", "Stap\u00b7fen", "und", "des", "Hir\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Wolken still, wie St\u00fcrme brausend,", "tokens": ["Wie", "Wol\u00b7ken", "still", ",", "wie", "St\u00fcr\u00b7me", "brau\u00b7send", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PTKVZ", "$,", "PWAV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zog dran vor\u00fcber ein Jahrtausend,", "tokens": ["Zog", "dran", "vor\u00b7\u00fc\u00b7ber", "ein", "Jahr\u00b7tau\u00b7send", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Lilie blieb des Lobes froh,", "tokens": ["Die", "Li\u00b7lie", "blieb", "des", "Lo\u00b7bes", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie kleide sich wie Salomo,", "tokens": ["Sie", "klei\u00b7de", "sich", "wie", "Sa\u00b7lo\u00b7mo", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "KOKOM", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Luft, drin nie das Erz erscholl,", "tokens": ["Die", "Luft", ",", "drin", "nie", "das", "Erz", "er\u00b7scholl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist noch von Friedeworten voll.", "tokens": ["Ist", "noch", "von", "Frie\u00b7de\u00b7wor\u00b7ten", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Drommetensto\u00df! Jach klimmt empor", "tokens": ["Drom\u00b7me\u00b7ten\u00b7sto\u00df", "!", "Jach", "klimmt", "em\u00b7por"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "NE", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Heer, das Schlacht und Raum verlor.", "tokens": ["Ein", "Heer", ",", "das", "Schlacht", "und", "Raum", "ver\u00b7lor", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kreuzritter sind's, von Saladin", "tokens": ["Kreuz\u00b7rit\u00b7ter", "sin\u00b7d's", ",", "von", "Sa\u00b7la\u00b7din"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "$,", "APPR", "NE"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Versprengt, die wild zur H\u00f6he fliehn,", "tokens": ["Ver\u00b7sprengt", ",", "die", "wild", "zur", "H\u00f6\u00b7he", "fliehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hei\u00df unter ihren Schritten her", "tokens": ["Hei\u00df", "un\u00b7ter", "ih\u00b7ren", "Schrit\u00b7ten", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Entflammt den d\u00fcrren Rasen er,", "tokens": ["Ent\u00b7flammt", "den", "d\u00fcr\u00b7ren", "Ra\u00b7sen", "er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In schwarzen Wolken wallt der Qualm.", "tokens": ["In", "schwar\u00b7zen", "Wol\u00b7ken", "wallt", "der", "Qualm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Schlachtrosse schnauben auf der Alm.", "tokens": ["Schlach\u00b7tros\u00b7se", "schnau\u00b7ben", "auf", "der", "Alm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Scharf pfeifen Sarazenenpfeile", "tokens": ["Scharf", "pfei\u00b7fen", "Sa\u00b7ra\u00b7zen\u00b7en\u00b7pfei\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Durch das Gedr\u00e4ng der Flucht und Eile.", "tokens": ["Durch", "das", "Ge\u00b7dr\u00e4ng", "der", "Flucht", "und", "Ei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Fort! Ein verf\u00e4rbter Purpur weht,", "tokens": ["Fort", "!", "Ein", "ver\u00b7f\u00e4rb\u00b7ter", "Pur\u00b7pur", "weht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ein junger K\u00f6nig wankt entkr\u00e4ftet,", "tokens": ["Ein", "jun\u00b7ger", "K\u00f6\u00b7nig", "wankt", "ent\u00b7kr\u00e4f\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Doch dieses Reiches Majest\u00e4t", "tokens": ["Doch", "die\u00b7ses", "Rei\u00b7ches", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Ist K\u00f6nig Christ, ans Kreuz geheftet.", "tokens": ["Ist", "K\u00f6\u00b7nig", "Christ", ",", "ans", "Kreuz", "ge\u00b7hef\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "NE", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Drum tragen sie das Kreuz voran,", "tokens": ["Drum", "tra\u00b7gen", "sie", "das", "Kreuz", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Der Welterbarmer schwebte dran,", "tokens": ["Der", "Wel\u00b7ter\u00b7bar\u00b7mer", "schweb\u00b7te", "dran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Das bittre Kreuz, davon herab", "tokens": ["Das", "bitt\u00b7re", "Kreuz", ",", "da\u00b7von", "her\u00b7ab"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PAV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Er seines Mordes Schuld vergab.", "tokens": ["Er", "sei\u00b7nes", "Mor\u00b7des", "Schuld", "ver\u00b7gab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Sie wuschen's dann mit rothen B\u00e4chen,", "tokens": ["Sie", "wu\u00b7schen's", "dann", "mit", "ro\u00b7then", "B\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Um des Erbarmers Tod zu r\u00e4chen ...", "tokens": ["Um", "des", "Er\u00b7bar\u00b7mers", "Tod", "zu", "r\u00e4\u00b7chen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Das W\u00fcthen, Morden, Bluten, Streiten", "tokens": ["Das", "W\u00fct\u00b7hen", ",", "Mor\u00b7den", ",", "Blu\u00b7ten", ",", "Strei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Ersteigt den Berg der Seligkeiten.", "tokens": ["Er\u00b7steigt", "den", "Berg", "der", "Se\u00b7lig\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Erklommen ist der Gipfel jetzt", "tokens": ["Er\u00b7klom\u00b7men", "ist", "der", "Gip\u00b7fel", "jetzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Und hinter ihm erbraust das Meer,", "tokens": ["Und", "hin\u00b7ter", "ihm", "er\u00b7braust", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Der Kurdenschleuder ausgesetzt,", "tokens": ["Der", "Kur\u00b7den\u00b7schleu\u00b7der", "aus\u00b7ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Steht auf dem Kulm das Christenheer.", "tokens": ["Steht", "auf", "dem", "Kulm", "das", "Chris\u00b7ten\u00b7heer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Drommetensto\u00df! \u201eDer Heiland lebt!", "tokens": ["Drom\u00b7me\u00b7ten\u00b7sto\u00df", "!", "\u201e", "Der", "Hei\u00b7land", "lebt", "!"], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Christus regiert!\u201c Der Berg erbebt.", "tokens": ["Chris\u00b7tus", "re\u00b7giert", "!", "\u201c", "Der", "Berg", "er\u00b7bebt", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "$(", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201ehilf, K\u00f6nig, der gekreuzigt wurde!\u201c", "tokens": ["\u201e", "hilf", ",", "K\u00f6\u00b7nig", ",", "der", "ge\u00b7kreu\u00b7zigt", "wur\u00b7de", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "$,", "NN", "$,", "PRELS", "VVPP", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u201ezielt auf das Kreuz!\u201c befiehlt der Kurde.", "tokens": ["\u201e", "zielt", "auf", "das", "Kreuz", "!", "\u201c", "be\u00b7fiehlt", "der", "Kur\u00b7de", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "ART", "NN", "$.", "$(", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u201ewie bl\u00f6de Falter um die Flamme,", "tokens": ["\u201e", "wie", "bl\u00f6\u00b7de", "Fal\u00b7ter", "um", "die", "Flam\u00b7me", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So flattern sie am Kreuzesstamme!\u201c", "tokens": ["So", "flat\u00b7tern", "sie", "am", "Kreu\u00b7zes\u00b7stam\u00b7me", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es saust. Steilnieder zu der Bucht", "tokens": ["Es", "saust", ".", "Steil\u00b7nie\u00b7der", "zu", "der", "Bucht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "St\u00fcrzt Ro\u00df und Reiter in die Schlucht.", "tokens": ["St\u00fcrzt", "Ro\u00df", "und", "Rei\u00b7ter", "in", "die", "Schlucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Kreuz, mit Glut und br\u00fcnst'ger Hast", "tokens": ["Das", "Kreuz", ",", "mit", "Glut", "und", "br\u00fcnst'\u00b7ger", "Hast"], "token_info": ["w