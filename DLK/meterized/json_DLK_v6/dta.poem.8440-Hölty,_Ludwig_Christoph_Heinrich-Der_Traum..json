{"dta.poem.8440": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Traum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Und flog auf ihren Schooss,", "tokens": ["Und", "flog", "auf", "ih\u00b7ren", "Scho\u00b7oss", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und zupft' ihr, um nicht lass zu sein,", "tokens": ["Und", "zupft'", "ihr", ",", "um", "nicht", "lass", "zu", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUI", "PTKNEG", "VVFIN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Busenschleifen los;", "tokens": ["Die", "Bu\u00b7sen\u00b7schlei\u00b7fen", "los", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und flog mit gaukelhaften Flug,", "tokens": ["Und", "flog", "mit", "gau\u00b7kel\u00b7haf\u00b7ten", "Flug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann auf die weisse Hand,", "tokens": ["Dann", "auf", "die", "weis\u00b7se", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Dann wieder auf das Busentuch,", "tokens": ["Dann", "wie\u00b7der", "auf", "das", "Bu\u00b7sen\u00b7tuch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und pickt' am rothen Band.", "tokens": ["Und", "pickt'", "am", "ro\u00b7then", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dann schwebt' ich auf ihr blondes Haar,", "tokens": ["Dann", "schwebt'", "ich", "auf", "ihr", "blon\u00b7des", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und zwitscherte vor Lust,", "tokens": ["Und", "zwit\u00b7scher\u00b7te", "vor", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ruhte, wann ich m\u00fcde war,", "tokens": ["Und", "ruh\u00b7te", ",", "wann", "ich", "m\u00fc\u00b7de", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An ihrer weissen Brust.", "tokens": ["An", "ih\u00b7rer", "weis\u00b7sen", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kein Veilchenbett' im Paradies", "tokens": ["Kein", "Veil\u00b7chen\u00b7bett'", "im", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Geht diesem Lager vor.", "tokens": ["Geht", "die\u00b7sem", "La\u00b7ger", "vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie schlief sichs da so s\u00fcss, so s\u00fcss,", "tokens": ["Wie", "schlief", "sichs", "da", "so", "s\u00fcss", ",", "so", "s\u00fcss", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "ADV", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "An ihres Busens Flor!", "tokens": ["An", "ih\u00b7res", "Bu\u00b7sens", "Flor", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie spielte, wie ich tiefer sank,", "tokens": ["Sie", "spiel\u00b7te", ",", "wie", "ich", "tie\u00b7fer", "sank", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit leisem Fingerschlag,", "tokens": ["Mit", "lei\u00b7sem", "Fin\u00b7ger\u00b7schlag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der mir durch Leib und Leben drang,", "tokens": ["Der", "mir", "durch", "Leib", "und", "Le\u00b7ben", "drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich frohen Schlnmmrer wach;", "tokens": ["Mich", "fro\u00b7hen", "Schlnmm\u00b7rer", "wach", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sah mich so wunderfreundlich an,", "tokens": ["Sah", "mich", "so", "wun\u00b7der\u00b7freund\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und bot den Mund mir dar:", "tokens": ["Und", "bot", "den", "Mund", "mir", "dar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dass ich es nicht beschreiben kann,", "tokens": ["Dass", "ich", "es", "nicht", "be\u00b7schrei\u00b7ben", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie froh, wie froh ich war.", "tokens": ["Wie", "froh", ",", "wie", "froh", "ich", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da trippelt' ich auf einem Bein,", "tokens": ["Da", "trip\u00b7pelt'", "ich", "auf", "ei\u00b7nem", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hatte so mein Spiel,", "tokens": ["Und", "hat\u00b7te", "so", "mein", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und spielt' ihr mit dem Fl\u00fcgelein", "tokens": ["Und", "spielt'", "ihr", "mit", "dem", "Fl\u00fc\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die rothe Wange k\u00fchl.", "tokens": ["Die", "ro\u00b7the", "Wan\u00b7ge", "k\u00fchl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch ach! kein Erdengl\u00fcck besteht,", "tokens": ["Doch", "ach", "!", "kein", "Er\u00b7den\u00b7gl\u00fcck", "be\u00b7steht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es sei Tag oder Nacht!", "tokens": ["Es", "sei", "Tag", "o\u00b7der", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Schnell war mein s\u00fcsser Traum verweht,", "tokens": ["Schnell", "war", "mein", "s\u00fcs\u00b7ser", "Traum", "ver\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und ich war aufgewacht.", "tokens": ["Und", "ich", "war", "auf\u00b7ge\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}