{"dta.poem.8844": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Mich erw\u00e4hlt, umschlossen h\u00e4lt vor Allen?", "tokens": ["Mich", "er\u00b7w\u00e4hlt", ",", "um\u00b7schlos\u00b7sen", "h\u00e4lt", "vor", "Al\u00b7len", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVPP", "VVFIN", "APPR", "NE", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Fragst, warum zu mir, dem Fernen, pilgernd", "tokens": ["Fragst", ",", "wa\u00b7rum", "zu", "mir", ",", "dem", "Fer\u00b7nen", ",", "pil\u00b7gernd"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$,", "PWAV", "APPR", "PPER", "$,", "ART", "ADJA", "$,", "VVPP"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Deine heimlichsten Gedanken wallen?", "tokens": ["Dei\u00b7ne", "heim\u00b7lichs\u00b7ten", "Ge\u00b7dan\u00b7ken", "wal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Wei\u00df ich's selbst? Vermag ich's selbst zu deuten,", "tokens": ["Wei\u00df", "ich's", "selbst", "?", "Ver\u00b7mag", "ich's", "selbst", "zu", "deu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$.", "VVFIN", "PIS", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Welch ein sch\u00f6ner Wahn dich \u00fcberfallen?", "tokens": ["Welch", "ein", "sch\u00f6\u00b7ner", "Wahn", "dich", "\u00fc\u00b7berf\u00b7al\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Glaubst du nicht, es sey mein Herz die Zither,", "tokens": ["Glaubst", "du", "nicht", ",", "es", "sey", "mein", "Herz", "die", "Zi\u00b7ther", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Deren Saiten allgemach verhallen?", "tokens": ["De\u00b7ren", "Sai\u00b7ten", "all\u00b7ge\u00b7mach", "ver\u00b7hal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "F\u00fchlst du nicht, da\u00df diese leichten Lieder", "tokens": ["F\u00fchlst", "du", "nicht", ",", "da\u00df", "die\u00b7se", "leich\u00b7ten", "Lie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Sterblich seyen, wie die Nachtigallen?", "tokens": ["Sterb\u00b7lich", "se\u00b7yen", ",", "wie", "die", "Nach\u00b7ti\u00b7gal\u00b7len", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "PWAV", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Giebst du dich f\u00fcr mich? Du gleichst dem Wilden,", "tokens": ["Giebst", "du", "dich", "f\u00fcr", "mich", "?", "Du", "gleichst", "dem", "Wil\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPER", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Eitlen Tand erkaufend mit Metallen.", "tokens": ["Eit\u00b7len", "Tand", "er\u00b7kau\u00b7fend", "mit", "Me\u00b7tal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}