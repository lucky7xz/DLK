{"dta.poem.3401": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Sch\u00f6n Sidselil und Ritter Ingild.  \n  Altd\u00e4nisch .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n Sidselil schn&#252;rte sich so knapp und", "tokens": ["Sch\u00f6n", "Sid\u00b7se\u00b7lil", "schn", "&#252;", "rte", "sich", "so", "knapp", "und"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "$(", "VVFIN", "PRF", "ADV", "ADJD", "KON"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "schlank,", "tokens": ["schlank", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Dass ihr die Milch aus den Br\u00fcsten sprang.", "tokens": ["Dass", "ihr", "die", "Milch", "aus", "den", "Br\u00fcs\u00b7ten", "sprang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "\u201ewas seh ich, herzliebes T\u00f6chterlein!", "tokens": ["\u201e", "was", "seh", "ich", ",", "herz\u00b7lie\u00b7bes", "T\u00f6ch\u00b7ter\u00b7lein", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u201edir spritzt ja die Milch aus den Br\u00fcsten dein!\u201c", "tokens": ["\u201e", "dir", "spritzt", "ja", "die", "Milch", "aus", "den", "Br\u00fcs\u00b7ten", "dein", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "PPOSAT", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "\u201ees ist nicht Milch, was den Busen mir n\u00e4sst.", "tokens": ["\u201e", "es", "ist", "nicht", "Milch", ",", "was", "den", "Bu\u00b7sen", "mir", "n\u00e4sst", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PTKNEG", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u201ees ist vom Meth, den ich eben gepresst.\u201c", "tokens": ["\u201e", "es", "ist", "vom", "Me\u00b7th", ",", "den", "ich", "e\u00b7ben", "ge\u00b7presst", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "APPRART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "\u201elehre du mich kennen Milch und Meth!", "tokens": ["\u201e", "leh\u00b7re", "du", "mich", "ken\u00b7nen", "Milch", "und", "Me\u00b7th", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PRF", "VVINF", "NN", "KON", "NE", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "\u201eist braun denn die Milch und weiss der Meth?\u201c", "tokens": ["\u201e", "ist", "braun", "denn", "die", "Milch", "und", "weiss", "der", "Me\u00b7th", "?", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "ADJD", "KON", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "\u201each Mutter vergebt, Herzmutter verzeiht!", "tokens": ["\u201e", "ach", "Mut\u00b7ter", "ver\u00b7gebt", ",", "Herz\u00b7mut\u00b7ter", "ver\u00b7zeiht", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "XY", "NN", "VVFIN", "$,", "NN", "VVFIN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u201eritter Ingild, der Wackre, hat um mich gefreyt.\u201c", "tokens": ["\u201e", "rit\u00b7ter", "In\u00b7gild", ",", "der", "Wack\u00b7re", ",", "hat", "um", "mich", "ge\u00b7freyt", ".", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "ART", "NN", "$,", "VAFIN", "APPR", "PPER", "VVPP", "$.", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.6": {"line.1": {"text": "\u201eritter Ingild, der Kecke, th\u00e4t um dich", "tokens": ["\u201e", "rit\u00b7ter", "In\u00b7gild", ",", "der", "Ke\u00b7cke", ",", "th\u00e4t", "um", "dich"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADJA", "NN", "$,", "ART", "NN", "$,", "VVFIN", "APPR", "PPER"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "freyn?", "tokens": ["freyn", "?"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "\u201ewas gab er dir denn f\u00fcr die Ehre dein?\u201c", "tokens": ["\u201e", "was", "gab", "er", "dir", "denn", "f\u00fcr", "die", "Eh\u00b7re", "dein", "?", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "PPER", "ADV", "APPR", "ART", "NN", "PPOSAT", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "\u201eer gab mir ein rothseidenes Kleid.", "tokens": ["\u201e", "er", "gab", "mir", "ein", "roth\u00b7sei\u00b7de\u00b7nes", "Kleid", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "\u201each, aber ich trug es mit Jammer und Leid.", "tokens": ["\u201e", "ach", ",", "a\u00b7ber", "ich", "trug", "es", "mit", "Jam\u00b7mer", "und", "Leid", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "KON", "PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.8": {"line.1": {"text": "\u201eer gab mir ein Paar silberspangige Schuh.", "tokens": ["\u201e", "er", "gab", "mir", "ein", "Paar", "sil\u00b7ber\u00b7span\u00b7gi\u00b7ge", "Schuh", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--++-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "\u201each aber, sie la'n mir nicht Rast noch Ruh.", "tokens": ["\u201e", "ach", "a\u00b7ber", ",", "sie", "la'n", "mir", "nicht", "Rast", "noch", "Ruh", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "ADV", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "NN", "ADV", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.9": {"line.1": {"text": "\u201eer gab mir eine Harfe von Gold,", "tokens": ["\u201e", "er", "gab", "mir", "ei\u00b7ne", "Har\u00b7fe", "von", "Gold", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "\u201edie meinen Jammer beschwichtigen sollt.", "tokens": ["\u201e", "die", "mei\u00b7nen", "Jam\u00b7mer", "be\u00b7schwich\u00b7ti\u00b7gen", "sollt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "\u201enun, Tochter, ich schw\u00f6re beym heiligen", "tokens": ["\u201e", "nun", ",", "Toch\u00b7ter", ",", "ich", "schw\u00f6\u00b7re", "beym", "hei\u00b7li\u00b7gen"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "$,", "NN", "$,", "PPER", "VVFIN", "APPRART", "ADJA"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Gott!", "tokens": ["Gott", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "\u201eihr beyde m\u00fcsst sterben den schm\u00e4hlichsten Tod.", "tokens": ["\u201e", "ihr", "bey\u00b7de", "m\u00fcsst", "ster\u00b7ben", "den", "schm\u00e4h\u00b7lichs\u00b7ten", "Tod", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "PIS", "VMFIN", "VVINF", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.11": {"line.1": {"text": "\u201eritter Ingild muss hangen hoch oben auf Gaal,", "tokens": ["\u201e", "rit\u00b7ter", "In\u00b7gild", "muss", "han\u00b7gen", "hoch", "o\u00b7ben", "auf", "Gaal", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "VMFIN", "VVINF", "ADJD", "ADV", "APPR", "NN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "\u201eund brennen musst du tief unten im Thal.\u201c", "tokens": ["\u201e", "und", "bren\u00b7nen", "musst", "du", "tief", "un\u00b7ten", "im", "Thal", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "VVINF", "VMFIN", "PPER", "ADJD", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}}, "stanza.12": {"line.1": {"text": "Sch\u00f6n Sidselil nahm ihre Harfe gut,", "tokens": ["Sch\u00f6n", "Sid\u00b7se\u00b7lil", "nahm", "ih\u00b7re", "Har\u00b7fe", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu schwichtigen ihren traurigen Muth.", "tokens": ["Zu", "schwich\u00b7ti\u00b7gen", "ih\u00b7ren", "trau\u00b7ri\u00b7gen", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.13": {"line.1": {"text": "Sie schlich wohl hin im Mondenschein", "tokens": ["Sie", "schlich", "wohl", "hin", "im", "Mon\u00b7den\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor Ritter Ingilds K\u00e4mmerlein.", "tokens": ["Vor", "Rit\u00b7ter", "In\u00b7gilds", "K\u00e4m\u00b7mer\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Sie schlug die lispelnden Saiten an,", "tokens": ["Sie", "schlug", "die", "lis\u00b7peln\u00b7den", "Sai\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zu wecken den schlafenden Rittersmann.", "tokens": ["Zu", "we\u00b7cken", "den", "schla\u00b7fen\u00b7den", "Rit\u00b7ters\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.15": {"line.1": {"text": "Sie schlug die Saiten rauschender an;", "tokens": ["Sie", "schlug", "die", "Sai\u00b7ten", "rau\u00b7schen\u00b7der", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Noch s\u00e4umte der traurende Rittersmann.", "tokens": ["Noch", "s\u00e4um\u00b7te", "der", "trau\u00b7ren\u00b7de", "Rit\u00b7ters\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.16": {"line.1": {"text": "Sie lispelte leise zum Schl\u00fcsselloch 'nein:", "tokens": ["Sie", "lis\u00b7pel\u00b7te", "lei\u00b7se", "zum", "Schl\u00fcs\u00b7sel\u00b7loch", "'n\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "NE", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "\u201ewach auf, Ritter Ingild, und lass mich ein.\u201c", "tokens": ["\u201e", "wach", "auf", ",", "Rit\u00b7ter", "In\u00b7gild", ",", "und", "lass", "mich", "ein", ".", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "PTKVZ", "$,", "NN", "NE", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.17": {"line.1": {"text": "\u201eich bin noch so m\u00fcde. Ich schlief erst ein.", "tokens": ["\u201e", "ich", "bin", "noch", "so", "m\u00fc\u00b7de", ".", "Ich", "schlief", "erst", "ein", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "ADV", "ADJD", "$.", "PPER", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "\u201eich stehe nicht auf, und lasse nicht ein.\u201c", "tokens": ["\u201e", "ich", "ste\u00b7he", "nicht", "auf", ",", "und", "las\u00b7se", "nicht", "ein", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "PTKVZ", "$,", "KON", "VVFIN", "PTKNEG", "PTKVZ", "$.", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.18": {"line.1": {"text": "\u201eritter Ingild steh auf, und lass mich ein.", "tokens": ["\u201e", "rit\u00b7ter", "In\u00b7gild", "steh", "auf", ",", "und", "lass", "mich", "ein", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "\u201eich habe gesprochen mit Mutter mein.", "tokens": ["\u201e", "ich", "ha\u00b7be", "ge\u00b7spro\u00b7chen", "mit", "Mut\u00b7ter", "mein", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "VVPP", "APPR", "NN", "PPOSAT", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.19": {"line.1": {"text": "\u201esie hat mir geschworen beym heiligen Gott,", "tokens": ["\u201e", "sie", "hat", "mir", "ge\u00b7schwo\u00b7ren", "beym", "hei\u00b7li\u00b7gen", "Gott", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "\u201ewir m\u00fcssten sterben den schm\u00e4hlichsten Tod;", "tokens": ["\u201e", "wir", "m\u00fcss\u00b7ten", "ster\u00b7ben", "den", "schm\u00e4h\u00b7lichs\u00b7ten", "Tod", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "VVINF", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.20": {"line.1": {"text": "\u201edu m\u00fcsstest hangen hoch oben auf Gaal,", "tokens": ["\u201e", "du", "m\u00fcss\u00b7test", "han\u00b7gen", "hoch", "o\u00b7ben", "auf", "Gaal", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "VVINF", "ADJD", "ADV", "APPR", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u201eund brennen m\u00fcsst' ich tief unten im Thal!\u201c", "tokens": ["\u201e", "und", "bren\u00b7nen", "m\u00fcsst'", "ich", "tief", "un\u00b7ten", "im", "Thal", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "VVINF", "VMFIN", "PPER", "ADJD", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}}, "stanza.21": {"line.1": {"text": "\u201enein, Liebchen, du sollst nicht brennen f\u00fcr mich;", "tokens": ["\u201e", "nein", ",", "Lieb\u00b7chen", ",", "du", "sollst", "nicht", "bren\u00b7nen", "f\u00fcr", "mich", ";"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "NN", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "APPR", "PPER", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u201eund ich, ich will nicht hangen f\u00fcr dich.", "tokens": ["\u201e", "und", "ich", ",", "ich", "will", "nicht", "han\u00b7gen", "f\u00fcr", "dich", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "APPR", "PPER", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.22": {"line.1": {"text": "\u201egeh eilend, und nimm dein Gold aus dem", "tokens": ["\u201e", "geh", "ei\u00b7lend", ",", "und", "nimm", "dein", "Gold", "aus", "dem"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "VVPP", "$,", "KON", "VVIMP", "PPOSAT", "NN", "APPR", "ART"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schrein,", "tokens": ["Schrein", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "\u201ederweile ich sattle den Rappen mein.\u201c", "tokens": ["\u201e", "der\u00b7wei\u00b7le", "ich", "satt\u00b7le", "den", "Rap\u00b7pen", "mein", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PRELS", "PPER", "VVFIN", "ART", "NN", "PPOSAT", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.23": {"line.1": {"text": "Er warf ihr \u00fcber den Mantel blau,", "tokens": ["Er", "warf", "ihr", "\u00fc\u00b7ber", "den", "Man\u00b7tel", "blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und hob sie auf seinen Rappen grau.", "tokens": ["Und", "hob", "sie", "auf", "sei\u00b7nen", "Rap\u00b7pen", "grau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.24": {"line.1": {"text": "Sie ritten so rasch, sie ritten so lang;", "tokens": ["Sie", "rit\u00b7ten", "so", "rasch", ",", "sie", "rit\u00b7ten", "so", "lang", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sch\u00f6n Sidselil \u00e4chzte so, schwer und bang.", "tokens": ["Sch\u00f6n", "Sid\u00b7se\u00b7lil", "\u00e4chz\u00b7te", "so", ",", "schwer", "und", "bang", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADV", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.25": {"line.1": {"text": "\u201esch\u00f6n Sidselil, wird dir der Weg zu lang,", "tokens": ["\u201e", "sch\u00f6n", "Sid\u00b7se\u00b7lil", ",", "wird", "dir", "der", "Weg", "zu", "lang", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NE", "$,", "VAFIN", "PPER", "ART", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u201eoder macht mein Sattel dir weh und bang?\u201c", "tokens": ["\u201e", "o\u00b7der", "macht", "mein", "Sat\u00b7tel", "dir", "weh", "und", "bang", "?", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "VVFIN", "PPOSAT", "NN", "PPER", "PTKVZ", "KON", "ADJD", "$.", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.26": {"line.1": {"text": "\u201eritter Ingild, der Weg wird mir nicht lang;", "tokens": ["\u201e", "rit\u00b7ter", "In\u00b7gild", ",", "der", "Weg", "wird", "mir", "nicht", "lang", ";"], "token_info": ["punct", "word", "word", "punct", "w