{"textgrid.poem.51997": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Lentz tritt wieder ein: Auch deine Frau die nimmt", "tokens": ["Der", "Lentz", "tritt", "wie\u00b7der", "ein", ":", "Auch", "dei\u00b7ne", "Frau", "die", "nimmt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$.", "ADV", "PPOSAT", "NN", "ART", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr numehr wieder vor auff Hirschberg zu zu reisen:", "tokens": ["Ihr", "nu\u00b7mehr", "wie\u00b7der", "vor", "auff", "Hirschberg", "zu", "zu", "rei\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPR", "APPR", "NN", "PTKZU", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Es ist wie ihr Geding. Die Wiege wird es weisen,", "tokens": ["Es", "ist", "wie", "ihr", "Ge\u00b7ding", ".", "Die", "Wie\u00b7ge", "wird", "es", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "PPOSAT", "NN", "$.", "ART", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was Sie allda verbracht, nachdem sie wieder k\u00f6mmt:", "tokens": ["Was", "Sie", "all\u00b7da", "ver\u00b7bracht", ",", "nach\u00b7dem", "sie", "wie\u00b7der", "k\u00f6mmt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVPP", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Und wenn sie fr\u00f6mmer w\u00e4r, als ie Sybilla war,", "tokens": ["Und", "wenn", "sie", "fr\u00f6m\u00b7mer", "w\u00e4r", ",", "als", "ie", "Sy\u00b7bil\u00b7la", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "KOUS", "ADV", "NE", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die dem Pompilius das theure Buch verla\u00dfen:", "tokens": ["Die", "dem", "Pom\u00b7pi\u00b7lius", "das", "theu\u00b7re", "Buch", "ver\u00b7la\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NE", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Doch kan sie in dem Bad auch blinde Flammen fa\u00dfen,", "tokens": ["Doch", "kan", "sie", "in", "dem", "Bad", "auch", "blin\u00b7de", "Flam\u00b7men", "fa\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "ART", "NE", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und stehet wegen Glutt im Wa\u00dfer in Gefahr.", "tokens": ["Und", "ste\u00b7het", "we\u00b7gen", "Glutt", "im", "Wa\u00b7\u00dfer", "in", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPRART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nu Stangen tr\u00e4gt dis Thier, davon man nennt die Bahn,", "tokens": ["Nu", "Stan\u00b7gen", "tr\u00e4gt", "dis", "Thier", ",", "da\u00b7von", "man", "nennt", "die", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PDS", "NN", "$,", "PAV", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wer sieht diesen Leib hier unterm Brunnen spielen", "tokens": ["Und", "wer", "sieht", "die\u00b7sen", "Leib", "hier", "un\u00b7term", "Brun\u00b7nen", "spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "PDAT", "NN", "ADV", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der nicht zum minsten solt auch ein Geweihlein f\u00fchlen,", "tokens": ["Der", "nicht", "zum", "mins\u00b7ten", "solt", "auch", "ein", "Ge\u00b7weih\u00b7lein", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPRART", "ADJA", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn beyde sind zu viel, und gehn euch M\u00e4nner an.", "tokens": ["Denn", "bey\u00b7de", "sind", "zu", "viel", ",", "und", "gehn", "euch", "M\u00e4n\u00b7ner", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PTKA", "PIS", "$,", "KON", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}