{"textgrid.poem.66675": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im F\u00f6hrenwald. Fast lautlos. Zeitvergessen.", "tokens": ["Im", "F\u00f6h\u00b7ren\u00b7wald", ".", "Fast", "laut\u00b7los", ".", "Zeit\u00b7ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$.", "ADV", "ADJD", "$.", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was leis in Wipfeln saust, singt Ewigkeit.", "tokens": ["Was", "leis", "in", "Wip\u00b7feln", "saust", ",", "singt", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPR", "NN", "VVFIN", "$,", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sanftk\u00fchles Moos. Des Lebens Lasten pressen", "tokens": ["Sanft\u00b7k\u00fch\u00b7les", "Moos", ".", "Des", "Le\u00b7bens", "Las\u00b7ten", "pres\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mir nicht die Brust, ich bin von Qual befreit.", "tokens": ["Mir", "nicht", "die", "Brust", ",", "ich", "bin", "von", "Qual", "be\u00b7freit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ART", "NN", "$,", "PPER", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ach, H\u00f6hn und Tiefen hab auch ich durchmessen,", "tokens": ["Ach", ",", "H\u00f6hn", "und", "Tie\u00b7fen", "hab", "auch", "ich", "durch\u00b7mes\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "KON", "NN", "VAFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wei\u00df, wie der Mensch in wildem Schmerze schreit,", "tokens": ["Wei\u00df", ",", "wie", "der", "Mensch", "in", "wil\u00b7dem", "Schmer\u00b7ze", "schreit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie rasch er jauchzt im Taumel der Sekunde,", "tokens": ["Wie", "rasch", "er", "jauchzt", "im", "Tau\u00b7mel", "der", "Se\u00b7kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie stolz er hinstelzt, wie er geht zugrunde.", "tokens": ["Wie", "stolz", "er", "hin\u00b7stelzt", ",", "wie", "er", "geht", "zu\u00b7grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Geheuchelt hab ich auch in Wort und Mienen \u2013", "tokens": ["Ge\u00b7heu\u00b7chelt", "hab", "ich", "auch", "in", "Wort", "und", "Mie\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie oft, da\u00df mir der Mut zersplittert war", "tokens": ["Wie", "oft", ",", "da\u00df", "mir", "der", "Mut", "zer\u00b7split\u00b7tert", "war"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und bin doch l\u00e4chelnden Gesichts erschienen", "tokens": ["Und", "bin", "doch", "l\u00e4\u00b7cheln\u00b7den", "Ge\u00b7sichts", "er\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und stach dem andern Heuchler noch den Star.", "tokens": ["Und", "stach", "dem", "an\u00b7dern", "Heuch\u00b7ler", "noch", "den", "Star", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich wollte mir den Preis der Welt verdienen", "tokens": ["Ich", "woll\u00b7te", "mir", "den", "Preis", "der", "Welt", "ver\u00b7die\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit Kraftgeflunker wie ein Janitschar:", "tokens": ["Mit", "Kraft\u00b7ge\u00b7flun\u00b7ker", "wie", "ein", "Ja\u00b7nit\u00b7schar", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Des Herzens Schwachheit hab ich \u00fcberpinselt,", "tokens": ["Des", "Her\u00b7zens", "Schwach\u00b7heit", "hab", "ich", "\u00fc\u00b7ber\u00b7pin\u00b7selt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Stramm dreingeschaut und innerlich gewinselt ...", "tokens": ["Stramm", "drein\u00b7ge\u00b7schaut", "und", "in\u00b7ner\u00b7lich", "ge\u00b7win\u00b7selt", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ihr zierlichen, ihr goldbetupften K\u00e4fer,", "tokens": ["Ihr", "zier\u00b7li\u00b7chen", ",", "ihr", "gold\u00b7be\u00b7tupf\u00b7ten", "K\u00e4\u00b7fer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie lauft ihr gr\u00fcnlichschillernd durchs Gewirr", "tokens": ["Wie", "lauft", "ihr", "gr\u00fcn\u00b7lich\u00b7schil\u00b7lernd", "durchs", "Ge\u00b7wirr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der weichen Moosesh\u00e4rchen! Minnesch\u00e4fer", "tokens": ["Der", "wei\u00b7chen", "Moo\u00b7ses\u00b7h\u00e4r\u00b7chen", "!", "Min\u00b7ne\u00b7sch\u00e4\u00b7fer"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$.", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Seid ihr und lockt mit lispelndem Gekirr,", "tokens": ["Seid", "ihr", "und", "lockt", "mit", "lis\u00b7peln\u00b7dem", "Ge\u00b7kirr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Das gar nicht h\u00f6rt so'n Mammutmenschenschl\u00e4fer,", "tokens": ["Das", "gar", "nicht", "h\u00f6rt", "so'n", "Mam\u00b7mut\u00b7men\u00b7schen\u00b7schl\u00e4\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PTKNEG", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ins Lustgarn euch: ihr lauscht nicht dem Geschwirr,", "tokens": ["Ins", "Lust\u00b7garn", "euch", ":", "ihr", "lauscht", "nicht", "dem", "Ge\u00b7schwirr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "$.", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dem seltsam wipfelwandernden, der Kronen,", "tokens": ["Dem", "selt\u00b7sam", "wip\u00b7fel\u00b7wan\u00b7dern\u00b7den", ",", "der", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In denen ewige R\u00e4tselweisen wohnen.", "tokens": ["In", "de\u00b7nen", "e\u00b7wi\u00b7ge", "R\u00e4t\u00b7sel\u00b7wei\u00b7sen", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Ihr seid in eurem s\u00fc\u00dfen Trieb befangen", "tokens": ["Ihr", "seid", "in", "eu\u00b7rem", "s\u00fc\u00b7\u00dfen", "Trieb", "be\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und klammert euch gar ernsthaft an das Gl\u00fcck,", "tokens": ["Und", "klam\u00b7mert", "euch", "gar", "ernst\u00b7haft", "an", "das", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das im Moment euch wonnig aufgegangen,", "tokens": ["Das", "im", "Mo\u00b7ment", "euch", "won\u00b7nig", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr seid berauscht und k\u00f6nnt nicht mehr zur\u00fcck.", "tokens": ["Ihr", "seid", "be\u00b7rauscht", "und", "k\u00f6nnt", "nicht", "mehr", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein K\u00e4ferlein bleibt an dem andern hangen,", "tokens": ["Ein", "K\u00e4\u00b7fer\u00b7lein", "bleibt", "an", "dem", "an\u00b7dern", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Natur spielt aus den alten Sch\u00f6pfertrick \u2013", "tokens": ["Na\u00b7tur", "spielt", "aus", "den", "al\u00b7ten", "Sch\u00f6p\u00b7fer\u00b7trick", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Den Knalleffekt, kurz wie Gewitterschauer,", "tokens": ["Den", "Knal\u00b7lef\u00b7fekt", ",", "kurz", "wie", "Ge\u00b7wit\u00b7ter\u00b7schau\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Spinne Willewelt liegt auf der Lauer.", "tokens": ["Die", "Spin\u00b7ne", "Wil\u00b7le\u00b7welt", "liegt", "auf", "der", "Lau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ist's anders denn mit uns vermummten Tieren,", "tokens": ["Ist's", "an\u00b7ders", "denn", "mit", "uns", "ver\u00b7mumm\u00b7ten", "Tie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die nur durch h\u00f6chst vollkommne Brillen schaun?", "tokens": ["Die", "nur", "durch", "h\u00f6chst", "voll\u00b7komm\u00b7ne", "Bril\u00b7len", "schaun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir k\u00f6nnen raffinierter r\u00e4sonieren,", "tokens": ["Wir", "k\u00f6n\u00b7nen", "raf\u00b7fi\u00b7nier\u00b7ter", "r\u00e4\u00b7so\u00b7nie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Traktieren kritisch Denken und Verdaun,", "tokens": ["Trak\u00b7tie\u00b7ren", "kri\u00b7tisch", "Den\u00b7ken", "und", "Ver\u00b7daun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wir sind zum Platzen reif im Selbstsezieren,", "tokens": ["Wir", "sind", "zum", "Plat\u00b7zen", "reif", "im", "Selbst\u00b7se\u00b7zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Begierig, \u00fcber jede Schnur zu haun", "tokens": ["Be\u00b7gie\u00b7rig", ",", "\u00fc\u00b7ber", "je\u00b7de", "Schnur", "zu", "haun"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "APPR", "PIAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und uns vertrackte Triebobjektsgesellen", "tokens": ["Und", "uns", "ver\u00b7track\u00b7te", "Trieb\u00b7ob\u00b7jekts\u00b7ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Vor Selbstbeguckung auf den Kopf zu stellen.", "tokens": ["Vor", "Selbst\u00b7be\u00b7gu\u00b7ckung", "auf", "den", "Kopf", "zu", "stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Das Menschenhirn schl\u00e4gt Riesenpurzelb\u00e4ume", "tokens": ["Das", "Men\u00b7schen\u00b7hirn", "schl\u00e4gt", "Rie\u00b7sen\u00b7pur\u00b7zel\u00b7b\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "(zwar das aus Feuer nur, nicht das aus Stroh),", "tokens": ["(", "zwar", "das", "aus", "Feu\u00b7er", "nur", ",", "nicht", "das", "aus", "Stroh", ")", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ART", "APPR", "NN", "ADV", "$,", "PTKNEG", "PDS", "APPR", "NE", "$(", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Schwingt sich durch seine eignen Weltenr\u00e4ume", "tokens": ["Schwingt", "sich", "durch", "sei\u00b7ne", "eig\u00b7nen", "Wel\u00b7ten\u00b7r\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ist des Nebelflecks im Auge froh.", "tokens": ["Und", "ist", "des", "Ne\u00b7bel\u00b7flecks", "im", "Au\u00b7ge", "froh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nach innen regt zum Tanz der Zukunftstr\u00e4ume", "tokens": ["Nach", "in\u00b7nen", "regt", "zum", "Tanz", "der", "Zu\u00b7kunfts\u00b7tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sich schon der \u00dcbermenschenembryo \u2013", "tokens": ["Sich", "schon", "der", "\u00dc\u00b7ber\u00b7men\u00b7sche\u00b7nemb\u00b7ryo", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ART", "NN", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Nur wenn die irdischen F\u00fchler uns erkranken,", "tokens": ["Nur", "wenn", "die", "ir\u00b7di\u00b7schen", "F\u00fch\u00b7ler", "uns", "er\u00b7kran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Verzichten wir auf Weltherrschaftsgedanken.", "tokens": ["Ver\u00b7zich\u00b7ten", "wir", "auf", "Welt\u00b7herr\u00b7schafts\u00b7ge\u0