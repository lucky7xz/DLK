{"textgrid.poem.38361": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "T t Turteltaube", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Turteltaub ohn allen Trost, will nicht mehr fr\u00f6hlich seyn,", "tokens": ["Die", "Tur\u00b7tel\u00b7taub", "ohn", "al\u00b7len", "Trost", ",", "will", "nicht", "mehr", "fr\u00f6h\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$,", "VMFIN", "PTKNEG", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Wenn ihren Gesell der Habich sto\u00dft, traurt sie und bleibt allein,", "tokens": ["Wenn", "ih\u00b7ren", "Ge\u00b7sell", "der", "Ha\u00b7bich", "sto\u00dft", ",", "traurt", "sie", "und", "bleibt", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "KON", "VVFIN", "ADV", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Wenn dir das Liebste, was du hast, der Tod nimmt mit Gewalt,", "tokens": ["Wenn", "dir", "das", "Liebs\u00b7te", ",", "was", "du", "hast", ",", "der", "Tod", "nimmt", "mit", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "PWS", "PPER", "VAFIN", "$,", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "So traure, sey kein frecher Gast, vergi\u00df es nicht so bald.", "tokens": ["So", "trau\u00b7re", ",", "sey", "kein", "fre\u00b7cher", "Gast", ",", "ver\u00b7gi\u00df", "es", "nicht", "so", "bald", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VAFIN", "PIAT", "ADJA", "NN", "$,", "VVIMP", "PPER", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}