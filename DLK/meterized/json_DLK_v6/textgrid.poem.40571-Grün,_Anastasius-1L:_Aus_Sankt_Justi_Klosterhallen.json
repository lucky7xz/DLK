{"textgrid.poem.40571": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus Sankt Justi Klosterhallen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus Sankt Justi Klosterhallen", "tokens": ["Aus", "Sankt", "Jus\u00b7ti", "Klos\u00b7ter\u00b7hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00f6nt ein tr\u00e4ges Todtenlied,", "tokens": ["T\u00f6nt", "ein", "tr\u00e4\u00b7ges", "Tod\u00b7ten\u00b7lied", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Glocken summen von den Th\u00fcrmen", "tokens": ["Glo\u00b7cken", "sum\u00b7men", "von", "den", "Th\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr den M\u00f6nch, der heut verschied.", "tokens": ["F\u00fcr", "den", "M\u00f6nch", ",", "der", "heut", "ver\u00b7schied", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Seht den Todten! Wie von welkem Blute", "tokens": ["Seht", "den", "Tod\u00b7ten", "!", "Wie", "von", "wel\u00b7kem", "Blu\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "PWAV", "APPR", "PWAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schlingt ein rother Reif sich um sein Haupt;", "tokens": ["Schlingt", "ein", "ro\u00b7ther", "Reif", "sich", "um", "sein", "Haupt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ob einst drauf zur Bu\u00df' ein Dornkranz ruhte?", "tokens": ["Ob", "einst", "drauf", "zur", "Bu\u00df'", "ein", "Dorn\u00b7kranz", "ruh\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PAV", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Nein, die Krone lag auf diesem Haupt!", "tokens": ["Nein", ",", "die", "Kro\u00b7ne", "lag", "auf", "die\u00b7sem", "Haupt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Die Kapuze zieht ein M\u00f6nch ihm", "tokens": ["Die", "Ka\u00b7pu\u00b7ze", "zieht", "ein", "M\u00f6nch", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tief jetzt \u00fcbers Auge zu,", "tokens": ["Tief", "jetzt", "\u00fc\u00b7bers", "Au\u00b7ge", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die b\u00f6se Spur der Krone", "tokens": ["Da\u00df", "die", "b\u00f6\u00b7se", "Spur", "der", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Drin verh\u00fcllt, verborgen ruh'.", "tokens": ["Drin", "ver\u00b7h\u00fcllt", ",", "ver\u00b7bor\u00b7gen", "ruh'", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Einst das Zepter hielt sein Arm erhoben;", "tokens": ["Einst", "das", "Zep\u00b7ter", "hielt", "sein", "Arm", "er\u00b7ho\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "R\u00fcttelte gleich dran die halbe Welt,", "tokens": ["R\u00fct\u00b7tel\u00b7te", "gleich", "dran", "die", "hal\u00b7be", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Er hielt fest und h\u00f6her es nach oben,", "tokens": ["Er", "hielt", "fest", "und", "h\u00f6\u00b7her", "es", "nach", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "ADJD", "PPER", "APPR", "ADV", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie ein Fels, der eine Tanne h\u00e4lt.", "tokens": ["Wie", "ein", "Fels", ",", "der", "ei\u00b7ne", "Tan\u00b7ne", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Diese Arme beugt dem Todten", "tokens": ["Die\u00b7se", "Ar\u00b7me", "beugt", "dem", "Tod\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jetzt ein Frater zu Sankt Just,", "tokens": ["Jetzt", "ein", "Fra\u00b7ter", "zu", "Sankt", "Just", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKZU", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dr\u00fcckt ein Kreuz darein, und beugt sie,", "tokens": ["Dr\u00fcckt", "ein", "Kreuz", "da\u00b7rein", ",", "und", "beugt", "sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PAV", "$,", "KON", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach so leicht! verschr\u00e4nkt zur Brust.", "tokens": ["Ach", "so", "leicht", "!", "ver\u00b7schr\u00e4nkt", "zur", "Brust", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ADJD", "$.", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie des Regenbogens Himmelsstiege", "tokens": ["Wie", "des", "Re\u00b7gen\u00b7bo\u00b7gens", "Him\u00b7mels\u00b7stie\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Glomm der Tag, der ihm das Licht beschied,", "tokens": ["Glomm", "der", "Tag", ",", "der", "ihm", "das", "Licht", "be\u00b7schied", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "K\u00f6n'ge schaukelten da seine Wiege,", "tokens": ["K\u00f6n'\u00b7ge", "schau\u00b7kel\u00b7ten", "da", "sei\u00b7ne", "Wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "K\u00f6niginnen sangen ihm das Lied.", "tokens": ["K\u00f6\u00b7ni\u00b7gin\u00b7nen", "san\u00b7gen", "ihm", "das", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Doch ein M\u00f6nchchor singt das Grablied", "tokens": ["Doch", "ein", "M\u00f6n\u00b7chchor", "singt", "das", "Grab\u00b7lied"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jetzt in alter Melodei,", "tokens": ["Jetzt", "in", "al\u00b7ter", "Me\u00b7lo\u00b7dei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie er singt, ob Grabeslegung", "tokens": ["Wie", "er", "singt", ",", "ob", "Gra\u00b7bes\u00b7le\u00b7gung"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder Auferstehung sei.", "tokens": ["O\u00b7der", "Auf\u00b7er\u00b7ste\u00b7hung", "sei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Seht, die Sonne sinkt, die aus den Reichen", "tokens": ["Seht", ",", "die", "Son\u00b7ne", "sinkt", ",", "die", "aus", "den", "Rei\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ART", "NN", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dieses Todten nie den Ausgang fand;", "tokens": ["Die\u00b7ses", "Tod\u00b7ten", "nie", "den", "Aus\u00b7gang", "fand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dieses Abendroth im Gau der Eichen", "tokens": ["Die\u00b7ses", "A\u00b7ben\u00b7droth", "im", "Gau", "der", "Ei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ist ein Morgenroth dem Palmenland.", "tokens": ["Ist", "ein", "Mor\u00b7gen\u00b7roth", "dem", "Pal\u00b7men\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Und die Glocken leiser klingen:", "tokens": ["Und", "die", "Glo\u00b7cken", "lei\u00b7ser", "klin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6ne Th\u00e4ler, lebet wohl!", "tokens": ["Sch\u00f6\u00b7ne", "Th\u00e4\u00b7ler", ",", "le\u00b7bet", "wohl", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die M\u00f6nche heiser singen:", "tokens": ["Und", "die", "M\u00f6n\u00b7che", "hei\u00b7ser", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schn\u00f6de Welt, o fahre wohl!", "tokens": ["Schn\u00f6\u00b7de", "Welt", ",", "o", "fah\u00b7re", "wohl", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "FM", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Einmal noch durchs Kirchenfenster nieder", "tokens": ["Ein\u00b7mal", "noch", "durchs", "Kir\u00b7chen\u00b7fens\u00b7ter", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "NN", "PTKVZ"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Blickt zum Sarg der Sonne mildes Roth,", "tokens": ["Blickt", "zum", "Sarg", "der", "Son\u00b7ne", "mil\u00b7des", "Roth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Was sie hier sieht, dort zu k\u00fcnden wieder:", "tokens": ["Was", "sie", "hier", "sieht", ",", "dort", "zu", "k\u00fcn\u00b7den", "wie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$,", "ADV", "PTKZU", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie der Herrscher beider Welten todt!", "tokens": ["Wie", "der", "Herr\u00b7scher", "bei\u00b7der", "Wel\u00b7ten", "todt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PIAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Hirt und Hirtin doch im Thale,", "tokens": ["Hirt", "und", "Hir\u00b7tin", "doch", "im", "Tha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie da Glocke klingt und Lied,", "tokens": ["Wie", "da", "Glo\u00b7cke", "klingt", "und", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "VVFIN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Beten still, entbl\u00f6\u00dften Hauptes,", "tokens": ["Be\u00b7ten", "still", ",", "ent\u00b7bl\u00f6\u00df\u00b7ten", "Haup\u00b7tes", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr den frommen M\u00f6nch, der schied.", "tokens": ["F\u00fcr", "den", "from\u00b7men", "M\u00f6nch", ",", "der", "schied", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}