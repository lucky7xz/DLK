{"dta.poem.8827": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Vi.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Sprich, o sprich, wo meine Seele blieb?", "tokens": ["Sprich", ",", "o", "sprich", ",", "wo", "mei\u00b7ne", "See\u00b7le", "blieb", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "FM", "ADJD", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Bald vergleich' ich solch ein Aug' der Nacht,", "tokens": ["Bald", "ver\u00b7gleich'", "ich", "solch", "ein", "Aug'", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Bald der Sonne, die die Nacht vertrieb,", "tokens": ["Bald", "der", "Son\u00b7ne", ",", "die", "die", "Nacht", "ver\u00b7trieb", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Krause Locke, ringle Gold in Gold,", "tokens": ["Krau\u00b7se", "Lo\u00b7cke", ",", "ring\u00b7le", "Gold", "in", "Gold", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Denn du mahnst an junger Reben Trieb!", "tokens": ["Denn", "du", "mahnst", "an", "jun\u00b7ger", "Re\u00b7ben", "Trieb", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPR", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Lebte wohl ein Alexander je,", "tokens": ["Leb\u00b7te", "wohl", "ein", "A\u00b7lex\u00b7an\u00b7der", "je", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NE", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Der so sch\u00f6ne Knoten frech zerhieb?", "tokens": ["Der", "so", "sch\u00f6\u00b7ne", "Kno\u00b7ten", "frech", "zer\u00b7hieb", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Wei\u00dfe Hand, verwalte Schenkenamt,", "tokens": ["Wei\u00b7\u00dfe", "Hand", ",", "ver\u00b7wal\u00b7te", "Schen\u00b7ke\u00b7namt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Gieb mir Wein, o gieb mir Wein, o gieb!", "tokens": ["Gieb", "mir", "Wein", ",", "o", "gieb", "mir", "Wein", ",", "o", "gieb", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,", "FM", "VVIMP", "PPER", "NN", "$,", "FM", "VVIMP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Was mir allzuhoch, verg\u00e4\u00df' ich gern,", "tokens": ["Was", "mir", "all\u00b7zu\u00b7hoch", ",", "ver\u00b7g\u00e4\u00df'", "ich", "gern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Aber ach, es ist mir allzulieb!", "tokens": ["A\u00b7ber", "ach", ",", "es", "ist", "mir", "all\u00b7zu\u00b7lieb", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PPER", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}