{"dta.poem.3299": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Amor und sein Bruder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Um die stille Mitternacht,", "tokens": ["Um", "die", "stil\u00b7le", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn allein die Liebe wacht;", "tokens": ["Wenn", "al\u00b7lein", "die", "Lie\u00b7be", "wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wenn die schattenvolle Welt", "tokens": ["Wenn", "die", "schat\u00b7ten\u00b7vol\u00b7le", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur der hohe Mond erhellt:", "tokens": ["Nur", "der", "ho\u00b7he", "Mond", "er\u00b7hellt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schlief die Nachbarinn Elmire;", "tokens": ["Schlief", "die", "Nach\u00b7ba\u00b7rinn", "El\u00b7mi\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenigstens ihr Alter schlief:", "tokens": ["We\u00b7nigs\u00b7tens", "ihr", "Al\u00b7ter", "schlief", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Als vor ihres Hauses Th\u00fcre", "tokens": ["Als", "vor", "ih\u00b7res", "Hau\u00b7ses", "Th\u00fc\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Cyperns Gottheit pocht\u2019, und rief.", "tokens": ["Cy\u00b7perns", "Got\u00b7theit", "pocht'", ",", "und", "rief", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$,", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer ist hier? wer l\u00e4rmt noch so?", "tokens": ["Wer", "ist", "hier", "?", "wer", "l\u00e4rmt", "noch", "so", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "$.", "PWS", "VVFIN", "ADV", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ach! mein g\u00fcldner Traum entfloh!", "tokens": ["Ach", "!", "mein", "g\u00fcld\u00b7ner", "Traum", "ent\u00b7floh", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rief die Magd halbschlafend aus,", "tokens": ["Rief", "die", "Magd", "halb\u00b7schla\u00b7fend", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "G\u00e4hnt\u2019 und taumelte vors Haus.", "tokens": ["G\u00e4hnt'", "und", "tau\u00b7mel\u00b7te", "vors", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Amor fleht\u2019 in ihren Armen;", "tokens": ["A\u00b7mor", "fleht'", "in", "ih\u00b7ren", "Ar\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und, wie alle Welt gesteht,", "tokens": ["Und", ",", "wie", "al\u00b7le", "Welt", "ge\u00b7steht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mu\u00df ein M\u00e4dchen sich erbarmen,", "tokens": ["Mu\u00df", "ein", "M\u00e4d\u00b7chen", "sich", "er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wann ein milder Amor fleht.", "tokens": ["Wann", "ein", "mil\u00b7der", "A\u00b7mor", "fleht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jhm wird willig aufgethan;", "tokens": ["Jhm", "wird", "wil\u00b7lig", "auf\u00b7ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sein Bruder h\u00e4ngt sich an:", "tokens": ["Und", "sein", "Bru\u00b7der", "h\u00e4ngt", "sich", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Halb bedeckt ein Ephe\u00fc-Kranz", "tokens": ["Halb", "be\u00b7deckt", "ein", "E\u00b7phe\u00b7\u00fc\u00b7Kranz"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Seines g\u00fcldnen Hornes Glanz.", "tokens": ["Sei\u00b7nes", "g\u00fcld\u00b7nen", "Hor\u00b7nes", "Glanz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seine schlauen Blicke brennen;", "tokens": ["Sei\u00b7ne", "schlau\u00b7en", "Bli\u00b7cke", "bren\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jede Sehne schwillt von Kraft:", "tokens": ["Je\u00b7de", "Seh\u00b7ne", "schwillt", "von", "Kraft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die ihn kennen wollen, nennen", "tokens": ["Die", "ihn", "ken\u00b7nen", "wol\u00b7len", ",", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "PPER", "VVINF", "VMFIN", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jhn den Gott der Hahnreyschaft.", "tokens": ["Jhn", "den", "Gott", "der", "Hahn\u00b7rey\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Amor thut sogleich bekannt,", "tokens": ["A\u00b7mor", "thut", "sog\u00b7leich", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lehnet an die n\u00e4chste Wand", "tokens": ["Leh\u00b7net", "an", "die", "n\u00e4chs\u00b7te", "Wand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seinen Bogen lachend hin,", "tokens": ["Sei\u00b7nen", "Bo\u00b7gen", "la\u00b7chend", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00fcpft und ruft mit frohem Sinn:", "tokens": ["H\u00fcpft", "und", "ruft", "mit", "fro\u00b7hem", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Troz der fest verschlossnen Th\u00fcre,", "tokens": ["Troz", "der", "fest", "ver\u00b7schloss\u00b7nen", "Th\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bruder! half ich dir herein.", "tokens": ["Bru\u00b7der", "!", "half", "ich", "dir", "her\u00b7ein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Jung und feurig ist Elmire:", "tokens": ["Jung", "und", "feu\u00b7rig", "ist", "El\u00b7mi\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VAFIN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "O sie wird nicht grausam seyn!", "tokens": ["O", "sie", "wird", "nicht", "grau\u00b7sam", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VAFIN", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}