{"textgrid.poem.47293": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich h\u00e4tte Herzzerrei\u00dfendes zu singen,", "tokens": ["Ich", "h\u00e4t\u00b7te", "Herz\u00b7zer\u00b7rei\u00b7\u00dfen\u00b7des", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wollt' ich enth\u00fcllen, was tief in mir lodert;", "tokens": ["Wollt'", "ich", "ent\u00b7h\u00fcl\u00b7len", ",", "was", "tief", "in", "mir", "lo\u00b7dert", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Ich m\u00fc\u00dfte mich zu falschen T\u00f6nen zwingen,", "tokens": ["Ich", "m\u00fc\u00df\u00b7te", "mich", "zu", "fal\u00b7schen", "T\u00f6\u00b7nen", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wollt' ich der Menge geben, was sie fodert.", "tokens": ["Wollt'", "ich", "der", "Men\u00b7ge", "ge\u00b7ben", ",", "was", "sie", "fo\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie helle Blumen aus der Erde dringen", "tokens": ["Wie", "hel\u00b7le", "Blu\u00b7men", "aus", "der", "Er\u00b7de", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und dunkler Tod still unter ihnen modert;", "tokens": ["Und", "dunk\u00b7ler", "Tod", "still", "un\u00b7ter", "ih\u00b7nen", "mo\u00b7dert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So soll mein Sinn sich sanft in Schmuck verh\u00fcllen", "tokens": ["So", "soll", "mein", "Sinn", "sich", "sanft", "in", "Schmuck", "ver\u00b7h\u00fcl\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "PRF", "ADJD", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und meine Trauer euch mit Lust erf\u00fcllen.", "tokens": ["Und", "mei\u00b7ne", "Trau\u00b7er", "euch", "mit", "Lust", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}