{"textgrid.poem.25086": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das mir sonst so froh erklungen,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das mir sonst so froh erklungen,", "tokens": ["Das", "mir", "sonst", "so", "froh", "er\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deinem Liede, o! warum", "tokens": ["Dei\u00b7nem", "Lie\u00b7de", ",", "o", "!", "wa\u00b7rum"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "FM", "$.", "PWAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In den gr\u00fcnen D\u00e4mmerungen", "tokens": ["In", "den", "gr\u00fc\u00b7nen", "D\u00e4m\u00b7me\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lausch' ich jetzt so trauerstumm?", "tokens": ["Lausch'", "ich", "jetzt", "so", "trau\u00b7er\u00b7stumm", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schwer von Wonnen, nun geschwunden,", "tokens": ["Schwer", "von", "Won\u00b7nen", ",", "nun", "ge\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Holde S\u00e4ngerin der Nacht,", "tokens": ["Hol\u00b7de", "S\u00e4n\u00b7ge\u00b7rin", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mahnt es mich an jene Stunden,", "tokens": ["Mahnt", "es", "mich", "an", "je\u00b7ne", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die ich selig hier durchwacht.", "tokens": ["Die", "ich", "se\u00b7lig", "hier", "durch\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wieder nun wallt von den Beeten", "tokens": ["Wie\u00b7der", "nun", "wallt", "von", "den", "Bee\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Bl\u00fctenodem durch die Luft,", "tokens": ["Bl\u00fc\u00b7te\u00b7no\u00b7dem", "durch", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch von fr\u00fchern, l\u00e4ngst verwehten", "tokens": ["Doch", "von", "fr\u00fc\u00b7hern", ",", "l\u00e4ngst", "ver\u00b7weh\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ADJA", "$,", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lenzen ist es nur der Duft;", "tokens": ["Len\u00b7zen", "ist", "es", "nur", "der", "Duft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und Erinnerungen fluten", "tokens": ["Und", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", "flu\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVFIN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Auf der T\u00f6ne Strom heran \u2013", "tokens": ["Auf", "der", "T\u00f6\u00b7ne", "Strom", "he\u00b7ran", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! mir will das Herz verbluten", "tokens": ["Ach", "!", "mir", "will", "das", "Herz", "ver\u00b7blu\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "VMFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In des Liedes s\u00fc\u00dfem Bann.", "tokens": ["In", "des", "Lie\u00b7des", "s\u00fc\u00b7\u00dfem", "Bann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Antwort einst mit frohem Pochen", "tokens": ["Ant\u00b7wort", "einst", "mit", "fro\u00b7hem", "Po\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gab es ihm, o Nachtigall!", "tokens": ["Gab", "es", "ihm", ",", "o", "Nach\u00b7ti\u00b7gall", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "FM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch in Herzen, die gebrochen,", "tokens": ["Doch", "in", "Her\u00b7zen", ",", "die", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Traurig t\u00f6nt sein Wiederhall!", "tokens": ["Trau\u00b7rig", "t\u00f6nt", "sein", "Wie\u00b7der\u00b7hall", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}