{"textgrid.poem.48458": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "32. Auf H. Lyon Bernullis, F\u00fcrstl. Holstein. Gesandten Hofejunkern, Namenstag", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was soll man anders tun an einem lieben Tage,", "tokens": ["Was", "soll", "man", "an\u00b7ders", "tun", "an", "ei\u00b7nem", "lie\u00b7ben", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PIS", "ADV", "VVINF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "als da\u00df man ganz entfreit von aller Not und Klage", "tokens": ["als", "da\u00df", "man", "ganz", "ent\u00b7freit", "von", "al\u00b7ler", "Not", "und", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "PIS", "ADV", "ADJD", "APPR", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "von Herzen fr\u00f6lich sei? Setz' alles Leid seitab", "tokens": ["von", "Her\u00b7zen", "fr\u00f6\u00b7lich", "sei", "?", "Setz'", "al\u00b7les", "Leid", "seit\u00b7ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "VAFIN", "$.", "VVFIN", "PIAT", "NN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und denke bei der Lust dem Himmel, der sie gab!", "tokens": ["und", "den\u00b7ke", "bei", "der", "Lust", "dem", "Him\u00b7mel", ",", "der", "sie", "gab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Guts k\u00f6mmt vom Guten her, dem Einigen, dem Wahren,", "tokens": ["Guts", "k\u00f6mmt", "vom", "Gu\u00b7ten", "her", ",", "dem", "Ei\u00b7ni\u00b7gen", ",", "dem", "Wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "von dem uns Keinem noch was B\u00f6ses widerfahren.", "tokens": ["von", "dem", "uns", "Kei\u00b7nem", "noch", "was", "B\u00f6\u00b7ses", "wi\u00b7der\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PIS", "ADV", "PWS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Zeit, die fleugt vorbei, die Jahre warten nicht;", "tokens": ["Die", "Zeit", ",", "die", "fleugt", "vor\u00b7bei", ",", "die", "Jah\u00b7re", "war\u00b7ten", "nicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "die Stunden schie\u00dfen fort. Ein ieder Blick, der spricht:", "tokens": ["die", "Stun\u00b7den", "schie\u00b7\u00dfen", "fort", ".", "Ein", "ie\u00b7der", "Blick", ",", "der", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "ART", "PIAT", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ergreif' mich, weil ich bin! Dich trifft nun deine Reihe,", "tokens": ["Er\u00b7greif'", "mich", ",", "weil", "ich", "bin", "!", "Dich", "trifft", "nun", "dei\u00b7ne", "Rei\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "PPER", "VAFIN", "$.", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Freund, und gebeut auch uns, da\u00df man sich mit dir freue!", "tokens": ["Freund", ",", "und", "ge\u00b7beut", "auch", "uns", ",", "da\u00df", "man", "sich", "mit", "dir", "freu\u00b7e", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVFIN", "ADV", "PPER", "$,", "KOUS", "PIS", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.11": {"text": "Bist du nur so bereit zur Fr\u00f6lichkeit als wir,", "tokens": ["Bist", "du", "nur", "so", "be\u00b7reit", "zur", "Fr\u00f6\u00b7lich\u00b7keit", "als", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "APPRART", "NN", "KOUS", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "so wird den ganzen Tag getrunken ganz kein Bier.", "tokens": ["so", "wird", "den", "gan\u00b7zen", "Tag", "ge\u00b7trun\u00b7ken", "ganz", "kein", "Bier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Auf heut' ist di\u00df zu schlecht. Wenn Pb\u00f6bus uns l\u00e4\u00dft scheinen", "tokens": ["Auf", "heut'", "ist", "di\u00df", "zu", "schlecht", ".", "Wenn", "Pb\u00f6\u00b7bus", "uns", "l\u00e4\u00dft", "schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VAFIN", "PDS", "PTKA", "ADJD", "$.", "KOUS", "NE", "PPER", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "den Tag, da uns zuerst die Mutter h\u00f6rte ", "tokens": ["den", "Tag", ",", "da", "uns", "zu\u00b7erst", "die", "Mut\u00b7ter", "h\u00f6r\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "da wills nicht sein ", "tokens": ["da", "wills", "nicht", "sein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PTKNEG", "VAINF"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "der gibt dem Herzen Herz' und st\u00e4rket Mark und Bein.", "tokens": ["der", "gibt", "dem", "Her\u00b7zen", "Her\u00b7z'", "und", "st\u00e4r\u00b7ket", "Mark", "und", "Bein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ART", "NN", "NN", "KON", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Das kan Ly\u00e4us tun, der Starke, der Bezwinger,", "tokens": ["Das", "kan", "Ly\u00b7\u00e4us", "tun", ",", "der", "Star\u00b7ke", ",", "der", "Be\u00b7zwin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "NE", "VVINF", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "der Lustfreund, Herzenstrost, Geistreger, Sinnendringer.", "tokens": ["der", "Lust\u00b7freund", ",", "Her\u00b7zen\u00b7strost", ",", "Geis\u00b7tre\u00b7ger", ",", "Sin\u00b7nen\u00b7drin\u00b7ger", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Heut' ist sein und dein Fest. Es st\u00fcnde Beiden kahl,", "tokens": ["Heut'", "ist", "sein", "und", "dein", "Fest", ".", "Es", "st\u00fcn\u00b7de", "Bei\u00b7den", "kahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VAINF", "KON", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PIS", "ADJD", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.20": {"text": "wenn du ihn l\u00fcdest nicht auf di\u00df dein fr\u00f6lichs Mahl.", "tokens": ["wenn", "du", "ihn", "l\u00fc\u00b7dest", "nicht", "auf", "di\u00df", "dein", "fr\u00f6\u00b7lichs", "Mahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "PTKNEG", "APPR", "PDS", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wer wolte lustig sein? So schicke denn nach Weine!", "tokens": ["Wer", "wol\u00b7te", "lus\u00b7tig", "sein", "?", "So", "schi\u00b7cke", "denn", "nach", "Wei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADJD", "VAINF", "$.", "ADV", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Geh, Junger, hol uns her den wertesten vom Rheine", "tokens": ["Geh", ",", "Jun\u00b7ger", ",", "hol", "uns", "her", "den", "wer\u00b7tes\u00b7ten", "vom", "Rhei\u00b7ne"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADJA", "$,", "VVFIN", "PRF", "APZR", "ART", "NN", "APPRART", "NE"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "und besten Lautertrank! Bring Zucker und Kanel,", "tokens": ["und", "bes\u00b7ten", "Lau\u00b7ter\u00b7trank", "!", "Bring", "Zu\u00b7cker", "und", "Ka\u00b7nel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$.", "NE", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Succat und Ingwer auch, des schwachen Magens Seel'!", "tokens": ["Suc\u00b7cat", "und", "Ing\u00b7wer", "auch", ",", "des", "schwa\u00b7chen", "Ma\u00b7gens", "Seel'", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "ADV", "$,", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Eil, hole was du solst! Du andrer lauf zum Garten", "tokens": ["Eil", ",", "ho\u00b7le", "was", "du", "solst", "!", "Du", "an\u00b7drer", "lauf", "zum", "Gar\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PWS", "PPER", "VMFIN", "$.", "PPER", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "und putz das Lusthaus auf, leg auf das Bret und Karten,", "tokens": ["und", "putz", "das", "Lust\u00b7haus", "auf", ",", "leg", "auf", "das", "Bret", "und", "Kar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "PTKVZ", "$,", "NE", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "befiehl auch, da\u00df straks wird der Schorstein angemacht,", "tokens": ["be\u00b7fiehl", "auch", ",", "da\u00df", "straks", "wird", "der", "Schor\u00b7stein", "an\u00b7ge\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "da\u00df uns gegl\u00fchter Wein nicht fehle durch die Nacht,", "tokens": ["da\u00df", "uns", "ge\u00b7gl\u00fch\u00b7ter", "Wein", "nicht", "feh\u00b7le", "durch", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "PTKNEG", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "und wenn man sein begehrt! Streu Blumen auf die B\u00e4nke", "tokens": ["und", "wenn", "man", "sein", "be\u00b7gehrt", "!", "Streu", "Blu\u00b7men", "auf", "die", "B\u00e4n\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "PPOSAT", "VVPP", "$.", "NE", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "und ordne das Confect! Vor allen so gedenke", "tokens": ["und", "ord\u00b7ne", "das", "Con\u00b7fect", "!", "Vor", "al\u00b7len", "so", "ge\u00b7den\u00b7ke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "ART", "NN", "$.", "APPR", "PIAT", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "der kalten Schalen wol, da\u00df sie also, wie du", "tokens": ["der", "kal\u00b7ten", "Scha\u00b7len", "wol", ",", "da\u00df", "sie", "al\u00b7so", ",", "wie", "du"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "KOUS", "PPER", "ADV", "$,", "PWAV", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "sie selbsten gerne magst, uns wird gerichtet zu!", "tokens": ["sie", "selbs\u00b7ten", "ger\u00b7ne", "magst", ",", "uns", "wird", "ge\u00b7rich\u00b7tet", "zu", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VMFIN", "$,", "PPER", "VAFIN", "VVPP", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wenn dann das Saitenspiel nun wird sein angekommen,", "tokens": ["Wenn", "dann", "das", "Sai\u00b7ten\u00b7spiel", "nun", "wird", "sein", "an\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "VAFIN", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "und du die meiste Zahl der Jungfern hast vernommen,", "tokens": ["und", "du", "die", "meis\u00b7te", "Zahl", "der", "Jung\u00b7fern", "hast", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "die uns gebeten sind, so komm und sags uns an!", "tokens": ["die", "uns", "ge\u00b7be\u00b7ten", "sind", ",", "so", "komm", "und", "sags", "uns", "an", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "$,", "ADV", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wer dann von uns nicht k\u00f6mmt, der ist kein guter Man.", "tokens": ["Wer", "dann", "von", "uns", "nicht", "k\u00f6mmt", ",", "der", "ist", "kein", "gu\u00b7ter", "Man", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "PPER", "PTKNEG", "VVFIN", "$,", "PRELS", "VAFIN", "PIAT", "ADJA", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Den 28. Brachmonatstag 1635. in Revel.", "tokens": ["Den", "28.", "Brach\u00b7mo\u00b7nats\u00b7tag", "16\u00b735", ".", "in", "Re\u00b7vel", "."], "token_info": ["word", "ordinal", "word", "number", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "CARD", "$.", "APPR", "NE", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}