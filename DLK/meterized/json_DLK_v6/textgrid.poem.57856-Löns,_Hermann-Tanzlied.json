{"textgrid.poem.57856": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Tanzlied", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Kuckuck und der Piedewitt", "tokens": ["Der", "Ku\u00b7ckuck", "und", "der", "Pie\u00b7de\u00b7witt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das sind zwei lust'ge Br\u00fcder,", "tokens": ["Das", "sind", "zwei", "lust'\u00b7ge", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die fliegen immer auf und ab,", "tokens": ["Die", "flie\u00b7gen", "im\u00b7mer", "auf", "und", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lassen sich nicht nieder;", "tokens": ["Und", "las\u00b7sen", "sich", "nicht", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Piedewiedewittwittwitt,", "tokens": ["Pie\u00b7de\u00b7wie\u00b7de\u00b7witt\u00b7witt\u00b7witt", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meinen Schatz den bin ich quitt,", "tokens": ["Mei\u00b7nen", "Schatz", "den", "bin", "ich", "quitt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nun mu\u00df ich gehn und wandern", "tokens": ["Nun", "mu\u00df", "ich", "gehn", "und", "wan\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und suchen einen andern.", "tokens": ["Und", "su\u00b7chen", "ei\u00b7nen", "an\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Fiedel und der Brummelba\u00df", "tokens": ["Die", "Fie\u00b7del", "und", "der", "Brum\u00b7mel\u00b7ba\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die h\u00f6r' ich voller Freude,", "tokens": ["Die", "h\u00f6r'", "ich", "vol\u00b7ler", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zum Tanze will ich morgen gehn", "tokens": ["Zum", "Tan\u00b7ze", "will", "ich", "mor\u00b7gen", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In meinem wei\u00dfen Kleide;", "tokens": ["In", "mei\u00b7nem", "wei\u00b7\u00dfen", "Klei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Piedewiedewittwittwitt,", "tokens": ["Pie\u00b7de\u00b7wie\u00b7de\u00b7witt\u00b7witt\u00b7witt", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meinen Schatz den bin ich quitt,", "tokens": ["Mei\u00b7nen", "Schatz", "den", "bin", "ich", "quitt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nun mu\u00df ich gehn und wandern,", "tokens": ["Nun", "mu\u00df", "ich", "gehn", "und", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mir suchen einen andern.", "tokens": ["Mir", "su\u00b7chen", "ei\u00b7nen", "an\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Rotr\u00f6slein und Vergi\u00dfmeinnicht", "tokens": ["Ro\u00b7tr\u00f6s\u00b7lein", "und", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das sind zwei sch\u00f6ne Gaben,", "tokens": ["Das", "sind", "zwei", "sch\u00f6\u00b7ne", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Junggeselle h\u00fcbsch und fein", "tokens": ["Ein", "Jung\u00b7ge\u00b7sel\u00b7le", "h\u00fcbsch", "und", "fein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der soll sie beide haben;", "tokens": ["Der", "soll", "sie", "bei\u00b7de", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PIS", "VAFIN", "$."], "meter": "-+-+-+-", "mea