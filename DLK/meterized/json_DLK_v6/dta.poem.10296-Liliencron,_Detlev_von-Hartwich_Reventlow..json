{"dta.poem.10296": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Hartwich Reventlow.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Graf. Alf hat deine Tochter verf&#252;hrt.             ", "tokens": ["Graf", ".", "Alf", "hat", "dei\u00b7ne", "Toch\u00b7ter", "verf", "&#252;", "hrt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["NE", "$.", "NE", "VAFIN", "PPOSAT", "NN", "VVFIN", "$(", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Herrn Hartwich das die Kehle schn\u00fcrt,", "tokens": ["Herrn", "Hart\u00b7wich", "das", "die", "Keh\u00b7le", "schn\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201egeh\u2019 hin, lieb Bruder, dem Grafen meld\u2019 an,", "tokens": ["\u201e", "geh'", "hin", ",", "lieb", "Bru\u00b7der", ",", "dem", "Gra\u00b7fen", "meld'", "an", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "ADJD", "NN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich mord\u2019 ihn, wo ich ihn treffen kann,", "tokens": ["Ich", "mord'", "ihn", ",", "wo", "ich", "ihn", "tref\u00b7fen", "kann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "PWAV", "PPER", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Caj ritt den Burgberg schnell hinauf,", "tokens": ["Caj", "ritt", "den", "Burg\u00b7berg", "schnell", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "He, Pf\u00f6rtner, schlie\u00df\u2019 die Riegel auf,", "tokens": ["He", ",", "Pf\u00f6rt\u00b7ner", ",", "schlie\u00df'", "die", "Rie\u00b7gel", "auf", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u201ewas schwatzt Herr Hartwich? So sag\u2019 ihm zur\u00fcck:", "tokens": ["\u201e", "was", "schwatzt", "Herr", "Hart\u00b7wich", "?", "So", "sag'", "ihm", "zu\u00b7r\u00fcck", ":"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "NN", "NE", "$.", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Graf Alf hielt in den Fingern ein St\u00fcck,", "tokens": ["Graf", "Alf", "hielt", "in", "den", "Fin\u00b7gern", "ein", "St\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Auf g\u00fcldener Sch\u00fcssel, mit Blut benetzt,", "tokens": ["Auf", "g\u00fcl\u00b7de\u00b7ner", "Sch\u00fcs\u00b7sel", ",", "mit", "Blut", "be\u00b7netzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Herr Hartwich taumelt und ruft entsetzt:", "tokens": ["Herr", "Hart\u00b7wich", "tau\u00b7melt", "und", "ruft", "ent\u00b7setzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "KON", "VVFIN", "VVPP", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Herr Hartwig ging im Sommerwald,", "tokens": ["Herr", "Hart\u00b7wig", "ging", "im", "Som\u00b7mer\u00b7wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da traf er einen J\u00e4ger bald,", "tokens": ["Da", "traf", "er", "ei\u00b7nen", "J\u00e4\u00b7ger", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "\u201edie Kleider zieh\u2019 aus, und gieb sie mir her,", "tokens": ["\u201e", "die", "Klei\u00b7der", "zieh'", "aus", ",", "und", "gieb", "sie", "mir", "her", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PTKVZ", "$,", "KON", "VVIMP", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der gab ihm zitternd Horn und Speer,", "tokens": ["Der", "gab", "ihm", "zit\u00b7ternd", "Horn", "und", "Speer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVPP", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Im Walde zog ein Hirsch vertraut,", "tokens": ["Im", "Wal\u00b7de", "zog", "ein", "Hirsch", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor des Grafen Kammer wird es laut,", "tokens": ["Vor", "des", "Gra\u00b7fen", "Kam\u00b7mer", "wird", "es", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "\u201egraf Alf, es zieht im Morgenrot", "tokens": ["\u201e", "graf", "Alf", ",", "es", "zieht", "im", "Mor\u00b7gen\u00b7rot"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "NE", "$,", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herr Hartwich stie\u00df den Grafen tot:", "tokens": ["Herr", "Hart\u00b7wich", "stie\u00df", "den", "Gra\u00b7fen", "tot", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Der Page sah\u2019s, Herrn Hartwichs Sohn,", "tokens": ["Der", "Pa\u00b7ge", "sah's", ",", "Herrn", "Hart\u00b7wichs", "Sohn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "NN", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201emaria sah\u2019s vom Himmelsthron,", "tokens": ["\u201e", "ma\u00b7ria", "sah's", "vom", "Him\u00b7melst\u00b7hron", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Er k\u00fc\u00dft seinen Knaben mit wildem Schmerz,", "tokens": ["Er", "k\u00fc\u00dft", "sei\u00b7nen", "Kna\u00b7ben", "mit", "wil\u00b7dem", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "\u201enun schilt dich nimmer ein Menschenherz", "tokens": ["\u201e", "nun", "schilt", "dich", "nim\u00b7mer", "ein", "Men\u00b7schen\u00b7herz"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "Stolz schreitet der Ritter den Burgberg hinab,", "tokens": ["Stolz", "schrei\u00b7tet", "der", "Rit\u00b7ter", "den", "Burg\u00b7berg", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "++--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Vier M\u00f6nche murmeln am Marmorgrab,", "tokens": ["Vier", "M\u00f6n\u00b7che", "mur\u00b7meln", "am", "Mar\u00b7mor\u00b7grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}