{"textgrid.poem.25889": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[du breitest um mich einen Himmel, tiefblau]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du breitest um mich einen Himmel, tiefblau,", "tokens": ["Du", "brei\u00b7test", "um", "mich", "ei\u00b7nen", "Him\u00b7mel", ",", "tief\u00b7blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und dein Sang ist darin tr\u00e4umend wie die vertr\u00e4umten wei\u00dfen Wolken.", "tokens": ["Und", "dein", "Sang", "ist", "da\u00b7rin", "tr\u00e4u\u00b7mend", "wie", "die", "ver\u00b7tr\u00e4um\u00b7ten", "wei\u00b7\u00dfen", "Wol\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PAV", "ADJD", "KOKOM", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "--+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Manchmal blickst du auf wie die Erde dunkel,", "tokens": ["Manch\u00b7mal", "blickst", "du", "auf", "wie", "die", "Er\u00b7de", "dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KOKOM", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Wie die Erde treu und tief und gut.", "tokens": ["Wie", "die", "Er\u00b7de", "treu", "und", "tief", "und", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dann hast du bei der warmen Erde geschlafen.", "tokens": ["Dann", "hast", "du", "bei", "der", "war\u00b7men", "Er\u00b7de", "ge\u00b7schla\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Dein Herz wuchs auf einer Sommerwiese.", "tokens": ["Dein", "Herz", "wuchs", "auf", "ei\u00b7ner", "Som\u00b7mer\u00b7wie\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Einfach wie Butterblumen", "tokens": ["Ein\u00b7fach", "wie", "But\u00b7ter\u00b7blu\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KOKOM", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und kr\u00e4ftig wie der Salbei.", "tokens": ["Und", "kr\u00e4f\u00b7tig", "wie", "der", "Sal\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wir atmen uns ein.", "tokens": ["Wir", "at\u00b7men", "uns", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Drau\u00dfen am dunstblauen Horizont", "tokens": ["Drau\u00b7\u00dfen", "am", "dunst\u00b7blau\u00b7en", "Ho\u00b7ri\u00b7zont"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Atmen sich Sommermeer und Sommerhimmel ineinander.", "tokens": ["At\u00b7men", "sich", "Som\u00b7mer\u00b7meer", "und", "Som\u00b7mer\u00b7him\u00b7mel", "in\u00b7ein\u00b7an\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "NN", "KON", "NN", "ADV", "$."], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}}}}}