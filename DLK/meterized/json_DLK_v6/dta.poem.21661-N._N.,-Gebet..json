{"dta.poem.21661": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Gebet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Der Du mich ahnungsvoll umkreist,", "tokens": ["Der", "Du", "mich", "ah\u00b7nungs\u00b7voll", "um\u00b7kreist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00f6r\u2019 mein Gebet, urew\u2019ger Geist!", "tokens": ["H\u00f6r'", "mein", "Ge\u00b7bet", ",", "u\u00b7re\u00b7w'\u00b7ger", "Geist", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Der Du von Anfang bis zu Ende,", "tokens": ["Der", "Du", "von", "An\u00b7fang", "bis", "zu", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Dir aufheb\u2019 ich meine H\u00e4nde.", "tokens": ["Zu", "Dir", "auf\u00b7heb'", "ich", "mei\u00b7ne", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In Schauern sink ich vor Dir hin,", "tokens": ["In", "Schau\u00b7ern", "sink", "ich", "vor", "Dir", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil ich Dir ganz ergeben bin.", "tokens": ["Weil", "ich", "Dir", "ganz", "er\u00b7ge\u00b7ben", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du bist die Leuchte meines Lebens,", "tokens": ["Du", "bist", "die", "Leuch\u00b7te", "mei\u00b7nes", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Du bist das Urbild meines Strebens.", "tokens": ["Du", "bist", "das", "Ur\u00b7bild", "mei\u00b7nes", "Stre\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Du bist\u2019s allein, der in mir schafft,", "tokens": ["Du", "bist's", "al\u00b7lein", ",", "der", "in", "mir", "schafft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Du bist der Pfad, Du bist die Kraft.", "tokens": ["Du", "bist", "der", "Pfad", ",", "Du", "bist", "die", "Kraft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Du bist die Tiefe, bist die H\u00f6he,", "tokens": ["Du", "bist", "die", "Tie\u00b7fe", ",", "bist", "die", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Das Meer, darin ich untergehe,", "tokens": ["Das", "Meer", ",", "da\u00b7rin", "ich", "un\u00b7ter\u00b7ge\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "In Dir nur bin ich st\u00e4t und stark,", "tokens": ["In", "Dir", "nur", "bin", "ich", "st\u00e4t", "und", "stark", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Du bist die Wurzel, bist das Mark.", "tokens": ["Du", "bist", "die", "Wur\u00b7zel", ",", "bist", "das", "Mark", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Du bist der Baum, daran ich ranke,", "tokens": ["Du", "bist", "der", "Baum", ",", "da\u00b7ran", "ich", "ran\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Du richtest mich, da\u00df ich nicht wanke,", "tokens": ["Du", "rich\u00b7test", "mich", ",", "da\u00df", "ich", "nicht", "wan\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Du bist der Strom, der mich durchquillt", "tokens": ["Du", "bist", "der", "Strom", ",", "der", "mich", "durch\u00b7quillt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und meiner Seele Gluthen stillt.", "tokens": ["Und", "mei\u00b7ner", "See\u00b7le", "Glu\u00b7then", "stillt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Du bist der Anker mir im Wetter,", "tokens": ["Du", "bist", "der", "An\u00b7ker", "mir", "im", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Bist mein Erl\u00f6ser, mein Erretter,", "tokens": ["Bist", "mein", "Er\u00b7l\u00f6\u00b7ser", ",", "mein", "Er\u00b7ret\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Du bist das Wort, der Klang, der Sinn,", "tokens": ["Du", "bist", "das", "Wort", ",", "der", "Klang", ",", "der", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "In dem ich lebe, web\u2019 und bin", "tokens": ["In", "dem", "ich", "le\u00b7be", ",", "web'", "und", "bin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "KON", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Du bist der Inhalt im Gef\u00e4\u00dfe,", "tokens": ["Du", "bist", "der", "In\u00b7halt", "im", "Ge\u00b7f\u00e4\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Nichts ist, nach dem ich Dich bem\u00e4\u00dfe,", "tokens": ["Nichts", "ist", ",", "nach", "dem", "ich", "Dich", "be\u00b7m\u00e4\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "$,", "APPR", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Du bist die Wahrheit, bist das Licht,", "tokens": ["Du", "bist", "die", "Wahr\u00b7heit", ",", "bist", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Das flammend aus der Seele bricht,", "tokens": ["Das", "flam\u00b7mend", "aus", "der", "See\u00b7le", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Du bist das Sch\u00f6ne, bist das Gute,", "tokens": ["Du", "bist", "das", "Sch\u00f6\u00b7ne", ",", "bist", "das", "Gu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "F\u00fcr das ich bin, f\u00fcr das ich blute \u2014", "tokens": ["F\u00fcr", "das", "ich", "bin", ",", "f\u00fcr", "das", "ich", "blu\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VAFIN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Trotz Noth und Tod f\u00fcr alle Zeit,", "tokens": ["Trotz", "Noth", "und", "Tod", "f\u00fcr", "al\u00b7le", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Urew\u2019ger Geist, sei benedeit!", "tokens": ["U\u00b7re\u00b7w'\u00b7ger", "Geist", ",", "sei", "be\u00b7ne\u00b7deit", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}