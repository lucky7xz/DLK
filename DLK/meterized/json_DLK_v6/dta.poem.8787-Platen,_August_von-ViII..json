{"dta.poem.8787": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "ViII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Und dessen ganze Seele schmachtet, allein zu seyn!", "tokens": ["Und", "des\u00b7sen", "gan\u00b7ze", "See\u00b7le", "schmach\u00b7tet", ",", "al\u00b7lein", "zu", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADJA", "NN", "VVFIN", "$,", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es schuf der unersch\u00f6pfte Sch\u00f6pfer Gesch\u00f6pfe rings,", "tokens": ["Es", "schuf", "der", "un\u00b7er\u00b7sch\u00f6pf\u00b7te", "Sch\u00f6p\u00b7fer", "Ge\u00b7sch\u00f6p\u00b7fe", "rings", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und nicht ein einzig Wesen trachtet, allein zu seyn:", "tokens": ["Und", "nicht", "ein", "ein\u00b7zig", "We\u00b7sen", "trach\u00b7tet", ",", "al\u00b7lein", "zu", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "ADJD", "NN", "VVFIN", "$,", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Allein zu seyn verschm\u00e4ht die Tulpe des Tulpenbeets,", "tokens": ["Al\u00b7lein", "zu", "seyn", "ver\u00b7schm\u00e4ht", "die", "Tul\u00b7pe", "des", "Tul\u00b7pen\u00b7beets", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VAINF", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Es scheut der Stern sich, wenn es nachtet, allein zu seyn;", "tokens": ["Es", "scheut", "der", "Stern", "sich", ",", "wenn", "es", "nach\u00b7tet", ",", "al\u00b7lein", "zu", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "$,", "KOUS", "PPER", "VVPP", "$,", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Verla\u00df den Stolz, der deine Seele so tief beth\u00f6rt,", "tokens": ["Ver\u00b7la\u00df", "den", "Stolz", ",", "der", "dei\u00b7ne", "See\u00b7le", "so", "tief", "be\u00b7th\u00f6rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Und der es f\u00fcr erhaben achtet, allein zu seyn!", "tokens": ["Und", "der", "es", "f\u00fcr", "er\u00b7ha\u00b7ben", "ach\u00b7tet", ",", "al\u00b7lein", "zu", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "APPR", "ADJD", "VVFIN", "$,", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}