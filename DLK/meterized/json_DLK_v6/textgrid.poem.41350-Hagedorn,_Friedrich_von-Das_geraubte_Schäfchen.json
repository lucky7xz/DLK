{"textgrid.poem.41350": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Das geraubte Sch\u00e4fchen", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Joabs Heldenheer die Kinder Ammon schreckte,", "tokens": ["Als", "Joabs", "Hel\u00b7den\u00b7heer", "die", "Kin\u00b7der", "Am\u00b7mon", "schreck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Und schon ganz Israel das Land um Rabba deckte,", "tokens": ["Und", "schon", "ganz", "Is\u00b7rael", "das", "Land", "um", "Rab\u00b7ba", "deck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "NE", "ART", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Wo der Gewaltigen und Hanons Unverstand", "tokens": ["Wo", "der", "Ge\u00b7wal\u00b7ti\u00b7gen", "und", "Ha\u00b7nons", "Un\u00b7ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "KON", "NE", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Boten sch\u00e4nden lie\u00df, die David abgesandt,", "tokens": ["Die", "Bo\u00b7ten", "sch\u00e4n\u00b7den", "lie\u00df", ",", "die", "Da\u00b7vid", "ab\u00b7ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$,", "PRELS", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da raubte sein Befehl Uria Gl\u00fcck und Leben", "tokens": ["Da", "raub\u00b7te", "sein", "Be\u00b7fehl", "U\u00b7ria", "Gl\u00fcck", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "NE", "NN", "KON", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Um das geliebte Weib, das ihm der Herr gegeben,", "tokens": ["Um", "das", "ge\u00b7lieb\u00b7te", "Weib", ",", "das", "ihm", "der", "Herr", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Tochter Eliams, die Davids Freundin war,", "tokens": ["Die", "Toch\u00b7ter", "E\u00b7li\u00b7ams", ",", "die", "Da\u00b7vids", "Freun\u00b7din", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "PRELS", "NE", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und, als sie ausgetraurt, ihm einen Sohn gebar.", "tokens": ["Und", ",", "als", "sie", "aus\u00b7ge\u00b7traurt", ",", "ihm", "ei\u00b7nen", "Sohn", "ge\u00b7bar", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "VVFIN", "$,", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dem Herrn mi\u00dffiel die That, und Nathan ward ersehen,", "tokens": ["Dem", "Herrn", "mi\u00df\u00b7fiel", "die", "That", ",", "und", "Na\u00b7than", "ward", "er\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "KON", "NE", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Worten Seines Zorns zum K\u00f6nige zu gehen.", "tokens": ["Mit", "Wor\u00b7ten", "Sei\u00b7nes", "Zorns", "zum", "K\u00f6\u00b7ni\u00b7ge", "zu", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er sprach: In einer Stadt befanden sich zugleich", "tokens": ["Er", "sprach", ":", "In", "ei\u00b7ner", "Stadt", "be\u00b7fan\u00b7den", "sich", "zu\u00b7gleich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "APPR", "ART", "NN", "VVFIN", "PRF", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zween M\u00e4nner; einer arm, der andre gro\u00df und reich.", "tokens": ["Zween", "M\u00e4n\u00b7ner", ";", "ei\u00b7ner", "arm", ",", "der", "and\u00b7re", "gro\u00df", "und", "reich", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "ART", "ADJD", "$,", "PRELS", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Reiche sahe stets in Tagen voller Freuden", "tokens": ["Der", "Rei\u00b7che", "sa\u00b7he", "stets", "in", "Ta\u00b7gen", "vol\u00b7ler", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "ADV", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Heerden seines Hofs auf gr\u00fcnen H\u00fcgeln weiden;", "tokens": ["Die", "Heer\u00b7den", "sei\u00b7nes", "Hofs", "auf", "gr\u00fc\u00b7nen", "H\u00fc\u00b7geln", "wei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Rinder unzerstreut bei jungen Farren ruhn;", "tokens": ["Die", "Rin\u00b7der", "un\u00b7zer\u00b7streut", "bei", "jun\u00b7gen", "Far\u00b7ren", "ruhn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Gei\u00df' und Widder Muth im Felde fr\u00f6hlich thun;", "tokens": ["Der", "Gei\u00df'", "und", "Wid\u00b7der", "Muth", "im", "Fel\u00b7de", "fr\u00f6h\u00b7lich", "thun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NN", "APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die L\u00e4mmer ohne Fehl um ihre M\u00fctter springen;", "tokens": ["Die", "L\u00e4m\u00b7mer", "oh\u00b7ne", "Fehl", "um", "ih\u00b7re", "M\u00fct\u00b7ter", "sprin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das Lastvieh durch den Klee mit reichen B\u00fcrden dringen;", "tokens": ["Das", "Last\u00b7vieh", "durch", "den", "Klee", "mit", "rei\u00b7chen", "B\u00fcr\u00b7den", "drin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Bl\u00fcten dicker Saat sich an den Wassern bl\u00e4hn,", "tokens": ["Die", "Bl\u00fc\u00b7ten", "di\u00b7cker", "Saat", "sich", "an", "den", "Was\u00b7sern", "bl\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und seiner Schnitter Flei\u00df die sch\u00f6nsten Halmen m\u00e4hn.", "tokens": ["Und", "sei\u00b7ner", "Schnit\u00b7ter", "Flei\u00df", "die", "sch\u00f6ns\u00b7ten", "Hal\u00b7men", "m\u00e4hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dem Armen, ach! was war dem Armen doch bescheeret?", "tokens": ["Dem", "Ar\u00b7men", ",", "ach", "!", "was", "war", "dem", "Ar\u00b7men", "doch", "be\u00b7schee\u00b7ret", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ITJ", "$.", "PWS", "VAFIN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ein einzig kleines Schaf, das er gekauft, gen\u00e4hret.", "tokens": ["Ein", "ein\u00b7zig", "klei\u00b7nes", "Schaf", ",", "das", "er", "ge\u00b7kauft", ",", "ge\u00b7n\u00e4h\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Das wuchs, und ward bei ihm und seinen Kindern gro\u00df,", "tokens": ["Das", "wuchs", ",", "und", "ward", "bei", "ihm", "und", "sei\u00b7nen", "Kin\u00b7dern", "gro\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KON", "VAFIN", "APPR", "PPER", "KON", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und kannte seinen Ruf, und schlief in seinem Schoo\u00df,", "tokens": ["Und", "kann\u00b7te", "sei\u00b7nen", "Ruf", ",", "und", "schlief", "in", "sei\u00b7nem", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und trank von seinem Kelch, und a\u00df von seinen Bissen,", "tokens": ["Und", "trank", "von", "sei\u00b7nem", "Kelch", ",", "und", "a\u00df", "von", "sei\u00b7nen", "Bis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und folgte seiner Hand, und lief nach seinen K\u00fcssen:", "tokens": ["Und", "folg\u00b7te", "sei\u00b7ner", "Hand", ",", "und", "lief", "nach", "sei\u00b7nen", "K\u00fcs\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Er hielte dieses Schaf, sein liebstes auf der Welt,", "tokens": ["Er", "hiel\u00b7te", "die\u00b7ses", "Schaf", ",", "sein", "liebs\u00b7tes", "auf", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$,", "PPOSAT", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wie in Jerusalem man eine Tochter h\u00e4lt.", "tokens": ["Wie", "in", "Je\u00b7ru\u00b7sa\u00b7lem", "man", "ei\u00b7ne", "Toch\u00b7ter", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NE", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Dem Reichen kam ein Gast; da\u00df der bewirthet w\u00fcrde,", "tokens": ["Dem", "Rei\u00b7chen", "kam", "ein", "Gast", ";", "da\u00df", "der", "be\u00b7wirt\u00b7het", "w\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$.", "KOUS", "ART", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Nahm er kein Rind, kein Schaf aus seiner Weid' und H\u00fcrde:", "tokens": ["Nahm", "er", "kein", "Rind", ",", "kein", "Schaf", "aus", "sei\u00b7ner", "Weid'", "und", "H\u00fcr\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$,", "PIAT", "NN", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die r\u00e4uberische Faust macht ihm ein Freudenmahl", "tokens": ["Die", "r\u00e4u\u00b7be\u00b7ri\u00b7sche", "Faust", "macht", "ihm", "ein", "Freu\u00b7den\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Von jenem wei\u00dfen Schaf, das er dem Armen stahl.", "tokens": ["Von", "je\u00b7nem", "wei\u00b7\u00dfen", "Schaf", ",", "das", "er", "dem", "Ar\u00b7men", "stahl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er schwieg, und David schwur: Der Frevler soll nicht leben!", "tokens": ["Er", "schwieg", ",", "und", "Da\u00b7vid", "schwur", ":", "Der", "Frev\u00b7ler", "soll", "nicht", "le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "NE", "VVFIN", "$.", "ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er soll nicht nur das Schaf vierf\u00e4ltig wiedergeben;", "tokens": ["Er", "soll", "nicht", "nur", "das", "Schaf", "vier\u00b7f\u00e4l\u00b7tig", "wie\u00b7der\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer solche Missethat in Israel beginnt,", "tokens": ["Wer", "sol\u00b7che", "Mis\u00b7se\u00b7that", "in", "Is\u00b7rael", "be\u00b7ginnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "So wahr der H\u00f6chste lebt! der ist des Todes Kind.", "tokens": ["So", "wahr", "der", "H\u00f6chs\u00b7te", "lebt", "!", "der", "ist", "des", "To\u00b7des", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "$.", "ART", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du, David, bist der Mann: erwidert der Prophete;", "tokens": ["Du", ",", "Da\u00b7vid", ",", "bist", "der", "Mann", ":", "er\u00b7wi\u00b7dert", "der", "Pro\u00b7phe\u00b7te", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$,", "VAFIN", "ART", "NN", "$.", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Will deine Seele noch, da\u00df man den R\u00e4uber t\u00f6dte?", "tokens": ["Will", "dei\u00b7ne", "See\u00b7le", "noch", ",", "da\u00df", "man", "den", "R\u00e4u\u00b7ber", "t\u00f6d\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "$,", "KOUS", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So spricht der Herr, dein Gott: Ich habe dich gebaut;", "tokens": ["So", "spricht", "der", "Herr", ",", "dein", "Gott", ":", "Ich", "ha\u00b7be", "dich", "ge\u00b7baut", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zum K\u00f6nige gesalbt; das Reich dir anvertraut;", "tokens": ["Zum", "K\u00f6\u00b7ni\u00b7ge", "ge\u00b7salbt", ";", "das", "Reich", "dir", "an\u00b7ver\u00b7traut", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$.", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den H\u00e4nden Sauls gewehrt; jetzt deines Volks verschonet;", "tokens": ["Den", "H\u00e4n\u00b7den", "Sauls", "ge\u00b7wehrt", ";", "jetzt", "dei\u00b7nes", "Volks", "ver\u00b7scho\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVPP", "$.", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und dir das Haus verliehn, in dem dein Herr gewohnet;", "tokens": ["Und", "dir", "das", "Haus", "ver\u00b7liehn", ",", "in", "dem", "dein", "Herr", "ge\u00b7woh\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVINF", "$,", "APPR", "ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Weiber deines Herrn gab ich in deinen Schoo\u00df;", "tokens": ["Die", "Wei\u00b7ber", "dei\u00b7nes", "Herrn", "gab", "ich", "in", "dei\u00b7nen", "Schoo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du bist in Israel, du bist in Juda gro\u00df.", "tokens": ["Du", "bist", "in", "Is\u00b7rael", ",", "du", "bist", "in", "Ju\u00b7da", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NE", "$,", "PPER", "VAFIN", "APPR", "NE", "ADJD", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.9": {"text": "Du bist durch mich ein Herr, ein Sieger und ein K\u00f6nig,", "tokens": ["Du", "bist", "durch", "mich", "ein", "Herr", ",", "ein", "Sie\u00b7ger", "und", "ein", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Du, des Isai Sohn. Ist dieses dir zu wenig,", "tokens": ["Du", ",", "des", "I\u00b7sai", "Sohn", ".", "Ist", "die\u00b7ses", "dir", "zu", "we\u00b7nig", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NE", "NN", "$.", "VAFIN", "PDAT", "PPER", "APPR", "PIS", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.11": {"text": "So f\u00fcg' ich mehr hinzu. Wie aber kannst du nun", "tokens": ["So", "f\u00fcg'", "ich", "mehr", "hin\u00b7zu", ".", "Wie", "a\u00b7ber", "kannst", "du", "nun"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "PWAV", "ADV", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Vor meinem Angesicht ein solches Uebel thun?", "tokens": ["Vor", "mei\u00b7nem", "An\u00b7ge\u00b7sicht", "ein", "sol\u00b7ches", "Ue\u00b7bel", "thun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Des Herrn Gebot verschm\u00e4hn, ihn und sein Wort verachten,", "tokens": ["Des", "Herrn", "Ge\u00b7bot", "ver\u00b7schm\u00e4hn", ",", "ihn", "und", "sein", "Wort", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,", "PPER", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und den Hethiter dir mit fremdem Schwerte schlachten?", "tokens": ["Und", "den", "He\u00b7thi\u00b7ter", "dir", "mit", "frem\u00b7dem", "Schwer\u00b7te", "schlach\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Durch dich fri\u00dft Ammons Schwert Uria, deinen Knecht.", "tokens": ["Durch", "dich", "fri\u00dft", "Am\u00b7mons", "Schwert", "U\u00b7ria", ",", "dei\u00b7nen", "Knecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "NE", "NE", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.16": {"text": "Sein Blut zeugt wider dich, und schreit zu mir um Recht.", "tokens": ["Sein", "Blut", "zeugt", "wi\u00b7der", "dich", ",", "und", "schreit", "zu", "mir", "um", "Recht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$,", "KON", "ADJD", "APPR", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Noch darfst du gar sein Weib jetzt, als dein Weib, umfassen!", "tokens": ["Noch", "darfst", "du", "gar", "sein", "Weib", "jetzt", ",", "als", "dein", "Weib", ",", "um\u00b7fas\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "ADV", "$,", "KOUS", "PPOSAT", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Drum soll das Rachschwert nie von deinem Hause lassen.", "tokens": ["Drum", "soll", "das", "Rach\u00b7schwert", "nie", "von", "dei\u00b7nem", "Hau\u00b7se", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So spricht der Herr, dein Gott: Zu desto gr\u00f6\u00dfrer Pein", "tokens": ["So", "spricht", "der", "Herr", ",", "dein", "Gott", ":", "Zu", "des\u00b7to", "gr\u00f6\u00df\u00b7rer", "Pein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Soll dir dein eignes Haus des Ungl\u00fccks Quelle sein.", "tokens": ["Soll", "dir", "dein", "eig\u00b7nes", "Haus", "des", "Un\u00b7gl\u00fccks", "Quel\u00b7le", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die Weiber will ich dir vor deinen Augen rauben,", "tokens": ["Die", "Wei\u00b7ber", "will", "ich", "dir", "vor", "dei\u00b7nen", "Au\u00b7gen", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und deinem N\u00e4chsten selbst der Strafe Lust erlauben:", "tokens": ["Und", "dei\u00b7nem", "N\u00e4chs\u00b7ten", "selbst", "der", "Stra\u00b7fe", "Lust", "er\u00b7lau\u00b7ben", ":"], "token_info": ["word", "word", "