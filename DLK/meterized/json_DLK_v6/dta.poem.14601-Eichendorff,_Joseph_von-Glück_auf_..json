{"dta.poem.14601": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcck auf .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wie klein wird sein da, was mich hat gehalten,", "tokens": ["Wie", "klein", "wird", "sein", "da", ",", "was", "mich", "hat", "ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "ADV", "$,", "PWS", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie wenig, was ich Irrender vollbracht,", "tokens": ["Wie", "we\u00b7nig", ",", "was", "ich", "Ir\u00b7ren\u00b7der", "voll\u00b7bracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "PWS", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch was den Felsen gl\u00e4ubig hat gespalten:", "tokens": ["Doch", "was", "den", "Fel\u00b7sen", "gl\u00e4u\u00b7big", "hat", "ge\u00b7spal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Sehnsucht treu steigt mit mir aus der Nacht", "tokens": ["Die", "Sehn\u00b7sucht", "treu", "steigt", "mit", "mir", "aus", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN", "APPR", "PPER", "APPR", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und legt mir an die wunderbaren Schwingen,", "tokens": ["Und", "legt", "mir", "an", "die", "wun\u00b7der\u00b7ba\u00b7ren", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die durch die Stille mich nach Hause bringen.", "tokens": ["Die", "durch", "die", "Stil\u00b7le", "mich", "nach", "Hau\u00b7se", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}