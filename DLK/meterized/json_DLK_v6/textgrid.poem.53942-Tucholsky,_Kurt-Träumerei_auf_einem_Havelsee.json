{"textgrid.poem.53942": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Tr\u00e4umerei auf einem Havelsee", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin Prokurist einer W\u00e4schefabrik,", "tokens": ["Ich", "bin", "Pro\u00b7ku\u00b7rist", "ei\u00b7ner", "W\u00e4\u00b7sche\u00b7fab\u00b7rik", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Sternberg, Guttmann ", "tokens": ["Stern\u00b7berg", ",", "Gutt\u00b7mann"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$,", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Mein Segelboot hei\u00dft \u203aHeil und Sieg\u2039,", "tokens": ["Mein", "Se\u00b7gel\u00b7boot", "hei\u00dft", "\u203a", "Heil", "und", "Sieg", "\u2039", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$(", "NN", "KON", "NN", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "zwei Stunden lieg ich hier schon", "tokens": ["zwei", "Stun\u00b7den", "lieg", "ich", "hier", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "und seh auf die Kiefern und in das Wasser hinein \u2013", "tokens": ["und", "seh", "auf", "die", "Kie\u00b7fern", "und", "in", "das", "Was\u00b7ser", "hin\u00b7ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "-+--+--+-+--+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "auf meinem Boot ganz allein.", "tokens": ["auf", "mei\u00b7nem", "Boot", "ganz", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Urlaub hatte ich im August,", "tokens": ["Ur\u00b7laub", "hat\u00b7te", "ich", "im", "Au\u00b7gust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "ich war in Norderney,", "tokens": ["ich", "war", "in", "Nor\u00b7der\u00b7ney", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "mit Lilly . . . ihre linke Brust", "tokens": ["mit", "Lil\u00b7ly", ".", ".", ".", "ih\u00b7re", "lin\u00b7ke", "Brust"], "token_info": ["word", "word", "punct", "punct", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "$.", "$.", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "sieht aus wie ein kleines Ei.", "tokens": ["sieht", "aus", "wie", "ein", "klei\u00b7nes", "Ei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn man sie da kneift, dann wird sie gemein \u2013", "tokens": ["Wenn", "man", "sie", "da", "kneift", ",", "dann", "wird", "sie", "ge\u00b7mein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "auf meinem Boot ganz allein.", "tokens": ["auf", "mei\u00b7nem", "Boot", "ganz", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Graske ist ein gemeiner Hund,", "tokens": ["Gras\u00b7ke", "ist", "ein", "ge\u00b7mei\u00b7ner", "Hund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "ein falsches Aas \u2013 er tut blo\u00df so . . .", "tokens": ["ein", "fal\u00b7sches", "Aas", "\u2013", "er", "tut", "blo\u00df", "so", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PPER", "VVFIN", "ADV", "ADV", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "er wei\u00df, der Alte ist nicht ganz gesund;", "tokens": ["er", "wei\u00df", ",", "der", "Al\u00b7te", "ist", "nicht", "ganz", "ge\u00b7sund", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "wenn mans merkt, bleibt er l\u00e4nger im B\u00fcro.", "tokens": ["wenn", "mans", "merkt", ",", "bleibt", "er", "l\u00e4n\u00b7ger", "im", "B\u00fc\u00b7ro", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und dem Junior kriecht er jetzt auch hinten rein \u2013", "tokens": ["Und", "dem", "Ju\u00b7ni\u00b7or", "kriecht", "er", "jetzt", "auch", "hin\u00b7ten", "rein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$("], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "auf meinem Boot ganz allein.", "tokens": ["auf", "mei\u00b7nem", "Boot", "ganz", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Mutter wird alt. Wie alt . . . warte mal:", "tokens": ["Mut\u00b7ter", "wird", "alt", ".", "Wie", "alt", ".", ".", ".", "war\u00b7te", "mal", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$.", "PWAV", "ADJD", "$.", "$.", "$.", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "vierundsechzig, nein: achtundsechzig, genau.", "tokens": ["vie\u00b7rund\u00b7sech\u00b7zig", ",", "nein", ":", "acht\u00b7und\u00b7sech\u00b7zig", ",", "ge\u00b7nau", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["CARD", "$,", "PTKANT", "$.", "CARD", "$,", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Grete soll ganz still sein; sie p\u00f6belt mit ihrem Personal", "tokens": ["Gre\u00b7te", "soll", "ganz", "still", "sein", ";", "sie", "p\u00f6\u00b7belt", "mit", "ih\u00b7rem", "Per\u00b7so\u00b7nal"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "ADV", "ADJD", "VAINF", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "wie eine Schl\u00e4chtersfrau.", "tokens": ["wie", "ei\u00b7ne", "Schl\u00e4ch\u00b7ters\u00b7frau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich frage mich: mu\u00df eigentlich Verwandtschaft sein?", "tokens": ["Ich", "fra\u00b7ge", "mich", ":", "mu\u00df", "ei\u00b7gent\u00b7lich", "Ver\u00b7wandt\u00b7schaft", "sein", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "VMFIN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "auf meinem Boot ganz allein.", "tokens": ["auf", "mei\u00b7nem", "Boot", "ganz", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Ich habe es schlie\u00dflich zu was gebracht,", "tokens": ["Ich", "ha\u00b7be", "es", "schlie\u00df\u00b7lich", "zu", "was", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "PRELS", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "ich geh auf den Presseball;", "tokens": ["ich", "geh", "auf", "den", "Pres\u00b7se\u00b7ball", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "++--+-+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "auf Reisen fahr ich Zweiter; die Jacht", "tokens": ["auf", "Rei\u00b7sen", "fahr", "ich", "Zwei\u00b7ter", ";", "die", "Jacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "NN", "$.", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "hier hie\u00df fr\u00fcher \u203aNachtigall\u2039.", "tokens": ["hier", "hie\u00df", "fr\u00fc\u00b7her", "\u203a", "Nach\u00b7ti\u00b7gall", "\u2039", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$(", "NN", "$(", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Quatsch. Jetzt hei\u00dft sie richtig. Manchmal lade ich Willi und Ottmar ein \u2013", "tokens": ["Quatsch", ".", "Jetzt", "hei\u00dft", "sie", "rich\u00b7tig", ".", "Manch\u00b7mal", "la\u00b7de", "ich", "Wil\u00b7li", "und", "Ott\u00b7mar", "ein", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "PPER", "ADJD", "$.", "ADV", "VVFIN", "PPER", "NE", "KON", "NE", "ART", "$("], "meter": "+-+-+-+-+--+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.6": {"text": "nein, Ottmar nicht, der hat mich bei den jungen Aktien", "tokens": ["nein", ",", "Ott\u00b7mar", "nicht", ",", "der", "hat", "mich", "bei", "den", "jun\u00b7gen", "Ak\u00b7ti\u00b7en"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NE", "PTKNEG", "$,", "PRELS", "VAFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "nicht mitgenommen \u2013 schlie\u00dflich werd ich dem Affen doch", "tokens": ["nicht", "mit\u00b7ge\u00b7nom\u00b7men", "\u2013", "schlie\u00df\u00b7lich", "werd", "ich", "dem", "Af\u00b7fen", "doch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVPP", "$(", "ADJD", "VAFIN", "PPER", "ART", "NN", "ADV"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "nicht nachlaufen, das hab ich nicht n\u00f6tig; stehen jetzt 192,", "tokens": ["nicht", "nach\u00b7lau\u00b7fen", ",", "das", "hab", "ich", "nicht", "n\u00f6\u00b7tig", ";", "ste\u00b7hen", "jetzt", "192", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "number", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "PDS", "VAFIN", "PPER", "PTKNEG", "ADJD", "$.", "VVFIN", "ADV", "CARD", "$,"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "193 . . . wo ist denn die Zeitung? \u2013", "tokens": [".", ".", ".", "wo", "ist", "denn", "die", "Zei\u00b7tung", "?", "\u2013"], "token_info": ["punct", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$.", "$.", "$.", "PWAV", "VAFIN", "ADV", "ART", "NN", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "auf meinem Boot ganz allein.", "tokens": ["auf", "mei\u00b7nem", "Boot", "ganz", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Das ist meine liebste Erholungszeit,", "tokens": ["Das", "ist", "mei\u00b7ne", "liebs\u00b7te", "Er\u00b7ho\u00b7lungs\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "auf meinem Boot ganz allein.", "tokens": ["auf", "mei\u00b7nem", "Boot", "ganz", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Kein Mensch ist zu sehen weit und breit \u2013", "tokens": ["Kein", "Mensch", "ist", "zu", "se\u00b7hen", "weit", "und", "breit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PTKZU", "VVINF", "ADJD", "KON", "ADJD", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "kann man einsamer sein?", "tokens": ["kann", "man", "ein\u00b7sa\u00b7mer", "sein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADJD", "VAINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Eine Welle gluckst. Ich bin einsam. Zwar", "tokens": ["Ei\u00b7ne", "Wel\u00b7le", "gluckst", ".", "Ich", "bin", "ein\u00b7sam", ".", "Zwar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "ADJD", "$.", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "die Inventur beginnt morgen,", "tokens": ["die", "In\u00b7ven\u00b7tur", "be\u00b7ginnt", "mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.7": {"text": "und wie die Sirenen mit schwimmendem Haar", "tokens": ["und", "wie", "die", "Si\u00b7re\u00b7nen", "mit", "schwim\u00b7men\u00b7dem", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "ziehn im See meine Sorgen:", "tokens": ["ziehn", "im", "See", "mei\u00b7ne", "Sor\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.7": {"line.1": {"text": "Lilly, Mama und die W\u00e4schefabrik,", "tokens": ["Lil\u00b7ly", ",", "Ma\u00b7ma", "und", "die", "W\u00e4\u00b7sche\u00b7fab\u00b7rik", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "die Reparatur von \u203aHeil und Sieg\u2039,", "tokens": ["die", "Re\u00b7pa\u00b7ra\u00b7tur", "von", "\u203a", "Heil", "und", "Sieg", "\u2039", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "KON", "NN", "$(", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Graske und Ottmar, der Egoist;", "tokens": ["Gras\u00b7ke", "und", "Ott\u00b7mar", ",", "der", "E\u00b7goist", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$,", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "wer im Silbenr\u00e4tsel \u203aFayence-Maler\u2039 ist \u2013;", "tokens": ["wer", "im", "Sil\u00b7ben\u00b7r\u00e4t\u00b7sel", "\u203a", "Fa\u00b7yence\u00b7Ma\u00b7ler", "\u2039", "ist", "\u2013", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["PWS", "APPRART", "NN", "$(", "NE", "$(", "VAFIN", "$(", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "der Krach mit dem Chef von der Expedition;", "tokens": ["der", "Krach", "mit", "dem", "Chef", "von", "der", "Ex\u00b7pe\u00b7di\u00b7ti\u00b7on", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+---+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "die Weihnachtsgratifikation \u2013", "tokens": ["die", "Weih\u00b7nachts\u00b7gra\u00b7ti\u00b7fi\u00b7ka\u00b7ti\u00b7on", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "sonst aber schwimme ich hier im m\u00e4rkischen Sonnenschein \u2013", "tokens": ["sonst", "a\u00b7ber", "schwim\u00b7me", "ich", "hier", "im", "m\u00e4r\u00b7ki\u00b7schen", "Son\u00b7nen\u00b7schein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "auf meinem Boot ganz allein.", "tokens": ["auf", "mei\u00b7nem", "Boot", "ganz", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}