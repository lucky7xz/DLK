{"dta.poem.2074": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Steinbock.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die\u00df Thier scheint eine wilde Ziege zu seyn, doch", "tokens": ["Die\u00df", "Thier", "scheint", "ei\u00b7ne", "wil\u00b7de", "Zie\u00b7ge", "zu", "seyn", ",", "doch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKZU", "VAINF", "$,", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "von besondrer Art,", "tokens": ["von", "be\u00b7sond\u00b7rer", "Art", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da es nur auf der Alpen Spitzen und nirgend sonst ge-", "tokens": ["Da", "es", "nur", "auf", "der", "Al\u00b7pen", "Spit\u00b7zen", "und", "nir\u00b7gend", "sonst", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "NN", "KON", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "funden ward.", "tokens": ["fun\u00b7den", "ward", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VAFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Am meisten gleichet es den Gemsen, doch siehet man es", "tokens": ["Am", "meis\u00b7ten", "glei\u00b7chet", "es", "den", "Gem\u00b7sen", ",", "doch", "sie\u00b7het", "man", "es"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "PIS", "VVFIN", "PPER", "ART", "NN", "$,", "ADV", "VVFIN", "PIS", "PPER"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "H\u00f6rner f\u00fchren", "tokens": ["H\u00f6r\u00b7ner", "f\u00fch\u00b7ren"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "So lang, da\u00df sie ihm, wenn er stehet, gemeiniglich", "tokens": ["So", "lang", ",", "da\u00df", "sie", "ihm", ",", "wenn", "er", "ste\u00b7het", ",", "ge\u00b7mei\u00b7nig\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,", "ADV"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "das Kreuz ber\u00fchren;", "tokens": ["das", "Kreuz", "be\u00b7r\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "So breit, da\u00df auf die drey Maa\u00df Wasser sich in ein ein-", "tokens": ["So", "breit", ",", "da\u00df", "auf", "die", "drey", "Maa\u00df", "Was\u00b7ser", "sich", "in", "ein", "ein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOUS", "APPR", "ART", "CARD", "NN", "NN", "PRF", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "zigs f\u00fcllen lassen:", "tokens": ["zigs", "f\u00fcl\u00b7len", "las\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Mit diesen H\u00f6rnern weis er nun an Felsen sich so fest zu", "tokens": ["Mit", "die\u00b7sen", "H\u00f6r\u00b7nern", "weis", "er", "nun", "an", "Fel\u00b7sen", "sich", "so", "fest", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "PTKVZ", "PPER", "ADV", "APPR", "NN", "PRF", "ADV", "ADJD", "PTKZU"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.12": {"text": "fassen,", "tokens": ["fas\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Da\u00df er, indem er sich daran gem\u00e4hlig hin und wieder", "tokens": ["Da\u00df", "er", ",", "in\u00b7dem", "er", "sich", "da\u00b7ran", "ge\u00b7m\u00e4h\u00b7lig", "hin", "und", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "PRF", "PAV", "ADJD", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.14": {"text": "schwingt,", "tokens": ["schwingt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Von einem Felsen zu dem andern bewundernsw\u00fcrdig schnell", "tokens": ["Von", "ei\u00b7nem", "Fel\u00b7sen", "zu", "dem", "an\u00b7dern", "be\u00b7wun\u00b7derns\u00b7w\u00fcr\u00b7dig", "schnell"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "ADJA", "ADJD", "ADJD"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "sich bringt.", "tokens": ["sich", "bringt", "."], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.17": {"text": "Ja, wenn er auch von einer H\u00f6h und unersteiglich steilen", "tokens": ["Ja", ",", "wenn", "er", "auch", "von", "ei\u00b7ner", "H\u00f6h", "und", "un\u00b7er\u00b7steig\u00b7lich", "stei\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "ADV", "APPR", "ART", "NN", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.18": {"text": "Spitzen", "tokens": ["Spit\u00b7zen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Herunterst\u00fcrzt, weis er die H\u00f6rner auf solche Weise vor-", "tokens": ["Her\u00b7un\u00b7ter\u00b7st\u00fcrzt", ",", "weis", "er", "die", "H\u00f6r\u00b7ner", "auf", "sol\u00b7che", "Wei\u00b7se", "vor"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "APPR", "PIAT", "NN", "TRUNC"], "meter": "-+-+---+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "zukehren,", "tokens": ["zu\u00b7keh\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "Und vor den ungeheuren Fall auf solche Weise sich zu", "tokens": ["Und", "vor", "den", "un\u00b7ge\u00b7heu\u00b7ren", "Fall", "auf", "sol\u00b7che", "Wei\u00b7se", "sich", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "PRF", "APPR"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.22": {"text": "sch\u00fctzen,", "tokens": ["sch\u00fct\u00b7zen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Da\u00df auch die allerh\u00f6chsten F\u00e4lle die Glieder ihm nicht", "tokens": ["Da\u00df", "auch", "die", "al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "F\u00e4l\u00b7le", "die", "Glie\u00b7der", "ihm", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ART", "NN", "PPER", "PTKNEG"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "leicht versehren.", "tokens": ["leicht", "ver\u00b7seh\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.25": {"text": "Er braucht sie gegen seine J\u00e4ger, so da\u00df er sie, wenn", "tokens": ["Er", "braucht", "sie", "ge\u00b7gen", "sei\u00b7ne", "J\u00e4\u00b7ger", ",", "so", "da\u00df", "er", "sie", ",", "wenn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,", "ADV", "KOUS", "PPER", "PPER", "$,", "KOUS"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.26": {"text": "er erbost,", "tokens": ["er", "er\u00b7bost", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.27": {"text": "Und einen Raum zum Anlauf hat, gar oft von hohen", "tokens": ["Und", "ei\u00b7nen", "Raum", "zum", "An\u00b7lauf", "hat", ",", "gar", "oft", "von", "ho\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VAFIN", "$,", "ADV", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Felsen sto\u00dft.", "tokens": ["Fel\u00b7sen", "sto\u00dft", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.29": {"text": "Sein Fleisch ist e\u00dfbar, und sein Fell ist gegen K\u00e4lte", "tokens": ["Sein", "Fleisch", "ist", "e\u00df\u00b7bar", ",", "und", "sein", "Fell", "ist", "ge\u00b7gen", "K\u00e4l\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "KON", "PPOSAT", "NN", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "trefflich gut;", "tokens": ["treff\u00b7lich", "gut", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.31": {"text": "Auch dienet in der Arzeney besonders dieses Thieres Blut,", "tokens": ["Auch", "die\u00b7net", "in", "der", "Ar\u00b7ze\u00b7ney", "be\u00b7son\u00b7ders", "die\u00b7ses", "Thie\u00b7res", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ADV", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}}}}