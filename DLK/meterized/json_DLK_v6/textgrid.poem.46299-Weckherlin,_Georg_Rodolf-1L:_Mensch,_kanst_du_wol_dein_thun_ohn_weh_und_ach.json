{"textgrid.poem.46299": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mensch, kanst du wol dein thun ohn weh und ach", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mensch, kanst du wol dein thun ohn weh und ach", "tokens": ["Mensch", ",", "kanst", "du", "wol", "dein", "thun", "ohn", "weh", "und", "ach"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VMFIN", "PPER", "ADV", "PPOSAT", "VVINF", "APPR", "NN", "KON", "XY"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "erwegen? Nein. Darum so bet und wach,", "tokens": ["er\u00b7we\u00b7gen", "?", "Nein", ".", "Da\u00b7rum", "so", "bet", "und", "wach", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PTKANT", "$.", "PAV", "ADV", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "da\u00df dich nicht find der b\u00f6se geist zu schwach.", "tokens": ["da\u00df", "dich", "nicht", "find", "der", "b\u00f6\u00b7se", "geist", "zu", "schwach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "ART", "ADJA", "NN", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "H\u00fct dich, da\u00df nicht hart als stein oder erz", "tokens": ["H\u00fct", "dich", ",", "da\u00df", "nicht", "hart", "als", "stein", "o\u00b7der", "erz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KOUS", "PTKNEG", "ADJD", "KOKOM", "ADJD", "KON", "NN"], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "der feind erfind dein hofnungsloses herz", "tokens": ["der", "feind", "er\u00b7find", "dein", "hof\u00b7nungs\u00b7lo\u00b7ses", "herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und dan hernach ab deinem verlust scherz.", "tokens": ["und", "dan", "her\u00b7nach", "ab", "dei\u00b7nem", "ver\u00b7lust", "scherz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "PPOSAT", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wan recht zu thun dir allzeit fremd und and,", "tokens": ["Wan", "recht", "zu", "thun", "dir", "all\u00b7zeit", "fremd", "und", "and", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PTKZU", "VVINF", "PPER", "ADV", "ADJD", "KON", "PIS", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.2": {"text": "und wan f\u00fcr got dein bestes thun ein tand,", "tokens": ["und", "wan", "f\u00fcr", "got", "dein", "bes\u00b7tes", "thun", "ein", "tand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "NE", "PPOSAT", "ADJA", "VVINF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "so bist du, mensch, in einem b\u00f6sen stand.", "tokens": ["so", "bist", "du", ",", "mensch", ",", "in", "ei\u00b7nem", "b\u00f6\u00b7sen", "stand", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ADJD", "$,", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Begehrest du, da\u00df Christ zeuch bei dir ein,", "tokens": ["Be\u00b7geh\u00b7rest", "du", ",", "da\u00df", "Christ", "zeuch", "bei", "dir", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "so mach dein herz von allen s\u00fcnden rein,", "tokens": ["so", "mach", "dein", "herz", "von", "al\u00b7len", "s\u00fcn\u00b7den", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "so ist es ihm ein angenehmer schrein.", "tokens": ["so", "ist", "es", "ihm", "ein", "an\u00b7ge\u00b7neh\u00b7mer", "schrein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ART", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Er\u00f6fne doch got deines herzens aub", "tokens": ["Er\u00b7\u00f6f\u00b7ne", "doch", "got", "dei\u00b7nes", "her\u00b7zens", "aub"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJD", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und merk, weil ja dein leben wie ein laub,", "tokens": ["und", "merk", ",", "weil", "ja", "dein", "le\u00b7ben", "wie", "ein", "laub", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ADV", "PPOSAT", "VVINF", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "da\u00df f\u00fcr dein heil das mittel ist der glaub.", "tokens": ["da\u00df", "f\u00fcr", "dein", "heil", "das", "mit\u00b7tel", "ist", "der", "glaub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "ADJD", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Demnach von got berufen werden all", "tokens": ["Dem\u00b7nach", "von", "got", "be\u00b7ru\u00b7fen", "wer\u00b7den", "all"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "NE", "VVPP", "VAINF", "PIAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "durch seines worts weltweit erh\u00f6rten hall,", "tokens": ["durch", "sei\u00b7nes", "worts", "welt\u00b7weit", "er\u00b7h\u00f6r\u00b7ten", "hall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "so sing auch du sein lob mit lautem schall.", "tokens": ["so", "sing", "auch", "du", "sein", "lob", "mit", "lau\u00b7tem", "schall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Bet tag und nacht, auf da\u00df er dich, zwar ohn", "tokens": ["Bet", "tag", "und", "nacht", ",", "auf", "da\u00df", "er", "dich", ",", "zwar", "ohn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN", "$,", "APPR", "KOUS", "PPER", "PRF", "$,", "ADV", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "verdienst, aus gnad mach weis wie schnee und rohn,", "tokens": ["ver\u00b7dienst", ",", "aus", "gnad", "mach", "weis", "wie", "schnee", "und", "rohn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NN", "VVFIN", "PTKVZ", "KOKOM", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und endlich bring mit wohn f\u00fcr seinen thron.", "tokens": ["und", "end\u00b7lich", "bring", "mit", "wohn", "f\u00fcr", "sei\u00b7nen", "thron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}