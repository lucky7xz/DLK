{"textgrid.poem.37340": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Stoffel wankte frohbewegt", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Stoffel wankte frohbewegt", "tokens": ["Der", "Stof\u00b7fel", "wank\u00b7te", "froh\u00b7be\u00b7wegt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sp\u00e4t in der Nacht nach Haus.", "tokens": ["Sp\u00e4t", "in", "der", "Nacht", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da ging, wie das zu kommen pflegt,", "tokens": ["Da", "ging", ",", "wie", "das", "zu", "kom\u00b7men", "pflegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "PDS", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm seine Pfeife aus.", "tokens": ["Ihm", "sei\u00b7ne", "Pfei\u00b7fe", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wer raucht, der raucht nicht gerne kalt.", "tokens": ["Wer", "raucht", ",", "der", "raucht", "nicht", "ger\u00b7ne", "kalt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PRELS", "VVFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie freut sich Stoffel da,", "tokens": ["Wie", "freut", "sich", "Stof\u00b7fel", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als er ganz dicht vor sich im Wald", "tokens": ["Als", "er", "ganz", "dicht", "vor", "sich", "im", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPR", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Kohlenfeuer sah.", "tokens": ["Ein", "Koh\u00b7len\u00b7feu\u00b7er", "sah", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Kohlen gl\u00fchn in einem Topf.", "tokens": ["Die", "Koh\u00b7len", "gl\u00fchn", "in", "ei\u00b7nem", "Topf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der frohe Stoffel dr\u00fcckt", "tokens": ["Der", "fro\u00b7he", "Stof\u00b7fel", "dr\u00fcckt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gleich eine in den Pfeifenkopf", "tokens": ["Gleich", "ei\u00b7ne", "in", "den", "Pfei\u00b7fen\u00b7kopf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und zieht als wie verr\u00fcckt.", "tokens": ["Und", "zieht", "als", "wie", "ver\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "KOKOM", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wohl sieht er, wie die Kohle gl\u00fcht,", "tokens": ["Wohl", "sieht", "er", ",", "wie", "die", "Koh\u00b7le", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur da\u00df sie gar nicht brennt.", "tokens": ["Nur", "da\u00df", "sie", "gar", "nicht", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da \u00fcberl\u00e4uft es sein Gem\u00fct,", "tokens": ["Da", "\u00fc\u00b7berl\u00b7\u00e4uft", "es", "sein", "Ge\u00b7m\u00fct", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er flucht Potzsapperment.", "tokens": ["Er", "flucht", "Potz\u00b7sap\u00b7per\u00b7ment", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das Wort war hier nicht recht am Platz.", "tokens": ["Das", "Wort", "war", "hier", "nicht", "recht", "am", "Platz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKNEG", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es folgt ein Donnerschlag.", "tokens": ["Es", "folgt", "ein", "Don\u00b7ner\u00b7schlag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Versunken ist der Zauberschatz", "tokens": ["Ver\u00b7sun\u00b7ken", "ist", "der", "Zau\u00b7ber\u00b7schatz"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis an den j\u00fcngsten Tag.", "tokens": ["Bis", "an", "den", "j\u00fcng\u00b7sten", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Pfeife f\u00e4llt vor Schreck und Graus", "tokens": ["Die", "Pfei\u00b7fe", "f\u00e4llt", "vor", "Schreck", "und", "Graus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf einen harten Stein.", "tokens": ["Auf", "ei\u00b7nen", "har\u00b7ten", "Stein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Golddukaten rollt heraus,", "tokens": ["Ein", "Gold\u00b7du\u00b7ka\u00b7ten", "rollt", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Blitzblank im Mondenschein.", "tokens": ["Blitz\u00b7blank", "im", "Mon\u00b7den\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "Von nun an, denkt der Stoffel schlau,", "tokens": ["Von", "nun", "an", ",", "denkt", "der", "Stof\u00b7fel", "schlau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PTKVZ", "$,", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schweig ich am rechten Ort.", "tokens": ["Schweig", "ich", "am", "rech\u00b7ten", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er kehrte heim zu seiner Frau", "tokens": ["Er", "kehr\u00b7te", "heim", "zu", "sei\u00b7ner", "Frau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sprach kein einzig Wort.", "tokens": ["Und", "sprach", "kein", "ein\u00b7zig", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}