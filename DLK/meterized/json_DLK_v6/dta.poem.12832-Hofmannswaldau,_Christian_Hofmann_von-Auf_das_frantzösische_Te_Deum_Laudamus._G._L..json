{"dta.poem.12832": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf das frantz\u00f6sische  Te Deum  \n Laudamus.  \n G. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Frantzen stimmet offt das Te Laudamus an,", "tokens": ["Ihr", "Frant\u00b7zen", "stim\u00b7met", "offt", "das", "Te", "Lau\u00b7da\u00b7mus", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da in den hertzen selbst ein ", "tokens": ["Da", "in", "den", "hert\u00b7zen", "selbst", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "ART"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Es ist kein wunder nicht, da\u00df ein verzagter hahn,", "tokens": ["Es", "ist", "kein", "wun\u00b7der", "nicht", ",", "da\u00df", "ein", "ver\u00b7zag\u00b7ter", "hahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "PTKNEG", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn er ein wetter merckt am allerhellsten singet.", "tokens": ["Wenn", "er", "ein", "wet\u00b7ter", "merckt", "am", "al\u00b7ler\u00b7hells\u00b7ten", "sin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}