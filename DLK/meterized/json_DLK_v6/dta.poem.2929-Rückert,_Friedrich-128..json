{"dta.poem.2929": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "128.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich denke, da\u00df auch dich zu Zeiten noch verwirret,", "tokens": ["Ich", "den\u00b7ke", ",", "da\u00df", "auch", "dich", "zu", "Zei\u00b7ten", "noch", "ver\u00b7wir\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADV", "PPER", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was in der Jugend mich so manichfach geirret;", "tokens": ["Was", "in", "der", "Ju\u00b7gend", "mich", "so", "ma\u00b7nich\u00b7fach", "ge\u00b7ir\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn den Ausspr\u00fcchen ich der Weisen aller Zeiten", "tokens": ["Wenn", "den", "Aus\u00b7spr\u00fc\u00b7chen", "ich", "der", "Wei\u00b7sen", "al\u00b7ler", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gieng gl\u00e4ubig nach und mich von ihnen gern lie\u00df leiten;", "tokens": ["Gieng", "gl\u00e4u\u00b7big", "nach", "und", "mich", "von", "ih\u00b7nen", "gern", "lie\u00df", "lei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "KON", "PRF", "APPR", "PPER", "ADV", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Da stellt' ich jeden mir als einen Leitstern vor,", "tokens": ["Da", "stellt'", "ich", "je\u00b7den", "mir", "als", "ei\u00b7nen", "Leits\u00b7tern", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "PPER", "KOUS", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und jede Perle nahm ich freudig in mein Ohr.", "tokens": ["Und", "je\u00b7de", "Per\u00b7le", "nahm", "ich", "freu\u00b7dig", "in", "mein", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wenn meine Spr\u00fcche nun, die goldnen, ich verglich,", "tokens": ["Wenn", "mei\u00b7ne", "Spr\u00fc\u00b7che", "nun", ",", "die", "gold\u00b7nen", ",", "ich", "ver\u00b7glich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "$,", "ART", "ADJA", "$,", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Staunen nahm ich wahr: sie widersprachen sich.", "tokens": ["Mit", "Stau\u00b7nen", "nahm", "ich", "wahr", ":", "sie", "wi\u00b7der\u00b7spra\u00b7chen", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und weil ich konnte nun nicht alle mehr zusammen", "tokens": ["Und", "weil", "ich", "konn\u00b7te", "nun", "nicht", "al\u00b7le", "mehr", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VMFIN", "ADV", "PTKNEG", "PIS", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Annehmen, hatt' ich Lust sie alle zu verdammen.", "tokens": ["An\u00b7neh\u00b7men", ",", "hatt'", "ich", "Lust", "sie", "al\u00b7le", "zu", "ver\u00b7dam\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "NN", "PPER", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Denn welchen h\u00e4tt' ich Recht dem andern vorzuziehn,", "tokens": ["Denn", "wel\u00b7chen", "h\u00e4tt'", "ich", "Recht", "dem", "an\u00b7dern", "vor\u00b7zu\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "VAFIN", "PPER", "NN", "ART", "ADJA", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da mir an seinem Platz jeder der rechte schien?", "tokens": ["Da", "mir", "an", "sei\u00b7nem", "Platz", "je\u00b7der", "der", "rech\u00b7te", "schien", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "PIS", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}}, "stanza.7": {"line.1": {"text": "Bis mir die Einsicht kam, da\u00df alle Weisheit bringt", "tokens": ["Bis", "mir", "die", "Ein\u00b7sicht", "kam", ",", "da\u00df", "al\u00b7le", "Weis\u00b7heit", "bringt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "$,", "KOUS", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bedingte Wahrheit nur, nicht Wahrheit unbedingt;", "tokens": ["Be\u00b7ding\u00b7te", "Wahr\u00b7heit", "nur", ",", "nicht", "Wahr\u00b7heit", "un\u00b7be\u00b7dingt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$,", "PTKNEG", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Da\u00df alles, was ist wahr in eigener Verbindung,", "tokens": ["Da\u00df", "al\u00b7les", ",", "was", "ist", "wahr", "in", "ei\u00b7ge\u00b7ner", "Ver\u00b7bin\u00b7dung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PWS", "VAFIN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und wie hervor es gieng aus eigener Empfindung,", "tokens": ["Und", "wie", "her\u00b7vor", "es", "gieng", "aus", "ei\u00b7ge\u00b7ner", "Emp\u00b7fin\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PTKVZ", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}}, "stanza.9": {"line.1": {"text": "Falsch wird, sobald man der Verbindung es entzieht,", "tokens": ["Falsch", "wird", ",", "so\u00b7bald", "man", "der", "Ver\u00b7bin\u00b7dung", "es", "ent\u00b7zieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "KOUS", "PIS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mit ver\u00e4nderter Empfindung es besieht.", "tokens": ["Und", "mit", "ver\u00b7\u00e4n\u00b7der\u00b7ter", "Emp\u00b7fin\u00b7dung", "es", "be\u00b7sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Seitdem lie\u00df ich gestellt, und so magst dus auch lassen,", "tokens": ["Seit\u00b7dem", "lie\u00df", "ich", "ge\u00b7stellt", ",", "und", "so", "magst", "dus", "auch", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "VVPP", "$,", "KON", "ADV", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Jedes an seinem Ort, und sah ein jedes passen,", "tokens": ["Je\u00b7des", "an", "sei\u00b7nem", "Ort", ",", "und", "sah", "ein", "je\u00b7des", "pas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Dankbar den Weisen all' f\u00fcr ihre Weisheitsspendung,", "tokens": ["Dank\u00b7bar", "den", "Wei\u00b7sen", "all'", "f\u00fcr", "ih\u00b7re", "Weis\u00b7heits\u00b7spen\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PIS", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und vorbehaltend mir die eigne Nutzanwendung.", "tokens": ["Und", "vor\u00b7be\u00b7hal\u00b7tend", "mir", "die", "eig\u00b7ne", "Nut\u00b7zan\u00b7wen\u00b7dung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Ich r\u00e4ume gleiches Recht dir ein auf dieses Buch;", "tokens": ["Ich", "r\u00e4u\u00b7me", "glei\u00b7ches", "Recht", "dir", "ein", "auf", "die\u00b7ses", "Buch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "PPER", "ART", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So widerspricht sich nicht der Spr\u00fcche Widerspruch.", "tokens": ["So", "wi\u00b7der\u00b7spricht", "sich", "nicht", "der", "Spr\u00fc\u00b7che", "Wi\u00b7der\u00b7spruch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}