{"textgrid.poem.62575": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Fall unserer ersten Eltern", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O harter S\u00fcnden-Fall! du Ursprung unsrer Noth!", "tokens": ["O", "har\u00b7ter", "S\u00fcn\u00b7den\u00b7Fall", "!", "du", "Ur\u00b7sprung", "uns\u00b7rer", "Noth", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "PPER", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ach! unsre Sch\u00f6nheit ist durch diesen Fall verschwunden,", "tokens": ["Ach", "!", "uns\u00b7re", "Sch\u00f6n\u00b7heit", "ist", "durch", "die\u00b7sen", "Fall", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPOSAT", "NN", "VAFIN", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hingegen haben sich nur Flecken eingefunden.", "tokens": ["Hin\u00b7ge\u00b7gen", "ha\u00b7ben", "sich", "nur", "Fle\u00b7cken", "ein\u00b7ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "ADV", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Nun riechen wir vor Gott als schn\u00f6der S\u00fcnden-Koth.", "tokens": ["Nun", "rie\u00b7chen", "wir", "vor", "Gott", "als", "schn\u00f6\u00b7der", "S\u00fcn\u00b7den\u00b7Koth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch unsers Heylands Blut und Leiden", "tokens": ["Doch", "un\u00b7sers", "Hey\u00b7lands", "Blut", "und", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Will uns mit neuem Purpur kleiden;", "tokens": ["Will", "uns", "mit", "neu\u00b7em", "Pur\u00b7pur", "klei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.7": {"text": "So heilget Jesus Tod", "tokens": ["So", "heil\u00b7get", "Je\u00b7sus", "Tod"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Den harten S\u00fcnden-Fall, den Ursprung unsrer Noth.", "tokens": ["Den", "har\u00b7ten", "S\u00fcn\u00b7den\u00b7Fall", ",", "den", "Ur\u00b7sprung", "uns\u00b7rer", "Noth", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}