{"dta.poem.13841": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die ernsthafte Fastnacht  1814.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ja der Herr leucht't selbst zum Tanze,", "tokens": ["Ja", "der", "Herr", "leucht't", "selbst", "zum", "Tan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Frisch denn, Kameraden mein!", "tokens": ["Frisch", "denn", ",", "Ka\u00b7me\u00b7ra\u00b7den", "mein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KON", "$,", "NN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Funkelnd sch\u00f6n im Mondesglanze", "tokens": ["Fun\u00b7kelnd", "sch\u00f6n", "im", "Mon\u00b7des\u00b7glan\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Strenges Lieb, mu\u00dft unser sein! \u2014", "tokens": ["Stren\u00b7ges", "Lieb", ",", "mu\u00dft", "un\u00b7ser", "sein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "VMFIN", "PPOSAT", "VAINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}