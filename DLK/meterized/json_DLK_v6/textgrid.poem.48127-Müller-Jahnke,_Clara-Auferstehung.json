{"textgrid.poem.48127": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Auferstehung", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch die prunkenden Hollen der Dome klingt", "tokens": ["Durch", "die", "prun\u00b7ken\u00b7den", "Hol\u00b7len", "der", "Do\u00b7me", "klingt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "ein rauschendes Festgel\u00e4ut \u2013", "tokens": ["ein", "rau\u00b7schen\u00b7des", "Fest\u00b7ge\u00b7l\u00e4ut", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "und ein Chor von jauchzenden Stimmen singt", "tokens": ["und", "ein", "Chor", "von", "jauch\u00b7zen\u00b7den", "Stim\u00b7men", "singt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "dem Auferstandenen heut.", "tokens": ["dem", "Auf\u00b7er\u00b7stan\u00b7de\u00b7nen", "heut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Der Ostern lieblicher Veilchenduft", "tokens": ["Der", "Os\u00b7tern", "lieb\u00b7li\u00b7cher", "Veil\u00b7chen\u00b7duft"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "verk\u00fcndet des Sch\u00f6pfers Lob,", "tokens": ["ver\u00b7k\u00fcn\u00b7det", "des", "Sch\u00f6p\u00b7fers", "Lob", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "der von des Erl\u00f6sers Grabeskluft", "tokens": ["der", "von", "des", "Er\u00b7l\u00f6\u00b7sers", "Gra\u00b7bes\u00b7kluft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "den steinernen Riegel hob.", "tokens": ["den", "stei\u00b7ner\u00b7nen", "Rie\u00b7gel", "hob", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Zeit der Wunder ist lang vorbei;", "tokens": ["Die", "Zeit", "der", "Wun\u00b7der", "ist", "lang", "vor\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "heut dringt aus eisiger Nacht", "tokens": ["heut", "dringt", "aus", "ei\u00b7si\u00b7ger", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "kein zitternder Erl\u00f6sungsschrei", "tokens": ["kein", "zit\u00b7tern\u00b7der", "Er\u00b7l\u00f6\u00b7sungs\u00b7schrei"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "empor zu des Himmels Pracht.", "tokens": ["em\u00b7por", "zu", "des", "Him\u00b7mels", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Heut gilt als ehernes Gebot,", "tokens": ["Heut", "gilt", "als", "e\u00b7her\u00b7nes", "Ge\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "was, einst ein blutiger Spott,", "tokens": ["was", ",", "einst", "ein", "blu\u00b7ti\u00b7ger", "Spott", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "die Schwachen verh\u00f6hnt und der Aermsten Not:", "tokens": ["die", "Schwa\u00b7chen", "ver\u00b7h\u00f6hnt", "und", "der", "A\u00b7erms\u00b7ten", "Not", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "hilf dir selbst, so hilft dir Gott!", "tokens": ["hilf", "dir", "selbst", ",", "so", "hilft", "dir", "Gott", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hilf dir selbst, mein Volk, das in Ketten schlief,", "tokens": ["Hilf", "dir", "selbst", ",", "mein", "Volk", ",", "das", "in", "Ket\u00b7ten", "schlief", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "PPOSAT", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "schau dich um: die Welt ist dein!", "tokens": ["schau", "dich", "um", ":", "die", "Welt", "ist", "dein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "ART", "NN", "VAFIN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie sargten unter die Felsen tief", "tokens": ["Sie", "sarg\u00b7ten", "un\u00b7ter", "die", "Fel\u00b7sen", "tief"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "deine heilige Freiheit ein.", "tokens": ["dei\u00b7ne", "hei\u00b7li\u00b7ge", "Frei\u00b7heit", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Heut pulst ihr Blut, und die Zeit ist um:", "tokens": ["Heut", "pulst", "ihr", "Blut", ",", "und", "die", "Zeit", "ist", "um", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "KON", "ART", "NN", "VAFIN", "APPR", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "\u00bberl\u00f6serin du lebst!\u00ab", "tokens": ["\u00bb", "er\u00b7l\u00f6\u00b7se\u00b7rin", "du", "lebst", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Zum Licht wird bl\u00fchen, was starr und stumm,", "tokens": ["Zum", "Licht", "wird", "bl\u00fc\u00b7hen", ",", "was", "starr", "und", "stumm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "VVINF", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "wenn du den Riegel hebst!", "tokens": ["wenn", "du", "den", "Rie\u00b7gel", "hebst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Schau dich um, mein Volk, im deutschen Land", "tokens": ["Schau", "dich", "um", ",", "mein", "Volk", ",", "im", "deut\u00b7schen", "Land"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "flackert der Fr\u00fchrotschein \u2013", "tokens": ["fla\u00b7ckert", "der", "Fr\u00fch\u00b7rot\u00b7schein", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "der selige Traum vom Wunder schwand,", "tokens": ["der", "se\u00b7li\u00b7ge", "Traum", "vom", "Wun\u00b7der", "schwand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "doch Wille und Kraft sind dein!", "tokens": ["doch", "Wil\u00b7le", "und", "Kraft", "sind", "dein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VAFIN", "PPOSAT", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der Wahltag werde dein Ostertag \u2013", "tokens": ["Der", "Wahl\u00b7tag", "wer\u00b7de", "dein", "Os\u00b7ter\u00b7tag", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "und du machst deinen Feind zum Spott,", "tokens": ["und", "du", "machst", "dei\u00b7nen", "Feind", "zum", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "und es dr\u00f6hnt dein wuchtiger Hammerschlag:", "tokens": ["und", "es", "dr\u00f6hnt", "dein", "wuch\u00b7ti\u00b7ger", "Ham\u00b7mer\u00b7schlag", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "hilf dir selbst, so hilft dir Gott!", "tokens": ["hilf", "dir", "selbst", ",", "so", "hilft", "dir", "Gott", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}