{"textgrid.poem.42223": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Waldweben", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwischen duftigen B\u00fcschen", "tokens": ["Zwi\u00b7schen", "duf\u00b7ti\u00b7gen", "B\u00fc\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Stie\u00df ich auf einen Quell;", "tokens": ["Stie\u00df", "ich", "auf", "ei\u00b7nen", "Quell", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Meinen Mund zu erfrischen,", "tokens": ["Mei\u00b7nen", "Mund", "zu", "er\u00b7fri\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "D\u00fcnkt er mich rein und hell.", "tokens": ["D\u00fcnkt", "er", "mich", "rein", "und", "hell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "KON", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Als ich mich satt getrunken,", "tokens": ["Als", "ich", "mich", "satt", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Tr\u00e4umend wankt ich zur Stadt,", "tokens": ["Tr\u00e4u\u00b7mend", "wankt", "ich", "zur", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Bin aufs Lager gesunken,", "tokens": ["Bin", "aufs", "La\u00b7ger", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Fiebernd und todesmatt.", "tokens": ["Fie\u00b7bernd", "und", "to\u00b7des\u00b7matt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hat kein Arzt sich gefunden,", "tokens": ["Hat", "kein", "Arzt", "sich", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "PRF", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Dessen Kunst mich geheilt;", "tokens": ["Des\u00b7sen", "Kunst", "mich", "ge\u00b7heilt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPER", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Werd auch nimmer gesunden,", "tokens": ["Werd", "auch", "nim\u00b7mer", "ge\u00b7sun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Bis mich der Tod ereilt. \u2013", "tokens": ["Bis", "mich", "der", "Tod", "er\u00b7eilt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ei du mein durstiger Knabe,", "tokens": ["Ei", "du", "mein", "durs\u00b7ti\u00b7ger", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Streife nicht durchs Geb\u00fcsch;", "tokens": ["Strei\u00b7fe", "nicht", "durchs", "Ge\u00b7b\u00fcsch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Bleib bei der Mutter und labe", "tokens": ["Bleib", "bei", "der", "Mut\u00b7ter", "und", "la\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "KON", "VVFIN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Fromm dich am Kaffeetisch.", "tokens": ["Fromm", "dich", "am", "Kaf\u00b7fee\u00b7tisch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}