{"dta.poem.4337": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Zum Winter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da auf dem strengen Elbe-Strohm ich nicht, wie", "tokens": ["Da", "auf", "dem", "stren\u00b7gen", "El\u00b7be\u00b7Strohm", "ich", "nicht", ",", "wie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "PPER", "PTKNEG", "$,", "PWAV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "vor, sein sch\u00f6nes Blau,", "tokens": ["vor", ",", "sein", "sch\u00f6\u00b7nes", "Blau", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Nein, von der itzo dunklen Luft gef\u00e4rbt, sein recht verd\u00fc-", "tokens": ["Nein", ",", "von", "der", "it\u00b7zo", "dunk\u00b7len", "Luft", "ge\u00b7f\u00e4rbt", ",", "sein", "recht", "ver\u00b7d\u00fc"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "ART", "ADV", "ADJA", "NN", "VVPP", "$,", "PPOSAT", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "stert Grau,", "tokens": ["stert", "Grau", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "So weit mein scharfes Auge tr\u00e4get,", "tokens": ["So", "weit", "mein", "schar\u00b7fes", "Au\u00b7ge", "tr\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Schon hie und dort mit Eis beleget,", "tokens": ["Schon", "hie", "und", "dort", "mit", "Eis", "be\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Jm weissen Ufer fliessen schau,", "tokens": ["Jm", "weis\u00b7sen", "U\u00b7fer", "flies\u00b7sen", "schau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Seh ich zugleich (bedeckt, erf\u00fcllt mit Schnee und Eis)", "tokens": ["Seh", "ich", "zu\u00b7gleich", "(", "be\u00b7deckt", ",", "er\u00b7f\u00fcllt", "mit", "Schnee", "und", "Eis", ")"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$(", "VVPP", "$,", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Den Himmel schwarz, die Erde wei\u00df.", "tokens": ["Den", "Him\u00b7mel", "schwarz", ",", "die", "Er\u00b7de", "wei\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Es deckt die Welt ein weiss- ein allgemeines Licht,", "tokens": ["Es", "deckt", "die", "Welt", "ein", "weiss", "ein", "all\u00b7ge\u00b7mei\u00b7nes", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "TRUNC", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nur da\u00df es hier ein Dach, ein Baum dort, unterbricht,", "tokens": ["Nur", "da\u00df", "es", "hier", "ein", "Dach", ",", "ein", "Baum", "dort", ",", "un\u00b7ter\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "ART", "NN", "$,", "ART", "NN", "ADV", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Durch deren schwarze Dunkelheit", "tokens": ["Durch", "de\u00b7ren", "schwar\u00b7ze", "Dun\u00b7kel\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Des weissen Schnees Heiterkeit", "tokens": ["Des", "weis\u00b7sen", "Schnees", "Hei\u00b7ter\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Noch weisser wird, noch mehr erh\u00f6ht.", "tokens": ["Noch", "weis\u00b7ser", "wird", ",", "noch", "mehr", "er\u00b7h\u00f6ht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Die B\u00e4ume, sonderlich die so erhaben stehn,", "tokens": ["Die", "B\u00e4u\u00b7me", ",", "son\u00b7der\u00b7lich", "die", "so", "er\u00b7ha\u00b7ben", "stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "ART", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Indem ein luckrer Reif sie decket,", "tokens": ["In\u00b7dem", "ein", "luck\u00b7rer", "Reif", "sie", "de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Und Aest\u2019 und Zweige ganz verstecket,", "tokens": ["Und", "A\u00b7est'", "und", "Zwei\u00b7ge", "ganz", "ver\u00b7ste\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ADV", "VVFIN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.18": {"text": "Sind, gegen dunkler Luft zumahl,", "tokens": ["Sind", ",", "ge\u00b7gen", "dunk\u00b7ler", "Luft", "zu\u00b7mahl", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Wie weisse Wolken anzusehn.", "tokens": ["Wie", "weis\u00b7se", "Wol\u00b7ken", "an\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Wodurch denn hier und dort,", "tokens": ["Wo\u00b7durch", "denn", "hier", "und", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Wo ihrer viel an manchem Ort,", "tokens": ["Wo", "ih\u00b7rer", "viel", "an", "man\u00b7chem", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Aus Schwarz und Wei\u00df, ein\u2019 Art von D\u00e4mmerung ent-", "tokens": ["Aus", "Schwarz", "und", "Wei\u00df", ",", "ein'", "Art", "von", "D\u00e4m\u00b7me\u00b7rung", "ent"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN", "$,", "ART", "NN", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "stehet,", "tokens": ["ste\u00b7het", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.24": {"text": "Die nicht unangenehm,", "tokens": ["Die", "nicht", "un\u00b7an\u00b7ge\u00b7nehm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Besonders, wenn es still und wenn die Luft bequehm.", "tokens": ["Be\u00b7son\u00b7ders", ",", "wenn", "es", "still", "und", "wenn", "die", "Luft", "be\u00b7quehm", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADJD", "KON", "KOUS", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Man mu\u00df hiebey, wenn mans erwegt, gestehn,", "tokens": ["Man", "mu\u00df", "hie\u00b7bey", ",", "wenn", "mans", "er\u00b7wegt", ",", "ge\u00b7stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "$,", "KOUS", "PIS", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Da\u00df auch die Welt im Winter sch\u00f6n.", "tokens": ["Da\u00df", "auch", "die", "Welt", "im", "Win\u00b7ter", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Zumahl f\u00fcr den, dem Gott zu dieser Zeit", "tokens": ["Zu\u00b7mahl", "f\u00fcr", "den", ",", "dem", "Gott", "zu", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "$,", "ART", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Die n\u00f6htige Bequehmlichkeit,", "tokens": ["Die", "n\u00f6h\u00b7ti\u00b7ge", "Be\u00b7quehm\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Durch Feur und warme Kleider, g\u00f6nnet.", "tokens": ["Durch", "Feur", "und", "war\u00b7me", "Klei\u00b7der", ",", "g\u00f6n\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Winter-Lust, wenn man auf Schritt-Schuh rennet,", "tokens": ["Die", "Win\u00b7ter\u00b7Lust", ",", "wenn", "man", "auf", "Schrit\u00b7tSchuh", "ren\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PIS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jm Schlitten f\u00e4hrt, geh\u00f6ret auch hieher.", "tokens": ["Jm", "Schlit\u00b7ten", "f\u00e4hrt", ",", "ge\u00b7h\u00f6\u00b7ret", "auch", "hie\u00b7her", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "VVFIN", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich spreche denn aufs neu, beym w\u00e4rmenden Camin,", "tokens": ["Ich", "spre\u00b7che", "denn", "aufs", "neu", ",", "beym", "w\u00e4r\u00b7men\u00b7den", "Ca\u00b7min", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJD", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn rohte Kohlen drauf, zu unserm Labsal, gl\u00fchn:", "tokens": ["Wenn", "roh\u00b7te", "Koh\u00b7len", "drauf", ",", "zu", "un\u00b7serm", "Lab\u00b7sal", ",", "gl\u00fchn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PTKVZ", "$,", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auch selbst im Frost, geb\u00fchrt dem Sch\u00f6pfer Dank und Ehr!", "tokens": ["Auch", "selbst", "im", "Frost", ",", "ge\u00b7b\u00fchrt", "dem", "Sch\u00f6p\u00b7fer", "Dank", "und", "Ehr", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$,", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}