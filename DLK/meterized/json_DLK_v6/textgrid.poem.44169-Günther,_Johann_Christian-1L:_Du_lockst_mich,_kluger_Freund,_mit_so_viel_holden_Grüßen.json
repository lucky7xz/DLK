sure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Um nur das Volck in Furcht und sich ins Bret zu sezen,", "tokens": ["Um", "nur", "das", "Volck", "in", "Furcht", "und", "sich", "ins", "Bret", "zu", "se\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ART", "NN", "APPR", "NN", "KON", "PRF", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Auf dem die Heucheley der Pharis\u00e4er lacht.", "tokens": ["Auf", "dem", "die", "Heu\u00b7che\u00b7ley", "der", "Pha\u00b7ri\u00b7s\u00e4\u00b7er", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Dies Volck wird meistentheils die Ursach meiner Sorgen,", "tokens": ["Dies", "Volck", "wird", "meis\u00b7ten\u00b7theils", "die", "Ur\u00b7sach", "mei\u00b7ner", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Wobey mein Herz von hier mehr Meilen weiterdenckt,", "tokens": ["Wo\u00b7bey", "mein", "Herz", "von", "hier", "mehr", "Mei\u00b7len", "wei\u00b7ter\u00b7denckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Als Leinwandjuden jezt blos zum Entlaufen borgen", "tokens": ["Als", "Lein\u00b7wand\u00b7ju\u00b7den", "jezt", "blos", "zum", "Ent\u00b7lau\u00b7fen", "bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "ADV", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Und Quasten jeder Mops an seine Prozeln henckt.", "tokens": ["Und", "Quas\u00b7ten", "je\u00b7der", "Mops", "an", "sei\u00b7ne", "Pro\u00b7zeln", "henckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Doch davon m\u00fcndlich mehr, denn was ich denck und f\u00fchle,", "tokens": ["Doch", "da\u00b7von", "m\u00fcnd\u00b7lich", "mehr", ",", "denn", "was", "ich", "denck", "und", "f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADJD", "ADV", "$,", "KON", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Das l\u00e4st sich nicht gar wohl der Feder anvertraun.", "tokens": ["Das", "l\u00e4st", "sich", "nicht", "gar", "wohl", "der", "Fe\u00b7der", "an\u00b7ver\u00b7traun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "PTKNEG", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Nun dencke selber nach, ob der was Nettes spiele,", "tokens": ["Nun", "den\u00b7cke", "sel\u00b7ber", "nach", ",", "ob", "der", "was", "Net\u00b7tes", "spie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$,", "KOUS", "ART", "PWS", "NN", "VVFIN", "$,"], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.68": {"text": "Dem Noth und Neid den Weg zur stillen Ruh verhaun.", "tokens": ["Dem", "Noth", "und", "Neid", "den", "Weg", "zur", "stil\u00b7len", "Ruh", "ver\u00b7haun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ART", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Der Geist der Poesie will aufger\u00e4umte Sinnen;", "tokens": ["Der", "Geist", "der", "Poe\u00b7sie", "will", "auf\u00b7ge\u00b7r\u00e4um\u00b7te", "Sin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VMFIN", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.70": {"text": "Ja, warthe, bis mein Fu\u00df den Linden n\u00e4her tritt,", "tokens": ["Ja", ",", "wart\u00b7he", ",", "bis", "mein", "Fu\u00df", "den", "Lin\u00b7den", "n\u00e4\u00b7her", "tritt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "ART", "NE", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "Da soll mir neue Kraft in diese Feder rinnen,", "tokens": ["Da", "soll", "mir", "neu\u00b7e", "Kraft", "in", "die\u00b7se", "Fe\u00b7der", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJA", "NN", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Die mir Calliope schon in der Wiege schnidt.", "tokens": ["Die", "mir", "Cal\u00b7li\u00b7o\u00b7pe", "schon", "in", "der", "Wie\u00b7ge", "schnidt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NE", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.73": {"text": "Dort soll der Rosenthal von meiner Fl\u00f6the klingen,", "tokens": ["Dort", "soll", "der", "Ro\u00b7sen\u00b7thal", "von", "mei\u00b7ner", "Fl\u00f6\u00b7the", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.74": {"text": "Die Nymphen werden selbst am Plei\u00dfenstrande stehn", "tokens": ["Die", "Nym\u00b7phen", "wer\u00b7den", "selbst", "am", "Plei\u00b7\u00dfen\u00b7stran\u00b7de", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.75": {"text": "Und mein vorhin alldort gar wohl bekandtes Singen", "tokens": ["Und", "mein", "vor\u00b7hin", "all\u00b7dort", "gar", "wohl", "be\u00b7kand\u00b7tes", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADV", "ADV", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.76": {"text": "Mit Blumen, Klee und Gra\u00df und Mund und Ku\u00df erh\u00f6hn.", "tokens": ["Mit", "Blu\u00b7men", ",", "Klee", "und", "Gra\u00df", "und", "Mund", "und", "Ku\u00df", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.77": {"text": "Von dort aus will ich dir mit s\u00fc\u00dfen Liedern dienen,", "tokens": ["Von", "dort", "aus", "will", "ich", "dir", "mit", "s\u00fc\u00b7\u00dfen", "Lie\u00b7dern", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APZR", "VMFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.78": {"text": "Wofern nur Gl\u00fcck und Gott den Vorsaz nicht verr\u00fcckt;", "tokens": ["Wo\u00b7fern", "nur", "Gl\u00fcck", "und", "Gott", "den", "Vor\u00b7saz", "nicht", "ver\u00b7r\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "KON", "NN", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.79": {"text": "Dort soll mein Lorbeerblat im deutschen Pindus gr\u00fcnen,", "tokens": ["Dort", "soll", "mein", "Lor\u00b7beer\u00b7blat", "im", "deut\u00b7schen", "Pin\u00b7dus", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.80": {"text": "Der ganz Europa fast mit seinen S\u00f6hnen schm\u00fcckt.", "tokens": ["Der", "ganz", "Eu\u00b7ro\u00b7pa", "fast", "mit", "sei\u00b7nen", "S\u00f6h\u00b7nen", "schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NE", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.81": {"text": "Jezt schwiz ich Tag und Nacht bey tollen Modereimen,", "tokens": ["Jezt", "schwiz", "ich", "Tag", "und", "Nacht", "bey", "tol\u00b7len", "Mo\u00b7de\u00b7rei\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.82": {"text": "Die guter Freunde Wort von meiner Hand erzwingt,", "tokens": ["Die", "gu\u00b7ter", "Freun\u00b7de", "Wort", "von", "mei\u00b7ner", "Hand", "er\u00b7zwingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.83": {"text": "Da mu\u00df mein Dichtergaul bisweilen kraftlos sch\u00e4umen,", "tokens": ["Da", "mu\u00df", "mein", "Dich\u00b7ter\u00b7gaul", "bis\u00b7wei\u00b7len", "kraft\u00b7los", "sch\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.84": {"text": "Zumahl wenn ihn der Wein ins Galoppiren bringt.", "tokens": ["Zu\u00b7mahl", "wenn", "ihn", "der", "Wein", "ins", "Ga\u00b7lop\u00b7pi\u00b7ren", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.85": {"text": "Nun aber hat er bald die Unruh \u00fcberstanden,", "tokens": ["Nun", "a\u00b7ber", "hat", "er", "bald", "die", "Un\u00b7ruh", "\u00fc\u00b7bers\u00b7tan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.86": {"text": "Bald wird mich Schlesien nur auf dem R\u00fccken sehn;", "tokens": ["Bald", "wird", "mich", "Schle\u00b7si\u00b7en", "nur", "auf", "dem", "R\u00fc\u00b7cken", "sehn", ";"], "token_info": ["word", "word", "word", "word", "wor