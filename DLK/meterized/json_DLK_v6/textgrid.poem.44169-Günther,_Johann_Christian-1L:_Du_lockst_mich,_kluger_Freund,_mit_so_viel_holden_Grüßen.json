{"textgrid.poem.44169": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du lockst mich, kluger Freund, mit so viel holden Gr\u00fc\u00dfen", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du lockst mich, kluger Freund, mit so viel holden Gr\u00fc\u00dfen", "tokens": ["Du", "lockst", "mich", ",", "klu\u00b7ger", "Freund", ",", "mit", "so", "viel", "hol\u00b7den", "Gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "APPR", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sehnst dich, wie man sagt, nach unserm Wiedersehn.", "tokens": ["Und", "sehnst", "dich", ",", "wie", "man", "sagt", ",", "nach", "un\u00b7serm", "Wie\u00b7der\u00b7sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "PIS", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich selber w\u00fcntsche mir, die Freude zu genie\u00dfen,", "tokens": ["Ich", "sel\u00b7ber", "w\u00fcnt\u00b7sche", "mir", ",", "die", "Freu\u00b7de", "zu", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bevor mein Aug und Fu\u00df sich aus dem Lande drehn.", "tokens": ["Be\u00b7vor", "mein", "Aug", "und", "Fu\u00df", "sich", "aus", "dem", "Lan\u00b7de", "drehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn glaube, da\u00df es mich von Herzensgrund entz\u00fccke,", "tokens": ["Denn", "glau\u00b7be", ",", "da\u00df", "es", "mich", "von", "Her\u00b7zens\u00b7grund", "ent\u00b7z\u00fc\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df Gl\u00fcck und Zufall mich in deine Gunst gebracht", "tokens": ["Da\u00df", "Gl\u00fcck", "und", "Zu\u00b7fall", "mich", "in", "dei\u00b7ne", "Gunst", "ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und da\u00df ich dir bereits mehr Sehnsuchtsseufzer schicke,", "tokens": ["Und", "da\u00df", "ich", "dir", "be\u00b7reits", "mehr", "Sehn\u00b7suchts\u00b7seuf\u00b7zer", "schi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als du und ich Taback zu Asch und Staub gemacht.", "tokens": ["Als", "du", "und", "ich", "Ta\u00b7back", "zu", "Asch", "und", "Staub", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KON", "PPER", "NN", "APPR", "NE", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich bin zwar schlecht, doch deutsch, das ist von treuem Herzen", "tokens": ["Ich", "bin", "zwar", "schlecht", ",", "doch", "deutsch", ",", "das", "ist", "von", "treu\u00b7em", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "PDS", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und lieb und suche stets Gem\u00fcther gleicher Art,", "tokens": ["Und", "lieb", "und", "su\u00b7che", "stets", "Ge\u00b7m\u00fc\u00b7ther", "glei\u00b7cher", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "VVFIN", "ADV", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die ehrlich, aufgeweckt und sonder Argwohn scherzen", "tokens": ["Die", "ehr\u00b7lich", ",", "auf\u00b7ge\u00b7weckt", "und", "son\u00b7der", "Arg\u00b7wohn", "scher\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "$,", "VVPP", "KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und derer Mund das Glas nicht in die Winckel spart.", "tokens": ["Und", "de\u00b7rer", "Mund", "das", "Glas", "nicht", "in", "die", "Win\u00b7ckel", "spart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "ART", "NN", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Drum, wo ich solche seh und meines gleichen finde,", "tokens": ["Drum", ",", "wo", "ich", "sol\u00b7che", "seh", "und", "mei\u00b7nes", "glei\u00b7chen", "fin\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PWAV", "PPER", "PIS", "VVFIN", "KON", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da paar ich mich so bald als unsre ganze Stadt,", "tokens": ["Da", "paar", "ich", "mich", "so", "bald", "als", "uns\u00b7re", "gan\u00b7ze", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "PPER", "PRF", "ADV", "ADV", "KOKOM", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "In welcher Herr und Frau mitsamt dem Hausgesinde", "tokens": ["In", "wel\u00b7cher", "Herr", "und", "Frau", "mit\u00b7samt", "dem", "Haus\u00b7ge\u00b7sin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "KON", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Den Spruch: Nicht gut allein, zur Lebensregul hat.", "tokens": ["Den", "Spruch", ":", "Nicht", "gut", "al\u00b7lein", ",", "zur", "Le\u00b7bens\u00b7re\u00b7gul", "hat", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PTKNEG", "ADJD", "ADV", "$,", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+----", "measure": "unknown.measure.tetra"}, "line.17": {"text": "Ich kenne dich ja schon an Neigung und Gem\u00fcthe", "tokens": ["Ich", "ken\u00b7ne", "dich", "ja", "schon", "an", "Nei\u00b7gung", "und", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und glaube, fehl ich nicht aus allzu gro\u00dfer Treu,", "tokens": ["Und", "glau\u00b7be", ",", "fehl", "ich", "nicht", "aus", "all\u00b7zu", "gro\u00b7\u00dfer", "Treu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "APPR", "PTKA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da\u00df die mit H\u00f6fligkeit mir nechst erwiesne G\u00fcte", "tokens": ["Da\u00df", "die", "mit", "H\u00f6f\u00b7lig\u00b7keit", "mir", "nechst", "er\u00b7wies\u00b7ne", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "APPR", "NN", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ein fest gelegter Grund zu unsrer Freundschaft sey.", "tokens": ["Ein", "fest", "ge\u00b7leg\u00b7ter", "Grund", "zu", "uns\u00b7rer", "Freund\u00b7schaft", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wofern ich dort zu frey und etwas grob gewesen,", "tokens": ["Wo\u00b7fern", "ich", "dort", "zu", "frey", "und", "et\u00b7was", "grob", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKA", "ADJD", "KON", "ADV", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So schreib es deinem Glas und meiner Schwachheit zu", "tokens": ["So", "schreib", "es", "dei\u00b7nem", "Glas", "und", "mei\u00b7ner", "Schwach\u00b7heit", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "PTKZU"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und las mich k\u00fcnftighin aus deinen Augen lesen,", "tokens": ["Und", "las", "mich", "k\u00fcnf\u00b7tig\u00b7hin", "aus", "dei\u00b7nen", "Au\u00b7gen", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da\u00df kein berauscht Vergehn der Freundschaft Einhalt thu.", "tokens": ["Da\u00df", "kein", "be\u00b7rauscht", "Ver\u00b7gehn", "der", "Freund\u00b7schaft", "Ein\u00b7halt", "thu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJD", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Du forderst \u00fcbrigens von meinem Dichterkrame,", "tokens": ["Du", "for\u00b7derst", "\u00fcb\u00b7ri\u00b7gens", "von", "mei\u00b7nem", "Dich\u00b7ter\u00b7kra\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ach, aber fordre nichts, es d\u00fcrfte dich gereun,", "tokens": ["Ach", ",", "a\u00b7ber", "ford\u00b7re", "nichts", ",", "es", "d\u00fcrf\u00b7te", "dich", "ge\u00b7reun", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PIS", "$,", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und der in dieser Kunst mir beygelegte Nahme", "tokens": ["Und", "der", "in", "die\u00b7ser", "Kunst", "mir", "bey\u00b7ge\u00b7leg\u00b7te", "Nah\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "PDAT", "NN", "PPER", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Durch so ein schlechtes Zeug mir wenig Ruhm verleihn,", "tokens": ["Durch", "so", "ein", "schlech\u00b7tes", "Zeug", "mir", "we\u00b7nig", "Ruhm", "ver\u00b7leihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "ADJA", "NN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.29": {"text": "Voraus in dieser Zeit, da fast kein Exulante", "tokens": ["Vo\u00b7raus", "in", "die\u00b7ser", "Zeit", ",", "da", "fast", "kein", "E\u00b7xul\u00b7an\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,", "KOUS", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Dergleichen schwere Last als jezt mein Phoebus tr\u00e4gt", "tokens": ["Derg\u00b7lei\u00b7chen", "schwe\u00b7re", "Last", "als", "jezt", "mein", "Phoe\u00b7bus", "tr\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "ADJA", "NN", "KOKOM", "ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und da der Schickung Zorn, die in mein Erbtheil brannte,", "tokens": ["Und", "da", "der", "Schi\u00b7ckung", "Zorn", ",", "die", "in", "mein", "E\u00b7rbtheil", "brann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Durch Misgunst und durch Noth mich hin und wider schl\u00e4gt.", "tokens": ["Durch", "Mis\u00b7gunst", "und", "durch", "Noth", "mich", "hin", "und", "wi\u00b7der", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "PPER", "PTKVZ", "KON", "APPR", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Man hat von Alters her das Sprichwort angenommen,", "tokens": ["Man", "hat", "von", "Al\u00b7ters", "her", "das", "Sprich\u00b7wort", "an\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "NN", "APZR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Kein Ort vergn\u00fcge mehr als unser Vaterland;", "tokens": ["Kein", "Ort", "ver\u00b7gn\u00fc\u00b7ge", "mehr", "als", "un\u00b7ser", "Va\u00b7ter\u00b7land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PIS", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ich seh es warlich nicht, es m\u00fcste denn noch kommen.", "tokens": ["Ich", "seh", "es", "war\u00b7lich", "nicht", ",", "es", "m\u00fcs\u00b7te", "denn", "noch", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "PPER", "VMFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Doch was so langsam kommt, das g\u00f6nnt man nur der Wand.", "tokens": ["Doch", "was", "so", "lang\u00b7sam", "kommt", ",", "das", "g\u00f6nnt", "man", "nur", "der", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADJD", "VVFIN", "$,", "PDS", "VVFIN", "PIS", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Das sch\u00f6ne Schlesien ist meinem Musenorden", "tokens": ["Das", "sch\u00f6\u00b7ne", "Schle\u00b7si\u00b7en", "ist", "mei\u00b7nem", "Mu\u00b7se\u00b7nor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Theils durch der G\u00f6nner Tod, theils durch der Misgunst List", "tokens": ["Theils", "durch", "der", "G\u00f6n\u00b7ner", "Tod", ",", "theils", "durch", "der", "Mis\u00b7gunst", "List"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "So \u00e4ngstlich, so verhast und so zum Eckel worden,", "tokens": ["So", "\u00e4ngst\u00b7lich", ",", "so", "ver\u00b7hast", "und", "so", "zum", "E\u00b7ckel", "wor\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "ADV", "APPRART", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Als keinem Isaschar der beste Frischling ist.", "tokens": ["Als", "kei\u00b7nem", "I\u00b7sasc\u00b7har", "der", "bes\u00b7te", "Frischling", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Das Land kan nicht davor, dem w\u00fcn