{"dta.poem.3823": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcckw\u00fcnschender Brief;  \n Als ein Priester sein geistliches Hirten-Amt antrat.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Du, der Tugenden Verehrer,", "tokens": ["Du", ",", "der", "Tu\u00b7gen\u00b7den", "Ver\u00b7eh\u00b7rer", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Treu, beliebt und frommer Lehrer,", "tokens": ["Treu", ",", "be\u00b7liebt", "und", "from\u00b7mer", "Leh\u00b7rer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jtzo stirbt ein Aaron;", "tokens": ["Jt\u00b7zo", "stirbt", "ein", "Aa\u00b7ron", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Du solst seine Stell erg\u00e4ntzen,", "tokens": ["Du", "solst", "sei\u00b7ne", "Stell", "er\u00b7g\u00e4nt\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Du solst nun in Ephod gl\u00e4ntzen,", "tokens": ["Du", "solst", "nun", "in", "E\u00b7phod", "gl\u00e4nt\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Eleasars fr\u00f6mster Sohn,", "tokens": ["E\u00b7lea\u00b7sars", "fr\u00f6ms\u00b7ter", "Sohn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Drum verwalt das Hirten-Amt,", "tokens": ["Drum", "ver\u00b7walt", "das", "Hir\u00b7ten\u00b7Amt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "So von deinen V\u00e4tern stammt.", "tokens": ["So", "von", "dei\u00b7nen", "V\u00e4\u00b7tern", "stammt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Man wird dich nunmehr erwehlen,", "tokens": ["Man", "wird", "dich", "nun\u00b7mehr", "er\u00b7weh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Weide du der Deinen Seelen,", "tokens": ["Wei\u00b7de", "du", "der", "Dei\u00b7nen", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimm dich deiner Schaafe an;", "tokens": ["Nimm", "dich", "dei\u00b7ner", "Schaa\u00b7fe", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Reinige die M\u00f6rder-Gruben", "tokens": ["Rei\u00b7ni\u00b7ge", "die", "M\u00f6r\u00b7der\u00b7Gru\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Frevelhafft- und b\u00f6ser Buben,", "tokens": ["Fre\u00b7vel\u00b7hafft", "und", "b\u00f6\u00b7ser", "Bu\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da dein Nachdruck strafen kan;", "tokens": ["Da", "dein", "Nach\u00b7druck", "stra\u00b7fen", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "So wirst du sie aus den Ketten", "tokens": ["So", "wirst", "du", "sie", "aus", "den", "Ket\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "APPR", "ART", "NN"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Eitler Sclaverey erretten.", "tokens": ["Eit\u00b7ler", "Scla\u00b7ve\u00b7rey", "er\u00b7ret\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn dein Amt mit Rechte eifert,", "tokens": ["Wenn", "dein", "Amt", "mit", "Rech\u00b7te", "ei\u00b7fert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Und die Bo\u00dfheit Schm\u00e4hung geifert,", "tokens": ["Und", "die", "Bo\u00df\u00b7heit", "Schm\u00e4\u00b7hung", "gei\u00b7fert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVPP", "$,"], "meter": "--+-+---", "measure": "anapaest.init"}, "line.3": {"text": "Wenn die Rotte Korah sticht,", "tokens": ["Wenn", "die", "Rot\u00b7te", "Ko\u00b7rah", "sticht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ey so la\u00df dich nichts verhindern,", "tokens": ["Ey", "so", "la\u00df", "dich", "nichts", "ver\u00b7hin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVIMP", "PPER", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zeige solchen frechen S\u00fcndern,", "tokens": ["Zei\u00b7ge", "sol\u00b7chen", "fre\u00b7chen", "S\u00fcn\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was dein Heiland JEsus spricht:", "tokens": ["Was", "dein", "Hei\u00b7land", "Je\u00b7sus", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df ein S\u00fcnder ohne Reu", "tokens": ["Da\u00df", "ein", "S\u00fcn\u00b7der", "oh\u00b7ne", "Reu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ewiglich verworffen sey.", "tokens": ["E\u00b7wig\u00b7lich", "ver\u00b7worf\u00b7fen", "sey", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn ein frommer Noah lehret,", "tokens": ["Wenn", "ein", "from\u00b7mer", "Noah", "leh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NE", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Was zur rechten Bu\u00df geh\u00f6ret,", "tokens": ["Was", "zur", "rech\u00b7ten", "Bu\u00df", "ge\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Treibt der S\u00fcnder seinen Spott.", "tokens": ["Treibt", "der", "S\u00fcn\u00b7der", "sei\u00b7nen", "Spott", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchrt er auch das fr\u00f6mmste Leben,", "tokens": ["F\u00fchrt", "er", "auch", "das", "fr\u00f6mms\u00b7te", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Das ein Muster kan abgeben,", "tokens": ["Das", "ein", "Mus\u00b7ter", "kan", "ab\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.6": {"text": "Lacht man doch des frommen Loth;", "tokens": ["Lacht", "man", "doch", "des", "from\u00b7men", "Loth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die allerschwerste B\u00fcrde", "tokens": ["Und", "die", "al\u00b7ler\u00b7schwers\u00b7te", "B\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hat die treue Priester-W\u00fcrde.", "tokens": ["Hat", "die", "treu\u00b7e", "Pries\u00b7ter\u00b7W\u00fcr\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aber du, ", "tokens": ["A\u00b7ber", "du", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Du, o auserwehlter Priester!", "tokens": ["Du", ",", "o", "au\u00b7ser\u00b7wehl\u00b7ter", "Pries\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denckest nicht einmahl daran,", "tokens": ["Den\u00b7ckest", "nicht", "ein\u00b7mahl", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn du weist, zu deinem Lohne", "tokens": ["Denn", "du", "weist", ",", "zu", "dei\u00b7nem", "Loh\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist der Lehrer Freuden-Crone,", "tokens": ["Ist", "der", "Leh\u00b7rer", "Freu\u00b7den\u00b7Cro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und das sch\u00f6ne Canaan.", "tokens": ["Und", "das", "sch\u00f6\u00b7ne", "Ca\u00b7naan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NE", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Dorten leucht dein Glantz empor,", "tokens": ["Dor\u00b7ten", "leucht", "dein", "Glantz", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Bey der Auserwehlten Chor.", "tokens": ["Bey", "der", "Au\u00b7ser\u00b7wehl\u00b7ten", "Chor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Unterdessen deine Freude,", "tokens": ["Un\u00b7ter\u00b7des\u00b7sen", "dei\u00b7ne", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem sch\u00f6nen Priester-Kleide,", "tokens": ["In", "dem", "sch\u00f6\u00b7nen", "Pries\u00b7ter\u00b7Klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00fcsse gantz ausnehmend seyn.", "tokens": ["M\u00fcs\u00b7se", "gantz", "aus\u00b7neh\u00b7mend", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner Jahre frohes Steigen,", "tokens": ["Dei\u00b7ner", "Jah\u00b7re", "fro\u00b7hes", "Stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Musten Nestors Ziel erreichen;", "tokens": ["Mus\u00b7ten", "Nes\u00b7tors", "Ziel", "er\u00b7rei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ja du schl\u00e4fst nicht eher ein,", "tokens": ["Ja", "du", "schl\u00e4fst", "nicht", "e\u00b7her", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bis dir GOtt, nach sp\u00e4ten Zeiten,", "tokens": ["Bis", "dir", "Gott", ",", "nach", "sp\u00e4\u00b7ten", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wird ein Ararat bereiten.", "tokens": ["Wird", "ein", "Ar\u00b7a\u00b7rat", "be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}