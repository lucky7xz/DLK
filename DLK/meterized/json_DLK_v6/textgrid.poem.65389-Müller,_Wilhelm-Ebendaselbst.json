{"textgrid.poem.65389": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Ebendaselbst", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich trink' alle Morgen zehn Becher leer", "tokens": ["Ich", "trink'", "al\u00b7le", "Mor\u00b7gen", "zehn", "Be\u00b7cher", "leer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "CARD", "NN", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mit hundert Leuten und mehr und mehr.", "tokens": ["Mit", "hun\u00b7dert", "Leu\u00b7ten", "und", "mehr", "und", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "KON", "ADV", "KON", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Zehn Tage trinken wir schon vereint,", "tokens": ["Zehn", "Ta\u00b7ge", "trin\u00b7ken", "wir", "schon", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und Keiner wei\u00df, wie's der Andre meint.", "tokens": ["Und", "Kei\u00b7ner", "wei\u00df", ",", "wie's", "der", "And\u00b7re", "meint", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "KOUS", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie trinken und ziehen ein saures Gesicht,", "tokens": ["Sie", "trin\u00b7ken", "und", "zie\u00b7hen", "ein", "sau\u00b7res", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sie gucken mich an und ver\u00e4ndern es nicht.", "tokens": ["Sie", "gu\u00b7cken", "mich", "an", "und", "ver\u00b7\u00e4n\u00b7dern", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "O Wasser, ist das die Wunderkraft,", "tokens": ["O", "Was\u00b7ser", ",", "ist", "das", "die", "Wun\u00b7der\u00b7kraft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "ART", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die allen Leiden Genesung schafft?", "tokens": ["Die", "al\u00b7len", "Lei\u00b7den", "Ge\u00b7ne\u00b7sung", "schafft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Ich wollt', in dem Sprudel fl\u00f6sse Wein,", "tokens": ["Ich", "wollt'", ",", "in", "dem", "Spru\u00b7del", "fl\u00f6s\u00b7se", "Wein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und es sch\u00f6pfte die sch\u00f6nste der Nymphen ein.", "tokens": ["Und", "es", "sch\u00f6pf\u00b7te", "die", "sch\u00f6ns\u00b7te", "der", "Nym\u00b7phen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Beim ersten Becher entfl\u00f6h' der Harm,", "tokens": ["Beim", "ers\u00b7ten", "Be\u00b7cher", "ent\u00b7fl\u00f6h'", "der", "Harm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Beim zweiten w\u00e4ren wir wohl und warm.", "tokens": ["Beim", "zwei\u00b7ten", "w\u00e4\u00b7ren", "wir", "wohl", "und", "warm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VAFIN", "PPER", "ADV", "KON", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}