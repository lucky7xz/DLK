{"textgrid.poem.44285": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "An den Mond", "genre": "verse", "period": "N.A.", "pub_year": 1804, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wandle, wandle, holder Schimmer!", "tokens": ["Wand\u00b7le", ",", "wand\u00b7le", ",", "hol\u00b7der", "Schim\u00b7mer", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wandle \u00fcber Flur und Au,", "tokens": ["Wand\u00b7le", "\u00fc\u00b7ber", "Flur", "und", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gleitend, wie ein k\u00fchner Schwimmer,", "tokens": ["Glei\u00b7tend", ",", "wie", "ein", "k\u00fch\u00b7ner", "Schwim\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In des stillen Meeres Blau.", "tokens": ["In", "des", "stil\u00b7len", "Mee\u00b7res", "Blau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sanft im Silberglanze schwebest", "tokens": ["Sanft", "im", "Sil\u00b7ber\u00b7glan\u00b7ze", "schwe\u00b7best"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du so still durchs Wolkenmeer,", "tokens": ["Du", "so", "still", "durchs", "Wol\u00b7ken\u00b7meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und durch deinen Blick belebest", "tokens": ["Und", "durch", "dei\u00b7nen", "Blick", "be\u00b7le\u00b7best"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du die Gegend rings umher.", "tokens": ["Du", "die", "Ge\u00b7gend", "rings", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Manchen dr\u00fccket schwerer Kummer,", "tokens": ["Man\u00b7chen", "dr\u00fc\u00b7cket", "schwe\u00b7rer", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Manchen lastet Qual und Pein;", "tokens": ["Man\u00b7chen", "las\u00b7tet", "Qual", "und", "Pein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch du wiegst in sanften Schlummer", "tokens": ["Doch", "du", "wiegst", "in", "sanf\u00b7ten", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00f6stend ihn, voll Mitleid, ein.", "tokens": ["Tr\u00f6s\u00b7tend", "ihn", ",", "voll", "Mit\u00b7leid", ",", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADJD", "NN", "$,", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sanfter, als die hei\u00dfe Sonne,", "tokens": ["Sanf\u00b7ter", ",", "als", "die", "hei\u00b7\u00dfe", "Son\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Winkt dein Schimmer Ruh und Freud,", "tokens": ["Winkt", "dein", "Schim\u00b7mer", "Ruh", "und", "Freud", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und erf\u00fcllt mit s\u00fc\u00dfer Wonne,", "tokens": ["Und", "er\u00b7f\u00fcllt", "mit", "s\u00fc\u00b7\u00dfer", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00f6stung und Vergessenheit.", "tokens": ["Tr\u00f6s\u00b7tung", "und", "Ver\u00b7ges\u00b7sen\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "H\u00fcllst in dichtbewachsnen Lauben", "tokens": ["H\u00fcllst", "in", "dicht\u00b7be\u00b7wachs\u00b7nen", "Lau\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit der sanften Fantasie", "tokens": ["Mit", "der", "sanf\u00b7ten", "Fan\u00b7ta\u00b7sie"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ganz den Dichter; machst ihn glauben,", "tokens": ["Ganz", "den", "Dich\u00b7ter", ";", "machst", "ihn", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$.", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine Muse weiche nie.", "tokens": ["Sei\u00b7ne", "Mu\u00b7se", "wei\u00b7che", "nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und auch mich hast du begeistert,", "tokens": ["Und", "auch", "mich", "hast", "du", "be\u00b7geis\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Der ich dir dies Liedchen sang,", "tokens": ["Der", "ich", "dir", "dies", "Lied\u00b7chen", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "PDS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meiner Seele dich bemeistert,", "tokens": ["Mei\u00b7ner", "See\u00b7le", "dich", "be\u00b7meis\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da mein Lied sich aufw\u00e4rts schwang!", "tokens": ["Da", "mein", "Lied", "sich", "auf\u00b7w\u00e4rts", "schwang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}