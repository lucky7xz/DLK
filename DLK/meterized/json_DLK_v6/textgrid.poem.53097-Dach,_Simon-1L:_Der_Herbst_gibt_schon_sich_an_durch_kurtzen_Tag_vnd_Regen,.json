{"textgrid.poem.53097": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Herbst gibt schon sich an durch kurtzen Tag vnd Regen,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Herbst gibt schon sich an durch kurtzen Tag vnd Regen,", "tokens": ["Der", "Herbst", "gibt", "schon", "sich", "an", "durch", "kurt\u00b7zen", "Tag", "vnd", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PRF", "APPR", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man lieset Wein vnd Obst, das Korn ist abgemeyt,", "tokens": ["Man", "lie\u00b7set", "Wein", "vnd", "Obst", ",", "das", "Korn", "ist", "ab\u00b7ge\u00b7meyt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "KON", "NN", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Braut spricht aber, jetzt sey jhre ", "tokens": ["Die", "Braut", "spricht", "a\u00b7ber", ",", "jetzt", "sey", "jhre"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "ADV", "VAFIN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist ", "tokens": ["Ist"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Wie kl\u00e4glich thut sie blo\u00df der lieben ", "tokens": ["Wie", "kl\u00e4g\u00b7lich", "thut", "sie", "blo\u00df", "der", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein, gebt sie jhr doch hin, geht aber was beyseit,", "tokens": ["Mein", ",", "gebt", "sie", "jhr", "doch", "hin", ",", "geht", "a\u00b7ber", "was", "bey\u00b7seit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$,", "VVFIN", "PPER", "PPER", "ADV", "PTKVZ", "$,", "VVFIN", "ADV", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gebt achtung, wie sie sich der ", "tokens": ["Gebt", "ach\u00b7tung", ",", "wie", "sie", "sich", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "NN", "$,", "PWAV", "PPER", "PRF", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd seht, sie wil sie gar auch in jhr Bette legen.", "tokens": ["Vnd", "seht", ",", "sie", "wil", "sie", "gar", "auch", "in", "jhr", "Bet\u00b7te", "le\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was macht jhr, Jungfraw Braut? jhr fehlt der Sachen gantz,", "tokens": ["Was", "macht", "jhr", ",", "Jung\u00b7fraw", "Braut", "?", "jhr", "fehlt", "der", "Sa\u00b7chen", "gantz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Wer nicht der ", "tokens": ["Wer", "nicht", "der"], "token_info": ["word", "word", "word"], "pos": ["PWS", "PTKNEG", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Doch geht es vns nicht an, jhr habt die ", "tokens": ["Doch", "geht", "es", "vns", "nicht", "an", ",", "jhr", "habt", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PRF", "PTKNEG", "PTKVZ", "$,", "PPER", "VAFIN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wisst aber gleichwol, da\u00df bey ", "tokens": ["Wisst", "a\u00b7ber", "gleich\u00b7wol", ",", "da\u00df", "bey"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "$,", "KOUS", "APPR"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gebraucht euch ihrer wol, vnd h\u00fcttet euch: sie stechen.", "tokens": ["Ge\u00b7braucht", "euch", "ih\u00b7rer", "wol", ",", "vnd", "h\u00fct\u00b7tet", "euch", ":", "sie", "ste\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "ADV", "$,", "KON", "VVFIN", "PPER", "$.", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}