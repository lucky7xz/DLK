{"textgrid.poem.40899": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Abfahrt von Innsbruck", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Innstrand harrt ein Schifflein beim ersten Fr\u00fchrothschein,", "tokens": ["Am", "Inns\u00b7trand", "harrt", "ein", "Schif\u00b7flein", "beim", "ers\u00b7ten", "Fr\u00fch\u00b7roth\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da stieg, verh\u00fcllt im Mantel, der kranke Kaiser ein,", "tokens": ["Da", "stieg", ",", "ver\u00b7h\u00fcllt", "im", "Man\u00b7tel", ",", "der", "kran\u00b7ke", "Kai\u00b7ser", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "APPRART", "NN", "$,", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die treue Eichentruhe lehnt d\u00fcster neben ihm,", "tokens": ["Die", "treu\u00b7e", "Ei\u00b7chen\u00b7tru\u00b7he", "lehnt", "d\u00fcs\u00b7ter", "ne\u00b7ben", "ihm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+--+----", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Fort schie\u00dft im raschen Strome das Schiff mit Ungest\u00fcm.", "tokens": ["Fort", "schie\u00dft", "im", "ra\u00b7schen", "Stro\u00b7me", "das", "Schiff", "mit", "Un\u00b7ge\u00b7st\u00fcm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Am Strande murmelt fragend nun Innsbrucks Volk im Kreis:", "tokens": ["Am", "Stran\u00b7de", "mur\u00b7melt", "fra\u00b7gend", "nun", "Inns\u00b7brucks", "Volk", "im", "Kreis", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "ADV", "ADV", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wohin so schnell und eilig, du d\u00fcstrer Kaisergreis?", "tokens": ["Wo\u00b7hin", "so", "schnell", "und", "ei\u00b7lig", ",", "du", "d\u00fcst\u00b7rer", "Kai\u00b7ser\u00b7greis", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "KON", "ADJD", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da schien von Maxens Lippen das Wort zur\u00fcckzuwehn:", "tokens": ["Da", "schien", "von", "Ma\u00b7xens", "Lip\u00b7pen", "das", "Wort", "zu\u00b7r\u00fcck\u00b7zu\u00b7wehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NE", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Lebt wohl, lebt wohl! Nach Oestreich will ich nun sterben gehn!", "tokens": ["Lebt", "wohl", ",", "lebt", "wohl", "!", "Nach", "O\u00b7e\u00b7streich", "will", "ich", "nun", "ster\u00b7ben", "gehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "VVFIN", "ADV", "$.", "APPR", "NE", "VMFIN", "PPER", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Es lehnt am Eichensarge sein Haupt, von Sorgen schwer,", "tokens": ["Es", "lehnt", "am", "Ei\u00b7chen\u00b7sar\u00b7ge", "sein", "Haupt", ",", "von", "Sor\u00b7gen", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zum Himmel blickt er d\u00fcster und d\u00fcster rings umher:", "tokens": ["Zum", "Him\u00b7mel", "blickt", "er", "d\u00fcs\u00b7ter", "und", "d\u00fcs\u00b7ter", "rings", "um\u00b7her", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u00bbdu sch\u00f6nes Land, dich liebt' ich so treu und inniglich,", "tokens": ["\u00bb", "du", "sch\u00f6\u00b7nes", "Land", ",", "dich", "liebt'", "ich", "so", "treu", "und", "in\u00b7nig\u00b7lich", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "O w\u00fc\u00dft' ich nur, ob gl\u00fccklich mein Volk auch sei durch mich!\u00ab", "tokens": ["O", "w\u00fc\u00dft'", "ich", "nur", ",", "ob", "gl\u00fcck\u00b7lich", "mein", "Volk", "auch", "sei", "durch", "mich", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$,", "KOUS", "ADJD", "PPOSAT", "NN", "ADV", "VAFIN", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Fluth umrauscht das Schifflein, und schnell vor Maxens Blick", "tokens": ["Die", "Fluth", "um\u00b7rauscht", "das", "Schif\u00b7flein", ",", "und", "schnell", "vor", "Ma\u00b7xens", "Blick"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "KON", "ADJD", "APPR", "NE", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Fliehn Th\u00e4ler, Berg' und Fl\u00e4chen, Geh\u00f6ft' und Stadt zur\u00fcck;", "tokens": ["Fliehn", "Th\u00e4\u00b7ler", ",", "Ber\u00b7g'", "und", "Fl\u00e4\u00b7chen", ",", "Ge\u00b7h\u00f6ft'", "und", "Stadt", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wohin er blickt, sprie\u00dft Leben und Segen, Kraft und Flei\u00df,", "tokens": ["Wo\u00b7hin", "er", "blickt", ",", "sprie\u00dft", "Le\u00b7ben", "und", "Se\u00b7gen", ",", "Kraft", "und", "Flei\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wohin er horcht, klingt Freude und Jubelsang und Preis.", "tokens": ["Wo\u00b7hin", "er", "horcht", ",", "klingt", "Freu\u00b7de", "und", "Ju\u00b7bel\u00b7sang", "und", "Preis", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Auf Wiesen klirrt die Sense, in W\u00e4ldern knallt das Rohr,", "tokens": ["Auf", "Wie\u00b7sen", "klirrt", "die", "Sen\u00b7se", ",", "in", "W\u00e4l\u00b7dern", "knallt", "das", "Rohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Gewaltige H\u00e4mmer stampfen durchs Thal im Donnerchor,", "tokens": ["Ge\u00b7wal\u00b7ti\u00b7ge", "H\u00e4m\u00b7mer", "stamp\u00b7fen", "durchs", "Thal", "im", "Don\u00b7ner\u00b7chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und aus dem Schlund der Schl\u00f6te qualmt's riesig, dicht und grau,", "tokens": ["Und", "aus", "dem", "Schlund", "der", "Schl\u00f6\u00b7te", "qualmt's", "rie\u00b7sig", ",", "dicht", "und", "grau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "NE", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da schien auf schwarzen S\u00e4ulen zu ruhn des Himmels Bau.", "tokens": ["Da", "schien", "auf", "schwar\u00b7zen", "S\u00e4u\u00b7len", "zu", "ruhn", "des", "Him\u00b7mels", "Bau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Und weiterhin dann Felder, die dicht voll Saaten stehn,", "tokens": ["Und", "wei\u00b7ter\u00b7hin", "dann", "Fel\u00b7der", ",", "die", "dicht", "voll", "Saa\u00b7ten", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "NN", "$,", "PRELS", "ADJD", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und Heerden, die fr\u00f6hlich bl\u00f6kend auf gr\u00fcnen Alpen gehn,", "tokens": ["Und", "Heer\u00b7den", ",", "die", "fr\u00f6h\u00b7lich", "bl\u00f6\u00b7kend", "auf", "gr\u00fc\u00b7nen", "Al\u00b7pen", "gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "ADJD", "ADJD", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und M\u00fchlen klappernd im Thale, von Fluthen rasch getrieben,", "tokens": ["Und", "M\u00fch\u00b7len", "klap\u00b7pernd", "im", "Tha\u00b7le", ",", "von", "Flut\u00b7hen", "rasch", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "APPRART", "NN", "$,", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die, spr\u00fchend, an den R\u00e4dern als Sternenregen zerstieben.", "tokens": ["Die", ",", "spr\u00fc\u00b7hend", ",", "an", "den", "R\u00e4\u00b7dern", "als", "Ster\u00b7nen\u00b7re\u00b7gen", "zer\u00b7stie\u00b7ben", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VVPP", "$,", "APPR", "ART", "NN", "KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Und rings auf allen Stra\u00dfen lebendiges, heitres Dr\u00e4ngen!", "tokens": ["Und", "rings", "auf", "al\u00b7len", "Stra\u00b7\u00dfen", "le\u00b7ben\u00b7di\u00b7ges", ",", "heit\u00b7res", "Dr\u00e4n\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Da st\u00e4ubt's von flinken Reitern, die rasch zum Ziele sprengen,", "tokens": ["Da", "st\u00e4ubt's", "von", "flin\u00b7ken", "Rei\u00b7tern", ",", "die", "rasch", "zum", "Zie\u00b7le", "spren\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da knarrt des Fuhrmanns Achse, von Fracht des Segens schwer,", "tokens": ["Da", "knarrt", "des", "Fuhr\u00b7manns", "A\u00b7chse", ",", "von", "Fracht", "des", "Se\u00b7gens", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,", "APPR", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und Wandrer wallen singend die sichre Bahn einher.", "tokens": ["Und", "Wand\u00b7rer", "wal\u00b7len", "sin\u00b7gend", "die", "sich\u00b7re", "Bahn", "ein\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Mit lustigem Ruderschlage, mit flatternden Wimpeln ziehn", "tokens": ["Mit", "lus\u00b7ti\u00b7gem", "Ru\u00b7der\u00b7schla\u00b7ge", ",", "mit", "flat\u00b7tern\u00b7den", "Wim\u00b7peln", "ziehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+--+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Im Strom viel r\u00fcstige Schiffe, wohl kreuzend her und hin,", "tokens": ["Im", "Strom", "viel", "r\u00fcs\u00b7ti\u00b7ge", "Schif\u00b7fe", ",", "wohl", "kreu\u00b7zend", "her", "und", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "ADJA", "NN", "$,", "ADV", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Von Sch\u00e4tzen voll und Waaren, reich bis zum tiefsten Raum;", "tokens": ["Von", "Sch\u00e4t\u00b7zen", "voll", "und", "Waa\u00b7ren", ",", "reich", "bis", "zum", "tiefs\u00b7ten", "Raum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "NN", "$,", "ADJD", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Doch Maxens Schiffer gr\u00fc\u00dfen, nun stolz, die Br\u00fcder kaum.", "tokens": ["Doch", "Ma\u00b7xens", "Schif\u00b7fer", "gr\u00fc\u00b7\u00dfen", ",", "nun", "stolz", ",", "die", "Br\u00fc\u00b7der", "kaum", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "VVINF", "$,", "ADV", "ADJD", "$,", "ART", "NN", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Sieh dort vor dem Geh\u00f6fte, in frischer Trift gelegen,", "tokens": ["Sieh", "dort", "vor", "dem", "Ge\u00b7h\u00f6f\u00b7te", ",", "in", "fri\u00b7scher", "Trift", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ART", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Spricht heitern Blicks ein Landmann just \u00fcber sein Kind den Segen", "tokens": ["Spricht", "hei\u00b7tern", "Blicks", "ein", "Land\u00b7mann", "just", "\u00fc\u00b7ber", "sein", "Kind", "den", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und lehrt's, in Drang und N\u00f6then sein Herz zu Gott zu wenden", "tokens": ["Und", "lehrt's", ",", "in", "Drang", "und", "N\u00f6\u00b7then", "sein", "Herz", "zu", "Gott", "zu", "wen\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "APPR", "NN", "KON", "NN", "PPOSAT", "NN", "APPR", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und beten f\u00fcr gute F\u00fcrsten mit aufgehobnen H\u00e4nden.", "tokens": ["Und", "be\u00b7ten", "f\u00fcr", "gu\u00b7te", "F\u00fcrs\u00b7ten", "mit", "auf\u00b7ge\u00b7hob\u00b7nen", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Und St\u00e4dte stehn am Ufer mit Mauern, schmuck und wei\u00df,", "tokens": ["Und", "St\u00e4d\u00b7te", "stehn", "am", "U\u00b7fer", "mit", "Mau\u00b7ern", ",", "schmuck", "und", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON"