{"textgrid.poem.67045": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Lorbeer", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dank dir! aus dem schnadernden Gedr\u00e4nge", "tokens": ["Dank", "dir", "!", "aus", "dem", "schna\u00b7dern\u00b7den", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nahmst du mich, Vertraute! Einsamkeit!", "tokens": ["Nahmst", "du", "mich", ",", "Ver\u00b7trau\u00b7te", "!", "Ein\u00b7sam\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "NN", "$.", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df ich gl\u00fchend von dem Lorbeer singe,", "tokens": ["Da\u00df", "ich", "gl\u00fc\u00b7hend", "von", "dem", "Lor\u00b7beer", "sin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dem so einzig sich mein Herz geweiht.", "tokens": ["Dem", "so", "ein\u00b7zig", "sich", "mein", "Herz", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "PRF", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Euch zu folgen, Gro\u00dfe! \u2013 Werd ichs k\u00f6nnen?", "tokens": ["Euch", "zu", "fol\u00b7gen", ",", "Gro\u00b7\u00dfe", "!", "\u2013", "Werd", "ichs", "k\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "ADJA", "$.", "$(", "VAFIN", "PIS", "VMFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wirds einst st\u00e4rker, eures J\u00fcnglings Lied?", "tokens": ["Wirds", "einst", "st\u00e4r\u00b7ker", ",", "eu\u00b7res", "J\u00fcng\u00b7lings", "Lied", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Soll ich in die Bahn, zum Ziel zu rennen,", "tokens": ["Soll", "ich", "in", "die", "Bahn", ",", "zum", "Ziel", "zu", "ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "$,", "APPRART", "NN", "PTKZU", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dem dies Auge so entgegengl\u00fcht?", "tokens": ["Dem", "dies", "Au\u00b7ge", "so", "ent\u00b7ge\u00b7gen\u00b7gl\u00fcht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Wann ein Klopstock in des Tempels Halle", "tokens": ["Wann", "ein", "Klops\u00b7tock", "in", "des", "Tem\u00b7pels", "Hal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "NE"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Seinem Gott das Flammenopfer bringt", "tokens": ["Sei\u00b7nem", "Gott", "das", "Flam\u00b7men\u00b7op\u00b7fer", "bringt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und in seiner Psalmen Jubelschalle", "tokens": ["Und", "in", "sei\u00b7ner", "Psal\u00b7men", "Ju\u00b7bel\u00b7schal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Himmelan sich seine Seele schwingt \u2013", "tokens": ["Him\u00b7me\u00b7lan", "sich", "sei\u00b7ne", "See\u00b7le", "schwingt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Wann mein Young in dunkeln Einsamkeiten", "tokens": ["Wann", "mein", "Y\u00b7o\u00b7ung", "in", "dun\u00b7keln", "Ein\u00b7sam\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Rings versammelnd seine Tote wacht,", "tokens": ["Rings", "ver\u00b7sam\u00b7melnd", "sei\u00b7ne", "To\u00b7te", "wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Himmlischer zu stimmen seine Saiten", "tokens": ["Himm\u00b7li\u00b7scher", "zu", "stim\u00b7men", "sei\u00b7ne", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKZU", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "F\u00fcr Begeistrungen der Mitternacht \u2013 \u2013", "tokens": ["F\u00fcr", "Be\u00b7geis\u00b7trun\u00b7gen", "der", "Mit\u00b7ter\u00b7nacht", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$(", "$("], "meter": "+-++--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Ha! der Wonne! ferne nur zu stehen,", "tokens": ["Ha", "!", "der", "Won\u00b7ne", "!", "fer\u00b7ne", "nur", "zu", "ste\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "$.", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Lauschend ihres Liedes Flammengu\u00df,", "tokens": ["Lau\u00b7schend", "ih\u00b7res", "Lie\u00b7des", "Flam\u00b7men\u00b7gu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ihres Geistes Sch\u00f6pfungen zu sehen,", "tokens": ["Ih\u00b7res", "Geis\u00b7tes", "Sch\u00f6p\u00b7fun\u00b7gen", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-++--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wahrlich! es ist Himmelsvorgenu\u00df.", "tokens": ["Wahr\u00b7lich", "!", "es", "ist", "Him\u00b7mels\u00b7vor\u00b7ge\u00b7nu\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VAFIN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Nein! ich wollte nichts auf dieser Erden!", "tokens": ["Nein", "!", "ich", "woll\u00b7te", "nichts", "auf", "die\u00b7ser", "Er\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VMFIN", "PIS", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dulden all der Welt Verfolgungen,", "tokens": ["Dul\u00b7den", "all", "der", "Welt", "Ver\u00b7fol\u00b7gun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Jedes Drangsal, jegliche Beschwerden,", "tokens": ["Je\u00b7des", "Dran\u00b7gsal", ",", "jeg\u00b7li\u00b7che", "Be\u00b7schwer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "All des Neiders bittre Schm\u00e4hungen \u2013 \u2013", "tokens": ["All", "des", "Nei\u00b7ders", "bitt\u00b7re", "Schm\u00e4\u00b7hun\u00b7gen", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$(", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Lieber Gott! wie oft ich Schwacher dachte,", "tokens": ["Lie\u00b7ber", "Gott", "!", "wie", "oft", "ich", "Schwa\u00b7cher", "dach\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$.", "PWAV", "ADV", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie ichs tr\u00f6stete, das arme Herz,", "tokens": ["Wie", "ichs", "tr\u00f6s\u00b7te\u00b7te", ",", "das", "ar\u00b7me", "Herz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn ich N\u00e4chte kummervoll durchwachte,", "tokens": ["Wenn", "ich", "N\u00e4ch\u00b7te", "kum\u00b7mer\u00b7voll", "durch\u00b7wach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "O so oft, so oft in meinem Schmerz,", "tokens": ["O", "so", "oft", ",", "so", "oft", "in", "mei\u00b7nem", "Schmerz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "$,", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Wann der Stolz ver\u00e4chtlich niederschaute,", "tokens": ["Wann", "der", "Stolz", "ver\u00b7\u00e4cht\u00b7lich", "nie\u00b7der\u00b7schau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wann der Eitle meiner spottete,", "tokens": ["Wann", "der", "Eit\u00b7le", "mei\u00b7ner", "spot\u00b7te\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dem vor meinen Sittenspr\u00fcchen graute,", "tokens": ["Dem", "vor", "mei\u00b7nen", "Sit\u00b7ten\u00b7spr\u00fc\u00b7chen", "grau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn oft selbst \u2013 mich floh \u2013 der Edlere;", "tokens": ["Wenn", "oft", "selbst", "\u2013", "mich", "floh", "\u2013", "der", "Ed\u00b7le\u00b7re", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "$(", "PPER", "VVFIN", "$(", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "O vielleicht, da\u00df diese Bitterkeiten \u2013", "tokens": ["O", "viel\u00b7leicht", ",", "da\u00df", "die\u00b7se", "Bit\u00b7ter\u00b7kei\u00b7ten", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "KOUS", "PDAT", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dacht ich \u2013 st\u00e4rker bilden deinen Geist!", "tokens": ["Dacht", "ich", "\u2013", "st\u00e4r\u00b7ker", "bil\u00b7den", "dei\u00b7nen", "Geist", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df die Stille h\u00f6her deine Saiten", "tokens": ["Da\u00df", "die", "Stil\u00b7le", "h\u00f6\u00b7her", "dei\u00b7ne", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Stimmt, zu m\u00e4nnlichem Gesang dich rei\u00dft!", "tokens": ["Stimmt", ",", "zu", "m\u00e4nn\u00b7li\u00b7chem", "Ge\u00b7sang", "dich", "rei\u00dft", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Aber still! Die goldne Bubentr\u00e4ume", "tokens": ["A\u00b7ber", "still", "!", "Die", "gold\u00b7ne", "Bu\u00b7bent\u00b7r\u00e4u\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "H\u00f6rt in ihrer Nacht die Zukunft nicht \u2013", "tokens": ["H\u00f6rt", "in", "ih\u00b7rer", "Nacht", "die", "Zu\u00b7kunft", "nicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "ART", "NN", "PTKNEG", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Schon so manche Fr\u00fcchte sch\u00f6ner Keime", "tokens": ["Schon", "so", "man\u00b7che", "Fr\u00fcch\u00b7te", "sch\u00f6\u00b7ner", "Kei\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Logen grausam mir ins Angesicht.", "tokens": ["Lo\u00b7gen", "grau\u00b7sam", "mir", "ins", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}