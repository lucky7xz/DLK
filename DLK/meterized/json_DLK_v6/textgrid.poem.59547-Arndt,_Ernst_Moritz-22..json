{"textgrid.poem.59547": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lockst du mich, du Gottesfrieden,", "tokens": ["Lockst", "du", "mich", ",", "du", "Got\u00b7tes\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu den sch\u00f6nen Himmelsauen,", "tokens": ["Zu", "den", "sch\u00f6\u00b7nen", "Him\u00b7mel\u00b7sau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die wir Dunkle, ach! hienieden", "tokens": ["Die", "wir", "Dunk\u00b7le", ",", "ach", "!", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "PPER", "VVFIN", "$,", "ITJ", "$.", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur in blassen Schatten schauen?", "tokens": ["Nur", "in", "blas\u00b7sen", "Schat\u00b7ten", "schau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lockst du mich, o Sehnsucht, immer,", "tokens": ["Lockst", "du", "mich", ",", "o", "Sehn\u00b7sucht", ",", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "FM", "NN", "$,", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie die Frommen Glockenl\u00e4uten,", "tokens": ["Wie", "die", "From\u00b7men", "Glo\u00b7cken\u00b7l\u00e4u\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wieder hin zum Sternenschimmer?", "tokens": ["Wie\u00b7der", "hin", "zum", "Ster\u00b7nen\u00b7schim\u00b7mer", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wieder in die alten Zeiten?", "tokens": ["Wie\u00b7der", "in", "die", "al\u00b7ten", "Zei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In die Zeiten, l\u00e4ngst vergangen?", "tokens": ["In", "die", "Zei\u00b7ten", ",", "l\u00e4ngst", "ver\u00b7gan\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Seelen Kindertage?", "tokens": ["In", "der", "See\u00b7len", "Kin\u00b7der\u00b7ta\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dahin schmachtest du, Verlangen?", "tokens": ["Da\u00b7hin", "schmach\u00b7test", "du", ",", "Ver\u00b7lan\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Dahin, Herz, mit jedem Schlage?", "tokens": ["Da\u00b7hin", ",", "Herz", ",", "mit", "je\u00b7dem", "Schla\u00b7ge", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "$,", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja, der Funke will zur Sonne,", "tokens": ["Ja", ",", "der", "Fun\u00b7ke", "will", "zur", "Son\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VMFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die Seele will zum Himmel,", "tokens": ["Und", "die", "See\u00b7le", "will", "zum", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Zu des stillen Lebens Wonne", "tokens": ["Zu", "des", "stil\u00b7len", "Le\u00b7bens", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aus dem tollen Erdgewimmel.", "tokens": ["Aus", "dem", "tol\u00b7len", "Erd\u00b7ge\u00b7wim\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nein, es ist kein Wahn der Tr\u00e4ume,", "tokens": ["Nein", ",", "es", "ist", "kein", "Wahn", "der", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist kein Irrlicht d\u00fcstrer N\u00e4chte,", "tokens": ["Ist", "kein", "Irr\u00b7licht", "d\u00fcst\u00b7rer", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mein sind jene Sternenr\u00e4ume,", "tokens": ["Mein", "sind", "je\u00b7ne", "Ster\u00b7nen\u00b7r\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mein sind jene G\u00f6tterrechte:", "tokens": ["Mein", "sind", "je\u00b7ne", "G\u00f6t\u00b7ter\u00b7rech\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Fremdling bin ich nur im Staube,", "tokens": ["Fremd\u00b7ling", "bin", "ich", "nur", "im", "Stau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meine Heimat such' ich wieder,", "tokens": ["Mei\u00b7ne", "Hei\u00b7mat", "such'", "ich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Meine gr\u00fcne Himmelslaube,", "tokens": ["Mei\u00b7ne", "gr\u00fc\u00b7ne", "Him\u00b7mels\u00b7lau\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Meine Himmelsblumen wieder.", "tokens": ["Mei\u00b7ne", "Him\u00b7mels\u00b7blu\u00b7men", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Was soll ich hienieden streben", "tokens": ["Was", "soll", "ich", "hien\u00b7ie\u00b7den", "stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwischen Kummer stets und Freude,", "tokens": ["Zwi\u00b7schen", "Kum\u00b7mer", "stets", "und", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem unruhvollen Leben", "tokens": ["In", "dem", "un\u00b7ruh\u00b7vol\u00b7len", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Minuten schnelle Beute?", "tokens": ["Der", "Mi\u00b7nu\u00b7ten", "schnel\u00b7le", "Beu\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie die V\u00f6glein auf den Zweigen", "tokens": ["Wie", "die", "V\u00f6\u00b7glein", "auf", "den", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wechselnd hin und wieder fliegen,", "tokens": ["Wech\u00b7selnd", "hin", "und", "wie\u00b7der", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schwebt des Menschen Tun und Neigen,", "tokens": ["Schwebt", "des", "Men\u00b7schen", "Tun", "und", "Nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schwebt sein W\u00fcnschen, sein Vergn\u00fcgen.", "tokens": ["Schwebt", "sein", "W\u00fcn\u00b7schen", ",", "sein", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie soll ich hienieden finden,", "tokens": ["Wie", "soll", "ich", "hien\u00b7ie\u00b7den", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was die hei\u00dfe Liebe stillet,", "tokens": ["Was", "die", "hei\u00b7\u00dfe", "Lie\u00b7be", "stil\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die Unruh' wilder S\u00fcnden", "tokens": ["Wo", "die", "Un\u00b7ruh'", "wil\u00b7der", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus der Erdenfreude quillet?", "tokens": ["Aus", "der", "Er\u00b7den\u00b7freu\u00b7de", "quil\u00b7let", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo wir heute lassen m\u00fcssen,", "tokens": ["Wo", "wir", "heu\u00b7te", "las\u00b7sen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVINF", "VMINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dem wir gestern angehangen?", "tokens": ["Dem", "wir", "ge\u00b7stern", "an\u00b7ge\u00b7han\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Wo Begierde und Gewissen", "tokens": ["Wo", "Be\u00b7gier\u00b7de", "und", "Ge\u00b7wis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sind in stetem Krieg befangen?", "tokens": ["Sind", "in", "ste\u00b7tem", "Krieg", "be\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Was soll ich hienieden schaffen,", "tokens": ["Was", "soll", "ich", "hien\u00b7ie\u00b7den", "schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier, wo nichts best\u00e4ndig bleibet?", "tokens": ["Hier", ",", "wo", "nichts", "be\u00b7st\u00e4n\u00b7dig", "blei\u00b7bet", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PIS", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo vom Staub und Blut der Waffen", "tokens": ["Wo", "vom", "Staub", "und", "Blut", "der", "Waf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "KON", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stets die wilde Rennbahn st\u00e4ubet?", "tokens": ["Stets", "die", "wil\u00b7de", "Renn\u00b7bahn", "st\u00e4u\u00b7bet", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo die L\u00fcge auf dem Throne", "tokens": ["Wo", "die", "L\u00fc\u00b7ge", "auf", "dem", "Thro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gaukelnde Orakel singet", "tokens": ["Gau\u00b7keln\u00b7de", "O\u00b7ra\u00b7kel", "sin\u00b7get"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und mit blut' ger Dornenkrone", "tokens": ["Und", "mit", "blut'", "ger", "Dor\u00b7nen\u00b7kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wahrheit kaum vernommen klinget?", "tokens": ["Wahr\u00b7heit", "kaum", "ver\u00b7nom\u00b7men", "klin\u00b7get", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Fahre hin, du Land der Tr\u00e4nen!", "tokens": ["Fah\u00b7re", "hin", ",", "du", "Land", "der", "Tr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hin, du Land der s\u00fc\u00dfen L\u00fcgen!", "tokens": ["Hin", ",", "du", "Land", "der", "s\u00fc\u00b7\u00dfen", "L\u00fc\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Damit wir uns hinnen sehnen,", "tokens": ["Da\u00b7mit", "wir", "uns", "hin\u00b7nen", "seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Darum mu\u00dft du viel betr\u00fcgen;", "tokens": ["Da\u00b7rum", "mu\u00dft", "du", "viel", "be\u00b7tr\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Damit wir das Feste wollen,", "tokens": ["Da\u00b7mit", "wir", "das", "Fes\u00b7te", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VMFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Darum mu\u00df in dir nichts bleiben,", "tokens": ["Da\u00b7rum", "mu\u00df", "in", "dir", "nichts", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "APPR", "PPER", "PIS", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Alles durcheinander rollen", "tokens": ["Al\u00b7les", "durch\u00b7ein\u00b7an\u00b7der", "rol\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PIS", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die Welle Welle treiben.", "tokens": ["Und", "die", "Wel\u00b7le", "Wel\u00b7le", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Locke, stiller Gottesfrieden!", "tokens": ["Lo\u00b7cke", ",", "stil\u00b7ler", "Got\u00b7tes\u00b7frie\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00fc\u00dfe Sehnsucht, schweige nimmer!", "tokens": ["S\u00fc\u00b7\u00dfe", "Sehn\u00b7sucht", ",", "schwei\u00b7ge", "nim\u00b7mer", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "A