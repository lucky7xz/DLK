{"textgrid.poem.53260": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[wer wei\u00df Bescheidt]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer wei\u00df Bescheidt,", "tokens": ["Wer", "wei\u00df", "Be\u00b7scheidt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der Sterbligkeit", "tokens": ["Der", "Ster\u00b7blig\u00b7keit"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mich seelig zu entladen,", "tokens": ["Mich", "see\u00b7lig", "zu", "ent\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Damit sie nicht", "tokens": ["Da\u00b7mit", "sie", "nicht"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Nach diesem Liecht", "tokens": ["Nach", "die\u00b7sem", "Liecht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Mir ewig m\u00f6ge schaden?", "tokens": ["Mir", "e\u00b7wig", "m\u00f6\u00b7ge", "scha\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das kan vnd thut", "tokens": ["Das", "kan", "vnd", "thut"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "KON", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Mein h\u00f6chstes Gut", "tokens": ["Mein", "h\u00f6chs\u00b7tes", "Gut"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Der reiche Brunn der Gnaden.", "tokens": ["Der", "rei\u00b7che", "Brunn", "der", "Gna\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Herr Jesu Christ,", "tokens": ["Herr", "Je\u00b7su", "Christ", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Du einig bist", "tokens": ["Du", "ei\u00b7nig", "bist"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJD", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der mich wei\u00df zu erretten,", "tokens": ["Der", "mich", "wei\u00df", "zu", "er\u00b7ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ob alle Noht", "tokens": ["Ob", "al\u00b7le", "Noht"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ja Hell' vnd Todt", "tokens": ["Ja", "Hell'", "vnd", "Todt"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKANT", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Mich gleich vmbgeben hetten,", "tokens": ["Mich", "gleich", "vmb\u00b7ge\u00b7ben", "het\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mein Trost, durch dich", "tokens": ["Mein", "Trost", ",", "durch", "dich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Befrey' ich mich", "tokens": ["Be\u00b7frey'", "ich", "mich"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Der schweren LeibesKetten.", "tokens": ["Der", "schwe\u00b7ren", "Lei\u00b7bes", "Ket\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wenn ich nun soll", "tokens": ["Wenn", "ich", "nun", "soll"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VMFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Des Lebens Zoll", "tokens": ["Des", "Le\u00b7bens", "Zoll"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Durch meinen Todt dir reichen,", "tokens": ["Durch", "mei\u00b7nen", "Todt", "dir", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd kommen bin", "tokens": ["Vnd", "kom\u00b7men", "bin"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVINF", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Von Witz vnd Sinn,", "tokens": ["Von", "Witz", "vnd", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Die R\u00f6hte mu\u00df verbleichen,", "tokens": ["Die", "R\u00f6h\u00b7te", "mu\u00df", "ver\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Zungen Krafft", "tokens": ["Der", "Zun\u00b7gen", "Krafft"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Nichts thut vnd schafft,", "tokens": ["Nichts", "thut", "vnd", "schafft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Wenn Ohr vnd Augen weichen;", "tokens": ["Wenn", "Ohr", "vnd", "Au\u00b7gen", "wei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wirst Du allein", "tokens": ["Wirst", "Du", "al\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Noch vmb mich seyn,", "tokens": ["Noch", "vmb", "mich", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VAINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mir Rhat vnd trost beybringen,", "tokens": ["Mir", "Rhat", "vnd", "trost", "bey\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df nicht mein Hertz", "tokens": ["Da\u00df", "nicht", "mein", "Hertz"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Durch grossen schmertz", "tokens": ["Durch", "gros\u00b7sen", "schmertz"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Des Todes mag zerspringen,", "tokens": ["Des", "To\u00b7des", "mag", "zer\u00b7sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wirst helffen mir", "tokens": ["Wirst", "helf\u00b7fen", "mir"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Der Frommen Zier", "tokens": ["Der", "From\u00b7men", "Zier"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Die Ehren-Kron' erringen.", "tokens": ["Die", "Eh\u00b7ren\u00b7Kron'", "er\u00b7rin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sonst wei\u00df ich nicht,", "tokens": ["Sonst", "wei\u00df", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Herr Christ, mein Liecht,", "tokens": ["Herr", "Christ", ",", "mein", "Liecht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Warumb du hier auff Erden", "tokens": ["Wa\u00b7rumb", "du", "hier", "auff", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das, was wir sind,", "tokens": ["Das", ",", "was", "wir", "sind", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ein schwaches Kind,", "tokens": ["Ein", "schwa\u00b7ches", "Kind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ohn schuld hast wollen werden,", "tokens": ["Ohn", "schuld", "hast", "wol\u00b7len", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "VAFIN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dich arm vnd schlecht", "tokens": ["Dich", "arm", "vnd", "schlecht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJD", "KON", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Als sonst ein Knecht", "tokens": ["Als", "sonst", "ein", "Knecht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Erweisen an Geberden,", "tokens": ["Er\u00b7wei\u00b7sen", "an", "Ge\u00b7ber\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Verachtet stehn,", "tokens": ["Ver\u00b7ach\u00b7tet", "stehn", ","], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Vnd m\u00fcssig gehn", "tokens": ["Vnd", "m\u00fcs\u00b7sig", "gehn"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVINF"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der Welt sampt jhren frewden,", "tokens": ["Der", "Welt", "sampt", "jhren", "frew\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Warumb Du dich", "tokens": ["Wa\u00b7rumb", "Du", "dich"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "PRF"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "So williglich", "tokens": ["So", "wil\u00b7lig\u00b7lich"], "token_info": ["word", "word"], "pos": ["ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Erzeigt in allem Leiden,", "tokens": ["Er\u00b7zeigt", "in", "al\u00b7lem", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vnd keine Noht", "tokens": ["Vnd", "kei\u00b7ne", "Noht"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Auch nicht den Todt", "tokens": ["Auch", "nicht", "den", "Todt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Zuletzt hast wollen meiden.", "tokens": ["Zu\u00b7letzt", "hast", "wol\u00b7len", "mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich aber bin", "tokens": ["Ich", "a\u00b7ber", "bin"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "In meinem Sinn", "tokens": ["In", "mei\u00b7nem", "Sinn"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der Sachen \u00fcberf\u00fchret,", "tokens": ["Der", "Sa\u00b7chen", "\u00fc\u00b7berf\u00b7\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df mir dein Blut", "tokens": ["Da\u00df", "mir", "dein", "Blut"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Das h\u00f6chste Gut", "tokens": ["Das", "h\u00f6chs\u00b7te", "Gut"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Der Seeligkeit gebieret,", "tokens": ["Der", "See\u00b7lig\u00b7keit", "ge\u00b7bie\u00b7ret", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vnd das mein Heil", "tokens": ["Vnd", "das", "mein", "Heil"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDS", "PPOSAT", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Des Himmels Theil", "tokens": ["Des", "Him\u00b7mels", "Theil"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Aus deinem Tode r\u00fchret.", "tokens": ["Aus", "dei\u00b7nem", "To\u00b7de", "r\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Nur schreib hinfort", "tokens": ["Nur", "schreib", "hin\u00b7fort"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Dein Glaubenswort", "tokens": ["Dein", "Glau\u00b7bens\u00b7wort"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Tief ein den schwachen sinnen,", "tokens": ["Tief", "ein", "den", "schwa\u00b7chen", "sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd schencke mir,", "tokens": ["Vnd", "schen\u00b7cke", "mir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Da\u00df ich in Dir", "tokens": ["Da\u00df", "ich", "in", "Dir"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Mag ferner krafft gewinnen,", "tokens": ["Mag", "fer\u00b7ner", "krafft", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vnd ist es zeit,", "tokens": ["Vnd", "ist", "es", "zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "So nim auch heut", "tokens": ["So", "nim", "auch", "heut"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Mich seeliglich von hinnen.", "tokens": ["Mich", "see\u00b7lig\u00b7lich", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPR", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}