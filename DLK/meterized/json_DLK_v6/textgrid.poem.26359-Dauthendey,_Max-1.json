{"textgrid.poem.26359": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wei\u00df flie\u00dft Nachmittaglicht. K\u00fchl der Himmel. Schicht an Schicht eisige Wolken.", "tokens": ["Wei\u00df", "flie\u00dft", "Nach\u00b7mit\u00b7tag\u00b7licht", ".", "K\u00fchl", "der", "Him\u00b7mel", ".", "Schicht", "an", "Schicht", "ei\u00b7si\u00b7ge", "Wol\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "$.", "NN", "ART", "NN", "$.", "NN", "APPR", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+---+-", "measure": "unknown.measure.octa.plus"}, "line.2": {"text": "Geronnen aus Asche und Hunger staubgrau steht die Hochsommerheide.", "tokens": ["Ge\u00b7ron\u00b7nen", "aus", "A\u00b7sche", "und", "Hun\u00b7ger", "staub\u00b7grau", "steht", "die", "Hoch\u00b7som\u00b7mer\u00b7hei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-+-+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "In Nacktheit wankt eine Schar, M\u00e4nner, Frauen, Greise.", "tokens": ["In", "Nackt\u00b7heit", "wankt", "ei\u00b7ne", "Schar", ",", "M\u00e4n\u00b7ner", ",", "Frau\u00b7en", ",", "Grei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Schwarze Wunden st\u00f6hnen an allen Br\u00fcsten. Dumpf dr\u00f6hnen die Herzen.", "tokens": ["Schwar\u00b7ze", "Wun\u00b7den", "st\u00f6h\u00b7nen", "an", "al\u00b7len", "Br\u00fcs\u00b7ten", ".", "Dumpf", "dr\u00f6h\u00b7nen", "die", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "PIAT", "NN", "$.", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+--+--+--+-", "measure": "trochaic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Schale Ruhe im Feld. Kahle Staubd\u00fcfte steigen. Die Schritte der Nackten im Takt", "tokens": ["Scha\u00b7le", "Ru\u00b7he", "im", "Feld", ".", "Kah\u00b7le", "Staub\u00b7d\u00fcf\u00b7te", "stei\u00b7gen", ".", "Die", "Schrit\u00b7te", "der", "Nack\u00b7ten", "im", "Takt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "NN", "$.", "NN", "NN", "VVINF", "$.", "ART", "NN", "ART", "NN", "APPRART", "NN"], "meter": "+-+--+--+--+--+--+--+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Mit dem blauen schaukelnden Reigen der Heideglocken und Astern.", "tokens": ["Mit", "dem", "blau\u00b7en", "schau\u00b7keln\u00b7den", "Rei\u00b7gen", "der", "Hei\u00b7de\u00b7glo\u00b7cken", "und", "As\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "--+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Milchlichte M\u00e4gde, schwalbenschlank, lesen geb\u00fcckt lila Minzbl\u00fcten, Wacholderkraut,", "tokens": ["Milch\u00b7lich\u00b7te", "M\u00e4g\u00b7de", ",", "schwal\u00b7ben\u00b7schlank", ",", "le\u00b7sen", "ge\u00b7b\u00fcckt", "li\u00b7la", "Minz\u00b7bl\u00fc\u00b7ten", ",", "Wa\u00b7chol\u00b7der\u00b7kraut", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,", "VVINF", "VVFIN", "ADJD", "NN", "$,", "NN", "$,"], "meter": "+--+-+-+-+-+--++--+-+", "measure": "iambic.octa.plus.invert"}, "line.4": {"text": "Wecken mit W\u00fcrzger\u00fcchen den bedr\u00fcckten Laut ihrer wundkranken Herzen.", "tokens": ["We\u00b7cken", "mit", "W\u00fcrz\u00b7ge\u00b7r\u00fc\u00b7chen", "den", "be\u00b7dr\u00fcck\u00b7ten", "Laut", "ih\u00b7rer", "wund\u00b7kran\u00b7ken", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "ADJA", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.invert"}, "line.5": {"text": "Am Weg gelbe Bl\u00fctenzepter der K\u00f6nigkerzen rauschen in jubelndem Gold,", "tokens": ["Am", "Weg", "gel\u00b7be", "Bl\u00fc\u00b7ten\u00b7zep\u00b7ter", "der", "K\u00f6\u00b7nig\u00b7ker\u00b7zen", "rau\u00b7schen", "in", "ju\u00b7beln\u00b7dem", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-+-+--+--+", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "Die Dirnen lauschen mit zagem Staunen, im Bl\u00fctenrauschen raunen k\u00fcnftige gl\u00fcckhelle Tage.", "tokens": ["Die", "Dir\u00b7nen", "lau\u00b7schen", "mit", "za\u00b7gem", "Stau\u00b7nen", ",", "im", "Bl\u00fc\u00b7ten\u00b7rau\u00b7schen", "rau\u00b7nen", "k\u00fcnf\u00b7ti\u00b7ge", "gl\u00fcck\u00b7hel\u00b7le", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,", "APPRART", "NN", "VVFIN", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+-+--+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Mit fahlen Augen rehschlanke Knaben traben dem Schwarme voraus,", "tokens": ["Mit", "fah\u00b7len", "Au\u00b7gen", "reh\u00b7schlan\u00b7ke", "Kna\u00b7ben", "tra\u00b7ben", "dem", "Schwar\u00b7me", "vo\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+-+--+--+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Saugen knirschend in Qualen das kranke Blut der eigenen Wunden.", "tokens": ["Sau\u00b7gen", "knir\u00b7schend", "in", "Qua\u00b7len", "das", "kran\u00b7ke", "Blut", "der", "ei\u00b7ge\u00b7nen", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "NN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Eine Grasm\u00fccke lispelt im Brombeergerank, wei\u00df ein Wiesel, eine Eidechse gr\u00fcn", "tokens": ["Ei\u00b7ne", "Gras\u00b7m\u00fc\u00b7cke", "lis\u00b7pelt", "im", "Brom\u00b7beer\u00b7ge\u00b7rank", ",", "wei\u00df", "ein", "Wie\u00b7sel", ",", "ei\u00b7ne", "Ei\u00b7dech\u00b7se", "gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ADJD"], "meter": "+-+--+--+--+--+-+-+--+", "measure": "trochaic.octa.plus.relaxed"}, "line.4": {"text": "Schl\u00fcpfen durch die Erdgr\u00fcfte, \u2013 k\u00fchn l\u00fcften sich junge Blicke, die Knaben h\u00fcpfen,", "tokens": ["Schl\u00fcp\u00b7fen", "durch", "die", "Erd\u00b7gr\u00fcf\u00b7te", ",", "\u2013", "k\u00fchn", "l\u00fcf\u00b7ten", "sich", "jun\u00b7ge", "Bli\u00b7cke", ",", "die", "Kna\u00b7ben", "h\u00fcp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "$(", "ADJD", "VVFIN", "PRF", "ADJA", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+--++--+-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.5": {"text": "Durchsp\u00e4hen die Weiten, einer Lerche Lied wirbelt; der Knaben todmattes Blut", "tokens": ["Durch\u00b7sp\u00e4\u00b7hen", "die", "Wei\u00b7ten", ",", "ei\u00b7ner", "Ler\u00b7che", "Lied", "wir\u00b7belt", ";", "der", "Kna\u00b7ben", "tod\u00b7mat\u00b7tes", "Blut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$.", "ART", "NN", "ADJA", "NN"], "meter": "-+--+-+-+-+-+-+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "Wirbelt mit in Hoffnungsr\u00f6te und Zukunftmut.", "tokens": ["Wir\u00b7belt", "mit", "in", "Hoff\u00b7nungs\u00b7r\u00f6\u00b7te", "und", "Zu\u00b7kunft\u00b7mut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Schwer schreiten Frauen, narzissenwei\u00dfe, umschlingen einander in hei\u00dfen Reihen, singen im Wandern.", "tokens": ["Schwer", "schrei\u00b7ten", "Frau\u00b7en", ",", "nar\u00b7zis\u00b7sen\u00b7wei\u00b7\u00dfe", ",", "um\u00b7schlin\u00b7gen", "ein\u00b7an\u00b7der", "in", "hei\u00b7\u00dfen", "Rei\u00b7hen", ",", "sin\u00b7gen", "im", "Wan\u00b7dern", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "VVFIN", "$,", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+--+-+--+--+--+-+-+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Haar flutet, str\u00f6mt \u00fcber wunddunkle Br\u00fcste. Ungestillt, l\u00fcstehungernd glutet ihr Atem.", "tokens": ["Haar", "flu\u00b7tet", ",", "str\u00f6mt", "\u00fc\u00b7ber", "wund\u00b7dunk\u00b7le", "Br\u00fcs\u00b7te", ".", "Un\u00b7ge\u00b7stillt", ",", "l\u00fcs\u00b7te\u00b7hun\u00b7gernd", "glu\u00b7tet", "ihr", "A\u00b7tem", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$.", "NN", "$,", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+--+--+-+-++-+-+--+-", "measure": "dactylic.tri.plus"}}, "stanza.5": {"line.1": {"text": "Sie pressen das Blut aus den Str\u00e4hnen, singen Mut den zagenden Greisen:", "tokens": ["Sie", "pres\u00b7sen", "das", "Blut", "aus", "den", "Str\u00e4h\u00b7nen", ",", "sin\u00b7gen", "Mut", "den", "za\u00b7gen\u00b7den", "Grei\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+-+--+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "\u00bbla\u00dft euer Herzrot am Wege, wir werden gesunden.", "tokens": ["\u00bb", "la\u00dft", "eu\u00b7er", "Herz\u00b7rot", "am", "We\u00b7ge", ",", "wir", "wer\u00b7den", "ge\u00b7sun\u00b7den", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPOSAT", "NN", "APPRART", "NN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-++-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "La\u00dft euer Herzrot zur\u00fcck, lacht eurer Wunden.", "tokens": ["La\u00dft", "eu\u00b7er", "Herz\u00b7rot", "zu\u00b7r\u00fcck", ",", "lacht", "eu\u00b7rer", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PTKVZ", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Kehren wir wieder, bl\u00fcht uns das scharlachne Gl\u00fcck,", "tokens": ["Keh\u00b7ren", "wir", "wie\u00b7der", ",", "bl\u00fcht", "uns", "das", "schar\u00b7lach\u00b7ne", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Wir werden lachend gesunden.\u00ab", "tokens": ["Wir", "wer\u00b7den", "la\u00b7chend", "ge\u00b7sun\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Der Greise Schar stockt. Von Falten durchwurzelt ein blutleer Gesicht spricht klagend zur\u00fcck:", "tokens": ["Der", "Grei\u00b7se", "Schar", "stockt", ".", "Von", "Fal\u00b7ten", "durch\u00b7wur\u00b7zelt", "ein", "blut\u00b7leer", "Ge\u00b7sicht", "spricht", "kla\u00b7gend", "zu\u00b7r\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$.", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+--+--+--+--+-+--+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "\u00bbgl\u00fcck lockt uns nirgends am Wege.", "tokens": ["\u00bb", "gl\u00fcck", "lockt", "uns", "nir\u00b7gends", "am", "We\u00b7ge", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Die Heiden kamen, die Heiden verbl\u00fchten,", "tokens": ["Die", "Hei\u00b7den", "ka\u00b7men", ",", "die", "Hei\u00b7den", "ver\u00b7bl\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wei\u00df gl\u00fchten Winter um Winter,", "tokens": ["Wei\u00df", "gl\u00fch\u00b7ten", "Win\u00b7ter", "um", "Win\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Jeder Morgen entz\u00fcndet dunkler die Wunden.", "tokens": ["Je\u00b7der", "Mor\u00b7gen", "ent\u00b7z\u00fcn\u00b7det", "dunk\u00b7ler", "die", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADJA", "ART", "NN", "$."], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Gesunden? \u2013 Wir werden nie mehr gesunden.\u00ab", "tokens": ["Ge\u00b7sun\u00b7den", "?", "\u2013", "Wir", "wer\u00b7den", "nie", "mehr", "ge\u00b7sun\u00b7den", ".", "\u00ab"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "$(", "PPER", "VAFIN", "ADV", "ADV", "VVPP", "$.", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Heulendes Lachen h\u00f6hnt. Die St\u00e4rksten werfen sich selber die Keulen an die zerschundene Brust,", "tokens": ["Heu\u00b7len\u00b7des", "La\u00b7chen", "h\u00f6hnt", ".", "Die", "St\u00e4rks\u00b7ten", "wer\u00b7fen", "sich", "sel\u00b7ber", "die", "Keu\u00b7len", "an", "die", "zer\u00b7schun\u00b7de\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "punct", "word", "word"