{"textgrid.poem.65833": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "107", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht eigne Sorgen, kein prophetisch Denken", "tokens": ["Nicht", "eig\u00b7ne", "Sor\u00b7gen", ",", "kein", "pro\u00b7phe\u00b7tisch", "Den\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADJA", "NN", "$,", "PIAT", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des weiten Erdballs, der von Zukunft tr\u00e4umt,", "tokens": ["Des", "wei\u00b7ten", "Erd\u00b7balls", ",", "der", "von", "Zu\u00b7kunft", "tr\u00e4umt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kann meiner treuen Liebe Frist beschr\u00e4nken,", "tokens": ["Kann", "mei\u00b7ner", "treu\u00b7en", "Lie\u00b7be", "Frist", "be\u00b7schr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als h\u00e4tt' ein Richterspruch sie eng umz\u00e4umt.", "tokens": ["Als", "h\u00e4tt'", "ein", "Rich\u00b7ter\u00b7spruch", "sie", "eng", "um\u00b7z\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "NN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Heil blieb der Mond in Todesfinsternis.", "tokens": ["Heil", "blieb", "der", "Mond", "in", "To\u00b7des\u00b7fins\u00b7ter\u00b7nis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ernsthafte Augurn spotten eigner Kunde;", "tokens": ["Ernst\u00b7haf\u00b7te", "Au\u00b7gurn", "spot\u00b7ten", "eig\u00b7ner", "Kun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Unsichres kr\u00f6nt sich selbst nun als gewi\u00df,", "tokens": ["Un\u00b7sich\u00b7res", "kr\u00f6nt", "sich", "selbst", "nun", "als", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ADV", "ADV", "KOKOM", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und Friedens \u00d6lzweig l\u00e4chelt ew'gem Bunde.", "tokens": ["Und", "Frie\u00b7dens", "\u00d6l\u00b7zweig", "l\u00e4\u00b7chelt", "ew'\u00b7gem", "Bun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nun, in den Tropfen dieser Balsamzeit", "tokens": ["Nun", ",", "in", "den", "Trop\u00b7fen", "die\u00b7ser", "Bal\u00b7sam\u00b7zeit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Steht meine Liebe frisch: Tod ist ihr hold;", "tokens": ["Steht", "mei\u00b7ne", "Lie\u00b7be", "frisch", ":", "Tod", "ist", "ihr", "hold", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "$.", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Ich leb' in armen Reimen ihm zum Neid,", "tokens": ["Ich", "leb'", "in", "ar\u00b7men", "Rei\u00b7men", "ihm", "zum", "Neid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Wenn er sprachlosen, dunkeln Herden grollt.", "tokens": ["Wenn", "er", "sprach\u00b7lo\u00b7sen", ",", "dun\u00b7keln", "Her\u00b7den", "grollt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und darin will ich fest dein Denkmal gr\u00fcnden,", "tokens": ["Und", "da\u00b7rin", "will", "ich", "fest", "dein", "Denk\u00b7mal", "gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VMFIN", "PPER", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Wenn eh'rne Gr\u00e4ber, wenn Tyrannenschilde schwinden.", "tokens": ["Wenn", "eh'r\u00b7ne", "Gr\u00e4\u00b7ber", ",", "wenn", "Ty\u00b7ran\u00b7nen\u00b7schil\u00b7de", "schwin\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}