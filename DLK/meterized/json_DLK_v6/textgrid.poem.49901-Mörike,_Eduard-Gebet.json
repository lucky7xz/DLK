{"textgrid.poem.49901": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Gebet", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr! schicke was du willt,", "tokens": ["Herr", "!", "schi\u00b7cke", "was", "du", "willt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PWS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein Liebes oder Leides;", "tokens": ["Ein", "Lie\u00b7bes", "o\u00b7der", "Lei\u00b7des", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich bin vergn\u00fcgt, da\u00df beides", "tokens": ["Ich", "bin", "ver\u00b7gn\u00fcgt", ",", "da\u00df", "bei\u00b7des"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KOUS", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus Deinen H\u00e4nden quillt.", "tokens": ["Aus", "Dei\u00b7nen", "H\u00e4n\u00b7den", "quillt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wollest mit Freuden", "tokens": ["Wol\u00b7lest", "mit", "Freu\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und wollest mit Leiden", "tokens": ["Und", "wol\u00b7lest", "mit", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Mich nicht \u00fcbersch\u00fctten!", "tokens": ["Mich", "nicht", "\u00fc\u00b7ber\u00b7sch\u00fct\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Doch in der Mitten", "tokens": ["Doch", "in", "der", "Mit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Liegt holdes Bescheiden.", "tokens": ["Liegt", "hol\u00b7des", "Be\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}