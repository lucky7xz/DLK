{"textgrid.poem.55315": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Eins und Alles", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Grenzenlosen sich zu finden,", "tokens": ["Im", "Gren\u00b7zen\u00b7lo\u00b7sen", "sich", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wird gern der Einzelne verschwinden,", "tokens": ["Wird", "gern", "der", "Ein\u00b7zel\u00b7ne", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da l\u00f6st sich aller \u00dcberdru\u00df;", "tokens": ["Da", "l\u00f6st", "sich", "al\u00b7ler", "\u00dc\u00b7berd\u00b7ru\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Statt hei\u00dfem W\u00fcnschen, wildem Wollen,", "tokens": ["Statt", "hei\u00b7\u00dfem", "W\u00fcn\u00b7schen", ",", "wil\u00b7dem", "Wol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Statt l\u00e4st'gem Fordern, strengem Sollen", "tokens": ["Statt", "l\u00e4st'\u00b7gem", "For\u00b7dern", ",", "stren\u00b7gem", "Sol\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sich aufzugeben ist Genu\u00df.", "tokens": ["Sich", "auf\u00b7zu\u00b7ge\u00b7ben", "ist", "Ge\u00b7nu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVIZU", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Weltseele, komm, uns zu durchdringen!", "tokens": ["Welt\u00b7see\u00b7le", ",", "komm", ",", "uns", "zu", "durch\u00b7drin\u00b7gen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dann mit dem Weltgeist selbst zu ringen", "tokens": ["Dann", "mit", "dem", "Welt\u00b7geist", "selbst", "zu", "rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wird unsrer Kr\u00e4fte Hochberuf.", "tokens": ["Wird", "uns\u00b7rer", "Kr\u00e4f\u00b7te", "Hoch\u00b7be\u00b7ruf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Teilnehmend f\u00fchren gute Geister,", "tokens": ["Teil\u00b7neh\u00b7mend", "f\u00fch\u00b7ren", "gu\u00b7te", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gelinde leitend, h\u00f6chste Meister,", "tokens": ["Ge\u00b7lin\u00b7de", "lei\u00b7tend", ",", "h\u00f6chs\u00b7te", "Meis\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu dem, der alles schafft und schuf.", "tokens": ["Zu", "dem", ",", "der", "al\u00b7les", "schafft", "und", "schuf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und umzuschaffen das Geschaffne,", "tokens": ["Und", "um\u00b7zu\u00b7schaf\u00b7fen", "das", "Ge\u00b7schaff\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Damit sich's nicht zum Starren waffne,", "tokens": ["Da\u00b7mit", "sich's", "nicht", "zum", "Star\u00b7ren", "waff\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "PTKNEG", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wirkt ewiges lebendiges Tun.", "tokens": ["Wirkt", "e\u00b7wi\u00b7ges", "le\u00b7ben\u00b7di\u00b7ges", "Tun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und was nicht war, nun will es werden", "tokens": ["Und", "was", "nicht", "war", ",", "nun", "will", "es", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PTKNEG", "VAFIN", "$,", "ADV", "VMFIN", "PPER", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zu reinen Sonnen, farbigen Erden,", "tokens": ["Zu", "rei\u00b7nen", "Son\u00b7nen", ",", "far\u00b7bi\u00b7gen", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "In keinem Falle darf es ruhn.", "tokens": ["In", "kei\u00b7nem", "Fal\u00b7le", "darf", "es", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es soll sich regen, schaffend handeln,", "tokens": ["Es", "soll", "sich", "re\u00b7gen", ",", "schaf\u00b7fend", "han\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJA", "$,", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erst sich gestalten, dann verwandeln;", "tokens": ["Erst", "sich", "ge\u00b7stal\u00b7ten", ",", "dann", "ver\u00b7wan\u00b7deln", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PRF", "VVPP", "$,", "ADV", "VVINF", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur scheinbar steht's Momente still.", "tokens": ["Nur", "schein\u00b7bar", "steht's", "Mo\u00b7men\u00b7te", "still", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Ewige regt sich fort in allen:", "tokens": ["Das", "E\u00b7wi\u00b7ge", "regt", "sich", "fort", "in", "al\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PRF", "PTKVZ", "APPR", "PIAT", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Denn alles mu\u00df in Nichts zerfallen,", "tokens": ["Denn", "al\u00b7les", "mu\u00df", "in", "Nichts", "zer\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn es im Sein beharren will.", "tokens": ["Wenn", "es", "im", "Sein", "be\u00b7har\u00b7ren", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}