{"textgrid.poem.24040": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Die Nonne", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einer Nacht, schw\u00fclhei\u00df, da ich schlief,", "tokens": ["In", "ei\u00b7ner", "Nacht", ",", "schw\u00fcl\u00b7hei\u00df", ",", "da", "ich", "schlief", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Da meine Seele nach Liebe rief", "tokens": ["Da", "mei\u00b7ne", "See\u00b7le", "nach", "Lie\u00b7be", "rief"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In Tr\u00e4umen,", "tokens": ["In", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Da ist einer gekommen;", "tokens": ["Da", "ist", "ei\u00b7ner", "ge\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "VVPP", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Hat mich bei der Hand genommen", "tokens": ["Hat", "mich", "bei", "der", "Hand", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ist fort mit mir gangen:", "tokens": ["Und", "ist", "fort", "mit", "mir", "gan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKVZ", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zwischen schwarzen B\u00e4umen", "tokens": ["Zwi\u00b7schen", "schwar\u00b7zen", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Tief", "tokens": ["Tief"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "In einen Wald voller Rauschen und Bangen.", "tokens": ["In", "ei\u00b7nen", "Wald", "vol\u00b7ler", "Rau\u00b7schen", "und", "Ban\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich sah ihn nicht an", "tokens": ["Ich", "sah", "ihn", "nicht", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PTKVZ"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Den fremden Mann,", "tokens": ["Den", "frem\u00b7den", "Mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mu\u00dfte an ihm hangen,", "tokens": ["Mu\u00df\u00b7te", "an", "ihm", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Als wie im Bann", "tokens": ["Als", "wie", "im", "Bann"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und mit ihm gehn.", "tokens": ["Und", "mit", "ihm", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVINF", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Er war ganz stumm.", "tokens": ["Er", "war", "ganz", "stumm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Aber Fl\u00fcstern ringsum", "tokens": ["A\u00b7ber", "Fl\u00fcs\u00b7tern", "ring\u00b7sum"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und in den B\u00fcschen ein schaurig Wehn", "tokens": ["Und", "in", "den", "B\u00fc\u00b7schen", "ein", "schau\u00b7rig", "Wehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJD", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Und Stimmengesumm.", "tokens": ["Und", "Stim\u00b7men\u00b7ge\u00b7summ", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Unter einer Linden im Walde tiefinnen,", "tokens": ["Un\u00b7ter", "ei\u00b7ner", "Lin\u00b7den", "im", "Wal\u00b7de", "tie\u00b7fin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Da blieb er stehn und lie\u00df mich los.", "tokens": ["Da", "blieb", "er", "stehn", "und", "lie\u00df", "mich", "los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVINF", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sah ich zwei Thr\u00e4nen gro\u00df", "tokens": ["Da", "sah", "ich", "zwei", "Thr\u00e4\u00b7nen", "gro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "CARD", "NN", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ihm aus den Augen rinnen.", "tokens": ["Ihm", "aus", "den", "Au\u00b7gen", "rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und sah, wie sein Antlitz war.", "tokens": ["Und", "sah", ",", "wie", "sein", "Ant\u00b7litz", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Das war wie der Tag so klar,", "tokens": ["Das", "war", "wie", "der", "Tag", "so", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "KOKOM", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Aber voll Trauern.", "tokens": ["A\u00b7ber", "voll", "Trau\u00b7ern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Und es kam ein Erschauern", "tokens": ["Und", "es", "kam", "ein", "Er\u00b7schau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ueber mich kalt,", "tokens": ["Ue\u00b7ber", "mich", "kalt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und in mir eine Gewalt", "tokens": ["Und", "in", "mir", "ei\u00b7ne", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Zwang mich in die Kniee", "tokens": ["Zwang", "mich", "in", "die", "Kni\u00b7ee"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Vor dem stummen Mann:", "tokens": ["Vor", "dem", "stum\u00b7men", "Mann", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "\u00bbherr, Herr, siehe.", "tokens": ["\u00bb", "herr", ",", "Herr", ",", "sie\u00b7he", "."], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "NN", "$,", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Siehe mich an, \u2013;", "tokens": ["Sie\u00b7he", "mich", "an", ",", "\u2013", ";"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "$(", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Was ist dein Wehe?\u00ab", "tokens": ["Was", "ist", "dein", "We\u00b7he", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Da f\u00fchl ich seine Hand", "tokens": ["Da", "f\u00fchl", "ich", "sei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und sehe,", "tokens": ["Und", "se\u00b7he", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Indessen ER verschwand,", "tokens": ["In\u00b7des\u00b7sen", "Er", "ver\u00b7schwand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Leuchten die heiligen Wunden.", "tokens": ["Leuch\u00b7ten", "die", "hei\u00b7li\u00b7gen", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Und habe IHN erkannt,", "tokens": ["Und", "ha\u00b7be", "IhN", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und habe mich heimgefunden", "tokens": ["Und", "ha\u00b7be", "mich", "heim\u00b7ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Aus Wald und Welt,", "tokens": ["Aus", "Wald", "und", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Darinnen Begehren rief,", "tokens": ["Da\u00b7rin\u00b7nen", "Be\u00b7geh\u00b7ren", "rief", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "In einen Frieden tief,", "tokens": ["In", "ei\u00b7nen", "Frie\u00b7den", "tief", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Von IHM erhellt.", "tokens": ["Von", "IhM", "er\u00b7hellt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}