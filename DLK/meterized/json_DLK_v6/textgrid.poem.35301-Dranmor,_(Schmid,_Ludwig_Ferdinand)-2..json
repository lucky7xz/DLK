{"textgrid.poem.35301": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn eine Reise lange w\u00e4hrt,", "tokens": ["Wenn", "ei\u00b7ne", "Rei\u00b7se", "lan\u00b7ge", "w\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Passagiere m\u00fcrrisch werden,", "tokens": ["Die", "Pas\u00b7sa\u00b7gie\u00b7re", "m\u00fcr\u00b7risch", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie da mit Worten und Geberden", "tokens": ["Wie", "da", "mit", "Wor\u00b7ten", "und", "Ge\u00b7ber\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So mancher aus der Rolle f\u00e4hrt,", "tokens": ["So", "man\u00b7cher", "aus", "der", "Rol\u00b7le", "f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der selbstgew\u00e4hlten, unhaltbaren,", "tokens": ["Der", "selbst\u00b7ge\u00b7w\u00e4hl\u00b7ten", ",", "un\u00b7halt\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sich seiner Bl\u00f6\u00dfe nicht mehr sch\u00e4mt,", "tokens": ["Sich", "sei\u00b7ner", "Bl\u00f6\u00b7\u00dfe", "nicht", "mehr", "sch\u00e4mt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Statt dem, der sich im stillen gr\u00e4mt,", "tokens": ["Statt", "dem", ",", "der", "sich", "im", "stil\u00b7len", "gr\u00e4mt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$,", "PRELS", "PRF", "APPRART", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die neue Pr\u00fcfung zu ersparen.", "tokens": ["Die", "neu\u00b7e", "Pr\u00fc\u00b7fung", "zu", "er\u00b7spa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die gro\u00dfen Schmerzen sind verscheucht;", "tokens": ["Die", "gro\u00b7\u00dfen", "Schmer\u00b7zen", "sind", "ver\u00b7scheucht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun gilt es, ruhig auszuharren", "tokens": ["Nun", "gilt", "es", ",", "ru\u00b7hig", "aus\u00b7zu\u00b7har\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bei kleinen Plagen: die Cigarren", "tokens": ["Bei", "klei\u00b7nen", "Pla\u00b7gen", ":", "die", "Ci\u00b7gar\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Beispiel werden immer feucht;", "tokens": ["Zum", "Bei\u00b7spiel", "wer\u00b7den", "im\u00b7mer", "feucht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die derbe Kost nicht zu vergessen,", "tokens": ["Die", "der\u00b7be", "Kost", "nicht", "zu", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Rollen, der kontr\u00e4re Wind \u2013", "tokens": ["Das", "Rol\u00b7len", ",", "der", "kont\u00b7r\u00e4\u00b7re", "Wind", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kurz, einer Schiffskaj\u00fcte sind", "tokens": ["Kurz", ",", "ei\u00b7ner", "Schiffs\u00b7ka\u00b7j\u00fc\u00b7te", "sind"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Freuden sparsam zugemessen.", "tokens": ["Die", "Freu\u00b7den", "spar\u00b7sam", "zu\u00b7ge\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ist vielleicht man, obendrein,", "tokens": ["Und", "ist", "viel\u00b7leicht", "man", ",", "o\u00b7ben\u00b7drein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PIS", "$,", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gen\u00f6tigt, durch Korallenriffe", "tokens": ["Ge\u00b7n\u00f6\u00b7tigt", ",", "durch", "Ko\u00b7ral\u00b7len\u00b7rif\u00b7fe"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVPP", "$,", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf tiefbeladnem leckem Schiffe", "tokens": ["Auf", "tief\u00b7be\u00b7lad\u00b7nem", "le\u00b7ckem", "Schif\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich nachts bei mattem Sternenschein", "tokens": ["Sich", "nachts", "bei", "mat\u00b7tem", "Ster\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und starker Str\u00f6mung durchzuwinden \u2013", "tokens": ["Und", "star\u00b7ker", "Str\u00f6\u00b7mung", "durch\u00b7zu\u00b7win\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Treulose See! Da mag f\u00fcrwahr", "tokens": ["Treu\u00b7lo\u00b7se", "See", "!", "Da", "mag", "f\u00fcr\u00b7wahr"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "ADV", "VMFIN", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "An so romantischer Gefahr", "tokens": ["An", "so", "ro\u00b7man\u00b7ti\u00b7scher", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "+-+-++-+", "measure": "unknown.measure.penta"}, "line.8": {"text": "Ein andrer sein Vergn\u00fcgen finden.", "tokens": ["Ein", "an\u00b7drer", "sein", "Ver\u00b7gn\u00fc\u00b7gen", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}