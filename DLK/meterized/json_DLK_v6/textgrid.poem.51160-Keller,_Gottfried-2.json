{"textgrid.poem.51160": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf seinem Bette liegt Galet,", "tokens": ["Auf", "sei\u00b7nem", "Bet\u00b7te", "liegt", "Ga\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weglachend seines Todes Weh.", "tokens": ["Weg\u00b7la\u00b7chend", "sei\u00b7nes", "To\u00b7des", "Weh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er schickt Panard den Morgengru\u00df,", "tokens": ["Er", "schickt", "Pa\u00b7nard", "den", "Mor\u00b7gen\u00b7gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sechs neue Lieder zum Genu\u00df.", "tokens": ["Sechs", "neu\u00b7e", "Lie\u00b7der", "zum", "Ge\u00b7nu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bberst wollt ich reimen, liebes Kind!", "tokens": ["\u00bb", "erst", "wollt", "ich", "rei\u00b7men", ",", "lie\u00b7bes", "Kind", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So viele, als Apostel sind.", "tokens": ["So", "vie\u00b7le", ",", "als", "A\u00b7pos\u00b7tel", "sind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "KOUS", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch hab ich's nur auf sechs gebracht,", "tokens": ["Doch", "hab", "ich's", "nur", "auf", "sechs", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "ADV", "APPR", "CARD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil schon der Totengr\u00e4ber wacht.", "tokens": ["Weil", "schon", "der", "To\u00b7ten\u00b7gr\u00e4\u00b7ber", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Totengr\u00e4ber an der T\u00fcr", "tokens": ["Der", "To\u00b7ten\u00b7gr\u00e4\u00b7ber", "an", "der", "T\u00fcr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit seinem Spaten lauscht herf\u00fcr.", "tokens": ["Mit", "sei\u00b7nem", "Spa\u00b7ten", "lauscht", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der hackt mich mit den andern sechs", "tokens": ["Der", "hackt", "mich", "mit", "den", "an\u00b7dern", "sechs"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "ART", "ADJA", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bald unter gr\u00fcnes Grasgew\u00e4chs.", "tokens": ["Bald", "un\u00b7ter", "gr\u00fc\u00b7nes", "Gras\u00b7ge\u00b7w\u00e4chs", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Leb wohl, mich d\u00fcnkt, nun mu\u00df es sein,", "tokens": ["Leb", "wohl", ",", "mich", "d\u00fcnkt", ",", "nun", "mu\u00df", "es", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "VVFIN", "$,", "ADV", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der beste Reim ist Rhein und Wein!\u00ab", "tokens": ["Der", "bes\u00b7te", "Reim", "ist", "Rhein", "und", "Wein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NE", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}