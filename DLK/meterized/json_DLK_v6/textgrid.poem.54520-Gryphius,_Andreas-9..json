{"textgrid.poem.54520": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du siehst es wil bey uns nun mehr denn finster werden/", "tokens": ["Du", "siehst", "es", "wil", "bey", "uns", "nun", "mehr", "denn", "fins\u00b7ter", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VMFIN", "APPR", "PPER", "ADV", "ADV", "ADV", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der reinen Liebe Flamm und heisser Andacht Gluth", "tokens": ["Der", "rei\u00b7nen", "Lie\u00b7be", "Flamm", "und", "heis\u00b7ser", "An\u00b7dacht", "Gluth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NE", "KON", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erkaltet und verl\u00f6scht/ der tollen Laster Fluth", "tokens": ["Er\u00b7kal\u00b7tet", "und", "ver\u00b7l\u00f6scht", "/", "der", "tol\u00b7len", "Las\u00b7ter", "Fluth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "VVFIN", "$(", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Reist ein/ und \u00fcberschwemmt den Krei\u00df best\u00fcrtzter Erden.", "tokens": ["Reist", "ein", "/", "und", "\u00fc\u00b7bersc\u00b7hwemmt", "den", "Krei\u00df", "be\u00b7st\u00fcrtz\u00b7ter", "Er\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$(", "KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wir zagen sonder Trost in schrecklichen Beschwerden/", "tokens": ["Wir", "za\u00b7gen", "son\u00b7der", "Trost", "in", "schreck\u00b7li\u00b7chen", "Be\u00b7schwer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "KON", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der ehrne Himmel dr\u00e4ut mit einer neuen Ruth/", "tokens": ["Der", "ehr\u00b7ne", "Him\u00b7mel", "dr\u00e4ut", "mit", "ei\u00b7ner", "neu\u00b7en", "Ruth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Str\u00f6me trocknen ein/ die Felsen schwitzen Blut/", "tokens": ["Die", "Str\u00f6\u00b7me", "trock\u00b7nen", "ein", "/", "die", "Fel\u00b7sen", "schwit\u00b7zen", "Blut", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "ART", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Hirten sind verzagt/ verirret sind die Heerden", "tokens": ["Die", "Hir\u00b7ten", "sind", "ver\u00b7zagt", "/", "ver\u00b7ir\u00b7ret", "sind", "die", "Heer\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "VVPP", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "O rei\u00df von Himmel ab! du Sturm/ der Feinde Schrecken/", "tokens": ["O", "rei\u00df", "von", "Him\u00b7mel", "ab", "!", "du", "Sturm", "/", "der", "Fein\u00b7de", "Schre\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "PTKVZ", "$.", "PPER", "NN", "$(", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Du keusche Gluth/ die uns mit Liebe kan entstecken/", "tokens": ["Du", "keu\u00b7sche", "Gluth", "/", "die", "uns", "mit", "Lie\u00b7be", "kan", "ent\u00b7ste\u00b7cken", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "PRELS", "PPER", "APPR", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Du Thau/ der uns mit Trost in dieser Angst erquickt/", "tokens": ["Du", "Thau", "/", "der", "uns", "mit", "Trost", "in", "die\u00b7ser", "Angst", "er\u00b7quickt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$(", "PRELS", "PPER", "APPR", "NN", "APPR", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Du Licht/ das unsre Nacht und Tunckelheit vertreibet/", "tokens": ["Du", "Licht", "/", "das", "uns\u00b7re", "Nacht", "und", "Tun\u00b7ckel\u00b7heit", "ver\u00b7trei\u00b7bet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$(", "ART", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Du Krafft/ durch die ein Hertz/ wenn alles f\u00e4llt/ doch bleibet:", "tokens": ["Du", "Krafft", "/", "durch", "die", "ein", "Hertz", "/", "wenn", "al\u00b7les", "f\u00e4llt", "/", "doch", "blei\u00b7bet", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$(", "APPR", 