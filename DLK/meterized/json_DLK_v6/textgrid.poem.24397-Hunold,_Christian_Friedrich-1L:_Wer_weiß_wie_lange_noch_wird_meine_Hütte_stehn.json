{"textgrid.poem.24397": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer wei\u00df/ wie lange noch wird meine H\u00fctte stehn/", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer wei\u00df/ wie lange noch wird meine H\u00fctte stehn/", "tokens": ["Wer", "wei\u00df", "/", "wie", "lan\u00b7ge", "noch", "wird", "mei\u00b7ne", "H\u00fct\u00b7te", "stehn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "PWAV", "ADV", "ADV", "VAFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So seh ich sie vieleicht auch in der Gluth vergehn.", "tokens": ["So", "seh", "ich", "sie", "vie\u00b7leicht", "auch", "in", "der", "Gluth", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gott/ straffet mich nun auch mit Feuer deine Hand/", "tokens": ["Gott", "/", "straf\u00b7fet", "mich", "nun", "auch", "mit", "Feu\u00b7er", "dei\u00b7ne", "Hand", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVFIN", "PPER", "ADV", "ADV", "APPR", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So sey dein Zorn auf mich nur ewig nicht entbrant.", "tokens": ["So", "sey", "dein", "Zorn", "auf", "mich", "nur", "e\u00b7wig", "nicht", "ent\u00b7brant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos