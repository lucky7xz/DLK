{"textgrid.poem.59906": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es kommen die Mission\u00e4re", "tokens": ["Es", "kom\u00b7men", "die", "Mis\u00b7si\u00b7o\u00b7n\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Zu uns vom Abendlande", "tokens": ["Zu", "uns", "vom", "A\u00b7bend\u00b7lan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und predigen fromme M\u00e4re", "tokens": ["Und", "pre\u00b7di\u00b7gen", "from\u00b7me", "M\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In schwarzem Bu\u00dfgewande:", "tokens": ["In", "schwar\u00b7zem", "Bu\u00df\u00b7ge\u00b7wan\u00b7de", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie alle Welt verdorben,", "tokens": ["Wie", "al\u00b7le", "Welt", "ver\u00b7dor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Versunken ganz im B\u00f6sen,", "tokens": ["Ver\u00b7sun\u00b7ken", "ganz", "im", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wie der Christ gestorben,", "tokens": ["Und", "wie", "der", "Christ", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Menschheit zu erl\u00f6sen.", "tokens": ["Die", "Menschheit", "zu", "er\u00b7l\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bbwir wurden auserkoren,", "tokens": ["\u00bb", "wir", "wur\u00b7den", "au\u00b7ser\u00b7ko\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die M\u00e4re zu verbreiten;", "tokens": ["Die", "M\u00e4\u00b7re", "zu", "ver\u00b7brei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer zweifelt, ist verloren", "tokens": ["Wer", "zwei\u00b7felt", ",", "ist", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr alle Ewigkeiten!\u00ab", "tokens": ["F\u00fcr", "al\u00b7le", "E\u00b7wig\u00b7kei\u00b7ten", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbihr wandelt dunkle Wege,", "tokens": ["\u00bb", "ihr", "wan\u00b7delt", "dunk\u00b7le", "We\u00b7ge", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir f\u00fchren euch zur Klarheit.\u00ab", "tokens": ["Wir", "f\u00fch\u00b7ren", "euch", "zur", "Klar\u00b7heit", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u2013 Doch: wer gibt mir Belege", "tokens": ["\u2013", "Doch", ":", "wer", "gibt", "mir", "Be\u00b7le\u00b7ge"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "KON", "$.", "PWS", "VVFIN", "PPER", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr eurer Worte Wahrheit?", "tokens": ["F\u00fcr", "eu\u00b7rer", "Wor\u00b7te", "Wahr\u00b7heit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich komme nicht zu Ende", "tokens": ["Ich", "kom\u00b7me", "nicht", "zu", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Guten wie im B\u00f6sen,", "tokens": ["Im", "Gu\u00b7ten", "wie", "im", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn nicht Hafisas H\u00e4nde", "tokens": ["Wenn", "nicht", "Ha\u00b7fi\u00b7sas", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die dunklen Zweifel l\u00f6sen.", "tokens": ["Die", "dunk\u00b7len", "Zwei\u00b7fel", "l\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du sch\u00f6ne Mission\u00e4rin!", "tokens": ["Du", "sch\u00f6\u00b7ne", "Mis\u00b7si\u00b7o\u00b7n\u00e4\u00b7rin", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Lehr' du mich Religion:", "tokens": ["Lehr'", "du", "mich", "Re\u00b7li\u00b7gi\u00b7on", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bei dir liegt die Gew\u00e4hr in", "tokens": ["Bei", "dir", "liegt", "die", "Ge\u00b7w\u00e4hr", "in"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "APPR"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Dem Blick des Auges schon.", "tokens": ["Dem", "Blick", "des", "Au\u00b7ges", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}