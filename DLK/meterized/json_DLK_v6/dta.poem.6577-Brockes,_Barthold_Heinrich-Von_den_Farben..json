{"dta.poem.6577": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Farben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir k\u00f6nnen noch verschiedne Gr\u00fcnde finden", "tokens": ["Wir", "k\u00f6n\u00b7nen", "noch", "ver\u00b7schied\u00b7ne", "Gr\u00fcn\u00b7de", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von dem, was eigentlich gef\u00e4rbte C\u00f6rper sind.", "tokens": ["Von", "dem", ",", "was", "ei\u00b7gent\u00b7lich", "ge\u00b7f\u00e4rb\u00b7te", "C\u00f6r\u00b7per", "sind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADV", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es k\u00f6nnten Menschen, welche blind,", "tokens": ["Es", "k\u00f6nn\u00b7ten", "Men\u00b7schen", ",", "wel\u00b7che", "blind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wol eh\u2019, ob Farben wei\u00df, wie oder schwartz, ergr\u00fcnden.", "tokens": ["Wol", "eh'", ",", "ob", "Far\u00b7ben", "wei\u00df", ",", "wie", "o\u00b7der", "schwartz", ",", "er\u00b7gr\u00fcn\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "NN", "VVFIN", "$,", "PWAV", "KON", "ADJD", "$,", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Sie haben, wie man sagt, gef\u00fchlt, an statt zu sehn,", "tokens": ["Sie", "ha\u00b7ben", ",", "wie", "man", "sagt", ",", "ge\u00b7f\u00fchlt", ",", "an", "statt", "zu", "sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWAV", "PIS", "VVFIN", "$,", "VVPP", "$,", "APPR", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und an der Fl\u00e4chen Tieff- und H\u00f6hn,", "tokens": ["Und", "an", "der", "Fl\u00e4\u00b7chen", "Tief\u00b7f", "und", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Ob selbe schwartz, ob selbe wei\u00df zu nennen;", "tokens": ["Ob", "sel\u00b7be", "schwartz", ",", "ob", "sel\u00b7be", "wei\u00df", "zu", "nen\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,", "KOUS", "ADJA", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Gar eigentlich begreiffen k\u00f6nnen.", "tokens": ["Gar", "ei\u00b7gent\u00b7lich", "be\u00b7greif\u00b7fen", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Gewohnheit oder Noth, die alles leichte macht,", "tokens": ["Ge\u00b7wohn\u00b7heit", "o\u00b7der", "Noth", ",", "die", "al\u00b7les", "leich\u00b7te", "macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PRELS", "PIAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da sie die Trauer-Nacht der Augen halb verbannt,", "tokens": ["Da", "sie", "die", "Trau\u00b7e\u00b7rNacht", "der", "Au\u00b7gen", "halb", "ver\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Hat eine zarte Krafft in ihre Finger bracht.", "tokens": ["Hat", "ei\u00b7ne", "zar\u00b7te", "Krafft", "in", "ih\u00b7re", "Fin\u00b7ger", "bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Jhr forschendes Gef\u00fchl der Nerven in der Hand,", "tokens": ["Ihr", "for\u00b7schen\u00b7des", "Ge\u00b7f\u00fchl", "der", "Ner\u00b7ven", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "K\u00f6nnt\u2019 eben so, als wie die Nerven ihrer Augen,", "tokens": ["K\u00f6nnt'", "e\u00b7ben", "so", ",", "als", "wie", "die", "Ner\u00b7ven", "ih\u00b7rer", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "$,", "KOUS", "KOKOM", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der Farben Unterscheid zu fassen taugen.", "tokens": ["Der", "Far\u00b7ben", "Un\u00b7ter\u00b7scheid", "zu", "fas\u00b7sen", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}