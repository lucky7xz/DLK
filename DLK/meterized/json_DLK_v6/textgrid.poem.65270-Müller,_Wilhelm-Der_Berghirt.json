{"textgrid.poem.65270": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Berghirt", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn auf dem h\u00f6chsten Fels ich steh',", "tokens": ["Wenn", "auf", "dem", "h\u00f6chs\u00b7ten", "Fels", "ich", "steh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In's tiefe Thal hernieder seh'", "tokens": ["In's", "tie\u00b7fe", "Thal", "her\u00b7nie\u00b7der", "seh'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und singe,", "tokens": ["Und", "sin\u00b7ge", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Fern aus dem tiefen dunkeln Thal", "tokens": ["Fern", "aus", "dem", "tie\u00b7fen", "dun\u00b7keln", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schwingt sich empor der Wiederhall", "tokens": ["Schwingt", "sich", "em\u00b7por", "der", "Wie\u00b7der\u00b7hall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PTKVZ", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Kl\u00fcfte.", "tokens": ["Der", "Kl\u00fcf\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Je weiter meine Stimme dringt,", "tokens": ["Je", "wei\u00b7ter", "mei\u00b7ne", "Stim\u00b7me", "dringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Je heller sie mir wiederklingt", "tokens": ["Je", "hel\u00b7ler", "sie", "mir", "wie\u00b7der\u00b7klingt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von unten.", "tokens": ["Von", "un\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "ADV", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Mein Liebchen wohnt so fern von mir,", "tokens": ["Mein", "Lieb\u00b7chen", "wohnt", "so", "fern", "von", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum sehn' ich mich so hei\u00df nach ihr", "tokens": ["Drum", "sehn'", "ich", "mich", "so", "hei\u00df", "nach", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "APPR", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hin\u00fcber!", "tokens": ["Hin\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "Viel steile Berge vor mir stehn,", "tokens": ["Viel", "stei\u00b7le", "Ber\u00b7ge", "vor", "mir", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Fl\u00fcsse sch\u00e4umend sich ergehn", "tokens": ["Die", "Fl\u00fcs\u00b7se", "sch\u00e4u\u00b7mend", "sich", "er\u00b7gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "PRF", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Thale.", "tokens": ["Im", "Tha\u00b7le", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.6": {"line.1": {"text": "Der Aar sich in die Wolken schwingt,", "tokens": ["Der", "Aar", "sich", "in", "die", "Wol\u00b7ken", "schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Gemse durch die Kl\u00fcfte springt", "tokens": ["Die", "Gem\u00b7se", "durch", "die", "Kl\u00fcf\u00b7te", "springt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hin\u00fcber!", "tokens": ["Hin\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.7": {"line.1": {"text": "Die Wolken ruhen auf der H\u00f6h',", "tokens": ["Die", "Wol\u00b7ken", "ru\u00b7hen", "auf", "der", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und durch die Nebel gl\u00e4nzt der Schnee", "tokens": ["Und", "durch", "die", "Ne\u00b7bel", "gl\u00e4nzt", "der", "Schnee"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Gipfel.", "tokens": ["Der", "Gip\u00b7fel", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.8": {"line.1": {"text": "Je stolzer mir mein M\u00e4dchen thut,", "tokens": ["Je", "stol\u00b7zer", "mir", "mein", "M\u00e4d\u00b7chen", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Je h\u00f6her steigt empor mein Muth", "tokens": ["Je", "h\u00f6\u00b7her", "steigt", "em\u00b7por", "mein", "Muth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PTKVZ", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Liebe.", "tokens": ["In", "Lie\u00b7be", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.9": {"line.1": {"text": "Ein Gl\u00f6ckchen klingt im stillen Thal,", "tokens": ["Ein", "Gl\u00f6ck\u00b7chen", "klingt", "im", "stil\u00b7len", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Essen rauchen \u00fcberall", "tokens": ["Die", "Es\u00b7sen", "rau\u00b7chen", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Dorfe.", "tokens": ["Im", "Dor\u00b7fe", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.10": {"line.1": {"text": "Ach, M\u00e4dchen, M\u00e4dchen, nimm mich bald!", "tokens": ["Ach", ",", "M\u00e4d\u00b7chen", ",", "M\u00e4d\u00b7chen", ",", "nimm", "mich", "bald", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "NN", "$,", "VVIMP", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es ist so \u00f6d', es ist so kalt", "tokens": ["Es", "ist", "so", "\u00f6d'", ",", "es", "ist", "so", "kalt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier oben.", "tokens": ["Hier", "o\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}