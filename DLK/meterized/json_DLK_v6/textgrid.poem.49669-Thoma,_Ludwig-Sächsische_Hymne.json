{"textgrid.poem.49669": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "S\u00e4chsische Hymne", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Singt mir das Lied vom Sachsenbrinzen,", "tokens": ["Singt", "mir", "das", "Lied", "vom", "Sach\u00b7sen\u00b7brin\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Brinzen Maximilichan,", "tokens": ["Vom", "Brin\u00b7zen", "Ma\u00b7xi\u00b7mi\u00b7li\u00b7chan", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der ohne mit dem Aug' zu blinzen", "tokens": ["Der", "oh\u00b7ne", "mit", "dem", "Aug'", "zu", "blin\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Babste zeicht den Digerzahn!", "tokens": ["Dem", "Babs\u00b7te", "zeicht", "den", "Di\u00b7ger\u00b7zahn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Singt mir das Lied des dapfern Streiters,", "tokens": ["Singt", "mir", "das", "Lied", "des", "dap\u00b7fern", "Strei\u00b7ters", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der sich als edler Sachsensohn", "tokens": ["Der", "sich", "als", "ed\u00b7ler", "Sach\u00b7sen\u00b7sohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der r\u00f6mschen G\u00e4rche ohne weiters", "tokens": ["Der", "r\u00f6m\u00b7schen", "G\u00e4r\u00b7che", "oh\u00b7ne", "wei\u00b7ters"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Entgegengestellt mit scharfem Ton!", "tokens": ["Ent\u00b7ge\u00b7gen\u00b7ge\u00b7stellt", "mit", "schar\u00b7fem", "Ton", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "----+-+-+", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Wie mu\u00df der zehnte Bius scheimen", "tokens": ["Wie", "mu\u00df", "der", "zehn\u00b7te", "Bius", "schei\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "In hintert\u00fcckscher Briesterwut,", "tokens": ["In", "hin\u00b7ter\u00b7t\u00fcck\u00b7scher", "Bries\u00b7ter\u00b7wut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df sich zu kiehnen Freiheitsdreimen", "tokens": ["Da\u00df", "sich", "zu", "kieh\u00b7nen", "Frei\u00b7heits\u00b7drei\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erfrecht des Brinzen L\u00f6wenmut,", "tokens": ["Er\u00b7frecht", "des", "Brin\u00b7zen", "L\u00f6\u00b7wen\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der in gadollscher M\u00e4nner Reihe", "tokens": ["Der", "in", "ga\u00b7doll\u00b7scher", "M\u00e4n\u00b7ner", "Rei\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der \u00dcberzeichung Blatz gew\u00e4hrt", "tokens": ["Der", "\u00dc\u00b7ber\u00b7zeic\u00b7hung", "Blatz", "ge\u00b7w\u00e4hrt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und mit der \u00e4chten deitschen Dreie", "tokens": ["Und", "mit", "der", "\u00e4ch\u00b7ten", "deit\u00b7schen", "Drei\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nach altgermanscher Art verf\u00e4hrt!", "tokens": ["Nach", "alt\u00b7ger\u00b7man\u00b7scher", "Art", "ver\u00b7f\u00e4hrt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dein Sachsenland steht dir zur Seite", "tokens": ["Dein", "Sach\u00b7sen\u00b7land", "steht", "dir", "zur", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf diesem Freiheitsgampfesfeld,", "tokens": ["Auf", "die\u00b7sem", "Frei\u00b7heits\u00b7gamp\u00b7fes\u00b7feld", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein teirer Brinz! Und machste Pleite,", "tokens": ["Mein", "tei\u00b7rer", "Brinz", "!", "Und", "machs\u00b7te", "Plei\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "KON", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So bist du doch ein Glaubensheld!", "tokens": ["So", "bist", "du", "doch", "ein", "Glau\u00b7bens\u00b7held", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dir hat die Freiheit doch ged\u00e4mmert", "tokens": ["Dir", "hat", "die", "Frei\u00b7heit", "doch", "ge\u00b7d\u00e4m\u00b7mert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In diesem t\u00fcckschen Briesterstrau\u00df.", "tokens": ["In", "die\u00b7sem", "t\u00fcck\u00b7schen", "Bries\u00b7ter\u00b7strau\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nu komm, mei Kutester, bel\u00e4mmert", "tokens": ["Nu", "komm", ",", "mei", "Ku\u00b7tes\u00b7ter", ",", "be\u00b7l\u00e4m\u00b7mert"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "$,", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In das geliebte G\u00e4nigshaus!", "tokens": ["In", "das", "ge\u00b7lieb\u00b7te", "G\u00e4\u00b7nigs\u00b7haus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}