{"textgrid.poem.56900": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Singe, o singe dich, Seele,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Singe, o singe dich, Seele,", "tokens": ["Sin\u00b7ge", ",", "o", "sin\u00b7ge", "dich", ",", "See\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "FM", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "\u00fcber den Eintag empor in die", "tokens": ["\u00fc\u00b7ber", "den", "Ein\u00b7tag", "em\u00b7por", "in", "die"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKVZ", "APPR", "ART"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "himmlischen Reiche der Sch\u00f6nheit!", "tokens": ["himm\u00b7li\u00b7schen", "Rei\u00b7che", "der", "Sch\u00f6n\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NE", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Bade in goldenen Str\u00f6men der T\u00f6ne dich rein", "tokens": ["Ba\u00b7de", "in", "gol\u00b7de\u00b7nen", "Str\u00f6\u00b7men", "der", "T\u00f6\u00b7ne", "dich", "rein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "ART", "NN", "PPER", "ADJD"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.5": {"text": "vom Staube der Sorgen!", "tokens": ["vom", "Stau\u00b7be", "der", "Sor\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Was dir die Welt geraubt, vergi\u00df es!", "tokens": ["Was", "dir", "die", "Welt", "ge\u00b7raubt", ",", "ver\u00b7gi\u00df", "es", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVPP", "$,", "VVIMP", "PPER", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was dir dein Los verwehrt,", "tokens": ["Was", "dir", "dein", "Los", "ver\u00b7wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "genie\u00df es im Traum!", "tokens": ["ge\u00b7nie\u00df", "es", "im", "Traum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Auf klingenden Wellen", "tokens": ["Auf", "klin\u00b7gen\u00b7den", "Wel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "kommen die heimlichsten Wunder", "tokens": ["kom\u00b7men", "die", "heim\u00b7lichs\u00b7ten", "Wun\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "wie D\u00fcfte", "tokens": ["wie", "D\u00fcf\u00b7te"], "token_info": ["word", "word"], "pos": ["PWAV", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "ferner G\u00e4rten", "tokens": ["fer\u00b7ner", "G\u00e4r\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADV", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "zu deinen leis zitternden Sinnen.", "tokens": ["zu", "dei\u00b7nen", "leis", "zit\u00b7tern\u00b7den", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Singe, singe, Seele des Menschen,", "tokens": ["Sin\u00b7ge", ",", "sin\u00b7ge", ",", "See\u00b7le", "des", "Men\u00b7schen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "vom Grauen der N\u00e4chte bedroht,", "tokens": ["vom", "Grau\u00b7en", "der", "N\u00e4ch\u00b7te", "be\u00b7droht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "dich empor,", "tokens": ["dich", "em\u00b7por", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "wo, lichtumg\u00fcrtet,", "tokens": ["wo", ",", "lich\u00b7tum\u00b7g\u00fcr\u00b7tet", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["PWAV", "$,", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "der Phantasien", "tokens": ["der", "Phan\u00b7ta\u00b7si\u00b7en"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "--+-+", "measure": "anapaest.init"}, "line.6": {"text": "jungfr\u00e4ulicher Reigen", "tokens": ["jung\u00b7fr\u00e4u\u00b7li\u00b7cher", "Rei\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "die zierlichen F\u00fc\u00dfe", "tokens": ["die", "zier\u00b7li\u00b7chen", "F\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "auf nie verbl\u00fchenden Wiesen", "tokens": ["auf", "nie", "ver\u00b7bl\u00fc\u00b7hen\u00b7den", "Wie\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "verf\u00fchrerisch setzt.", "tokens": ["ver\u00b7f\u00fch\u00b7re\u00b7risch", "setzt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}