{"textgrid.poem.31721": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich lie\u00df das Ro\u00df zu Tale lenken,", "tokens": ["Ich", "lie\u00df", "das", "Ro\u00df", "zu", "Ta\u00b7le", "len\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da traf ich zwei Gesellen fein,", "tokens": ["Da", "traf", "ich", "zwei", "Ge\u00b7sel\u00b7len", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das war in einer alten Schenken", "tokens": ["Das", "war", "in", "ei\u00b7ner", "al\u00b7ten", "Schen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der rote und der wei\u00dfe Wein.", "tokens": ["Der", "ro\u00b7te", "und", "der", "wei\u00b7\u00dfe", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie sahn mich an aus gro\u00dfen Kr\u00fcgen,", "tokens": ["Sie", "sahn", "mich", "an", "aus", "gro\u00b7\u00dfen", "Kr\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Gold und Rosen schauten sie.", "tokens": ["Wie", "Gold", "und", "Ro\u00b7sen", "schau\u00b7ten", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Herz empfand ein still Vergn\u00fcgen,", "tokens": ["Mein", "Herz", "emp\u00b7fand", "ein", "still", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir ward, ich wu\u00dfte selbst nicht wie.", "tokens": ["Mir", "ward", ",", "ich", "wu\u00df\u00b7te", "selbst", "nicht", "wie", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VVFIN", "ADV", "PTKNEG", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Kaum sah ich hell den Wei\u00dfen funkeln,", "tokens": ["Kaum", "sah", "ich", "hell", "den", "Wei\u00b7\u00dfen", "fun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da half kein Bitten und kein Flehn.", "tokens": ["Da", "half", "kein", "Bit\u00b7ten", "und", "kein", "Flehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sah ich, ach, den Roten, Dunkeln \u2013", "tokens": ["Und", "sah", "ich", ",", "ach", ",", "den", "Ro\u00b7ten", ",", "Dun\u00b7keln", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ITJ", "$,", "ART", "NN", "$,", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da war es gleich um mich geschehn!", "tokens": ["Da", "war", "es", "gleich", "um", "mich", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wollt wandern ich am Morgen gerne:", "tokens": ["Wollt", "wan\u00b7dern", "ich", "am", "Mor\u00b7gen", "ger\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "PPER", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sah mich der Rote lockend an.", "tokens": ["Sah", "mich", "der", "Ro\u00b7te", "lo\u00b7ckend", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wollt ich ziehn beim Glanz der Sterne \u2013", "tokens": ["Und", "wollt", "ich", "ziehn", "beim", "Glanz", "der", "Ster\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hatt's mir der Wei\u00dfe angetan!", "tokens": ["Hatt's", "mir", "der", "Wei\u00b7\u00dfe", "an\u00b7ge\u00b7tan", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Mir war's, zwei tolle Teufel zwackten,", "tokens": ["Mir", "wa\u00b7r's", ",", "zwei", "tol\u00b7le", "Teu\u00b7fel", "zwack\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "CARD", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der ein am Bart mich armen Tropf,", "tokens": ["Der", "ein", "am", "Bart", "mich", "ar\u00b7men", "Tropf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "APPRART", "NN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Indes des andern F\u00e4uste packten", "tokens": ["In\u00b7des", "des", "an\u00b7dern", "F\u00e4us\u00b7te", "pack\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und z\u00f6gen mich an meinem Zopf.", "tokens": ["Und", "z\u00f6\u00b7gen", "mich", "an", "mei\u00b7nem", "Zopf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sie zogen mich von Nacht bis Morgen,", "tokens": ["Sie", "zo\u00b7gen", "mich", "von", "Nacht", "bis", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zwackten von Woche mich zu Mond:", "tokens": ["Zwack\u00b7ten", "von", "Wo\u00b7che", "mich", "zu", "Mond", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPER", "APPR", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und Jahr und Tag hab ich verborgen", "tokens": ["Und", "Jahr", "und", "Tag", "hab", "ich", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bei den Gesellen schon gewohnt.", "tokens": ["Bei", "den", "Ge\u00b7sel\u00b7len", "schon", "ge\u00b7wohnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Nun oft, wenn in den Lindenb\u00e4umen", "tokens": ["Nun", "oft", ",", "wenn", "in", "den", "Lin\u00b7den\u00b7b\u00e4u\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der stille Mond spazierengeht:", "tokens": ["Der", "stil\u00b7le", "Mond", "spa\u00b7zie\u00b7ren\u00b7geht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da ist's, da\u00df mir ein seltsam Tr\u00e4umen", "tokens": ["Da", "ist's", ",", "da\u00df", "mir", "ein", "selt\u00b7sam", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "KOUS", "PPER", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Leis schauernd durch die Seele weht.", "tokens": ["Leis", "schau\u00b7ernd", "durch", "die", "See\u00b7le", "weht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Da tr\u00e4um ich wohl: die alte Schenke,", "tokens": ["Da", "tr\u00e4um", "ich", "wohl", ":", "die", "al\u00b7te", "Schen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die w\u00fcrde endlich still und leer \u2013", "tokens": ["Die", "w\u00fcr\u00b7de", "end\u00b7lich", "still", "und", "leer", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie brach zusammen \u2013 und ich tr\u00e4nke", "tokens": ["Sie", "brach", "zu\u00b7sam\u00b7men", "\u2013", "und", "ich", "tr\u00e4n\u00b7ke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$(", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl nimmer Oberingelheimer mehr.", "tokens": ["Wohl", "nim\u00b7mer", "O\u00b7be\u00b7rin\u00b7gel\u00b7hei\u00b7mer", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}