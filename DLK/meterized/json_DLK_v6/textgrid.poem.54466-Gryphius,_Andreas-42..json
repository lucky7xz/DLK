{"textgrid.poem.54466": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gleich als ein wanders mann'/ im fall die tr\u00fcbe nacht", "tokens": ["Gleich", "als", "ein", "wan\u00b7ders", "mann'", "/", "im", "fall", "die", "tr\u00fc\u00b7be", "nacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "$(", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mitt dicker f\u00fcnsternus lufft erdt vndt see verdecket", "tokens": ["Mitt", "di\u00b7cker", "f\u00fcns\u00b7ter\u00b7nus", "lufft", "erdt", "vndt", "see", "ver\u00b7de\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Irt trawrig hin vnd her/ vnd mitt vill furcht erschrecket'", "tokens": ["Irt", "traw\u00b7rig", "hin", "vnd", "her", "/", "vnd", "mitt", "vill", "furcht", "er\u00b7schre\u00b7cket'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "PTKVZ", "KON", "PTKVZ", "$(", "KON", "APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.4": {"text": "Weis nirgendts wo er geht; siht nimmer was er macht:", "tokens": ["Weis", "nir\u00b7gendts", "wo", "er", "geht", ";", "siht", "nim\u00b7mer", "was", "er", "macht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PWAV", "PPER", "VVFIN", "$.", "VVFIN", "ADV", "PWS", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Nicht anders ists mit mir. doch wen der mond' auffwacht", "tokens": ["Nicht", "an\u00b7ders", "ists", "mit", "mir", ".", "doch", "wen", "der", "mond'", "auff\u00b7wacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VAFIN", "APPR", "PPER", "$.", "KON", "PWS", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vndt seiner stralen kertz' im wolcken-haus anstecket", "tokens": ["Vndt", "sei\u00b7ner", "stra\u00b7len", "kertz'", "im", "wol\u00b7cken\u00b7haus", "an\u00b7ste\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bald findt er weg vnd raht/ so wirdt mein Geist erwecket:", "tokens": ["Bald", "findt", "er", "weg", "vnd", "raht", "/", "so", "wirdt", "mein", "Geist", "er\u00b7we\u00b7cket", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "NN", "$(", "ADV", "VAFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wen jhr mich ewrer gunst vndt schreiben w\u00fcrdig acht.", "tokens": ["Wen", "jhr", "mich", "ew\u00b7rer", "gunst", "vndt", "schrei\u00b7ben", "w\u00fcr\u00b7dig", "acht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PRF", "PPOSAT", "NN", "KON", "VVFIN", "ADJD", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Last ja den leit-stern fest wol ed'le Jungfraw stehen.", "tokens": ["Last", "ja", "den", "leit\u00b7stern", "fest", "wol", "ed'\u00b7le", "Jung\u00b7fraw", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.10": {"text": "Last ja dis sch\u00f6ne licht mir nimmer vntergehen:", "tokens": ["Last", "ja", "dis", "sch\u00f6\u00b7ne", "licht", "mir", "nim\u00b7mer", "vn\u00b7ter\u00b7ge\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PDS", "ADJA", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.11": {"text": "Das licht/ drin redlikeit vnd tugend sich ergetzt.", "tokens": ["Das", "licht", "/", "drin", "red\u00b7li\u00b7keit", "vnd", "tu\u00b7gend", "sich", "er\u00b7getzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "VVFIN", "KON", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wehrt bin ich nicht das jhr mir was ich wil gewehret", "tokens": ["Wehrt", "bin", "ich", "nicht", "das", "jhr", "mir", "was", "ich", "wil", "ge\u00b7weh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "PTKNEG", "ART", "PPER", "PPER", "PWS", "PPER", "VMFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch w\u00e4rmbt der Sonnen glantz was frost vnd schnee verheeret.", "tokens": ["Doch", "w\u00e4rmbt", "der", "Son\u00b7nen", "glantz", "was", "frost", "vnd", "schnee", "ver\u00b7hee\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "NN", "PWS", "ADJD", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wie niedrig bis auch ligt wie hoch sie wirdt gesch\u00e4tzt.", "tokens": ["Wie", "nied\u00b7rig", "bis", "auch", "ligt", "wie", "hoch", "sie", "wirdt", "ge\u00b7sch\u00e4tzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "ADV", "VVFIN", "KOKOM", "ADJD", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}