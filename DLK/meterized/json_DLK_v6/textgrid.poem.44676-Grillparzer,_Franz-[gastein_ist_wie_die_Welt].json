{"textgrid.poem.44676": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[gastein ist wie die Welt]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gastein ist wie die Welt.", "tokens": ["Gas\u00b7tein", "ist", "wie", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KOKOM", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Das Beste, was uns drin zu Teile f\u00e4llt,", "tokens": ["Das", "Bes\u00b7te", ",", "was", "uns", "drin", "zu", "Tei\u00b7le", "f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist wohl, trotz dem, was wir dar\u00fcber lesen,", "tokens": ["Ist", "wohl", ",", "trotz", "dem", ",", "was", "wir", "da\u00b7r\u00fc\u00b7ber", "le\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "APPR", "ART", "$,", "PRELS", "PPER", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Hoffnung stets, die Tr\u00f6sterin gewesen.", "tokens": ["Die", "Hoff\u00b7nung", "stets", ",", "die", "Tr\u00f6s\u00b7te\u00b7rin", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch geht man \u2013 aus der Welt wie aus Gastein \u2013", "tokens": ["Doch", "geht", "man", "\u2013", "aus", "der", "Welt", "wie", "aus", "Gas\u00b7tein", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$(", "APPR", "ART", "NN", "KOKOM", "APPR", "NN", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Stellt Gl\u00fcck und Heilung sich wohl sp\u00e4ter ein.", "tokens": ["Stellt", "Gl\u00fcck", "und", "Hei\u00b7lung", "sich", "wohl", "sp\u00e4\u00b7ter", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PRF", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}