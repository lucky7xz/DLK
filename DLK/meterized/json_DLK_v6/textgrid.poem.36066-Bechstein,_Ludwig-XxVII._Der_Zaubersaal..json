{"textgrid.poem.36066": {"metadata": {"author": {"name": "Bechstein, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "XxVII. Der Zaubersaal.", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heil, ewges Heil dem Herrscher, der Kunst und Musen liebt;", "tokens": ["Heil", ",", "ew\u00b7ges", "Heil", "dem", "Herr\u00b7scher", ",", "der", "Kunst", "und", "Mu\u00b7sen", "liebt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der, f\u00fcr das Gute gl\u00fchend, zugleich auch Sch\u00f6nes \u00fcbt!", "tokens": ["Der", ",", "f\u00fcr", "das", "Gu\u00b7te", "gl\u00fc\u00b7hend", ",", "zu\u00b7gleich", "auch", "Sch\u00f6\u00b7nes", "\u00fcbt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "NN", "ADJD", "$,", "ADV", "ADV", "NE", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Denn Gutes ist die Richtschnur, bestimmend Menschenwerth,", "tokens": ["Denn", "Gu\u00b7tes", "ist", "die", "Richt\u00b7schnur", ",", "be\u00b7stim\u00b7mend", "Men\u00b7schen\u00b7werth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Doch Sch\u00f6nes ist der Lichtstrahl, der uns zum Gott verkl\u00e4rt.", "tokens": ["Doch", "Sch\u00f6\u00b7nes", "ist", "der", "Licht\u00b7strahl", ",", "der", "uns", "zum", "Gott", "ver\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-++-+-+-+", "measure": "unknown.measure.septa"}}, "stanza.2": {"line.1": {"text": "Der Kaiser lohnet f\u00fcrstlich den wunderbaren Mann,", "tokens": ["Der", "Kai\u00b7ser", "loh\u00b7net", "f\u00fcrst\u00b7lich", "den", "wun\u00b7der\u00b7ba\u00b7ren", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der solcher Dinge m\u00e4chtig, wie Keiner je begann.", "tokens": ["Der", "sol\u00b7cher", "Din\u00b7ge", "m\u00e4ch\u00b7tig", ",", "wie", "Kei\u00b7ner", "je", "be\u00b7gann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADJD", "$,", "PWAV", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Oft schliesst er mit dem Magus sich in die Zimmer ein,", "tokens": ["Oft", "schliesst", "er", "mit", "dem", "Ma\u00b7gus", "sich", "in", "die", "Zim\u00b7mer", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-++-+-+-+-+", "measure": "unknown.measure.septa"}, "line.4": {"text": "Oft geht mit Faust er huldvoll lustwandelnd durch den Hain. \u2013", "tokens": ["Oft", "geht", "mit", "Faust", "er", "huld\u00b7voll", "lust\u00b7wan\u00b7delnd", "durch", "den", "Hain", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "PPER", "ADJD", "ADJD", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Einst als der gute Kaiser fr\u00fch aus dem Schlaf erwacht,", "tokens": ["Einst", "als", "der", "gu\u00b7te", "Kai\u00b7ser", "fr\u00fch", "aus", "dem", "Schlaf", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Glaubt er sich noch umfangen von einem Traum der Nacht.", "tokens": ["Glaubt", "er", "sich", "noch", "um\u00b7fan\u00b7gen", "von", "ei\u00b7nem", "Traum", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "VVPP", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die W\u00e4nde seines Zimmers sind frisch mit Gr\u00fcn belaubt,", "tokens": ["Die", "W\u00e4n\u00b7de", "sei\u00b7nes", "Zim\u00b7mers", "sind", "frisch", "mit", "Gr\u00fcn", "be\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ein golddurchwobner Lorbeer umrauscht sein Herrscherhaupt.", "tokens": ["Ein", "gold\u00b7durch\u00b7wob\u00b7ner", "Lor\u00b7beer", "um\u00b7rauscht", "sein", "Herr\u00b7scher\u00b7haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Und rings ist holder Maiglanz, rings bunte Blumenzier,", "tokens": ["Und", "rings", "ist", "hol\u00b7der", "Mai\u00b7glanz", ",", "rings", "bun\u00b7te", "Blu\u00b7men\u00b7zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADJA", "NN", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der Boden Wiesenteppich, die Deck' ein Lustrevier,", "tokens": ["Der", "Bo\u00b7den", "Wie\u00b7sen\u00b7tep\u00b7pich", ",", "die", "Deck'", "ein", "Lus\u00b7tre\u00b7vier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wo sich in hoher W\u00f6lbung der B\u00e4ume Laub verschlingt,", "tokens": ["Wo", "sich", "in", "ho\u00b7her", "W\u00f6l\u00b7bung", "der", "B\u00e4u\u00b7me", "Laub", "ver\u00b7schlingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPR", "ADJA", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Darin manch lustges V\u00f6glein die zarten Lieder singt.", "tokens": ["Da\u00b7rin", "manch", "lust\u00b7ges", "V\u00f6\u00b7glein", "die", "zar\u00b7ten", "Lie\u00b7der", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}}, "stanza.5": {"line.1": {"text": "Und Fr\u00fcchte neigen schwellend sich in die Kaiserhand,", "tokens": ["Und", "Fr\u00fcch\u00b7te", "nei\u00b7gen", "schwel\u00b7lend", "sich", "in", "die", "Kai\u00b7ser\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Aus Bl\u00e4tterf\u00fclle quellend rings um des Lagers Rand.", "tokens": ["Aus", "Bl\u00e4t\u00b7ter\u00b7f\u00fcl\u00b7le", "quel\u00b7lend", "rings", "um", "des", "La\u00b7gers", "Rand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Es leuchten neben Bl\u00fcthen Limonen goldhell, s\u00fcss,", "tokens": ["Es", "leuch\u00b7ten", "ne\u00b7ben", "Bl\u00fc\u00b7then", "Li\u00b7mo\u00b7nen", "gold\u00b7hell", ",", "s\u00fcss", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der Feige weiche Saftfrucht mahnt an das Paradies.", "tokens": ["Der", "Fei\u00b7ge", "wei\u00b7che", "Saft\u00b7frucht", "mahnt", "an", "das", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Der Herrscher hebt verwundert sich von der Lagerstatt,", "tokens": ["Der", "Herr\u00b7scher", "hebt", "ver\u00b7wun\u00b7dert", "sich", "von", "der", "La\u00b7ger\u00b7statt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Er sieht sich an der Lustpracht, am Farbenschmelz nicht satt.", "tokens": ["Er", "sieht", "sich", "an", "der", "Lust\u00b7pracht", ",", "am", "Far\u00b7ben\u00b7schmelz", "nicht", "satt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,", "APPRART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-++-+-+-+", "measure": "unknown.measure.septa"}, "line.3": {"text": "Wie sind die Blumen duftvoll, die hier so schnell erbl\u00fcht!", "tokens": ["Wie", "sind", "die", "Blu\u00b7men", "duft\u00b7voll", ",", "die", "hier", "so", "schnell", "er\u00b7bl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADJD", "$,", "PRELS", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wie diese Trauben reif schon, da noch der Sommer gl\u00fcht?", "tokens": ["Wie", "die\u00b7se", "Trau\u00b7ben", "reif", "schon", ",", "da", "noch", "der", "Som\u00b7mer", "gl\u00fcht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "ADJD", "ADV", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Und Nachtigallenbrautlied ert\u00f6nt mit hellem Schlag,", "tokens": ["Und", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7braut\u00b7lied", "er\u00b7t\u00f6nt", "mit", "hel\u00b7lem", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und doch ist l\u00e4ngst vor\u00fcber des Jahres l\u00e4ngster Tag!", "tokens": ["Und", "doch", "ist", "l\u00e4ngst", "vor\u00b7\u00fc\u00b7ber", "des", "Jah\u00b7res", "l\u00e4ngs\u00b7ter", "Tag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wenn diese Pracht kein Traum ist, die mich so hold umlacht,", "tokens": ["Wenn", "die\u00b7se", "Pracht", "kein", "Traum", "ist", ",", "die", "mich", "so", "hold", "um\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PIAT", "NN", "VAFIN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So muss sie Zauberwerk sein, das Faust mir zugedacht.", "tokens": ["So", "muss", "sie", "Zau\u00b7ber\u00b7werk", "sein", ",", "das", "Faust", "mir", "zu\u00b7ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VAINF", "$,", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Noch lange weilt der Kaiser, und sinnt, ob er nicht tr\u00e4umt;", "tokens": ["Noch", "lan\u00b7ge", "weilt", "der", "Kai\u00b7ser", ",", "und", "sinnt", ",", "ob", "er", "nicht", "tr\u00e4umt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die K\u00e4mmerlinge wunderts, dass der Gebieter s\u00e4umt.", "tokens": ["Die", "K\u00e4m\u00b7mer\u00b7lin\u00b7ge", "wun\u00b7derts", ",", "dass", "der", "Ge\u00b7bie\u00b7ter", "s\u00e4umt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und f\u00fcrchtend f\u00fcr sein Wohlsein aufthun die Th\u00fcr sie sacht,", "tokens": ["Und", "f\u00fcrch\u00b7tend", "f\u00fcr", "sein", "Wohl\u00b7sein", "auf\u00b7thun", "die", "Th\u00fcr", "sie", "sacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da sie der Wunderprachtglanz mit Recht erstaunen macht.", "tokens": ["Da", "sie", "der", "Wun\u00b7der\u00b7pracht\u00b7glanz", "mit", "Recht", "er\u00b7stau\u00b7nen", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Und fr\u00f6hlich spricht der Kaiser: \u00bbRuft mir den Hof herein!", "tokens": ["Und", "fr\u00f6h\u00b7lich", "spricht", "der", "Kai\u00b7ser", ":", "\u00bb", "Ruft", "mir", "den", "Hof", "her\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$.", "$(", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Lasst Damen mir und Ritter des Zaubers Zeugen sein!", "tokens": ["Lasst", "Da\u00b7men", "mir", "und", "Rit\u00b7ter", "des", "Zau\u00b7bers", "Zeu\u00b7gen", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "KON", "NN", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Auch ruft mir die Gesandtschaft, dass sie die Pracht erblickt,", "tokens": ["Auch", "ruft", "mir", "die", "Ge\u00b7sandt\u00b7schaft", ",", "dass", "sie", "die", "Pracht", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Mit der ein trauter Freund uns die Wohnung ausgeschm\u00fcckt!\u00ab", "tokens": ["Mit", "der", "ein", "trau\u00b7ter", "Freund", "uns", "die", "Woh\u00b7nung", "aus\u00b7ge\u00b7schm\u00fcckt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PRELS", "ART", "ADJA", "NN", "PPER", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "\u00bbund ruft mir her den Meister, der freundlich uns erfreut,", "tokens": ["\u00bb", "und", "ruft", "mir", "her", "den", "Meis\u00b7ter", ",", "der", "freund\u00b7lich", "uns", "er\u00b7freut", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der auf den Lebensbecher uns Rosenbl\u00e4tter streut!", "tokens": ["Der", "auf", "den", "Le\u00b7bens\u00b7be\u00b7cher", "uns", "Ro\u00b7sen\u00b7bl\u00e4t\u00b7ter", "streut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Denn Kunst ist eine Rose, gar herrlich ist ihr Bl\u00fchn,", "tokens": ["Denn", "Kunst", "ist", "ei\u00b7ne", "Ro\u00b7se", ",", "gar", "herr\u00b7lich", "ist", "ihr", "Bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "NN", "$,", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Stets lohne wackern K\u00fcnstlern des Dankes Immergr\u00fcn!\u00ab", "tokens": ["Stets", "loh\u00b7ne", "wa\u00b7ckern", "K\u00fcnst\u00b7lern", "des", "Dan\u00b7kes", "Im\u00b7mer\u00b7gr\u00fcn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "Und alle Ritter kamen herbei dem Herrscherruf,", "tokens": ["Und", "al\u00b7le", "Rit\u00b7ter", "ka\u00b7men", "her\u00b7bei", "dem", "Herr\u00b7scher\u00b7ruf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es schaun des Hofes Damen die Pracht, die Zauber schuf.", "tokens": ["Es", "schaun", "des", "Ho\u00b7fes", "Da\u00b7men", "die", "Pracht", ",", "die", "Zau\u00b7ber", "schuf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da schritt der hohe Meister zum Bl\u00fcthenraum heran,", "tokens": ["Da", "schritt", "der", "ho\u00b7he", "Meis\u00b7ter", "zum", "Bl\u00fc\u00b7then\u00b7raum", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"