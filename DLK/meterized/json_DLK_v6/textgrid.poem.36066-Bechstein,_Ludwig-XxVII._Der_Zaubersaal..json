ahl.", "tokens": ["Drauf", "dun\u00b7kelt", "sich's", ",", "und", "lieb\u00b7lich", "gl\u00e4nzt", "Mon\u00b7d", "und", "Ster\u00b7nen\u00b7strahl", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "$,", "KON", "ADJD", "VVFIN", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.13": {"line.1": {"text": "Dann wird das Schattend\u00fcster vom Morgenroth erhellt,", "tokens": ["Dann", "wird", "das", "Schat\u00b7ten\u00b7d\u00fcs\u00b7ter", "vom", "Mor\u00b7gen\u00b7roth", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ein sanfter Regen tr\u00e4nket die junge Bl\u00fcthenwelt.", "tokens": ["Ein", "sanf\u00b7ter", "Re\u00b7gen", "tr\u00e4n\u00b7ket", "die", "jun\u00b7ge", "Bl\u00fc\u00b7then\u00b7welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Sonne hehr im Goldglanz sich hinter B\u00e4umen hebt,", "tokens": ["Die", "Son\u00b7ne", "hehr", "im", "Gold\u00b7glanz", "sich", "hin\u00b7ter", "B\u00e4u\u00b7men", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und \u00fcberm Zauberlustwald ein Regenbogen schwebt.", "tokens": ["Und", "\u00fc\u00b7berm", "Zau\u00b7ber\u00b7lust\u00b7wald", "ein", "Re\u00b7gen\u00b7bo\u00b7gen", "schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.14": {"line.1": {"text": "Und wie noch die Versammlung mit stillem Staunen schaut,", "tokens": ["Und", "wie", "noch", "die", "Ver\u00b7samm\u00b7lung", "mit", "stil\u00b7lem", "Stau\u00b7nen", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wird bleich der Wolken Lichtschein, des Himmels Glanz ergraut.", "tokens": ["Wird", "bleich", "der", "Wol\u00b7ken", "Licht\u00b7schein", ",", "des", "Him\u00b7mels", "Glanz", "er\u00b7graut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "NN", "$,", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Sonne h\u00fcllet Nacht ein, und Blitze kreuzen hell,", "tokens": ["Die", "Son\u00b7ne", "h\u00fcl\u00b7let", "Nacht", "ein", ",", "und", "Blit\u00b7ze", "kreu\u00b7zen", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "PTKVZ", "$,", "KON", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und jedem gl\u00fchen Blitzstrahl folgt gleich der Donner schnell.", "tokens": ["Und", "je\u00b7dem", "gl\u00fc\u00b7hen", "Blitz\u00b7strahl", "folgt", "gleich", "der", "Don\u00b7ner", "schnell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.15": {"line.1": {"text": "Und bald abtr\u00e4ufelt schneekalt feink\u00f6rn'ger Hagel kraus;", "tokens": ["Und", "bald", "ab\u00b7tr\u00e4u\u00b7felt", "schnee\u00b7kalt", "fein\u00b7k\u00f6rn'\u00b7ger", "Ha\u00b7gel", "kraus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADJD", "ADJA", "NN", "PTKVZ", "$."], "meter": "---+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Und Damen fliehn und Ritter zum Zaubersaal hinaus.", "tokens": ["Und", "Da\u00b7men", "fliehn", "und", "Rit\u00b7ter", "zum", "Zau\u00b7ber\u00b7saal", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "KON", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Bald Schlossen prasselnd schmettern auf die, die sp\u00e4t entfliehn,", "tokens": ["Bald", "Schlos\u00b7sen", "pras\u00b7selnd", "schmet\u00b7tern", "auf", "die", ",", "die", "sp\u00e4t", "ent\u00b7fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVPP", "VVFIN", "APPR", "ART", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Doch sicher sitzt der Kaiser, ihn schirmt ein Baldachin.", "tokens": ["Doch", "si\u00b7cher", "sitzt", "der", "Kai\u00b7ser", ",", "ihn", "schirmt", "ein", "Bal\u00b7da\u00b7ch\u00b7in", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.16": {"line.1": {"text": "Und mit dem sch\u00f6nen Kunstwald, den All' mit Lust geschaut,", "tokens": ["Und", "mit", "dem", "sch\u00f6\u00b7nen", "Kunst\u00b7wald", ",", "den", "All'", "mit"