{"textgrid.poem.48057": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Fruchthauch", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich liebe dich, du weicher Wind,", "tokens": ["Ich", "lie\u00b7be", "dich", ",", "du", "wei\u00b7cher", "Wind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "der schmeichelnd meine Stirn umf\u00e4ngt,", "tokens": ["der", "schmei\u00b7chelnd", "mei\u00b7ne", "Stirn", "um\u00b7f\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "der mir erstorbenen Sommergl\u00fccks", "tokens": ["der", "mir", "ers\u00b7tor\u00b7be\u00b7nen", "Som\u00b7mer\u00b7gl\u00fccks"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Fruchtkeime in die Seele senkt . . .", "tokens": ["Frucht\u00b7kei\u00b7me", "in", "die", "See\u00b7le", "senkt", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}