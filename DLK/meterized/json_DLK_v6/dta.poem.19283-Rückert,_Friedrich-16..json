{"dta.poem.19283": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du mu\u00dft dein dunkles Selbst zum hellen Selbst erweitern;", "tokens": ["Du", "mu\u00dft", "dein", "dunk\u00b7les", "Selbst", "zum", "hel\u00b7len", "Selbst", "er\u00b7wei\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur die Verschlossenheit ist in Gefahr zu scheitern.", "tokens": ["Nur", "die", "Ver\u00b7schlos\u00b7sen\u00b7heit", "ist", "in", "Ge\u00b7fahr", "zu", "schei\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dem Ich, dem Schifflein, steht Nicht-Ich, die Klipp' entgegen,", "tokens": ["Dem", "Ich", ",", "dem", "Schif\u00b7flein", ",", "steht", "Nicht\u00b7Ich", ",", "die", "Klipp'", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "ART", "NN", "$,", "VVFIN", "NE", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und der Nothwendigkeit ist Freiheit unterlegen.", "tokens": ["Und", "der", "Noth\u00b7wen\u00b7dig\u00b7keit", "ist", "Frei\u00b7heit", "un\u00b7ter\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "NN", "VVINF", "$."], "meter": "-+++-+-+-+-+-", "measure": "unknown.measure.septa"}}, "stanza.3": {"line.1": {"text": "Doch schlie\u00df in Gott dich auf, und f\u00fchl' dich unbezwinglich,", "tokens": ["Doch", "schlie\u00df", "in", "Gott", "dich", "auf", ",", "und", "f\u00fchl'", "dich", "un\u00b7be\u00b7zwing\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Alldurchdringenden durchdrungen undurchdringlich.", "tokens": ["Vom", "A\u00b7lldurch\u00b7drin\u00b7gen\u00b7den", "durch\u00b7drun\u00b7gen", "un\u00b7durch\u00b7dring\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das Nicht-Ich war dein Feind; nun sieh, Nichts ist als Ich!", "tokens": ["Das", "Nicht\u00b7Ich", "war", "dein", "Feind", ";", "nun", "sieh", ",", "Nichts", "ist", "als", "Ich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "$,", "PIS", "VAFIN", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Worin denn f\u00fcrchtetest du zu verlieren dich!", "tokens": ["Wo\u00b7rin", "denn", "f\u00fcrch\u00b7te\u00b7test", "du", "zu", "ver\u00b7lie\u00b7ren", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PTKZU", "VVINF", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}