{"dta.poem.11607": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Zurechtweisung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "In dieser Winterfr\u00fche", "tokens": ["In", "die\u00b7ser", "Win\u00b7ter\u00b7fr\u00fc\u00b7he"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie ist mir doch zu Muth!", "tokens": ["Wie", "ist", "mir", "doch", "zu", "Muth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Morgenroth, ich gl\u00fche", "tokens": ["O", "Mor\u00b7gen\u00b7roth", ",", "ich", "gl\u00fc\u00b7he"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von deinem Jugendblut.", "tokens": ["Von", "dei\u00b7nem", "Ju\u00b7gend\u00b7blut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es gl\u00fcht der alte Felsen,", "tokens": ["Es", "gl\u00fcht", "der", "al\u00b7te", "Fel\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die W\u00e4lder Funken spr\u00fchn,", "tokens": ["Die", "W\u00e4l\u00b7der", "Fun\u00b7ken", "spr\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Berauschte Nebel w\u00e4lzen", "tokens": ["Be\u00b7rauschte", "Ne\u00b7bel", "w\u00e4l\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Sich in der Tiefe hin.", "tokens": ["Sich", "in", "der", "Tie\u00b7fe", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie von der H\u00f6he nieder", "tokens": ["Wie", "von", "der", "H\u00f6\u00b7he", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der reinste Himmel flimmt,", "tokens": ["Der", "reins\u00b7te", "Him\u00b7mel", "flimmt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der um die Rosenglieder", "tokens": ["Der", "um", "die", "Ro\u00b7seng\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Entz\u00fcckter Engel schwimmt!", "tokens": ["Ent\u00b7z\u00fcck\u00b7ter", "En\u00b7gel", "schwimmt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mit thatenfroher Eile", "tokens": ["Mit", "tha\u00b7ten\u00b7fro\u00b7her", "Ei\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erhebt sich Geist und Sinn,", "tokens": ["Er\u00b7hebt", "sich", "Geist", "und", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und fl\u00fcgelt goldne Pfeile", "tokens": ["Und", "fl\u00fc\u00b7gelt", "gold\u00b7ne", "Pfei\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch alle Ferne hin.", "tokens": ["Durch", "al\u00b7le", "Fer\u00b7ne", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Auf Burgen m\u00f6cht' ich springen,", "tokens": ["Auf", "Bur\u00b7gen", "m\u00f6cht'", "ich", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In alter F\u00fcrsten Schlo\u00df,", "tokens": ["In", "al\u00b7ter", "F\u00fcrs\u00b7ten", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "M\u00f6cht' hohe Lieder singen,", "tokens": ["M\u00f6cht'", "ho\u00b7he", "Lie\u00b7der", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mich schwingen auf das Ro\u00df.", "tokens": ["Mich", "schwin\u00b7gen", "auf", "das", "Ro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und stolzen Siegeswagen", "tokens": ["Und", "stol\u00b7zen", "Sie\u00b7ges\u00b7wa\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "St\u00fcrzt' ich mich brausend nach,", "tokens": ["St\u00fcrzt'", "ich", "mich", "brau\u00b7send", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Harfe wird zerschlagen,", "tokens": ["Die", "Har\u00b7fe", "wird", "zer\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die nur von Liebe sprach.", "tokens": ["Die", "nur", "von", "Lie\u00b7be", "sprach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wie? singst du so vermessen,", "tokens": ["Wie", "?", "singst", "du", "so", "ver\u00b7mes\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Herz, hast du nicht bedacht,", "tokens": ["Herz", ",", "hast", "du", "nicht", "be\u00b7dacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hast, N\u00e4rrchen, ganz vergessen,", "tokens": ["Hast", ",", "N\u00e4rr\u00b7chen", ",", "ganz", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was dich so trunken macht?", "tokens": ["Was", "dich", "so", "trun\u00b7ken", "macht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ach, wohl! was aus mir singet,", "tokens": ["Ach", ",", "wohl", "!", "was", "aus", "mir", "sin\u00b7get", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "$.", "PWS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist nur der Liebe Gl\u00fcck,", "tokens": ["Ist", "nur", "der", "Lie\u00b7be", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die wirren T\u00f6ne schlinget", "tokens": ["Die", "wir\u00b7ren", "T\u00f6\u00b7ne", "schlin\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie sanft in sich zur\u00fcck.", "tokens": ["Sie", "sanft", "in", "sich", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Was hilft, was hilft mein Sehnen?", "tokens": ["Was", "hilft", ",", "was", "hilft", "mein", "Seh\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geliebte, w\u00e4rst du hier!", "tokens": ["Ge\u00b7lieb\u00b7te", ",", "w\u00e4rst", "du", "hier", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In tausend Freudethr\u00e4nen", "tokens": ["In", "tau\u00b7send", "Freu\u00b7det\u00b7hr\u00e4\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verging' die Erde mir.", "tokens": ["Ver\u00b7ging'", "die", "Er\u00b7de", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}