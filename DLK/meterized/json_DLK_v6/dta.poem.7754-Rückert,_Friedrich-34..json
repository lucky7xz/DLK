{"dta.poem.7754": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "34.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den Meister sah ich Nachts, von einer Kerze Schimmer", "tokens": ["Den", "Meis\u00b7ter", "sah", "ich", "Nachts", ",", "von", "ei\u00b7ner", "Ker\u00b7ze", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hell angeleuchtet, gehn gedankentief durchs Zimmer.", "tokens": ["Hell", "an\u00b7ge\u00b7leuch\u00b7tet", ",", "gehn", "ge\u00b7dan\u00b7ken\u00b7tief", "durchs", "Zim\u00b7mer", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "$,", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Den Boden schien er mit der Sohle nicht zu r\u00fchren,", "tokens": ["Den", "Bo\u00b7den", "schien", "er", "mit", "der", "Soh\u00b7le", "nicht", "zu", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gespr\u00e4che leise, die ich nicht vernahm, zu f\u00fchren.", "tokens": ["Ge\u00b7spr\u00e4\u00b7che", "lei\u00b7se", ",", "die", "ich", "nicht", "ver\u00b7nahm", ",", "zu", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Aufschlug er dann den Blick, und als er stehn mich sah,", "tokens": ["Auf\u00b7schlug", "er", "dann", "den", "Blick", ",", "und", "als", "er", "stehn", "mich", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,", "KON", "KOUS", "PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sprach er: Bist du da? und ich sagte: Meister, ja.", "tokens": ["Sprach", "er", ":", "Bist", "du", "da", "?", "und", "ich", "sag\u00b7te", ":", "Meis\u00b7ter", ",", "ja", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$.", "VAFIN", "PPER", "ADV", "$.", "KON", "PPER", "VVFIN", "$.", "NN", "$,", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "\u201ewie lange?\u201c Lange schon. Dann sprach er weiter nichts:", "tokens": ["\u201e", "wie", "lan\u00b7ge", "?", "\u201c", "Lan\u00b7ge", "schon", ".", "Dann", "sprach", "er", "wei\u00b7ter", "nichts", ":"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADV", "$.", "$(", "NN", "ADV", "$.", "ADV", "VVFIN", "PPER", "ADV", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich aber bat: O gib mir einen Stral des Lichts!", "tokens": ["Ich", "a\u00b7ber", "bat", ":", "O", "gib", "mir", "ei\u00b7nen", "Stral", "des", "Lichts", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$.", "NE", "VVIMP", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Er sprach: Ich war bei Gott, er hat mich eingeladen:", "tokens": ["Er", "sprach", ":", "Ich", "war", "bei", "Gott", ",", "er", "hat", "mich", "ein\u00b7ge\u00b7la\u00b7den", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VAFIN", "APPR", "NN", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu w\u00e4hlen eine mir von seinen Wundergnaden;", "tokens": ["Zu", "w\u00e4h\u00b7len", "ei\u00b7ne", "mir", "von", "sei\u00b7nen", "Wun\u00b7derg\u00b7na\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Zu schweben in der Luft, zu wandeln auf dem Meer,", "tokens": ["Zu", "schwe\u00b7ben", "in", "der", "Luft", ",", "zu", "wan\u00b7deln", "auf", "dem", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$,", "PTKZU", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu sehn unsichtbares, und solcher Gnaden mehr.", "tokens": ["Zu", "sehn", "un\u00b7sicht\u00b7ba\u00b7res", ",", "und", "sol\u00b7cher", "Gna\u00b7den", "mehr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "$,", "KON", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ich aber w\u00e4hlte mir von allem diesen Nichts,", "tokens": ["Ich", "a\u00b7ber", "w\u00e4hl\u00b7te", "mir", "von", "al\u00b7lem", "die\u00b7sen", "Nichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "APPR", "PIS", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und war zufrieden mit dem Glanz des Angesichts.", "tokens": ["Und", "war", "zu\u00b7frie\u00b7den", "mit", "dem", "Glanz", "des", "An\u00b7ge\u00b7sichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Der Meister schwieg; ich sprach: Warum nicht w\u00e4hltest du,", "tokens": ["Der", "Meis\u00b7ter", "schwieg", ";", "ich", "sprach", ":", "Wa\u00b7rum", "nicht", "w\u00e4hl\u00b7test", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "$.", "PWAV", "PTKNEG", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihn zu erkennen selbst? Da rief er laut mir zu:", "tokens": ["Ihn", "zu", "er\u00b7ken\u00b7nen", "selbst", "?", "Da", "rief", "er", "laut", "mir", "zu", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "ADV", "$.", "ADV", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Schweig! Ihn erkennen d\u00fcrft' ich wollen? Nein, nein, nein!", "tokens": ["Schweig", "!", "Ihn", "er\u00b7ken\u00b7nen", "d\u00fcrft'", "ich", "wol\u00b7len", "?", "Nein", ",", "nein", ",", "nein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "VVINF", "VMFIN", "PPER", "VMFIN", "$.", "PTKANT", "$,", "PTKANT", "$,", "PTKANT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich will nicht, da\u00df Ihn wer erkenn' als Er allein.", "tokens": ["Ich", "will", "nicht", ",", "da\u00df", "Ihn", "wer", "er\u00b7kenn'", "als", "Er", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "$,", "KOUS", "PPER", "PWS", "VVFIN", "KOUS", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}