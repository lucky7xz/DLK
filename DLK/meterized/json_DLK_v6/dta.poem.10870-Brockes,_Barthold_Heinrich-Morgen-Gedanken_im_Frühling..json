{"dta.poem.10870": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Morgen-Gedanken  \n im Fr\u00fchling.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Herr, mein Gott! was mu\u00df ich sehn!", "tokens": ["Herr", ",", "mein", "Gott", "!", "was", "mu\u00df", "ich", "sehn", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$.", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Herr, mein Gott! wie wundersch\u00f6n", "tokens": ["Herr", ",", "mein", "Gott", "!", "wie", "wun\u00b7der\u00b7sch\u00f6n"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "$.", "PWAV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist, nach itzt verschwundner Nacht,", "tokens": ["Ist", ",", "nach", "itzt", "ver\u00b7schwund\u00b7ner", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Des bebl\u00fchmten Fr\u00fchlings Pracht!", "tokens": ["Des", "be\u00b7bl\u00fchm\u00b7ten", "Fr\u00fch\u00b7lings", "Pracht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles gl\u00e4nzet, alles bl\u00fchet,", "tokens": ["Al\u00b7les", "gl\u00e4n\u00b7zet", ",", "al\u00b7les", "bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alles funkelt, alles gl\u00fchet,", "tokens": ["Al\u00b7les", "fun\u00b7kelt", ",", "al\u00b7les", "gl\u00fc\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In der Sonnen Lebens-Stral;", "tokens": ["In", "der", "Son\u00b7nen", "Le\u00b7bens\u00b7S\u00b7tral", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Felder, W\u00e4lder, Berg, und Thal,", "tokens": ["Fel\u00b7der", ",", "W\u00e4l\u00b7der", ",", "Berg", ",", "und", "Thal", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Schimmern, in gef\u00e4rbten Flammen,", "tokens": ["Schim\u00b7mern", ",", "in", "ge\u00b7f\u00e4rb\u00b7ten", "Flam\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Alles die\u00df zeigt mir zusammen,", "tokens": ["Al\u00b7les", "die\u00df", "zeigt", "mir", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PDS", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.11": {"text": "Da\u00df die Wunder allzumal", "tokens": ["Da\u00df", "die", "Wun\u00b7der", "all\u00b7zu\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Blo\u00df aus dir, o Sonne, stammen:", "tokens": ["Blo\u00df", "aus", "dir", ",", "o", "Son\u00b7ne", ",", "stam\u00b7men", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,", "FM", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Doch auch die\u00df; ", "tokens": ["Doch", "auch", "die\u00df", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "PDS", "$."], "meter": "+--", "measure": "dactylic.init"}, "line.14": {"text": "Stamme blo\u00df aus Gott, allein.", "tokens": ["Stam\u00b7me", "blo\u00df", "aus", "Gott", ",", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$,", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}