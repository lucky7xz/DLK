{"textgrid.poem.47084": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbmai-Lilien, ihr sch\u00fcttelt eure Glocken,", "tokens": ["\u00bb", "mai\u00b7Li\u00b7li\u00b7en", ",", "ihr", "sch\u00fct\u00b7telt", "eu\u00b7re", "Glo\u00b7cken", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wen wollet ihr zur Maienandacht laden?\u00ab", "tokens": ["Wen", "wol\u00b7let", "ihr", "zur", "Mai\u00b7en\u00b7an\u00b7dacht", "la\u00b7den", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie, die von selbst sonst ging auf diesen Pfaden,", "tokens": ["Sie", ",", "die", "von", "selbst", "sonst", "ging", "auf", "die\u00b7sen", "Pfa\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "ADV", "ADV", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Soll, da sie s\u00e4umt, jetzt unser L\u00e4uten locken.", "tokens": ["Soll", ",", "da", "sie", "s\u00e4umt", ",", "jetzt", "un\u00b7ser", "L\u00e4u\u00b7ten", "lo\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbmai-Lilien, la\u00dft eu'r Gel\u00e4ute stocken;", "tokens": ["\u00bb", "mai\u00b7Li\u00b7li\u00b7en", ",", "la\u00dft", "eu'r", "Ge\u00b7l\u00e4u\u00b7te", "sto\u00b7cken", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$,", "VVIMP", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Soeben stocket ", "tokens": ["Soe\u00b7ben", "sto\u00b7cket"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Ach, sieh, der Tau, in welchem wir uns baden,", "tokens": ["Ach", ",", "sieh", ",", "der", "Tau", ",", "in", "wel\u00b7chem", "wir", "uns", "ba\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "$,", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gerinnt zu Reif, so sehr sind wir erschrocken.", "tokens": ["Ge\u00b7rinnt", "zu", "Reif", ",", "so", "sehr", "sind", "wir", "er\u00b7schro\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,", "ADV", "ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbmai-Lilien, da eure Lust zur Beute", "tokens": ["\u00bb", "mai\u00b7Li\u00b7li\u00b7en", ",", "da", "eu\u00b7re", "Lust", "zur", "Beu\u00b7te"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "NE", "$,", "KOUS", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Todes ward, was kann euch Trost erzeigen?\u00ab", "tokens": ["Des", "To\u00b7des", "ward", ",", "was", "kann", "euch", "Trost", "er\u00b7zei\u00b7gen", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PWS", "VMFIN", "PPER", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df du uns gleich von hinnen nehmest heute", "tokens": ["Da\u00df", "du", "uns", "gleich", "von", "hin\u00b7nen", "neh\u00b7mest", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und gebest ", "tokens": ["Und", "ge\u00b7best"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Da\u00df dort ", "tokens": ["Da\u00df", "dort"], "token_info": ["word", "word"], "pos": ["KOUS", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Sprich, willst du? \u00bbJa!\u00ab Wir danken dir mit Neigen.", "tokens": ["Sprich", ",", "willst", "du", "?", "\u00bb", "Ja", "!", "\u00ab", "Wir", "dan\u00b7ken", "dir", "mit", "Nei\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VMFIN", "PPER", "$.", "$(", "PTKANT", "$.", "$(", "PPER", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}