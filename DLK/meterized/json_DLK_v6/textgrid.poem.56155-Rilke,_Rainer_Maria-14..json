{"textgrid.poem.56155": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siehe die Blumen, diese dem Irdischen treuen,", "tokens": ["Sie\u00b7he", "die", "Blu\u00b7men", ",", "die\u00b7se", "dem", "Ir\u00b7di\u00b7schen", "treu\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$,", "PDS", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "denen wir Schicksal vom Rande des Schicksals leihn, \u2013", "tokens": ["de\u00b7nen", "wir", "Schick\u00b7sal", "vom", "Ran\u00b7de", "des", "Schick\u00b7sals", "leihn", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PRELS", "PPER", "NN", "APPRART", "NN", "ART", "NN", "VVINF", "$,", "$("], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "aber wer wei\u00df es! Wenn sie ihr Welken bereuen,", "tokens": ["a\u00b7ber", "wer", "wei\u00df", "es", "!", "Wenn", "sie", "ihr", "Wel\u00b7ken", "be\u00b7reu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "$.", "KOUS", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "ist es an uns, ihre Reue zu sein.", "tokens": ["ist", "es", "an", "uns", ",", "ih\u00b7re", "Reu\u00b7e", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "$,", "PPOSAT", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Alles will schweben. Da gehn wir umher wie Beschwerer,", "tokens": ["Al\u00b7les", "will", "schwe\u00b7ben", ".", "Da", "gehn", "wir", "um\u00b7her", "wie", "Be\u00b7schwe\u00b7rer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "VVINF", "$.", "ADV", "VVFIN", "PPER", "PTKVZ", "KOKOM", "NN", "$,"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.2": {"text": "legen auf alles uns selbst, vom Gewichte entz\u00fcckt;", "tokens": ["le\u00b7gen", "auf", "al\u00b7les", "uns", "selbst", ",", "vom", "Ge\u00b7wich\u00b7te", "ent\u00b7z\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIS", "PPER", "ADV", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "o was sind wir den Dingen f\u00fcr zehrende Lehrer,", "tokens": ["o", "was", "sind", "wir", "den", "Din\u00b7gen", "f\u00fcr", "zeh\u00b7ren\u00b7de", "Leh\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "PWS", "VAFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "weil ihnen ewige Kindheit gl\u00fcckt.", "tokens": ["weil", "ih\u00b7nen", "e\u00b7wi\u00b7ge", "Kind\u00b7heit", "gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "N\u00e4hme sie einer ins innige Schlafen und schliefe", "tokens": ["N\u00e4h\u00b7me", "sie", "ei\u00b7ner", "ins", "in\u00b7ni\u00b7ge", "Schla\u00b7fen", "und", "schlie\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "APPRART", "ADJA", "NN", "KON", "VVFIN"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.2": {"text": "tief mit den Dingen \u2013: o wie k\u00e4me er leicht,", "tokens": ["tief", "mit", "den", "Din\u00b7gen", "\u2013", ":", "o", "wie", "k\u00e4\u00b7me", "er", "leicht", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$(", "$.", "FM", "KOKOM", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.3": {"text": "anders zum anderen Tag, aus der gemeinsamen Tiefe.", "tokens": ["an\u00b7ders", "zum", "an\u00b7de\u00b7ren", "Tag", ",", "aus", "der", "ge\u00b7mein\u00b7sa\u00b7men", "Tie\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+-", "measure": "dactylic.tri.plus"}}, "stanza.4": {"line.1": {"text": "Oder er bliebe vielleicht; und sie bl\u00fchten und priesen", "tokens": ["O\u00b7der", "er", "blie\u00b7be", "viel\u00b7leicht", ";", "und", "sie", "bl\u00fch\u00b7ten", "und", "prie\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$.", "KON", "PPER", "VVFIN", "KON", "NN"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "ihn, den Bekehrten, der nun den Ihrigen gleicht,", "tokens": ["ihn", ",", "den", "Be\u00b7kehr\u00b7ten", ",", "der", "nun", "den", "Ih\u00b7ri\u00b7gen", "gleicht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$,", "PRELS", "ADV", "ART", "PPOSS", "VVFIN", "$,"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "allen den stillen Geschwistern im Winde der Wiesen.", "tokens": ["al\u00b7len", "den", "stil\u00b7len", "Ge\u00b7schwis\u00b7tern", "im", "Win\u00b7de", "der", "Wie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}}}}}