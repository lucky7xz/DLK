{"dta.poem.9917": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Meine zwey behaarte l\u00f6cher/ die gantz nah beysammen sind/", "tokens": ["Mei\u00b7ne", "zwey", "be\u00b7haar\u00b7te", "l\u00f6\u00b7cher", "/", "die", "gantz", "nah", "bey\u00b7sam\u00b7men", "sind", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "CARD", "VVFIN", "ADJD", "$(", "ART", "ADV", "ADJD", "VVPP", "VAFIN", "$("], "meter": "+-+-+-+--+--+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Stehen offen iederman/ der sich nur hinein will wagen:", "tokens": ["Ste\u00b7hen", "of\u00b7fen", "ie\u00b7der\u00b7man", "/", "der", "sich", "nur", "hin\u00b7ein", "will", "wa\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PIS", "$(", "PRELS", "PRF", "ADV", "APZR", "VMFIN", "VVINF", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Was von k\u00e4lt erstarret ist/ das wird da von hitze sagen/", "tokens": ["Was", "von", "k\u00e4lt", "er\u00b7star\u00b7ret", "ist", "/", "das", "wird", "da", "von", "hit\u00b7ze", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "VVFIN", "VVPP", "VAFIN", "$(", "PDS", "VAFIN", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "--+-+--+---+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Weil man drinnen solche w\u00e4rme/ wie bey einem ofen/ findt.", "tokens": ["Weil", "man", "drin\u00b7nen", "sol\u00b7che", "w\u00e4r\u00b7me", "/", "wie", "bey", "ei\u00b7nem", "o\u00b7fen", "/", "findt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PIAT", "NN", "$(", "KOKOM", "APPR", "ART", "NN", "$(", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}