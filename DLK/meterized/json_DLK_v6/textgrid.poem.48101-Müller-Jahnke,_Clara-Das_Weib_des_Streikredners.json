{"textgrid.poem.48101": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Das Weib des Streikredners", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbo bleib \u2013 nein geh! Es darf nicht sein!\u00ab", "tokens": ["\u00bb", "o", "bleib", "\u2013", "nein", "geh", "!", "Es", "darf", "nicht", "sein", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "FM", "$(", "PTKANT", "VVFIN", "$.", "PPER", "VMFIN", "PTKNEG", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u2013 sie dr\u00e4ngt ihn selbst in Kampf und Krieg, \u2013", "tokens": ["\u2013", "sie", "dr\u00e4ngt", "ihn", "selbst", "in", "Kampf", "und", "Krieg", ",", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbdie Werkgenossen harren dein,", "tokens": ["\u00bb", "die", "Werk\u00b7ge\u00b7nos\u00b7sen", "har\u00b7ren", "dein", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "auf deinen Lippen liegt der Sieg!\u00ab", "tokens": ["auf", "dei\u00b7nen", "Lip\u00b7pen", "liegt", "der", "Sieg", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er seufzt und streift mit scheuem Blick", "tokens": ["Er", "seufzt", "und", "streift", "mit", "scheu\u00b7em", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "die Lagerstatt an kahler Wand,", "tokens": ["die", "La\u00b7ger\u00b7statt", "an", "kah\u00b7ler", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "darin sein Kind am Fieber litt \u2013", "tokens": ["da\u00b7rin", "sein", "Kind", "am", "Fie\u00b7ber", "litt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "ein letzter Ku\u00df, ein ferner Schritt . . .", "tokens": ["ein", "letz\u00b7ter", "Ku\u00df", ",", "ein", "fer\u00b7ner", "Schritt", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "der Fr\u00fchlingssturm braust \u00fcber Land.", "tokens": ["der", "Fr\u00fch\u00b7lings\u00b7sturm", "braust", "\u00fc\u00b7ber", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und sternlos ist die M\u00e4rzennacht.", "tokens": ["Und", "stern\u00b7los", "ist", "die", "M\u00e4r\u00b7zen\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die blasse Mutter sitzt und sinnt,", "tokens": ["Die", "blas\u00b7se", "Mut\u00b7ter", "sitzt", "und", "sinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "mit tr\u00e4nenleerem Blick bewacht", "tokens": ["mit", "tr\u00e4\u00b7nen\u00b7lee\u00b7rem", "Blick", "be\u00b7wacht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "sie das geliebte letzte Kind.", "tokens": ["sie", "das", "ge\u00b7lieb\u00b7te", "letz\u00b7te", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein R\u00f6cheln aus des Knaben Brust,", "tokens": ["Ein", "R\u00f6\u00b7cheln", "aus", "des", "Kna\u00b7ben", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "ein Atemholen bang und schwer \u2013", "tokens": ["ein", "A\u00b7tem\u00b7ho\u00b7len", "bang", "und", "schwer", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "und heut im Haus kein Bissen Brot,", "tokens": ["und", "heut", "im", "Haus", "kein", "Bis\u00b7sen", "Brot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "im Glase keinen Tropfen mehr!", "tokens": ["im", "Gla\u00b7se", "kei\u00b7nen", "Trop\u00b7fen", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie wei\u00df: sie ringen um ein Ziel,", "tokens": ["Sie", "wei\u00df", ":", "sie", "rin\u00b7gen", "um", "ein", "Ziel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "so sternenlicht, so freudenreich \u2013", "tokens": ["so", "ster\u00b7nen\u00b7licht", ",", "so", "freu\u00b7den\u00b7reich", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und vierzig Tage w\u00e4hrt das Spiel,", "tokens": ["und", "vier\u00b7zig", "Ta\u00b7ge", "w\u00e4hrt", "das", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und heute f\u00e4llt der letzte Streich:", "tokens": ["und", "heu\u00b7te", "f\u00e4llt", "der", "letz\u00b7te", "Streich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "die Herrn des Goldes beugen sich", "tokens": ["die", "Herrn", "des", "Gol\u00b7des", "beu\u00b7gen", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "der st\u00e4rkeren Macht im heiligen Krieg \u2013", "tokens": ["der", "st\u00e4r\u00b7ke\u00b7ren", "Macht", "im", "hei\u00b7li\u00b7gen", "Krieg", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "und ", "tokens": ["und"], "token_info": ["word"], "pos": ["KON"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Und wenn das Morgenrot erwacht,", "tokens": ["Und", "wenn", "das", "Mor\u00b7gen\u00b7rot", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "dann kehrt er heim und bringt den Sieg!", "tokens": ["dann", "kehrt", "er", "heim", "und", "bringt", "den", "Sieg", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O k\u00e4m er bald und br\u00e4chte mit", "tokens": ["O", "k\u00e4m", "er", "bald", "und", "br\u00e4ch\u00b7te", "mit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ein st\u00e4rkend Labsal f\u00fcr das Kind!", "tokens": ["ein", "st\u00e4r\u00b7kend", "Lab\u00b7sal", "f\u00fcr", "das", "Kind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es schleicht die Zeit mit Schneckenschritt,", "tokens": ["Es", "schleicht", "die", "Zeit", "mit", "Schne\u00b7cken\u00b7schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "die sonst wie flutend Wasser rinnt.", "tokens": ["die", "sonst", "wie", "flu\u00b7tend", "Was\u00b7ser", "rinnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "KOKOM", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und doch, die dunkle Stunde kommt;", "tokens": ["Und", "doch", ",", "die", "dunk\u00b7le", "Stun\u00b7de", "kommt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "aufst\u00f6hnt das Kind in Todespein,", "tokens": ["auf\u00b7st\u00f6hnt", "das", "Kind", "in", "To\u00b7de\u00b7spein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "die H\u00e4ndchen zucken qualbeschwingt \u2013", "tokens": ["die", "H\u00e4nd\u00b7chen", "zu\u00b7cken", "qual\u00b7be\u00b7schwingt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "aus der gel\u00e4hmten Kehle dringt", "tokens": ["aus", "der", "ge\u00b7l\u00e4hm\u00b7ten", "Keh\u00b7le", "dringt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "ein heiser Stammeln: \u00bbMutter \u2013 Wein \u2013\u00ab", "tokens": ["ein", "hei\u00b7ser", "Stam\u00b7meln", ":", "\u00bb", "Mut\u00b7ter", "\u2013", "Wein", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$(", "NN", "$(", "NN", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie f\u00e4hrt empor aus kurzer Rast,", "tokens": ["Sie", "f\u00e4hrt", "em\u00b7por", "aus", "kur\u00b7zer", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "sie greift zum letzten St\u00fcmpfchen Lichts,", "tokens": ["sie", "greift", "zum", "letz\u00b7ten", "St\u00fcmpf\u00b7chen", "Lichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "sie sucht und sucht in irrer Hast", "tokens": ["sie", "sucht", "und", "sucht", "in", "ir\u00b7rer", "Hast"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "im Schub und Schrank und findet nichts \u2013", "tokens": ["im", "Schub", "und", "Schrank", "und", "fin\u00b7det", "nichts", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "KON", "VVFIN", "PIS", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und wie sie schaudernd r\u00fcckw\u00e4rts sieht:", "tokens": ["und", "wie", "sie", "schau\u00b7dernd", "r\u00fcck\u00b7w\u00e4rts", "sieht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "der Mund so gro\u00df, das Auge leer . . .", "tokens": ["der", "Mund", "so", "gro\u00df", ",", "das", "Au\u00b7ge", "leer", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,", "ART", "NN", "ADJD", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "sie steht und starrt \u2013 \u00bbAllewige Macht!\u00ab", "tokens": ["sie", "steht", "und", "starrt", "\u2013", "\u00bb", "Al\u00b7le\u00b7wi\u00b7ge", "Macht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$(", "$(", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "und hart und trostlos schweigt die Nacht,", "tokens": ["und", "hart", "und", "trost\u00b7los", "schweigt", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "und an die D\u00fcne rauscht das Meer.", "tokens": ["und", "an", "die", "D\u00fc\u00b7ne", "rauscht", "das", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbdu gnadenreiche Meeresflut!\u00ab", "tokens": ["\u00bb", "du", "gna\u00b7den\u00b7rei\u00b7che", "Mee\u00b7res\u00b7flut", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u2013 Sie senkt das Haupt in herber Pein,", "tokens": ["\u2013", "Sie", "senkt", "das", "Haupt", "in", "her\u00b7ber", "Pein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "von ihren Lippen perlt das Blut.", "tokens": ["von", "ih\u00b7ren", "Lip\u00b7pen", "perlt", "das", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Diele dr\u00f6hnt, er st\u00fcrmt herein", "tokens": ["Die", "Die\u00b7le", "dr\u00f6hnt", ",", "er", "st\u00fcrmt", "her\u00b7ein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "mit blitzendem Auge, raschem Wort, \u2013", "tokens": ["mit", "blit\u00b7zen\u00b7dem", "Au\u00b7ge", ",", "ra\u00b7schem", "Wort", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "da stockt sein Fu\u00df, sein Atem fliegt . . .", "tokens": ["da", "stockt", "sein", "Fu\u00df", ",", "sein", "A\u00b7tem", "fliegt", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "sie aber hebt die Arme, und", "tokens": ["sie", "a\u00b7ber", "hebt", "die", "Ar\u00b7me", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "$,", "KON"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "mit blassem, l\u00e4chelndem Duldermund", "tokens": ["mit", "blas\u00b7sem", ",", "l\u00e4\u00b7cheln\u00b7dem", "Dul\u00b7der\u00b7mund"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "spricht sie das Friedenswort: \u00bbGesiegt!\u00ab", "tokens": ["spricht", "sie", "das", "Frie\u00b7dens\u00b7wort", ":", "\u00bb", "Ge\u00b7siegt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "$(", "VVPP", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}