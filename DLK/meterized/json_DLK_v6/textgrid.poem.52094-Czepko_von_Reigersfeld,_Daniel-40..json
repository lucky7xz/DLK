{"textgrid.poem.52094": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "40.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr B\u00fcrger, seht den Sohn der reichen Heb'Amm an:", "tokens": ["Ihr", "B\u00fcr\u00b7ger", ",", "seht", "den", "Sohn", "der", "rei\u00b7chen", "He\u00b7b'\u00b7Amm", "an", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wie mit den Herren er sein Geld und Gut verthan:", "tokens": ["Wie", "mit", "den", "Her\u00b7ren", "er", "sein", "Geld", "und", "Gut", "ver\u00b7than", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PPER", "PPOSAT", "NN", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch Sohn und Mutter ist in eine Werckstatt kommen,", "tokens": ["Doch", "Sohn", "und", "Mut\u00b7ter", "ist", "in", "ei\u00b7ne", "Wercks\u00b7tatt", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er hat es eingesteckt, wo sie es raus genommen.", "tokens": ["Er", "hat", "es", "ein\u00b7ge\u00b7steckt", ",", "wo", "sie", "es", "raus", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,", "PWAV", "PPER", "PPER", "ADV", "