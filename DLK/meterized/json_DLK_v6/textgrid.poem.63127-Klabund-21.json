{"textgrid.poem.63127": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "21", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sanduhr rinnt. Das Licht verbrennt.", "tokens": ["Die", "San\u00b7duhr", "rinnt", ".", "Das", "Licht", "ver\u00b7brennt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man f\u00e4rbt sich den Bart mit Listen.", "tokens": ["Man", "f\u00e4rbt", "sich", "den", "Bart", "mit", "Lis\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So richt ich denn mein Testament", "tokens": ["So", "richt", "ich", "denn", "mein", "Tes\u00b7ta\u00b7ment"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie alle guten Christen.", "tokens": ["Wie", "al\u00b7le", "gu\u00b7ten", "Chris\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wo ist mein fester Blick? Ich bin", "tokens": ["Wo", "ist", "mein", "fes\u00b7ter", "Blick", "?", "Ich", "bin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VAFIN", "PPOSAT", "ADJA", "NN", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein S\u00e4ufer und taumle und stiere.", "tokens": ["Ein", "S\u00e4u\u00b7fer", "und", "taum\u00b7le", "und", "stie\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ich vermache mein Doppel- und Stoppelkinn", "tokens": ["Ich", "ver\u00b7ma\u00b7che", "mein", "Dop\u00b7pel", "und", "Stop\u00b7pel\u00b7kinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "TRUNC", "KON", "NN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Meinem Hofbarbiere.", "tokens": ["Mei\u00b7nem", "Hof\u00b7bar\u00b7bie\u00b7re", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Hier dieses Herz: es zuckte und hing", "tokens": ["Hier", "die\u00b7ses", "Herz", ":", "es", "zuck\u00b7te", "und", "hing"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PDAT", "NN", "$.", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "An allem Erlauchten und Edeln.", "tokens": ["An", "al\u00b7lem", "Er\u00b7lauch\u00b7ten", "und", "E\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Es mag ein f\u00fcnfzehnj\u00e4hriges Ding", "tokens": ["Es", "mag", "ein", "f\u00fcnf\u00b7zehn\u00b7j\u00e4h\u00b7ri\u00b7ges", "Ding"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die Fliegen sich damit wedeln.", "tokens": ["Die", "Flie\u00b7gen", "sich", "da\u00b7mit", "we\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "PAV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Hier diese Hand: einst Hieb und Stich", "tokens": ["Hier", "die\u00b7se", "Hand", ":", "einst", "Hieb", "und", "Stich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PDAT", "NN", "$.", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beim Becher und beim Degen -", "tokens": ["Beim", "Be\u00b7cher", "und", "beim", "De\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie mag versteint und verkn\u00f6chert sich", "tokens": ["Sie", "mag", "ver\u00b7steint", "und", "ver\u00b7kn\u00f6\u00b7chert", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVPP", "KON", "VVFIN", "PRF"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "An eines Bischofs Wange legen.", "tokens": ["An", "ei\u00b7nes", "Bi\u00b7schofs", "Wan\u00b7ge", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Mein Liebeswerkzeug sei vermacht", "tokens": ["Mein", "Lie\u00b7bes\u00b7werk\u00b7zeug", "sei", "ver\u00b7macht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der lieben s\u00fc\u00dfen Margot.", "tokens": ["Der", "lie\u00b7ben", "s\u00fc\u00b7\u00dfen", "Mar\u00b7got", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie betet es an um Mitternacht", "tokens": ["Sie", "be\u00b7tet", "es", "an", "um", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPR", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Im f\u00fcrchterlichsten Argot.", "tokens": ["Im", "f\u00fcrch\u00b7ter\u00b7lichs\u00b7ten", "Ar\u00b7got", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und meinen Ha\u00df: ich schenke ihn", "tokens": ["Und", "mei\u00b7nen", "Ha\u00df", ":", "ich", "schen\u00b7ke", "ihn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An jedermann und alle.", "tokens": ["An", "je\u00b7der\u00b7mann", "und", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "KON", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie sollen ihn sich auf Flaschen ziehn", "tokens": ["Sie", "sol\u00b7len", "ihn", "sich", "auf", "Fla\u00b7schen", "ziehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PRF", "APPR", "NN", "VVINF"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als Gift und gr\u00fcne Galle.", "tokens": ["Als", "Gift", "und", "gr\u00fc\u00b7ne", "Gal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Mein Wappen und mein Rittertum", "tokens": ["Mein", "Wap\u00b7pen", "und", "mein", "Rit\u00b7ter\u00b7tum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Einem unehlichen Kinde:", "tokens": ["Ei\u00b7nem", "u\u00b7neh\u00b7li\u00b7chen", "Kin\u00b7de", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Es schrei meine Ehre und meinen Ruhm", "tokens": ["Es", "schrei", "mei\u00b7ne", "Eh\u00b7re", "und", "mei\u00b7nen", "Ruhm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "In alle Budiken und Winde.", "tokens": ["In", "al\u00b7le", "Bu\u00b7di\u00b7ken", "und", "Win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Gegeben Gef\u00e4ngnis Meung sur Loire,", "tokens": ["Ge\u00b7ge\u00b7ben", "Ge\u00b7f\u00e4ng\u00b7nis", "Meung", "sur", "Loi\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "NE", "NE", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verlaust, wie ein Tier hinter St\u00e4ben,", "tokens": ["Ver\u00b7laust", ",", "wie", "ein", "Tier", "hin\u00b7ter", "St\u00e4\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Von einem, der einst ein Dichter war", "tokens": ["Von", "ei\u00b7nem", ",", "der", "einst", "ein", "Dich\u00b7ter", "war"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "$,", "PRELS", "ADV", "ART", "NN", "VAFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In diesem und jenem Leben.", "tokens": ["In", "die\u00b7sem", "und", "je\u00b7nem", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "KON", "PDAT", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}}}}