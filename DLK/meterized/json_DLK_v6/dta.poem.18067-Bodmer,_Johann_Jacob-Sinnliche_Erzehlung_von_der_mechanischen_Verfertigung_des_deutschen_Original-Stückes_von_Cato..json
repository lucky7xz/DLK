{"dta.poem.18067": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Sinnliche Erzehlung  \n von der mechanischen  \n Verfertigung des deutschen  \n Original-St\u00fcckes von Cato.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198462", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hier zieht die Freyheit noch die lezte Kraft zusammen,", "tokens": ["Hier", "zieht", "die", "Frey\u00b7heit", "noch", "die", "lez\u00b7te", "Kraft", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit der die Republick gewi\u00df zu Grunde geht,", "tokens": ["Mit", "der", "die", "Re\u00b7pub\u00b7lick", "ge\u00b7wi\u00df", "zu", "Grun\u00b7de", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wann sie einmal f\u00e4llt, wol niemal aufersteht.", "tokens": ["Und", "wann", "sie", "ein\u00b7mal", "f\u00e4llt", ",", "wol", "nie\u00b7mal", "auf\u00b7er\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVFIN", "$,", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}