{"textgrid.poem.57022": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das b\u00f6hmische Dorf", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Palmstr\u00f6m reist, mit einem Herrn v. Korf,", "tokens": ["Palm\u00b7str\u00f6m", "reist", ",", "mit", "ei\u00b7nem", "Herrn", "v.", "Korf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "abbreviation", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "APPR", "ART", "NN", "APPR", "NE", "$,"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.2": {"text": "in ein sogenanntes B\u00f6hmisches Dorf.", "tokens": ["in", "ein", "so\u00b7ge\u00b7nann\u00b7tes", "B\u00f6h\u00b7mi\u00b7sches", "Dorf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Unverst\u00e4ndlich bleibt ihm alles dort,", "tokens": ["Un\u00b7ver\u00b7st\u00e4nd\u00b7lich", "bleibt", "ihm", "al\u00b7les", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PIS", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "von dem ersten bis zum letzten Wort.", "tokens": ["von", "dem", "ers\u00b7ten", "bis", "zum", "letz\u00b7ten", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Auch v. Korf (der nur des Reimes wegen", "tokens": ["Auch", "v.", "Korf", "(", "der", "nur", "des", "Rei\u00b7mes", "we\u00b7gen"], "token_info": ["word", "abbreviation", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "$(", "ART", "ADV", "ART", "NN", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ihn begleitet) ist um Rat verlegen.", "tokens": ["ihn", "be\u00b7glei\u00b7tet", ")", "ist", "um", "Rat", "ver\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$(", "VAFIN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Doch just dieses macht ihn bla\u00df vor Gl\u00fcck.", "tokens": ["Doch", "just", "die\u00b7ses", "macht", "ihn", "bla\u00df", "vor", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Tiefentz\u00fcckt kehrt unser Freund zur\u00fcck.", "tokens": ["Tie\u00b7fent\u00b7z\u00fcckt", "kehrt", "un\u00b7ser", "Freund", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Und er schreibt in seine Wochenchronik:", "tokens": ["Und", "er", "schreibt", "in", "sei\u00b7ne", "Wo\u00b7chen\u00b7chro\u00b7nik", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Wieder ein Erlebnis, voll von Honig!", "tokens": ["Wie\u00b7der", "ein", "Er\u00b7leb\u00b7nis", ",", "voll", "von", "Ho\u00b7nig", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}