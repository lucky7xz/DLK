{"dta.poem.11788": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "74.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gewohnheit ist so stark, da\u00df selber die Natur", "tokens": ["Ge\u00b7wohn\u00b7heit", "ist", "so", "stark", ",", "da\u00df", "sel\u00b7ber", "die", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu thun scheint, was sie thut, oft aus Gewohnheit nur;", "tokens": ["Zu", "thun", "scheint", ",", "was", "sie", "thut", ",", "oft", "aus", "Ge\u00b7wohn\u00b7heit", "nur", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADV", "APPR", "NN", "ADV", "$."], "meter": "+-+--+++-+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Da\u00df die gewohnte Zeit dich hungrig scheint zu machen,", "tokens": ["Da\u00df", "die", "ge\u00b7wohn\u00b7te", "Zeit", "dich", "hung\u00b7rig", "scheint", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "ADJD", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und durstig, schl\u00e4frig auch, und selbst vom Schlaf erwachen.", "tokens": ["Und", "durs\u00b7tig", ",", "schl\u00e4f\u00b7rig", "auch", ",", "und", "selbst", "vom", "Schlaf", "er\u00b7wa\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "ADV", "$,", "KON", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn zu gewohnter Zeit sich Hunger eingefunden", "tokens": ["Wenn", "zu", "ge\u00b7wohn\u00b7ter", "Zeit", "sich", "Hun\u00b7ger", "ein\u00b7ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN", "PRF", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Durst, und Schl\u00e4frigkeit, z\u00e4hlst du villeicht die Stunden.", "tokens": ["Und", "Durst", ",", "und", "Schl\u00e4f\u00b7rig\u00b7keit", ",", "z\u00e4hlst", "du", "vil\u00b7leicht", "die", "Stun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "KON", "NN", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Wer aber z\u00e4hlte sie, wann ich im Schlummer lag,", "tokens": ["Wer", "a\u00b7ber", "z\u00e4hl\u00b7te", "sie", ",", "wann", "ich", "im", "Schlum\u00b7mer", "lag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erwach' und h\u00f6re den gewohnten Glockenschlag?", "tokens": ["Er\u00b7wach'", "und", "h\u00f6\u00b7re", "den", "ge\u00b7wohn\u00b7ten", "Glo\u00b7cken\u00b7schlag", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Drum ist Gewohnheit nicht ein Aeu\u00dferliches nur,", "tokens": ["Drum", "ist", "Ge\u00b7wohn\u00b7heit", "nicht", "ein", "A\u00b7e\u00b7u\u00b7\u00dfer\u00b7li\u00b7ches", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "NN", "PTKNEG", "ART", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Wie unser Sprichwort spricht: die andere Natur.", "tokens": ["Wie", "un\u00b7ser", "Sprich\u00b7wort", "spricht", ":", "die", "an\u00b7de\u00b7re", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Mach von der einen Joch dich durch die andre frei,", "tokens": ["Mach", "von", "der", "ei\u00b7nen", "Joch", "dich", "durch", "die", "and\u00b7re", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ART", "NN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht mache, da\u00df sie selbst ein zweites Joch dir sei.", "tokens": ["Nicht", "ma\u00b7che", ",", "da\u00df", "sie", "selbst", "ein", "zwei\u00b7tes", "Joch", "dir", "sei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}