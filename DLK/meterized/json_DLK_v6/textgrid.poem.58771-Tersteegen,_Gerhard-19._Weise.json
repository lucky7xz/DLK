{"textgrid.poem.58771": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "19. Weise", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Untadlig weisester Verstand,", "tokens": ["Un\u00b7tad\u00b7lig", "wei\u00b7ses\u00b7ter", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Urgrund dir allein bekannt,", "tokens": ["Des", "Ur\u00b7grund", "dir", "al\u00b7lein", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Weisheit zeugt dies Kunstgeb\u00e4ude;", "tokens": ["Von", "Weis\u00b7heit", "zeugt", "dies", "Kunst\u00b7ge\u00b7b\u00e4u\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PDS", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie weislich bin ich selbst gemacht,", "tokens": ["Wie", "weis\u00b7lich", "bin", "ich", "selbst", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie weislich werd' ich wiederbracht,", "tokens": ["Wie", "weis\u00b7lich", "werd'", "ich", "wie\u00b7der\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie weislich spro\u00dft aus Angst die Freude!", "tokens": ["Wie", "weis\u00b7lich", "spro\u00dft", "aus", "Angst", "die", "Freu\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "All's, was du machst, ist weis' und sch\u00f6n,", "tokens": ["All's", ",", "was", "du", "machst", ",", "ist", "weis'", "und", "sch\u00f6n", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich glaub's, auch wo ich's nicht kann sehn.", "tokens": ["Ich", "glaub's", ",", "auch", "wo", "ich's", "nicht", "kann", "sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$,", "ADV", "PWAV", "PIS", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Halleluja! Halleluja!", "tokens": ["Hal\u00b7le\u00b7lu\u00b7ja", "!", "Hal\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "ITJ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}