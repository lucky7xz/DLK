{"textgrid.poem.58058": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "96. Sanftmut l\u00f6scht der H\u00f6lle Glut", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jesus durch gelass'nes Lieben", "tokens": ["Je\u00b7sus", "durch", "ge\u00b7lass'\u00b7nes", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat des Vaters Zorn vertrieben", "tokens": ["Hat", "des", "Va\u00b7ters", "Zorn", "ver\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der H\u00f6lle Grimmigkeit;", "tokens": ["Und", "der", "H\u00f6l\u00b7le", "Grim\u00b7mig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann dich nun sein Geist durchflie\u00dfen,", "tokens": ["Kann", "dich", "nun", "sein", "Geist", "durch\u00b7flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wirst du auch durch Lieb' vers\u00fc\u00dfen", "tokens": ["Wirst", "du", "auch", "durch", "Lieb'", "ver\u00b7s\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aller Menschen Bitterkeit.", "tokens": ["Al\u00b7ler", "Men\u00b7schen", "Bit\u00b7ter\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}