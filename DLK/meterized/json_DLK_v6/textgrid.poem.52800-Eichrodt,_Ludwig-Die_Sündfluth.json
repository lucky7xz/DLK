{"textgrid.poem.52800": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die S\u00fcndfluth", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur Luft und See,", "tokens": ["Nur", "Luft", "und", "See", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Kein Land, kein Baum,", "tokens": ["Kein", "Land", ",", "kein", "Baum", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "O weh, o weh!", "tokens": ["O", "weh", ",", "o", "weh", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "FM", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Der Herr z\u00fcrnt sehr,", "tokens": ["Der", "Herr", "z\u00fcrnt", "sehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der Mensch, das Vieh,", "tokens": ["Der", "Mensch", ",", "das", "Vieh", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Schwimmt todt im Meer!", "tokens": ["Schwimmt", "todt", "im", "Meer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Die Arch nur h\u00e4lt,", "tokens": ["Die", "Arch", "nur", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wie weis' ist dies!", "tokens": ["Wie", "weis'", "ist", "dies", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PDS", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Den Keim der Welt:", "tokens": ["Den", "Keim", "der", "Welt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Mensch, Rind und Schwein,", "tokens": ["Mensch", ",", "Rind", "und", "Schwein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und was das Best',", "tokens": ["Und", "was", "das", "Best'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Auch schon den Wein!", "tokens": ["Auch", "schon", "den", "Wein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Ach, ein Glas Wein", "tokens": ["Ach", ",", "ein", "Glas", "Wein"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "NN", "NN"], "meter": "+-++", "measure": "unknown.measure.tri"}, "line.2": {"text": "Bei Weib und Sang,", "tokens": ["Bei", "Weib", "und", "Sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "O Mensch schmeckt fein.", "tokens": ["O", "Mensch", "schmeckt", "fein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}