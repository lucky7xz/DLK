{"textgrid.poem.59626": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Das Lied vom heiligen deutschen Lande", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es klang von hohen Ehren", "tokens": ["Es", "klang", "von", "ho\u00b7hen", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein heller Wunderklang,", "tokens": ["Ein", "hel\u00b7ler", "Wun\u00b7der\u00b7klang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie l\u00e4ngst verschollne M\u00e4ren", "tokens": ["Wie", "l\u00e4ngst", "ver\u00b7scholl\u00b7ne", "M\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er durch die Seelen drang,", "tokens": ["Er", "durch", "die", "See\u00b7len", "drang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie Wasser aus den Tiefen", "tokens": ["Wie", "Was\u00b7ser", "aus", "den", "Tie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zum Himmel sch\u00e4umend spr\u00fchn,", "tokens": ["Zum", "Him\u00b7mel", "sch\u00e4u\u00b7mend", "spr\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie Geister, welche schliefen,", "tokens": ["Wie", "Geis\u00b7ter", ",", "wel\u00b7che", "schlie\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Mitternacht durchziehn.", "tokens": ["Die", "Mit\u00b7ter\u00b7nacht", "durch\u00b7ziehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So fa\u00dft' es alle Herzen,", "tokens": ["So", "fa\u00dft'", "es", "al\u00b7le", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So klang's durch jede Brust,", "tokens": ["So", "klang's", "durch", "je\u00b7de", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Voll hei\u00dfer Weheschmerzen,", "tokens": ["Voll", "hei\u00b7\u00dfer", "We\u00b7hesc\u00b7hmer\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Voll hei\u00dfer Wonnelust;", "tokens": ["Voll", "hei\u00b7\u00dfer", "Won\u00b7ne\u00b7lust", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie Menschen in Gewittern", "tokens": ["Wie", "Men\u00b7schen", "in", "Ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Glanz des H\u00f6chsten sehn,", "tokens": ["Den", "Glanz", "des", "H\u00f6chs\u00b7ten", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mit Freude und mit Zittern", "tokens": ["Mit", "Freu\u00b7de", "und", "mit", "Zit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In seiner Macht vergehn.", "tokens": ["In", "sei\u00b7ner", "Macht", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Denn Gott, der alte Retter,", "tokens": ["Denn", "Gott", ",", "der", "al\u00b7te", "Ret\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der droben wandeln geht,", "tokens": ["Der", "dro\u00b7ben", "wan\u00b7deln", "geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erschien in Blitz und Wetter", "tokens": ["Er\u00b7schien", "in", "Blitz", "und", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In hehrer Majest\u00e4t;", "tokens": ["In", "heh\u00b7rer", "Ma\u00b7jes\u00b7t\u00e4t", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Als Richter wollt' er kommen", "tokens": ["Als", "Rich\u00b7ter", "wollt'", "er", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Herab vom Himmelreich,", "tokens": ["Her\u00b7ab", "vom", "Him\u00b7mel\u00b7reich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Drum freut euch all ihr Frommen,", "tokens": ["Drum", "freut", "euch", "all", "ihr", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ihr Frevler, werdet bleich.", "tokens": ["Ihr", "Frev\u00b7ler", ",", "wer\u00b7det", "bleich", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wer kann die Taten sprechen,", "tokens": ["Wer", "kann", "die", "Ta\u00b7ten", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Gott der Herr getan,", "tokens": ["Die", "Gott", "der", "Herr", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wodurch er Schanden brechen", "tokens": ["Wo\u00b7durch", "er", "Schan\u00b7den", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Ehren lohnen kann?", "tokens": ["Und", "Eh\u00b7ren", "loh\u00b7nen", "kann", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer z\u00e4hlt die edlen Toten,", "tokens": ["Wer", "z\u00e4hlt", "die", "ed\u00b7len", "To\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die trotzig auf das Kreuz", "tokens": ["Die", "trot\u00b7zig", "auf", "das", "Kreuz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sich k\u00fchn zur S\u00fchnung boten", "tokens": ["Sich", "k\u00fchn", "zur", "S\u00fch\u00b7nung", "bo\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Im s\u00fc\u00dfen Himmelreiz?", "tokens": ["Im", "s\u00fc\u00b7\u00dfen", "Him\u00b7mel\u00b7reiz", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wer z\u00e4hlt die Wundertaten,", "tokens": ["Wer", "z\u00e4hlt", "die", "Wun\u00b7der\u00b7ta\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Preise mannigfalt,", "tokens": ["Die", "Prei\u00b7se", "man\u00b7nig\u00b7falt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die also sch\u00f6n geraten", "tokens": ["Die", "al\u00b7so", "sch\u00f6n", "ge\u00b7ra\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch Gottes Allgewalt?", "tokens": ["Durch", "Got\u00b7tes", "All\u00b7ge\u00b7walt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Wahn ist nun zerstoben,", "tokens": ["Der", "Wahn", "ist", "nun", "zer\u00b7sto\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zermalmt die Tyrannei,", "tokens": ["Zer\u00b7malmt", "die", "Ty\u00b7ran\u00b7nei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Mensch blickt hin nach oben", "tokens": ["Der", "Mensch", "blickt", "hin", "nach", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und jauchzet: Wir sind frei!", "tokens": ["Und", "jauch\u00b7zet", ":", "Wir", "sind", "frei", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Das war der Klang der Ehren,", "tokens": ["Das", "war", "der", "Klang", "der", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das war die Wunderzeit,", "tokens": ["Das", "war", "die", "Wun\u00b7der\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die, selig im Geb\u00e4ren,", "tokens": ["Die", ",", "se\u00b7lig", "im", "Ge\u00b7b\u00e4\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich ihrer Wehen freut;", "tokens": ["Sich", "ih\u00b7rer", "We\u00b7hen", "freut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das brauset in den Tiefen,", "tokens": ["Das", "brau\u00b7set", "in", "den", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das blitzt am Firmament,", "tokens": ["Das", "blitzt", "am", "Fir\u00b7ma\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Geister, welche schliefen,", "tokens": ["Die", "Geis\u00b7ter", ",", "wel\u00b7che", "schlie\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Jetzt jedes Kind erkennt.", "tokens": ["Jetzt", "je\u00b7des", "Kind", "er\u00b7kennt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sie schreiten sch\u00f6n ger\u00fcstet", "tokens": ["Sie", "schrei\u00b7ten", "sch\u00f6n", "ge\u00b7r\u00fcs\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Daher im Himmelschein,", "tokens": ["Da\u00b7her", "im", "Him\u00b7mel\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und jedes Herz gel\u00fcstet", "tokens": ["Und", "je\u00b7des", "Herz", "ge\u00b7l\u00fcs\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In ihrer Schar zu sein;", "tokens": ["In", "ih\u00b7rer", "Schar", "zu", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So wie die Kindlein eigen", "tokens": ["So", "wie", "die", "Kin\u00b7dlein", "ei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der lieben Mutter sind,", "tokens": ["Der", "lie\u00b7ben", "Mut\u00b7ter", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Naht ihnen frommes Neigen", "tokens": ["Naht", "ih\u00b7nen", "from\u00b7mes", "Nei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein jedes Menschenkind.", "tokens": ["Ein", "je\u00b7des", "Men\u00b7schen\u00b7kind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der erste ist der ", "tokens": ["Der", "ers\u00b7te", "ist", "der"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "VAFIN", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Er tr\u00e4gt den Kreuzesbaum", "tokens": ["Er", "tr\u00e4gt", "den", "Kreu\u00b7zes\u00b7baum"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und blicket von dem Staube", "tokens": ["Und", "bli\u00b7cket", "von", "dem", "Stau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hinauf zum Sternenraum:", "tokens": ["Hin\u00b7auf", "zum", "Ster\u00b7nen\u00b7raum", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hienieden ist sein Sehnen", "tokens": ["Hien\u00b7ie\u00b7den", "ist", "sein", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und seine Freude nicht,", "tokens": ["Und", "sei\u00b7ne", "Freu\u00b7de", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Himmel nur lockt Tr\u00e4nen", "tokens": ["Der", "Him\u00b7mel", "nur", "lockt", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Von seinem Angesicht.", "tokens": ["Von", "sei\u00b7nem", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Von allen Himmelsbr\u00e4uten", "tokens": ["Von", "al\u00b7len", "Him\u00b7mels\u00b7br\u00e4u\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das allersch\u00f6nste Kind", "tokens": ["Das", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Kind"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Geht ", "tokens": ["Geht"], "token_