{"textgrid.poem.48130": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "1L: Durch Fr\u00fchlingsst\u00fcrme und Wolkenflor", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch Fr\u00fchlingsst\u00fcrme und Wolkenflor", "tokens": ["Durch", "Fr\u00fch\u00b7lings\u00b7st\u00fcr\u00b7me", "und", "Wol\u00b7ken\u00b7flor"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "bricht das ewige Licht hervor,", "tokens": ["bricht", "das", "e\u00b7wi\u00b7ge", "Licht", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "aus Blut erbl\u00fchen die Rosen;", "tokens": ["aus", "Blut", "er\u00b7bl\u00fc\u00b7hen", "die", "Ro\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "es kommt nach tausendj\u00e4hrigem Leid", "tokens": ["es", "kommt", "nach", "tau\u00b7send\u00b7j\u00e4h\u00b7ri\u00b7gem", "Leid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "ein Tag voll Maienseligkeit", "tokens": ["ein", "Tag", "voll", "Mai\u00b7en\u00b7se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "den Fried- und Freudelosen.", "tokens": ["den", "Frie\u00b7d", "und", "Freu\u00b7de\u00b7lo\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Mit klirrendem Eise ging der Strom,", "tokens": ["Mit", "klir\u00b7ren\u00b7dem", "Ei\u00b7se", "ging", "der", "Strom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "schwarz schattend unter dem Himmelsdom", "tokens": ["schwarz", "schat\u00b7tend", "un\u00b7ter", "dem", "Him\u00b7mels\u00b7dom"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "lag die br\u00fctende Wolke;", "tokens": ["lag", "die", "br\u00fc\u00b7ten\u00b7de", "Wol\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "doch Keime spro\u00dften, des Lebens voll,", "tokens": ["doch", "Kei\u00b7me", "spro\u00df\u00b7ten", ",", "des", "Le\u00b7bens", "voll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "und ein Ahnen wuchs, und ein Sehnen schwoll", "tokens": ["und", "ein", "Ah\u00b7nen", "wuchs", ",", "und", "ein", "Seh\u00b7nen", "schwoll"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "KON", "ART", "NN", "ADJD"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "tief in dem fronenden Volke.", "tokens": ["tief", "in", "dem", "fro\u00b7nen\u00b7den", "Vol\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Und die Schollen trieben ins offene Meer,", "tokens": ["Und", "die", "Schol\u00b7len", "trie\u00b7ben", "ins", "of\u00b7fe\u00b7ne", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "die Donner rollten, wie Schicksal schwer;", "tokens": ["die", "Don\u00b7ner", "roll\u00b7ten", ",", "wie", "Schick\u00b7sal", "schwer", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "NN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "grell spr\u00fchten die blauen Flammen;", "tokens": ["grell", "spr\u00fch\u00b7ten", "die", "blau\u00b7en", "Flam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "ein Hammer fiel, und ein Eisen sprang:", "tokens": ["ein", "Ham\u00b7mer", "fiel", ",", "und", "ein", "Ei\u00b7sen", "sprang", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "ein tiefer, t\u00f6nender Glockenklang", "tokens": ["ein", "tie\u00b7fer", ",", "t\u00f6\u00b7nen\u00b7der", "Glo\u00b7cken\u00b7klang"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJD", "$,", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "ruft die Freien zum Fest zusammen.", "tokens": ["ruft", "die", "Frei\u00b7en", "zum", "Fest", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Zum Fest der Bl\u00fcte, zum heiligen Mai!", "tokens": ["Zum", "Fest", "der", "Bl\u00fc\u00b7te", ",", "zum", "hei\u00b7li\u00b7gen", "Mai", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wer die Freiheit sucht und sie f\u00fchlt, ist frei,", "tokens": ["Wer", "die", "Frei\u00b7heit", "sucht", "und", "sie", "f\u00fchlt", ",", "ist", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "KON", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "und n\u00e4hrten ihn Disteln und Treber.", "tokens": ["und", "n\u00e4hr\u00b7ten", "ihn", "Dis\u00b7teln", "und", "Tre\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Er feiert mit uns. Und Sibiriens Eis", "tokens": ["Er", "fei\u00b7ert", "mit", "uns", ".", "Und", "Si\u00b7bi\u00b7ri\u00b7ens", "Eis"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$.", "KON", "NE", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "wird wie Dufthauch Italiens. Und lockend und leis", "tokens": ["wird", "wie", "Duf\u00b7thauch", "I\u00b7ta\u00b7li\u00b7ens", ".", "Und", "lo\u00b7ckend", "und", "leis"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "KOKOM", "NN", "NE", "$.", "KON", "VVPP", "KON", "ADJD"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "geht der Maiwind \u00fcber die Gr\u00e4ber . . .", "tokens": ["geht", "der", "Mai\u00b7wind", "\u00fc\u00b7ber", "die", "Gr\u00e4\u00b7ber", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$.", "$.", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Kein Saatkorn ist tot. Und umsonst kein Blut", "tokens": ["Kein", "Saat\u00b7korn", "ist", "tot", ".", "Und", "um\u00b7sonst", "kein", "Blut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "$.", "KON", "ADV", "PIAT", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "f\u00fcr die Freiheit vergossen. Wachsende Glut", "tokens": ["f\u00fcr", "die", "Frei\u00b7heit", "ver\u00b7gos\u00b7sen", ".", "Wach\u00b7sen\u00b7de", "Glut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF", "$.", "NN", "NN"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "wird die Aehren f\u00fcllen und streifen.", "tokens": ["wird", "die", "A\u00b7eh\u00b7ren", "f\u00fcl\u00b7len", "und", "strei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVINF", "KON", "VVFIN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und feiern wir heute das Bl\u00fctenfest:", "tokens": ["Und", "fei\u00b7ern", "wir", "heu\u00b7te", "das", "Bl\u00fc\u00b7ten\u00b7fest", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "die Sonne der Menschheit steigt, und l\u00e4\u00dft", "tokens": ["die", "Son\u00b7ne", "der", "Menschheit", "steigt", ",", "und", "l\u00e4\u00dft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "unsern Enkeln die Fr\u00fcchte reifen.", "tokens": ["un\u00b7sern", "En\u00b7keln", "die", "Fr\u00fcch\u00b7te", "rei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}