{"textgrid.poem.39063": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Der Rhein", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der deutsche Rhein \u2013! Wie klingt das Wort so m\u00e4chtig!", "tokens": ["Der", "deut\u00b7sche", "Rhein", "\u2013", "!", "Wie", "klingt", "das", "Wort", "so", "m\u00e4ch\u00b7tig", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$(", "$.", "PWAV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schon sehn wir ihn, den goldig gr\u00fcnen Strom,", "tokens": ["Schon", "sehn", "wir", "ihn", ",", "den", "gol\u00b7dig", "gr\u00fc\u00b7nen", "Strom", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "PRELS", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "mit heitern St\u00e4dten, Burgen stolz und pr\u00e4chtig,", "tokens": ["mit", "hei\u00b7tern", "St\u00e4d\u00b7ten", ",", "Bur\u00b7gen", "stolz", "und", "pr\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "die Lurlei dort und dort den K\u00f6lner Dom!", "tokens": ["die", "Lur\u00b7lei", "dort", "und", "dort", "den", "K\u00f6l\u00b7ner", "Dom", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der freie Rhein \u2013! Ged\u00e4chtnis unsrer Siege,", "tokens": ["Der", "frei\u00b7e", "Rhein", "\u2013", "!", "Ge\u00b7d\u00e4cht\u00b7nis", "uns\u00b7rer", "Sie\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$(", "$.", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "du mit dem Blut der Edelsten getauft,", "tokens": ["du", "mit", "dem", "Blut", "der", "E\u00b7dels\u00b7ten", "ge\u00b7tauft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Ruhm unsrer V\u00e4ter, die in heil'gem Kriege", "tokens": ["Ruhm", "uns\u00b7rer", "V\u00e4\u00b7ter", ",", "die", "in", "heil'\u00b7gem", "Krie\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "mit Liedern nicht, mit Schwertern dich erkauft! \u2013", "tokens": ["mit", "Lie\u00b7dern", "nicht", ",", "mit", "Schwer\u00b7tern", "dich", "er\u00b7kauft", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "PTKNEG", "$,", "APPR", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich sah ihn auch \u2013 es war ein b\u00f6ses Zeichen,", "tokens": ["Ich", "sah", "ihn", "auch", "\u2013", "es", "war", "ein", "b\u00f6\u00b7ses", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$(", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Novemberwolken hingen dr\u00fcber hin,", "tokens": ["No\u00b7vem\u00b7ber\u00b7wol\u00b7ken", "hin\u00b7gen", "dr\u00fc\u00b7ber", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PAV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "nicht str\u00f6men, nein, mich d\u00fcnkt, ich sah sie schleichen,", "tokens": ["nicht", "str\u00f6\u00b7men", ",", "nein", ",", "mich", "d\u00fcnkt", ",", "ich", "sah", "sie", "schlei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "PTKANT", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "die goldne Flut wie eine Bettlerin;", "tokens": ["die", "gold\u00b7ne", "Flut", "wie", "ei\u00b7ne", "Bett\u00b7le\u00b7rin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "als klagte sie, da\u00df noch mit Zoll und Banden", "tokens": ["als", "klag\u00b7te", "sie", ",", "da\u00df", "noch", "mit", "Zoll", "und", "Ban\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "$,", "KOUS", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "sie ungestraft der Fremdling knechten darf,", "tokens": ["sie", "un\u00b7ge\u00b7straft", "der", "Fremd\u00b7ling", "knech\u00b7ten", "darf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "da\u00df noch ein Wort, verf\u00e4lscht und mi\u00dfverstanden,", "tokens": ["da\u00df", "noch", "ein", "Wort", ",", "ver\u00b7f\u00e4lscht", "und", "mi\u00df\u00b7ver\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "sie von des Meeres keuschem Busen warf.", "tokens": ["sie", "von", "des", "Mee\u00b7res", "keu\u00b7schem", "Bu\u00b7sen", "warf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich sah das Land \u2013 die Traube sah ich reifen,", "tokens": ["Ich", "sah", "das", "Land", "\u2013", "die", "Trau\u00b7be", "sah", "ich", "rei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die rechte Milch, um M\u00e4nner gro\u00dfzuziehn,", "tokens": ["die", "rech\u00b7te", "Milch", ",", "um", "M\u00e4n\u00b7ner", "gro\u00df\u00b7zu\u00b7ziehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUI", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "lie\u00df weit hinaus mein flammend Auge schweifen,", "tokens": ["lie\u00df", "weit", "hin\u00b7aus", "mein", "flam\u00b7mend", "Au\u00b7ge", "schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APZR", "PPOSAT", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "dem nie ein Traumbild lieblicher erschien:", "tokens": ["dem", "nie", "ein", "Traum\u00b7bild", "lieb\u00b7li\u00b7cher", "er\u00b7schien", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein lautes Echo donnernd fortzutragen,", "tokens": ["Ein", "lau\u00b7tes", "E\u00b7cho", "don\u00b7nernd", "fort\u00b7zu\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "schien Strom und Tal und Felsen mir bereit;", "tokens": ["schien", "Strom", "und", "Tal", "und", "Fel\u00b7sen", "mir", "be\u00b7reit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "KON", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch \u2013 grad heraus, man darf das Wort nicht wagen!", "tokens": ["Doch", "\u2013", "grad", "he\u00b7raus", ",", "man", "darf", "das", "Wort", "nicht", "wa\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "ADV", "PTKVZ", "$,", "PIS", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das freie Wort, ihr wi\u00dft es, ist gefeit!", "tokens": ["Das", "frei\u00b7e", "Wort", ",", "ihr", "wi\u00dft", "es", ",", "ist", "ge\u00b7feit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wer hat nun Recht zu sagen und zu singen", "tokens": ["Wer", "hat", "nun", "Recht", "zu", "sa\u00b7gen", "und", "zu", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ADV", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "vom freien Rhein, dem freien deutschen Sohn?", "tokens": ["vom", "frei\u00b7en", "Rhein", ",", "dem", "frei\u00b7en", "deut\u00b7schen", "Sohn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NE", "$,", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O diese Lieder, die so mutig klingen,", "tokens": ["O", "die\u00b7se", "Lie\u00b7der", ",", "die", "so", "mu\u00b7tig", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "$,", "PRELS", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "beim ew'gen Gott, sie d\u00fcnken mich wie Hohn.", "tokens": ["beim", "ew'\u00b7gen", "Gott", ",", "sie", "d\u00fcn\u00b7ken", "mich", "wie", "Hohn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ja, wolltet ihr erw\u00e4gen und bedenken,", "tokens": ["Ja", ",", "woll\u00b7tet", "ihr", "er\u00b7w\u00e4\u00b7gen", "und", "be\u00b7den\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VMFIN", "PPER", "VVFIN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "welch stolzes Wort von eurer Lippe kam,", "tokens": ["welch", "stol\u00b7zes", "Wort", "von", "eu\u00b7rer", "Lip\u00b7pe", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "ihr m\u00fc\u00dftet ja das Auge niedersenken,", "tokens": ["ihr", "m\u00fc\u00df\u00b7tet", "ja", "das", "Au\u00b7ge", "nie\u00b7der\u00b7sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "mit bittern Tr\u00e4nen voller Zorn und Scham! \u2013", "tokens": ["mit", "bit\u00b7tern", "Tr\u00e4\u00b7nen", "vol\u00b7ler", "Zorn", "und", "Scham", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Es gilt nicht dir, der du zuerst gesungen", "tokens": ["Es", "gilt", "nicht", "dir", ",", "der", "du", "zu\u00b7erst", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPER", "$,", "PRELS", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "das stolze Wort vom freien deutschen Rhein,", "tokens": ["das", "stol\u00b7ze", "Wort", "vom", "frei\u00b7en", "deut\u00b7schen", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "das durch die Welt sich adlergleich geschwungen:", "tokens": ["das", "durch", "die", "Welt", "sich", "ad\u00b7lerg\u00b7leich", "ge\u00b7schwun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dich schlie\u00df' im Geist in meinen Arm ich ein!", "tokens": ["Dich", "schlie\u00df'", "im", "Geist", "in", "mei\u00b7nen", "Arm", "ich", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Aus voller Brust ist dir das Lied gequollen,", "tokens": ["Aus", "vol\u00b7ler", "Brust", "ist", "dir", "das", "Lied", "ge\u00b7quol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "und nicht im K\u00e4fig hast du es bewahrt:", "tokens": ["und", "nicht", "im", "K\u00e4\u00b7fig", "hast", "du", "es", "be\u00b7wahrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPRART", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Frei fliegt es hin, wohin die Winde wollen \u2013", "tokens": ["Frei", "fliegt", "es", "hin", ",", "wo\u00b7hin", "die", "Win\u00b7de", "wol\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "ART", "NN", "VMFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Du tatest recht! Und das ist S\u00e4ngerart.", "tokens": ["Du", "ta\u00b7test", "recht", "!", "Und", "das", "ist", "S\u00e4n\u00b7ge\u00b7rart", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "KON", "PDS", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Euch gilt mein Ruf, ihr F\u00fcrsten und Vasallen,", "tokens": ["Euch", "gilt", "mein", "Ruf", ",", "ihr", "F\u00fcrs\u00b7ten", "und", "Va\u00b7sal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "in deren H\u00e4nden unser Schicksal liegt! \u2013", "tokens": ["in", "de\u00b7ren", "H\u00e4n\u00b7den", "un\u00b7ser", "Schick\u00b7sal", "liegt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Euch Deutschen gilt es, nah und fern, euch allen,", "tokens": ["Euch", "Deut\u00b7schen", "gilt", "es", ",", "nah", "und", "fern", ",", "euch", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$,", "PPER", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "so weit ein Hauch von deutschem Munde fliegt:", "tokens": ["so", "weit", "ein", "Hauch", "von", "deut\u00b7schem", "Mun\u00b7de", "fliegt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit euch zuerst m\u00fc\u00dft ihr den Kampf beginnen!", "tokens": ["Mit", "euch", "zu\u00b7erst", "m\u00fc\u00dft", "ihr", "den", "Kampf", "be\u00b7gin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Soll unverf\u00fchrt von heiserem Geschrei", "tokens": ["Soll", "un\u00b7ver\u00b7f\u00fchrt", "von", "hei\u00b7se\u00b7rem", "Ge\u00b7schrei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "und ungetr\u00fcbt des Rheines Welle rinnen,", "tokens": ["und", "un\u00b7ge\u00b7tr\u00fcbt", "des", "Rhei\u00b7nes", "Wel\u00b7le", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "so seid zuerst ihr selber deutsch und frei!", "tokens": ["so", "seid", "zu\u00b7erst", "ihr", "sel\u00b7ber", "deutsch", "und", "frei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Denn k\u00e4me nun die Stunde der Gefahren,", "tokens": ["Denn", "k\u00e4\u00b7me", "nun", "die", "Stun\u00b7de", "der", "Ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die wir am Himmel d\u00e4mmernd schon gesehn,", "tokens": ["die", "wir", "am", "Him\u00b7mel", "d\u00e4m\u00b7mernd", "schon", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVPP", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "ich meine wohl, ihr w\u00fcrdet bald gewahren,", "tokens": ["ich", "mei\u00b7ne", "wohl", ",", "ihr", "w\u00fcr\u00b7det", "bald", "ge\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "da\u00df es nicht leicht ist, Schlachten zu bestehn.", "tokens": ["da\u00df", "es", "nicht", "leicht", "ist", ",", "Schlach\u00b7ten", "zu", "be\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VAFIN", "$,", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nicht jene Burgen werden niedersteigen,", "tokens": ["Nicht", "je\u00b7ne", "Bur\u00b7gen", "wer\u00b7den", "nie\u00b7ders\u00b7tei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PDAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "die M\u00e4dchen k\u00fcssen, aber k\u00e4mpfen nicht,", "tokens": ["die", "M\u00e4d\u00b7chen", "k\u00fcs\u00b7sen", ",", "a\u00b7ber", "k\u00e4mp\u00b7fen", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ADV", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "die stummen Fische, glaubt mir, werden schweigen,", "tokens": ["die", "stum\u00b7men", "Fi\u00b7sche", ",", "glaubt", "mir", ",", "wer\u00b7den", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "$,", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "und Ruder brechen, wo ein Reich zerbricht.", "tokens": ["und", "Ru\u00b7der", "bre\u00b7chen", ",", "wo", "ein", "Reich", "zer\u00b7bricht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "'s gibt einen andern, kr\u00e4ftigern Genossen", "tokens": ["'s", "gibt", "ei\u00b7nen", "an\u00b7dern", ",", "kr\u00e4f\u00b7ti\u00b7gern", "Ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "als jener Tr\u00fcmmer br\u00f6ckelndes Gestein:", "tokens": ["als", "je\u00b7ner", "Tr\u00fcm\u00b7mer", "br\u00f6\u00b7ckeln\u00b7des", "Ge\u00b7stein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer ihm den Arm, den Busen ihm erschlossen,", "tokens": ["Wer", "ihm", "den", "Arm", ",", "den", "Bu\u00b7sen", "ihm", "er\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "$,", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "der siegt durch ihn \u2013 und auch durch ihn allein!", "tokens": ["der", "siegt", "durch", "ihn", "\u2013", "und", "auch", "durch", "ihn", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "APPR", "PPER", "$(", "KON", "ADV", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Feuer ist's, das unausl\u00f6schlich z\u00fcndet,", "tokens": ["Ein", "Feu\u00b7er", "ist's", ",", "das", "un\u00b7aus\u00b7l\u00f6schlich", "z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "ein Zauberwort, das Mauern niederrei\u00dft \u2013", "tokens": ["ein", "Zau\u00b7ber\u00b7wort", ",", "das", "Mau\u00b7ern", "nie\u00b7der\u00b7rei\u00dft", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "drum frisch gewagt und euch mit ihm verb\u00fcndet:", "tokens": ["drum", "frisch", "ge\u00b7wagt", "und", "euch", "mit", "ihm", "ver\u00b7b\u00fcn\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "VVPP", "KON", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Es ist der deutsche, ist der freie Geist!", "tokens": ["Es", "ist", "der", "deut\u00b7sche", ",", "ist", "der", "frei\u00b7e", "Geist", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Gebt frei das Wort, ihr Herrn auf euren Thronen!", "tokens": ["Gebt", "frei", "das", "Wort", ",", "ihr", "Herrn", "auf", "eu\u00b7ren", "Thro\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "ART", "NN", "$,", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So wird das andre sich von selbst befrein.", "tokens": ["So", "wird", "das", "and\u00b7re", "sich", "von", "selbst", "be\u00b7fr\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "PRF", "APPR", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wagt's und vertraut! In allen euren Kronen,", "tokens": ["Wagt's", "und", "ver\u00b7traut", "!", "In", "al\u00b7len", "eu\u00b7ren", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$.", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "wo gibt's ein hellres, edleres Gestein?", "tokens": ["wo", "gibt's", "ein", "hell\u00b7res", ",", "ed\u00b7le\u00b7res", "Ge\u00b7stein", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Presse frei! Uns selber macht zum Richter,", "tokens": ["Die", "Pres\u00b7se", "frei", "!", "Uns", "sel\u00b7ber", "macht", "zum", "Rich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "PPER", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "das Volk ist reif! Ich wag's und sag' es laut:", "tokens": ["das", "Volk", "ist", "reif", "!", "Ich", "wag's", "und", "sag'", "es", "laut", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "PPER", "VVFIN", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Auf eure Weisen baut, auf eure Dichter,", "tokens": ["Auf", "eu\u00b7re", "Wei\u00b7sen", "baut", ",", "auf", "eu\u00b7re", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "sie, denen Gott noch Gr\u00f6\u00dfres auch vertraut!", "tokens": ["sie", ",", "de\u00b7nen", "Gott", "noch", "Gr\u00f6\u00df\u00b7res", "auch", "ver\u00b7traut", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "NN", "ADV", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Sei deutsch, mein Volk! Verlern den krummen R\u00fccken,", "tokens": ["Sei", "deutsch", ",", "mein", "Volk", "!", "Ver\u00b7lern", "den", "krum\u00b7men", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "PPOSAT", "NN", "$.", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "an den du selbst unw\u00fcrdig dich gew\u00f6hnt!", "tokens": ["an", "den", "du", "selbst", "un\u00b7w\u00fcr\u00b7dig", "dich", "ge\u00b7w\u00f6hnt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "ADV", "ADJD", "PPER", "VVPP", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.3": {"text": "Mit freier Stirn, gradaufw\u00e4rts mu\u00dft du blicken,", "tokens": ["Mit", "frei\u00b7er", "Stirn", ",", "grad\u00b7auf\u00b7w\u00e4rts", "mu\u00dft", "du", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "vom eignen Mut gesittigt und versch\u00f6nt.", "tokens": ["vom", "eig\u00b7nen", "Mut", "ge\u00b7sit\u00b7tigt", "und", "ver\u00b7sch\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es kann den F\u00fcrsten selber nicht gefallen,", "tokens": ["Es", "kann", "den", "F\u00fcrs\u00b7ten", "sel\u00b7ber", "nicht", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "dies schmeichlerisch dem\u00fctige Geschlecht \u2013", "tokens": ["dies", "schmeich\u00b7le\u00b7risch", "de\u00b7m\u00fc\u00b7ti\u00b7ge", "Ge\u00b7schlecht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "ein offnes Auge! So geziemt es allen,", "tokens": ["ein", "off\u00b7nes", "Au\u00b7ge", "!", "So", "ge\u00b7ziemt", "es", "al\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "zu Boden sieht das Tier nur und der Knecht. \u2013", "tokens": ["zu", "Bo\u00b7den", "sieht", "das", "Tier", "nur", "und", "der", "Knecht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ADV", "KON", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "So wird's erreicht! Und wenn in k\u00fcnft'gen Tagen", "tokens": ["So", "wird's", "er\u00b7reicht", "!", "Und", "wenn", "in", "k\u00fcnft'\u00b7gen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "VVPP", "$.", "KON", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "das stolze Frankreich unsern Rhein begehrt,", "tokens": ["das", "stol\u00b7ze", "Fran\u00b7kreich", "un\u00b7sern", "Rhein", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "KON", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "wir werden es mit L\u00e4cheln dann ertragen,", "tokens": ["wir", "wer\u00b7den", "es", "mit", "L\u00e4\u00b7cheln", "dann", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "dann ohne Lieder, doch die Hand am Schwert.", "tokens": ["dann", "oh\u00b7ne", "Lie\u00b7der", ",", "doch", "die", "Hand", "am", "Schwert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Denn dann gelang's, ihn ewig fest zu flechten:", "tokens": ["Denn", "dann", "ge\u00b7lang's", ",", "ihn", "e\u00b7wig", "fest", "zu", "flech\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "$,", "PPER", "ADJD", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die goldne Freiheit soll die Fessel sein!", "tokens": ["Die", "gold\u00b7ne", "Frei\u00b7heit", "soll", "die", "Fes\u00b7sel", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dann lohnt es sich, bis in den Tod zu fechten,", "tokens": ["Dann", "lohnt", "es", "sich", ",", "bis", "in", "den", "Tod", "zu", "fech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "KOUS", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "dann, deutsch und frei, dann bleibt er unser Rhein!", "tokens": ["dann", ",", "deutsch", "und", "frei", ",", "dann", "bleibt", "er", "un\u00b7ser", "Rhein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "KON", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}