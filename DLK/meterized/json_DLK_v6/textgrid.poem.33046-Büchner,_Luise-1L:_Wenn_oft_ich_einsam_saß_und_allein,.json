{"textgrid.poem.33046": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn oft ich einsam sa\u00df und allein,", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn oft ich einsam sa\u00df und allein,", "tokens": ["Wenn", "oft", "ich", "ein\u00b7sam", "sa\u00df", "und", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ADJD", "VVFIN", "KON", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Dann wiegte der lieblichste Traum mich ein,", "tokens": ["Dann", "wieg\u00b7te", "der", "lieb\u00b7lichs\u00b7te", "Traum", "mich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Sein weicher Arm mich liebend umschlang,", "tokens": ["Sein", "wei\u00b7cher", "Arm", "mich", "lie\u00b7bend", "um\u00b7schlang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Sein Mund die s\u00fc\u00dfesten Lieder sang.", "tokens": ["Sein", "Mund", "die", "s\u00fc\u00b7\u00dfes\u00b7ten", "Lie\u00b7der", "sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Er legt' auf's Herz sich erfrischend und mild,", "tokens": ["Er", "legt'", "auf's", "Herz", "sich", "er\u00b7fri\u00b7schend", "und", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PRF", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie Thau auf d\u00fcrstende Blumen quillt,", "tokens": ["Wie", "Thau", "auf", "d\u00fcrs\u00b7ten\u00b7de", "Blu\u00b7men", "quillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er s\u00e4uselt' um mich wie im Schilfe der Wind", "tokens": ["Er", "s\u00e4u\u00b7selt'", "um", "mich", "wie", "im", "Schil\u00b7fe", "der", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "KOKOM", "APPRART", "NN", "ART", "NN"], "meter": "-+--++-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und k\u00fchlte die brennende Stirne lind.", "tokens": ["Und", "k\u00fchl\u00b7te", "die", "bren\u00b7nen\u00b7de", "Stir\u00b7ne", "lind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Er war so heiter, so golden sch\u00f6n,", "tokens": ["Er", "war", "so", "hei\u00b7ter", ",", "so", "gol\u00b7den", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie die Sonne strahlt um der Berge H\u00f6h'n,", "tokens": ["Wie", "die", "Son\u00b7ne", "strahlt", "um", "der", "Ber\u00b7ge", "H\u00f6h'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wenn sie noch einmal aus Wolken bricht,", "tokens": ["Wenn", "sie", "noch", "ein\u00b7mal", "aus", "Wol\u00b7ken", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Eh' in Nacht versinket ihr gl\u00e4nzend Licht.", "tokens": ["Eh'", "in", "Nacht", "ver\u00b7sin\u00b7ket", "ihr", "gl\u00e4n\u00b7zend", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Umwoben von seinem Zauberband", "tokens": ["Um\u00b7wo\u00b7ben", "von", "sei\u00b7nem", "Zau\u00b7ber\u00b7band"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verga\u00df ich des Lebens Schmerz und Tand,", "tokens": ["Ver\u00b7ga\u00df", "ich", "des", "Le\u00b7bens", "Schmerz", "und", "Tand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "War reich von seliger Ahnung erf\u00fcllt,", "tokens": ["War", "reich", "von", "se\u00b7li\u00b7ger", "Ah\u00b7nung", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Wie einst sich des Herzens R\u00e4thsel enth\u00fcllt.", "tokens": ["Wie", "einst", "sich", "des", "Her\u00b7zens", "R\u00e4th\u00b7sel", "ent\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PRF", "ART", "NN", "NN", "VVPP", "$."], "meter": "----+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Und wenn ich traurig und m\u00fcde war,", "tokens": ["Und", "wenn", "ich", "trau\u00b7rig", "und", "m\u00fc\u00b7de", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dann schlo\u00df ich zum Traume mein Augenpaar,", "tokens": ["Dann", "schlo\u00df", "ich", "zum", "Trau\u00b7me", "mein", "Au\u00b7gen\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und tr\u00e4umte Frieden mir in die Brust,", "tokens": ["Und", "tr\u00e4um\u00b7te", "Frie\u00b7den", "mir", "in", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bis nicht mehr des Schmerzes ich war bewu\u00dft,", "tokens": ["Bis", "nicht", "mehr", "des", "Schmer\u00b7zes", "ich", "war", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PTKNEG", "ADV", "ART", "NN", "PPER", "VAFIN", "ADJD", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Bis Himmelswonne die Seele durchzog \u2013", "tokens": ["Bis", "Him\u00b7mels\u00b7won\u00b7ne", "die", "See\u00b7le", "durch\u00b7zog", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ach! da\u00df der grausame Traum nur log;", "tokens": ["Ach", "!", "da\u00df", "der", "grau\u00b7sa\u00b7me", "Traum", "nur", "log", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Er ist dahin, das Erwachen war schwer,", "tokens": ["Er", "ist", "da\u00b7hin", ",", "das", "Er\u00b7wa\u00b7chen", "war", "schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Herz, mein Herz, o, tr\u00e4ume nicht mehr!", "tokens": ["Herz", ",", "mein", "Herz", ",", "o", ",", "tr\u00e4u\u00b7me", "nicht", "mehr", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "FM", "$,", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}