{"dta.poem.15642": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "75.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bist du gedankenlos, so geht mit offnen Ohren,", "tokens": ["Bist", "du", "ge\u00b7dan\u00b7ken\u00b7los", ",", "so", "geht", "mit", "off\u00b7nen", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit offnen Augen dir der Sinn der Welt verloren.", "tokens": ["Mit", "off\u00b7nen", "Au\u00b7gen", "dir", "der", "Sinn", "der", "Welt", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Sinne sind dir voll, doch hast du nichts davon;", "tokens": ["Die", "Sin\u00b7ne", "sind", "dir", "voll", ",", "doch", "hast", "du", "nichts", "da\u00b7von", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$,", "ADV", "VAFIN", "PPER", "PIS", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Aug' erlischt das Bild, im Ohre stirbt der Ton.", "tokens": ["Im", "Aug'", "er\u00b7lischt", "das", "Bild", ",", "im", "Oh\u00b7re", "stirbt", "der", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Bist du gedankenvoll, so geht es dir noch schlimmer,", "tokens": ["Bist", "du", "ge\u00b7dan\u00b7ken\u00b7voll", ",", "so", "geht", "es", "dir", "noch", "schlim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du merkst nur dumpf um dich verworrnen Klang und Schimmer.", "tokens": ["Du", "merkst", "nur", "dumpf", "um", "dich", "ver\u00b7worr\u00b7nen", "Klang", "und", "Schim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "PPER", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Den Sinnen selbst entgeht der Au\u00dfenwelt Gewinnst,", "tokens": ["Den", "Sin\u00b7nen", "selbst", "ent\u00b7geht", "der", "Au\u00b7\u00dfen\u00b7welt", "Ge\u00b7winnst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil du im Inneren Gedankenf\u00e4den spinnst.", "tokens": ["Weil", "du", "im", "In\u00b7ne\u00b7ren", "Ge\u00b7dan\u00b7ken\u00b7f\u00e4\u00b7den", "spinnst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Begl\u00fcckt nur, wenn du so zu spinnen lernst den Faden,", "tokens": ["Be\u00b7gl\u00fcckt", "nur", ",", "wenn", "du", "so", "zu", "spin\u00b7nen", "lernst", "den", "Fa\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PPER", "ADV", "PTKZU", "VVINF", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er den Dingen nicht, noch ihm die Dinge schaden;", "tokens": ["Da\u00df", "er", "den", "Din\u00b7gen", "nicht", ",", "noch", "ihm", "die", "Din\u00b7ge", "scha\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "$,", "ADV", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wenn offner Sinn ergreift und h\u00e4lt der Bilder Schwanken,", "tokens": ["Wenn", "off\u00b7ner", "Sinn", "er\u00b7greift", "und", "h\u00e4lt", "der", "Bil\u00b7der", "Schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und das Gem\u00fcth daraus webt ewige Gedanken.", "tokens": ["Und", "das", "Ge\u00b7m\u00fcth", "da\u00b7raus", "webt", "e\u00b7wi\u00b7ge", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PAV", "VVFIN", "ADJA", "NN", "$."], "meter": "+--+--++-+-+-", "measure": "dactylic.di.plus"}}}}}