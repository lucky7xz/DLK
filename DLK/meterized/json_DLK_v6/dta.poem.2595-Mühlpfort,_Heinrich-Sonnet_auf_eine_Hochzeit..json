{"dta.poem.2595": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet auf eine Hochzeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nun will Cupido sich mit Myrten wieder schm\u00fccken/", "tokens": ["Nun", "will", "Cu\u00b7pi\u00b7do", "sich", "mit", "Myr\u00b7ten", "wie\u00b7der", "schm\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NE", "PRF", "APPR", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er legt die Wermuth hin so nur bey Gr\u00e4bern n\u00fctzt/", "tokens": ["Er", "legt", "die", "Wer\u00b7muth", "hin", "so", "nur", "bey", "Gr\u00e4\u00b7bern", "n\u00fctzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADV", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und weil die Mutter selbst im Sternen Schmucke blitzt/", "tokens": ["Und", "weil", "die", "Mut\u00b7ter", "selbst", "im", "Ster\u00b7nen", "Schmu\u00b7cke", "blitzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "APPRART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So ist er auch bem\u00fcht sich zu dem Fest zu schicken/", "tokens": ["So", "ist", "er", "auch", "be\u00b7m\u00fcht", "sich", "zu", "dem", "Fest", "zu", "schi\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er sieht die sch\u00f6ne Braut mit h\u00f6chster Anmuth blicken/", "tokens": ["Er", "sieht", "die", "sch\u00f6\u00b7ne", "Braut", "mit", "h\u00f6chs\u00b7ter", "An\u00b7muth", "bli\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wie der holde Glantz des Liebsten Hertze ritzt.", "tokens": ["Und", "wie", "der", "hol\u00b7de", "Glantz", "des", "Liebs\u00b7ten", "Hert\u00b7ze", "ritzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er lacht/ da\u00df er so scharff hat seinen Pfeil gespitzt/", "tokens": ["Er", "lacht", "/", "da\u00df", "er", "so", "scharff", "hat", "sei\u00b7nen", "Pfeil", "ge\u00b7spitzt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und ihn so meisterlich kan in das Hertze dr\u00fccken.", "tokens": ["Und", "ihn", "so", "meis\u00b7ter\u00b7lich", "kan", "in", "das", "Hert\u00b7ze", "dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJD", "VMFIN", "APPR", "PDS", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was will sie/ Jungfer Braut/ noch ihre Glut verdecken?", "tokens": ["Was", "will", "sie", "/", "Jung\u00b7fer", "Braut", "/", "noch", "ih\u00b7re", "Glut", "ver\u00b7de\u00b7cken", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$(", "NE", "NN", "$(", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Liebe deckt man nicht wie sehr man sie verhelt/", "tokens": ["Die", "Lie\u00b7be", "deckt", "man", "nicht", "wie", "sehr", "man", "sie", "ver\u00b7helt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "PTKNEG", "KOKOM", "ADV", "PIS", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Jhr ewig Zunder brennt im Mittelpunct der Seel/", "tokens": ["Ihr", "e\u00b7wig", "Zun\u00b7der", "brennt", "im", "Mit\u00b7tel\u00b7punct", "der", "Seel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die nichts als Gegengunst des Liebsten kan erstecken.", "tokens": ["Die", "nichts", "als", "Ge\u00b7gen\u00b7gunst", "des", "Liebs\u00b7ten", "kan", "er\u00b7ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "KOKOM", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und weil sie der gewi\u00df/ so mag sie sich drauf freuen/", "tokens": ["Und", "weil", "sie", "der", "ge\u00b7wi\u00df", "/", "so", "mag", "sie", "sich", "drauf", "freu\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "ADV", "$(", "ADV", "VMFIN", "PPER", "PRF", "PAV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Venus selbsten will umbs Bette Blumen streuen.", "tokens": ["Die", "Ve\u00b7nus", "selbs\u00b7ten", "will", "umbs", "Bet\u00b7te", "Blu\u00b7men", "streu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "APPRART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}