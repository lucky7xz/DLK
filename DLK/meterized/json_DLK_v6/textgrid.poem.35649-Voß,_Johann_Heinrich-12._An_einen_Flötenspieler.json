{"textgrid.poem.35649": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "12. An einen Fl\u00f6tenspieler", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der du mein Herz entf\u00fchrtest,", "tokens": ["Der", "du", "mein", "Herz", "ent\u00b7f\u00fchr\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als deinem allmachtvollen Hauch", "tokens": ["Als", "dei\u00b7nem", "all\u00b7macht\u00b7vol\u00b7len", "Hauch"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Fl\u00f6te Ton", "tokens": ["Der", "Fl\u00f6\u00b7te", "Ton"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Im leisen Schmachten sich verlor;", "tokens": ["Im", "lei\u00b7sen", "Schmach\u00b7ten", "sich", "ver\u00b7lor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dann schnell durch tausend Irren", "tokens": ["Dann", "schnell", "durch", "tau\u00b7send", "Ir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum h\u00f6chsten, hellen Gipfel stieg,", "tokens": ["Zum", "h\u00f6chs\u00b7ten", ",", "hel\u00b7len", "Gip\u00b7fel", "stieg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und silberrein,", "tokens": ["Und", "sil\u00b7ber\u00b7rein", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Wie Philomele, trillerte.", "tokens": ["Wie", "Phi\u00b7lo\u00b7me\u00b7le", ",", "tril\u00b7ler\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PWAV", "NE", "$,", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "O! mehr hast du gesieget,", "tokens": ["O", "!", "mehr", "hast", "du", "ge\u00b7sie\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-----+-", "measure": "unknown.measure.single"}, "line.2": {"text": "Als jene fabelhafte Schar,", "tokens": ["Als", "je\u00b7ne", "fa\u00b7bel\u00b7haf\u00b7te", "Schar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Fels und Wald", "tokens": ["Die", "Fels", "und", "Wald"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Hy\u00e4n' und Tiger f\u00fchlen hie\u00df,", "tokens": ["Hy\u00b7\u00e4n'", "und", "Ti\u00b7ger", "f\u00fch\u00b7len", "hie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Das eitle Flitterm\u00e4dchen", "tokens": ["Das", "eit\u00b7le", "Flit\u00b7ter\u00b7m\u00e4d\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verga\u00df bei dir des F\u00e4cherspiels,", "tokens": ["Ver\u00b7ga\u00df", "bei", "dir", "des", "F\u00e4\u00b7cher\u00b7spiels", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des losen Winks,", "tokens": ["Des", "lo\u00b7sen", "Winks", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und aller ihrer M\u00e4dchenkunst.", "tokens": ["Und", "al\u00b7ler", "ih\u00b7rer", "M\u00e4d\u00b7chen\u00b7kunst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Des kl\u00fcgelnden Gefl\u00fcsters", "tokens": ["Des", "kl\u00fc\u00b7geln\u00b7den", "Ge\u00b7fl\u00fcs\u00b7ters"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verga\u00df das bunte Stutzerchen,", "tokens": ["Ver\u00b7ga\u00df", "das", "bun\u00b7te", "Stut\u00b7zer\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verga\u00df es selbst,", "tokens": ["Ver\u00b7ga\u00df", "es", "selbst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Da\u00df du ein deutscher Meister warst.", "tokens": ["Da\u00df", "du", "ein", "deut\u00b7scher", "Meis\u00b7ter", "warst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}