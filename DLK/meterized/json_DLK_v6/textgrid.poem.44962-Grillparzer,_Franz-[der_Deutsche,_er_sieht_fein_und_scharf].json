{"textgrid.poem.44962": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[der Deutsche, er sieht fein und scharf]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Deutsche, er sieht fein und scharf,", "tokens": ["Der", "Deut\u00b7sche", ",", "er", "sieht", "fein", "und", "scharf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Fehlts nicht an einem Augenglase,", "tokens": ["Fehlts", "nicht", "an", "ei\u00b7nem", "Au\u00b7gen\u00b7gla\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit einem Kommentar auf seiner Nase", "tokens": ["Mit", "ei\u00b7nem", "Kom\u00b7men\u00b7tar", "auf", "sei\u00b7ner", "Na\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schaut weiter er, als man erwarten darf,", "tokens": ["Schaut", "wei\u00b7ter", "er", ",", "als", "man", "er\u00b7war\u00b7ten", "darf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "$,", "KOUS", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}}, "stanza.2": {"line.1": {"text": "Erforscht der Dichter Herz und Nieren,", "tokens": ["Er\u00b7forscht", "der", "Dich\u00b7ter", "Herz", "und", "Nie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kennt jede Schwellung ihrer Brust,", "tokens": ["Kennt", "je\u00b7de", "Schwel\u00b7lung", "ih\u00b7rer", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wei\u00df mehr von Dante und Shakespearen,", "tokens": ["Wei\u00df", "mehr", "von", "Dan\u00b7te", "und", "Sha\u00b7ke\u00b7spe\u00b7a\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NE", "KON", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Als jene beide selbst gewu\u00dft.", "tokens": ["Als", "je\u00b7ne", "bei\u00b7de", "selbst", "ge\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Allein, gebrichts am Augenglase,", "tokens": ["Al\u00b7lein", ",", "ge\u00b7brichts", "am", "Au\u00b7gen\u00b7gla\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verdunkelt sich sein bl\u00f6der Stern,", "tokens": ["Ver\u00b7dun\u00b7kelt", "sich", "sein", "bl\u00f6\u00b7der", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, was geschieht vor seiner Nase,", "tokens": ["Und", ",", "was", "ge\u00b7schieht", "vor", "sei\u00b7ner", "Na\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Liegt ihm auf hundert Meilen fern.", "tokens": ["Liegt", "ihm", "auf", "hun\u00b7dert", "Mei\u00b7len", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}