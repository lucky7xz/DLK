{"textgrid.poem.32244": {"metadata": {"author": {"name": "Fischart, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was hilffts, O Teutschland, da\u00df dir gfallt", "genre": "verse", "period": "N.A.", "pub_year": 1568, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was hilffts, O Teutschland, da\u00df dir gfallt", "tokens": ["Was", "hilffts", ",", "O", "Teutschland", ",", "da\u00df", "dir", "gfallt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "NE", "NE", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Dis Bild so herrlich Sighafft gstallt,", "tokens": ["Dis", "Bild", "so", "herr\u00b7lich", "Sig\u00b7hafft", "gstallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADV", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df es bedeit der Teutschen Macht,", "tokens": ["Da\u00df", "es", "be\u00b7deit", "der", "Teut\u00b7schen", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die vnter sich der Welt Macht bracht,", "tokens": ["Die", "vn\u00b7ter", "sich", "der", "Welt", "Macht", "bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PRF", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd da\u00df du weyst, da\u00df dein Vralten", "tokens": ["Vnd", "da\u00df", "du", "weyst", ",", "da\u00df", "dein", "Vral\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Den Namen mit Ruhm han erhalten,", "tokens": ["Den", "Na\u00b7men", "mit", "Ruhm", "han", "er\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Wann du dasselbig last veralten,", "tokens": ["Wann", "du", "das\u00b7sel\u00b7big", "last", "ver\u00b7al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Was dein Voralten dir erhalten,", "tokens": ["Was", "dein", "Vor\u00b7al\u00b7ten", "dir", "er\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wann nicht dasselbig willt verwaren,", "tokens": ["Wann", "nicht", "das\u00b7sel\u00b7big", "willt", "ver\u00b7wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Was dein Vorfahren dir vorsparen,", "tokens": ["Was", "dein", "Vor\u00b7fah\u00b7ren", "dir", "vor\u00b7spa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Wann nicht den Namen willt vermehren,", "tokens": ["Wann", "nicht", "den", "Na\u00b7men", "willt", "ver\u00b7meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Der auff dich erbt von grossen Ehren?", "tokens": ["Der", "auff", "dich", "erbt", "von", "gros\u00b7sen", "Eh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Was ists, da\u00df man sich r\u00fchmet hoch", "tokens": ["Was", "ists", ",", "da\u00df", "man", "sich", "r\u00fch\u00b7met", "hoch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "KOUS", "PIS", "PRF", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Der Elttern vnd folgt jhn nicht noch,", "tokens": ["Der", "Elt\u00b7tern", "vnd", "folgt", "jhn", "nicht", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.15": {"text": "Bist Alter Tugent grosser Preiser,", "tokens": ["Bist", "Al\u00b7ter", "Tu\u00b7gent", "gros\u00b7ser", "Prei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Aber der Tugend keyn Erweiser,", "tokens": ["A\u00b7ber", "der", "Tu\u00b7gend", "keyn", "Er\u00b7wei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.17": {"text": "Thust gut Alt Sitten hoch erheben,", "tokens": ["Thust", "gut", "Alt", "Sit\u00b7ten", "hoch", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Vnd schickst doch nit darnach dein leben?", "tokens": ["Vnd", "schickst", "doch", "nit", "dar\u00b7nach", "dein", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "PAV", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Was r\u00fchmst du dich vil Adelichs", "tokens": ["Was", "r\u00fchmst", "du", "dich", "vil", "A\u00b7de\u00b7lichs"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Vnd thust doch nichts, dan Tadelichs?", "tokens": ["Vnd", "thust", "doch", "nichts", ",", "dan", "Ta\u00b7de\u00b7lichs", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIS", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Was Ruhm hat der jung Adler doch,", "tokens": ["Was", "Ruhm", "hat", "der", "jung", "Ad\u00b7ler", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "ART", "ADJD", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Wann er sich r\u00fchmt der Eltern hoch,", "tokens": ["Wann", "er", "sich", "r\u00fchmt", "der", "El\u00b7tern", "hoch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Wie sie frei wohnten inn Bergs kl\u00fcfften", "tokens": ["Wie", "sie", "frei", "wohn\u00b7ten", "inn", "Bergs", "kl\u00fcff\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "APPR", "NE", "VVFIN"], "meter": "+-++-+-+-", "measure": "unknown.measure.penta"}, "line.24": {"text": "Vnd frei Regierten inn den L\u00fcfften,", "tokens": ["Vnd", "frei", "Re\u00b7gier\u00b7ten", "inn", "den", "L\u00fcff\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Vnd er sitzt gfesselt auff der Stangen,", "tokens": ["Vnd", "er", "sitzt", "gfes\u00b7selt", "auff", "der", "Stan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.26": {"text": "Mu\u00df, was der Mensch nur will, jm fangen?", "tokens": ["Mu\u00df", ",", "was", "der", "Mensch", "nur", "will", ",", "jm", "fan\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "$,", "PRELS", "ART", "NN", "ADV", "VMFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Also was ist dir f\u00fcr eyn Ehr,", "tokens": ["Al\u00b7so", "was", "ist", "dir", "f\u00fcr", "eyn", "Ehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Wann r\u00fchmst die Alten Teutschen sehr,", "tokens": ["Wann", "r\u00fchmst", "die", "Al\u00b7ten", "Teut\u00b7schen", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Wie sie f\u00fcr jhre Freiheyt stritten", "tokens": ["Wie", "sie", "f\u00fcr", "jhre", "Frei\u00b7heyt", "strit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Vnd keynen b\u00f6sen Nachbarn litten,", "tokens": ["Vnd", "key\u00b7nen", "b\u00f6\u00b7sen", "Nach\u00b7barn", "lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Vnd du achtst nicht der Freiheyt dein,", "tokens": ["Vnd", "du", "achtst", "nicht", "der", "Frei\u00b7heyt", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Kanst kaum inn deim Land sicher sein,", "tokens": ["Kanst", "kaum", "inn", "deim", "Land", "si\u00b7cher", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PPOSAT", "NN", "ADJD", "VAINF", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.33": {"text": "Last dir dein Nachbarn sein Pferd binden,", "tokens": ["Last", "dir", "dein", "Nach\u00b7barn", "sein", "Pferd", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+--+-++--", "measure": "iambic.tetra.invert"}, "line.34": {"text": "An deinen Zaun fornen vnd hinden?", "tokens": ["An", "dei\u00b7nen", "Zaun", "for\u00b7nen", "vnd", "hin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.35": {"text": "Sollt auch solch Feyger art geb\u00fcren,", "tokens": ["Sollt", "auch", "solch", "Fey\u00b7ger", "art", "ge\u00b7b\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Da\u00df sie soll Kron vnd Scepter f\u00fchren?", "tokens": ["Da\u00df", "sie", "soll", "Kron", "vnd", "Scep\u00b7ter", "f\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Ja jr gb\u00fcrt f\u00fcr den K\u00f6nigsstab", "tokens": ["Ja", "jr", "gb\u00fcrt", "f\u00fcr", "den", "K\u00f6\u00b7nigs\u00b7stab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Eyn H\u00f6ltzin Ro\u00df, welchs sie nur hab,", "tokens": ["Eyn", "H\u00f6lt\u00b7zin", "Ro\u00df", ",", "welchs", "sie", "nur", "hab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,", "PRELS", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Vnd f\u00fchre f\u00fcr den Adler K\u00fcn", "tokens": ["Vnd", "f\u00fch\u00b7re", "f\u00fcr", "den", "Ad\u00b7ler", "K\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Eyn bundte Atzel nun forthin,", "tokens": ["Eyn", "bund\u00b7te", "At\u00b7zel", "nun", "for\u00b7thin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Vnd f\u00fcr den Weltapffel eyn Ball,", "tokens": ["Vnd", "f\u00fcr", "den", "Wel\u00b7tapf\u00b7fel", "eyn", "Ball", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Den man schl\u00e4gt, wann er hupfft im fall,", "tokens": ["Den", "man", "schl\u00e4gt", ",", "wann", "er", "hupfft", "im", "fall", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.43": {"text": "Weil heut doch schier keym ernst ist mehr,", "tokens": ["Weil", "heut", "doch", "schier", "keym", "ernst", "ist", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ADJD", "APPRART", "ADJD", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Handzuhaben Freiheyt vnd Ehr,", "tokens": ["Hand\u00b7zu\u00b7ha\u00b7ben", "Frei\u00b7heyt", "vnd", "Ehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.45": {"text": "Sonder man schertzt nur mit der Freiheyt,", "tokens": ["Son\u00b7der", "man", "schertzt", "nur", "mit", "der", "Frei\u00b7heyt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.46": {"text": "Sucht fremde Sitten, Br\u00e4uch vnd Neuheyt,", "tokens": ["Sucht", "frem\u00b7de", "Sit\u00b7ten", ",", "Br\u00e4uch", "vnd", "Neu\u00b7heyt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Vnd f\u