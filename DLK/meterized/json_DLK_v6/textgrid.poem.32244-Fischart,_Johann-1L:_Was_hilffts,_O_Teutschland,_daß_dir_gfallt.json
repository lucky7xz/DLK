.relaxed"}, "line.53": {"text": "Nichts ist, da\u00df man f\u00fcrmalt die Welt", "tokens": ["Nichts", "ist", ",", "da\u00df", "man", "f\u00fcr\u00b7malt", "die", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "$,", "KOUS", "PIS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Vnd kaum eyn Stuck der Welt erh\u00e4llt;", "tokens": ["Vnd", "kaum", "eyn", "Stuck", "der", "Welt", "er\u00b7h\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Sonder man mu\u00df erweisen fein", "tokens": ["Son\u00b7der", "man", "mu\u00df", "er\u00b7wei\u00b7sen", "fein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VMFIN", "VVINF", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.56": {"text": "Di\u00df, des man will ger\u00fchmet sein,", "tokens": ["Di\u00df", ",", "des", "man", "will", "ge\u00b7r\u00fch\u00b7met", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ART", "PIS", "VMFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.57": {"text": "Vnd nicht der Alten Wacker Thaten", "tokens": ["Vnd", "nicht", "der", "Al\u00b7ten", "Wa\u00b7cker", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.58": {"text": "Sch\u00e4nden mit vnthun vngerahten.", "tokens": ["Sch\u00e4n\u00b7den", "mit", "vn\u00b7thun", "vn\u00b7ge\u00b7rah\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.2"