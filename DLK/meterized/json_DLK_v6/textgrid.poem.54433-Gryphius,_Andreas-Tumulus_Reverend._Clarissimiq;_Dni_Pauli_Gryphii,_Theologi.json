{"textgrid.poem.54433": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Tumulus Reverend. Clarissimiq; Dni Pauli Gryphii, Theologi", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Christum frey bekand/ vnd seine Stimm erhoben/", "tokens": ["Der", "Chris\u00b7tum", "frey", "be\u00b7kand", "/", "vnd", "sei\u00b7ne", "Stimm", "er\u00b7ho\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$(", "KON", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gleich einer Feld Posaun/ den holt Er au\u00df der Welt/", "tokens": ["Gleich", "ei\u00b7ner", "Feld", "Po\u00b7saun", "/", "den", "holt", "Er", "au\u00df", "der", "Welt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NE", "$(", "ART", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Eh denn die Blutt-Tromet au\u00df seines Grimmes Zelt", "tokens": ["Eh", "denn", "die", "Blut\u00b7tTro\u00b7met", "au\u00df", "sei\u00b7nes", "Grim\u00b7mes", "Zelt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ART", "NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "+--++--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Erschall/ vnd eh sein Zorn so scharff fieng an zu toben;", "tokens": ["Er\u00b7schall", "/", "vnd", "eh", "sein", "Zorn", "so", "scharff", "fi\u00b7eng", "an", "zu", "to\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KON", "KOUS", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Sein Leib ligt hier vnnd ruht gantz sicher bi\u00df von oben", "tokens": ["Sein", "Leib", "ligt", "hier", "vnnd", "ruht", "gantz", "si\u00b7cher", "bi\u00df", "von", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "KON", "VVFIN", "ADV", "ADJD", "ADV", "APPR", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Prinz wird brechen ein/ dehm jeder vorgestellt", "tokens": ["Der", "Prinz", "wird", "bre\u00b7chen", "ein", "/", "dehm", "je\u00b7der", "vor\u00b7ge\u00b7stellt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVINF", "ART", "$(", "PRELS", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sol werden/ dehn der Todt in seinen Armen helt.", "tokens": ["Sol", "wer\u00b7den", "/", "dehn", "der", "Todt", "in", "sei\u00b7nen", "Ar\u00b7men", "helt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VAINF", "$(", "VVIMP", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Seel ist schon bey dehn/ die Gott dort Ewig loben/", "tokens": ["Die", "Seel", "ist", "schon", "bey", "dehn", "/", "die", "Gott", "dort", "E\u00b7wig", "lo\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "NE", "$(", "ART", "NN", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vnd wartet auff die Cron/ darmit ihr trewer flei\u00df/", "tokens": ["Vnd", "war\u00b7tet", "auff", "die", "Cron", "/", "dar\u00b7mit", "ihr", "tre\u00b7wer", "flei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$(", "PAV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ihr Lehren/ Ihre M\u00fch/ Ihr K\u00e4mpffen/ Angst vnd Schwei\u00df/", "tokens": ["Ihr", "Leh\u00b7ren", "/", "Ih\u00b7re", "M\u00fch", "/", "Ihr", "K\u00e4mpf\u00b7fen", "/", "Angst", "vnd", "Schwei\u00df", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ihr Eyfer/ welcher nie der frechen Laster schonet/", "tokens": ["Ihr", "Ey\u00b7fer", "/", "wel\u00b7cher", "nie", "der", "fre\u00b7chen", "Las\u00b7ter", "scho\u00b7net", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ihr wisssen das allein auff Gottes Ehr gewand/", "tokens": ["Ihr", "wiss\u00b7sen", "das", "al\u00b7lein", "auff", "Got\u00b7tes", "Ehr", "ge\u00b7wand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "ADV", "APPR", "NN", "NN", "VAPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ihr Leiden vnd Gedult/ vons Richters rechten Hand/", "tokens": ["Ihr", "Lei\u00b7den", "vnd", "Ge\u00b7dult", "/", "vons", "Rich\u00b7ters", "rech\u00b7ten", "Hand", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$(", "NE", "NE", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "An jenem grossen Tag sol werden wol belohnet.", "tokens": ["An", "je\u00b7nem", "gros\u00b7sen", "Tag", "sol", "wer\u00b7den", "wol", "be\u00b7loh\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VMFIN", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}