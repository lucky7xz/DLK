{"textgrid.poem.59184": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "403. Armut ist s\u00fc\u00df, dem der sie liebt", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Armut Tugend ist zu sch\u00e4tzen;", "tokens": ["Der", "Ar\u00b7mut", "Tu\u00b7gend", "ist", "zu", "sch\u00e4t\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie ist ein Kreuz der Wollust, ein Erg\u00f6tzen,", "tokens": ["Sie", "ist", "ein", "Kreuz", "der", "Wol\u00b7lust", ",", "ein", "Er\u00b7g\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das keine Last noch St\u00f6rung gibt,", "tokens": ["Das", "kei\u00b7ne", "Last", "noch", "St\u00f6\u00b7rung", "gibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn er nur selbst, der Arme, Armut liebt.", "tokens": ["Wenn", "er", "nur", "selbst", ",", "der", "Ar\u00b7me", ",", "Ar\u00b7mut", "liebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "$,", "ART", "NN", "$,", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}