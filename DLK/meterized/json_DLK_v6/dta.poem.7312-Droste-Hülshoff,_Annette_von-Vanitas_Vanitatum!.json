{"dta.poem.7312": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Vanitas Vanitatum!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr saht ihn nicht im Gl\u00fccke,", "tokens": ["Ihr", "saht", "ihn", "nicht", "im", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als Schaaren ihm gefolgt,", "tokens": ["Als", "Schaa\u00b7ren", "ihm", "ge\u00b7folgt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Einem seiner Blicke", "tokens": ["Mit", "Ei\u00b7nem", "sei\u00b7ner", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er jeden Ha\u00df erdolcht,", "tokens": ["Er", "je\u00b7den", "Ha\u00df", "er\u00b7dolcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Blut an seinen H\u00e4nden", "tokens": ["Das", "Blut", "an", "sei\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie K\u00f6nigspurpur fast,", "tokens": ["Wie", "K\u00f6\u00b7nigs\u00b7pur\u00b7pur", "fast", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und flammenden Gel\u00e4nden", "tokens": ["Und", "flam\u00b7men\u00b7den", "Ge\u00b7l\u00e4n\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Entstieg des Nimbus Glast;", "tokens": ["Ent\u00b7stieg", "des", "Nim\u00b7bus", "Glast", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Saht nicht, wie stolz getragen", "tokens": ["Saht", "nicht", ",", "wie", "stolz", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schulfreund und Kamerad", "tokens": ["Schul\u00b7freund", "und", "Ka\u00b7me\u00b7rad"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Stirn, mit welchem Zagen", "tokens": ["Die", "Stirn", ",", "mit", "wel\u00b7chem", "Za\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PWAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Fremdling ihm genaht,", "tokens": ["Der", "Fremd\u00b7ling", "ihm", "ge\u00b7naht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn mit Kolosses Schreiten", "tokens": ["Wenn", "mit", "Ko\u00b7los\u00b7ses", "Schrei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+----+-", "measure": "dactylic.init"}, "line.6": {"text": "Das Klippenthor er stie\u00df,", "tokens": ["Das", "Klip\u00b7pen\u00b7thor", "er", "stie\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die kleinen Segel gleiten", "tokens": ["Die", "klei\u00b7nen", "Se\u00b7gel", "glei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "An seiner Sohle lie\u00df.", "tokens": ["An", "sei\u00b7ner", "Soh\u00b7le", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr habt ihn nicht gesehen,", "tokens": ["Ihr", "habt", "ihn", "nicht", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Augen jugendklar,", "tokens": ["Ihr", "Au\u00b7gen", "ju\u00b7gend\u00b7klar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du Haupt wo Ringel wehen", "tokens": ["Du", "Haupt", "wo", "Rin\u00b7gel", "we\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "PWAV", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von s\u00fc\u00dfem Lockenhaar;", "tokens": ["Von", "s\u00fc\u00b7\u00dfem", "Lo\u00b7cken\u00b7haar", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "J\u00fcnglinge, bl\u00fchnde Frauen,", "tokens": ["J\u00fcng\u00b7lin\u00b7ge", ",", "bl\u00fchn\u00b7de", "Frau\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr saht ihn nicht im Glanz,", "tokens": ["Ihr", "saht", "ihn", "nicht", "im", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ihn, seines Landes Grauen", "tokens": ["Ihn", ",", "sei\u00b7nes", "Lan\u00b7des", "Grau\u00b7en"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und allergr\u00fcnsten Kranz.", "tokens": ["Und", "al\u00b7ler\u00b7gr\u00fcns\u00b7ten", "Kranz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Vielleicht doch saht ihr streifen", "tokens": ["Viel\u00b7leicht", "doch", "saht", "ihr", "strei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den alten kranken Leun,", "tokens": ["Den", "al\u00b7ten", "kran\u00b7ken", "Leun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Saht seine M\u00e4hne schleifen", "tokens": ["Saht", "sei\u00b7ne", "M\u00e4h\u00b7ne", "schlei\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und zittern sein Gebein,", "tokens": ["Und", "zit\u00b7tern", "sein", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Saht wie die breiten Pranken", "tokens": ["Saht", "wie", "die", "brei\u00b7ten", "Pran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Er matt und st\u00f6hnend hob,", "tokens": ["Er", "matt", "und", "st\u00f6h\u00b7nend", "hob", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie taumelnd seine Flanken", "tokens": ["Wie", "tau\u00b7melnd", "sei\u00b7ne", "Flan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Er l\u00e4ngs der Mauer schob.", "tokens": ["Er", "l\u00e4ngs", "der", "Mau\u00b7er", "schob", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und Scheitel saht ihr, wei\u00dfe,", "tokens": ["Und", "Schei\u00b7tel", "saht", "ihr", ",", "wei\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "$,", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am Fensterglase sp\u00e4hn,", "tokens": ["Am", "Fens\u00b7ter\u00b7gla\u00b7se", "sp\u00e4hn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die dann mit scheuem Flei\u00dfe", "tokens": ["Die", "dann", "mit", "scheu\u00b7em", "Flei\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich hinter'n Vorhang drehn;", "tokens": ["Sich", "hin\u00b7ter'n", "Vor\u00b7hang", "drehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vernahmt der Knaben Lachen,", "tokens": ["Ver\u00b7nahmt", "der", "Kna\u00b7ben", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Greise schmerzlich Ach,", "tokens": ["Der", "Grei\u00b7se", "schmerz\u00b7lich", "Ach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ITJ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wenn er im freien flachen", "tokens": ["Wenn", "er", "im", "frei\u00b7en", "fla\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gel\u00e4nd' zusammen brach.", "tokens": ["Ge\u00b7l\u00e4nd'", "zu\u00b7sam\u00b7men", "brach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Allein ihr horcht als rede", "tokens": ["Al\u00b7lein", "ihr", "horcht", "als", "re\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "KOKOM", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich von dem Tartarchan,", "tokens": ["Ich", "von", "dem", "Tar\u00b7tar\u00b7chan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Augen weit und \u00f6de", "tokens": ["Mit", "Au\u00b7gen", "weit", "und", "\u00f6\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Starrt ihr euch lange an,", "tokens": ["Starrt", "ihr", "euch", "lan\u00b7ge", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und Einer ruft: \u201eO schauet,", "tokens": ["Und", "Ei\u00b7ner", "ruft", ":", "\u201e", "O", "schau\u00b7et", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$.", "$(", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie man ein Ehrenmal", "tokens": ["Wie", "man", "ein", "Eh\u00b7ren\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Obscurem Burschen bauet!", "tokens": ["Ob\u00b7scu\u00b7rem", "Bur\u00b7schen", "bau\u00b7et", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wer war der General?\u201c", "tokens": ["Wer", "war", "der", "Ge\u00b7ne\u00b7ral", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}