{"textgrid.poem.34896": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie tanzt. Wie sie das Leibchen wiegt!", "tokens": ["Sie", "tanzt", ".", "Wie", "sie", "das", "Leib\u00b7chen", "wiegt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie jedes Glied sich zierlich biegt!", "tokens": ["Wie", "je\u00b7des", "Glied", "sich", "zier\u00b7lich", "biegt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das ist ein Flattern und ein Schwingen,", "tokens": ["Das", "ist", "ein", "Flat\u00b7tern", "und", "ein", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um wahrlich aus der Haut zu springen.", "tokens": ["Um", "wahr\u00b7lich", "aus", "der", "Haut", "zu", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie tanzt. Wenn sie sich wirbelnd dreht", "tokens": ["Sie", "tanzt", ".", "Wenn", "sie", "sich", "wir\u00b7belnd", "dreht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "KOUS", "PPER", "PRF", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf einem Fu\u00df, und stillesteht", "tokens": ["Auf", "ei\u00b7nem", "Fu\u00df", ",", "und", "stil\u00b7les\u00b7teht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am End' mit ausgestreckten Armen.", "tokens": ["Am", "End'", "mit", "aus\u00b7ge\u00b7streck\u00b7ten", "Ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mag Gott sich meiner Vernunft erbarmen!", "tokens": ["Mag", "Gott", "sich", "mei\u00b7ner", "Ver\u00b7nunft", "er\u00b7bar\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PRF", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie tanzt. Derselbe Tanz ist das,", "tokens": ["Sie", "tanzt", ".", "Der\u00b7sel\u00b7be", "Tanz", "ist", "das", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PDAT", "NN", "VAFIN", "PDS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den einst die Tochter Herodias'", "tokens": ["Den", "einst", "die", "Toch\u00b7ter", "He\u00b7ro\u00b7dias'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Getanzt vor dem Judenk\u00f6nig Herodes.", "tokens": ["Ge\u00b7tanzt", "vor", "dem", "Ju\u00b7den\u00b7k\u00f6\u00b7nig", "He\u00b7ro\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihr Auge spr\u00fcht wie Blitze des Todes.", "tokens": ["Ihr", "Au\u00b7ge", "spr\u00fcht", "wie", "Blit\u00b7ze", "des", "To\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KOKOM", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Sie tanzt mich rasend \u2013 ich werde toll \u2013", "tokens": ["Sie", "tanzt", "mich", "ra\u00b7send", "\u2013", "ich", "wer\u00b7de", "toll", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "$(", "PPER", "VAFIN", "ADJD", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sprich, Weib, was ich dir schenken soll?", "tokens": ["Sprich", ",", "Weib", ",", "was", "ich", "dir", "schen\u00b7ken", "soll", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "PWS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du l\u00e4chelst? Heda! Trabanten! L\u00e4ufer!", "tokens": ["Du", "l\u00e4\u00b7chelst", "?", "He\u00b7da", "!", "Tra\u00b7ban\u00b7ten", "!", "L\u00e4u\u00b7fer", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NE", "$.", "NN", "$.", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Man schlage ab das Haupt dem T\u00e4ufer!", "tokens": ["Man", "schla\u00b7ge", "ab", "das", "Haupt", "dem", "T\u00e4u\u00b7fer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}