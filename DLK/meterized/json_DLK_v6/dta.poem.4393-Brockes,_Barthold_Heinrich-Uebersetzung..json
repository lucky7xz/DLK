{"dta.poem.4393": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Uebersetzung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie klein ist doch das kleine Theil von unsrer Welt,", "tokens": ["Wie", "klein", "ist", "doch", "das", "klei\u00b7ne", "Theil", "von", "uns\u00b7rer", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "aus Fluht und Land,", "tokens": ["aus", "Fluht", "und", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wovon der Mensch der Lehns-Herr ist! Der Rest ist nichts,", "tokens": ["Wo\u00b7von", "der", "Mensch", "der", "Lehns\u00b7Herr", "ist", "!", "Der", "Rest", "ist", "nichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VAFIN", "$.", "ART", "NN", "VAFIN", "PIS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "als W\u00fcsteneyen,", "tokens": ["als", "W\u00fcs\u00b7te\u00b7ne\u00b7yen", ","], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Als Klippen, schroffe Gegenden, gefrorne Meere, brennend", "tokens": ["Als", "Klip\u00b7pen", ",", "schrof\u00b7fe", "Ge\u00b7gen\u00b7den", ",", "ge\u00b7fror\u00b7ne", "Mee\u00b7re", ",", "bren\u00b7nend"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KOUS", "NN", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Sand,", "tokens": ["Sand", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Von Wunder-Thieren, Gift und Stacheln, von Todt und", "tokens": ["Von", "Wun\u00b7der\u00b7Thie\u00b7ren", ",", "Gift", "und", "Sta\u00b7cheln", ",", "von", "Todt", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,", "APPR", "NN", "KON"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Mord ein Aufenthalt.", "tokens": ["Mord", "ein", "Auf\u00b7ent\u00b7halt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Die\u00df ist von unsrer Erden-Charte die melancholische Ge-", "tokens": ["Die\u00df", "ist", "von", "uns\u00b7rer", "Er\u00b7den\u00b7Char\u00b7te", "die", "me\u00b7lan\u00b7cho\u00b7li\u00b7sche", "Ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+--", "measure": "unknown.measure.septa"}, "line.10": {"text": "stalt,", "tokens": ["stalt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Noch finsterer die W\u00fcrklichkeit. Derselben Erden wilde", "tokens": ["Noch", "fins\u00b7te\u00b7rer", "die", "W\u00fcrk\u00b7lich\u00b7keit", ".", "Der\u00b7sel\u00b7ben", "Er\u00b7den", "wil\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "PDAT", "NN", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.12": {"text": "W\u00fcste", "tokens": ["W\u00fcs\u00b7te"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Ist von uns eine Chart\u2019, ein Abri\u00df. So nah benachbart", "tokens": ["Ist", "von", "uns", "ei\u00b7ne", "Chart'", ",", "ein", "Ab\u00b7ri\u00df", ".", "So", "nah", "be\u00b7nach\u00b7bart"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPER", "ART", "NN", "$,", "ART", "NN", "$.", "ADV", "ADJD", "VVPP"], "meter": "---+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "sind die L\u00fcste", "tokens": ["sind", "die", "L\u00fcs\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Des \u00fcberm\u00fcht\u2019gen Erden-Herrn mit dem so weiten Reich", "tokens": ["Des", "\u00fc\u00b7ber\u00b7m\u00fcht'\u00b7gen", "Er\u00b7den\u00b7Herrn", "mit", "dem", "so", "wei\u00b7ten", "Reich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.16": {"text": "der Pein,", "tokens": ["der", "Pein", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.17": {"text": "Wo tiefe Plagen uns ersch\u00fctternd, uns gleichsam drohen", "tokens": ["Wo", "tie\u00b7fe", "Pla\u00b7gen", "uns", "er\u00b7sch\u00fct\u00b7ternd", ",", "uns", "gleich\u00b7sam", "dro\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "PPER", "VVPP", "$,", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "zu zerreissen,", "tokens": ["zu", "zer\u00b7reis\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "In welchem laute Sorgen heulen, wo gift\u2019ge Leidenschaf-", "tokens": ["In", "wel\u00b7chem", "lau\u00b7te", "Sor\u00b7gen", "heu\u00b7len", ",", "wo", "gift'\u00b7ge", "Lei\u00b7den\u00b7schaf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN", "VVINF", "$,", "PWAV", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "ten beissen,", "tokens": ["ten", "beis\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["FM", "FM", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "Ein r\u00e4uberisches Heer von Elend best\u00e4ndig dreut uns um-", "tokens": ["Ein", "r\u00e4u\u00b7be\u00b7ri\u00b7sches", "Heer", "von", "E\u00b7lend", "be\u00b7st\u00e4n\u00b7dig", "dreut", "uns", "um"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "ADJD", "VVFIN", "PPER", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.22": {"text": "zubringen,", "tokens": ["zu\u00b7brin\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.23": {"text": "Und ein verr\u00e4htrisches Geschick sich \u00f6ffnet, um uns zu ver-", "tokens": ["Und", "ein", "ver\u00b7r\u00e4h\u00b7tri\u00b7sches", "Ge\u00b7schick", "sich", "\u00f6ff\u00b7net", ",", "um", "uns", "zu", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,", "KOUI", "PPER", "APPR", "TRUNC"], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.24": {"text": "schlingen.", "tokens": ["schlin\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}