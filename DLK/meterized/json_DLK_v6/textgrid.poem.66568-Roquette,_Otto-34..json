{"textgrid.poem.66568": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "34.", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwarum, ihr Poetenpack,", "tokens": ["\u00bb", "wa\u00b7rum", ",", "ihr", "Poe\u00b7ten\u00b7pack", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWAV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Qu\u00e4lt ihr euch noch mit Gedichten?", "tokens": ["Qu\u00e4lt", "ihr", "euch", "noch", "mit", "Ge\u00b7dich\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Praktisch f\u00fcllt man heut den Sack", "tokens": ["Prak\u00b7tisch", "f\u00fcllt", "man", "heut", "den", "Sack"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PIS", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich mit lohnendern Gewichten!\u00ab", "tokens": ["Sich", "mit", "loh\u00b7nen\u00b7dern", "Ge\u00b7wich\u00b7ten", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Warum bl\u00fcht der Fruchtbaum fort,", "tokens": ["Wa\u00b7rum", "bl\u00fcht", "der", "Frucht\u00b7baum", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohn' um Gunst und Lohn zu werben?", "tokens": ["Ohn'", "um", "Gunst", "und", "Lohn", "zu", "wer\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Warum, wenn sein Leben dorrt,", "tokens": ["Wa\u00b7rum", ",", "wenn", "sein", "Le\u00b7ben", "dorrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Will er noch in Bl\u00fcthen sterben?", "tokens": ["Will", "er", "noch", "in", "Bl\u00fc\u00b7then", "ster\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}