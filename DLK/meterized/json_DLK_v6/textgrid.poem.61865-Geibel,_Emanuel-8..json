{"textgrid.poem.61865": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ist auch dieser Bann gebrochen,", "tokens": ["Nun", "ist", "auch", "die\u00b7ser", "Bann", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und friedlich schlie\u00dft der Tag und klar \u2013", "tokens": ["Und", "fried\u00b7lich", "schlie\u00dft", "der", "Tag", "und", "klar", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir gr\u00fc\u00dften uns mit Herzenspochen,", "tokens": ["Wir", "gr\u00fc\u00df\u00b7ten", "uns", "mit", "Her\u00b7zen\u00b7spo\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch ward kein Wort von dem gesprochen,", "tokens": ["Doch", "ward", "kein", "Wort", "von", "dem", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was unsrer Jugend Traum einst war.", "tokens": ["Was", "uns\u00b7rer", "Ju\u00b7gend", "Traum", "einst", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vom Stern und Unstern meiner Reise,", "tokens": ["Vom", "Stern", "und", "Uns\u00b7tern", "mei\u00b7ner", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Land Homers erz\u00e4hlt' ich ihr;", "tokens": ["Vom", "Land", "Ho\u00b7mers", "er\u00b7z\u00e4hlt'", "ich", "ihr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "VVFIN", "PPER", "PPER", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie sprach vom alten Freundeskreise,", "tokens": ["Sie", "sprach", "vom", "al\u00b7ten", "Freun\u00b7des\u00b7krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch flo\u00df die Red' uns tr\u00e4g und leise,", "tokens": ["Doch", "flo\u00df", "die", "Red'", "uns", "tr\u00e4g", "und", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und endlich ganz verstummten wir.", "tokens": ["Und", "end\u00b7lich", "ganz", "ver\u00b7stumm\u00b7ten", "wir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da sprang sie auf, und rasch wie immer", "tokens": ["Da", "sprang", "sie", "auf", ",", "und", "rasch", "wie", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "ADJD", "KOKOM", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Gefa\u00dft, ergriff sie meine Hand,", "tokens": ["Ge\u00b7fa\u00dft", ",", "er\u00b7griff", "sie", "mei\u00b7ne", "Hand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und zog mich aus des Mittags Schimmer", "tokens": ["Und", "zog", "mich", "aus", "des", "Mit\u00b7tags", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ins hohe, halbverh\u00e4ngte Zimmer,", "tokens": ["Ins", "ho\u00b7he", ",", "halb\u00b7ver\u00b7h\u00e4ng\u00b7te", "Zim\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wo ihres Knaben Wiege stand.", "tokens": ["Wo", "ih\u00b7res", "Kna\u00b7ben", "Wie\u00b7ge", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie bog sich auf das Kind hernieder", "tokens": ["Sie", "bog", "sich", "auf", "das", "Kind", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und winkte l\u00e4chelnd mir zu nahn;", "tokens": ["Und", "wink\u00b7te", "l\u00e4\u00b7chelnd", "mir", "zu", "nahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "APPR", "ADJA", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verschlafen dehnt' es ros'ge Glieder,", "tokens": ["Ver\u00b7schla\u00b7fen", "dehnt'", "es", "ros'\u00b7ge", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und jetzt erhub's die Augenlider", "tokens": ["Und", "jetzt", "er\u00b7hub's", "die", "Au\u00b7gen\u00b7li\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "NE", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und sah mit ihrem Blick mich an.", "tokens": ["Und", "sah", "mit", "ih\u00b7rem", "Blick", "mich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da hab' ich's auf die hei\u00dfen Wangen", "tokens": ["Da", "hab'", "ich's", "auf", "die", "hei\u00b7\u00dfen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gek\u00fc\u00dft mit leisem Segenswort,", "tokens": ["Ge\u00b7k\u00fc\u00dft", "mit", "lei\u00b7sem", "Se\u00b7gens\u00b7wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und all mein Trauern und Verlangen", "tokens": ["Und", "all", "mein", "Trau\u00b7ern", "und", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "War wie ein Rauch im Wind zergangen,", "tokens": ["War", "wie", "ein", "Rauch", "im", "Wind", "zer\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und frei und heiter schritt ich fort.", "tokens": ["Und", "frei", "und", "hei\u00b7ter", "schritt", "ich", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}