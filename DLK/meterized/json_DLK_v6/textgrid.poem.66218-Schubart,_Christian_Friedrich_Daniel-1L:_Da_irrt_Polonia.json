{"textgrid.poem.66218": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da irrt Polonia", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da irrt Polonia", "tokens": ["Da", "irrt", "Po\u00b7lo\u00b7nia"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "NE"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Mit fliegendem Haare,", "tokens": ["Mit", "flie\u00b7gen\u00b7dem", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Mit jammerbleichem Gesichte,", "tokens": ["Mit", "jam\u00b7mer\u00b7blei\u00b7chem", "Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ringt \u00fcber dem Haupte", "tokens": ["Ringt", "\u00fc\u00b7ber", "dem", "Haup\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die H\u00e4nde. Gro\u00dfe Tropfen", "tokens": ["Die", "H\u00e4n\u00b7de", ".", "Gro\u00b7\u00dfe", "Trop\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hangen am Auge, das bricht", "tokens": ["Han\u00b7gen", "am", "Au\u00b7ge", ",", "das", "bricht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPRART", "NN", "$,", "PDS", "VVFIN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Und langsam starrt \u2013 und stirbt,", "tokens": ["Und", "lang\u00b7sam", "starrt", "\u2013", "und", "stirbt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$(", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Doch sie stirbt nicht!", "tokens": ["Doch", "sie", "stirbt", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Versagt ist ihr des Todes Trost.", "tokens": ["Ver\u00b7sagt", "ist", "ihr", "des", "To\u00b7des", "Trost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sie f\u00e4hrt auf, schwankt und sinkt", "tokens": ["Sie", "f\u00e4hrt", "auf", ",", "schwankt", "und", "sinkt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Nieder an der Felsenwand", "tokens": ["Nie\u00b7der", "an", "der", "Fel\u00b7sen\u00b7wand"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Und schreit: ach meine Kinder,", "tokens": ["Und", "schreit", ":", "ach", "mei\u00b7ne", "Kin\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "XY", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Wo seid ihr? Ausges\u00e4t", "tokens": ["Wo", "seid", "ihr", "?", "Aus\u00b7ge\u00b7s\u00e4t"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "In fremdes Volk und h\u00fclflos.", "tokens": ["In", "frem\u00b7des", "Volk", "und", "h\u00fcl\u00b7flos", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "O Sobieski, gro\u00dfer Sohn,", "tokens": ["O", "So\u00b7bies\u00b7ki", ",", "gro\u00b7\u00dfer", "Sohn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Wo bist du? schau herab!", "tokens": ["Wo", "bist", "du", "?", "schau", "her\u00b7ab", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "H\u00f6rst du nicht am Arme", "tokens": ["H\u00f6rst", "du", "nicht", "am", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.18": {"text": "Deines tapfern Volks die Fessel rasseln?", "tokens": ["Dei\u00b7nes", "tap\u00b7fern", "Volks", "die", "Fes\u00b7sel", "ras\u00b7seln", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Siehst du nicht den R\u00e4uber", "tokens": ["Siehst", "du", "nicht", "den", "R\u00e4u\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.20": {"text": "Aus W\u00e4ldern st\u00fcrzen", "tokens": ["Aus", "W\u00e4l\u00b7dern", "st\u00fcr\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.21": {"text": "Und dein Land verw\u00fcsten? \u2013", "tokens": ["Und", "dein", "Land", "ver\u00b7w\u00fcs\u00b7ten", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.22": {"text": "Ach der Greis versammelt seine Kinder,", "tokens": ["Ach", "der", "Greis", "ver\u00b7sam\u00b7melt", "sei\u00b7ne", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Seine Enkel um sich her", "tokens": ["Sei\u00b7ne", "En\u00b7kel", "um", "sich", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "PRF", "APZR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Und z\u00fcckt das Schwert und w\u00fcrgt sie nieder.", "tokens": ["Und", "z\u00fcckt", "das", "Schwert", "und", "w\u00fcrgt", "sie", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Sterbt! so spricht er w\u00fcthend,", "tokens": ["Sterbt", "!", "so", "spricht", "er", "w\u00fct\u00b7hend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.26": {"text": "Was ist ein Leben ohne Freiheit?", "tokens": ["Was", "ist", "ein", "Le\u00b7ben", "oh\u00b7ne", "Frei\u00b7heit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Ha, er rollt die offnen Augen,", "tokens": ["Ha", ",", "er", "rollt", "die", "off\u00b7nen", "Au\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Durchsto\u00dft die Brust und sinkt", "tokens": ["Durch\u00b7sto\u00dft", "die", "Brust", "und", "sinkt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Auf seiner Kinder Leichen nieder. \u2013", "tokens": ["Auf", "sei\u00b7ner", "Kin\u00b7der", "Lei\u00b7chen", "nie\u00b7der", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "So klagt Polonia.", "tokens": ["So", "klagt", "Po\u00b7lo\u00b7nia", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}