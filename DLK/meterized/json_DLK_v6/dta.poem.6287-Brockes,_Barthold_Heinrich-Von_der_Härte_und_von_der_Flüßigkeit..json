{"dta.poem.6287": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der H\u00e4rte und von der  \n Fl\u00fc\u00dfigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie nun in C\u00f6rpern immerzu", "tokens": ["Wie", "nun", "in", "C\u00f6r\u00b7pern", "im\u00b7mer\u00b7zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "NE", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bewegung wechselt mit der Ruh:", "tokens": ["Be\u00b7we\u00b7gung", "wech\u00b7selt", "mit", "der", "Ruh", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So weicht die H\u00e4rt\u2019 auch offt der Fl\u00fc\u00dfigkeit.", "tokens": ["So", "weicht", "die", "H\u00e4rt'", "auch", "offt", "der", "Fl\u00fc\u00b7\u00dfig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Fl\u00fc\u00dfige wird fest, auch die Beschaffenheit", "tokens": ["Das", "Fl\u00fc\u00b7\u00dfi\u00b7ge", "wird", "fest", ",", "auch", "die", "Be\u00b7schaf\u00b7fen\u00b7heit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des dichtesten Metalls weicht selber schnellen Flammen,", "tokens": ["Des", "dich\u00b7tes\u00b7ten", "Me\u00b7talls", "weicht", "sel\u00b7ber", "schnel\u00b7len", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}}}}}