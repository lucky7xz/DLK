{"textgrid.poem.39290": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "Die Rose im Meer", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es schwamm im Meer, im rauschenden Meer,", "tokens": ["Es", "schwamm", "im", "Meer", ",", "im", "rau\u00b7schen\u00b7den", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Eine sturmgebrochne Rose her,", "tokens": ["Ei\u00b7ne", "sturm\u00b7ge\u00b7broch\u00b7ne", "Ro\u00b7se", "her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Eine Rose, voll und licht;", "tokens": ["Ei\u00b7ne", "Ro\u00b7se", ",", "voll", "und", "licht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie schwamm auf schaukelnder Wogenbahn", "tokens": ["Sie", "schwamm", "auf", "schau\u00b7keln\u00b7der", "Wo\u00b7gen\u00b7bahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Hinab, hinan,", "tokens": ["Hin\u00b7ab", ",", "hi\u00b7nan", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Rings um sie rauschte der Ozean,", "tokens": ["Rings", "um", "sie", "rauschte", "der", "O\u00b7ze\u00b7an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und er verschlang sie nicht.", "tokens": ["Und", "er", "ver\u00b7schlang", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie ein rosig Weib, das traumbesiegt", "tokens": ["Wie", "ein", "ro\u00b7sig", "Weib", ",", "das", "traum\u00b7be\u00b7siegt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "ADJD", "NN", "$,", "PDS", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Auf gr\u00fcner, schwellender Matte liegt,", "tokens": ["Auf", "gr\u00fc\u00b7ner", ",", "schwel\u00b7len\u00b7der", "Mat\u00b7te", "liegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So lag sie auf gr\u00fcner Flut;", "tokens": ["So", "lag", "sie", "auf", "gr\u00fc\u00b7ner", "Flut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der bl\u00fchende Schein, der Farbenduft", "tokens": ["Der", "bl\u00fc\u00b7hen\u00b7de", "Schein", ",", "der", "Far\u00b7ben\u00b7duft"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "In Meer und Luft", "tokens": ["In", "Meer", "und", "Luft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Durchglomm die smaragdene Wassergruft", "tokens": ["Durch\u00b7glomm", "die", "sma\u00b7rag\u00b7de\u00b7ne", "Was\u00b7ser\u00b7gruft"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Mit reiner Rosenglut.", "tokens": ["Mit", "rei\u00b7ner", "Ro\u00b7seng\u00b7lut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Wellen k\u00fc\u00dften sich gar nicht satt.", "tokens": ["Die", "Wel\u00b7len", "k\u00fc\u00df\u00b7ten", "sich", "gar", "nicht", "satt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf perlenstrahlender Lagerstatt", "tokens": ["Auf", "per\u00b7len\u00b7strah\u00b7len\u00b7der", "La\u00b7ger\u00b7statt"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Erwachte die Fei der See:", "tokens": ["Er\u00b7wach\u00b7te", "die", "Fei", "der", "See", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Was leuchtet \u00fcber dem feuchten Schwall,", "tokens": ["Was", "leuch\u00b7tet", "\u00fc\u00b7ber", "dem", "feuch\u00b7ten", "Schwall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "All\u00fcberall?", "tokens": ["Al\u00b7l\u00fc\u00b7be\u00b7rall", "?"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Es flammt wie der gl\u00fchende Sonnenball", "tokens": ["Es", "flammt", "wie", "der", "gl\u00fc\u00b7hen\u00b7de", "Son\u00b7nen\u00b7ball"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Und tut dem Auge nicht weh!", "tokens": ["Und", "tut", "dem", "Au\u00b7ge", "nicht", "weh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Die Muscheln schminkten sich rosenrot,", "tokens": ["Die", "Mu\u00b7scheln", "schmink\u00b7ten", "sich", "ro\u00b7sen\u00b7rot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Korallen sch\u00e4mten sich fast zu Tod,", "tokens": ["Die", "Ko\u00b7ral\u00b7len", "sch\u00e4m\u00b7ten", "sich", "fast", "zu", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Verwundert schaute das Meer:", "tokens": ["Ver\u00b7wun\u00b7dert", "schau\u00b7te", "das", "Meer", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Wo kamest Du her, wer magst Du sein,", "tokens": ["Wo", "ka\u00b7mest", "Du", "her", ",", "wer", "magst", "Du", "sein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$,", "PWS", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Du sch\u00f6ner Schein?", "tokens": ["Du", "sch\u00f6\u00b7ner", "Schein", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Fielst Du vom Felsen ins Meer hinein,", "tokens": ["Fielst", "Du", "vom", "Fel\u00b7sen", "ins", "Meer", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Fielst Du vom Himmel her?", "tokens": ["Fielst", "Du", "vom", "Him\u00b7mel", "her", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Welt erk\u00e4ltenden Wellentau", "tokens": ["Der", "Welt", "er\u00b7k\u00e4l\u00b7ten\u00b7den", "Wel\u00b7len\u00b7tau"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Durchschwimmst Du allein, Du sch\u00f6ne Frau,", "tokens": ["Durch\u00b7schwimmst", "Du", "al\u00b7lein", ",", "Du", "sch\u00f6\u00b7ne", "Frau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und machst ihn farbig ergl\u00fchn.", "tokens": ["Und", "machst", "ihn", "far\u00b7big", "er\u00b7gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Wir wissen es nicht, woher Du schwammst,", "tokens": ["Wir", "wis\u00b7sen", "es", "nicht", ",", "wo\u00b7her", "Du", "schwammst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Woher Du flammst,", "tokens": ["Wo\u00b7her", "Du", "flammst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ob Du von der Erde, vom Himmel stammst,", "tokens": ["Ob", "Du", "von", "der", "Er\u00b7de", ",", "vom", "Him\u00b7mel", "stammst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Genug, wir sehen Dich bl\u00fchn!", "tokens": ["Ge\u00b7nug", ",", "wir", "se\u00b7hen", "Dich", "bl\u00fchn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}