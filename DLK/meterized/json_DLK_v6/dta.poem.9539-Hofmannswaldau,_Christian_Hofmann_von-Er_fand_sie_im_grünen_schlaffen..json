{"dta.poem.9539": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Er fand sie im gr\u00fcnen schlaffen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer liegt mein paradie\u00df mit rosen \u00fcberdeckt/", "tokens": ["Hjer", "liegt", "mein", "pa\u00b7ra\u00b7die\u00df", "mit", "ro\u00b7sen", "\u00fc\u00b7berd\u00b7eckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "APPR", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die br\u00fcste regen sich/ mich mehr und mehr zu qu\u00e4len/", "tokens": ["Die", "br\u00fcs\u00b7te", "re\u00b7gen", "sich", "/", "mich", "mehr", "und", "mehr", "zu", "qu\u00e4\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "PRF", "$(", "PPER", "ADV", "KON", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der ambra steigt hervor aus ihrer s\u00fcssen k\u00e4len/", "tokens": ["Der", "am\u00b7bra", "steigt", "her\u00b7vor", "aus", "ih\u00b7rer", "s\u00fcs\u00b7sen", "k\u00e4\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hir ligt mein paradies im gr\u00fcnen ausgestreckt.", "tokens": ["Hir", "ligt", "mein", "pa\u00b7ra\u00b7dies", "im", "gr\u00fc\u00b7nen", "aus\u00b7ge\u00b7streckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Komm/ geu\u00df auf ihren mund dein perlenes confect/", "tokens": ["Komm", "/", "geu\u00df", "auf", "ih\u00b7ren", "mund", "dein", "per\u00b7le\u00b7nes", "con\u00b7fect", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVIMP", "APPR", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du linder Zephir du/ bring ihr die sanften seelen", "tokens": ["Du", "lin\u00b7der", "Ze\u00b7phir", "du", "/", "bring", "ihr", "die", "sanf\u00b7ten", "see\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "PPER", "$(", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Aus deinen brunnen her/ mit ihr mich zu verm\u00e4hlen.", "tokens": ["Aus", "dei\u00b7nen", "brun\u00b7nen", "her", "/", "mit", "ihr", "mich", "zu", "ver\u00b7m\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$(", "APPR", "PPER", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Schlaf; aber da\u00df sie nicht dadurch werd\u2019 aufgeweckt/", "tokens": ["Schlaf", ";", "a\u00b7ber", "da\u00df", "sie", "nicht", "da\u00b7durch", "werd'", "auf\u00b7ge\u00b7weckt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "KOUS", "PPER", "PTKNEG", "PAV", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "St. Dryas! St. Napee! bleibt dort in dem gep\u00fcsche/", "tokens": ["St.", "Dryas", "!", "St.", "Na\u00b7pee", "!", "bleibt", "dort", "in", "dem", "ge\u00b7p\u00fcsc\u00b7he", "/"], "token_info": ["abbreviation", "word", "punct", "abbreviation", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "NE", "NE", "$.", "VVFIN", "ADV", "APPR", "ART", "ADJA", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Dieweil ich manchen ku\u00df auf ihrem mund erwische/", "tokens": ["Die\u00b7weil", "ich", "man\u00b7chen", "ku\u00df", "auf", "ih\u00b7rem", "mund", "er\u00b7wi\u00b7sche", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sol euer sch\u00f6ner chor nicht mit ihr spielen gehn?", "tokens": ["Sol", "eu\u00b7er", "sch\u00f6\u00b7ner", "chor", "nicht", "mit", "ihr", "spie\u00b7len", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "PTKNEG", "APPR", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Indessen schlafe du hier unter diesen b\u00e4umen;", "tokens": ["In\u00b7des\u00b7sen", "schla\u00b7fe", "du", "hier", "un\u00b7ter", "die\u00b7sen", "b\u00e4u\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sehnst du denn aber dich nach sanften liebes-tr\u00e4umen/", "tokens": ["Sehnst", "du", "denn", "a\u00b7ber", "dich", "nach", "sanf\u00b7ten", "lie\u00b7bes\u00b7tr\u00e4u\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So wache pl\u00f6tzlich auf/ hier kanst du einen sehn.", "tokens": ["So", "wa\u00b7che", "pl\u00f6tz\u00b7lich", "auf", "/", "hier", "kanst", "du", "ei\u00b7nen", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ADJD", "PTKVZ", "$(", "ADV", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}