{"textgrid.poem.67422": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An die Gr\u00e4fin Br\u00fchl", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Menschen Leben beschr\u00e4nkt ein enger Raum,", "tokens": ["Des", "Men\u00b7schen", "Le\u00b7ben", "be\u00b7schr\u00e4nkt", "ein", "en\u00b7ger", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein engerer beschr\u00e4nket seinen Sinn,", "tokens": ["Ein", "en\u00b7ge\u00b7rer", "be\u00b7schr\u00e4n\u00b7ket", "sei\u00b7nen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sein Herz den engsten. Um sich her zu sehn,", "tokens": ["Sein", "Herz", "den", "engs\u00b7ten", ".", "Um", "sich", "her", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "$.", "KOUI", "PRF", "APZR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zu ordnen, was man kann, unschuldig zu", "tokens": ["Zu", "ord\u00b7nen", ",", "was", "man", "kann", ",", "un\u00b7schul\u00b7dig", "zu"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "PIS", "VMFIN", "$,", "ADJD", "PTKZU"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Genie\u00dfen, was uns die Vorsicht g\u00f6nnt,", "tokens": ["Ge\u00b7nie\u00b7\u00dfen", ",", "was", "uns", "die", "Vor\u00b7sicht", "g\u00f6nnt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und dankbar froh hinwegzugehn:", "tokens": ["Und", "dank\u00b7bar", "froh", "hin\u00b7weg\u00b7zu\u00b7gehn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das ist des Menschenlebens Geschichte, ist", "tokens": ["Das", "ist", "des", "Men\u00b7schen\u00b7le\u00b7bens", "Ge\u00b7schich\u00b7te", ",", "ist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$,", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Nicht Idee, es ist Gef\u00fchl.", "tokens": ["Nicht", "I\u00b7dee", ",", "es", "ist", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PPER", "VAFIN", "NN", "$."], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}}}}}