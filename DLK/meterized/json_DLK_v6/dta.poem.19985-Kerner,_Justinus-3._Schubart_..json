{"dta.poem.19985": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "3.  \n  Schubart .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihn stie\u00dfen sie aus frischen Lebensg\u00e4rten", "tokens": ["Ihn", "stie\u00b7\u00dfen", "sie", "aus", "fri\u00b7schen", "Le\u00b7bens\u00b7g\u00e4r\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In dunkle, modernde Gew\u00f6lbe nieder,", "tokens": ["In", "dunk\u00b7le", ",", "mo\u00b7dern\u00b7de", "Ge\u00b7w\u00f6l\u00b7be", "nie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit Ketten seine H\u00e4nde sie beschwerten:", "tokens": ["Mit", "Ket\u00b7ten", "sei\u00b7ne", "H\u00e4n\u00b7de", "sie", "be\u00b7schwer\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da stiegen Heil'ge liebend zu ihm nieder,", "tokens": ["Da", "stie\u00b7gen", "Heil'\u00b7ge", "lie\u00b7bend", "zu", "ihm", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wurden fortan Freund' ihm und Gef\u00e4hrten;", "tokens": ["Und", "wur\u00b7den", "for\u00b7tan", "Freund'", "ihm", "und", "Ge\u00b7f\u00e4hr\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "NN", "PPER", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So sang begeistert er die frommen Lieder.", "tokens": ["So", "sang", "be\u00b7geis\u00b7tert", "er", "die", "from\u00b7men", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und als den Kerker sie ihm aufgeschlossen,", "tokens": ["Und", "als", "den", "Ker\u00b7ker", "sie", "ihm", "auf\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Schien ihm die Welt von Graun und Nacht umflossen.", "tokens": ["Schien", "ihm", "die", "Welt", "von", "Graun", "und", "Nacht", "um\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "NE", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}