{"textgrid.poem.42649": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: O du, der du an mancher Tafelrunde", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du, der du an mancher Tafelrunde", "tokens": ["O", "du", ",", "der", "du", "an", "man\u00b7cher", "Ta\u00b7fel\u00b7run\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PRELS", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir Wonne gabst, o k\u00f6niglicher Wein!", "tokens": ["Mir", "Won\u00b7ne", "gabst", ",", "o", "k\u00f6\u00b7nig\u00b7li\u00b7cher", "Wein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Beseele mich, und lass mit frohem Munde", "tokens": ["Be\u00b7see\u00b7le", "mich", ",", "und", "lass", "mit", "fro\u00b7hem", "Mun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mich deines Lobs entz\u00fcckten Herold seyn!", "tokens": ["Mich", "dei\u00b7nes", "Lobs", "ent\u00b7z\u00fcck\u00b7ten", "He\u00b7rold", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du offenbarst des Heuchlers schlauste L\u00fcgen,", "tokens": ["Du", "of\u00b7fen\u00b7barst", "des", "Heuch\u00b7lers", "schlaus\u00b7te", "L\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Machst, G\u00f6ttersaft! den Freund uns doppelt werth,", "tokens": ["Machst", ",", "G\u00f6t\u00b7ter\u00b7saft", "!", "den", "Freund", "uns", "dop\u00b7pelt", "werth", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$.", "ART", "NN", "PPER", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und f\u00fcllst das Herz mit traulichem Vergn\u00fcgen,", "tokens": ["Und", "f\u00fcllst", "das", "Herz", "mit", "trau\u00b7li\u00b7chem", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Liebe selbst nicht halb so dauernd n\u00e4hrt.", "tokens": ["Das", "Lie\u00b7be", "selbst", "nicht", "halb", "so", "dau\u00b7ernd", "n\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKNEG", "ADJD", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Liebe Glut erkaltet mit den Jahren:", "tokens": ["Der", "Lie\u00b7be", "Glut", "er\u00b7kal\u00b7tet", "mit", "den", "Jah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr s\u00fcsser Rausch fliegt nur zu bald dahin,", "tokens": ["Ihr", "s\u00fcs\u00b7ser", "Rausch", "fliegt", "nur", "zu", "bald", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "APPR", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Indess, o Wein, noch M\u00e4nner, grau an Haaren,", "tokens": ["In\u00b7dess", ",", "o", "Wein", ",", "noch", "M\u00e4n\u00b7ner", ",", "grau", "an", "Haa\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NN", "$,", "ADV", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Trotz Schlag und Gicht, von deinem Feuer gl\u00fchn.", "tokens": ["Trotz", "Schlag", "und", "Gicht", ",", "von", "dei\u00b7nem", "Feu\u00b7er", "gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du st\u00e4rkst den Geist, giebst Nahrung und Gedeihen,", "tokens": ["Du", "st\u00e4rkst", "den", "Geist", ",", "giebst", "Nah\u00b7rung", "und", "Ge\u00b7dei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und str\u00f6mest Kraft in alle Glieder mir,", "tokens": ["Und", "str\u00f6\u00b7mest", "Kraft", "in", "al\u00b7le", "Glie\u00b7der", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "PIAT", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du tr\u00f6stest mich, wenn Sorg' und Gram mir dr\u00e4uen,", "tokens": ["Du", "tr\u00f6s\u00b7test", "mich", ",", "wenn", "Sor\u00b7g'", "und", "Gram", "mir", "dr\u00e4u\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "NN", "KON", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und meinen Muth, wem dank' ich ihn, als dir?", "tokens": ["Und", "mei\u00b7nen", "Muth", ",", "wem", "dank'", "ich", "ihn", ",", "als", "dir", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PWS", "VVFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Drum sey mein Freund! Von deiner Glut begeistert,", "tokens": ["Drum", "sey", "mein", "Freund", "!", "Von", "dei\u00b7ner", "Glut", "be\u00b7geis\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPOSAT", "NN", "$.", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wandr' ich beherzt durch's Labyrinth der Welt,", "tokens": ["Wandr'", "ich", "be\u00b7herzt", "durch's", "La\u00b7by\u00b7rinth", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bis einst der Tod, der alles \u00fcbermeistert,", "tokens": ["Bis", "einst", "der", "Tod", ",", "der", "al\u00b7les", "\u00fc\u00b7ber\u00b7meis\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "$,", "PRELS", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auch mich dem Schwall der Schatten zugesellt.", "tokens": ["Auch", "mich", "dem", "Schwall", "der", "Schat\u00b7ten", "zu\u00b7ge\u00b7sellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}