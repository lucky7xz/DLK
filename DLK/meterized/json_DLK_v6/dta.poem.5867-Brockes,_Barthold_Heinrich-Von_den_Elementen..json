{"dta.poem.5867": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Doch da ein Element von einem jeden Dinge", "tokens": ["Doch", "da", "ein", "E\u00b7le\u00b7ment", "von", "ei\u00b7nem", "je\u00b7den", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ist, durch welches es zuerst das Seyn empfinge;", "tokens": ["Das", "ist", ",", "durch", "wel\u00b7ches", "es", "zu\u00b7erst", "das", "Seyn", "emp\u00b7fin\u00b7ge", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "APPR", "PRELS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mu\u00df man es unter duncklen Bildern", "tokens": ["Mu\u00df", "man", "es", "un\u00b7ter", "dunck\u00b7len", "Bil\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fantastischer Jdeen ja nicht schildern.", "tokens": ["Fan\u00b7tas\u00b7ti\u00b7scher", "Jdeen", "ja", "nicht", "schil\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}