{"textgrid.poem.40762": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sehnsuchtkrank nach dem geliebten Jungen,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sehnsuchtkrank nach dem geliebten Jungen,", "tokens": ["Sehn\u00b7sucht\u00b7krank", "nach", "dem", "ge\u00b7lieb\u00b7ten", "Jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dessen Blick ihr tief ins Herz gedrungen,", "tokens": ["Des\u00b7sen", "Blick", "ihr", "tief", "ins", "Herz", "ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPOSAT", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sprach das M\u00e4gdlein beichtend zu dem Pater:", "tokens": ["Sprach", "das", "M\u00e4gd\u00b7lein", "beich\u00b7tend", "zu", "dem", "Pa\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "\u00bbfrommer M\u00f6nch, des Seelenheils Berather,", "tokens": ["\u00bb", "from\u00b7mer", "M\u00f6nch", ",", "des", "See\u00b7len\u00b7heils", "Be\u00b7ra\u00b7ther", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wi\u00dft, so streng das Haus mein Vormund h\u00fctet,", "tokens": ["Wi\u00dft", ",", "so", "streng", "das", "Haus", "mein", "Vor\u00b7mund", "h\u00fc\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Gegen jedes M\u00e4nnleins Einla\u00df w\u00fcthet,", "tokens": ["Ge\u00b7gen", "je\u00b7des", "M\u00e4nn\u00b7leins", "Ein\u00b7la\u00df", "w\u00fct\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Wu\u00dfte doch mein Liebster einzudringen,", "tokens": ["Wu\u00df\u00b7te", "doch", "mein", "Liebs\u00b7ter", "ein\u00b7zu\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Im Gewand der Magd mu\u00dft' ihm's gelingen.", "tokens": ["Im", "Ge\u00b7wand", "der", "Magd", "mu\u00dft'", "ihm's", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Sagt ihm nun, da\u00df er nicht wiederkehre,", "tokens": ["Sagt", "ihm", "nun", ",", "da\u00df", "er", "nicht", "wie\u00b7der\u00b7keh\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Da\u00df ich b\u00fc\u00dfend ihm den Einla\u00df wehre;", "tokens": ["Da\u00df", "ich", "b\u00fc\u00b7\u00dfend", "ihm", "den", "Ein\u00b7la\u00df", "weh\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Bringt die\u00df Ringlein, das er mir gegeben,", "tokens": ["Bringt", "die\u00df", "Rin\u00b7glein", ",", "das", "er", "mir", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "NN", "$,", "PRELS", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Ihm zur\u00fcck als Abschiedspfand f\u00fcrs Leben.\u00ab", "tokens": ["Ihm", "zu\u00b7r\u00fcck", "als", "Ab\u00b7schieds\u00b7pfand", "f\u00fcrs", "Le\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PTKVZ", "KOKOM", "NN", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Ei, wie schlau sprach die so scheinbar Spr\u00f6de,", "tokens": ["Ei", ",", "wie", "schlau", "sprach", "die", "so", "schein\u00b7bar", "Spr\u00f6\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADJD", "VVFIN", "ART", "ADV", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Ei, wie war der M\u00f6nch so blind, so bl\u00f6de,", "tokens": ["Ei", ",", "wie", "war", "der", "M\u00f6nch", "so", "blind", ",", "so", "bl\u00f6\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,", "ADV", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Denn das Ringlein sagt ihm's selbst am Ende,", "tokens": ["Denn", "das", "Rin\u00b7glein", "sagt", "ihm's", "selbst", "am", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Da\u00df es nicht geformt f\u00fcr Frauenh\u00e4nde.", "tokens": ["Da\u00df", "es", "nicht", "ge\u00b7formt", "f\u00fcr", "Frau\u00b7en\u00b7h\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVPP", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Klar doch ward der Botschaft Sinn dem Jungen,", "tokens": ["Klar", "doch", "ward", "der", "Bot\u00b7schaft", "Sinn", "dem", "Jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "ART", "NN", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dessen Herz ihr s\u00fc\u00dfer Blick bezwungen;", "tokens": ["Des\u00b7sen", "Herz", "ihr", "s\u00fc\u00b7\u00dfer", "Blick", "be\u00b7zwun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Dem's noch nie gelang zu ihr zu kommen,", "tokens": ["Dem's", "noch", "nie", "ge\u00b7lang", "zu", "ihr", "zu", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "VVFIN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Jetzt wohl wei\u00df er's: Magdgewand wird frommen!", "tokens": ["Jetzt", "wohl", "wei\u00df", "er's", ":", "Magd\u00b7ge\u00b7wand", "wird", "from\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "$.", "NN", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "H\u00e4ndek\u00fcssend spricht er zu dem Pater:", "tokens": ["H\u00e4n\u00b7de\u00b7k\u00fcs\u00b7send", "spricht", "er", "zu", "dem", "Pa\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "\u00bbfrommer M\u00f6nch, ihr, unsres Heils Berather,", "tokens": ["\u00bb", "from\u00b7mer", "M\u00f6nch", ",", "ihr", ",", "uns\u00b7res", "Heils", "Be\u00b7ra\u00b7ther", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "PPER", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Sagt der Maid, wie tief mich's schmerzt zu weichen,", "tokens": ["Sagt", "der", "Maid", ",", "wie", "tief", "mich's", "schmerzt", "zu", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PWAV", "ADJD", "PIS", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Ihr Gebot doch ehr' ich; de\u00df als Zeichen", "tokens": ["Ihr", "Ge\u00b7bot", "doch", "ehr'", "ich", ";", "de\u00df", "als", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "PPER", "$.", "ART", "KOUS", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Bringt zur\u00fcck die\u00df Armband ihr von Golde,", "tokens": ["Bringt", "zu\u00b7r\u00fcck", "die\u00df", "Arm\u00b7band", "ihr", "von", "Gol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "PDS", "NN", "PPER", "APPR", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Das mir einst als Huldpfand bot die Holde.\u00ab \u2013", "tokens": ["Das", "mir", "einst", "als", "Huld\u00b7pfand", "bot", "die", "Hol\u00b7de", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PDS", "PPER", "ADV", "KOUS", "NN", "VVFIN", "ART", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Ei, wie ist der Knabe schlau nicht minder,", "tokens": ["Ei", ",", "wie", "ist", "der", "Kna\u00b7be", "schlau", "nicht", "min\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VAFIN", "ART", "NN", "ADJD", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Doch wie blieb der M\u00f6nch ein Bl\u00f6d' und Blinder,", "tokens": ["Doch", "wie", "blieb", "der", "M\u00f6nch", "ein", "Bl\u00f6d'", "und", "Blin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "ART", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Denn sonst m\u00fc\u00dft' ihm's selbst die\u00df Armband sagen,", "tokens": ["Denn", "sonst", "m\u00fc\u00dft'", "ihm's", "selbst", "die\u00df", "Arm\u00b7band", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADV", "PDS", "NN", "VVINF", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.14": {"text": "Da\u00df nicht M\u00e4nner solchen Goldreif tragen!", "tokens": ["Da\u00df", "nicht", "M\u00e4n\u00b7ner", "sol\u00b7chen", "Gold\u00b7reif", "tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Abends als die Sternlein aufgegangen,", "tokens": ["A\u00b7bends", "als", "die", "Stern\u00b7lein", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Halten Knab' und Maid sich liebumfangen,", "tokens": ["Hal\u00b7ten", "Knab'", "und", "Maid", "sich", "lieb\u00b7um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Drau\u00dfen bl\u00fchn und gl\u00fchn verschwiegne Rosen,", "tokens": ["Drau\u00b7\u00dfen", "bl\u00fchn", "und", "gl\u00fchn", "ver\u00b7schwieg\u00b7ne", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "KON", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Innen bl\u00fcht's und gl\u00fcht's von Ku\u00df und Kosen,", "tokens": ["In\u00b7nen", "bl\u00fcht's", "und", "gl\u00fcht's", "von", "Ku\u00df", "und", "Ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "NE", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Lachend segnen sie die Liebesnoten", "tokens": ["La\u00b7chend", "seg\u00b7nen", "sie", "die", "Lie\u00b7bes\u00b7no\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ihres Witzes und den blinden Boten;", "tokens": ["Ih\u00b7res", "Wit\u00b7zes", "und", "den", "blin\u00b7den", "Bo\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Doch die T\u00e4ublein ahnen nicht im Neste,", "tokens": ["Doch", "die", "T\u00e4ub\u00b7lein", "ah\u00b7nen", "nicht", "im", "Nes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "PTKNEG", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wer der Schlauste Aller und der Beste.", "tokens": ["Wer", "der", "Schlaus\u00b7te", "Al\u00b7ler", "und", "der", "Bes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Einsam an dem Fenster seiner Zelle", "tokens": ["Ein\u00b7sam", "an", "dem", "Fens\u00b7ter", "sei\u00b7ner", "Zel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Lehnt der M\u00f6nch und blickt zur Sternenhelle,", "tokens": ["Lehnt", "der", "M\u00f6nch", "und", "blickt", "zur", "Ster\u00b7nen\u00b7hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Saugt den W\u00fcrzehauch der Blumenglocken,", "tokens": ["Saugt", "den", "W\u00fcr\u00b7ze\u00b7hauch", "der", "Blu\u00b7men\u00b7glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "H\u00f6rt des Sprossers Locken und Frohlocken,", "tokens": ["H\u00f6rt", "des", "Spros\u00b7sers", "Lo\u00b7cken", "und", "Froh\u00b7lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+++-", "measure": "unknown.measure.hexa"}, "line.5": {"text": "Und er denkt der Maid und denkt des Knaben:", "tokens": ["Und", "er", "denkt", "der", "Maid", "und", "denkt", "des", "Kna\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "\u00bbwas mir selbst versagt, mag's Andre laben!\u00ab", "tokens": ["\u00bb", "was", "mir", "selbst", "ver\u00b7sagt", ",", "mag's", "And\u00b7re", "la\u00b7ben", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "PPER", "ADV", "VVPP", "$,", "VMFIN", "PIS", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Gleichwie Rosenschein bei Sternenlichte", "tokens": ["Gleich\u00b7wie", "Ro\u00b7sen\u00b7schein", "bei", "Ster\u00b7nen\u00b7lich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Spielt ihm L\u00e4cheln auf dem Angesichte,", "tokens": ["Spielt", "ihm", "L\u00e4\u00b7cheln", "auf", "dem", "An\u00b7ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "\u00bbbleibt nur in dem Wahn, ihr guten Kinder,", "tokens": ["\u00bb", "bleibt", "nur", "in", "dem", "Wahn", ",", "ihr", "gu\u00b7ten", "Kin\u00b7der", ","