{"dta.poem.13088": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Schilder-Kunst fieng an in Unlust zu gerathen,", "tokens": ["Die", "Schil\u00b7der\u00b7Kunst", "fi\u00b7eng", "an", "in", "Un\u00b7lust", "zu", "ge\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und sprach: \u201eDie Mahlerey besteht in Licht und Schatten.", "tokens": ["Und", "sprach", ":", "\u201e", "Die", "Mah\u00b7le\u00b7rey", "be\u00b7steht", "in", "Licht", "und", "Schat\u00b7ten", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ewas ist das ganze Rund der irrdischen Natur?", "tokens": ["\u201e", "was", "ist", "das", "gan\u00b7ze", "Rund", "der", "irr\u00b7di\u00b7schen", "Na\u00b7tur", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "210\u201dL\u00f6scht Licht und Schatten aus! wo findt man eine Spuhr?", "tokens": ["\"", "L\u00f6scht", "Licht", "und", "Schat\u00b7ten", "aus", "!", "wo", "findt", "man", "ei\u00b7ne", "Spuhr", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NN", "KON", "NN", "PTKVZ", "$.", "PWAV", "VVFIN", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201ewas sehen wir davon? auch selber eure Wercke", "tokens": ["\u201e", "was", "se\u00b7hen", "wir", "da\u00b7von", "?", "auch", "sel\u00b7ber", "eu\u00b7re", "Wer\u00b7cke"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PWS", "VVFIN", "PPER", "PAV", "$.", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201everliehren ohne Licht und Schatten ihre St\u00e4rcke.", "tokens": ["\u201e", "ver\u00b7lieh\u00b7ren", "oh\u00b7ne", "Licht", "und", "Schat\u00b7ten", "ih\u00b7re", "St\u00e4r\u00b7cke", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "NN", "KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eso geht der Pensel vor. Die Schnizerinn fiel ein:", "tokens": ["\u201e", "so", "geht", "der", "Pen\u00b7sel", "vor", ".", "Die", "Sch\u00b7ni\u00b7ze\u00b7rinn", "fiel", "ein", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}