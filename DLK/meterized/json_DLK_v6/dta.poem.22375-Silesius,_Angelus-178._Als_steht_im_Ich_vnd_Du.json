{"dta.poem.22375": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "178. Als steht im Ich vnd Du", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "(sch\u00f6pffer vnd Gesch\u00f6pffe.)", "tokens": ["(", "sch\u00f6pf\u00b7fer", "vnd", "Ge\u00b7sch\u00f6pf\u00b7fe", ".", ")"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "KON", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Nichts ist als ", "tokens": ["Nichts", "ist", "als"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VAFIN", "KOKOM"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "nicht sein/", "tokens": ["nicht", "sein", "/"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VAINF", "$("], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "So ist GOtt nicht mehr GOtt/ un\u0303 f\u00e4llt der Him\u0303el ein.", "tokens": ["So", "ist", "Gott", "nicht", "mehr", "Gott", "/", "u\u00f1", "f\u00e4llt", "der", "Him\u0303el", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "PTKNEG", "PIAT", "NN", "$(", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Besihe den Begihrer am Ende.", "tokens": ["Be\u00b7si\u00b7he", "den", "Be\u00b7gih\u00b7rer", "am", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}}}}