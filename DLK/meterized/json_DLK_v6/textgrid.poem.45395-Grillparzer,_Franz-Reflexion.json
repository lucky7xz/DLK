{"textgrid.poem.45395": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Reflexion", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Denken ist nicht der Empfindung geschenkt;", "tokens": ["Das", "Den\u00b7ken", "ist", "nicht", "der", "Emp\u00b7fin\u00b7dung", "ge\u00b7schenkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Es wirkt als leitende Macht.", "tokens": ["Es", "wirkt", "als", "lei\u00b7ten\u00b7de", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nicht was der Dichter beim Dichten denkt,", "tokens": ["Nicht", "was", "der", "Dich\u00b7ter", "beim", "Dich\u00b7ten", "denkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PWS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nein, was er von jeher gedacht.", "tokens": ["Nein", ",", "was", "er", "von", "je\u00b7her", "ge\u00b7dacht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWS", "PPER", "APPR", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}