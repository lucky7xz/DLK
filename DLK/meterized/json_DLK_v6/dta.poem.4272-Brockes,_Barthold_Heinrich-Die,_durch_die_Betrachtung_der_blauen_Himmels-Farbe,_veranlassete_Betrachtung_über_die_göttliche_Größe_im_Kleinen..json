{"dta.poem.4272": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die,  \n durch die  \n  Betrachtung  \n der  \n  blauen Himmels-Farbe,  \n veranlassete Betrachtung  \n \u00fcber  \n  die g\u00f6ttliche Gr\u00f6\u00dfe im Kleinen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Wolken hatten sich verzogen,", "tokens": ["Die", "Wol\u00b7ken", "hat\u00b7ten", "sich", "ver\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Firmaments gew\u00f6lbter Bogen,", "tokens": ["Des", "Fir\u00b7ma\u00b7ments", "ge\u00b7w\u00f6lb\u00b7ter", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von Regen, Feuchtigkeit und Thau,", "tokens": ["Von", "Re\u00b7gen", ",", "Feuch\u00b7tig\u00b7keit", "und", "Thau", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von D\u00fcnsten und von Nebel rein,", "tokens": ["Von", "D\u00fcns\u00b7ten", "und", "von", "Ne\u00b7bel", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erf\u00fcllt mit nichts, als Sonnenschein,", "tokens": ["Er\u00b7f\u00fcllt", "mit", "nichts", ",", "als", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIS", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zeigt ein so sch\u00f6n, so heiter Blau,", "tokens": ["Zeigt", "ein", "so", "sch\u00f6n", ",", "so", "hei\u00b7ter", "Blau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADV", "ADJD", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df kaum an dessen klaren Zier", "tokens": ["Da\u00df", "kaum", "an", "des\u00b7sen", "kla\u00b7ren", "Zier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein lieblich gl\u00e4nzender Sapphir,", "tokens": ["Ein", "lieb\u00b7lich", "gl\u00e4n\u00b7zen\u00b7der", "Sap\u00b7phir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mit seinem holden Schimmer, reichet,", "tokens": ["Mit", "sei\u00b7nem", "hol\u00b7den", "Schim\u00b7mer", ",", "rei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und alles fast, was auf der Welt,", "tokens": ["Und", "al\u00b7les", "fast", ",", "was", "auf", "der", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "$,", "PRELS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Uns, durch ein sch\u00f6nes Blau, gef\u00e4llt,", "tokens": ["Uns", ",", "durch", "ein", "sch\u00f6\u00b7nes", "Blau", ",", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Vor dem Sapphir des Himmels weichet.", "tokens": ["Vor", "dem", "Sap\u00b7phir", "des", "Him\u00b7mels", "wei\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich sp\u00fchrt\u2019, ob dieser Augen-Weide,", "tokens": ["Ich", "sp\u00fchrt'", ",", "ob", "die\u00b7ser", "Au\u00b7gen\u00b7Wei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein\u2019 innigliche Seelen-Freude;", "tokens": ["Ein'", "in\u00b7nig\u00b7li\u00b7che", "See\u00b7len\u00b7Freu\u00b7de", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Doch dacht\u2019 ich auch zugleich dabey,", "tokens": ["Doch", "dacht'", "ich", "auch", "zu\u00b7gleich", "da\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Woher das Gl\u00e4nzen, so wir sehen,", "tokens": ["Wo\u00b7her", "das", "Gl\u00e4n\u00b7zen", ",", "so", "wir", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch eigentlich wohl m\u00f6gt\u2019 entstehen,", "tokens": ["Doch", "ei\u00b7gent\u00b7lich", "wohl", "m\u00f6gt'", "ent\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und was desselben Ursach\u2019 sey.", "tokens": ["Und", "was", "des\u00b7sel\u00b7ben", "Ur\u00b7sach'", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PDAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was man am Firmament erblickt,", "tokens": ["Was", "man", "am", "Fir\u00b7ma\u00b7ment", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn es so lieblich blau geschm\u00fcckt,", "tokens": ["Wenn", "es", "so", "lieb\u00b7lich", "blau", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist ganz von allem, was hienieden", "tokens": ["Ist", "ganz", "von", "al\u00b7lem", ",", "was", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "PIS", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von uns erblickt wird, unterschieden.", "tokens": ["Von", "uns", "er\u00b7blickt", "wird", ",", "un\u00b7ter\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Hier siehet man das Sonnen-Licht", "tokens": ["Hier", "sie\u00b7het", "man", "das", "Son\u00b7nen\u00b7Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Von allen C\u00f6rpern r\u00fcckwerts prallen,", "tokens": ["Von", "al\u00b7len", "C\u00f6r\u00b7pern", "r\u00fcck\u00b7werts", "pral\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und so in unser Auge fallen;", "tokens": ["Und", "so", "in", "un\u00b7ser", "Au\u00b7ge", "fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So aber dorten nicht geschicht.", "tokens": ["So", "a\u00b7ber", "dor\u00b7ten", "nicht", "ge\u00b7schicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dort senkt der Blick sich, nebst dem Schein", "tokens": ["Dort", "senkt", "der", "Blick", "sich", ",", "nebst", "dem", "Schein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vom Licht, in eine Tief\u2019 hinein,", "tokens": ["Vom", "Licht", ",", "in", "ei\u00b7ne", "Tie\u00b7f'", "hin\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Die sonder Ende, sonder Grenzen.", "tokens": ["Die", "son\u00b7der", "En\u00b7de", ",", "son\u00b7der", "Gren\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Hier trifft den Blick kein ander Licht,", "tokens": ["Hier", "trifft", "den", "Blick", "kein", "an\u00b7der", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PIAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als was sich auf die C\u00f6rper bricht,", "tokens": ["Als", "was", "sich", "auf", "die", "C\u00f6r\u00b7per", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELS", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, blo\u00df durch Winkel, uns ber\u00fchret,", "tokens": ["Und", ",", "blo\u00df", "durch", "Win\u00b7kel", ",", "uns", "be\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "APPR", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn dorten in des Himmels H\u00f6h\u2019", "tokens": ["Wenn", "dor\u00b7ten", "in", "des", "Him\u00b7mels", "H\u00f6h'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Man, in gerader Linie,", "tokens": ["Man", ",", "in", "ge\u00b7ra\u00b7der", "Li\u00b7nie", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Gar keine Gegenw\u00fcrfe sp\u00fchret.", "tokens": ["Gar", "kei\u00b7ne", "Ge\u00b7gen\u00b7w\u00fcr\u00b7fe", "sp\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Es scheint, man werd\u2019 in diesem Reich,", "tokens": ["Es", "scheint", ",", "man", "werd'", "in", "die\u00b7sem", "Reich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PIS", "VAFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Licht, und durch die Luft, zugleich,", "tokens": ["Durch", "Licht", ",", "und", "durch", "die", "Luft", ",", "zu\u00b7gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "APPR", "ART", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als die durchsichtig, hingef\u00fchret", "tokens": ["Als", "die", "durch\u00b7sich\u00b7tig", ",", "hin\u00b7ge\u00b7f\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KOUS", "ART", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In jenes allgemeine Leer,", "tokens": ["In", "je\u00b7nes", "all\u00b7ge\u00b7mei\u00b7ne", "Leer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Worinn der Sonnen-Strahlen Heer", "tokens": ["Wo\u00b7rinn", "der", "Son\u00b7nen\u00b7Strah\u00b7len", "Heer"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zuletzt sich endlich selbst verlieret.", "tokens": ["Zu\u00b7letzt", "sich", "end\u00b7lich", "selbst", "ver\u00b7lie\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Es scheint demnach die Lieblichkeit", "tokens": ["Es", "scheint", "dem\u00b7nach", "die", "Lieb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von diesem wundersch\u00f6nen Blauen,", "tokens": ["Von", "die\u00b7sem", "wun\u00b7der\u00b7sch\u00f6\u00b7nen", "Blau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So wir am Firmamente schauen,", "tokens": ["So", "wir", "am", "Fir\u00b7ma\u00b7men\u00b7te", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur blo\u00df aus Licht und Dunkelheit,", "tokens": ["Nur", "blo\u00df", "aus", "Licht", "und", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So viel man fasset, zu besteh\u2019n;", "tokens": ["So", "viel", "man", "fas\u00b7set", ",", "zu", "be\u00b7steh'n", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Indem wir gleichsam eine Nacht,", "tokens": ["In\u00b7dem", "wir", "gleich\u00b7sam", "ei\u00b7ne", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die sich dem Licht zur Fulge macht,", "tokens": ["Die", "sich", "dem", "Licht", "zur", "Ful\u00b7ge", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Durchs Licht, das sonst nicht sichtbar, seh\u2019n.", "tokens": ["Durchs", "Licht", ",", "das", "sonst", "nicht", "sicht\u00b7bar", ",", "seh'", "n."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "abbreviation"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADV", "PTKNEG", "ADJD", "$,", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Die dunkle Tiefe scheint das Gl\u00e4nzen", "tokens": ["Die", "dunk\u00b7le", "Tie\u00b7fe", "scheint", "das", "Gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des unerme\u00dfnen Sonnen-Lichts,", "tokens": ["Des", "un\u00b7er\u00b7me\u00df\u00b7nen", "Son\u00b7nen\u00b7Lichts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jedoch des menschlichen Gesichts,", "tokens": ["Je\u00b7doch", "des", "menschli\u00b7chen", "Ge\u00b7sichts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und Geister Kraft, nicht zu begrenzen,", "tokens": ["Und", "Geis\u00b7ter", "Kraft", ",", "nicht", "zu", "be\u00b7gren\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "$,", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Indem wir ihre Tiefe trennen,", "tokens": ["In\u00b7dem", "wir", "ih\u00b7re", "Tie\u00b7fe", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und, in den Boden-losen H\u00f6h\u2019n,", "tokens": ["Und", ",", "in", "den", "Bo\u00b7den\u00b7lo\u00b7sen", "H\u00f6h'n", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Viel Millionen Sonnen seh\u2019n,", "tokens": ["Viel", "Mil\u00b7lion\u00b7en", "Son\u00b7nen", "seh'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVINF", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Und GOtt darinn verehren k\u00f6nnen.", "tokens": ["Und", "Gott", "da\u00b7rinn", "ver\u00b7eh\u00b7ren", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PAV", "VVFIN", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ach, la\u00dft uns denn den HErrn der Sternen-", "tokens": ["Ach", ",", "la\u00dft", "uns", "denn", "den", "Herrn", "der", "Ster\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "ADV", "ART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und auch zugleich erkennen lernen", "tokens": ["Und", "auch", "zu\u00b7gleich", "er\u00b7ken\u00b7nen", "ler\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVINF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Gr\u00f6sse Seiner Weisheit, Macht,", "tokens": ["Die", "Gr\u00f6s\u00b7se", "Sei\u00b7ner", "Weis\u00b7heit", ",", "Macht", ","], "token_info": ["word", "word", "word", "word", "pun