{"textgrid.poem.66060": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Mond scheint auf mein Lager,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mond scheint auf mein Lager,", "tokens": ["Der", "Mond", "scheint", "auf", "mein", "La\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich schlafe nicht,", "tokens": ["Ich", "schla\u00b7fe", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Meine gefalteten H\u00e4nde ruhen", "tokens": ["Mei\u00b7ne", "ge\u00b7fal\u00b7te\u00b7ten", "H\u00e4n\u00b7de", "ru\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "In seinem Licht.", "tokens": ["In", "sei\u00b7nem", "Licht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Meine Seele ist still, sie kehrte", "tokens": ["Mei\u00b7ne", "See\u00b7le", "ist", "still", ",", "sie", "kehr\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPER", "VVFIN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Von Gott zur\u00fcck,", "tokens": ["Von", "Gott", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und mein Herz hat nur einen Gedanken:", "tokens": ["Und", "mein", "Herz", "hat", "nur", "ei\u00b7nen", "Ge\u00b7dan\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Dich und dein Gl\u00fcck.", "tokens": ["Dich", "und", "dein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPOSAT", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}