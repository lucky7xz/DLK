{"textgrid.poem.47079": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was gestern war, o la\u00df es mich vergessen!", "tokens": ["Was", "ge\u00b7stern", "war", ",", "o", "la\u00df", "es", "mich", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "$,", "FM", "VVIMP", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was morgen sein wird, la\u00df mich nicht ermessen!", "tokens": ["Was", "mor\u00b7gen", "sein", "wird", ",", "la\u00df", "mich", "nicht", "er\u00b7mes\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAINF", "VAFIN", "$,", "VVIMP", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df mich versinken in das sch\u00f6ne Heut'!", "tokens": ["La\u00df", "mich", "ver\u00b7sin\u00b7ken", "in", "das", "sch\u00f6\u00b7ne", "Heut'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "La\u00df mich einmal ganz unbefangen scheinen,", "tokens": ["La\u00df", "mich", "ein\u00b7mal", "ganz", "un\u00b7be\u00b7fan\u00b7gen", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O la\u00df mich thun, als k\u00f6nnt' ich niemals weinen,", "tokens": ["O", "la\u00df", "mich", "thun", ",", "als", "k\u00f6nnt'", "ich", "nie\u00b7mals", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVINF", "$,", "KOUS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da mir dein Blick einmal ein L\u00e4cheln beut.", "tokens": ["Da", "mir", "dein", "Blick", "ein\u00b7mal", "ein", "L\u00e4\u00b7cheln", "beut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Was k\u00fcmmert's mich, da\u00df vor dem Lenz ein Winter?", "tokens": ["Was", "k\u00fcm\u00b7mert's", "mich", ",", "da\u00df", "vor", "dem", "Lenz", "ein", "Win\u00b7ter", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Was k\u00fcmmert's mich, da\u00df einer ist dahinter?", "tokens": ["Was", "k\u00fcm\u00b7mert's", "mich", ",", "da\u00df", "ei\u00b7ner", "ist", "da\u00b7hin\u00b7ter", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PIS", "VAFIN", "PAV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wenn zwischeninn der kurze Lenz mich freut!", "tokens": ["Wenn", "zwi\u00b7sche\u00b7ninn", "der", "kur\u00b7ze", "Lenz", "mich", "freut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}