{"dta.poem.19618": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Auf Ponte Sisto.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00df ist das Dunkel nach Gluten des Tags! Auf d\u00e4mmernder", "tokens": ["S\u00fc\u00df", "ist", "das", "Dun\u00b7kel", "nach", "Glu\u00b7ten", "des", "Tags", "!", "Auf", "d\u00e4m\u00b7mern\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "APPR", "NN", "ART", "NN", "$.", "APPR", "NE"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Schau' ich die Ufer entlang dieser unsterblichen Stadt.", "tokens": ["Schau'", "ich", "die", "U\u00b7fer", "ent\u00b7lang", "die\u00b7ser", "uns\u00b7terb\u00b7li\u00b7chen", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPO", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Burgen und Tempel verwachsen zu Einer gewaltigen Sage!", "tokens": ["Bur\u00b7gen", "und", "Tem\u00b7pel", "ver\u00b7wach\u00b7sen", "zu", "Ei\u00b7ner", "ge\u00b7wal\u00b7ti\u00b7gen", "Sa\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Unter mir h\u00fctet der Strom manchen verschollenen Hort.", "tokens": ["Un\u00b7ter", "mir", "h\u00fc\u00b7tet", "der", "Strom", "man\u00b7chen", "ver\u00b7schol\u00b7le\u00b7nen", "Hort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "PIAT", "ADJA", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Dort in der Flut eines Nachens Gespenst! Ist's ein fl\u00fcchtiger", "tokens": ["Dort", "in", "der", "Flut", "ei\u00b7nes", "Na\u00b7chens", "Ge\u00b7spenst", "!", "Ist's", "ein", "fl\u00fcch\u00b7ti\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "NN", "$.", "NE", "ART", "ADJA"], "meter": "+--+--+--+--+-+", "measure": "diphilius"}, "line.6": {"text": "Ist es der \u201eJakob vom Kahn\u201c", "tokens": ["Ist", "es", "der", "\u201e", "Ja\u00b7kob", "vom", "Kahn", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "$(", "NE", "APPRART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gellend erhebt sich Gesang in dem Boot zum Ruhme des Liebchens.", "tokens": ["Gel\u00b7lend", "er\u00b7hebt", "sich", "Ge\u00b7sang", "in", "dem", "Boot", "zum", "Ruh\u00b7me", "des", "Lieb\u00b7chens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PRF", "NN", "APPR", "ART", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Horch! Ein lebendiger Mund fordert lebendiges Gl\u00fcck.", "tokens": ["Horch", "!", "Ein", "le\u00b7ben\u00b7di\u00b7ger", "Mund", "for\u00b7dert", "le\u00b7ben\u00b7di\u00b7ges", "Gl\u00fcck", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}}}}}