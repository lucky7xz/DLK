{"textgrid.poem.25541": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wei\u00dft du, Grambezwinger du!", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wei\u00dft du, Grambezwinger du!", "tokens": ["Wei\u00dft", "du", ",", "Gram\u00b7be\u00b7zwin\u00b7ger", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df mein ", "tokens": ["Da\u00df", "mein"], "token_info": ["word", "word"], "pos": ["KOUS", "PPOSAT"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Alle Nacht sich qu\u00e4le?", "tokens": ["Al\u00b7le", "Nacht", "sich", "qu\u00e4\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Stille doch mit deinem Trank'", "tokens": ["Stil\u00b7le", "doch", "mit", "dei\u00b7nem", "Trank'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Diesen wunderbaren Zank", "tokens": ["Die\u00b7sen", "wun\u00b7der\u00b7ba\u00b7ren", "Zank"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zwischen Leib und Seele.", "tokens": ["Zwi\u00b7schen", "Leib", "und", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Aber, aber, trinkest du", "tokens": ["A\u00b7ber", ",", "a\u00b7ber", ",", "trin\u00b7kest", "du"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "$,", "ADV", "$,", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihr zum letzten male zu,", "tokens": ["Ihr", "zum", "letz\u00b7ten", "ma\u00b7le", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Ha! zum letzten male!", "tokens": ["Ha", "!", "zum", "letz\u00b7ten", "ma\u00b7le", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Dann so reiche mir geschwind", "tokens": ["Dann", "so", "rei\u00b7che", "mir", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Auch f\u00fcr immer, liebes Kind,", "tokens": ["Auch", "f\u00fcr", "im\u00b7mer", ",", "lie\u00b7bes", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Deine Schlummer-Schaale.", "tokens": ["Dei\u00b7ne", "Schlum\u00b7mer\u00b7Schaa\u00b7le", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}