{"dta.poem.21414": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sehnte mich nach Torfgeruch,", "tokens": ["Ich", "sehn\u00b7te", "mich", "nach", "Torf\u00b7ge\u00b7ruch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach deutschem Tabaksdampfe;", "tokens": ["Nach", "deut\u00b7schem", "Ta\u00b7baks\u00b7damp\u00b7fe", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es bebte mein Fu\u00df vor Ungeduld,", "tokens": ["Es", "beb\u00b7te", "mein", "Fu\u00df", "vor", "Un\u00b7ge\u00b7duld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da\u00df er deutschen Boden stampfe.", "tokens": ["Da\u00df", "er", "deut\u00b7schen", "Bo\u00b7den", "stamp\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich seufzte des Nachts, und sehnte mich,", "tokens": ["Ich", "seufz\u00b7te", "des", "Nachts", ",", "und", "sehn\u00b7te", "mich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADV", "$,", "KON", "VVFIN", "PPER", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da\u00df ich sie wieders\u00e4he,", "tokens": ["Da\u00df", "ich", "sie", "wie\u00b7der\u00b7s\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die alte Frau, die am Dammthor wohnt;", "tokens": ["Die", "al\u00b7te", "Frau", ",", "die", "am", "Damm\u00b7thor", "wohnt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Lottchen wohnt in der N\u00e4he.", "tokens": ["Das", "Lott\u00b7chen", "wohnt", "in", "der", "N\u00e4\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Auch jenem edlen alten Herrn,", "tokens": ["Auch", "je\u00b7nem", "ed\u00b7len", "al\u00b7ten", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der immer mich ausgescholten", "tokens": ["Der", "im\u00b7mer", "mich", "aus\u00b7ge\u00b7schol\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "PPER", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und immer gro\u00dfm\u00fcthig besch\u00fctzt, auch ihm", "tokens": ["Und", "im\u00b7mer", "gro\u00df\u00b7m\u00fct\u00b7hig", "be\u00b7sch\u00fctzt", ",", "auch", "ihm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "ADJD", "VVPP", "$,", "ADV", "PPER"], "meter": "-+-+++-+-+", "measure": "zehnsilber"}, "line.4": {"text": "Hat mancher Seufzer gegolten.", "tokens": ["Hat", "man\u00b7cher", "Seuf\u00b7zer", "ge\u00b7gol\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Ich wollte wieder aus seinem Mund", "tokens": ["Ich", "woll\u00b7te", "wie\u00b7der", "aus", "sei\u00b7nem", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vernehmen den \u201edummen Jungen!\u201c", "tokens": ["Ver\u00b7neh\u00b7men", "den", "\u201e", "dum\u00b7men", "Jun\u00b7gen", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "$(", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das hat mir immer wie Musik", "tokens": ["Das", "hat", "mir", "im\u00b7mer", "wie", "Mu\u00b7sik"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "KOKOM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Herzen nachgeklungen.", "tokens": ["Im", "Her\u00b7zen", "nach\u00b7ge\u00b7klun\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}