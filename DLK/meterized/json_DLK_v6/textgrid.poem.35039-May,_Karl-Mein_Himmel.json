{"textgrid.poem.35039": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Mein Himmel", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ist der Himmel doch so weit", "tokens": ["Wie", "ist", "der", "Him\u00b7mel", "doch", "so", "weit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entfernt von mir mit seinen Sternen!", "tokens": ["Ent\u00b7fernt", "von", "mir", "mit", "sei\u00b7nen", "Ster\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er baut zur Grenzenlosigkeit", "tokens": ["Er", "baut", "zur", "Gren\u00b7zen\u00b7lo\u00b7sig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich auf durch unme\u00dfbare Fernen.", "tokens": ["Sich", "auf", "durch", "un\u00b7me\u00df\u00b7ba\u00b7re", "Fer\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es reicht mein schwacher Blick nicht hin,", "tokens": ["Es", "reicht", "mein", "schwa\u00b7cher", "Blick", "nicht", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mir nur die n\u00e4chste Welt zu zeigen;", "tokens": ["Mir", "nur", "die", "n\u00e4chs\u00b7te", "Welt", "zu", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich f\u00fchle, da\u00df ich Erde bin,", "tokens": ["Ich", "f\u00fch\u00b7le", ",", "da\u00df", "ich", "Er\u00b7de", "bin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht wert, zu ihr empor zu steigen.", "tokens": ["Nicht", "wert", ",", "zu", "ihr", "em\u00b7por", "zu", "stei\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "APPR", "PPER", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie ist der Himmel doch so nah!", "tokens": ["Wie", "ist", "der", "Him\u00b7mel", "doch", "so", "nah", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er strahlt in mir mit tausend Sternen.", "tokens": ["Er", "strahlt", "in", "mir", "mit", "tau\u00b7send", "Ster\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fchl ich ihn nicht, er ist doch da;", "tokens": ["F\u00fchl", "ich", "ihn", "nicht", ",", "er", "ist", "doch", "da", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "PTKNEG", "$,", "PPER", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich mu\u00df ihn nur erfassen lernen.", "tokens": ["Ich", "mu\u00df", "ihn", "nur", "er\u00b7fas\u00b7sen", "ler\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die ganze Unerme\u00dflichkeit", "tokens": ["Die", "gan\u00b7ze", "Un\u00b7er\u00b7me\u00df\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Liebe darf ich in mir tragen;", "tokens": ["Der", "Lie\u00b7be", "darf", "ich", "in", "mir", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es hemmt sie weder Raum noch Zeit,", "tokens": ["Es", "hemmt", "sie", "we\u00b7der", "Raum", "noch", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mich auf zu Gott, dem Herrn zu tragen.", "tokens": ["Mich", "auf", "zu", "Gott", ",", "dem", "Herrn", "zu", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "APPR", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Unendlich und doch endlich ist", "tokens": ["Un\u00b7end\u00b7lich", "und", "doch", "end\u00b7lich", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADV", "ADV", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Himmel um die kleine Erde,", "tokens": ["Der", "Him\u00b7mel", "um", "die", "klei\u00b7ne", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch du in meinem Herzen bist", "tokens": ["Doch", "du", "in", "mei\u00b7nem", "Her\u00b7zen", "bist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der, den ich ewig haben werde.", "tokens": ["Der", ",", "den", "ich", "e\u00b7wig", "ha\u00b7ben", "wer\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "PPER", "ADJD", "VAINF", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was andern Himmeln drohen mag,", "tokens": ["Was", "an\u00b7dern", "Him\u00b7meln", "dro\u00b7hen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dir hat es nicht und nie zu gelten:", "tokens": ["Dir", "hat", "es", "nicht", "und", "nie", "zu", "gel\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "KON", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fcr dich giebts keinen letzten Tag", "tokens": ["F\u00fcr", "dich", "giebts", "kei\u00b7nen", "letz\u00b7ten", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und keinen Untergang der Welten.", "tokens": ["Und", "kei\u00b7nen", "Un\u00b7ter\u00b7gang", "der", "Wel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie ist der Himmel doch so weit,", "tokens": ["Wie", "ist", "der", "Him\u00b7mel", "doch", "so", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wie so nahe kann er liegen,", "tokens": ["Und", "wie", "so", "na\u00b7he", "kann", "er", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "ADJD", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn \u00fcber unsre Bl\u00f6digkeit", "tokens": ["Wenn", "\u00fc\u00b7ber", "uns\u00b7re", "Bl\u00f6\u00b7dig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Glaube und die Liebe siegen.", "tokens": ["Der", "Glau\u00b7be", "und", "die", "Lie\u00b7be", "sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich blick empor; ich schau in mich;", "tokens": ["Ich", "blick", "em\u00b7por", ";", "ich", "schau", "in", "mich", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "PPER", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dort darf ich nichts, hier Alles hoffen.", "tokens": ["Dort", "darf", "ich", "nichts", ",", "hier", "Al\u00b7les", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "$,", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Gott und Herr, ich bitte dich,", "tokens": ["Mein", "Gott", "und", "Herr", ",", "ich", "bit\u00b7te", "dich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Erhalt mir diesen Himmel offen!", "tokens": ["Er\u00b7halt", "mir", "die\u00b7sen", "Him\u00b7mel", "of\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}