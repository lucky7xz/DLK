{"textgrid.poem.58491": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "529. Die beste Gegenwehr", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein schlechtes Herz, geweiht zu Gottes Friedensstadt,", "tokens": ["Mein", "schlech\u00b7tes", "Herz", ",", "ge\u00b7weiht", "zu", "Got\u00b7tes", "Frie\u00b7dens\u00b7stadt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVPP", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein' feste Burg im Grund, den Namen Jesus, hat;", "tokens": ["Ein'", "fes\u00b7te", "Burg", "im", "Grund", ",", "den", "Na\u00b7men", "Je\u00b7sus", ",", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,", "ART", "NN", "NE", "$,", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Feind' erbost die Stadt bestreiten,", "tokens": ["Die", "Feind'", "er\u00b7bost", "die", "Stadt", "be\u00b7strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie wird belagert gar und \u00f6fters hart bedr\u00e4ngt,", "tokens": ["Sie", "wird", "be\u00b7la\u00b7gert", "gar", "und", "\u00f6f\u00b7ters", "hart", "be\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ADV", "KON", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Manch giftiges Gescho\u00df wird auf sie losgesprengt", "tokens": ["Manch", "gif\u00b7ti\u00b7ges", "Ge\u00b7scho\u00df", "wird", "auf", "sie", "los\u00b7ge\u00b7sprengt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bei Tag, bei Nacht, von allen Seiten.", "tokens": ["Bei", "Tag", ",", "bei", "Nacht", ",", "von", "al\u00b7len", "Sei\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich Schwacher fall' nicht aus, da\u00df ich die Feind' vertreib',", "tokens": ["Ich", "Schwa\u00b7cher", "fall'", "nicht", "aus", ",", "da\u00df", "ich", "die", "Feind'", "ver\u00b7treib'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "PTKNEG", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich schlie\u00df' vielmehr mich ein und in Gott stille bleib',", "tokens": ["Ich", "schlie\u00df'", "viel\u00b7mehr", "mich", "ein", "und", "in", "Gott", "stil\u00b7le", "bleib'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "PTKVZ", "KON", "APPR", "NN", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So hat kein Sturm 'was zu bedeuten.", "tokens": ["So", "hat", "kein", "Sturm", "'was", "zu", "be\u00b7deu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "NE", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}