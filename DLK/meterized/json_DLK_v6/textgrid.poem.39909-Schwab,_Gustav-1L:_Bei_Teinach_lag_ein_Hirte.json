{"textgrid.poem.39909": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bei Teinach lag ein Hirte", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bei Teinach lag ein Hirte", "tokens": ["Bei", "Tei\u00b7nach", "lag", "ein", "Hir\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und schlief im gr\u00fcnen Gras,", "tokens": ["Und", "schlief", "im", "gr\u00fc\u00b7nen", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Derweil sein Heerdlein irrte", "tokens": ["Der\u00b7weil", "sein", "Heerd\u00b7lein", "irr\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und frische Kr\u00e4uter las;", "tokens": ["Und", "fri\u00b7sche", "Kr\u00e4u\u00b7ter", "las", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den f\u00fchrt' um ein Jahrhundert", "tokens": ["Den", "f\u00fchrt'", "um", "ein", "Jahr\u00b7hun\u00b7dert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein seltner Traum zur\u00fcck,", "tokens": ["Ein", "selt\u00b7ner", "Traum", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er stand und warf verwundert", "tokens": ["Er", "stand", "und", "warf", "ver\u00b7wun\u00b7dert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In's D\u00f6rflein seinen Blick.", "tokens": ["In's", "D\u00f6r\u00b7flein", "sei\u00b7nen", "Blick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die H\u00e4user, die er wachend", "tokens": ["Die", "H\u00e4u\u00b7ser", ",", "die", "er", "wa\u00b7chend"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als alt und grau gekannt,", "tokens": ["Als", "alt", "und", "grau", "ge\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie standen jung und lachend", "tokens": ["Sie", "stan\u00b7den", "jung", "und", "la\u00b7chend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit roter Ziegelwand.", "tokens": ["Mit", "ro\u00b7ter", "Zie\u00b7gel\u00b7wand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wo jetzt ist zu schauen", "tokens": ["Und", "wo", "jetzt", "ist", "zu", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "VAFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das sch\u00f6ne Gotteshaus,", "tokens": ["Das", "sch\u00f6\u00b7ne", "Got\u00b7tes\u00b7haus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Fing man erst an zu bauen,", "tokens": ["Fing", "man", "erst", "an", "zu", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und hieb den Grundstein aus.", "tokens": ["Und", "hieb", "den", "Grund\u00b7stein", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Maurer waren fertig,", "tokens": ["Die", "Mau\u00b7rer", "wa\u00b7ren", "fer\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie ruhten aus vom Flei\u00df,", "tokens": ["Sie", "ruh\u00b7ten", "aus", "vom", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und des Befehls gew\u00e4rtig", "tokens": ["Und", "des", "Be\u00b7fehls", "ge\u00b7w\u00e4r\u00b7tig"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch standen sie im Kreis;", "tokens": ["Noch", "stan\u00b7den", "sie", "im", "Kreis", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da kam ein Zug gegangen", "tokens": ["Da", "kam", "ein", "Zug", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In feierlicher Pracht,", "tokens": ["In", "fei\u00b7er\u00b7li\u00b7cher", "Pracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mit Federn, M\u00e4nteln, Spangen,", "tokens": ["Mit", "Fe\u00b7dern", ",", "M\u00e4n\u00b7teln", ",", "Span\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nach jener Zeiten Tracht.", "tokens": ["Nach", "je\u00b7ner", "Zei\u00b7ten", "Tracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und ohne lang zu fragen", "tokens": ["Und", "oh\u00b7ne", "lang", "zu", "fra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ward's ihm im Traume klar,", "tokens": ["Ward's", "ihm", "im", "Trau\u00b7me", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Da\u00df der im gold'gen Kragen", "tokens": ["Da\u00df", "der", "im", "gold'\u00b7gen", "Kra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Herzog selber war.", "tokens": ["Der", "Her\u00b7zog", "sel\u00b7ber", "war", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Neuste drein zu stiften", "tokens": ["Das", "Neus\u00b7te", "drein", "zu", "stif\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Tritt der zum hohlen Stein,", "tokens": ["Tritt", "der", "zum", "hoh\u00b7len", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Mit blanken M\u00fcnzen, Schriften,", "tokens": ["Mit", "blan\u00b7ken", "M\u00fcn\u00b7zen", ",", "Schrif\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und neuem, edlem Wein.", "tokens": ["Und", "neu\u00b7em", ",", "ed\u00b7lem", "Wein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da wird erst von der Gabe", "tokens": ["Da", "wird", "erst", "von", "der", "Ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein hohes Glas gef\u00fcllt,", "tokens": ["Ein", "ho\u00b7hes", "Glas", "ge\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Damit zu s\u00fc\u00dfer Labe", "tokens": ["Da\u00b7mit", "zu", "s\u00fc\u00b7\u00dfer", "La\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Herr den Durst sich stillt.", "tokens": ["Der", "Herr", "den", "Durst", "sich", "stillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und sieh, da f\u00e4llt dem F\u00fcrsten", "tokens": ["Und", "sieh", ",", "da", "f\u00e4llt", "dem", "F\u00fcrs\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Hirt' in das Gesicht,", "tokens": ["Der", "Hirt'", "in", "das", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er sieht ihm an sein D\u00fcrsten,", "tokens": ["Er", "sieht", "ihm", "an", "sein", "D\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Reicht ihm das Glas und spricht:", "tokens": ["Reicht", "ihm", "das", "Glas", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbtrink', Freund! es ist der beste", "tokens": ["\u00bb", "trink'", ",", "Freund", "!", "es", "ist", "der", "bes\u00b7te"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "$,", "NN", "$.", "PPER", "VAFIN", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus meinem Neckarthal,", "tokens": ["Aus", "mei\u00b7nem", "Ne\u00b7ck\u00b7ar\u00b7thal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Du kommst zu solchem Feste", "tokens": ["Du", "kommst", "zu", "sol\u00b7chem", "Fes\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch wohl nicht noch einmal.\u00ab", "tokens": ["Doch", "wohl", "nicht", "noch", "ein\u00b7mal", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Schon f\u00fchlet an den Lippen", "tokens": ["Schon", "f\u00fch\u00b7let", "an", "den", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Hirte sich das Glas,", "tokens": ["Der", "Hir\u00b7te", "sich", "das", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und eben wollt' er nippen, \u2013", "tokens": ["Und", "e\u00b7ben", "wollt'", "er", "nip\u00b7pen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "VVINF", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da wacht er auf im Gras.", "tokens": ["Da", "wacht", "er", "auf", "im", "Gras", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Er blickt um sich erschrocken,", "tokens": ["Er", "blickt", "um", "sich", "er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er f\u00fchlt die Hand sich leer,", "tokens": ["Er", "f\u00fchlt", "die", "Hand", "sich", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er f\u00fchlt den Mund sich trocken,", "tokens": ["Er", "f\u00fchlt", "den", "Mund", "sich", "tro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ach! es fehlt noch mehr!", "tokens": ["Und", "ach", "!", "es", "fehlt", "noch", "mehr", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wein l\u00e4\u00dft sich wieder kaufen,", "tokens": ["Wein", "l\u00e4\u00dft", "sich", "wie\u00b7der", "kau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Doch wie er tr\u00e4umet hier,", "tokens": ["Doch", "wie", "er", "tr\u00e4u\u00b7met", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ist ihm davon gelaufen", "tokens": ["Ist", "ihm", "da\u00b7von", "ge\u00b7lau\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PAV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Heerde sch\u00f6nster Stier.", "tokens": ["Der", "Heer\u00b7de", "sch\u00f6ns\u00b7ter", "Stier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Er richtet sich mit Fluchen", "tokens": ["Er", "rich\u00b7tet", "sich", "mit", "Flu\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom leeren Boden auf,", "tokens": ["Vom", "lee\u00b7ren", "Bo\u00b7den", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Fl\u00fcchtigen zu suchen", "tokens": ["Den", "Fl\u00fcch\u00b7ti\u00b7gen", "zu", "su\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Beginnt er seinen Lauf;", "tokens": ["Be\u00b7ginnt", "er", "sei\u00b7nen", "Lauf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bis wo in B\u00fcschen stille", "tokens": ["Bis", "wo", "in", "B\u00fc\u00b7schen", "stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PWAV", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sich birgt ein alt Gestein,", "tokens": ["Sich", "birgt", "ein", "alt", "Ge\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Von dort h\u00f6rt er Gebr\u00fclle,", "tokens": ["Von", "dort", "h\u00f6rt", "er", "Ge\u00b7br\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.