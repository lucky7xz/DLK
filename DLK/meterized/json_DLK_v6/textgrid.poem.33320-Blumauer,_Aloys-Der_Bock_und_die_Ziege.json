{"textgrid.poem.33320": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Der Bock und die Ziege", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu einem Bock, der, weil er schwarz von Haar", "tokens": ["Zu", "ei\u00b7nem", "Bock", ",", "der", ",", "weil", "er", "schwarz", "von", "Haar"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "$,", "PRELS", "$,", "KOUS", "PPER", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von langem Bart, und finst'rer Stirne war,", "tokens": ["Von", "lan\u00b7gem", "Bart", ",", "und", "finst'\u00b7rer", "Stir\u00b7ne", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ganz einem hochgelehrten Meister", "tokens": ["Ganz", "ei\u00b7nem", "hoch\u00b7ge\u00b7lehr\u00b7ten", "Meis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An Ausseh'n und an Mienen glich,", "tokens": ["An", "Aus\u00b7seh'n", "und", "an", "Mie\u00b7nen", "glich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Und der, durch jeden Kampf noch dreister,", "tokens": ["Und", "der", ",", "durch", "je\u00b7den", "Kampf", "noch", "dreis\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "APPR", "PIAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In keinem Bocksgefechte wich,", "tokens": ["In", "kei\u00b7nem", "Bocks\u00b7ge\u00b7fech\u00b7te", "wich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den die Natur statt dem Gehirne", "tokens": ["Den", "die", "Na\u00b7tur", "statt", "dem", "Ge\u00b7hir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "APPR", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Mit zwei steinharten Kn\u00f6pfen an der Stirne", "tokens": ["Mit", "zwei", "stein\u00b7har\u00b7ten", "Kn\u00f6p\u00b7fen", "an", "der", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Zum Kampf versah, der, wenn er stie\u00df,", "tokens": ["Zum", "Kampf", "ver\u00b7sah", ",", "der", ",", "wenn", "er", "stie\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "PRELS", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sich und den Gegner stets in eine Lache schmi\u00df,", "tokens": ["Sich", "und", "den", "Geg\u00b7ner", "stets", "in", "ei\u00b7ne", "La\u00b7che", "schmi\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KON", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und, wenn die Scham den Gegenpart vertrieben,", "tokens": ["Und", ",", "wenn", "die", "Scham", "den", "Ge\u00b7gen\u00b7part", "ver\u00b7trie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Der Letzte auf dem Platz geblieben.", "tokens": ["Der", "Letz\u00b7te", "auf", "dem", "Platz", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Der endlich, weil sein Herz so hart", "tokens": ["Der", "end\u00b7lich", ",", "weil", "sein", "Herz", "so", "hart"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "$,", "KOUS", "PPOSAT", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Als seine Stirne schien \u2013 der Heerde F\u00fchrer ward:", "tokens": ["Als", "sei\u00b7ne", "Stir\u00b7ne", "schien", "\u2013", "der", "Heer\u00b7de", "F\u00fch\u00b7rer", "ward", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$(", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Zu diesem Bock kam eine durst'ge Ziege,", "tokens": ["Zu", "die\u00b7sem", "Bock", "kam", "ei\u00b7ne", "dur\u00b7st'\u00b7ge", "Zie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Und flehte, wie die Armuth flehen kann,", "tokens": ["Und", "fleh\u00b7te", ",", "wie", "die", "Ar\u00b7muth", "fle\u00b7hen", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Ihn um ein bischen Klee f\u00fcr ihre Jungen an.", "tokens": ["Ihn", "um", "ein", "bi\u00b7schen", "Klee", "f\u00fcr", "ih\u00b7re", "Jun\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.18": {"text": "\u00bbmeinst du, da\u00df ich mein Futter gratis kriege?\u00ab", "tokens": ["\u00bb", "meinst", "du", ",", "da\u00df", "ich", "mein", "Fut\u00b7ter", "gra\u00b7tis", "krie\u00b7ge", "?", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPOSAT", "NN", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Erwiederte der Bock, der wie", "tokens": ["Er\u00b7wie\u00b7der\u00b7te", "der", "Bock", ",", "der", "wie"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NE", "$,", "PRELS", "KOKOM"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Ein Wuch'rer nur auf Pf\u00e4nder lieh,", "tokens": ["Ein", "Wuch'\u00b7rer", "nur", "auf", "Pf\u00e4n\u00b7der", "lieh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "\u00bbzu schenken hab' ich nichts; doch weil da steht geschrieben,", "tokens": ["\u00bb", "zu", "schen\u00b7ken", "hab'", "ich", "nichts", ";", "doch", "weil", "da", "steht", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKZU", "VVINF", "VAFIN", "PPER", "PIS", "$.", "ADV", "KOUS", "ADV", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Man m\u00fcsse seinen N\u00e4chsten lieben,", "tokens": ["Man", "m\u00fcs\u00b7se", "sei\u00b7nen", "N\u00e4chs\u00b7ten", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "So will ich, wenn du zahlst, und Sicherheit", "tokens": ["So", "will", "ich", ",", "wenn", "du", "zahlst", ",", "und", "Si\u00b7cher\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Mir schaffen kannst, auf eine kurze Zeit", "tokens": ["Mir", "schaf\u00b7fen", "kannst", ",", "auf", "ei\u00b7ne", "kur\u00b7ze", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "VMFIN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Von meinem Futter dir, so viel du brauchest leihen.\u00ab", "tokens": ["Von", "mei\u00b7nem", "Fut\u00b7ter", "dir", ",", "so", "viel", "du", "brau\u00b7chest", "lei\u00b7hen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "$,", "ADV", "ADV", "PPER", "VVFIN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "\u00bbich w\u00fcrde nicht das Zahlen scheuen,", "tokens": ["\u00bb", "ich", "w\u00fcr\u00b7de", "nicht", "das", "Zah\u00b7len", "scheu\u00b7en", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Wenn du mir borgtest, sprach die Ziege; aber wer", "tokens": ["Wenn", "du", "mir", "borg\u00b7test", ",", "sprach", "die", "Zie\u00b7ge", ";", "a\u00b7ber", "wer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "ART", "NN", "$.", "KON", "PWS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Verb\u00fcrgt f\u00fcr Arme sich? und ach, ein Pfand, woher?", "tokens": ["Ver\u00b7b\u00fcrgt", "f\u00fcr", "Ar\u00b7me", "sich", "?", "und", "ach", ",", "ein", "Pfand", ",", "wo\u00b7her", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PRF", "$.", "KON", "XY", "$,", "ART", "NN", "$,", "PWAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Du hast ja noch an deinem Leib, versetzte", "tokens": ["Du", "hast", "ja", "noch", "an", "dei\u00b7nem", "Leib", ",", "ver\u00b7setz\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Der z\u00e4he Filz, ein sch\u00f6nes Fell,\u00ab und sch\u00e4tzte", "tokens": ["Der", "z\u00e4\u00b7he", "Filz", ",", "ein", "sch\u00f6\u00b7nes", "Fell", ",", "\u00ab", "und", "sch\u00e4tz\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,", "$(", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Mit einem Blick den Werth; \u00bbverpf\u00e4nde mir", "tokens": ["Mit", "ei\u00b7nem", "Blick", "den", "Werth", ";", "\u00bb", "ver\u00b7pf\u00e4n\u00b7de", "mir"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$.", "$(", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Den Balg inde\u00df', in warmen Sommertagen", "tokens": ["Den", "Balg", "in\u00b7de\u00df'", ",", "in", "war\u00b7men", "Som\u00b7mer\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Pflegt man ja keinen Pelz zu tragen:", "tokens": ["Pflegt", "man", "ja", "kei\u00b7nen", "Pelz", "zu", "tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "La\u00df mir zur Sicherheit ihn hier,", "tokens": ["La\u00df", "mir", "zur", "Si\u00b7cher\u00b7heit", "ihn", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Ich will ihn dir bewahren vor den Schaben.", "tokens": ["Ich", "will", "ihn", "dir", "be\u00b7wah\u00b7ren", "vor", "den", "Scha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPER", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Im Winter, wenn du zahlst, magst du ihn wieder, haben.\u00ab", "tokens": ["Im", "Win\u00b7ter", ",", "wenn", "du", "zahlst", ",", "magst", "du", "ihn", "wie\u00b7der", ",", "ha\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "PPER", "ADV", "$,", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was war zu thun? Die Ziege brauchte Klee.", "tokens": ["Was", "war", "zu", "thun", "?", "Die", "Zie\u00b7ge", "brauch\u00b7te", "Klee", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PTKZU", "VVINF", "$.", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um sich aus ihrer Noth herauszuwinden,", "tokens": ["Um", "sich", "aus", "ih\u00b7rer", "Noth", "her\u00b7aus\u00b7zu\u00b7win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Lie\u00df sie geduldig sich von ihrem Wuch'rer schinden,", "tokens": ["Lie\u00df", "sie", "ge\u00b7dul\u00b7dig", "sich", "von", "ih\u00b7rem", "Wuch'\u00b7rer", "schin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gab ihm den Balg, und f\u00fctterte", "tokens": ["Gab", "ihm", "den", "Balg", ",", "und", "f\u00fct\u00b7ter\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu Haus die Jungen satt. Der Winter kam heran", "tokens": ["Zu", "Haus", "die", "Jun\u00b7gen", "satt", ".", "Der", "Win\u00b7ter", "kam", "he\u00b7ran"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "ADJD", "$.", "ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und strenger Frost hielt sie zum Zahlen an.", "tokens": ["Und", "stren\u00b7ger", "Frost", "hielt", "sie", "zum", "Zah\u00b7len", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sie darbte k\u00fcmmerlich vom Munde", "tokens": ["Sie", "darb\u00b7te", "k\u00fcm\u00b7mer\u00b7lich", "vom", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sich jeden Bissen ab, und lief zur Stunde", "tokens": ["Sich", "je\u00b7den", "Bis\u00b7sen", "ab", ",", "und", "lief", "zur", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "PIAT", "NN", "PTKVZ", "$,", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Zum Gl\u00e4ubiger, ihr Pfand zu l\u00f6sen, hin.", "tokens": ["Zum", "Gl\u00e4u\u00b7bi\u00b7ger", ",", "ihr", "Pfand", "zu", "l\u00f6\u00b7sen", ",", "hin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Der Bock, mit Brillen auf der Nase,", "tokens": ["Der", "Bock", ",", "mit", "Bril\u00b7len", "auf", "der", "Na\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "APPR", "NN", 