{"dta.poem.18061": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "XvIII.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198462", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Fuchs lief ehmals \u00fcber Land,", "tokens": ["Ein", "Fuchs", "lief", "eh\u00b7mals", "\u00fc\u00b7ber", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als er ein Menschenbildni\u00df fand;", "tokens": ["Als", "er", "ein", "Men\u00b7schen\u00b7bild\u00b7ni\u00df", "fand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus Holz gehauen, kunstesvoll,", "tokens": ["Aus", "Holz", "ge\u00b7hau\u00b7en", ",", "kuns\u00b7tes\u00b7voll", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Haupt gezieret, als es soll,", "tokens": ["Sein", "Haupt", "ge\u00b7zie\u00b7ret", ",", "als", "es", "soll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "KOUS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Stirne sch\u00f6n, die Augen klar,", "tokens": ["Die", "Stir\u00b7ne", "sch\u00f6n", ",", "die", "Au\u00b7gen", "klar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Wangen roth, ein silbern Haar;", "tokens": ["Die", "Wan\u00b7gen", "roth", ",", "ein", "sil\u00b7bern", "Haar", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Mund war klein, die Z\u00e4hne wei\u00df;", "tokens": ["Der", "Mund", "war", "klein", ",", "die", "Z\u00e4h\u00b7ne", "wei\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es war gezieret auf den Preis.", "tokens": ["Es", "war", "ge\u00b7zie\u00b7ret", "auf", "den", "Preis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Als nun der Fuchs das Bild ersah,", "tokens": ["Als", "nun", "der", "Fuchs", "das", "Bild", "er\u00b7sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NE", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Erstaunt er sehr, und sagte da:", "tokens": ["Er\u00b7staunt", "er", "sehr", ",", "und", "sag\u00b7te", "da", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Potz Stern! welch Wunder mag das seyn?", "tokens": ["Potz", "Stern", "!", "welch", "Wun\u00b7der", "mag", "das", "seyn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAT", "NN", "VMFIN", "PDS", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ist das ein Mensch, was vor ein Schein!", "tokens": ["Ist", "das", "ein", "Mensch", ",", "was", "vor", "ein", "Schein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Indem er diese Worte sprach:", "tokens": ["In\u00b7dem", "er", "die\u00b7se", "Wor\u00b7te", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Gieng er hin, wo das Bildni\u00df lag.", "tokens": ["Gieng", "er", "hin", ",", "wo", "das", "Bild\u00b7ni\u00df", "lag", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Er kehrt es hin, er kehrt es her;", "tokens": ["Er", "kehrt", "es", "hin", ",", "er", "kehrt", "es", "her", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Betrachtets wohl, da mercket er,", "tokens": ["Be\u00b7trach\u00b7tets", "wohl", ",", "da", "mer\u00b7cket", "er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Da\u00df seine Augen ohne Licht;", "tokens": ["Da\u00df", "sei\u00b7ne", "Au\u00b7gen", "oh\u00b7ne", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Da\u00df es ein Mund ist, der nicht spricht;", "tokens": ["Da\u00df", "es", "ein", "Mund", "ist", ",", "der", "nicht", "spricht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,", "PRELS", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Die H\u00e4nde nie zu Wercke gehn,", "tokens": ["Die", "H\u00e4n\u00b7de", "nie", "zu", "Wer\u00b7cke", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Die F\u00fcsse immer stille stehn.", "tokens": ["Die", "F\u00fcs\u00b7se", "im\u00b7mer", "stil\u00b7le", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Der Fuchs ward dieses ungern innen,", "tokens": ["Der", "Fuchs", "ward", "die\u00b7ses", "un\u00b7gern", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "PDAT", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Er dacht in seinen Fuchses-Sinnen:", "tokens": ["Er", "dacht", "in", "sei\u00b7nen", "Fuch\u00b7ses\u00b7Sin\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Was soll das Auge sonder Licht?", "tokens": ["Was", "soll", "das", "Au\u00b7ge", "son\u00b7der", "Licht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Was soll der Mund, so er nicht spricht?", "tokens": ["Was", "soll", "der", "Mund", ",", "so", "er", "nicht", "spricht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "$,", "ADV", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Die H\u00e4nde, die zu Werck nicht gehn?", "tokens": ["Die", "H\u00e4n\u00b7de", ",", "die", "zu", "Werck", "nicht", "gehn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Die F\u00fcsse, die nur stille stehn?", "tokens": ["Die", "F\u00fcs\u00b7se", ",", "die", "nur", "stil\u00b7le", "stehn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Wer sie mit reichem Schmuck bekleidt,", "tokens": ["Wer", "sie", "mit", "rei\u00b7chem", "Schmuck", "be\u00b7kleidt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Der treibet grosse Ueppigkeit.", "tokens": ["Der", "trei\u00b7bet", "gros\u00b7se", "Uep\u00b7pig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}