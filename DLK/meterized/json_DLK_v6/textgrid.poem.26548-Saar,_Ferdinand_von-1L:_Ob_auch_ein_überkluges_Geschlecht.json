{"textgrid.poem.26548": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ob auch ein \u00fcberkluges Geschlecht", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob auch ein \u00fcberkluges Geschlecht", "tokens": ["Ob", "auch", "ein", "\u00fc\u00b7berk\u00b7lu\u00b7ges", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Dich bel\u00e4chelt als Unverstand;", "tokens": ["Dich", "be\u00b7l\u00e4\u00b7chelt", "als", "Un\u00b7ver\u00b7stand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Ob der banausische Schwarm,", "tokens": ["Ob", "der", "ba\u00b7nau\u00b7si\u00b7sche", "Schwarm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Der in den Tempel der Kunst sich dr\u00e4ngt,", "tokens": ["Der", "in", "den", "Tem\u00b7pel", "der", "Kunst", "sich", "dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Um bei des Altars heiliger Flamme", "tokens": ["Um", "bei", "des", "Al\u00b7tars", "hei\u00b7li\u00b7ger", "Flam\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Mahlzeit zu halten,", "tokens": ["Mahl\u00b7zeit", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Dir, weil du den Mann nicht n\u00e4hrst,", "tokens": ["Dir", ",", "weil", "du", "den", "Mann", "nicht", "n\u00e4hrst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "PPER", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Hochm\u00fcthig den R\u00fccken kehrt,", "tokens": ["Hoch\u00b7m\u00fct\u00b7hig", "den", "R\u00fc\u00b7cken", "kehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Inde\u00df ein Heer frecher St\u00fcmper", "tokens": ["In\u00b7de\u00df", "ein", "Heer", "fre\u00b7cher", "St\u00fcm\u00b7per"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Dich entweiht zu nichtigem Spiel:", "tokens": ["Dich", "ent\u00b7weiht", "zu", "nich\u00b7ti\u00b7gem", "Spiel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Immer und ewig", "tokens": ["Im\u00b7mer", "und", "e\u00b7wig"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KON", "ADJD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Bleibst du, hochaufstrebende Lyrik,", "tokens": ["Bleibst", "du", ",", "hoch\u00b7auf\u00b7stre\u00b7ben\u00b7de", "Ly\u00b7rik", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+---+", "measure": "unknown.measure.tetra"}, "line.13": {"text": "Bl\u00fcthe und Krone der Dichtkunst.", "tokens": ["Bl\u00fc\u00b7the", "und", "Kro\u00b7ne", "der", "Dicht\u00b7kunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Denn \u00fcberall sonst befehden sich Stoff und Form,", "tokens": ["Denn", "\u00fc\u00b7be\u00b7rall", "sonst", "be\u00b7feh\u00b7den", "sich", "Stoff", "und", "Form", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PRF", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und der Meister selbst,", "tokens": ["Und", "der", "Meis\u00b7ter", "selbst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Der den Zwiespalt zu l\u00f6sen scheint,", "tokens": ["Der", "den", "Zwies\u00b7palt", "zu", "l\u00f6\u00b7sen", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In tiefster Brust empfindet er", "tokens": ["In", "tiefs\u00b7ter", "Brust", "emp\u00b7fin\u00b7det", "er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vor dem beendeten Werk", "tokens": ["Vor", "dem", "be\u00b7en\u00b7de\u00b7ten", "Werk"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Vorwurfsvollen Mi\u00dfklang", "tokens": ["Vor\u00b7wurfs\u00b7vol\u00b7len", "Mi\u00df\u00b7klang"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Des Unbew\u00e4ltigten.", "tokens": ["Des", "Un\u00b7be\u00b7w\u00e4l\u00b7tig\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.8": {"text": "Du aber, athmend reinster Empfindung Hauch,", "tokens": ["Du", "a\u00b7ber", ",", "ath\u00b7mend", "reins\u00b7ter", "Emp\u00b7fin\u00b7dung", "Hauch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJD", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Folgst in sanften Rhythmen", "tokens": ["Folgst", "in", "sanf\u00b7ten", "Rhyth\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Willig dem Geist", "tokens": ["Wil\u00b7lig", "dem", "Geist"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Und lenkst ihn zuletzt,", "tokens": ["Und", "lenkst", "ihn", "zu\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Da du Worte hast f\u00fcr das Unsagbare,", "tokens": ["Da", "du", "Wor\u00b7te", "hast", "f\u00fcr", "das", "Un\u00b7sag\u00b7ba\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "Siegreich hinan zu ahnungsvollster Erkenntni\u00df.", "tokens": ["Sieg\u00b7reich", "hi\u00b7nan", "zu", "ah\u00b7nungs\u00b7volls\u00b7ter", "Er\u00b7kennt\u00b7ni\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Und wie du der Freude H\u00f6hen", "tokens": ["Und", "wie", "du", "der", "Freu\u00b7de", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ART", "NN", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.15": {"text": "Als leuchtendste Rose schm\u00fcckst,", "tokens": ["Als", "leuch\u00b7tends\u00b7te", "Ro\u00b7se", "schm\u00fcckst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Bl\u00fchst du auch, schwermuthsvoll,", "tokens": ["Bl\u00fchst", "du", "auch", ",", "schwer\u00b7muths\u00b7voll", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Als Passiflore hervor", "tokens": ["Als", "Pas\u00b7sif\u00b7lo\u00b7re", "her\u00b7vor"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NN", "PTKVZ"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.18": {"text": "Aus den Abgr\u00fcnden des Lebens.", "tokens": ["Aus", "den", "Ab\u00b7gr\u00fcn\u00b7den", "des", "Le\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}}}}}