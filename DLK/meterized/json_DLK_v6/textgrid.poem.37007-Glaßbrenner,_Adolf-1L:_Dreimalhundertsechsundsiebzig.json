{"textgrid.poem.37007": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dreimalhundertsechsundsiebzig", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dreimalhundertsechsundsiebzig", "tokens": ["Drei\u00b7mal\u00b7hun\u00b7dert\u00b7sech\u00b7sund\u00b7sieb\u00b7zig"], "token_info": ["word"], "pos": ["CARD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Scudi ist mir dieser Raugraf,", "tokens": ["Scu\u00b7di", "ist", "mir", "die\u00b7ser", "Raug\u00b7raf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PDAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Der im Leben f\u00fchrt' ein gro\u00dfes", "tokens": ["Der", "im", "Le\u00b7ben", "f\u00fchrt'", "ein", "gro\u00b7\u00dfes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Haus und itzo ruht im kleinen,", "tokens": ["Haus", "und", "it\u00b7zo", "ruht", "im", "klei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "VVFIN", "APPRART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mir f\u00fcr meine Tischlerarbeit", "tokens": ["Mir", "f\u00fcr", "mei\u00b7ne", "Tischler\u00b7ar\u00b7beit"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Annoch schuldig! Aber treff' ich", "tokens": ["An\u00b7noch", "schul\u00b7dig", "!", "A\u00b7ber", "treff'", "ich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$.", "KON", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Jenseits einst mit ihm zusammen,", "tokens": ["Jen\u00b7seits", "einst", "mit", "ihm", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Soll Gott ", "tokens": ["Soll", "Gott"], "token_info": ["word", "word"], "pos": ["VMFIN", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Will ich diesen fashionablen", "tokens": ["Will", "ich", "die\u00b7sen", "fa\u00b7shi\u00b7o\u00b7nab\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PDAT", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Raugraf ", "tokens": ["Raug\u00b7raf"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Will ", "tokens": ["Will"], "token_info": ["word"], "pos": ["VMFIN"], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Herr auf Schmalen, Kahlen, Prahlen,", "tokens": ["Herr", "auf", "Schma\u00b7len", ",", "Kah\u00b7len", ",", "Prah\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Mit dem allerdicksten Bambus", "tokens": ["Mit", "dem", "al\u00b7ler\u00b7dicks\u00b7ten", "Bam\u00b7bus"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}