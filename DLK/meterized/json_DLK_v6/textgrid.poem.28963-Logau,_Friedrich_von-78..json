{"textgrid.poem.28963": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "78.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im achten Monden bracht ein Kind Sirona, und die Leute zehlen;", "tokens": ["Im", "ach\u00b7ten", "Mon\u00b7den", "bracht", "ein", "Kind", "Si\u00b7ro\u00b7na", ",", "und", "die", "Leu\u00b7te", "zeh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "NE", "$,", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Weil Buch sie selbst gehalten hat, so frag auch sie! ihr wird nichts fehlen.", "tokens": ["Weil", "Buch", "sie", "selbst", "ge\u00b7hal\u00b7ten", "hat", ",", "so", "frag", "auch", "sie", "!", "ihr", "wird", "nichts", "feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ADV", "VVPP", "VAFIN", "$,", "ADV", "VVFIN", "ADV", "PPER", "$.", "PPER", "VAFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}}}}