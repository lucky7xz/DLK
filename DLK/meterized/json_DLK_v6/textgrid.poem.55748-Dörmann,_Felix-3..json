{"textgrid.poem.55748": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O la\u00df bei Dir mich wohnen,", "tokens": ["O", "la\u00df", "bei", "Dir", "mich", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bei Dir mich immer sein,", "tokens": ["Bei", "Dir", "mich", "im\u00b7mer", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "ADV", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erl\u00f6sung kann mir werden", "tokens": ["Er\u00b7l\u00f6\u00b7sung", "kann", "mir", "wer\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bei Dir, bei Dir allein.", "tokens": ["Bei", "Dir", ",", "bei", "Dir", "al\u00b7lein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Denn nur bei Dir ist Frieden", "tokens": ["Denn", "nur", "bei", "Dir", "ist", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPER", "VAFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und stilles, tiefes Gl\u00fcck,", "tokens": ["Und", "stil\u00b7les", ",", "tie\u00b7fes", "Gl\u00fcck", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O sei nicht grausam, sto\u00df' mich", "tokens": ["O", "sei", "nicht", "grau\u00b7sam", ",", "sto\u00df'", "mich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VAFIN", "PTKNEG", "ADJD", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht in die Nacht zur\u00fcck.", "tokens": ["Nicht", "in", "die", "Nacht", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}