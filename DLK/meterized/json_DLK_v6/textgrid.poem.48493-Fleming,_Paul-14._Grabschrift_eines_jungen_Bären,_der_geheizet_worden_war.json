{"textgrid.poem.48493": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "14. Grabschrift eines jungen B\u00e4ren, der geheizet worden war", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich, der ich klein und jung von meiner Mutter kam,", "tokens": ["Ich", ",", "der", "ich", "klein", "und", "jung", "von", "mei\u00b7ner", "Mut\u00b7ter", "kam", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADJD", "KON", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "von welcher mich die Macht der strengen Bauren nahm,", "tokens": ["von", "wel\u00b7cher", "mich", "die", "Macht", "der", "stren\u00b7gen", "Bau\u00b7ren", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ward in der Stadt verkauft, daselbsten mich zu \u00fcben", "tokens": ["ward", "in", "der", "Stadt", "ver\u00b7kauft", ",", "da\u00b7selbs\u00b7ten", "mich", "zu", "\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$,", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "was in der Dienstbarkeit f\u00fcr Freiheit wird getrieben.", "tokens": ["was", "in", "der", "Dienst\u00b7bar\u00b7keit", "f\u00fcr", "Frei\u00b7heit", "wird", "ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "F\u00fcr wilde ward ich zahm, begriffe manche Kunst,", "tokens": ["F\u00fcr", "wil\u00b7de", "ward", "ich", "zahm", ",", "be\u00b7grif\u00b7fe", "man\u00b7che", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VAFIN", "PPER", "VVFIN", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "doch t\u00e4te mir die Welt darf\u00fcr gar kleine Gunst.", "tokens": ["doch", "t\u00e4\u00b7te", "mir", "die", "Welt", "dar\u00b7f\u00fcr", "gar", "klei\u00b7ne", "Gunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PAV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich wei\u00df von keiner Schuld, als da\u00df ich allzuk\u00fchner", "tokens": ["Ich", "wei\u00df", "von", "kei\u00b7ner", "Schuld", ",", "als", "da\u00df", "ich", "all\u00b7zu\u00b7k\u00fch\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,", "KOKOM", "KOUS", "PPER", "PIS"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "erhascht hab' und verzehrt so manche sch\u00f6ne H\u00fcner.", "tokens": ["er\u00b7hascht", "hab'", "und", "ver\u00b7zehrt", "so", "man\u00b7che", "sch\u00f6\u00b7ne", "H\u00fc\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "KON", "VVFIN", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mein ganzes Leben war ein steter langer Tanz.", "tokens": ["Mein", "gan\u00b7zes", "Le\u00b7ben", "war", "ein", "ste\u00b7ter", "lan\u00b7ger", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Zuletzte kriegt' ich noch darvon den M\u00e4rtrer-Kranz.", "tokens": ["Zu\u00b7letz\u00b7te", "kriegt'", "ich", "noch", "dar\u00b7von", "den", "M\u00e4r\u00b7trer\u00b7Kranz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PAV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Soll euch nicht sein, wie mir, ihr Br\u00fcder und ihr Schwestern", "tokens": ["Soll", "euch", "nicht", "sein", ",", "wie", "mir", ",", "ihr", "Br\u00fc\u00b7der", "und", "ihr", "Schwes\u00b7tern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "VAINF", "$,", "PWAV", "PPER", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "so bleibet, wie ihr solt, in euren wilden Nestern!", "tokens": ["so", "blei\u00b7bet", ",", "wie", "ihr", "solt", ",", "in", "eu\u00b7ren", "wil\u00b7den", "Nes\u00b7tern", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "PPER", "VMFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}