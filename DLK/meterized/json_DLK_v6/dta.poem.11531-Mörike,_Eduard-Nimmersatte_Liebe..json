{"dta.poem.11531": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Nimmersatte Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "So ist die Lieb! So ist die Lieb!", "tokens": ["So", "ist", "die", "Lieb", "!", "So", "ist", "die", "Lieb", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$.", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit K\u00fcssen nicht zu stillen;", "tokens": ["Mit", "K\u00fcs\u00b7sen", "nicht", "zu", "stil\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer ist der Thor und will ein Sieb", "tokens": ["Wer", "ist", "der", "Thor", "und", "will", "ein", "Sieb"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "KON", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit eitel Wasser f\u00fcllen?", "tokens": ["Mit", "ei\u00b7tel", "Was\u00b7ser", "f\u00fcl\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und sch\u00f6pf'st du an die tausend Jahr,", "tokens": ["Und", "sch\u00f6pf'st", "du", "an", "die", "tau\u00b7send", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und k\u00fcssest ewig, ewig gar,", "tokens": ["Und", "k\u00fcs\u00b7sest", "e\u00b7wig", ",", "e\u00b7wig", "gar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du thust ihr nie zu Willen.", "tokens": ["Du", "thust", "ihr", "nie", "zu", "Wil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Lieb, die Lieb hat alle Stund", "tokens": ["Die", "Lieb", ",", "die", "Lieb", "hat", "al\u00b7le", "Stund"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Neu wunderlich Gel\u00fcsten,", "tokens": ["Neu", "wun\u00b7der\u00b7lich", "Ge\u00b7l\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "NN", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir bissen uns die Lippen wund,", "tokens": ["Wir", "bis\u00b7sen", "uns", "die", "Lip\u00b7pen", "wund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da wir uns heute k\u00fc\u00dften.", "tokens": ["Da", "wir", "uns", "heu\u00b7te", "k\u00fc\u00df\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das M\u00e4dchen hielt in guter Ruh,", "tokens": ["Das", "M\u00e4d\u00b7chen", "hielt", "in", "gu\u00b7ter", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie's L\u00e4mmlein unter'm Messer;", "tokens": ["Wie's", "L\u00e4mm\u00b7lein", "un\u00b7ter'm", "Mes\u00b7ser", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ihr Auge bat: nur immer zu,", "tokens": ["Ihr", "Au\u00b7ge", "bat", ":", "nur", "im\u00b7mer", "zu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Je weher, desto besser!", "tokens": ["Je", "we\u00b7her", ",", "des\u00b7to", "bes\u00b7ser", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "So ist die Lieb, und war auch so,", "tokens": ["So", "ist", "die", "Lieb", ",", "und", "war", "auch", "so", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "KON", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie lang es Liebe gibt,", "tokens": ["Wie", "lang", "es", "Lie\u00b7be", "gibt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und anders war Herr Salomo,", "tokens": ["Und", "an\u00b7ders", "war", "Herr", "Sa\u00b7lo\u00b7mo", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Weise, nicht verliebt.", "tokens": ["Der", "Wei\u00b7se", ",", "nicht", "ver\u00b7liebt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}