{"textgrid.poem.66745": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Solling, auf sonnigen Waldesh\u00f6hn", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Solling, auf sonnigen Waldesh\u00f6hn", "tokens": ["Im", "Sol\u00b7ling", ",", "auf", "son\u00b7ni\u00b7gen", "Wal\u00b7des\u00b7h\u00f6hn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Altheimischer Weserlande,", "tokens": ["Al\u00b7thei\u00b7mi\u00b7scher", "We\u00b7ser\u00b7lan\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hinschlenderten wir \u2013 der Tag war sch\u00f6n \u2013", "tokens": ["Hin\u00b7schlen\u00b7der\u00b7ten", "wir", "\u2013", "der", "Tag", "war", "sch\u00f6n", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "ART", "NN", "VAFIN", "ADJD", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "An reifender Felder Rande.", "tokens": ["An", "rei\u00b7fen\u00b7der", "Fel\u00b7der", "Ran\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Du stecktest Gerste, Roggen und Korn,", "tokens": ["Du", "steck\u00b7test", "Gers\u00b7te", ",", "Rog\u00b7gen", "und", "Korn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Drei volle, wiegende \u00c4hren,", "tokens": ["Drei", "vol\u00b7le", ",", "wie\u00b7gen\u00b7de", "\u00c4h\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zu roten Raden und Rittersporn \u2013", "tokens": ["Zu", "ro\u00b7ten", "Ra\u00b7den", "und", "Rit\u00b7ter\u00b7sporn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wir schwelgten in l\u00e4ndlichen Sph\u00e4ren.", "tokens": ["Wir", "schwelg\u00b7ten", "in", "l\u00e4nd\u00b7li\u00b7chen", "Sph\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Da, wie wir so streiften den Rain entlang \u2013", "tokens": ["Da", ",", "wie", "wir", "so", "streif\u00b7ten", "den", "Rain", "ent\u00b7lang", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "ADV", "VVFIN", "ART", "NN", "APPO", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Lerchen stiegen im Blauen", "tokens": ["Die", "Ler\u00b7chen", "stie\u00b7gen", "im", "Blau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und sangen dem Sommer den Jubelgesang \u2013", "tokens": ["Und", "san\u00b7gen", "dem", "Som\u00b7mer", "den", "Ju\u00b7bel\u00b7ge\u00b7sang", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sah ich scharf zu Boden dich schauen.", "tokens": ["Sah", "ich", "scharf", "zu", "Bo\u00b7den", "dich", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ein altes Hufeisen lag bestaubt", "tokens": ["Ein", "al\u00b7tes", "Huf\u00b7ei\u00b7sen", "lag", "be\u00b7staubt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "VVPP"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zur Rechten im Fahrgeleise ...", "tokens": ["Zur", "Rech\u00b7ten", "im", "Fahr\u00b7ge\u00b7lei\u00b7se", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du nahmst es \u2013 wir haben gleich dran geglaubt \u2013", "tokens": ["Du", "nahmst", "es", "\u2013", "wir", "ha\u00b7ben", "gleich", "dran", "ge\u00b7glaubt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "PPER", "VAFIN", "ADV", "PAV", "VVPP", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ich trug's ... es ging mit auf die Reise.", "tokens": ["Ich", "trug's", "...", "es", "ging", "mit", "auf", "die", "Rei\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "APPR", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Im Koffer verpackt zwischen Prosa und Reim,", "tokens": ["Im", "Kof\u00b7fer", "ver\u00b7packt", "zwi\u00b7schen", "Pro\u00b7sa", "und", "Reim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Siebensachen, Andenken und Kragen,", "tokens": ["Sie\u00b7ben\u00b7sa\u00b7chen", ",", "An\u00b7den\u00b7ken", "und", "Kra\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Hat das Nageleisen ins neue Heim", "tokens": ["Hat", "das", "Na\u00b7ge\u00b7lei\u00b7sen", "ins", "neu\u00b7e", "Heim"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Uns hannoversche Erde getragen.", "tokens": ["Uns", "han\u00b7no\u00b7ver\u00b7sche", "Er\u00b7de", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Erde vielleicht von derselben Spur,", "tokens": ["Er\u00b7de", "viel\u00b7leicht", "von", "der\u00b7sel\u00b7ben", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Wo vor Zeiten zu Rosse nach Bremen", "tokens": ["Wo", "vor", "Zei\u00b7ten", "zu", "Ros\u00b7se", "nach", "Bre\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "APPR", "NN", "APPR", "NE"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Mein Vater geritten stromabw\u00e4rts die Tour,", "tokens": ["Mein", "Va\u00b7ter", "ge\u00b7rit\u00b7ten", "stro\u00b7mab\u00b7w\u00e4rts", "die", "Tour", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Gold f\u00fcr Getreide zu nehmen.", "tokens": ["Gold", "f\u00fcr", "Ge\u00b7trei\u00b7de", "zu", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.7": {"line.1": {"text": "Erde vom selben Pfade vielleicht,", "tokens": ["Er\u00b7de", "vom", "sel\u00b7ben", "Pfa\u00b7de", "viel\u00b7leicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "ADV", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wo im sommerlich bl\u00fchenden Schmucke", "tokens": ["Wo", "im", "som\u00b7mer\u00b7lich", "bl\u00fc\u00b7hen\u00b7den", "Schmu\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "ADJD", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Die Mutter heiter die Hand ihm gereicht", "tokens": ["Die", "Mut\u00b7ter", "hei\u00b7ter", "die", "Hand", "ihm", "ge\u00b7reicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "ART", "NN", "PPER", "VVPP"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zur R\u00fcckkehr mit liebendem Drucke ...", "tokens": ["Zur", "R\u00fcck\u00b7kehr", "mit", "lie\u00b7ben\u00b7dem", "Dru\u00b7cke", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Nun h\u00e4ngt uns das alte Hufeisen schlicht", "tokens": ["Nun", "h\u00e4ngt", "uns", "das", "al\u00b7te", "Huf\u00b7ei\u00b7sen", "schlicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "An dicker, geschmiedeter Kette,", "tokens": ["An", "di\u00b7cker", ",", "ge\u00b7schmie\u00b7de\u00b7ter", "Ket\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Umschlie\u00dft des Eingangs gl\u00fchendes Licht", "tokens": ["Um\u00b7schlie\u00dft", "des", "Ein\u00b7gangs", "gl\u00fc\u00b7hen\u00b7des", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und st\u00e4rkt und segnet die St\u00e4tte.", "tokens": ["Und", "st\u00e4rkt", "und", "seg\u00b7net", "die", "St\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Das soll mit seiner gebogenen Kraft", "tokens": ["Das", "soll", "mit", "sei\u00b7ner", "ge\u00b7bo\u00b7ge\u00b7nen", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Um die leuchtende Birne sich kr\u00fcmmen,", "tokens": ["Um", "die", "leuch\u00b7ten\u00b7de", "Bir\u00b7ne", "sich", "kr\u00fcm\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Neu Leben mit wurzelz\u00e4her Haft", "tokens": ["Neu", "Le\u00b7ben", "mit", "wur\u00b7zel\u00b7z\u00e4\u00b7her", "Haft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Soll zaubrisch drin gl\u00fchen und glimmen.", "tokens": ["Soll", "zaub\u00b7risch", "drin", "gl\u00fc\u00b7hen", "und", "glim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ADV", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.10": {"line.1": {"text": "Es k\u00fcnde den Freunden ein echtes Herein!", "tokens": ["Es", "k\u00fcn\u00b7de", "den", "Freun\u00b7den", "ein", "ech\u00b7tes", "Her\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Soll treu sich und wirksam erweisen,", "tokens": ["Soll", "treu", "sich", "und", "wirk\u00b7sam", "er\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PRF", "KON", "ADJD", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und mag es ein K\u00f6hlerglaube nur sein,", "tokens": ["Und", "mag", "es", "ein", "K\u00f6h\u00b7ler\u00b7glau\u00b7be", "nur", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ART", "NN", "ADV", "VAINF", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Uns ist es ein Glaube von Eisen.", "tokens": ["Uns", "ist", "es", "ein", "Glau\u00b7be", "von", "Ei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}