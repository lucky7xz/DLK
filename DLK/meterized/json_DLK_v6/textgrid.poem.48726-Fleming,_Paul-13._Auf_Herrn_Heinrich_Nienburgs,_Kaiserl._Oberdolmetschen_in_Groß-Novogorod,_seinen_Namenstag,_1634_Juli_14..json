{"textgrid.poem.48726": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "13. Auf Herrn Heinrich Nienburgs, Kaiserl. Oberdolmetschen in Gro\u00df-Novogorod, seinen Namenstag, 1634 Juli 14.", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zeuch hin, du schwaches Band, doch st\u00e4rker, als die Ketten,", "tokens": ["Zeuch", "hin", ",", "du", "schwa\u00b7ches", "Band", ",", "doch", "st\u00e4r\u00b7ker", ",", "als", "die", "Ket\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,", "ADV", "ADJD", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die Bronteus h\u00e4mret aus; kein Eisen gleicht dir nicht.", "tokens": ["die", "Bron\u00b7teus", "h\u00e4m\u00b7ret", "aus", ";", "kein", "Ei\u00b7sen", "gleicht", "dir", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "PIAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Erz verschlei\u00dft f\u00fcr dir, der schwache Stahl zerbricht.", "tokens": ["Das", "Erz", "ver\u00b7schlei\u00dft", "f\u00fcr", "dir", ",", "der", "schwa\u00b7che", "Stahl", "zer\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zeuch hin, bind' ihn, den Freund, da\u00df er sich nicht kan retten.", "tokens": ["Zeuch", "hin", ",", "bind'", "ihn", ",", "den", "Freund", ",", "da\u00df", "er", "sich", "nicht", "kan", "ret\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "VAFIN", "PPER", "$,", "ART", "NN", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Bind' ihn, den lieben Freund. Ach da\u00df wir Sachen h\u00e4tten,", "tokens": ["Bind'", "ihn", ",", "den", "lie\u00b7ben", "Freund", ".", "Ach", "da\u00df", "wir", "Sa\u00b7chen", "h\u00e4t\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ART", "ADJA", "NN", "$.", "ITJ", "KOUS", "PPER", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die dich mehr machten wert, da\u00df dieses sch\u00f6ne Licht,", "tokens": ["die", "dich", "mehr", "mach\u00b7ten", "wert", ",", "da\u00df", "die\u00b7ses", "sch\u00f6\u00b7ne", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "ADJD", "$,", "KOUS", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "der Tag recht w\u00fcrd erh\u00f6ht! Doch h\u00f6re, was er spricht.", "tokens": ["der", "Tag", "recht", "w\u00fcrd", "er\u00b7h\u00f6ht", "!", "Doch", "h\u00f6\u00b7re", ",", "was", "er", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "VVPP", "$.", "KON", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich wei\u00df, er nimmt verlieb, und solt ich auch drauf wetten.", "tokens": ["Ich", "wei\u00df", ",", "er", "nimmt", "ver\u00b7lieb", ",", "und", "solt", "ich", "auch", "drauf", "wet\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADJD", "$,", "KON", "VMFIN", "PPER", "ADV", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Vermeld' ihm di\u00df dabei, wenn du ihn feste hast,", "tokens": ["Ver\u00b7meld'", "ihm", "di\u00df", "da\u00b7bei", ",", "wenn", "du", "ihn", "fes\u00b7te", "hast", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDS", "PAV", "$,", "KOUS", "PPER", "PPER", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da\u00df, wenn es ja sein mu\u00df, und ich soll sein sein Gast,", "tokens": ["da\u00df", ",", "wenn", "es", "ja", "sein", "mu\u00df", ",", "und", "ich", "soll", "sein", "sein", "Gast", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "ADV", "VAINF", "VMFIN", "$,", "KON", "PPER", "VMFIN", "VAINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "das doch nicht n\u00f6tig ist, er nur nicht wolle meinen,", "tokens": ["das", "doch", "nicht", "n\u00f6\u00b7tig", "ist", ",", "er", "nur", "nicht", "wol\u00b7le", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PTKNEG", "ADJD", "VAFIN", "$,", "PPER", "ADV", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "die L\u00f6sung steh' auf Kost. Je besser er sich h\u00e4lt,", "tokens": ["die", "L\u00f6\u00b7sung", "steh'", "auf", "Kost", ".", "Je", "bes\u00b7ser", "er", "sich", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$.", "ADV", "ADJD", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "je fester bind' ihn denn. Wenn dieses ihm gef\u00e4llt,", "tokens": ["je", "fes\u00b7ter", "bind'", "ihn", "denn", ".", "Wenn", "die\u00b7ses", "ihm", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "ADV", "$.", "KOUS", "PDAT", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so bleibe, wo du bist, und denk' an mich, den deinen.", "tokens": ["so", "blei\u00b7be", ",", "wo", "du", "bist", ",", "und", "denk'", "an", "mich", ",", "den", "dei\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "PPER", "VAFIN", "$,", "KON", "VVFIN", "APPR", "PPER", "$,", "PRELS", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}