{"dta.poem.10025": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlings-Betrachtungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mich erquicken,", "tokens": ["Mich", "er\u00b7qui\u00b7cken", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Mich entz\u00fccken,", "tokens": ["Mich", "ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "In der holden Fr\u00fchlings-Zeit,", "tokens": ["In", "der", "hol\u00b7den", "Fr\u00fch\u00b7lings\u00b7Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle Dinge, die ich sehe.", "tokens": ["Al\u00b7le", "Din\u00b7ge", ",", "die", "ich", "se\u00b7he", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da ja, wo ich geh\u2019 und stehe:", "tokens": ["Da", "ja", ",", "wo", "ich", "geh'", "und", "ste\u00b7he", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Alles voller Lieblichkeit.", "tokens": ["Al\u00b7les", "vol\u00b7ler", "Lieb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Durch der gr\u00fcnen Erde Pracht,", "tokens": ["Durch", "der", "gr\u00fc\u00b7nen", "Er\u00b7de", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die Bluhmen, durch die Bl\u00fchte,", "tokens": ["Durch", "die", "Bluh\u00b7men", ",", "durch", "die", "Bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird, durchs Auge, mein Gem\u00fcthe", "tokens": ["Wird", ",", "durchs", "Au\u00b7ge", ",", "mein", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "$,", "APPRART", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mecht bezaubernd angelacht.", "tokens": ["Mecht", "be\u00b7zau\u00b7bernd", "an\u00b7ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Die gelinden lauen L\u00fcffte,", "tokens": ["Die", "ge\u00b7lin\u00b7den", "lau\u00b7en", "L\u00fcff\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Voller Balsam-reicher D\u00fcffte,", "tokens": ["Vol\u00b7ler", "Bal\u00b7sam\u00b7rei\u00b7cher", "D\u00fcff\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Treibt des holden Zephirs Spiel", "tokens": ["Treibt", "des", "hol\u00b7den", "Ze\u00b7phirs", "Spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zum Geruch und zum Gef\u00fchl.", "tokens": ["Zum", "Ge\u00b7ruch", "und", "zum", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Auf den glatten Wellen wallen,", "tokens": ["Auf", "den", "glat\u00b7ten", "Wel\u00b7len", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie auf gl\u00e4ntzenden Crystallen,", "tokens": ["Wie", "auf", "gl\u00e4nt\u00b7zen\u00b7den", "Crys\u00b7tal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jm best\u00e4ndig regen Licht,", "tokens": ["Jm", "be\u00b7st\u00e4n\u00b7dig", "re\u00b7gen", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tausend Strahlen, tausend Blitze,", "tokens": ["Tau\u00b7send", "Strah\u00b7len", ",", "tau\u00b7send", "Blit\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ergetzen das Gesicht:", "tokens": ["Und", "er\u00b7get\u00b7zen", "das", "Ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sonderlich wenn selbe, zwischen", "tokens": ["Son\u00b7der\u00b7lich", "wenn", "sel\u00b7be", ",", "zwi\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "KOUS", "ADJA", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Noch nicht dick bewachsnen B\u00fcschen,", "tokens": ["Noch", "nicht", "dick", "be\u00b7wachs\u00b7nen", "B\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und durch junge Weiden glimmen.", "tokens": ["Und", "durch", "jun\u00b7ge", "Wei\u00b7den", "glim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Kleine Lichter, welche schwimmen", "tokens": ["Klei\u00b7ne", "Lich\u00b7ter", ",", "wel\u00b7che", "schwim\u00b7men"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Auf dem Laub\u2019 und auf der Fluth,", "tokens": ["Auf", "dem", "Laub'", "und", "auf", "der", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Bald in wei\u00df-bald blauer Gluth,", "tokens": ["Bald", "in", "wei\u00df\u00b7bald", "blau\u00b7er", "Gluth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Treffen mit gef\u00e4rbtem Schertz", "tokens": ["Tref\u00b7fen", "mit", "ge\u00b7f\u00e4rb\u00b7tem", "Schertz"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Durch die Augen unser Hertz.", "tokens": ["Durch", "die", "Au\u00b7gen", "un\u00b7ser", "Hertz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Seht die leichten V\u00f6gel fliegen;", "tokens": ["Seht", "die", "leich\u00b7ten", "V\u00f6\u00b7gel", "flie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6ret, wie sie sich vergn\u00fcgen;", "tokens": ["H\u00f6\u00b7ret", ",", "wie", "sie", "sich", "ver\u00b7gn\u00fc\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seht, wie die bebl\u00fchmten Hecken", "tokens": ["Seht", ",", "wie", "die", "be\u00b7bl\u00fchm\u00b7ten", "He\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jhr geflochtnes Rest verstecken!", "tokens": ["Ihr", "ge\u00b7flocht\u00b7nes", "Rest", "ver\u00b7ste\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schlupffet dort nach seinem Reste", "tokens": ["Schlupf\u00b7fet", "dort", "nach", "sei\u00b7nem", "Res\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ein verliebt und emsigs Paar;", "tokens": ["Ein", "ver\u00b7liebt", "und", "em\u00b7sigs", "Paar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVPP", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "H\u00fcpffet hier durch Laub und Aeste", "tokens": ["H\u00fcpf\u00b7fet", "hier", "durch", "Laub", "und", "A\u00b7es\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Eine bunt-gef\u00e4rbte Schaar;", "tokens": ["Ei\u00b7ne", "bunt\u00b7ge\u00b7f\u00e4rb\u00b7te", "Schaar", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Seht, wie sie die K\u00f6pffchen drehn,", "tokens": ["Seht", ",", "wie", "sie", "die", "K\u00f6pffc\u00b7hen", "drehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Und des Fr\u00fchlings Pracht besehn;", "tokens": ["Und", "des", "Fr\u00fch\u00b7lings", "Pracht", "be\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "H\u00f6rt, wie gurgeln sie so sch\u00f6n!", "tokens": ["H\u00f6rt", ",", "wie", "gur\u00b7geln", "sie", "so", "sch\u00f6n", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "H\u00f6ret, wie sie musiciren:", "tokens": ["H\u00f6\u00b7ret", ",", "wie", "sie", "mu\u00b7si\u00b7ci\u00b7ren", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "La\u00df dich doch ihr Beyspiel r\u00fchren,", "tokens": ["La\u00df", "dich", "doch", "ihr", "Bey\u00b7spiel", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebster Mensch, la\u00df Dem zu Ehren,", "tokens": ["Liebs\u00b7ter", "Mensch", ",", "la\u00df", "Dem", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVIMP", "ART", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der die Welt so sch\u00f6n geschm\u00fcckt,", "tokens": ["Der", "die", "Welt", "so", "sch\u00f6n", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und, durch sie, dich fast entz\u00fcckt;", "tokens": ["Und", ",", "durch", "sie", ",", "dich", "fast", "ent\u00b7z\u00fcckt", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "PPER", "$,", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Auch ein frohes Danck-Lied h\u00f6ren.", "tokens": ["Auch", "ein", "fro\u00b7hes", "Dan\u00b7ck\u00b7Lied", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}