{"textgrid.poem.45138": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Die Kunst", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man h\u00f6rt vom Fortschritt der neuen Zeit,", "tokens": ["Man", "h\u00f6rt", "vom", "Fort\u00b7schritt", "der", "neu\u00b7en", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie ist auch vom alten Wege weit,", "tokens": ["Sie", "ist", "auch", "vom", "al\u00b7ten", "We\u00b7ge", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Doch wird es ab vom Verfolge hangen,", "tokens": ["Doch", "wird", "es", "ab", "vom", "Ver\u00b7fol\u00b7ge", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKVZ", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sonst w\u00e4r sie vielleicht nur seitw\u00e4rts gegangen.", "tokens": ["Sonst", "w\u00e4r", "sie", "viel\u00b7leicht", "nur", "seit\u00b7w\u00e4rts", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}