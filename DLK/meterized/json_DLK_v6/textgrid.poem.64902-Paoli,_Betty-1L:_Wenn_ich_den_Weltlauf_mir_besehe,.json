{"textgrid.poem.64902": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn ich den Weltlauf mir besehe,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich den Weltlauf mir besehe,", "tokens": ["Wenn", "ich", "den", "Welt\u00b7lauf", "mir", "be\u00b7se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Tag f\u00fcr Tag das H\u00f6h're weicht,", "tokens": ["Dem", "Tag", "f\u00fcr", "Tag", "das", "H\u00f6h'\u00b7re", "weicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da f\u00fchl' ich, wie ein bittres Wehe", "tokens": ["Da", "f\u00fchl'", "ich", ",", "wie", "ein", "bitt\u00b7res", "We\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich bis zu meinem Herzen schleicht;", "tokens": ["Sich", "bis", "zu", "mei\u00b7nem", "Her\u00b7zen", "schleicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da m\u00f6cht' ich hei\u00dfe Thr\u00e4nen weinen", "tokens": ["Da", "m\u00f6cht'", "ich", "hei\u00b7\u00dfe", "Thr\u00e4\u00b7nen", "wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In diesen See von Schmerz und Blut,", "tokens": ["In", "die\u00b7sen", "See", "von", "Schmerz", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da m\u00f6chte ich die Erde reinen", "tokens": ["Da", "m\u00f6ch\u00b7te", "ich", "die", "Er\u00b7de", "rei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit meines F\u00fchlens Flammenglut;", "tokens": ["Mit", "mei\u00b7nes", "F\u00fch\u00b7lens", "Flam\u00b7men\u00b7glut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da m\u00f6cht' ich auf den Gr\u00e4bern beten", "tokens": ["Da", "m\u00f6cht'", "ich", "auf", "den", "Gr\u00e4\u00b7bern", "be\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Gro\u00dfen, die in vor'ger Zeit", "tokens": ["Der", "Gro\u00b7\u00dfen", ",", "die", "in", "vor'\u00b7ger", "Zeit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Recht der Menschheit k\u00fchn vertreten", "tokens": ["Das", "Recht", "der", "Menschheit", "k\u00fchn", "ver\u00b7tre\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und ihrem Dienst sich ganz geweiht.", "tokens": ["Und", "ih\u00b7rem", "Dienst", "sich", "ganz", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und jammernd m\u00f6chte ich sie fragen", "tokens": ["Und", "jam\u00b7mernd", "m\u00f6ch\u00b7te", "ich", "sie", "fra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die M\u00e4rtyrer im Grabe dort:", "tokens": ["Die", "M\u00e4r\u00b7ty\u00b7rer", "im", "Gra\u00b7be", "dort", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wof\u00fcr habt ihr gek\u00e4mpft, ertragen,", "tokens": ["Wo\u00b7f\u00fcr", "habt", "ihr", "ge\u00b7k\u00e4mpft", ",", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVPP", "$,", "VVINF", "$,"], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wenn schon verschollen euer Wort?", "tokens": ["Wenn", "schon", "ver\u00b7schol\u00b7len", "eu\u00b7er", "Wort", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was ihr der Welt an Gl\u00fcck erworben,", "tokens": ["Was", "ihr", "der", "Welt", "an", "Gl\u00fcck", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sagt euch des Weltenschmerzes Chor! \u2013", "tokens": ["Sagt", "euch", "des", "Wel\u00b7ten\u00b7schmer\u00b7zes", "Chor", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wof\u00fcr, wof\u00fcr seid ihr gestorben,", "tokens": ["Wo\u00b7f\u00fcr", ",", "wo\u00b7f\u00fcr", "seid", "ihr", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wenn Alles schlecht blieb, wie zuvor?", "tokens": ["Wenn", "Al\u00b7les", "schlecht", "blieb", ",", "wie", "zu\u00b7vor", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVFIN", "$,", "PWAV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}