{"dta.poem.16173": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "227.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du meine Mutter nicht, doch, Erde, meine Amme,", "tokens": ["Du", "mei\u00b7ne", "Mut\u00b7ter", "nicht", ",", "doch", ",", "Er\u00b7de", ",", "mei\u00b7ne", "Am\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKNEG", "$,", "ADV", "$,", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von deren Milch gen\u00e4hrt bl\u00fcht meine Geistesflamme!", "tokens": ["Von", "de\u00b7ren", "Milch", "ge\u00b7n\u00e4hrt", "bl\u00fcht", "mei\u00b7ne", "Geis\u00b7tes\u00b7flam\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "VVPP", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du hast zur Freude mir dich immer bunt geschm\u00fcckt,", "tokens": ["Du", "hast", "zur", "Freu\u00b7de", "mir", "dich", "im\u00b7mer", "bunt", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "PPER", "PRF", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und unter Blumen mich am Busen festgedr\u00fcckt.", "tokens": ["Und", "un\u00b7ter", "Blu\u00b7men", "mich", "am", "Bu\u00b7sen", "fest\u00b7ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "In deinem Bande lernt' ich stehn und gehn, mich wiegen", "tokens": ["In", "dei\u00b7nem", "Ban\u00b7de", "lernt'", "ich", "stehn", "und", "gehn", ",", "mich", "wie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "VVINF", "KON", "VVINF", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Traum der Lust, und nun lernt' ich dir zu entfliegen.", "tokens": ["Im", "Traum", "der", "Lust", ",", "und", "nun", "lernt'", "ich", "dir", "zu", "ent\u00b7flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "KON", "ADV", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Leb wohl! vom Segen sei des Himmels \u00fcberthaut,", "tokens": ["Leb", "wohl", "!", "vom", "Se\u00b7gen", "sei", "des", "Him\u00b7mels", "\u00fc\u00b7bert\u00b7haut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$.", "APPRART", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der zur Erziehung mich solang dir anvertraut.", "tokens": ["Der", "zur", "Er\u00b7zie\u00b7hung", "mich", "so\u00b7lang", "dir", "an\u00b7ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PPER", "VMFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Dort nach dem weiten Haus des Vaters geht mein Lauf,", "tokens": ["Dort", "nach", "dem", "wei\u00b7ten", "Haus", "des", "Va\u00b7ters", "geht", "mein", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Mutter such' ich dort, die unbekannte, auf,", "tokens": ["Die", "Mut\u00b7ter", "such'", "ich", "dort", ",", "die", "un\u00b7be\u00b7kann\u00b7te", ",", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "ART", "ADJA", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die hohe, die sich mir im Traum nicht hat verhehlt,", "tokens": ["Die", "ho\u00b7he", ",", "die", "sich", "mir", "im", "Traum", "nicht", "hat", "ver\u00b7hehlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PRF", "PPER", "APPRART", "NN", "PTKNEG", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Ammenm\u00e4rchen hast du mir von ihr erz\u00e4hlt.", "tokens": ["Und", "Am\u00b7men\u00b7m\u00e4r\u00b7chen", "hast", "du", "mir", "von", "ihr", "er\u00b7z\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}