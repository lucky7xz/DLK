{"textgrid.poem.36687": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Thurm der Einsamkeit, den ich lieb', o festes", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Thurm der Einsamkeit, den ich lieb', o festes", "tokens": ["Thurm", "der", "Ein\u00b7sam\u00b7keit", ",", "den", "ich", "lieb'", ",", "o", "fes\u00b7tes"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "FM", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Uralt rundes R\u00f6mergebild, du Seufzer", "tokens": ["Ur\u00b7alt", "run\u00b7des", "R\u00f6\u00b7mer\u00b7ge\u00b7bild", ",", "du", "Seuf\u00b7zer"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADJA", "NN", "$,", "PPER", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Byrons, der Campagna ger\u00fchmt als", "tokens": ["By\u00b7rons", ",", "der", "Cam\u00b7pag\u00b7na", "ge\u00b7r\u00fchmt", "als"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "VVPP", "KOKOM"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Capo di Bove!", "tokens": ["Ca\u00b7po", "di", "Bo\u00b7ve", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Dich lobpreisend singet ein Lied der Dichter,", "tokens": ["Dich", "lob\u00b7prei\u00b7send", "sin\u00b7get", "ein", "Lied", "der", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Gern an Gr\u00e4bern weilend, weil seine Lieben", "tokens": ["Gern", "an", "Gr\u00e4\u00b7bern", "wei\u00b7lend", ",", "weil", "sei\u00b7ne", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VVPP", "$,", "KOUS", "PPOSAT", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "All' im Grabe schlummern f\u00fcr ihn, und selbst sein", "tokens": ["All'", "im", "Gra\u00b7be", "schlum\u00b7mern", "f\u00fcr", "ihn", ",", "und", "selbst", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "APPRART", "NN", "VVFIN", "APPR", "PPER", "$,", "KON", "ADV", "PPOSAT"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Glaub' an die Lieben.", "tokens": ["Glaub'", "an", "die", "Lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Aber wie erreicht dich Gesang? Ein Wort ist", "tokens": ["A\u00b7ber", "wie", "er\u00b7reicht", "dich", "Ge\u00b7sang", "?", "Ein", "Wort", "ist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "VVPP", "PPER", "NN", "$.", "ART", "NN", "VAFIN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Wenig f\u00fcr den Tod, und der Mensch zerst\u00f6rt nur,", "tokens": ["We\u00b7nig", "f\u00fcr", "den", "Tod", ",", "und", "der", "Mensch", "zer\u00b7st\u00f6rt", "nur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "KON", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Aber baut die Vorwelt nicht auf. Doch ist der", "tokens": ["A\u00b7ber", "baut", "die", "Vor\u00b7welt", "nicht", "auf", ".", "Doch", "ist", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "PTKVZ", "$.", "KON", "VAFIN", "ART"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Dichter ihr Echo.", "tokens": ["Dich\u00b7ter", "ihr", "E\u00b7cho", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Gleich der Windharf' ist er, die hoch in alten", "tokens": ["Gleich", "der", "Wind\u00b7ha\u00b7rf'", "ist", "er", ",", "die", "hoch", "in", "al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "PPER", "$,", "PRELS", "ADJD", "APPR", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Moosbewachsnen Th\u00fcrmen das Spiel der L\u00fcfte", "tokens": ["Moos\u00b7be\u00b7wachs\u00b7nen", "Th\u00fcr\u00b7men", "das", "Spiel", "der", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Wechselnd regt, und selig verrauscht in holden", "tokens": ["Wech\u00b7selnd", "regt", ",", "und", "se\u00b7lig", "ver\u00b7rauscht", "in", "hol\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "KON", "ADJD", "VVFIN", "APPR", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Str\u00f6men von Wohllaut.", "tokens": ["Str\u00f6\u00b7men", "von", "Wohl\u00b7laut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "So, mein ewig Trauergew\u00f6lb', bewegst auch", "tokens": ["So", ",", "mein", "e\u00b7wig", "Trau\u00b7er\u00b7ge\u00b7w\u00f6lb'", ",", "be\u00b7wegst", "auch"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "PPOSAT", "ADJD", "NN", "$,", "VVFIN", "ADV"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du die Seele mir, wenn ich dein nur denke,", "tokens": ["Du", "die", "See\u00b7le", "mir", ",", "wenn", "ich", "dein", "nur", "den\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PPER", "$,", "KOUS", "PPER", "PPOSAT", "ADV", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wie dein graues Rund so erhaben einsam", "tokens": ["Wie", "dein", "grau\u00b7es", "Rund", "so", "er\u00b7ha\u00b7ben", "ein\u00b7sam"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "ADJD"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Aus den Geb\u00fcschen.", "tokens": ["Aus", "den", "Ge\u00b7b\u00fc\u00b7schen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "++-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Weit in menschenleere Campagna hinblickt,", "tokens": ["Weit", "in", "men\u00b7schen\u00b7lee\u00b7re", "Cam\u00b7pag\u00b7na", "hin\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sichtbar schon aus luftiger Ferne, kr\u00f6nend", "tokens": ["Sicht\u00b7bar", "schon", "aus", "luf\u00b7ti\u00b7ger", "Fer\u00b7ne", ",", "kr\u00f6\u00b7nend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "ADV", "APPR", "ADJA", "NN", "$,", "VVPP"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Deinen H\u00fcgel, wie mit des Schattengottes", "tokens": ["Dei\u00b7nen", "H\u00fc\u00b7gel", ",", "wie", "mit", "des", "Schat\u00b7ten\u00b7got\u00b7tes"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "+-+-++-+-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "M\u00e4chtiger Krone.", "tokens": ["M\u00e4ch\u00b7ti\u00b7ger", "Kro\u00b7ne", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Denn vor dir, o K\u00f6nig der \u00f6den Wildni\u00df,", "tokens": ["Denn", "vor", "dir", ",", "o", "K\u00f6\u00b7nig", "der", "\u00f6\u00b7den", "Wild\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "FM", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Neigen tief die Nachbarn sich, der Cypressen", "tokens": ["Nei\u00b7gen", "tief", "die", "Nach\u00b7barn", "sich", ",", "der", "Cyp\u00b7res\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADJD", "ART", "NN", "PRF", "$,", "ART", "NN"], "meter": "+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.3": {"text": "Melancholisch Heldengew\u00e4chs, gesellt der", "tokens": ["Me\u00b7lan\u00b7cho\u00b7lisch", "Hel\u00b7den\u00b7ge\u00b7w\u00e4chs", ",", "ge\u00b7sellt", "der"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "VVPP", "ART"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Schweigenden Trauer,", "tokens": ["Schwei\u00b7gen\u00b7den", "Trau\u00b7er", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Neigt sich halb versch\u00fcttet Gem\u00e4u'r, durchbrochner", "tokens": ["Neigt", "sich", "halb", "ver\u00b7sch\u00fct\u00b7tet", "Ge\u00b7m\u00e4u'r", ",", "durch\u00b7broch\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PRF", "ADJD", "VVPP", "NN", "$,", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Th\u00fcrme Wand, verwitterte Reste langer", "tokens": ["Th\u00fcr\u00b7me", "Wand", ",", "ver\u00b7wit\u00b7ter\u00b7te", "Res\u00b7te", "lan\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Blut'ger K\u00e4mpfe, die der Colonna stolzes", "tokens": ["Blut'\u00b7ger", "K\u00e4mp\u00b7fe", ",", "die", "der", "Co\u00b7lon\u00b7na", "stol\u00b7zes"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "ART", "NE", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Haus mit dem Stuhle", "tokens": ["Haus", "mit", "dem", "Stuh\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Petri einst in kr\u00e4ftigen Ritterzeiten", "tokens": ["Pe\u00b7tri", "einst", "in", "kr\u00e4f\u00b7ti\u00b7gen", "Rit\u00b7ter\u00b7zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Durchfocht. Solche Nachbarn in hoher Ruhe", "tokens": ["Durch\u00b7focht", ".", "Sol\u00b7che", "Nach\u00b7barn", "in", "ho\u00b7her", "Ru\u00b7he"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PIAT", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Ueberschau'st du: selbst in die tempelvollen,", "tokens": ["Ue\u00b7ber\u00b7schau'st", "du", ":", "selbst", "in", "die", "tem\u00b7pel\u00b7vol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Un\u00fcbersehbar'n", "tokens": ["Un\u00b7\u00fcber\u00b7seh\u00b7ba\u00b7r'n"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "R\u00f6merebnen blickst du hinaus, die Rennbahn", "tokens": ["R\u00f6\u00b7me\u00b7reb\u00b7nen", "blickst", "du", "hin\u00b7aus", ",", "die", "Renn\u00b7bahn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Legte Caracalla zu deinen F\u00fc\u00dfen,", "tokens": ["Leg\u00b7te", "Ca\u00b7ra\u00b7cal\u00b7la", "zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "+----+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Und in jenem H\u00fcgel verbirgt die schatt'ge", "tokens": ["Und", "in", "je\u00b7nem", "H\u00fc\u00b7gel", "ver\u00b7birgt", "die", "schatt'\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "NN", "VVFIN", "ART", "ADJA"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Grotte des Numa", "tokens": ["Grot\u00b7te", "des", "Nu\u00b7ma"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "Dir die heil'ge Quelle, bei deren K\u00fchlung", "tokens": ["Dir", "die", "heil'\u00b7ge", "Quel\u00b7le", ",", "bei", "de\u00b7ren", "K\u00fch\u00b7lung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ART", "ADJA", "NN", "$,", "APPR", "PRELAT", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Er in stillem Umgang mit einer Gottheit", "tokens": ["Er", "in", "stil\u00b7lem", "Um\u00b7gang", "mit", "ei\u00b7ner", "Got\u00b7theit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Einst der siebenh\u00fcglichen Roma gro\u00dfe", "tokens": ["Einst", "der", "sie\u00b7ben\u00b7h\u00fcg\u00b7li\u00b7chen", "Ro\u00b7ma", "gro\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Zukunft berathen.", "tokens": ["Zu\u00b7kunft", "be\u00b7ra\u00b7then", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "Und sie selbst, der ird'sche Olympus, l\u00e4chelt", "tokens": ["Und", "sie", "selbst", ",", "der", "ird'\u00b7sche", "O\u00b7lym\u00b7pus", ",", "l\u00e4\u00b7chelt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "ADV", "$,", "ART", "ADJA", "NE", "$,", "VVFIN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dir entgegen, gl\u00e4nzend im Lichte der Sonne,", "tokens": ["Dir", "ent\u00b7ge\u00b7gen", ",", "gl\u00e4n\u00b7zend", "im", "Lich\u00b7te", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Dort vom paradiesischen immergr\u00fcnen", "tokens": ["Dort", "vom", "pa\u00b7ra\u00b7die\u00b7si\u00b7schen", "im\u00b7mer\u00b7gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "H\u00fcgel des Janus,", "tokens": ["H\u00fc\u00b7gel", "des", "Ja\u00b7nus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.13": {"line.1": {"text": "Mit St. Petri Kuppel, die eines Erdballs", "tokens": ["Mit", "St.", "Pe\u00b7tri", "Kup\u00b7pel", ",", "die", "ei\u00b7nes", "Erd\u00b7balls"], "token_info": ["word", "abbreviation", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "NE", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schattenbild vergleichbar, im Himmel dunkelt,", "tokens": ["Schat\u00b7ten\u00b7bild", "ver\u00b7gleich\u00b7bar", ",", "im", "Him\u00b7mel", "dun\u00b7kelt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Hingestreckt die Berge, von allem Sch\u00f6nen", "tokens": ["Hin\u00b7ge\u00b7streckt", "die", "Ber\u00b7ge", ",", "von", "al\u00b7lem", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "APPR", "PIS", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Wahrlich das sch\u00f6nste.", "tokens": ["Wahr\u00b7lich", "das", "sch\u00f6ns\u00b7te", "."], "token_info": ["wor