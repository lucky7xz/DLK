{"textgrid.poem.66167": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Bei der Einweihung der Soldatenkirche zu Ludwigsburg", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Monument, mit Schwei\u00df und Blut bespritzt,", "tokens": ["Kein", "Mo\u00b7nu\u00b7ment", ",", "mit", "Schwei\u00df", "und", "Blut", "be\u00b7spritzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kein goldnes Haus, wo stolz auf Marmorquader", "tokens": ["Kein", "gold\u00b7nes", "Haus", ",", "wo", "stolz", "auf", "Mar\u00b7mor\u00b7qua\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$,", "PWAV", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der falsche Ruhm mit Fluch beladen sitzt,", "tokens": ["Der", "fal\u00b7sche", "Ruhm", "mit", "Fluch", "be\u00b7la\u00b7den", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Erth\u00fcrmt sich heute unser Vater!", "tokens": ["E\u00b7rth\u00fcrmt", "sich", "heu\u00b7te", "un\u00b7ser", "Va\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie bald zerst\u00e4ubt ein Monument von Erz,", "tokens": ["Wie", "bald", "zer\u00b7st\u00e4ubt", "ein", "Mo\u00b7nu\u00b7ment", "von", "Erz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auch eingeweiht mit asiat'schem Pompe!", "tokens": ["Auch", "ein\u00b7ge\u00b7weiht", "mit", "a\u00b7siat'\u00b7schem", "Pom\u00b7pe", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Zur ew'gen Katakombe.", "tokens": ["Zur", "ew'\u00b7gen", "Ka\u00b7ta\u00b7kom\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er spricht \u2013 und Felsenrippen stehen da!", "tokens": ["Er", "spricht", "\u2013", "und", "Fel\u00b7sen\u00b7rip\u00b7pen", "ste\u00b7hen", "da", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Geweiht als Heiligthum zu seines Gottes Ehre!", "tokens": ["Ge\u00b7weiht", "als", "Hei\u00b7lig\u00b7thum", "zu", "sei\u00b7nes", "Got\u00b7tes", "Eh\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und Jehovah! und Jehovah!", "tokens": ["Und", "Je\u00b7ho\u00b7vah", "!", "und", "Je\u00b7ho\u00b7vah", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "$.", "KON", "NE", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Ert\u00f6nen Kanzel und Alt\u00e4re!", "tokens": ["Er\u00b7t\u00f6\u00b7nen", "Kan\u00b7zel", "und", "Al\u00b7t\u00e4\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Schon eilt der F\u00fcrst, wie Salomo,", "tokens": ["Schon", "eilt", "der", "F\u00fcrst", ",", "wie", "Sa\u00b7lo\u00b7mo", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PWAV", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ins Gotteshaus an seiner Kinder Spitze,", "tokens": ["Ins", "Got\u00b7tes\u00b7haus", "an", "sei\u00b7ner", "Kin\u00b7der", "Spit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und seine Andacht steigt, wie Loh", "tokens": ["Und", "sei\u00b7ne", "An\u00b7dacht", "steigt", ",", "wie", "Loh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,", "PWAV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Opfer auf zum Sternensitze.", "tokens": ["Vom", "Op\u00b7fer", "auf", "zum", "Ster\u00b7nen\u00b7sit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ha! Rauchgew\u00f6lk erf\u00fcllt das Haus,", "tokens": ["Ha", "!", "Rauch\u00b7ge\u00b7w\u00f6lk", "er\u00b7f\u00fcllt", "das", "Haus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Donner der Erh\u00f6rung reden", "tokens": ["Die", "Don\u00b7ner", "der", "Er\u00b7h\u00f6\u00b7rung", "re\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Aus einer Wetternacht heraus,", "tokens": ["Aus", "ei\u00b7ner", "Wet\u00b7ter\u00b7nacht", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die segnen und nicht t\u00f6dten:", "tokens": ["Die", "seg\u00b7nen", "und", "nicht", "t\u00f6d\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Himmel ist f\u00fcr meinen Stuhl zu klein,", "tokens": ["Der", "Him\u00b7mel", "ist", "f\u00fcr", "mei\u00b7nen", "Stuhl", "zu", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu klein f\u00fcr meinen Schemel ist die Erde!", "tokens": ["Zu", "klein", "f\u00fcr", "mei\u00b7nen", "Sche\u00b7mel", "ist", "die", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch weih' ich dieses Haus zu meiner Wohnung ein", "tokens": ["Doch", "weih'", "ich", "die\u00b7ses", "Haus", "zu", "mei\u00b7ner", "Woh\u00b7nung", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PDAT", "NN", "APPR", "PPOSAT", "NN", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und den Altar zu meinem Opferherde!", "tokens": ["Und", "den", "Al\u00b7tar", "zu", "mei\u00b7nem", "Op\u00b7fer\u00b7her\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "So spricht der Herr. Der hohen Andacht Glut", "tokens": ["So", "spricht", "der", "Herr", ".", "Der", "ho\u00b7hen", "An\u00b7dacht", "Glut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ergie\u00dft sich schon in Jubellieder!", "tokens": ["Er\u00b7gie\u00dft", "sich", "schon", "in", "Ju\u00b7bel\u00b7lie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es str\u00f6mt der Taufe Krystallflut!", "tokens": ["Es", "str\u00f6mt", "der", "Tau\u00b7fe", "Krys\u00b7tall\u00b7flut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vers\u00f6hnung tr\u00e4uft vom Kelche nieder.", "tokens": ["Ver\u00b7s\u00f6h\u00b7nung", "tr\u00e4uft", "vom", "Kel\u00b7che", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Des frommen Priesters Stimme f\u00e4llt", "tokens": ["Des", "from\u00b7men", "Pries\u00b7ters", "Stim\u00b7me", "f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ins d\u00fcrre Herz, wie goldner Regen.", "tokens": ["Ins", "d\u00fcr\u00b7re", "Herz", ",", "wie", "gold\u00b7ner", "Re\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PWAV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der S\u00fcnder denkt ans Heil der Welt,", "tokens": ["Der", "S\u00fcn\u00b7der", "denkt", "ans", "Heil", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fchlt den bluterkauften Segen.", "tokens": ["Und", "f\u00fchlt", "den", "blu\u00b7ter\u00b7kauf\u00b7ten", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Die Krieger heben die gest\u00e4hlte Hand", "tokens": ["Die", "Krie\u00b7ger", "he\u00b7ben", "die", "ge\u00b7st\u00e4hl\u00b7te", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu Gott am Tag der Tempelweihe,", "tokens": ["Zu", "Gott", "am", "Tag", "der", "Tem\u00b7pel\u00b7wei\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie schw\u00f6ren Gott, dem Vaterland,", "tokens": ["Sie", "schw\u00f6\u00b7ren", "Gott", ",", "dem", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dir, o ", "tokens": ["Und", "dir", ",", "o"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "PPER", "$,", "FM"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.10": {"line.1": {"text": "Und das Soldatenkind h\u00fcpft an der Mutter Brust,", "tokens": ["Und", "das", "Sol\u00b7da\u00b7ten\u00b7kind", "h\u00fcpft", "an", "der", "Mut\u00b7ter", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Vorgef\u00fchl der k\u00fcnft'gen Wonne trunken;", "tokens": ["Vom", "Vor\u00b7ge\u00b7f\u00fchl", "der", "k\u00fcnft'\u00b7gen", "Won\u00b7ne", "trun\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Kl\u00e4ger selbst f\u00fchlt heut' nur Himmelslust,", "tokens": ["Der", "Kl\u00e4\u00b7ger", "selbst", "f\u00fchlt", "heut'", "nur", "Him\u00b7mels\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADV", "ADV", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Aufs Tempelpflaster hingesunken.", "tokens": ["Aufs", "Tem\u00b7pel\u00b7pflas\u00b7ter", "hin\u00b7ge\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Und Ludwigsburg umringt im Strahlenkreis", "tokens": ["Und", "Lud\u00b7wigs\u00b7burg", "um\u00b7ringt", "im", "Strah\u00b7len\u00b7kreis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den gro\u00dfen Stifter dieser Scene.", "tokens": ["Den", "gro\u00b7\u00dfen", "Stif\u00b7ter", "die\u00b7ser", "Sce\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In Psalmenfl\u00fcgen t\u00f6nt ihr Preis,", "tokens": ["In", "Psal\u00b7men\u00b7fl\u00fc\u00b7gen", "t\u00f6nt", "ihr", "Preis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es gl\u00fcht ihr Dank in jeder Thr\u00e4ne.", "tokens": ["Es", "gl\u00fcht", "ihr", "Dank", "in", "je\u00b7der", "Thr\u00e4\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Wer sind die hundert Armen dort,", "tokens": ["Wer", "sind", "die", "hun\u00b7dert", "Ar\u00b7men", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "CARD", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um die der Dank die Seraphsschwingen breitet?", "tokens": ["Um", "die", "der", "Dank", "die", "Se\u00b7raphs\u00b7schwin\u00b7gen", "brei\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hat uns gespeist, getr\u00e4nkt, gekleidet!", "tokens": ["Hat", "uns", "ge\u00b7speist", ",", "ge\u00b7tr\u00e4nkt", ",", "ge\u00b7klei\u00b7det", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Soldatenwaisen fallen aufs Gesicht,", "tokens": ["Sol\u00b7da\u00b7ten\u00b7wai\u00b7sen", "fal\u00b7len", "aufs", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit \u00fcberm Haupt geschlungnen H\u00e4nden;", "tokens": ["Mit", "\u00fc\u00b7berm", "Haupt", "ge\u00b7schlung\u00b7nen", "H\u00e4n\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und was der Waisen Einfalt spricht,", "tokens": ["Und", "was", "der", "Wai\u00b7sen", "Ein\u00b7falt", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schallt himmelan von wiedert\u00f6nenden W\u00e4nden.", "tokens": ["Schallt", "him\u00b7me\u00b7lan", "von", "wie\u00b7der\u00b7t\u00f6\u00b7nen\u00b7den", "W\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.14": {"line.1": {"text": "Kein Psalmendonner spricht so laut,", "tokens": ["Kein", "Psal\u00b7men\u00b7don\u00b7ner", "spricht", "so", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Waisendank und wie des Armen Z\u00e4hre;", "tokens": ["Wie", "Wai\u00b7sen\u00b7dank", "und", "wie", "des", "Ar\u00b7men", "Z\u00e4h\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word"