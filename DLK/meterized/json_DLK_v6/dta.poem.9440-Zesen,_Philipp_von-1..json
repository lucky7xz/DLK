{"dta.poem.9440": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach Liebster/ Ich liege schon nackend darnieder/", "tokens": ["Ach", "Liebs\u00b7ter", "/", "Ich", "lie\u00b7ge", "schon", "na\u00b7ckend", "dar\u00b7nie\u00b7der", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$(", "PPER", "VVFIN", "ADV", "ADJD", "PAV", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wie soll ich die Kleider anzihn/", "tokens": ["Wie", "soll", "ich", "die", "Klei\u00b7der", "an\u00b7zihn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ART", "NN", "VVIZU", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wie soll ich mir wider", "tokens": ["Wie", "soll", "ich", "mir", "wi\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "PTKVZ"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Die F\u00fc\u00dfe besudeln/ mein sch\u00f6nster Rubien.", "tokens": ["Die", "F\u00fc\u00b7\u00dfe", "be\u00b7su\u00b7deln", "/", "mein", "sch\u00f6ns\u00b7ter", "Ru\u00b7bi\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$(", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Doch stecket durchs fenster mein Liebster die h\u00e4nde/", "tokens": ["Doch", "ste\u00b7cket", "durchs", "fens\u00b7ter", "mein", "Liebs\u00b7ter", "die", "h\u00e4n\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "ART", "ADJA", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Mein Leben im hertzen erzittert daf\u00fcr.", "tokens": ["Mein", "Le\u00b7ben", "im", "hert\u00b7zen", "er\u00b7zit\u00b7tert", "da\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVFIN", "PAV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Ich machte mich eylend zum selbigen Ende/", "tokens": ["Ich", "mach\u00b7te", "mich", "ey\u00b7lend", "zum", "sel\u00b7bi\u00b7gen", "En\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "APPRART", "ADJA", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "und wolte dem Liebsten er\u00f6fnen die Th\u00fcr.", "tokens": ["und", "wol\u00b7te", "dem", "Liebs\u00b7ten", "er\u00b7\u00f6f\u00b7nen", "die", "Th\u00fcr."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "VMFIN", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Es troffen die H\u00e4nde", "tokens": ["Es", "trof\u00b7fen", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Mit Myrrhen ohn ende", "tokens": ["Mit", "Myr\u00b7rhen", "ohn", "en\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "und waren benetzt.", "tokens": ["und", "wa\u00b7ren", "be\u00b7netzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ich hatte dem Liebsten er\u00f6fnet die Th\u00fcre/", "tokens": ["Ich", "hat\u00b7te", "dem", "Liebs\u00b7ten", "er\u00b7\u00f6f\u00b7net", "die", "Th\u00fc\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ach! aber wo war er doch hin?", "tokens": ["Ach", "!", "a\u00b7ber", "wo", "war", "er", "doch", "hin", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KON", "PWAV", "VAFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Weil keinen ich sp\u00fcre.", "tokens": ["Weil", "kei\u00b7nen", "ich", "sp\u00fc\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "PPER", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ich suchte den Sch\u00f6nen mit traurigem Sinn;", "tokens": ["Ich", "such\u00b7te", "den", "Sch\u00f6\u00b7nen", "mit", "trau\u00b7ri\u00b7gem", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Ich ruffte doch kont\u2019 ich den Liebsten nicht h\u00f6ren/", "tokens": ["Ich", "ruff\u00b7te", "doch", "kont'", "ich", "den", "Liebs\u00b7ten", "nicht", "h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Die W\u00e4chter beraubten und schlugen mich wund.", "tokens": ["Die", "W\u00e4ch\u00b7ter", "be\u00b7raub\u00b7ten", "und", "schlu\u00b7gen", "mich", "wund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Euch T\u00f6chter Jerusalem will ich beschw\u00f6ren/", "tokens": ["Euch", "T\u00f6ch\u00b7ter", "Je\u00b7ru\u00b7sa\u00b7lem", "will", "ich", "be\u00b7schw\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "NE", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+--+-+---+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Findt jemand Mein Hertze/ so macht es Jhm kunt/", "tokens": ["Findt", "je\u00b7mand", "Mein", "Hert\u00b7ze", "/", "so", "macht", "es", "Jhm", "kunt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$("], "meter": "-+--+--+---", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Wie da\u00df ich vor Liebe", "tokens": ["Wie", "da\u00df", "ich", "vor", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "PPER", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Mich hefftig betr\u00fcbe", "tokens": ["Mich", "heff\u00b7tig", "be\u00b7tr\u00fc\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJD", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "und lagerhafft sey.", "tokens": ["und", "la\u00b7ger\u00b7hafft", "sey", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VAFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}