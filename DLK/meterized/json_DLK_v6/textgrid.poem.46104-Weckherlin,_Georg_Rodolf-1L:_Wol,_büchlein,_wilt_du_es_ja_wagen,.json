{"textgrid.poem.46104": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wol, b\u00fcchlein, wilt du es ja wagen,", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wol, b\u00fcchlein, wilt du es ja wagen,", "tokens": ["Wol", ",", "b\u00fcch\u00b7lein", ",", "wilt", "du", "es", "ja", "wa\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "$,", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "so zeuch hinaus mit gutem mut,", "tokens": ["so", "zeuch", "hin\u00b7aus", "mit", "gu\u00b7tem", "mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "dan demnach dein gewissen gut,", "tokens": ["dan", "dem\u00b7nach", "dein", "ge\u00b7wis\u00b7sen", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "PPOSAT", "ADJA", "ADJD", "$,"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.4": {"text": "so gilt es gleich, was man wird sagen.", "tokens": ["so", "gilt", "es", "gleich", ",", "was", "man", "wird", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PRELS", "PIS", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dem\u00fctig k\u00fcssend zu begr\u00fc\u00dfen", "tokens": ["De\u00b7m\u00fc\u00b7tig", "k\u00fcs\u00b7send", "zu", "be\u00b7gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "der gro\u00dfm\u00e4chtigen g\u00f6ttin hand,", "tokens": ["der", "gro\u00df\u00b7m\u00e4ch\u00b7ti\u00b7gen", "g\u00f6t\u00b7tin", "hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "soll dich misgunst und unverstand", "tokens": ["soll", "dich", "mis\u00b7gunst", "und", "un\u00b7ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "weder verhindern noch verdrie\u00dfen.", "tokens": ["we\u00b7der", "ver\u00b7hin\u00b7dern", "noch", "ver\u00b7drie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Gefallen kanst du gar nicht allen,", "tokens": ["Ge\u00b7fal\u00b7len", "kanst", "du", "gar", "nicht", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "PTKNEG", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "gefallen vilen, ist zu vil:", "tokens": ["ge\u00b7fal\u00b7len", "vi\u00b7len", ",", "ist", "zu", "vil", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VVINF", "$,", "VAFIN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "hast also dein gewisses spil,", "tokens": ["hast", "al\u00b7so", "dein", "ge\u00b7wis\u00b7ses", "spil", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "da\u00df du wirst wenigen gefallen.", "tokens": ["da\u00df", "du", "wirst", "we\u00b7ni\u00b7gen", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Jedoch wie wenig diser seien,", "tokens": ["Je\u00b7doch", "wie", "we\u00b7nig", "di\u00b7ser", "sei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PIAT", "PDS", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "seind sie gnug, weil sie gut und from:", "tokens": ["seind", "sie", "gnug", ",", "weil", "sie", "gut", "und", "from", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "und sie, bei denen du willkom,", "tokens": ["und", "sie", ",", "bei", "de\u00b7nen", "du", "will\u00b7kom", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPR", "PRELS", "PPER", "VMFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "f\u00fcr unfall werden dich schon freien.", "tokens": ["f\u00fcr", "un\u00b7fall", "wer\u00b7den", "dich", "schon", "frei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wan du dan so wol aufgenommen", "tokens": ["Wan", "du", "dan", "so", "wol", "auf\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ADV", "VVPP"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "f\u00fcr andern k\u00e4mest auch herf\u00fcr,", "tokens": ["f\u00fcr", "an\u00b7dern", "k\u00e4\u00b7mest", "auch", "her\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "so wollen fr\u00f6lich bald nach dir", "tokens": ["so", "wol\u00b7len", "fr\u00f6\u00b7lich", "bald", "nach", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADJD", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "mehr sch\u00f6nere geschwistrigt kommen.", "tokens": ["mehr", "sch\u00f6\u00b7ne\u00b7re", "ge\u00b7schwist\u00b7rigt", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}