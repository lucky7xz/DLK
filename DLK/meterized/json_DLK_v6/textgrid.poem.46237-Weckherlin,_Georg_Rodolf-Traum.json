{"textgrid.poem.46237": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Traum", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah in meinem schlaf ein bild gleich einem got,", "tokens": ["Ich", "sah", "in", "mei\u00b7nem", "schlaf", "ein", "bild", "gleich", "ei\u00b7nem", "got", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "auf einem reichen thron ganz pr\u00e4chtiglich erhaben,", "tokens": ["auf", "ei\u00b7nem", "rei\u00b7chen", "thron", "ganz", "pr\u00e4ch\u00b7tig\u00b7lich", "er\u00b7ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "in dessen dienst und schutz zugleich aus lust und not", "tokens": ["in", "des\u00b7sen", "dienst", "und", "schutz", "zu\u00b7gleich", "aus", "lust", "und", "not"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "KON", "ADJD", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "sich die torechte leut stets haufenweis begaben.", "tokens": ["sich", "die", "to\u00b7rech\u00b7te", "leut", "stets", "hau\u00b7fen\u00b7weis", "be\u00b7ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVFIN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich sah, wie dises bild dem wahren got zu spot", "tokens": ["Ich", "sah", ",", "wie", "di\u00b7ses", "bild", "dem", "wah\u00b7ren", "got", "zu", "spot"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PDAT", "NN", "ART", "ADJA", "NN", "APPR", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "empfieng, zwar niemal sat, gel\u00fcbd, lob, opfergaben", "tokens": ["emp\u00b7fi\u00b7eng", ",", "zwar", "nie\u00b7mal", "sat", ",", "ge\u00b7l\u00fcbd", ",", "lob", ",", "op\u00b7fer\u00b7ga\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "$,", "ADV", "ADV", "VVFIN", "$,", "ADJD", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "und gab auch wem es wolt das leben und den tod", "tokens": ["und", "gab", "auch", "wem", "es", "wolt", "das", "le\u00b7ben", "und", "den", "tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PWS", "PPER", "VMFIN", "PDS", "VVINF", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und pflag sich mit rach, straf und bosheit zu erlaben.", "tokens": ["und", "pflag", "sich", "mit", "rach", ",", "straf", "und", "bos\u00b7heit", "zu", "er\u00b7la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NE", "$,", "VVFIN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und ob der himmel schon oftmal, des bilds undank", "tokens": ["Und", "ob", "der", "him\u00b7mel", "schon", "oft\u00b7mal", ",", "des", "bilds", "un\u00b7dank"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "ADV", "$,", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "zu strafen, seine stern versamlete mit wunder,", "tokens": ["zu", "stra\u00b7fen", ",", "sei\u00b7ne", "stern", "ver\u00b7sam\u00b7le\u00b7te", "mit", "wun\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "so war doch des bilds stim noch lauter dan der dunder;", "tokens": ["so", "war", "doch", "des", "bilds", "stim", "noch", "lau\u00b7ter", "dan", "der", "dun\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "VVFIN", "ADV", "ADJD", "ADV", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bis endlich, als sein stolz war in dem h\u00f6chsten schwank,", "tokens": ["Bis", "end\u00b7lich", ",", "als", "sein", "stolz", "war", "in", "dem", "h\u00f6chs\u00b7ten", "schwank", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "KOUS", "PPOSAT", "ADJD", "VAFIN", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "da schlug ein schneller blitz das sch\u00f6ne bild herunder,", "tokens": ["da", "schlug", "ein", "schnel\u00b7ler", "blitz", "das", "sch\u00f6\u00b7ne", "bild", "her\u00b7un\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "verkehrend seinen pracht in kot, w\u00fcrm und gestank.", "tokens": ["ver\u00b7keh\u00b7rend", "sei\u00b7nen", "pracht", "in", "kot", ",", "w\u00fcrm", "und", "ge\u00b7stank", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "NN", "APPR", "NE", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}