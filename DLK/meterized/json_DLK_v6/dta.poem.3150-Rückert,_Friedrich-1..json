{"dta.poem.3150": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gar manches sagt nicht rein brahman'sches der Brahman;", "tokens": ["Gar", "man\u00b7ches", "sagt", "nicht", "rein", "brah\u00b7man'\u00b7sches", "der", "Brah\u00b7man", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PTKNEG", "ADJD", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sei es rein menschlich nur, so nehmen wir es an.", "tokens": ["Sei", "es", "rein", "menschlich", "nur", ",", "so", "neh\u00b7men", "wir", "es", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADJD", "ADV", "$,", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch dieses, was aus gar zu fremden Augen schaut,", "tokens": ["Doch", "die\u00b7ses", ",", "was", "aus", "gar", "zu", "frem\u00b7den", "Au\u00b7gen", "schaut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "PRELS", "APPR", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat ein europischer Bekannter ihm vertraut.", "tokens": ["Hat", "ein", "eu\u00b7ro\u00b7pi\u00b7scher", "Be\u00b7kann\u00b7ter", "ihm", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}