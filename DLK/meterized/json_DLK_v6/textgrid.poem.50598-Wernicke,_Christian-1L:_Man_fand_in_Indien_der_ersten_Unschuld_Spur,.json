{"textgrid.poem.50598": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Man fand in Indien der ersten Unschuld Spur,", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man fand in Indien der ersten Unschuld Spur,", "tokens": ["Man", "fand", "in", "In\u00b7di\u00b7en", "der", "ers\u00b7ten", "Un\u00b7schuld", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NE", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ein Volk, das ohne Zwang gehorchte der Natur,", "tokens": ["Ein", "Volk", ",", "das", "oh\u00b7ne", "Zwang", "ge\u00b7horch\u00b7te", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das einen eiteln Ruhm nie sucht' in der Gefahr", "tokens": ["Das", "ei\u00b7nen", "ei\u00b7teln", "Ruhm", "nie", "sucht'", "in", "der", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "ADJA", "NN", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und keine Tugend kannt', als die kein Laster war,", "tokens": ["Und", "kei\u00b7ne", "Tu\u00b7gend", "kannt'", ",", "als", "die", "kein", "Las\u00b7ter", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,", "KOUS", "ART", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So da\u00df hier jedermann erkannte mit Erfreuen,", "tokens": ["So", "da\u00df", "hier", "je\u00b7der\u00b7mann", "er\u00b7kann\u00b7te", "mit", "Er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ADV", "PIS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es sei die ", "tokens": ["Es", "sei", "die"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}}}}