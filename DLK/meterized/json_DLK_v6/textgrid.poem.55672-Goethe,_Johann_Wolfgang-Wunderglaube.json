{"textgrid.poem.55672": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Wunderglaube", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zerbrach einmal eine sch\u00f6ne Schal'", "tokens": ["Zer\u00b7brach", "ein\u00b7mal", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Schal'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und wollte schier verzweifeln;", "tokens": ["Und", "woll\u00b7te", "schier", "ver\u00b7zwei\u00b7feln", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Unart und \u00dcbereil zumal", "tokens": ["Un\u00b7art", "und", "\u00dc\u00b7be\u00b7reil", "zu\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KOUS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00fcnscht ich zu allen Teufeln.", "tokens": ["W\u00fcnscht", "ich", "zu", "al\u00b7len", "Teu\u00b7feln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Erst rast' ich aus, dann weint ich weich", "tokens": ["Erst", "rast'", "ich", "aus", ",", "dann", "weint", "ich", "weich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beim traurigen Scherbelesen;", "tokens": ["Beim", "trau\u00b7ri\u00b7gen", "Scher\u00b7be\u00b7le\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das jammerte Gott, er schuf es gleich", "tokens": ["Das", "jam\u00b7mer\u00b7te", "Gott", ",", "er", "schuf", "es", "gleich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So ganz, als wie es gewesen.", "tokens": ["So", "ganz", ",", "als", "wie", "es", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PWAV", "PPER", "VAPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}