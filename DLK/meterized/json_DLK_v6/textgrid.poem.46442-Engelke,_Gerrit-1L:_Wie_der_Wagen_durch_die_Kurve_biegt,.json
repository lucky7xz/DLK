{"textgrid.poem.46442": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie der Wagen durch die Kurve biegt,", "genre": "verse", "period": "N.A.", "pub_year": 1913, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie der Wagen durch die Kurve biegt,", "tokens": ["Wie", "der", "Wa\u00b7gen", "durch", "die", "Kur\u00b7ve", "biegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wie die blanke Schienenstrecke vor ihm liegt:", "tokens": ["Wie", "die", "blan\u00b7ke", "Schie\u00b7nen\u00b7stre\u00b7cke", "vor", "ihm", "liegt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Walzt er st\u00e4rker, schneller.", "tokens": ["Walzt", "er", "st\u00e4r\u00b7ker", ",", "schnel\u00b7ler", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Die Motore unterm Boden rattern,", "tokens": ["Die", "Mo\u00b7to\u00b7re", "un\u00b7term", "Bo\u00b7den", "rat\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Von den Leitungsdr\u00e4hten knattern", "tokens": ["Von", "den", "Lei\u00b7tungs\u00b7dr\u00e4h\u00b7ten", "knat\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Funken.", "tokens": ["Fun\u00b7ken", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Scharf vor\u00fcber an Laternen, Frauenmoden,", "tokens": ["Scharf", "vor\u00b7\u00fc\u00b7ber", "an", "La\u00b7ter\u00b7nen", ",", "Frau\u00b7en\u00b7mo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$,", "NN", "$,"], "meter": "+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Bild an Bild, Ladenschild, Pferdetritt, Menschenschritt \u2013", "tokens": ["Bild", "an", "Bild", ",", "La\u00b7den\u00b7schild", ",", "Pfer\u00b7de\u00b7tritt", ",", "Men\u00b7schen\u00b7schritt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$("], "meter": "+-++--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Sch\u00fctternd walzt und wiegt der Wagenboden,", "tokens": ["Sch\u00fct\u00b7ternd", "walzt", "und", "wiegt", "der", "Wa\u00b7gen\u00b7bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Meine Sinne walzen, wiegen mit!:", "tokens": ["Mei\u00b7ne", "Sin\u00b7ne", "wal\u00b7zen", ",", "wie\u00b7gen", "mit", "!", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,", "VVFIN", "PTKVZ", "$.", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Voller Strom! Voller Strom!", "tokens": ["Vol\u00b7ler", "Strom", "!", "Vol\u00b7ler", "Strom", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Der ganze Wagen, mit den Menschen drinnen,", "tokens": ["Der", "gan\u00b7ze", "Wa\u00b7gen", ",", "mit", "den", "Men\u00b7schen", "drin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Saust und summt und singt mit meinen Sinnen.", "tokens": ["Saust", "und", "summt", "und", "singt", "mit", "mei\u00b7nen", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Das Wagensingen sausebraust, es schwillt!", "tokens": ["Das", "Wa\u00b7gen\u00b7sin\u00b7gen", "sau\u00b7se\u00b7braust", ",", "es", "schwillt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Pl\u00f6tzlich schrillt", "tokens": ["Pl\u00f6tz\u00b7lich", "schrillt"], "token_info": ["word", "word"], "pos": ["ADJD", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Die Klingel! \u2013", "tokens": ["Die", "Klin\u00b7gel", "!", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ART", "NN", "$.", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Der Stromgesang ist aus \u2013", "tokens": ["Der", "Strom\u00b7ge\u00b7sang", "ist", "aus", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich steige aus \u2013", "tokens": ["Ich", "stei\u00b7ge", "aus", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Weiter walzt der Wagen.", "tokens": ["Wei\u00b7ter", "walzt", "der", "Wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}