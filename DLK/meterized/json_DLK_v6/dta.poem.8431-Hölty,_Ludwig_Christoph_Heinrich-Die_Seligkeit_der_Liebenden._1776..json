{"dta.poem.8431": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Seligkeit der Liebenden.  \n 1776.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Die seinen Jugendtraum begr\u00fcsst,", "tokens": ["Die", "sei\u00b7nen", "Ju\u00b7gend\u00b7traum", "be\u00b7gr\u00fcs\u00b7st", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Arm um Arm, und Geist um Geist sich windet,", "tokens": ["Wenn", "Arm", "um", "Arm", ",", "und", "Geist", "um", "Geist", "sich", "win\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "$,", "KON", "NN", "APPR", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Seel' in Seele sich ergiesst,", "tokens": ["Und", "Seel'", "in", "See\u00b7le", "sich", "er\u00b7giesst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Liebe macht zum Goldpalast die H\u00fctte,", "tokens": ["Die", "Lie\u00b7be", "macht", "zum", "Gold\u00b7pa\u00b7last", "die", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Streut auf die Wildniss Tanz und Spiel,", "tokens": ["Streut", "auf", "die", "Wild\u00b7niss", "Tanz", "und", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Enth\u00fcllet uns der Gottheit leise Tritte,", "tokens": ["Ent\u00b7h\u00fcl\u00b7let", "uns", "der", "Got\u00b7theit", "lei\u00b7se", "Trit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Giebt uns des Himmels Vorgef\u00fchl!", "tokens": ["Giebt", "uns", "des", "Him\u00b7mels", "Vor\u00b7ge\u00b7f\u00fchl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie macht das Herz der Schwermut fr\u00fchlingsheiter;", "tokens": ["Sie", "macht", "das", "Herz", "der", "Schwer\u00b7mut", "fr\u00fch\u00b7lings\u00b7hei\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie bettet uns auf Rosenaun;", "tokens": ["Sie", "bet\u00b7tet", "uns", "auf", "Ro\u00b7se\u00b7naun", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und hebet uns auf eine Himmelsleiter,", "tokens": ["Und", "he\u00b7bet", "uns", "auf", "ei\u00b7ne", "Him\u00b7mels\u00b7lei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo wir den Glanz der Gottheit schaun.", "tokens": ["Wo", "wir", "den", "Glanz", "der", "Got\u00b7theit", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie giebt dem Kranz des Morgens hellre R\u00f6the,", "tokens": ["Sie", "giebt", "dem", "Kranz", "des", "Mor\u00b7gens", "hell\u00b7re", "R\u00f6\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und lichter Gr\u00fcn dem Schattenwald,", "tokens": ["Und", "lich\u00b7ter", "Gr\u00fcn", "dem", "Schat\u00b7ten\u00b7wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und s\u00fcssern Klang der sp\u00e4ten Abendfl\u00f6te,", "tokens": ["Und", "s\u00fcs\u00b7sern", "Klang", "der", "sp\u00e4\u00b7ten", "A\u00b7bend\u00b7fl\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die aus des Dorfes B\u00fcschen schallt.", "tokens": ["Die", "aus", "des", "Dor\u00b7fes", "B\u00fc\u00b7schen", "schallt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Liebenden sind schon zu bessern Zonen", "tokens": ["Die", "Lie\u00b7ben\u00b7den", "sind", "schon", "zu", "bes\u00b7sern", "Zo\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf Fl\u00fcgeln ihrer Lieb' erh\u00f6ht,", "tokens": ["Auf", "Fl\u00fc\u00b7geln", "ih\u00b7rer", "Lieb'", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Empfahen schon des Himmels goldne Kronen,", "tokens": ["E\u00b7mpfa\u00b7hen", "schon", "des", "Him\u00b7mels", "gold\u00b7ne", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Eh ihr Gewand von Staub verweht.", "tokens": ["Eh", "ihr", "Ge\u00b7wand", "von", "Staub", "ver\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Sie k\u00fcmmern sich um keine Erdeng\u00fcter,", "tokens": ["Sie", "k\u00fcm\u00b7mern", "sich", "um", "kei\u00b7ne", "Er\u00b7den\u00b7g\u00fc\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind sich die ganze weite Welt,", "tokens": ["Sind", "sich", "die", "gan\u00b7ze", "wei\u00b7te", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und spotten dein, du stolzer Weltgebieter,", "tokens": ["Und", "spot\u00b7ten", "dein", ",", "du", "stol\u00b7zer", "Welt\u00b7ge\u00b7bie\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor dem der Erdkreis niederf\u00e4llt.", "tokens": ["Vor", "dem", "der", "Erd\u00b7kreis", "nie\u00b7der\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Sanft hingeschmiegt auf seidne Fr\u00fchlingsrasen,", "tokens": ["Sanft", "hin\u00b7ge\u00b7schmiegt", "auf", "seid\u00b7ne", "Fr\u00fch\u00b7lings\u00b7ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf Blumen eines Quellenrands,", "tokens": ["Auf", "Blu\u00b7men", "ei\u00b7nes", "Quel\u00b7len\u00b7rands", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verlachen sie die bunten Seisenblasen", "tokens": ["Ver\u00b7la\u00b7chen", "sie", "die", "bun\u00b7ten", "Sei\u00b7sen\u00b7bla\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des lieben leeren Erdentands.", "tokens": ["Des", "lie\u00b7ben", "lee\u00b7ren", "Er\u00b7den\u00b7tands", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ein Druck der Hand, der durch das Leben sch\u00fcttert,", "tokens": ["Ein", "Druck", "der", "Hand", ",", "der", "durch", "das", "Le\u00b7ben", "sch\u00fct\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und eines Blickes Trunkenheit,", "tokens": ["Und", "ei\u00b7nes", "Bli\u00b7ckes", "Trun\u00b7ken\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Feuerkuss, der von der Lippe zittert,", "tokens": ["Ein", "Feu\u00b7er\u00b7kuss", ",", "der", "von", "der", "Lip\u00b7pe", "zit\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Giebt ihnen Engelseligkeit.", "tokens": ["Giebt", "ih\u00b7nen", "En\u00b7gel\u00b7se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ein Blick der Lieb\u2019, aus dem die Seele blicket,", "tokens": ["Ein", "Blick", "der", "Lieb'", ",", "aus", "dem", "die", "See\u00b7le", "bli\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In dem ein Engel sich verkl\u00e4rt,", "tokens": ["In", "dem", "ein", "En\u00b7gel", "sich", "ver\u00b7kl\u00e4rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein s\u00fcsser Wink, den die Geliebte nicket,", "tokens": ["Ein", "s\u00fcs\u00b7ser", "Wink", ",", "den", "die", "Ge\u00b7lieb\u00b7te", "ni\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist tausend dieser Erden wehrt.", "tokens": ["Ist", "tau\u00b7send", "die\u00b7ser", "Er\u00b7den", "wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Ein Herzenskuss, den selber Engel neiden,", "tokens": ["Ein", "Her\u00b7zens\u00b7kuss", ",", "den", "sel\u00b7ber", "En\u00b7gel", "nei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "K\u00fcsst ihren Morgenschlummer wach;", "tokens": ["K\u00fcsst", "ih\u00b7ren", "Mor\u00b7gen\u00b7schlum\u00b7mer", "wach", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Reihentanz von ewigjungen Freuden", "tokens": ["Ein", "Rei\u00b7hen\u00b7tanz", "von", "e\u00b7wig\u00b7jun\u00b7gen", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Umschlingt den lieben langen Tag.", "tokens": ["Um\u00b7schlingt", "den", "lie\u00b7ben", "lan\u00b7gen", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ein s\u00fcsser Schlaf sinkt auf ihr keusches Bette,", "tokens": ["Ein", "s\u00fcs\u00b7ser", "Schlaf", "sinkt", "auf", "ihr", "keu\u00b7sches", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie auf die Lauben Edens sank.", "tokens": ["Wie", "auf", "die", "Lau\u00b7ben", "E\u00b7dens", "sank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Endlicher misst ihrer Freuden Kette,", "tokens": ["Kein", "End\u00b7li\u00b7cher", "misst", "ih\u00b7rer", "Freu\u00b7den", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-----+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Wer nicht den Kelch der Liebe trank.", "tokens": ["Wer", "nicht", "den", "Kelch", "der", "Lie\u00b7be", "trank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}