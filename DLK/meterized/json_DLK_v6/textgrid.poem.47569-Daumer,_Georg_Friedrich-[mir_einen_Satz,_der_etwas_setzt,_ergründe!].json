{"textgrid.poem.47569": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[mir einen Satz, der etwas setzt, ergr\u00fcnde!]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbmir einen Satz, der etwas setzt, ergr\u00fcnde!", "tokens": ["\u00bb", "mir", "ei\u00b7nen", "Satz", ",", "der", "et\u00b7was", "setzt", ",", "er\u00b7gr\u00fcn\u00b7de", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wahrheit, Hafis, ohn' alle Furcht verk\u00fcnde!", "tokens": ["Wahr\u00b7heit", ",", "Ha\u00b7fis", ",", "ohn'", "al\u00b7le", "Furcht", "ver\u00b7k\u00fcn\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein Weltenlicht, das von bew\u00f6lkter Erde", "tokens": ["Ein", "Wel\u00b7ten\u00b7licht", ",", "das", "von", "be\u00b7w\u00f6lk\u00b7ter", "Er\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wegscheuche jede Finsterni\u00df, entz\u00fcnde!\u00ab", "tokens": ["Weg\u00b7scheu\u00b7che", "je\u00b7de", "Fins\u00b7ter\u00b7ni\u00df", ",", "ent\u00b7z\u00fcn\u00b7de", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "PIAT", "NN", "$,", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So h\u00f6re denn, o Sch\u00fcler, meine Rede,", "tokens": ["So", "h\u00f6\u00b7re", "denn", ",", "o", "Sch\u00fc\u00b7ler", ",", "mei\u00b7ne", "Re\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und n\u00fctze sie, als eine gute Pfr\u00fcnde!", "tokens": ["Und", "n\u00fct\u00b7ze", "sie", ",", "als", "ei\u00b7ne", "gu\u00b7te", "Pfr\u00fcn\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein Schatte nur ganz ohne Wesen w\u00e4re", "tokens": ["Ein", "Schat\u00b7te", "nur", "ganz", "oh\u00b7ne", "We\u00b7sen", "w\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADV", "APPR", "NN", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer vor dem Herrn in aller Reine st\u00fcnde.", "tokens": ["Wer", "vor", "dem", "Herrn", "in", "al\u00b7ler", "Rei\u00b7ne", "st\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Lebendig ist die S\u00fcnde nur im Leben;", "tokens": ["Le\u00b7ben\u00b7dig", "ist", "die", "S\u00fcn\u00b7de", "nur", "im", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Das Leben, es bestehet in der S\u00fcnde.", "tokens": ["Das", "Le\u00b7ben", ",", "es", "be\u00b7ste\u00b7het", "in", "der", "S\u00fcn\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}