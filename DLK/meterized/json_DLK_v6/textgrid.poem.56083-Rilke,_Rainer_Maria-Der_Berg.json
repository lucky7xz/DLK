{"textgrid.poem.56083": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Der Berg", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sechsunddrei\u00dfig Mal und hundert Mal", "tokens": ["Sech\u00b7sund\u00b7drei\u00b7\u00dfig", "Mal", "und", "hun\u00b7dert", "Mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "KON", "CARD", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "hat der Maler jenen Berg geschrieben,", "tokens": ["hat", "der", "Ma\u00b7ler", "je\u00b7nen", "Berg", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PDAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "weggerissen, wieder hingetrieben", "tokens": ["weg\u00b7ge\u00b7ris\u00b7sen", ",", "wie\u00b7der", "hin\u00b7ge\u00b7trie\u00b7ben"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVPP", "$,", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "(sechsunddrei\u00dfig Mal und hundert Mal)", "tokens": ["(", "sech\u00b7sund\u00b7drei\u00b7\u00dfig", "Mal", "und", "hun\u00b7dert", "Mal", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "CARD", "NN", "KON", "CARD", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "zu dem unbegreiflichen Vulkane,", "tokens": ["zu", "dem", "un\u00b7be\u00b7greif\u00b7li\u00b7chen", "Vul\u00b7ka\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "selig, voll Versuchung, ohne Rat, \u2013", "tokens": ["se\u00b7lig", ",", "voll", "Ver\u00b7su\u00b7chung", ",", "oh\u00b7ne", "Rat", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADJD", "$,", "ADJD", "NN", "$,", "KOUI", "NN", "$,", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "w\u00e4hrend der mit Umri\u00df Angetane", "tokens": ["w\u00e4h\u00b7rend", "der", "mit", "Um\u00b7ri\u00df", "An\u00b7ge\u00b7ta\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "APPR", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "seiner Herrlichkeit nicht Einhalt tat:", "tokens": ["sei\u00b7ner", "Herr\u00b7lich\u00b7keit", "nicht", "Ein\u00b7halt", "tat", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "tausendmal aus allen Tagen tauchend,", "tokens": ["tau\u00b7send\u00b7mal", "aus", "al\u00b7len", "Ta\u00b7gen", "tau\u00b7chend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "N\u00e4chte ohne gleichen von sich ab", "tokens": ["N\u00e4ch\u00b7te", "oh\u00b7ne", "glei\u00b7chen", "von", "sich", "ab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "APPR", "PRF", "PTKVZ"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "fallen lassend, alle wie zu knapp;", "tokens": ["fal\u00b7len", "las\u00b7send", ",", "al\u00b7le", "wie", "zu", "knapp", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "PIS", "KOKOM", "PTKA", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "jedes Bild im Augenblick verbrauchend,", "tokens": ["je\u00b7des", "Bild", "im", "Au\u00b7gen\u00b7blick", "ver\u00b7brau\u00b7chend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "von Gestalt gesteigert zu Gestalt,", "tokens": ["von", "Ge\u00b7stalt", "ge\u00b7stei\u00b7gert", "zu", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "teilnahmslos und weit und ohne Meinung \u2013,", "tokens": ["teil\u00b7nahms\u00b7los", "und", "weit", "und", "oh\u00b7ne", "Mei\u00b7nung", "\u2013", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "KON", "ADJD", "KON", "APPR", "NN", "$(", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "um auf einmal wissend, wie Erscheinung,", "tokens": ["um", "auf", "ein\u00b7mal", "wis\u00b7send", ",", "wie", "Er\u00b7schei\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ADV", "VVPP", "$,", "PWAV", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "sich zu heben hinter jedem Spalt.", "tokens": ["sich", "zu", "he\u00b7ben", "hin\u00b7ter", "je\u00b7dem", "Spalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKZU", "VVINF", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}