{"textgrid.poem.53047": {"metadata": {"author": {"name": "Lohenstein, Daniel Casper von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohin hat sich der Geist der Menschen nicht geschwungen?", "genre": "verse", "period": "N.A.", "pub_year": 1659, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohin hat sich der Geist der Menschen nicht geschwungen?", "tokens": ["Wo\u00b7hin", "hat", "sich", "der", "Geist", "der", "Men\u00b7schen", "nicht", "ge\u00b7schwun\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PRF", "ART", "NN", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die kleine Welt reicht hin, wie weit die gro\u00dfe gr\u00e4nzt;", "tokens": ["Die", "klei\u00b7ne", "Welt", "reicht", "hin", ",", "wie", "weit", "die", "gro\u00b7\u00dfe", "gr\u00e4nzt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,", "PWAV", "ADJD", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn ist der spr\u00f6de Leib gleich nur von Thon entsprungen,", "tokens": ["Denn", "ist", "der", "spr\u00f6\u00b7de", "Leib", "gleich", "nur", "von", "Thon", "ent\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "ADV", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So sieht man doch, da\u00df Gott aus diesen Schlacken gl\u00e4nzt,", "tokens": ["So", "sieht", "man", "doch", ",", "da\u00df", "Gott", "aus", "die\u00b7sen", "Schla\u00b7cken", "gl\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$,", "KOUS", "NN", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df etwas Himmlisches beseele das Gehirne,", "tokens": ["Da\u00df", "et\u00b7was", "Himm\u00b7li\u00b7sches", "be\u00b7see\u00b7le", "das", "Ge\u00b7hir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Ursprung sei von Gott, das Wesen vom Gestirne.", "tokens": ["Der", "Ur\u00b7sprung", "sei", "von", "Gott", ",", "das", "We\u00b7sen", "vom", "Ge\u00b7stir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Sonne der Vernunft, das Auge des Gem\u00fcthes", "tokens": ["Die", "Son\u00b7ne", "der", "Ver\u00b7nunft", ",", "das", "Au\u00b7ge", "des", "Ge\u00b7m\u00fc\u00b7thes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Macht uns zu Herrn der Welt, zu Meistern der Natur.", "tokens": ["Macht", "uns", "zu", "Herrn", "der", "Welt", ",", "zu", "Meis\u00b7tern", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "ART", "NN", "$,", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Panther d\u00e4mpft vor ihr das Sch\u00e4umen des Gebl\u00fctes;", "tokens": ["Der", "Pan\u00b7ther", "d\u00e4mpft", "vor", "ihr", "das", "Sch\u00e4u\u00b7men", "des", "Ge\u00b7bl\u00fc\u00b7tes", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie nimmt der Schlang' ihr Gift durch einen kr\u00e4ft'gen Schwur;", "tokens": ["Sie", "nimmt", "der", "Schlang'", "ihr", "Gift", "durch", "ei\u00b7nen", "kr\u00e4ft'\u00b7gen", "Schwur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie lehrt uns Drachen kirr'n und auf den L\u00f6wen reiten,", "tokens": ["Sie", "lehrt", "uns", "Dra\u00b7chen", "kirr'n", "und", "auf", "den", "L\u00f6\u00b7wen", "rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "NN", "KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Adler \u00fcbereil'n, das Krokodil bestreiten.", "tokens": ["Die", "Ad\u00b7ler", "\u00fc\u00b7ber\u00b7eil'n", ",", "das", "Kro\u00b7ko\u00b7dil", "be\u00b7strei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Elemente selbst sind M\u00e4gde des Verstandes.", "tokens": ["Die", "E\u00b7le\u00b7men\u00b7te", "selbst", "sind", "M\u00e4g\u00b7de", "des", "Ver\u00b7stan\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Durch Leinwand und ein Bret zwingt man das gro\u00dfe Meer;", "tokens": ["Durch", "Lein\u00b7wand", "und", "ein", "Bret", "zwingt", "man", "das", "gro\u00b7\u00dfe", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ART", "NN", "VVFIN", "PIS", "ART", "ADJA", "NN", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Wir machen aus der See ein fruchtbar St\u00fccke Landes,", "tokens": ["Wir", "ma\u00b7chen", "aus", "der", "See", "ein", "frucht\u00b7bar", "St\u00fc\u00b7cke", "Lan\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJD", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wo erst Klippe war, kommt jetzt ein Segel her.", "tokens": ["Und", "wo", "erst", "Klip\u00b7pe", "war", ",", "kommt", "jetzt", "ein", "Se\u00b7gel", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "NN", "VAFIN", "$,", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie raubt Korall' und Perl' aus Amphitrite's Grunde,", "tokens": ["Sie", "raubt", "Ko\u00b7rall'", "und", "Perl'", "aus", "Am\u00b7phi\u00b7tri\u00b7te's", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gr\u00e4bt Gold und andres Mark aus der Gebirge Schlunde.", "tokens": ["Gr\u00e4bt", "Gold", "und", "and\u00b7res", "Mark", "aus", "der", "Ge\u00b7bir\u00b7ge", "Schlun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "ADJA", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wir spielen mit der Gluth und kurzweil'n mit den Flammen,", "tokens": ["Wir", "spie\u00b7len", "mit", "der", "Gluth", "und", "kurz\u00b7weil'n", "mit", "den", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sie uns minder Weh, als Salamandern, thun;", "tokens": ["Da\u00df", "sie", "uns", "min\u00b7der", "Weh", ",", "als", "Sa\u00b7la\u00b7man\u00b7dern", ",", "thun", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "NN", "$,", "KOUS", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir theilen Wind und Luft auf tausend Art vonsammen", "tokens": ["Wir", "thei\u00b7len", "Wind", "und", "Luft", "auf", "tau\u00b7send", "Art", "von\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "APPR", "CARD", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und machen, da\u00df ihr Flug sich hemmen mu\u00df und ruh'n;", "tokens": ["Und", "ma\u00b7chen", ",", "da\u00df", "ihr", "Flug", "sich", "hem\u00b7men", "mu\u00df", "und", "ruh'n", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "KOUS", "PPOSAT", "NN", "PRF", "VVINF", "VMFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wir kehr'n die Berg' in Thal; im Abgrund bau'n wir Kl\u00fcfte", "tokens": ["Wir", "kehr'n", "die", "Ber\u00b7g'", "in", "Thal", ";", "im", "Ab\u00b7grund", "bau'n", "wir", "Kl\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$.", "APPRART", "NN", "VVFIN", "PPER", "NN"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Und gro\u00dfe St\u00e4dt' in's Meer, Th\u00fcrm' \u00fcber Wolk- und L\u00fcfte.", "tokens": ["Und", "gro\u00b7\u00dfe", "St\u00e4dt'", "in's", "Meer", ",", "Th\u00fcrm'", "\u00fc\u00b7ber", "Wol\u00b7k", "und", "L\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPRART", "NN", "$,", "NN", "APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Ja, eines Menschen Geist kann tausend Wun'der stiften,", "tokens": ["Ja", ",", "ei\u00b7nes", "Men\u00b7schen", "Geist", "kann", "tau\u00b7send", "Wun'\u00b7der", "stif\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "NN", "VMFIN", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn Flei\u00df die Sinne sch\u00e4rft und Weisheit den Verstand.", "tokens": ["Wenn", "Flei\u00df", "die", "Sin\u00b7ne", "sch\u00e4rft", "und", "Weis\u00b7heit", "den", "Ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVFIN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Welt, das gro\u00dfe Buch, steckt in gelehrten Schriften,", "tokens": ["Die", "Welt", ",", "das", "gro\u00b7\u00dfe", "Buch", ",", "steckt", "in", "ge\u00b7lehr\u00b7ten", "Schrif\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Daraus uns der Natur Geheimni\u00df wird bekannt;", "tokens": ["Da\u00b7raus", "uns", "der", "Na\u00b7tur", "Ge\u00b7heim\u00b7ni\u00df", "wird", "be\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ART", "NN", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ja, ein scharfsinn'ger Geist ist f\u00e4hig, das zu lernen,", "tokens": ["Ja", ",", "ein", "scha\u00b7rf\u00b7sinn'\u00b7ger", "Geist", "ist", "f\u00e4\u00b7hig", ",", "das", "zu", "ler\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "ADJA", "NN", "VAFIN", "ADJD", "$,", "PRELS", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Was \u00fcber die Natur, was au\u00dfer Welt und Sternen.", "tokens": ["Was", "\u00fc\u00b7ber", "die", "Na\u00b7tur", ",", "was", "au\u00b7\u00dfer", "Welt", "und", "Ster\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "$,", "PRELS", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "In dem Gehirne steckt's Register der Geschichte,", "tokens": ["In", "dem", "Ge\u00b7hir\u00b7ne", "steckt's", "Re\u00b7gis\u00b7ter", "der", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sein Ged\u00e4chtni\u00df ist die Mappe ganzer Welt;", "tokens": ["Und", "sein", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "ist", "die", "Map\u00b7pe", "gan\u00b7zer", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er zeucht Wald, Stein und Wild durch Harfen und Gedichte,", "tokens": ["Er", "zeucht", "Wald", ",", "Stein", "und", "Wild", "durch", "Har\u00b7fen", "und", "Ge\u00b7dich\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "KON", "NE", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schafft durch Beredtsamkeit, da\u00df Grimm und P\u00f6bel f\u00e4llt,", "tokens": ["Schafft", "durch", "Be\u00b7redt\u00b7sam\u00b7keit", ",", "da\u00df", "Grimm", "und", "P\u00f6\u00b7bel", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "KOUS", "NE", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zwingt durch Gesetzes Zaum der rauhen V\u00f6lker Sitten,", "tokens": ["Zwingt", "durch", "Ge\u00b7set\u00b7zes", "Zaum", "der", "rau\u00b7hen", "V\u00f6l\u00b7ker", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "NN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df tausend L\u00e4ndern kann ein einig Haupt gebieten;", "tokens": ["Da\u00df", "tau\u00b7send", "L\u00e4n\u00b7dern", "kann", "ein", "ei\u00b7nig", "Haupt", "ge\u00b7bie\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "NN", "VMFIN", "ART", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ergr\u00fcndet ohne Blei die Tiefen tiefer Fl\u00fcsse,", "tokens": ["Er\u00b7gr\u00fcn\u00b7det", "oh\u00b7ne", "Blei", "die", "Tie\u00b7fen", "tie\u00b7fer", "Fl\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mi\u00dft ohne Messen ab See, Berge, Th\u00fcrme, Land;", "tokens": ["Mi\u00dft", "oh\u00b7ne", "Mes\u00b7sen", "ab", "See", ",", "Ber\u00b7ge", ",", "Th\u00fcr\u00b7me", ",", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "APPR", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn er legt St\u00e4dt' in Grund, von Feldern machet Risse,", "tokens": ["Wenn", "er", "legt", "St\u00e4dt'", "in", "Grund", ",", "von", "Fel\u00b7dern", "ma\u00b7chet", "Ris\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", 