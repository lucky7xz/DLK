{"textgrid.poem.59344": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als noch Lieb mit mir im Bunde,", "tokens": ["Als", "noch", "Lieb", "mit", "mir", "im", "Bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hatt ich Ruhe keine Stunde;", "tokens": ["Hatt", "ich", "Ru\u00b7he", "kei\u00b7ne", "Stun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn im Schlo\u00df noch alle schliefen,", "tokens": ["Wenn", "im", "Schlo\u00df", "noch", "al\u00b7le", "schlie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ADV", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "War's, als ob s\u00fc\u00df' Stimmen riefen,", "tokens": ["Wa\u00b7r's", ",", "als", "ob", "s\u00fc\u00df'", "Stim\u00b7men", "rie\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOKOM", "KOUS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "T\u00f6nend bis zum Herzensgrunde:", "tokens": ["T\u00f6\u00b7nend", "bis", "zum", "Her\u00b7zens\u00b7grun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00bbauf! schon goldne Strahlen dringen,", "tokens": ["\u00bb", "auf", "!", "schon", "gold\u00b7ne", "Strah\u00b7len", "drin\u00b7gen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Heiter funkeln Wald und Garten,", "tokens": ["Hei\u00b7ter", "fun\u00b7keln", "Wald", "und", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Neu erquickt die V\u00f6gel singen,", "tokens": ["Neu", "er\u00b7quickt", "die", "V\u00f6\u00b7gel", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "L\u00e4\u00dft du so dein Liebchen warten?\u00ab", "tokens": ["L\u00e4\u00dft", "du", "so", "dein", "Lieb\u00b7chen", "war\u00b7ten", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und vom Lager mu\u00dft ich springen.", "tokens": ["Und", "vom", "La\u00b7ger", "mu\u00dft", "ich", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch kein Licht noch sah ich grauen,", "tokens": ["Doch", "kein", "Licht", "noch", "sah", "ich", "grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Drau\u00dfen durch die n\u00e4chtlich lauen", "tokens": ["Drau\u00b7\u00dfen", "durch", "die", "n\u00e4cht\u00b7lich", "lau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "R\u00e4ume nur die Wolken flogen,", "tokens": ["R\u00e4u\u00b7me", "nur", "die", "Wol\u00b7ken", "flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df die Seele, mitgezogen,", "tokens": ["Da\u00df", "die", "See\u00b7le", ",", "mit\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gern versank im tiefen Schauen \u2013", "tokens": ["Gern", "ver\u00b7sank", "im", "tie\u00b7fen", "Schau\u00b7en", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unten dann die weite Runde,", "tokens": ["Un\u00b7ten", "dann", "die", "wei\u00b7te", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schl\u00f6sser gl\u00e4nzend fern erhoben,", "tokens": ["Schl\u00f6s\u00b7ser", "gl\u00e4n\u00b7zend", "fern", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nachtigallen aus dem Grunde,", "tokens": ["Nach\u00b7ti\u00b7gal\u00b7len", "aus", "dem", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Alles wie im Traum verwoben,", "tokens": ["Al\u00b7les", "wie", "im", "Traum", "ver\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Miteinander still im Bunde.", "tokens": ["Mi\u00b7tein\u00b7an\u00b7der", "still", "im", "Bun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}}, "stanza.3": {"line.1": {"text": "Wach blieb ich am Fenster stehen,", "tokens": ["Wach", "blieb", "ich", "am", "Fens\u00b7ter", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fchler schon die L\u00fcfte wehen,", "tokens": ["K\u00fch\u00b7ler", "schon", "die", "L\u00fcf\u00b7te", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rot schon rings des Himmels S\u00e4ume,", "tokens": ["Rot", "schon", "rings", "des", "Him\u00b7mels", "S\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Regten frischer sich die B\u00e4ume,", "tokens": ["Reg\u00b7ten", "fri\u00b7scher", "sich", "die", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Stimmen h\u00f6rt ich fernab gehen:", "tokens": ["Stim\u00b7men", "h\u00f6rt", "ich", "fern\u00b7ab", "ge\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und durch T\u00fcren, \u00f6de Bogen,", "tokens": ["Und", "durch", "T\u00fc\u00b7ren", ",", "\u00f6\u00b7de", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Z\u00fcrnend, da\u00df die Riegel klungen,", "tokens": ["Z\u00fcr\u00b7nend", ",", "da\u00df", "die", "Rie\u00b7gel", "klun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bin ich heimlich ausgezogen,", "tokens": ["Bin", "ich", "heim\u00b7lich", "aus\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Bis befreit aufs Ro\u00df geschwungen,", "tokens": ["Bis", "be\u00b7freit", "aufs", "Ro\u00df", "ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Morgenwinde mich umflogen.", "tokens": ["Mor\u00b7gen\u00b7win\u00b7de", "mich", "um\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "L\u00e4\u00dft der Morgen von den H\u00f6hen", "tokens": ["L\u00e4\u00dft", "der", "Mor\u00b7gen", "von", "den", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weit die roten Fahnen wehen,", "tokens": ["Weit", "die", "ro\u00b7ten", "Fah\u00b7nen", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Widerhall in allen L\u00fcften,", "tokens": ["Wi\u00b7der\u00b7hall", "in", "al\u00b7len", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Losgerissen aus den Kl\u00fcften", "tokens": ["Los\u00b7ge\u00b7ris\u00b7sen", "aus", "den", "Kl\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Silberner die Str\u00f6me gehen:", "tokens": ["Sil\u00b7ber\u00b7ner", "die", "Str\u00f6\u00b7me", "ge\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Sp\u00fcrt der Mann die frischen Geister,", "tokens": ["Sp\u00fcrt", "der", "Mann", "die", "fri\u00b7schen", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Drau\u00dfen auf dem Feld, zu Pferde", "tokens": ["Drau\u00b7\u00dfen", "auf", "dem", "Feld", ",", "zu", "Pfer\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Alle \u00c4ngste keck zerrei\u00dft er,", "tokens": ["Al\u00b7le", "\u00c4ngs\u00b7te", "keck", "zer\u00b7rei\u00dft", "er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Dampfend unter ihm die Erde,", "tokens": ["Damp\u00b7fend", "un\u00b7ter", "ihm", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "F\u00fchlt er hier sich Herr und Meister.", "tokens": ["F\u00fchlt", "er", "hier", "sich", "Herr", "und", "Meis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PRF", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und so \u00f6ffnet ich die schw\u00fcle", "tokens": ["Und", "so", "\u00f6ff\u00b7net", "ich", "die", "schw\u00fc\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Brust aufatmend in der K\u00fchle!", "tokens": ["Brust", "auf\u00b7at\u00b7mend", "in", "der", "K\u00fch\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Locken fort aus Stirn und Wange,", "tokens": ["Lo\u00b7cken", "fort", "aus", "Stirn", "und", "Wan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df der Strom mich ganz umfange,", "tokens": ["Da\u00df", "der", "Strom", "mich", "ganz", "um\u00b7fan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Frei das blaue Meer umsp\u00fcle,", "tokens": ["Frei", "das", "blau\u00b7e", "Meer", "um\u00b7sp\u00fc\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit den Wolken, eilig fliehend,", "tokens": ["Mit", "den", "Wol\u00b7ken", ",", "ei\u00b7lig", "flie\u00b7hend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit der Str\u00f6me lichtem Gr\u00fc\u00dfen", "tokens": ["Mit", "der", "Str\u00f6\u00b7me", "lich\u00b7tem", "Gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die Gedanken fr\u00f6hlich ziehend,", "tokens": ["Die", "Ge\u00b7dan\u00b7ken", "fr\u00f6h\u00b7lich", "zie\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Weit voraus vor Wolken, Fl\u00fcssen \u2013", "tokens": ["Weit", "vo\u00b7raus", "vor", "Wol\u00b7ken", ",", "Fl\u00fcs\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "PTKVZ", "APPR", "NN", "$,", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Ach! ich f\u00fchlte, da\u00df ich bl\u00fchend!", "tokens": ["Ach", "!", "ich", "f\u00fchl\u00b7te", ",", "da\u00df", "ich", "bl\u00fc\u00b7hend", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und im sch\u00f6nen Garten droben,", "tokens": ["Und", "im", "sch\u00f6\u00b7nen", "Gar\u00b7ten", "dro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie aus Tr\u00e4umen erst gehoben,", "tokens": ["Wie", "aus", "Tr\u00e4u\u00b7men", "erst", "ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sah ich still mein M\u00e4dchen stehen,", "tokens": ["Sah", "ich", "still", "mein", "M\u00e4d\u00b7chen", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcber Flu\u00df und W\u00e4lder gehen", "tokens": ["\u00dc\u00b7ber", "Flu\u00df", "und", "W\u00e4l\u00b7der", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Von der heitern Warte oben", "tokens": ["Von", "der", "hei\u00b7tern", "War\u00b7te", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihre Augen licht und helle,", "tokens": ["Ih\u00b7re", "Au\u00b7gen", "licht", "und", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wann der Liebste kommen werde. \u2013", "tokens": ["Wann", "der", "Liebs\u00b7te", "kom\u00b7men", "wer\u00b7de", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "VAFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ja! da kam die Sonne schnelle,", "tokens": ["Ja", "!", "da", "kam", "die", "Son\u00b7ne", "schnel\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und weit um die ganze Erde", "tokens": ["Und", "weit", "um", "die", "gan\u00b7ze", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "War es morgensch\u00f6n und helle!", "tokens": ["War", "es", "mor\u00b7gen\u00b7sch\u00f6n", "und", "hel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}