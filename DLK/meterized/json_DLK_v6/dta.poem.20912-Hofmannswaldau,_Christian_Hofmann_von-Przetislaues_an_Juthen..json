{"dta.poem.20912": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Przetislaues an Juthen.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer schreibet/ dessen Hand und Auge du nicht", "tokens": ["Hjer", "schrei\u00b7bet", "/", "des\u00b7sen", "Hand", "und", "Au\u00b7ge", "du", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$(", "PRELAT", "NN", "KON", "NN", "PPER", "PTKNEG"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "kennest/", "tokens": ["ken\u00b7nest", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Der dich nur durch Bericht allein hat ange-", "tokens": ["Der", "dich", "nur", "durch", "Be\u00b7richt", "al\u00b7lein", "hat", "an\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "APPR", "NN", "ADV", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "schaut/", "tokens": ["schaut", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Erweg\u2019 eh\u2019 als du mich zu k\u00fchn und th\u00f6richt nennest/", "tokens": ["Er\u00b7weg'", "eh'", "als", "du", "mich", "zu", "k\u00fchn", "und", "th\u00f6\u00b7richt", "nen\u00b7nest", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NE", "KOUS", "PPER", "PRF", "PTKA", "ADJD", "KON", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-++--", "measure": "unknown.measure.hexa"}, "line.6": {"text": "Wie Lieb und Hoffnung uns viel fremde Schl\u00f6sser", "tokens": ["Wie", "Lieb", "und", "Hoff\u00b7nung", "uns", "viel", "frem\u00b7de", "Schl\u00f6s\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "PPER", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "baut.", "tokens": ["baut", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Ich wei\u00df es die Vernunfft reimt dieses nicht zusam-", "tokens": ["Ich", "wei\u00df", "es", "die", "Ver\u00b7nunfft", "reimt", "die\u00b7ses", "nicht", "zu\u00b7sam"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "VVFIN", "PDS", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "men/", "tokens": ["men", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.10": {"text": "Ich war durch dich beraubt/ un\u0303 sahest mich doch nicht/", "tokens": ["Ich", "war", "durch", "dich", "be\u00b7raubt", "/", "u\u00f1", "sa\u00b7hest", "mich", "doch", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "$(", "KON", "VVFIN", "PPER", "ADV", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Doch dencke/ da\u00df die Brunst mit kr\u00e4fftenreichen", "tokens": ["Doch", "den\u00b7cke", "/", "da\u00df", "die", "Brunst", "mit", "kr\u00e4ff\u00b7ten\u00b7rei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$(", "KOUS", "ART", "NN", "APPR", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Flammen/", "tokens": ["Flam\u00b7men", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Durch Mauer der Vernunfft und der Gesetze bricht.", "tokens": ["Durch", "Mau\u00b7er", "der", "Ver\u00b7nunfft", "und", "der", "Ge\u00b7set\u00b7ze", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ich mu\u00df es nur gestehn und schreib es unverholen/", "tokens": ["Ich", "mu\u00df", "es", "nur", "ge\u00b7stehn", "und", "schreib", "es", "un\u00b7ver\u00b7ho\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVPP", "KON", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Dieweil die Feder mir durch Liebe wird bewegt/", "tokens": ["Die\u00b7weil", "die", "Fe\u00b7der", "mir", "durch", "Lie\u00b7be", "wird", "be\u00b7wegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "APPR", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Du hast/ dir unbewust/ das Hertze mir gestohlen/", "tokens": ["Du", "hast", "/", "dir", "un\u00b7be\u00b7wust", "/", "das", "Hert\u00b7ze", "mir", "ge\u00b7stoh\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "PPER", "ADJD", "$(", "PDS", "VVFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und in das Kloster hin nechst den Altar gelegt.", "tokens": ["Und", "in", "das", "Klos\u00b7ter", "hin", "nechst", "den", "Al\u00b7tar", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.18": {"text": "Sucht mancher nicht mit Angst ein Schaf viel Tag", "tokens": ["Sucht", "man\u00b7cher", "nicht", "mit", "Angst", "ein", "Schaf", "viel", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PTKNEG", "APPR", "NN", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "und Wochen?", "tokens": ["und", "Wo\u00b7chen", "?"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.20": {"text": "Ist mancher nicht bem\u00fcht zu finden einen Stein?", "tokens": ["Ist", "man\u00b7cher", "nicht", "be\u00b7m\u00fcht", "zu", "fin\u00b7den", "ei\u00b7nen", "Stein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PTKNEG", "VVPP", "PTKZU", "VVINF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wie solt ich Armer denn nicht auch mein Hertze suchen?", "tokens": ["Wie", "solt", "ich", "Ar\u00b7mer", "denn", "nicht", "auch", "mein", "Hert\u00b7ze", "su\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "NN", "ADV", "PTKNEG", "ADV", "PPOSAT", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Man wei\u00df da\u00df ohne di\u00df wir todt und nichtig seyn.", "tokens": ["Man", "wei\u00df", "da\u00df", "oh\u00b7ne", "di\u00df", "wir", "todt", "und", "nich\u00b7tig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KOUS", "APPR", "PDS", "PPER", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Doch nehm\u2019 ich ohne dich mein Hertze nicht zur\u00fccke/", "tokens": ["Doch", "nehm'", "ich", "oh\u00b7ne", "dich", "mein", "Hert\u00b7ze", "nicht", "zu\u00b7r\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPER", "PPOSAT", "VVFIN", "PTKNEG", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ich will die R\u00e4uberin und auch den Raub zugleich/", "tokens": ["Ich", "will", "die", "R\u00e4u\u00b7be\u00b7rin", "und", "auch", "den", "Raub", "zu\u00b7gleich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "KON", "ADV", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Nicht wundre dich darob/ es seyn zwey liebe St\u00fccke/", "tokens": ["Nicht", "wund\u00b7re", "dich", "da\u00b7rob", "/", "es", "seyn", "zwey", "lie\u00b7be", "St\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PRF", "PAV", "$(", "PPER", "VAINF", "CARD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ich achte sie vielmehr als meines Vatern Reich.", "tokens": ["Ich", "ach\u00b7te", "sie", "viel\u00b7mehr", "als", "mei\u00b7nes", "Va\u00b7tern", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KOKOM", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Es kan mein Hertz und Du nicht Kloster Luft vertragen/", "tokens": ["Es", "kan", "mein", "Hertz", "und", "Du", "nicht", "Klos\u00b7ter", "Luft", "ver\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "KON", "PPER", "PTKNEG", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die Kutte/ wie mich deucht/ steht beyden \u00fcbel an/", "tokens": ["Die", "Kut\u00b7te", "/", "wie", "mich", "deucht", "/", "steht", "bey\u00b7den", "\u00fc\u00b7bel", "an", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAV", "PPER", "VVFIN", "$(", "VVFIN", "PIAT", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der/ dessen Psalm du singst/ wird dir es selber sagen/", "tokens": ["Der", "/", "des\u00b7sen", "Psalm", "du", "singst", "/", "wird", "dir", "es", "sel\u00b7ber", "sa\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PRELAT", "NN", "PPER", "VVFIN", "$(", "VAFIN", "PPER", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Da\u00df Brunst und Jugend nicht gebunden werden kan;", "tokens": ["Da\u00df", "Brunst", "und", "Ju\u00b7gend", "nicht", "ge\u00b7bun\u00b7den", "wer\u00b7den", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PTKNEG", "VVPP", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Mein Fr\u00e4ulein/ solt du dich die Glocke meistern lassen?", "tokens": ["Mein", "Fr\u00e4u\u00b7lein", "/", "solt", "du", "dich", "die", "Glo\u00b7cke", "meis\u00b7tern", "las\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VMFIN", "PPER", "PRF", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Solst du dem kalten Ertzt stets zu Gebothe stehn?", "tokens": ["Solst", "du", "dem", "kal\u00b7ten", "Ertzt", "stets", "zu", "Ge\u00b7bo\u00b7the", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Soll denn dein zarter Arm nur Holtz und Stein \u00fcm-", "tokens": ["Soll", "denn", "dein", "zar\u00b7ter", "Arm", "nur", "Holtz", "und", "Stein", "\u00fcm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPOSAT", "ADJA", "NN", "ADV", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "fassen?", "tokens": ["fas\u00b7sen", "?"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.35": {"text": "Wilst du gesund und jung zu deinem Grabe gehn?", "tokens": ["Wilst", "du", "ge\u00b7sund", "und", "jung", "zu", "dei\u00b7nem", "Gra\u00b7be", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "KON", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wilst du die Zelle dir vor einen Thron erwehlen?", "tokens": ["Wilst", "du", "die", "Zel\u00b7le", "dir", "vor", "ei\u00b7nen", "Thron", "er\u00b7weh\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Verwest dein sch\u00f6ner Leib im Kloster vor der Zeit?", "tokens": ["Ver\u00b7west", "dein", "sch\u00f6\u00b7ner", "Leib", "im", "Klos\u00b7ter", "vor", "der", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Will dann dein s\u00fcsser Mund nur Vater unser zehlen?", "tokens": ["Will", "dann", "dein", "s\u00fcs\u00b7ser", "Mund", "nur", "Va\u00b7ter", "un\u00b7ser", "zeh\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "ADJA", "NN", "ADV", "NN", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Soll deine Rose seyn im Fr\u00fchling abgemeit?", "tokens": ["Soll", "dei\u00b7ne", "Ro\u00b7se", "seyn", "im", "Fr\u00fch\u00b7ling", "ab\u00b7ge\u00b7meit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VAINF", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Nein diese Blume war zu etwas mehr gebohren/", "tokens": ["Nein", "die\u00b7se", "Blu\u00b7me", "war", "zu", "et\u00b7was", "mehr", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PDAT", "NN", "VAFIN", "PTKA", "PIS", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Es \u00f6ffnet sich vor Sie das Paradie\u00df der Welt/", "tokens": ["Es", "\u00f6ff\u00b7net", "sich", "vor", "Sie", "das", "Pa\u00b7ra\u00b7die\u00df", "der", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Es hat der Thau der Lust ihr sch\u00f6nes Blat erkohren/", "tokens": ["Es", "hat", "der", "Thau", "der", "Lust", "ihr", "sch\u00f6\u00b7nes", "Blat", "er\u00b7koh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Und will als Perle hier auf Nacker seyn gestellt.", "tokens": ["Und", "will", "als", "Per\u00b7le", "hier", "auf", "Na\u00b7cker", "seyn", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "KOUS", "NN", "ADV", "APPR", "NN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Der Klostergarten ist zuschlecht dich zuverschlissen/", "tokens": ["Der", "Klos\u00b7ter\u00b7gar\u00b7ten", "ist", "zu\u00b7schlecht", "dich", "zu\u00b7ver\u00b7schlis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Kein Auge kennt allhier die Hohheit deiner Pracht/", "tokens": ["Kein", "Au\u00b7ge", "kennt", "all\u00b7hier", "die", "Hoh\u00b7heit", "dei\u00b7ner", "Pracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV",