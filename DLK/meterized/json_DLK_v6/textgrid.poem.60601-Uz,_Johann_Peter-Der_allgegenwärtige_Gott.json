{"textgrid.poem.60601": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Der allgegenw\u00e4rtige Gott", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Allgegenw\u00e4rtiger! ich bin", "tokens": ["All\u00b7ge\u00b7gen\u00b7w\u00e4r\u00b7ti\u00b7ger", "!", "ich", "bin"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADJD", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir nicht verborgen, wo ich bin,", "tokens": ["Dir", "nicht", "ver\u00b7bor\u00b7gen", ",", "wo", "ich", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVPP", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wollt auch auf schnellen Schwingen", "tokens": ["Wollt", "auch", "auf", "schnel\u00b7len", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mich an die fernsten Meere hin", "tokens": ["Mich", "an", "die", "ferns\u00b7ten", "Mee\u00b7re", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Morgenr\u00f6the bringen.", "tokens": ["Die", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Flieg' ich zum Himmel hoch empor,", "tokens": ["Flieg'", "ich", "zum", "Him\u00b7mel", "hoch", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich unter seinem Sternenchor", "tokens": ["Mich", "un\u00b7ter", "sei\u00b7nem", "Ster\u00b7nen\u00b7chor"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Glanze zu verstecken:", "tokens": ["Im", "Glan\u00b7ze", "zu", "ver\u00b7ste\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So bist du da, ziehst mich hervor,", "tokens": ["So", "bist", "du", "da", ",", "ziehst", "mich", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Schande wird mich decken.", "tokens": ["Und", "Schan\u00b7de", "wird", "mich", "de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Steig' ich vor deinem Zorn hinab", "tokens": ["Steig'", "ich", "vor", "dei\u00b7nem", "Zorn", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ins Haus der Finsterni\u00df, das Grab:", "tokens": ["Ins", "Haus", "der", "Fins\u00b7ter\u00b7ni\u00df", ",", "das", "Grab", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist auch da zugegen,", "tokens": ["Du", "bist", "auch", "da", "zu\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und schwingest deinen Richterstab", "tokens": ["Und", "schwin\u00b7gest", "dei\u00b7nen", "Rich\u00b7ter\u00b7stab"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf schauervollen Wegen.", "tokens": ["Auf", "schau\u00b7er\u00b7vol\u00b7len", "We\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der alles schuf, ist \u00fcberall!", "tokens": ["Der", "al\u00b7les", "schuf", ",", "ist", "\u00fc\u00b7be\u00b7rall", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O f\u00fcrchterlicher Donnerschall", "tokens": ["O", "f\u00fcrch\u00b7ter\u00b7li\u00b7cher", "Don\u00b7ner\u00b7schall"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In aller S\u00fcnder Ohren!", "tokens": ["In", "al\u00b7ler", "S\u00fcn\u00b7der", "Oh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie furchten keinen Ueberfall:", "tokens": ["Sie", "furch\u00b7ten", "kei\u00b7nen", "Ue\u00b7ber\u00b7fall", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun zittern sie, die Thoren!", "tokens": ["Nun", "zit\u00b7tern", "sie", ",", "die", "Tho\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Gott des Himmels ist so nah?", "tokens": ["Der", "Gott", "des", "Him\u00b7mels", "ist", "so", "nah", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Heiligste der Wesen sah,", "tokens": ["Das", "Hei\u00b7ligs\u00b7te", "der", "We\u00b7sen", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Was wir im Herzen dachten?", "tokens": ["Was", "wir", "im", "Her\u00b7zen", "dach\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was in verschwiegner Nacht geschah,", "tokens": ["Was", "in", "ver\u00b7schwieg\u00b7ner", "Nacht", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wann wir und Satan wachten?", "tokens": ["Wann", "wir", "und", "Sa\u00b7tan", "wach\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wir Narren haben wohl gedacht,", "tokens": ["Wir", "Nar\u00b7ren", "ha\u00b7ben", "wohl", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du w\u00fcrdest uns, o Mitternacht,", "tokens": ["Du", "w\u00fcr\u00b7dest", "uns", ",", "o", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit schwarzen Fl\u00fcgeln decken!", "tokens": ["Mit", "schwar\u00b7zen", "Fl\u00fc\u00b7geln", "de\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Umsonst! Gott wacht um uns, er wacht", "tokens": ["Um\u00b7sonst", "!", "Gott", "wacht", "um", "uns", ",", "er", "wacht"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$.", "NN", "VVFIN", "APPR", "PPER", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit allen seinen Schrecken!", "tokens": ["Mit", "al\u00b7len", "sei\u00b7nen", "Schre\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Noch keines Herzens b\u00f6ser Rath,", "tokens": ["Noch", "kei\u00b7nes", "Her\u00b7zens", "b\u00f6\u00b7ser", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr S\u00fcnder, keine schn\u00f6de That", "tokens": ["Ihr", "S\u00fcn\u00b7der", ",", "kei\u00b7ne", "schn\u00f6\u00b7de", "That"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist seinem Aug entronnen!", "tokens": ["Ist", "sei\u00b7nem", "Aug", "ent\u00b7ron\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er kennet den geheimen Pfad", "tokens": ["Er", "ken\u00b7net", "den", "ge\u00b7hei\u00b7men", "Pfad"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Staubes und der Sonnen.", "tokens": ["Des", "Stau\u00b7bes", "und", "der", "Son\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O gehe nicht, Herr, ins Gericht,", "tokens": ["O", "ge\u00b7he", "nicht", ",", "Herr", ",", "ins", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$,", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Wenn wider die gelobte Pflicht", "tokens": ["Wenn", "wi\u00b7der", "die", "ge\u00b7lob\u00b7te", "Pflicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir, deine Knechte, handeln!", "tokens": ["Wir", ",", "dei\u00b7ne", "Knech\u00b7te", ",", "han\u00b7deln", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "La\u00df uns vor deinem Angesicht", "tokens": ["La\u00df", "uns", "vor", "dei\u00b7nem", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In frommer Ehrfurcht wandeln!", "tokens": ["In", "from\u00b7mer", "Ehr\u00b7furcht", "wan\u00b7deln", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Da\u00df deinem Auge nichts entflieht,", "tokens": ["Da\u00df", "dei\u00b7nem", "Au\u00b7ge", "nichts", "ent\u00b7flieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was war, und ist, und einst geschieht,", "tokens": ["Was", "war", ",", "und", "ist", ",", "und", "einst", "ge\u00b7schieht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "KON", "VAFIN", "$,", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sey meine Ruh und Freude!", "tokens": ["Sey", "mei\u00b7ne", "Ruh", "und", "Freu\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Gott, der alles weis und sieht,", "tokens": ["Ein", "Gott", ",", "der", "al\u00b7les", "weis", "und", "sieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der sieht auch, wenn ich leide.", "tokens": ["Der", "sieht", "auch", ",", "wenn", "ich", "lei\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}