{"textgrid.poem.61584": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Freuden, die rosigen T\u00e4nzerinnen,", "tokens": ["Die", "Freu\u00b7den", ",", "die", "ro\u00b7si\u00b7gen", "T\u00e4n\u00b7ze\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mit Kr\u00e4nzen und Fackeln, mit Spiel und Gesang,", "tokens": ["Mit", "Kr\u00e4n\u00b7zen", "und", "Fa\u00b7ckeln", ",", "mit", "Spiel", "und", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wie fliehn sie auf schimmernden Sohlen von hinnen!", "tokens": ["Wie", "fliehn", "sie", "auf", "schim\u00b7mern\u00b7den", "Soh\u00b7len", "von", "hin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "APPR", "ADV", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Aber der Kummer hat schleichenden Gang.", "tokens": ["A\u00b7ber", "der", "Kum\u00b7mer", "hat", "schlei\u00b7chen\u00b7den", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Verhallt ist das Fest und das s\u00fc\u00dfe Gel\u00e4chter", "tokens": ["Ver\u00b7hallt", "ist", "das", "Fest", "und", "das", "s\u00fc\u00b7\u00dfe", "Ge\u00b7l\u00e4ch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der schw\u00e4rmenden Dirnen, ach, eh' ich's gedacht;", "tokens": ["Der", "schw\u00e4r\u00b7men\u00b7den", "Dir\u00b7nen", ",", "ach", ",", "eh'", "ich's", "ge\u00b7dacht", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ITJ", "$,", "KOUS", "PIS", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nun tappt er ums Haus mir, ein grimmiger W\u00e4chter,", "tokens": ["Nun", "tappt", "er", "ums", "Haus", "mir", ",", "ein", "grim\u00b7mi\u00b7ger", "W\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und ruft mir die langsamen Stunden der Nacht.", "tokens": ["Und", "ruft", "mir", "die", "lang\u00b7sa\u00b7men", "Stun\u00b7den", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}