{"dta.poem.10351": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XiI.  \n  Die Fensterschau .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Der bleiche Heinrich ging vorbei,", "tokens": ["Der", "blei\u00b7che", "Hein\u00b7rich", "ging", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sch\u00f6n Hedwig lag am Fenster.", "tokens": ["Sch\u00f6n", "Hed\u00b7wig", "lag", "am", "Fens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie sprach halblaut: Gott steh mir bei,", "tokens": ["Sie", "sprach", "hal\u00b7blaut", ":", "Gott", "steh", "mir", "bei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der unten schaut bleich wie Gespenster!", "tokens": ["Der", "un\u00b7ten", "schaut", "bleich", "wie", "Ge\u00b7spens\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der unten erhub sein Aug in die H\u00f6h',", "tokens": ["Der", "un\u00b7ten", "er\u00b7hub", "sein", "Aug", "in", "die", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hinschmachtend nach Hedewigs Fenster.", "tokens": ["Hin\u00b7schmach\u00b7tend", "nach", "He\u00b7de\u00b7wigs", "Fens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NE", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sch\u00f6n Hedwig ergriff es wie Liebesweh,", "tokens": ["Sch\u00f6n", "Hed\u00b7wig", "er\u00b7griff", "es", "wie", "Lie\u00b7bes\u00b7weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PPER", "KOKOM", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Auch sie ward bleich wie Gespenster.", "tokens": ["Auch", "sie", "ward", "bleich", "wie", "Ge\u00b7spens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADJD", "KOKOM", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6n Hedwig stand nun mit Liebesharm", "tokens": ["Sch\u00f6n", "Hed\u00b7wig", "stand", "nun", "mit", "Lie\u00b7bes\u00b7harm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Allt\u00e4glich lauernd am Fenster.", "tokens": ["All\u00b7t\u00e4g\u00b7lich", "lau\u00b7ernd", "am", "Fens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bald aber lag sie in Heinrichs Arm,", "tokens": ["Bald", "a\u00b7ber", "lag", "sie", "in", "Hein\u00b7richs", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "NE", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Alln\u00e4chtlich zur Zeit der Gespenster.", "tokens": ["All\u00b7n\u00e4cht\u00b7lich", "zur", "Zeit", "der", "Ge\u00b7spens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}