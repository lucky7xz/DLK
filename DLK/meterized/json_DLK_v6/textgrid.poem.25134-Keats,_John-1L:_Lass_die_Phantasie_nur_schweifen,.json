{"textgrid.poem.25134": {"metadata": {"author": {"name": "Keats, John", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lass die Phantasie nur schweifen,", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lass die Phantasie nur schweifen,", "tokens": ["Lass", "die", "Phan\u00b7ta\u00b7sie", "nur", "schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freude will zuhaus nicht reifen;", "tokens": ["Freu\u00b7de", "will", "zu\u00b7haus", "nicht", "rei\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denk, dein kleines Gl\u00fcck zerflie\u00dft:", "tokens": ["Denk", ",", "dein", "klei\u00b7nes", "Gl\u00fcck", "zer\u00b7flie\u00dft", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Regen, der aufs Pflaster gie\u00dft.", "tokens": ["Re\u00b7gen", ",", "der", "aufs", "Pflas\u00b7ter", "gie\u00dft", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Drum la\u00df Phantasie nur streifen,", "tokens": ["Drum", "la\u00df", "Phan\u00b7ta\u00b7sie", "nur", "strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Weiter als Gedanken schweifen,", "tokens": ["Wei\u00b7ter", "als", "Ge\u00b7dan\u00b7ken", "schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Riegle auf des Geistes Tor \u2013", "tokens": ["Rieg\u00b7le", "auf", "des", "Geis\u00b7tes", "Tor", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Lichtw\u00e4rts segelt sie empor.", "tokens": ["Licht\u00b7w\u00e4rts", "se\u00b7gelt", "sie", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "S\u00fc\u00dfe Phantasie la\u00df frei,", "tokens": ["S\u00fc\u00b7\u00dfe", "Phan\u00b7ta\u00b7sie", "la\u00df", "frei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Sommers Freude flieht vorbei,", "tokens": ["Som\u00b7mers", "Freu\u00b7de", "flieht", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Und des Lenzes liebe Lust", "tokens": ["Und", "des", "Len\u00b7zes", "lie\u00b7be", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Welkt wie all sein Bl\u00fctenblust;", "tokens": ["Welkt", "wie", "all", "sein", "Bl\u00fc\u00b7ten\u00b7blust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Herbstes rote Fr\u00fcchte auch \u2013", "tokens": ["Herbs\u00b7tes", "ro\u00b7te", "Fr\u00fcch\u00b7te", "auch", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Rot von Tau und Nebelrauch \u2013", "tokens": ["Rot", "von", "Tau", "und", "Ne\u00b7bel\u00b7rauch", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Sind dir \u00dcberdru\u00df. Was nun?", "tokens": ["Sind", "dir", "\u00dc\u00b7berd\u00b7ru\u00df", ".", "Was", "nun", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$.", "PWS", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Still am Herde sollst du ruhn,", "tokens": ["Still", "am", "Her\u00b7de", "sollst", "du", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Wenn die Glut zu Glanz entfacht", "tokens": ["Wenn", "die", "Glut", "zu", "Glanz", "ent\u00b7facht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Geistert durch die Winternacht.", "tokens": ["Geis\u00b7tert", "durch", "die", "Win\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Wenn die Erde stumm und kalt", "tokens": ["Wenn", "die", "Er\u00b7de", "stumm", "und", "kalt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Und der Schnee sich klebrig ballt", "tokens": ["Und", "der", "Schnee", "sich", "kleb\u00b7rig", "ballt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PRF", "ADJD", "VVFIN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.21": {"text": "Um des Bauern plumpen Schuh,", "tokens": ["Um", "des", "Bau\u00b7ern", "plum\u00b7pen", "Schuh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Nacht sich dehnt der Mittnacht zu", "tokens": ["Nacht", "sich", "dehnt", "der", "Mitt\u00b7nacht", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "VVFIN", "ART", "NN", "PTKZU"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Und aus ihrem Dunkelland", "tokens": ["Und", "aus", "ih\u00b7rem", "Dun\u00b7kel\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Alles Wirkliche verbannt,", "tokens": ["Al\u00b7les", "Wirk\u00b7li\u00b7che", "ver\u00b7bannt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Ruhe dann und la\u00df von hinnen \u2013", "tokens": ["Ru\u00b7he", "dann", "und", "la\u00df", "von", "hin\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "VVIMP", "APPR", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Ehrfurcht leite dies Beginnen \u2013", "tokens": ["Ehr\u00b7furcht", "lei\u00b7te", "dies", "Be\u00b7gin\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PDS", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Phantasie zu hohem Flug!", "tokens": ["Phan\u00b7ta\u00b7sie", "zu", "ho\u00b7hem", "Flug", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Genien dienen ihr genug.", "tokens": ["Ge\u00b7ni\u00b7en", "die\u00b7nen", "ihr", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Winter wei\u00df nur Frost zu weben \u2013", "tokens": ["Win\u00b7ter", "wei\u00df", "nur", "Frost", "zu", "we\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Alles bringt sie wieder dar:", "tokens": ["Al\u00b7les", "bringt", "sie", "wie\u00b7der", "dar", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Sommer, der dir gl\u00fchend war,", "tokens": ["Som\u00b7mer", ",", "der", "dir", "gl\u00fc\u00b7hend", "war", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "All des Maimonds Bl\u00fctenlast,", "tokens": ["All", "des", "Mai\u00b7monds", "Bl\u00fc\u00b7ten\u00b7last", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Tauigen Stiel und dornigen Ast;", "tokens": ["Tau\u00b7i\u00b7gen", "Stiel", "und", "dor\u00b7ni\u00b7gen", "Ast", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.34": {"text": "All des Herbstes reifen Segen,", "tokens": ["All", "des", "Herbs\u00b7tes", "rei\u00b7fen", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Frucht und Duft und sanften Regen,", "tokens": ["Frucht", "und", "Duft", "und", "sanf\u00b7ten", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Mischt sie dir zu seligem Trank \u2013", "tokens": ["Mischt", "sie", "dir", "zu", "se\u00b7li\u00b7gem", "Trank", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.37": {"text": "Schl\u00fcrfe ihn und sag ihr Dank.", "tokens": ["Schl\u00fcr\u00b7fe", "ihn", "und", "sag", "ihr", "Dank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Schl\u00fcrfe ihn \u2013 und zu dir zieht,", "tokens": ["Schl\u00fcr\u00b7fe", "ihn", "\u2013", "und", "zu", "dir", "zieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "KON", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Ferneher ein Erntelied;", "tokens": ["Fer\u00b7ne\u00b7her", "ein", "Ern\u00b7te\u00b7lied", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "Reife Halme h\u00f6rst du fallen,", "tokens": ["Rei\u00b7fe", "Hal\u00b7me", "h\u00f6rst", "du", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "H\u00f6rst den Sang der Nachtigallen,", "tokens": ["H\u00f6rst", "den", "Sang", "der", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Lerchenlust, die im April", "tokens": ["Ler\u00b7chen\u00b7lust", ",", "die", "im", "Ap\u00b7ril"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Nie den Jubel enden will;", "tokens": ["Nie", "den", "Ju\u00b7bel", "en\u00b7den", "will", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.44": {"text": "H\u00f6rst den rauhen Ruf der Kr\u00e4hen,", "tokens": ["H\u00f6rst", "den", "rau\u00b7hen", "Ruf", "der", "Kr\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Die nach Halm und Reisig sp\u00e4hen,", "tokens": ["Die", "nach", "Halm", "und", "Rei\u00b7sig", "sp\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Und du siehst im ersten Gr\u00fcn", "tokens": ["Und", "du", "siehst", "im", "ers\u00b7ten", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.47": {"text": "Enzian und Primeln bl\u00fchn,", "tokens": ["En\u00b7zi\u00b7an", "und", "Pri\u00b7meln", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.48": {"text": "Lilien in wei\u00dfer Pracht,", "tokens": ["Li\u00b7li\u00b7en", "in", "wei\u00b7\u00dfer", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.49": {"text": "Rose, die zur Sonne lacht,", "tokens": ["Ro\u00b7se", ",", "die", "zur", "Son\u00b7ne", "lacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.50": {"text": "Und das mailiche Frohlocken", "tokens": ["Und", "das", "mai\u00b7li\u00b7che", "Froh\u00b7lo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Blauer Hyazinthenglocken,", "tokens": ["Blau\u00b7er", "Hy\u00b7a\u00b7zin\u00b7then\u00b7glo\u00b7cken", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Zweige, Bl\u00e4tter, Bl\u00fctentaschen,", "tokens": ["Zwei\u00b7ge", ",", "Bl\u00e4t\u00b7ter", ",", "Bl\u00fc\u00b7ten\u00b7ta\u00b7schen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.53": {"text": "Die der Regen blank gewaschen.", "tokens": ["Die", "der", "Re\u00b7gen", "blank", "ge\u00b7wa\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Siehst die Feldmaus, die erwacht", "tokens": ["Siehst", "die", "Feld\u00b7maus", ",", "die", "er\u00b7wacht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.55": {"text": "Lugt aus ihrem Winterschacht,", "tokens": ["Lugt", "aus", "ih\u00b7rem", "Win\u00b7ter\u00b7schacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.56": {"text": "Schlange, die vom Schlafen mager,", "tokens": ["Schlan\u00b7ge", ",", "die", "vom", "Schla\u00b7fen", "ma\u00b7ger", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.57": {"text": "Lauert im durchsonnten Lager;", "tokens": ["Lau\u00b7ert", "im", "durch\u00b7sonn\u00b7ten", "La\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"text": "Siehst den Dornbusch Nestchen wiegen,", "tokens": ["Siehst", "den", "Dorn\u00b7busch", "Nest\u00b7chen", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.59": {"text": "Drin gefleckte Eier liegen,", "tokens": ["Drin", "ge\u00b7fleck\u00b7te", "Ei\u00b7er", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.60": {"text": "Und im moosigen Bett versteckt", "tokens": ["Und", "im", "moo\u00b7si\u00b7gen", "Bett", "ver\u00b7steckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVPP"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.61": {"text": "Feldhuhn, das die Fl\u00fcgel streckt.", "tokens": ["Feld\u00b7huhn", ",", "das", "die", "Fl\u00fc\u00b7gel", "streckt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.62": {"text": "H\u00f6rst die Bienen, die im Gr\u00fcn", "tokens": ["H\u00f6rst", "die", "Bie\u00b7nen", ",", "die", "im", "Gr\u00fcn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.63": {"text": "Summend hin und wieder ziehn,", "tokens": ["Sum\u00b7mend", "hin", "und", "wie\u00b7der", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.64": {"text": "Eicheln, die zu Boden schlagen,", "tokens": ["Ei\u00b7cheln", ",", "die", "zu", "Bo\u00b7den", "schla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.65": {"text": "Und des Herbstwinds Sang und Klagen.", "tokens": ["Und", "des", "Herbst\u00b7winds", "Sang", "und", "Kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "S\u00fc\u00dfe Phantasie, la\u00df frei!", "tokens": ["S\u00fc\u00b7\u0