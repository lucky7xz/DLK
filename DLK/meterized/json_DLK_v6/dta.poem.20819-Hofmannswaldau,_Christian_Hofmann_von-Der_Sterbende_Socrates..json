{"dta.poem.20819": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Als nun der Tag zum letzten selbst war kommen/", "tokens": ["Als", "nun", "der", "Tag", "zum", "letz\u00b7ten", "selbst", "war", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPRART", "ADJA", "ADV", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df dieser Schatz uns solte seyn benommen/", "tokens": ["Da\u00df", "die\u00b7ser", "Schatz", "uns", "sol\u00b7te", "seyn", "be\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PPER", "VMFIN", "PPOSAT", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und durch den Spruch/ den das Verh\u00e4ngnis macht/", "tokens": ["Und", "durch", "den", "Spruch", "/", "den", "das", "Ver\u00b7h\u00e4ng\u00b7nis", "macht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$(", "ART", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem Cerberus sein Bissen war gebracht/", "tokens": ["Dem", "Cer\u00b7be\u00b7rus", "sein", "Bis\u00b7sen", "war", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den nicht vielmehr der sch\u00f6ne Geist beweget/", "tokens": ["Den", "nicht", "viel\u00b7mehr", "der", "sch\u00f6\u00b7ne", "Geist", "be\u00b7we\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als etwan der so Thorheits Flecken tr\u00e4get/", "tokens": ["Als", "et\u00b7wan", "der", "so", "Thor\u00b7heits", "Fle\u00b7cken", "tr\u00e4\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADV", "NN", "NN", "VVFIN", "$("], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "So nahmen wir auch die Gelegenheit:", "tokens": ["So", "nah\u00b7men", "wir", "auch", "die", "Ge\u00b7le\u00b7gen\u00b7heit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Erbaut zu seyn durch seine Freundligkeit.", "tokens": ["Er\u00b7baut", "zu", "seyn", "durch", "sei\u00b7ne", "Freund\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VAINF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Den Heldenmuth nam keine Furcht gefangen/", "tokens": ["Den", "Hel\u00b7den\u00b7muth", "nam", "kei\u00b7ne", "Furcht", "ge\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wie nah ihm auch das Ende kam gegangen.", "tokens": ["Wie", "nah", "ihm", "auch", "das", "En\u00b7de", "kam", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "ART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Er ward auch nicht durch Todes-Angst ger\u00fchrt/", "tokens": ["Er", "ward", "auch", "nicht", "durch", "To\u00b7des\u00b7Angst", "ge\u00b7r\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "So uns gar leicht Witz und Verstand entf\u00fchrt/", "tokens": ["So", "uns", "gar", "leicht", "Witz", "und", "Ver\u00b7stand", "ent\u00b7f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "ADJD", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Wer ist doch sonst/ wann er den Tag erblicket/", "tokens": ["Wer", "ist", "doch", "sonst", "/", "wann", "er", "den", "Tag", "er\u00b7bli\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "$(", "PWAV", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Durch den der Leib wird aus der Welt ger\u00fccket/", "tokens": ["Durch", "den", "der", "Leib", "wird", "aus", "der", "Welt", "ge\u00b7r\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Er sey denn gantz ein Engel oder Stein/", "tokens": ["Er", "sey", "denn", "gantz", "ein", "En\u00b7gel", "o\u00b7der", "Stein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Da\u00df er nicht solt in Angst und Schrecken seyn?", "tokens": ["Da\u00df", "er", "nicht", "solt", "in", "Angst", "und", "Schre\u00b7cken", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VMFIN", "APPR", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Drum hat gewi\u00df der Engel reines Wesen", "tokens": ["Drum", "hat", "ge\u00b7wi\u00df", "der", "En\u00b7gel", "rei\u00b7nes", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Den Socrates erzeuget und erlesen;", "tokens": ["Den", "So\u00b7cra\u00b7tes", "er\u00b7zeu\u00b7get", "und", "er\u00b7le\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "KON", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.19": {"text": "Denn unser Sinn ist warlich allzuschlecht/", "tokens": ["Denn", "un\u00b7ser", "Sinn", "ist", "war\u00b7lich", "all\u00b7zu\u00b7schlecht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Und h\u00f6rt mit Angst des Todes strenges Recht/", "tokens": ["Und", "h\u00f6rt", "mit", "Angst", "des", "To\u00b7des", "stren\u00b7ges", "Recht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Jhm aber war es leicht alhier zu weisen/", "tokens": ["Jhm", "a\u00b7ber", "war", "es", "leicht", "al\u00b7hier", "zu", "wei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "ADJD", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Ein Hertz aus Stein/ und einen Mund von Eisen/", "tokens": ["Ein", "Hertz", "aus", "Stein", "/", "und", "ei\u00b7nen", "Mund", "von", "Ei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$(", "KON", "ART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Es zeiget uns Gesichte/ Hand und Mund/", "tokens": ["Es", "zei\u00b7get", "uns", "Ge\u00b7sich\u00b7te", "/", "Hand", "und", "Mund", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Wie Geist und Sinn recht im Gewichte stund/", "tokens": ["Wie", "Geist", "und", "Sinn", "recht", "im", "Ge\u00b7wich\u00b7te", "stund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "ADJD", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Und da\u00df man ihn weit h\u00f6her muste plagen", "tokens": ["Und", "da\u00df", "man", "ihn", "weit", "h\u00f6\u00b7her", "mus\u00b7te", "pla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "PPER", "ADJD", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Der Thr\u00e4nen Bach den Wangen abzujagen;", "tokens": ["Der", "Thr\u00e4\u00b7nen", "Bach", "den", "Wan\u00b7gen", "ab\u00b7zu\u00b7ja\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Die Seufzer selbst so allzuleicht entgehn/", "tokens": ["Die", "Seuf\u00b7zer", "selbst", "so", "all\u00b7zu\u00b7leicht", "ent\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Die musten ihm stets zu gebote stehn/", "tokens": ["Die", "mus\u00b7ten", "ihm", "stets", "zu", "ge\u00b7bo\u00b7te", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "APPR", "ADJA", "VVINF", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.29": {"text": "Sein treues Weib lag auch zu seinen F\u00fcssen", "tokens": ["Sein", "treu\u00b7es", "Weib", "lag", "auch", "zu", "sei\u00b7nen", "F\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Verblast/ best\u00fcrtzt/ erb\u00e4rmlich abgerissen/", "tokens": ["Ver\u00b7blast", "/", "be\u00b7st\u00fcrtzt", "/", "er\u00b7b\u00e4rm\u00b7lich", "ab\u00b7ge\u00b7ris\u00b7sen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "VVPP", "$(", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Sie konte nicht der Uberma\u00df der Pein", "tokens": ["Sie", "kon\u00b7te", "nicht", "der", "U\u00b7ber\u00b7ma\u00df", "der", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Durch ihr Geschrey wie sonst erleichtert seyn/", "tokens": ["Durch", "ihr", "Ge\u00b7schrey", "wie", "sonst", "er\u00b7leich\u00b7tert", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KOKOM", "ADV", "VVPP", "VAINF", "$("], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.33": {"text": "Sie hielt den Sohn des Socrates in Armen/", "tokens": ["Sie", "hielt", "den", "Sohn", "des", "So\u00b7cra\u00b7tes", "in", "Ar\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "NN", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.34": {"text": "Und predigt\u2019 ihm vom schl\u00e4frigen Erbarmen/", "tokens": ["Und", "pre\u00b7digt'", "ihm", "vom", "schl\u00e4f\u00b7ri\u00b7gen", "Er\u00b7bar\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Als welcher nicht vor dieser letzten Fahrt", "tokens": ["Als", "wel\u00b7cher", "nicht", "vor", "die\u00b7ser", "letz\u00b7ten", "Fahrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRELS", "PTKNEG", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Ein Seuffzerlein zu dem Ged\u00e4chtni\u00df ward.", "tokens": ["Ein", "Seuff\u00b7zer\u00b7lein", "zu", "dem", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "ward", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Mein werther Mann begunte sie zu sagen", "tokens": ["Mein", "wert\u00b7her", "Mann", "be\u00b7gun\u00b7te", "sie", "zu", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Ach w\u00fcrden wir zugleiche hingetragen/", "tokens": ["Ach", "w\u00fcr\u00b7den", "wir", "zu\u00b7glei\u00b7che", "hin\u00b7ge\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Ach weh! wann dir die allzulange Ruh/", "tokens": ["Ach", "weh", "!", "wann", "dir", "die", "all\u00b7zu\u00b7lan\u00b7ge", "Ruh", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$.", "PWAV", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Das reine Licht der Augen dr\u00fccket zu/", "tokens": ["Das", "rei\u00b7ne", "Licht", "der", "Au\u00b7gen", "dr\u00fc\u00b7cket", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "PTKZU", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "So wirst du denn bey des Cocytus Fl\u00fcssen/", "tokens": ["So", "wirst", "du", "denn", "bey", "des", "Co\u00b7cy\u00b7tus", "Fl\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "ART", "NE", "NN", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.42": {"text": "Gar schlechten Schein der Liebligkeit geniessen;", "tokens": ["Gar", "schlech\u00b7ten", "Schein", "der", "Lieb\u00b7lig\u00b7keit", "ge\u00b7nies\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Und w\u00e4ren wir gleich allesambt um dich/", "tokens": ["Und", "w\u00e4\u00b7ren", "wir", "gleich", "al\u00b7le\u00b7sambt", "um", "dich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "So kentest du die Freunde gleich wie mich.", "tokens": ["So", "ken\u00b7test", "du", "die", "Freun\u00b7de", "gleich", "wie", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}