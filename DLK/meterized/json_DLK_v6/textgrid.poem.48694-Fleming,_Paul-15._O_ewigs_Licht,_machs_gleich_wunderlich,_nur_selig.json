{"textgrid.poem.48694": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "15. O ewigs Licht, machs gleich wunderlich, nur selig", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geu\u00df deinen Eifer aus mit Kr\u00fcgen und mit Mulden,", "tokens": ["Geu\u00df", "dei\u00b7nen", "Ei\u00b7fer", "aus", "mit", "Kr\u00fc\u00b7gen", "und", "mit", "Mul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "zeuch alle deinen Ernst zusammen wider mich,", "tokens": ["zeuch", "al\u00b7le", "dei\u00b7nen", "Ernst", "zu\u00b7sam\u00b7men", "wi\u00b7der", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "PPOSAT", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "zermalme meinen Leib und stell dich wider mich,", "tokens": ["zer\u00b7mal\u00b7me", "mei\u00b7nen", "Leib", "und", "stell", "dich", "wi\u00b7der", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "ADJD", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "verschleu\u00df mir ganz und gar die Kammern deiner Hulden!", "tokens": ["ver\u00b7schleu\u00df", "mir", "ganz", "und", "gar", "die", "Kam\u00b7mern", "dei\u00b7ner", "Hul\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "KON", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein H\u00f6h'res noch hab' ich verb\u00fchrt mit meinen Schulden.", "tokens": ["Ein", "H\u00f6h'\u00b7res", "noch", "hab'", "ich", "ver\u00b7b\u00fchrt", "mit", "mei\u00b7nen", "Schul\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mach meiner Seelen Angst, sto\u00df meinen Geist und sprich:", "tokens": ["Mach", "mei\u00b7ner", "See\u00b7len", "Angst", ",", "sto\u00df", "mei\u00b7nen", "Geist", "und", "sprich", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$,", "VVFIN", "PPOSAT", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hin, wo man ewig weint und siehet keinen Stich!", "tokens": ["Hin", ",", "wo", "man", "e\u00b7wig", "weint", "und", "sie\u00b7het", "kei\u00b7nen", "Stich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PIS", "ADJD", "VVFIN", "KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Di\u00df Alles bin ich wert und mehr noch zu erdulden.", "tokens": ["Di\u00df", "Al\u00b7les", "bin", "ich", "wert", "und", "mehr", "noch", "zu", "er\u00b7dul\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "PPER", "ADJD", "KON", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zu mitten dieses Zorns so denk auch deiner Gnaden,", "tokens": ["Zu", "mit\u00b7ten", "die\u00b7ses", "Zorns", "so", "denk", "auch", "dei\u00b7ner", "Gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PDAT", "NN", "ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da\u00df, wenn du mich bringst um, du dir nicht selbst tust Schaden,", "tokens": ["da\u00df", ",", "wenn", "du", "mich", "bringst", "um", ",", "du", "dir", "nicht", "selbst", "tust", "Scha\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "PRF", "VVFIN", "PTKVZ", "$,", "PPER", "PPER", "PTKNEG", "ADV", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "schau meinen Zaler an, denn salb du deinen Sohn.", "tokens": ["schau", "mei\u00b7nen", "Za\u00b7ler", "an", ",", "denn", "salb", "du", "dei\u00b7nen", "Sohn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Tu Recht, Gerechtigkeit! Was wilst du an mein Leben?", "tokens": ["Tu", "Recht", ",", "Ge\u00b7rech\u00b7tig\u00b7keit", "!", "Was", "wilst", "du", "an", "mein", "Le\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$.", "PWS", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er hat f\u00fcr mich an dich mehr, als ich soll, gegeben,", "tokens": ["Er", "hat", "f\u00fcr", "mich", "an", "dich", "mehr", ",", "als", "ich", "soll", ",", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "APPR", "PPER", "ADV", "$,", "KOUS", "PPER", "VMFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df auch f\u00fcr meine Schuld der Himmel sei mein Lohn.", "tokens": ["da\u00df", "auch", "f\u00fcr", "mei\u00b7ne", "Schuld", "der", "Him\u00b7mel", "sei", "mein", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}