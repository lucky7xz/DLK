, "mei\u00b7ne", "lie\u00b7be", "Toch\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich kauf dir ein paar Str\u00fcmpf.", "tokens": ["Ich", "kauf", "dir", "ein", "paar", "Str\u00fcmpf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ja, ja meine liebe Mutter,", "tokens": ["Ja", ",", "ja", "mei\u00b7ne", "lie\u00b7be", "Mut\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6n Zwicklen darin;", "tokens": ["Sch\u00f6n", "Zwick\u00b7len", "da\u00b7rin", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "PAV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Kann wahrlich nicht spinnen,", "tokens": ["Kann", "wahr\u00b7lich", "nicht", "spin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Von wegen meinem Finger,", "tokens": ["Von", "we\u00b7gen", "mei\u00b7nem", "Fin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mein Finger thut weh.", "tokens": ["Mein", "Fin\u00b7ger", "thut", "weh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Spinn, spinn, meine liebe Tochter,", "t