{"dta.poem.2191": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der N\u00e4chster.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was hei\u00dfet Seyn? ich fass' es nicht.", "tokens": ["Was", "hei\u00b7\u00dfet", "Seyn", "?", "ich", "fass'", "es", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch giebt mir die Vernunft so viel Bericht:", "tokens": ["Doch", "giebt", "mir", "die", "Ver\u00b7nunft", "so", "viel", "Be\u00b7richt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df man das Seyn allein", "tokens": ["Da\u00df", "man", "das", "Seyn", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Von Gott mit Wahrheit sagen k\u00f6nne.", "tokens": ["Von", "Gott", "mit", "Wahr\u00b7heit", "sa\u00b7gen", "k\u00f6n\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Auch giebt die Schrift den Unterricht,", "tokens": ["Auch", "giebt", "die", "Schrift", "den", "Un\u00b7ter\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df Er: Ich bin der, der Ich bin, Sich nenne.", "tokens": ["Da\u00df", "Er", ":", "Ich", "bin", "der", ",", "der", "Ich", "bin", ",", "Sich", "nen\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$.", "PPER", "VAFIN", "ART", "$,", "PRELS", "PPER", "VAFIN", "$,", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Kann eine Kreatur denn seyn?", "tokens": ["Kann", "ei\u00b7ne", "Kre\u00b7a\u00b7tur", "denn", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "KON", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nach meiner Meynung sag ich: Nein.", "tokens": ["Nach", "mei\u00b7ner", "Mey\u00b7nung", "sag", "ich", ":", "Nein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$.", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und w\u00fcrd\u2019 ich denen, die mich fragen,", "tokens": ["Und", "w\u00fcrd'", "ich", "de\u00b7nen", ",", "die", "mich", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PDS", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Viel eh\u2019, als da\u00df wir seyn, da\u00df wir nur werden, sagen.", "tokens": ["Viel", "eh'", ",", "als", "da\u00df", "wir", "seyn", ",", "da\u00df", "wir", "nur", "wer\u00b7den", ",", "sa\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOKOM", "KOUS", "PPER", "VAINF", "$,", "KOUS", "PPER", "ADV", "VAINF", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}