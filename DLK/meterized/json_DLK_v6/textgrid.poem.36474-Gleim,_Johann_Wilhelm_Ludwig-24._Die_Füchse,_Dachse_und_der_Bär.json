{"textgrid.poem.36474": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "24. Die F\u00fcchse, Dachse und der B\u00e4r", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbden L\u00f6wen mit der gro\u00dfen Seele,", "tokens": ["\u00bb", "den", "L\u00f6\u00b7wen", "mit", "der", "gro\u00b7\u00dfen", "See\u00b7le", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den k\u00f6nnt ihr wohl,", "tokens": ["Den", "k\u00f6nnt", "ihr", "wohl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ihr kleinen Gecken,", "tokens": ["Ihr", "klei\u00b7nen", "Ge\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ein wenig necken!", "tokens": ["Ein", "we\u00b7nig", "ne\u00b7cken", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Da seht! er liegt in seiner H\u00f6hle,", "tokens": ["Da", "seht", "!", "er", "liegt", "in", "sei\u00b7ner", "H\u00f6h\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So gro\u00dfmutsvoll!", "tokens": ["So", "gro\u00df\u00b7muts\u00b7voll", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und macht, ein Weiser und ein K\u00f6nig,", "tokens": ["Und", "macht", ",", "ein", "Wei\u00b7ser", "und", "ein", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Aus eurem Spott, und eurem Schimpf sich wenig,", "tokens": ["Aus", "eu\u00b7rem", "Spott", ",", "und", "eu\u00b7rem", "Schimpf", "sich", "we\u00b7nig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ihr k\u00f6nnt's ja wohl!", "tokens": ["Ihr", "k\u00f6nnt's", "ja", "wohl", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "W\u00e4r' aber unter euch ein Tiger,", "tokens": ["W\u00e4r'", "a\u00b7ber", "un\u00b7ter", "euch", "ein", "Ti\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und mutig und noch jung,", "tokens": ["Und", "mu\u00b7tig", "und", "noch", "jung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Dann th\u00e4t er einen Sprung", "tokens": ["Dann", "th\u00e4t", "er", "ei\u00b7nen", "Sprung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Aus seiner H\u00f6hle, w\u00fcrde Sieger,", "tokens": ["Aus", "sei\u00b7ner", "H\u00f6h\u00b7le", ",", "w\u00fcr\u00b7de", "Sie\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und ihr, ihr kleinen Gecken,", "tokens": ["Und", "ihr", ",", "ihr", "klei\u00b7nen", "Ge\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Entfl\u00f6ht in Dornen und in Hecken!\u00ab", "tokens": ["Ent\u00b7fl\u00f6ht", "in", "Dor\u00b7nen", "und", "in", "He\u00b7cken", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zu F\u00fcchsen und zu Dachsen die umher", "tokens": ["Zu", "F\u00fcch\u00b7sen", "und", "zu", "Dach\u00b7sen", "die", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Um des Monarchen H\u00f6hle standen,", "tokens": ["Um", "des", "Mon\u00b7ar\u00b7chen", "H\u00f6h\u00b7le", "stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sprach's in entfernten Landen,", "tokens": ["Sprach's", "in", "ent\u00b7fern\u00b7ten", "Lan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Kanzler B\u00e4r.", "tokens": ["Der", "Kanz\u00b7ler", "B\u00e4r", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}