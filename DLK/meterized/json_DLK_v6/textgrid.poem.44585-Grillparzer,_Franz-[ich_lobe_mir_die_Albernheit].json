{"textgrid.poem.44585": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[ich lobe mir die Albernheit]", "genre": "verse", "period": "N.A.", "pub_year": 1851, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich lobe mir die Albernheit,", "tokens": ["Ich", "lo\u00b7be", "mir", "die", "Al\u00b7bern\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will sagen: die gesunde,", "tokens": ["Will", "sa\u00b7gen", ":", "die", "ge\u00b7sun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$.", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie liegt, fast wie der Stoff dem Kleid,", "tokens": ["Sie", "liegt", ",", "fast", "wie", "der", "Stoff", "dem", "Kleid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Selbst unserm Geist zugrunde,", "tokens": ["Selbst", "un\u00b7serm", "Geist", "zu\u00b7grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie glaubt, was eben jeder glaubt,", "tokens": ["Sie", "glaubt", ",", "was", "e\u00b7ben", "je\u00b7der", "glaubt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie denkt, was alle denken,", "tokens": ["Sie", "denkt", ",", "was", "al\u00b7le", "den\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie tut, was jedermann erlaubt,", "tokens": ["Sie", "tut", ",", "was", "je\u00b7der\u00b7mann", "er\u00b7laubt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Zweifel wird sie kr\u00e4nken.", "tokens": ["Kein", "Zwei\u00b7fel", "wird", "sie", "kr\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So lebt sie einfach, schlicht und still,", "tokens": ["So", "lebt", "sie", "ein\u00b7fach", ",", "schlicht", "und", "still", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht streitig und nicht w\u00e4hlig,", "tokens": ["Nicht", "strei\u00b7tig", "und", "nicht", "w\u00e4h\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "KON", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn der Tod nun kommen will,", "tokens": ["Und", "wenn", "der", "Tod", "nun", "kom\u00b7men", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird etwa gar sie selig.", "tokens": ["Wird", "et\u00b7wa", "gar", "sie", "se\u00b7lig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Allein usw.", "tokens": ["Al\u00b7lein", "us\u00b7w."], "token_info": ["word", "abbreviation"], "pos": ["ADV", "NE"], "meter": "-+", "measure": "iambic.single"}}}}}