{"textgrid.poem.67607": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Habicht scho\u00df im sch\u00f6nsten Schall", "tokens": ["Der", "Ha\u00b7bicht", "scho\u00df", "im", "sch\u00f6ns\u00b7ten", "Schall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der kleinen Nachtigall", "tokens": ["Der", "klei\u00b7nen", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hinab und bi\u00df sie todt.", "tokens": ["Hin\u00b7ab", "und", "bi\u00df", "sie", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "\u00bbdu schlechter Freund der Virtuosen!\u00ab", "tokens": ["\u00bb", "du", "schlech\u00b7ter", "Freund", "der", "Vir\u00b7tu\u00b7o\u00b7sen", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sprach K\u00f6nig Aar. Der aber ward nicht roth:", "tokens": ["Sprach", "K\u00f6\u00b7nig", "Aar", ".", "Der", "a\u00b7ber", "ward", "nicht", "roth", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NE", "$.", "ART", "ADV", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "\u00bbwas? liebzukosen", "tokens": ["\u00bb", "was", "?", "lieb\u00b7zu\u00b7ko\u00b7sen"], "token_info": ["punct", "word", "punct", "word"], "pos": ["$(", "PWS", "$.", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "In Hungersnoth?\u00ab", "tokens": ["In", "Hun\u00b7gers\u00b7noth", "?", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["APPR", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Was werden alle Musen neun", "tokens": ["Was", "wer\u00b7den", "al\u00b7le", "Mu\u00b7sen", "neun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PIAT", "NN", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Gott Apollo h\u00f6chstmit sein,", "tokens": ["Und", "Gott", "A\u00b7pol\u00b7lo", "h\u00f6chst\u00b7mit", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NE", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn unsre Kinder Hungers schrein", "tokens": ["Wenn", "uns\u00b7re", "Kin\u00b7der", "Hun\u00b7gers", "schrein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und unsre L\u00e4nder darben", "tokens": ["Und", "uns\u00b7re", "L\u00e4n\u00b7der", "dar\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und unsre Prasser starben?", "tokens": ["Und", "uns\u00b7re", "Pras\u00b7ser", "star\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}