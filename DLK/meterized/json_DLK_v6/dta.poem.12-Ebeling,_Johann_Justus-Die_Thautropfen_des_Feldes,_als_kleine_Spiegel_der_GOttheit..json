{"dta.poem.12": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n Thautropfen des Feldes,  \n als  \n  kleine Spiegel der GOttheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Thau das Kind der Morgenr\u00f6the,", "tokens": ["Der", "Thau", "das", "Kind", "der", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Liegt auf der gr\u00fcnen Feld-Tapete", "tokens": ["Liegt", "auf", "der", "gr\u00fc\u00b7nen", "Feld\u00b7Ta\u00b7pe\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und schimmert in den klaren Schein,", "tokens": ["Und", "schim\u00b7mert", "in", "den", "kla\u00b7ren", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als wenn die Tropfen Perlen seyn.", "tokens": ["Als", "wenn", "die", "Trop\u00b7fen", "Per\u00b7len", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O! welch ein Anblik g\u00fcldner Wonne", "tokens": ["O", "!", "welch", "ein", "An\u00b7blik", "g\u00fcld\u00b7ner", "Won\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PWAT", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Entstehet, wenn das Licht der Sonne,", "tokens": ["Ent\u00b7ste\u00b7het", ",", "wenn", "das", "Licht", "der", "Son\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "In dessen hellen R\u00fcnde strahlt:", "tokens": ["In", "des\u00b7sen", "hel\u00b7len", "R\u00fcn\u00b7de", "strahlt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mir deucht ich seh dein Angesichte,", "tokens": ["Mir", "deucht", "ich", "seh", "dein", "An\u00b7ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "O ", "tokens": ["O"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Als wie im Spiegel abgemahlt.", "tokens": ["Als", "wie", "im", "Spie\u00b7gel", "ab\u00b7ge\u00b7mahlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es wird der Thau aus Dunst gebohren,", "tokens": ["Es", "wird", "der", "Thau", "aus", "Dunst", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der in der k\u00fchlen Nacht gefroren,", "tokens": ["Der", "in", "der", "k\u00fch\u00b7len", "Nacht", "ge\u00b7fro\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und rollend sich in Tr\u00f6pfgen senkt,", "tokens": ["Und", "rol\u00b7lend", "sich", "in", "Tr\u00f6pf\u00b7gen", "senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Morgens gr\u00fcne Saaten tr\u00e4nkt.", "tokens": ["Des", "Mor\u00b7gens", "gr\u00fc\u00b7ne", "Saa\u00b7ten", "tr\u00e4nkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es strahlt aus dieser klaren N\u00e4sse,", "tokens": ["Es", "strahlt", "aus", "die\u00b7ser", "kla\u00b7ren", "N\u00e4s\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Sch\u00f6pfer! deine Wundergr\u00f6sse,", "tokens": ["Mein", "Sch\u00f6p\u00b7fer", "!", "dei\u00b7ne", "Wun\u00b7der\u00b7gr\u00f6s\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die sich in das Gem\u00fcte dr\u00fckt:", "tokens": ["Die", "sich", "in", "das", "Ge\u00b7m\u00fc\u00b7te", "dr\u00fckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und sehen die entz\u00fckten Sinnen,", "tokens": ["Und", "se\u00b7hen", "die", "ent\u00b7z\u00fck\u00b7ten", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "So viele Cristallinen rinnen:", "tokens": ["So", "vie\u00b7le", "Cris\u00b7tal\u00b7li\u00b7nen", "rin\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "So deucht uns, da\u00df wir dich erblikt.", "tokens": ["So", "deucht", "uns", ",", "da\u00df", "wir", "dich", "er\u00b7blikt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es l\u00e4st uns jeder Tropfe lesen,", "tokens": ["Es", "l\u00e4st", "uns", "je\u00b7der", "Trop\u00b7fe", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du seist ein liebreich g\u00fctig Wesen,", "tokens": ["Du", "seist", "ein", "lieb\u00b7reich", "g\u00fc\u00b7tig", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df durch des Thaues Balsamkraft,", "tokens": ["Da\u00df", "durch", "des", "Thau\u00b7es", "Bal\u00b7sam\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Saaten frische Nahrung schaft.", "tokens": ["Den", "Saa\u00b7ten", "fri\u00b7sche", "Nah\u00b7rung", "schaft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erblikket ein ger\u00fchrt Gem\u00fcte,", "tokens": ["Er\u00b7blik\u00b7ket", "ein", "ge\u00b7r\u00fchrt", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die klaren Spiegel deiner ", "tokens": ["Die", "kla\u00b7ren", "Spie\u00b7gel", "dei\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So wird es inniglich erfreut;", "tokens": ["So", "wird", "es", "in\u00b7nig\u00b7lich", "er\u00b7freut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es deucht uns da\u00df dein Gnadenseegen,", "tokens": ["Es", "deucht", "uns", "da\u00df", "dein", "Gna\u00b7den\u00b7see\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wenn sich die Tropfen wimmernd regen,", "tokens": ["Wenn", "sich", "die", "Trop\u00b7fen", "wim\u00b7mernd", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Das Feld mit Manna \u00fcberstreut.", "tokens": ["Das", "Feld", "mit", "Man\u00b7na", "\u00fc\u00b7bers\u00b7treut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie wunderbahr wird man entz\u00fckket,", "tokens": ["Wie", "wun\u00b7der\u00b7bahr", "wird", "man", "ent\u00b7z\u00fck\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PIS", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wenn sich die Sonn darin abdr\u00fckket,", "tokens": ["Wenn", "sich", "die", "Sonn", "da\u00b7rin", "ab\u00b7dr\u00fck\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "PAV", "VVFIN", "$,"], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und durch die Strahlenreiche Pracht,", "tokens": ["Und", "durch", "die", "Strah\u00b7len\u00b7rei\u00b7che", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Thau zu kleinen Sonnen macht,", "tokens": ["Den", "Thau", "zu", "klei\u00b7nen", "Son\u00b7nen", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Silbertropfen \u00fcberg\u00fcldet,", "tokens": ["Die", "Sil\u00b7ber\u00b7trop\u00b7fen", "\u00fc\u00b7berg\u00b7\u00fcl\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So mannigfaltig f\u00e4rbt und bildet.", "tokens": ["So", "man\u00b7nig\u00b7fal\u00b7tig", "f\u00e4rbt", "und", "bil\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "O HErr! wenn dies das Aug erwegt;", "tokens": ["O", "Herr", "!", "wenn", "dies", "das", "Aug", "er\u00b7wegt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "KOUS", "PDS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So wird dem Geist durch Thau-Cristallen,", "tokens": ["So", "wird", "dem", "Geist", "durch", "Thau\u00b7Cris\u00b7tal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und durch ihr gl\u00e4nzend Wiederprallen,", "tokens": ["Und", "durch", "ihr", "gl\u00e4n\u00b7zend", "Wie\u00b7der\u00b7pral\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.5": {"line.1": {"text": "Wenn man mit Andacht \u00fcberdenket,", "tokens": ["Wenn", "man", "mit", "An\u00b7dacht", "\u00fc\u00b7ber\u00b7den\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie feuchter Thau die Felder tr\u00e4nket,", "tokens": ["Wie", "feuch\u00b7ter", "Thau", "die", "Fel\u00b7der", "tr\u00e4n\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie sein klebricht fettes Na\u00df,", "tokens": ["Und", "wie", "sein", "kleb\u00b7richt", "fet\u00b7tes", "Na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erfrischt das aufgekeimmte Gra\u00df:", "tokens": ["Er\u00b7frischt", "das", "auf\u00b7ge\u00b7keimm\u00b7te", "Gra\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So sehn wir auf den gr\u00fcnen Fluren,", "tokens": ["So", "sehn", "wir", "auf", "den", "gr\u00fc\u00b7nen", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "In allen Furchen eingedr\u00fckt;", "tokens": ["In", "al\u00b7len", "Fur\u00b7chen", "ein\u00b7ge\u00b7dr\u00fckt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So merken wir ein weises Walten,", "tokens": ["So", "mer\u00b7ken", "wir", "ein", "wei\u00b7ses", "Wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df Vieh und Menschen zu erhalten,", "tokens": ["Da\u00df", "Vieh", "und", "Men\u00b7schen", "zu", "er\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die Frucht mit Seegensthau erquikt.", "tokens": ["Die", "Frucht", "mit", "See\u00b7genst\u00b7hau", "er\u00b7quikt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der schw\u00fclen Sommer-Tage Hizze,", "tokens": ["Der", "schw\u00fc\u00b7len", "Som\u00b7mer\u00b7Ta\u00b7ge", "Hiz\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verdorrt durch die gestrahlten Blizze", "tokens": ["Ver\u00b7dorrt", "durch", "die", "ge\u00b7strahl\u00b7ten", "Bliz\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sonne, Garten, Wald und Feld;", "tokens": ["Der", "Son\u00b7ne", ",", "Gar\u00b7ten", ",", "Wald", "und", "Feld", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch wenn des Himmels Tieffe schwellt,", "tokens": ["Doch", "wenn", "des", "Him\u00b7mels", "Tief\u00b7fe", "schwellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sich in k\u00fchlen Thau ergiesset,", "tokens": ["Und", "sich", "in", "k\u00fch\u00b7len", "Thau", "er\u00b7gies\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und \u00fcber das verdorrte fliesset,", "tokens": ["Und", "\u00fc\u00b7ber", "das", "ver\u00b7dorr\u00b7te", "flies\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So schenkt der 