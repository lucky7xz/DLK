ich mag dein Angesichte", "tokens": ["Wie", "herr\u00b7lich", "mag", "dein", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Auf jenen Tabors H\u00f6hen seyn,", "tokens": ["Auf", "je\u00b7nen", "Ta\u00b7bors", "H\u00f6\u00b7hen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Jedoch wer kan das hier beschreiben,", "tokens": ["Je\u00b7doch", "wer", "kan", "das", "hier", "be\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VMFIN", "PDS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da alles noch mu\u00df St\u00fckwerk bleiben,", "tokens": ["Da", "al\u00b7les", "noch", "mu\u00df", "St\u00fck\u00b7werk", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VMFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Da wir uns blos aufs K\u00fcnftge freun.", "tokens": ["Da", "wir", "uns", "blos", "aufs", "K\u00fcnft\u00b7ge", "freun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}