{"textgrid.poem.38040": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Klagelied Philipp Landgrafs aus Hessen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwer, langweilig ist mir mein Zeit,", "tokens": ["Schwer", ",", "lang\u00b7wei\u00b7lig", "ist", "mir", "mein", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Mein Herz mich treibt zu Klagen,", "tokens": ["Mein", "Herz", "mich", "treibt", "zu", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Viel Untreu, Mi\u00dfgunst, Ha\u00df und Neid,", "tokens": ["Viel", "Un\u00b7treu", ",", "Mi\u00df\u00b7gunst", ",", "Ha\u00df", "und", "Neid", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach ich jezund mu\u00df tragen,", "tokens": ["Ach", "ich", "je\u00b7zund", "mu\u00df", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Viel falscher List zu dieser Frist", "tokens": ["Viel", "fal\u00b7scher", "List", "zu", "die\u00b7ser", "Frist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wird mir zu lang mit Schmerzen,", "tokens": ["Wird", "mir", "zu", "lang", "mit", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKA", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df ich oft klag", "tokens": ["Da\u00df", "ich", "oft", "klag"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "All Nacht und Tag,", "tokens": ["All", "Nacht", "und", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Doch denk ich Gotts im Herzen.", "tokens": ["Doch", "denk", "ich", "Gotts", "im", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Schwer, langweilig ist mir mein Zeit,", "tokens": ["Schwer", ",", "lang\u00b7wei\u00b7lig", "ist", "mir", "mein", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "In Trauren bin ich sitzen,", "tokens": ["In", "Trau\u00b7ren", "bin", "ich", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "All meine Freund mir weichen weit,", "tokens": ["All", "mei\u00b7ne", "Freund", "mir", "wei\u00b7chen", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich stellen an die Spitzen,", "tokens": ["Mich", "stel\u00b7len", "an", "die", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zu denen ich hab stetiglich", "tokens": ["Zu", "de\u00b7nen", "ich", "hab", "ste\u00b7tig\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mich aller Treu versehen,", "tokens": ["Mich", "al\u00b7ler", "Treu", "ver\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die setzen gar", "tokens": ["Die", "set\u00b7zen", "gar"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Mich in Gefahr,", "tokens": ["Mich", "in", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Niemand will bey mir stehen.", "tokens": ["Nie\u00b7mand", "will", "bey", "mir", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Schwer, langweilig ist mir mein Zeit,", "tokens": ["Schwer", ",", "lang\u00b7wei\u00b7lig", "ist", "mir", "mein", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ach Gott mich wollst erg\u00f6tzen,", "tokens": ["Ach", "Gott", "mich", "wollst", "er\u00b7g\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Steh du allzeit auf meiner Seit,", "tokens": ["Steh", "du", "all\u00b7zeit", "auf", "mei\u00b7ner", "Seit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf dich mein Hoffen setze,", "tokens": ["Auf", "dich", "mein", "Hof\u00b7fen", "set\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sieh zu mein Gott, wie ich ein Spott", "tokens": ["Sieh", "zu", "mein", "Gott", ",", "wie", "ich", "ein", "Spott"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$,", "PWAV", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bin unter meinen Feinden,", "tokens": ["Bin", "un\u00b7ter", "mei\u00b7nen", "Fein\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich ruf hinauf,", "tokens": ["Ich", "ruf", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Ach Herr wach auf,", "tokens": ["Ach", "Herr", "wach", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "La\u00df deine G\u00fct erscheinen.", "tokens": ["La\u00df", "dei\u00b7ne", "G\u00fct", "er\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Schwer, langweilig ist mir mein Zeit,", "tokens": ["Schwer", ",", "lang\u00b7wei\u00b7lig", "ist", "mir", "mein", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wie ist es mir doch kommen,", "tokens": ["Wie", "ist", "es", "mir", "doch", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "All meine Macht und Herrlichkeit", "tokens": ["All", "mei\u00b7ne", "Macht", "und", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hast du von mir genommen;", "tokens": ["Hast", "du", "von", "mir", "ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So wei\u00df ich doch, wie tief und hoch,", "tokens": ["So", "wei\u00df", "ich", "doch", ",", "wie", "tief", "und", "hoch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PWAV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Gnad sich streckt am Ende,", "tokens": ["Dein", "Gnad", "sich", "streckt", "am", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wie weit und breit", "tokens": ["Wie", "weit", "und", "breit"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Barmherzigkeit,", "tokens": ["Barm\u00b7her\u00b7zig\u00b7keit", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Die wollest du mir senden.", "tokens": ["Die", "wol\u00b7lest", "du", "mir", "sen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Schwer, langweilig ist mir mein Zeit,", "tokens": ["Schwer", ",", "lang\u00b7wei\u00b7lig", "ist", "mir", "mein", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "In Hoffnung thu ich harren,", "tokens": ["In", "Hoff\u00b7nung", "thu", "ich", "har\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gedanken sind mir Herzeleid,", "tokens": ["Ge\u00b7dan\u00b7ken", "sind", "mir", "Her\u00b7ze\u00b7leid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach Gott kehr um die Karten,", "tokens": ["Ach", "Gott", "kehr", "um", "die", "Kar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fchr mich doch auf geradem Weg", "tokens": ["F\u00fchr", "mich", "doch", "auf", "ge\u00b7ra\u00b7dem", "Weg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Zu meinem Land und Leuten,", "tokens": ["Zu", "mei\u00b7nem", "Land", "und", "Leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zu Kindern mein", "tokens": ["Zu", "Kin\u00b7dern", "mein"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "PPOSAT"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Ach f\u00fchr mich heim,", "tokens": ["Ach", "f\u00fchr", "mich", "heim", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Ach Gott thu f\u00fcr mich streiten.", "tokens": ["Ach", "Gott", "thu", "f\u00fcr", "mich", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VVFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Schwer, langweilig ist mir mein Zeit,", "tokens": ["Schwer", ",", "lang\u00b7wei\u00b7lig", "ist", "mir", "mein", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ich wollt mein H\u00f6rnlein gellte,", "tokens": ["Ich", "wollt", "mein", "H\u00f6rn\u00b7lein", "gell\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In J\u00e4gerweis, nach gutem Brauch,", "tokens": ["In", "J\u00e4\u00b7ger\u00b7weis", ",", "nach", "gu\u00b7tem", "Brauch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durchs Holz und auch im Felde;", "tokens": ["Durchs", "Holz", "und", "auch", "im", "Fel\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So Gottes Wort, mein h\u00f6chster Hort,", "tokens": ["So", "Got\u00b7tes", "Wort", ",", "mein", "h\u00f6chs\u00b7ter", "Hort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In meinem Land sollt klingen,", "tokens": ["In", "mei\u00b7nem", "Land", "sollt", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und h\u00fcten fein,", "tokens": ["Und", "h\u00fc\u00b7ten", "fein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Die Sch\u00e4flein mein,", "tokens": ["Die", "Sch\u00e4f\u00b7lein", "mein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Und Gottes Lob besingen.", "tokens": ["Und", "Got\u00b7tes", "Lob", "be\u00b7sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Schwer, langweilig ist mir mein Zeit,", "tokens": ["Schwer", ",", "lang\u00b7wei\u00b7lig", "ist", "mir", "mein", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Gott \u00f6ffne deine Ohren,", "tokens": ["Gott", "\u00f6ff\u00b7ne", "dei\u00b7ne", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn meine Stimm ist schwach vor Leid,", "tokens": ["Denn", "mei\u00b7ne", "Stimm", "ist", "schwach", "vor", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Ruf ist nicht verloren,", "tokens": ["Mein", "Ruf", "ist", "nicht", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein Herz und Muth, mein Leib und Gut", "tokens": ["Mein", "Herz", "und", "Muth", ",", "mein", "Leib", "und", "Gut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ergeb ich ihm bey Zeiten,", "tokens": ["Er\u00b7geb", "ich", "ihm", "bey", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich bin gewi\u00df", "tokens": ["I