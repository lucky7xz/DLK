{"textgrid.poem.66039": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Gebet", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr, la\u00df mich hungern dann und wann,", "tokens": ["Herr", ",", "la\u00df", "mich", "hun\u00b7gern", "dann", "und", "wann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "VVFIN", "ADV", "KON", "PWAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Satt sein macht stumpf und tr\u00e4ge,", "tokens": ["Satt", "sein", "macht", "stumpf", "und", "tr\u00e4\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "VVFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und schick mir Feinde, Mann um Mann,", "tokens": ["Und", "schick", "mir", "Fein\u00b7de", ",", "Mann", "um", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kampf h\u00e4lt die Kr\u00e4fte rege.", "tokens": ["Kampf", "h\u00e4lt", "die", "Kr\u00e4f\u00b7te", "re\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gib leichten Fu\u00df zu Spiel und Tanz,", "tokens": ["Gib", "leich\u00b7ten", "Fu\u00df", "zu", "Spiel", "und", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Flugkraft in goldne Ferne,", "tokens": ["Flug\u00b7kraft", "in", "gold\u00b7ne", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und h\u00e4ng den Kranz, den vollen Kranz,", "tokens": ["Und", "h\u00e4ng", "den", "Kranz", ",", "den", "vol\u00b7len", "Kranz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir h\u00f6her in die Sterne.", "tokens": ["Mir", "h\u00f6\u00b7her", "in", "die", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}