{"textgrid.poem.41709": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einstmals die Schwalben kamen", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einstmals die Schwalben kamen", "tokens": ["Einst\u00b7mals", "die", "Schwal\u00b7ben", "ka\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Mit fr\u00f6hlichem Gesang,", "tokens": ["Mit", "fr\u00f6h\u00b7li\u00b7chem", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt ziehen fort sie wieder", "tokens": ["Jetzt", "zie\u00b7hen", "fort", "sie", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und schweigen alle bang.", "tokens": ["Und", "schwei\u00b7gen", "al\u00b7le", "bang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das ist ein gutes Zeichen:", "tokens": ["Das", "ist", "ein", "gu\u00b7tes", "Zei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie brachten Lieder her,", "tokens": ["Sie", "brach\u00b7ten", "Lie\u00b7der", "her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die bleiben ", "tokens": ["Die", "blei\u00b7ben"], "token_info": ["word", "word"], "pos": ["ART", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Drum singen ", "tokens": ["Drum", "sin\u00b7gen"], "token_info": ["word", "word"], "pos": ["PAV", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Wir aber k\u00f6nnen singen", "tokens": ["Wir", "a\u00b7ber", "k\u00f6n\u00b7nen", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun auch bei Eis und Schnee \u2013", "tokens": ["Nun", "auch", "bei", "Eis", "und", "Schnee", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Schwalben stille ziehen", "tokens": ["Die", "Schwal\u00b7ben", "stil\u00b7le", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Habt Dank! \u2013 Ade! Ade!", "tokens": ["Habt", "Dank", "!", "\u2013", "A\u00b7de", "!", "A\u00b7de", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "NN", "$.", "$(", "NN", "$.", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}