{"textgrid.poem.66072": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eine Musik lieb ich mehr", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine Musik lieb ich mehr", "tokens": ["Ei\u00b7ne", "Mu\u00b7sik", "lieb", "ich", "mehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Als die sch\u00f6nste der gr\u00f6\u00dften Meister.", "tokens": ["Als", "die", "sch\u00f6ns\u00b7te", "der", "gr\u00f6\u00df\u00b7ten", "Meis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "T\u00e4glich klingt sie um mich her,", "tokens": ["T\u00e4g\u00b7lich", "klingt", "sie", "um", "mich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Klingt t\u00e4glich lauter und dreister.", "tokens": ["Klingt", "t\u00e4g\u00b7lich", "lau\u00b7ter", "und", "dreis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich liebe sie sehr, und doch, es gibt", "tokens": ["Ich", "lie\u00b7be", "sie", "sehr", ",", "und", "doch", ",", "es", "gibt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KON", "ADV", "$,", "PPER", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Stunden, da mu\u00df ich sie schelten,", "tokens": ["Stun\u00b7den", ",", "da", "mu\u00df", "ich", "sie", "schel\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VMFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Dann ist f\u00fcr die, die das Herz so liebt,", "tokens": ["Dann", "ist", "f\u00fcr", "die", ",", "die", "das", "Herz", "so", "liebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ART", "$,", "PRELS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Ein Donnerwetter nicht selten.", "tokens": ["Ein", "Don\u00b7ner\u00b7wet\u00b7ter", "nicht", "sel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Da schweigt sie wohl erschrocken still,", "tokens": ["Da", "schweigt", "sie", "wohl", "er\u00b7schro\u00b7cken", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch dauert die Pause nicht lange,", "tokens": ["Doch", "dau\u00b7ert", "die", "Pau\u00b7se", "nicht", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und wenn ich der Ruhe mich freuen will,", "tokens": ["Und", "wenn", "ich", "der", "Ru\u00b7he", "mich", "freu\u00b7en", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ist sie wieder im besten Gange.", "tokens": ["Ist", "sie", "wie\u00b7der", "im", "bes\u00b7ten", "Gan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Zuletzt geb ich mich doch darein", "tokens": ["Zu\u00b7letzt", "geb", "ich", "mich", "doch", "da\u00b7rein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "PAV"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und lache: la\u00df klingen, la\u00df klingen!", "tokens": ["Und", "la\u00b7che", ":", "la\u00df", "klin\u00b7gen", ",", "la\u00df", "klin\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "VVIMP", "VVINF", "$,", "VVIMP", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und h\u00f6r durch des Hauses Sonnenschein", "tokens": ["Und", "h\u00f6r", "durch", "des", "Hau\u00b7ses", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vier Kinderf\u00fc\u00dfe springen.", "tokens": ["Vier", "Kin\u00b7der\u00b7f\u00fc\u00b7\u00dfe", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}