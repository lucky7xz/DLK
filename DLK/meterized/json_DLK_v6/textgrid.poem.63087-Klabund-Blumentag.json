{"textgrid.poem.63087": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Blumentag", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie befreit ich atme!", "tokens": ["Wie", "be\u00b7freit", "ich", "at\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Keckheit wurde Pflicht \u2013", "tokens": ["Keck\u00b7heit", "wur\u00b7de", "Pflicht", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "L\u00e4chelnd zieh ich vom Gesicht", "tokens": ["L\u00e4\u00b7chelnd", "zieh", "ich", "vom", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schleiertuch der Fatme.", "tokens": ["Schlei\u00b7er\u00b7tuch", "der", "Fat\u00b7me", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Denn wie Morgenl\u00e4nderin", "tokens": ["Denn", "wie", "Mor\u00b7gen\u00b7l\u00e4n\u00b7de\u00b7rin"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWAV", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ging ich sonst beh\u00fctet,", "tokens": ["Ging", "ich", "sonst", "be\u00b7h\u00fc\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mutter hat gew\u00fctet,", "tokens": ["Mut\u00b7ter", "hat", "ge\u00b7w\u00fc\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wenn ich l\u00e4chelte...", "tokens": ["Wenn", "ich", "l\u00e4\u00b7chel\u00b7te", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Aber heute springt mein Blick", "tokens": ["A\u00b7ber", "heu\u00b7te", "springt", "mein", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcber alle H\u00fcrden,", "tokens": ["\u00dc\u00b7ber", "al\u00b7le", "H\u00fcr\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Meines Standes B\u00fcrden", "tokens": ["Mei\u00b7nes", "Stan\u00b7des", "B\u00fcr\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Werfe ich zur\u00fcck.", "tokens": ["Wer\u00b7fe", "ich", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Keinem Gegenblicke will ich wehren,", "tokens": ["Kei\u00b7nem", "Ge\u00b7gen\u00b7bli\u00b7cke", "will", "ich", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schaffner und Kommis \u2013", "tokens": ["Schaff\u00b7ner", "und", "Kom\u00b7mis", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ach, ich wu\u00dfte nie,", "tokens": ["Ach", ",", "ich", "wu\u00df\u00b7te", "nie", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Da\u00df sie liebe Menschen w\u00e4ren.", "tokens": ["Da\u00df", "sie", "lie\u00b7be", "Men\u00b7schen", "w\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hefte ich die Margerite", "tokens": ["Hef\u00b7te", "ich", "die", "Mar\u00b7ge\u00b7ri\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihnen an die Brust,", "tokens": ["Ih\u00b7nen", "an", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "F\u00fchle ich Lust um Lust,", "tokens": ["F\u00fch\u00b7le", "ich", "Lust", "um", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Wie mein Herz erzittert...", "tokens": ["Wie", "mein", "Herz", "er\u00b7zit\u00b7tert", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}