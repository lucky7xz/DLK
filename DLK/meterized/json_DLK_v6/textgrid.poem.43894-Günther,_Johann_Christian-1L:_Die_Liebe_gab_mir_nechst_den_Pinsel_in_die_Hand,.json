{"textgrid.poem.43894": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Liebe gab mir nechst den Pinsel in die Hand,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe gab mir nechst den Pinsel in die Hand,", "tokens": ["Die", "Lie\u00b7be", "gab", "mir", "nechst", "den", "Pin\u00b7sel", "in", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Meisterst\u00fccke von den Bildern", "tokens": ["Das", "Meis\u00b7ter\u00b7st\u00fc\u00b7cke", "von", "den", "Bil\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der gr\u00f6sten Sch\u00f6nheit abzuschildern,", "tokens": ["Der", "gr\u00f6s\u00b7ten", "Sch\u00f6n\u00b7heit", "ab\u00b7zu\u00b7schil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu deren Dienst ich oft die Leyer angewand.", "tokens": ["Zu", "de\u00b7ren", "Dienst", "ich", "oft", "die", "Le\u00b7yer", "an\u00b7ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein Ungehorsam ward sehr \u00fcbel aufgenommen;", "tokens": ["Mein", "Un\u00b7ge\u00b7hor\u00b7sam", "ward", "sehr", "\u00fc\u00b7bel", "auf\u00b7ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich sch\u00fczte zwar die Ohnmacht vor,", "tokens": ["Ich", "sch\u00fcz\u00b7te", "zwar", "die", "Ohn\u00b7macht", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Ausflucht fand ein taubes Ohr,", "tokens": ["Die", "Aus\u00b7flucht", "fand", "ein", "tau\u00b7bes", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Drum eil ich, dem Befehl gezwungen nachzukommen.", "tokens": ["Drum", "eil", "ich", ",", "dem", "Be\u00b7fehl", "ge\u00b7zwun\u00b7gen", "nach\u00b7zu\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "ART", "NN", "VVPP", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mein Engel, fluche nicht auf die Verwegenheit,", "tokens": ["Mein", "En\u00b7gel", ",", "flu\u00b7che", "nicht", "auf", "die", "Ver\u00b7we\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der sich ein Mensch jezt unterwindet;", "tokens": ["Der", "sich", "ein", "Mensch", "jezt", "un\u00b7ter\u00b7win\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich weis, da\u00df auch ein Stern verblindet,", "tokens": ["Ich", "weis", ",", "da\u00df", "auch", "ein", "Stern", "ver\u00b7blin\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "KOUS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So bald der Sonnen Glanz ihm Schein und Licht verbeuth;", "tokens": ["So", "bald", "der", "Son\u00b7nen", "Glanz", "ihm", "Schein", "und", "Licht", "ver\u00b7beuth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch die Tyrannin meiner Sinnen,", "tokens": ["Doch", "die", "Ty\u00b7ran\u00b7nin", "mei\u00b7ner", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die dir mein freyes Herz als einen Sclaven schenckt,", "tokens": ["Die", "dir", "mein", "frey\u00b7es", "Herz", "als", "ei\u00b7nen", "Scla\u00b7ven", "schenckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "ADJA", "NN", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Entschuldigt mein Beginnen", "tokens": ["Ent\u00b7schul\u00b7digt", "mein", "Be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und macht, da\u00df meine Faust auf die Erf\u00fcllung denckt.", "tokens": ["Und", "macht", ",", "da\u00df", "mei\u00b7ne", "Faust", "auf", "die", "Er\u00b7f\u00fcl\u00b7lung", "denckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mein Phoebus z\u00fcrnt mit mir, da\u00df ich aus seinem Orden", "tokens": ["Mein", "Phoe\u00b7bus", "z\u00fcrnt", "mit", "mir", ",", "da\u00df", "ich", "aus", "sei\u00b7nem", "Or\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein \u00dcberl\u00e4ufer bin;", "tokens": ["Ein", "\u00dc\u00b7berl\u00b7\u00e4u\u00b7fer", "bin", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Geist verl\u00e4st den kalten Sinn,", "tokens": ["Sein", "Geist", "ver\u00b7l\u00e4st", "den", "kal\u00b7ten", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der troz der Poesie zu einem Mahler worden.", "tokens": ["Der", "troz", "der", "Poe\u00b7sie", "zu", "ei\u00b7nem", "Mah\u00b7ler", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "ART", "NN", "VAPP", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Steht mir Apollo nicht mehr bey,", "tokens": ["Steht", "mir", "A\u00b7pol\u00b7lo", "nicht", "mehr", "bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "PTKNEG", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So mag die Warheit ihn vertreten,", "tokens": ["So", "mag", "die", "War\u00b7heit", "ihn", "ver\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und diese macht, weil ich den Beystand ausgebeten,", "tokens": ["Und", "die\u00b7se", "macht", ",", "weil", "ich", "den", "Beys\u00b7tand", "aus\u00b7ge\u00b7be\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Statt meiner hier dein Conterfey.", "tokens": ["Statt", "mei\u00b7ner", "hier", "dein", "Con\u00b7ter\u00b7fey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der ordentliche Bau, das Uhrwerck deiner Glieder,", "tokens": ["Der", "or\u00b7dent\u00b7li\u00b7che", "Bau", ",", "das", "Uhr\u00b7werck", "dei\u00b7ner", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Streckt wie ein Cedernbaum den wohlgesezten Leib.", "tokens": ["Streckt", "wie", "ein", "Ce\u00b7dern\u00b7baum", "den", "wohl\u00b7ge\u00b7sez\u00b7ten", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist nun die Majest\u00e4t ein Weib,", "tokens": ["Ist", "nun", "die", "Ma\u00b7jes\u00b7t\u00e4t", "ein", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So giebt sie sich in dir als ihrer Tochter wieder.", "tokens": ["So", "giebt", "sie", "sich", "in", "dir", "als", "ih\u00b7rer", "Toch\u00b7ter", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPER", "KOUS", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Ehrfurcht k\u00fcst dein Bild,", "tokens": ["Die", "Ehr\u00b7furcht", "k\u00fcst", "dein", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Bey welchem die Natur geschwizet,", "tokens": ["Bey", "wel\u00b7chem", "die", "Na\u00b7tur", "ge\u00b7schwi\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als sie das zarte Fleisch aus Alabast geschnizet,", "tokens": ["Als", "sie", "das", "zar\u00b7te", "Fleisch", "aus", "A\u00b7la\u00b7bast", "ge\u00b7schni\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Durch welchen der Saphir von Schneckenblute schwillt.", "tokens": ["Durch", "wel\u00b7chen", "der", "Sa\u00b7phir", "von", "Schne\u00b7cken\u00b7blu\u00b7te", "schwillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die Artigkeit lehrt dich die F\u00fc\u00dfe sezen,", "tokens": ["Die", "Ar\u00b7tig\u00b7keit", "lehrt", "dich", "die", "F\u00fc\u00b7\u00dfe", "se\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der sonderbare Gang", "tokens": ["Der", "son\u00b7der\u00b7ba\u00b7re", "Gang"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist k\u00fcnstlich, aber ohne Zwang;", "tokens": ["Ist", "k\u00fcnst\u00b7lich", ",", "a\u00b7ber", "oh\u00b7ne", "Zwang", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Schritt ist schon genug, die Freyheit wegzuhezen.", "tokens": ["Ein", "Schritt", "ist", "schon", "ge\u00b7nug", ",", "die", "Frey\u00b7heit", "weg\u00b7zu\u00b7he\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "$,", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Stra\u00dfe heftet dir viel hundert Augen an,", "tokens": ["Die", "Stra\u00b7\u00dfe", "hef\u00b7tet", "dir", "viel", "hun\u00b7dert", "Au\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn dich die Gratien bald hin bald her begleiten;", "tokens": ["Wenn", "dich", "die", "Gra\u00b7ti\u00b7en", "bald", "hin", "bald", "her", "be\u00b7glei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADV", "ADV", "APZR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich selber kan", "tokens": ["Ich", "sel\u00b7ber", "kan"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Den alten Adam nicht bestreiten", "tokens": ["Den", "al\u00b7ten", "A\u00b7dam", "nicht", "be\u00b7strei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und weiche meiner Leidenschaft,", "tokens": ["Und", "wei\u00b7che", "mei\u00b7ner", "Lei\u00b7den\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sobald mein Fenster sich an deiner Art vergaft.", "tokens": ["So\u00b7bald", "mein", "Fens\u00b7ter", "sich", "an", "dei\u00b7ner", "Art", "ver\u00b7gaft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Betracht ich auch dein Haupt, so seh ich eine Nacht,", "tokens": ["Be\u00b7tracht", "ich", "auch", "dein", "Haupt", ",", "so", "seh", "ich", "ei\u00b7ne", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit der dein schwarzes Haar den Himmel \u00fcberdecket,", "tokens": ["Mit", "der", "dein", "schwar\u00b7zes", "Haar", "den", "Him\u00b7mel", "\u00fc\u00b7berd\u00b7e\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "An welchem Amors Glut zwey Lichter aufgestecket,", "tokens": ["An", "wel\u00b7chem", "A\u00b7mors", "Glut", "zwey", "Lich\u00b7ter", "auf\u00b7ge\u00b7ste\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NE", "NN", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die seine Mutter oft zu ihren Spiegeln macht.", "tokens": ["Die", "sei\u00b7ne", "Mut\u00b7ter", "oft", "zu", "ih\u00b7ren", "Spie\u00b7geln", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "O wunderbare Finstern\u00fc\u00df,", "tokens": ["O", "wun\u00b7der\u00b7ba\u00b7re", "Fins\u00b7ter\u00b7n\u00fc\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die ihr gerolltes Pech um deinen Nacken schl\u00e4get,", "tokens": ["Die", "ihr", "ge\u00b7roll\u00b7tes", "Pech", "um", "dei\u00b7nen", "Na\u00b7cken", "schl\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN