{"dta.poem.20036": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "An das Herz im Fr\u00fchling .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es wollen V\u00f6gel wieder singen,", "tokens": ["Es", "wol\u00b7len", "V\u00f6\u00b7gel", "wie\u00b7der", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es wollen Blumen wieder bl\u00fch'n,", "tokens": ["Es", "wol\u00b7len", "Blu\u00b7men", "wie\u00b7der", "bl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Herz! kannst du dich nicht bezwingen,", "tokens": ["Mein", "Herz", "!", "kannst", "du", "dich", "nicht", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur einmal noch der Lust ergl\u00fchn?", "tokens": ["Nur", "ein\u00b7mal", "noch", "der", "Lust", "er\u00b7gl\u00fchn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was nimmer Leben durfte hoffen,", "tokens": ["Was", "nim\u00b7mer", "Le\u00b7ben", "durf\u00b7te", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O sieh! das blickt jetzt frisch hinauf,", "tokens": ["O", "sieh", "!", "das", "blickt", "jetzt", "frisch", "hin\u00b7auf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "PDS", "VVFIN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat dich so sehr ein Frost getroffen,", "tokens": ["Hat", "dich", "so", "sehr", "ein", "Frost", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df du dich nimmer richtest auf?", "tokens": ["Da\u00df", "du", "dich", "nim\u00b7mer", "rich\u00b7test", "auf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es schafft, es klopft, es m\u00f6cht sich heben,", "tokens": ["Es", "schafft", ",", "es", "klopft", ",", "es", "m\u00f6cht", "sich", "he\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch kann es nicht, es ist zu krank!", "tokens": ["Doch", "kann", "es", "nicht", ",", "es", "ist", "zu", "krank", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "$,", "PPER", "VAFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So schafft, so klopft, man h\u00f6rt's mit Beben,", "tokens": ["So", "schafft", ",", "so", "klopft", ",", "man", "h\u00f6rt's", "mit", "Be\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "$,", "PIS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Sarge der Scheintodte bang.", "tokens": ["Im", "Sar\u00b7ge", "der", "Schein\u00b7tod\u00b7te", "bang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Dann kommen eilend seine Lieben,", "tokens": ["Dann", "kom\u00b7men", "ei\u00b7lend", "sei\u00b7ne", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Befrei'n ihn aus des Grabes Graus.", "tokens": ["Be\u00b7frei'n", "ihn", "aus", "des", "Gra\u00b7bes", "Graus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du Herz aus dieser Brust, der tr\u00fcben,", "tokens": ["Du", "Herz", "aus", "die\u00b7ser", "Brust", ",", "der", "tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "PDAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kommst du ach! nimmermehr heraus!!", "tokens": ["Kommst", "du", "ach", "!", "nim\u00b7mer\u00b7mehr", "he\u00b7raus", "!!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ITJ", "$.", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}