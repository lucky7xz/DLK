{"dta.poem.21849": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "V.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht/ die T\u00fchr/ die Gunst/ verborge/", "tokens": ["Die", "Nacht", "/", "die", "T\u00fchr", "/", "die", "Gunst", "/", "ver\u00b7bor\u00b7ge", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$(", "ART", "NN", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "mein Gl\u00fckk/ mich froh/ was ich so offt verlan-", "tokens": ["mein", "Gl\u00fckk", "/", "mich", "froh", "/", "was", "ich", "so", "offt", "ver\u00b7lan"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PPER", "ADJD", "$(", "PWS", "PPER", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sey stille Nacht gek\u00fc\u00dft/ ihr Pforten", "tokens": ["Sey", "stil\u00b7le", "Nacht", "ge\u00b7k\u00fc\u00dft", "/", "ihr", "Pfor\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADJA", "NN", "VVPP", "$(", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und du geliebte Gunst werd\u2019 je und je ge-", "tokens": ["und", "du", "ge\u00b7lieb\u00b7te", "Gunst", "werd'", "je", "und", "je", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJA", "NN", "VAFIN", "ADV", "KON", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}