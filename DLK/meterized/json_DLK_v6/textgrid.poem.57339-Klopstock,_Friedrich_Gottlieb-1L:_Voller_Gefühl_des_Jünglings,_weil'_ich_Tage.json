{"textgrid.poem.57339": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Voller Gef\u00fchl des J\u00fcnglings, weil' ich Tage", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Voller Gef\u00fchl des J\u00fcnglings, weil' ich Tage", "tokens": ["Vol\u00b7ler", "Ge\u00b7f\u00fchl", "des", "J\u00fcng\u00b7lings", ",", "weil'", "ich", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN", "$,", "KOUS", "PPER", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Auf dem Ross', und dem Stahl', ich seh des Lenzes", "tokens": ["Auf", "dem", "Ross'", ",", "und", "dem", "Stahl'", ",", "ich", "seh", "des", "Len\u00b7zes"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Gr\u00fcne B\u00e4ume froh dann, und froh des Winters", "tokens": ["Gr\u00fc\u00b7ne", "B\u00e4u\u00b7me", "froh", "dann", ",", "und", "froh", "des", "Win\u00b7ters"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJD", "ADV", "$,", "KON", "ADJD", "ART", "NN"], "meter": "+-+----+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "D\u00fcrre bebl\u00fctet.", "tokens": ["D\u00fcr\u00b7re", "be\u00b7bl\u00fc\u00b7tet", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Und der geflohnen Sonnen, die ich sahe,", "tokens": ["Und", "der", "ge\u00b7floh\u00b7nen", "Son\u00b7nen", ",", "die", "ich", "sa\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind so wenig doch nicht, und auf dem Scheitel", "tokens": ["Sind", "so", "we\u00b7nig", "doch", "nicht", ",", "und", "auf", "dem", "Schei\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PIS", "ADV", "PTKNEG", "$,", "KON", "APPR", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Bl\u00fchet mir es winterlich schon, auch ist es", "tokens": ["Bl\u00fc\u00b7het", "mir", "es", "win\u00b7ter\u00b7lich", "schon", ",", "auch", "ist", "es"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "ADV", "$,", "ADV", "VAFIN", "PPER"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Hier und da \u00f6de.", "tokens": ["Hier", "und", "da", "\u00f6\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "ADJD", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Wenn ich diess frische Leben regsam athme;", "tokens": ["Wenn", "ich", "diess", "fri\u00b7sche", "Le\u00b7ben", "reg\u00b7sam", "ath\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "H\u00f6r' ich dich denn auch wohl, mit Geistes Ohre,", "tokens": ["H\u00f6r'", "ich", "dich", "denn", "auch", "wohl", ",", "mit", "Geis\u00b7tes", "Oh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADV", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dich dein Tr\u00f6pfchen leises Ger\u00e4usches tr\u00e4ufeln,", "tokens": ["Dich", "dein", "Tr\u00f6pf\u00b7chen", "lei\u00b7ses", "Ge\u00b7r\u00e4u\u00b7sches", "tr\u00e4u\u00b7feln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Weinende Weide.", "tokens": ["Wei\u00b7nen\u00b7de", "Wei\u00b7de", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Nicht die Zipresse, denn nur traurig ist sie;", "tokens": ["Nicht", "die", "Zip\u00b7res\u00b7se", ",", "denn", "nur", "trau\u00b7rig", "ist", "sie", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "KON", "ADV", "ADJD", "VAFIN", "PPER", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Du bist traurig und sch\u00f6n, du ihre Schwester,", "tokens": ["Du", "bist", "trau\u00b7rig", "und", "sch\u00f6n", ",", "du", "ih\u00b7re", "Schwes\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD", "$,", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "O es pflanze dich an das Grab der Freund mir,", "tokens": ["O", "es", "pflan\u00b7ze", "dich", "an", "das", "Grab", "der", "Freund", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "ART", "NN", "PPER", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Weide der Thr\u00e4nen!", "tokens": ["Wei\u00b7de", "der", "Thr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "J\u00fcnglinge schlummern hin, und Greise bleiben", "tokens": ["J\u00fcng\u00b7lin\u00b7ge", "schlum\u00b7mern", "hin", ",", "und", "Grei\u00b7se", "blei\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PTKVZ", "$,", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wach. Es schleichet der Tod nun hier, nun dort hin,", "tokens": ["Wach", ".", "Es", "schlei\u00b7chet", "der", "Tod", "nun", "hier", ",", "nun", "dort", "hin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "ART", "NN", "ADV", "ADV", "$,", "ADV", "ADV", "PTKVZ", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Hebt die Sichel, eilt, dass er schneide, wartet", "tokens": ["Hebt", "die", "Si\u00b7chel", ",", "eilt", ",", "dass", "er", "schnei\u00b7de", ",", "war\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Oft nicht der \u00c4hre.", "tokens": ["Oft", "nicht", "der", "\u00c4h\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Weiss auch der Mensch, wenn, ihm des Todes Ruf schallt?", "tokens": ["Weiss", "auch", "der", "Mensch", ",", "wenn", ",", "ihm", "des", "To\u00b7des", "Ruf", "schallt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$,", "KOUS", "$,", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seine Antwort darauf? Wer dann mich klagen", "tokens": ["Sei\u00b7ne", "Ant\u00b7wort", "da\u00b7rauf", "?", "Wer", "dann", "mich", "kla\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PAV", "$.", "PWS", "ADV", "PPER", "VVINF"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "H\u00f6rt, verzeih dem Thoren sein Ach; denn gl\u00fccklich", "tokens": ["H\u00f6rt", ",", "ver\u00b7zeih", "dem", "Tho\u00b7ren", "sein", "Ach", ";", "denn", "gl\u00fcck\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "VVIMP", "ART", "NN", "PPOSAT", "NN", "$.", "KON", "ADJD"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "War ich durch Frohsinn!", "tokens": ["War", "ich", "durch", "Froh\u00b7sinn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}