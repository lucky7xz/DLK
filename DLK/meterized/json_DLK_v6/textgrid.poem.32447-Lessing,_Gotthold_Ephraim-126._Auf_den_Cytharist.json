{"textgrid.poem.32447": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "126. Auf den Cytharist", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jahr aus, Jahr ein reimt Cytharist", "tokens": ["Jahr", "aus", ",", "Jahr", "ein", "reimt", "Cyt\u00b7ha\u00b7rist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "NN", "ART", "VVFIN", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Zweihundert Vers' in Einem Tage;", "tokens": ["Zwei\u00b7hun\u00b7dert", "Ver\u00b7s'", "in", "Ei\u00b7nem", "Ta\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch drucken l\u00e4\u00dft er nichts. Entscheidet mir die Frage,", "tokens": ["Doch", "dru\u00b7cken", "l\u00e4\u00dft", "er", "nichts", ".", "Ent\u00b7schei\u00b7det", "mir", "die", "Fra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "PPER", "PIS", "$.", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ob er mehr klug, mehr unklug ist.", "tokens": ["Ob", "er", "mehr", "klug", ",", "mehr", "un\u00b7klug", "ist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}