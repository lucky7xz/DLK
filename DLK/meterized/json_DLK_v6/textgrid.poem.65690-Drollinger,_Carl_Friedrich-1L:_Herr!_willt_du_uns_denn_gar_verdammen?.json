{"textgrid.poem.65690": {"metadata": {"author": {"name": "Drollinger, Carl Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr! willt du uns denn gar verdammen?", "genre": "verse", "period": "N.A.", "pub_year": 1715, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr! willt du uns denn gar verdammen?", "tokens": ["Herr", "!", "willt", "du", "uns", "denn", "gar", "ver\u00b7dam\u00b7men", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VMFIN", "PPER", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sind wir auf ewig ausgetahn?", "tokens": ["Sind", "wir", "auf", "e\u00b7wig", "aus\u00b7ge\u00b7tahn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie schlagen deines Zornes Flammen", "tokens": ["Wie", "schla\u00b7gen", "dei\u00b7nes", "Zor\u00b7nes", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von allen Seiten auf uns an?", "tokens": ["Von", "al\u00b7len", "Sei\u00b7ten", "auf", "uns", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir ligen schm\u00e4hlich auf der Erde.", "tokens": ["Wir", "li\u00b7gen", "schm\u00e4h\u00b7lich", "auf", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Hilf, o Erl\u00f6ser, hilf geschwind!", "tokens": ["Hilf", ",", "o", "Er\u00b7l\u00f6\u00b7ser", ",", "hilf", "ge\u00b7schwind", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NN", "$,", "VVIMP", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gedenke, da\u00df wir deine Heerde", "tokens": ["Ge\u00b7den\u00b7ke", ",", "da\u00df", "wir", "dei\u00b7ne", "Heer\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und dein erworbnes Erbe sind!", "tokens": ["Und", "dein", "er\u00b7worb\u00b7nes", "Er\u00b7be", "sind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gedenke deines Zions wieder,", "tokens": ["Ge\u00b7den\u00b7ke", "dei\u00b7nes", "Zi\u00b7ons", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Berges, da man dich verehrt;", "tokens": ["Des", "Ber\u00b7ges", ",", "da", "man", "dich", "ver\u00b7ehrt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PIS", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und st\u00fcrze bald die Feinde nider,", "tokens": ["Und", "st\u00fcr\u00b7ze", "bald", "die", "Fein\u00b7de", "ni\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die deinen Tempel so verst\u00f6rt!", "tokens": ["Die", "dei\u00b7nen", "Tem\u00b7pel", "so", "ver\u00b7st\u00f6rt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kein Grimm ist ihrem Grimm zu gleichen;", "tokens": ["Kein", "Grimm", "ist", "ih\u00b7rem", "Grimm", "zu", "glei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NE", "VAFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie br\u00fcllen um dein Haus herum;", "tokens": ["Sie", "br\u00fcl\u00b7len", "um", "dein", "Haus", "he\u00b7rum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und setzen ihre Gra\u00fcelzeichen", "tokens": ["Und", "set\u00b7zen", "ih\u00b7re", "Gra\u00fc\u00b7el\u00b7zei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In deiner Allmacht Heiligtum.", "tokens": ["In", "dei\u00b7ner", "All\u00b7macht", "Hei\u00b7lig\u00b7tum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich schaue Beil und Aexte blicken.", "tokens": ["Ich", "schau\u00b7e", "Beil", "und", "A\u00b7ex\u00b7te", "bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Halt innen du verwegne Schar!", "tokens": ["Halt", "in\u00b7nen", "du", "ver\u00b7weg\u00b7ne", "Schar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du schl\u00e4gest einen Schmuck zu St\u00fccken,", "tokens": ["Du", "schl\u00e4\u00b7gest", "ei\u00b7nen", "Schmuck", "zu", "St\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der aller V\u00f6lker Wunder war.", "tokens": ["Der", "al\u00b7ler", "V\u00f6l\u00b7ker", "Wun\u00b7der", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Umsonst! die Fackeln sind vorhanden.", "tokens": ["Um\u00b7sonst", "!", "die", "Fa\u00b7ckeln", "sind", "vor\u00b7han\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Tempel wird der Flammen Raub.", "tokens": ["Der", "Tem\u00b7pel", "wird", "der", "Flam\u00b7men", "Raub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo erst das Heiligste gestanden,", "tokens": ["Wo", "erst", "das", "Hei\u00b7ligs\u00b7te", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Erscheint ein Hauffe Schutt und Staub.", "tokens": ["Er\u00b7scheint", "ein", "Hauf\u00b7fe", "Schutt", "und", "Staub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie ruffen: Drauf mit Mord und Br\u00e4nden!", "tokens": ["Sie", "ruf\u00b7fen", ":", "Drauf", "mit", "Mord", "und", "Br\u00e4n\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PAV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dies Volk mu\u00df ausgetilget seyn.", "tokens": ["Dies", "Volk", "mu\u00df", "aus\u00b7ge\u00b7til\u00b7get", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Toben rei\u00dft an allen Enden,", "tokens": ["Ihr", "To\u00b7ben", "rei\u00dft", "an", "al\u00b7len", "En\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Herr! deine Ha\u00fcser grimmig ein.", "tokens": ["Herr", "!", "dei\u00b7ne", "Ha\u00fc\u00b7ser", "grim\u00b7mig", "ein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Nichts zeiget uns mehr dein Erbarmen,", "tokens": ["Nichts", "zei\u00b7get", "uns", "mehr", "dein", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie unsern V\u00e4tern vormals, an;", "tokens": ["Wie", "un\u00b7sern", "V\u00e4\u00b7tern", "vor\u00b7mals", ",", "an", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und kein Prophete sagt uns Armen,", "tokens": ["Und", "kein", "Pro\u00b7phe\u00b7te", "sagt", "uns", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie lang dein Grimm noch w\u00e4hren kan.", "tokens": ["Wie", "lang", "dein", "Grimm", "noch", "w\u00e4h\u00b7ren", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NE", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O Herr! wie lange willt du dulden,", "tokens": ["O", "Herr", "!", "wie", "lan\u00b7ge", "willt", "du", "dul\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAV", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df dich ein frevels Volk verlacht?", "tokens": ["Da\u00df", "dich", "ein", "fre\u00b7vels", "Volk", "ver\u00b7lacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie ha\u00fcffen immer Schuld auf Schulden,", "tokens": ["Sie", "ha\u00fcf\u00b7fen", "im\u00b7mer", "Schuld", "auf", "Schul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und spotten aller deiner Macht.", "tokens": ["Und", "spot\u00b7ten", "al\u00b7ler", "dei\u00b7ner", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kan deine Rache sich verweilen?", "tokens": ["Kan", "dei\u00b7ne", "Ra\u00b7che", "sich", "ver\u00b7wei\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hast du denn keinen Donner mehr?", "tokens": ["Hast", "du", "denn", "kei\u00b7nen", "Don\u00b7ner", "mehr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf, spiele doch mit Tausend Keilen", "tokens": ["Auf", ",", "spie\u00b7le", "doch", "mit", "Tau\u00b7send", "Kei\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "$,", "VVFIN", "ADV", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nach deiner Widersacher Heer!", "tokens": ["Nach", "dei\u00b7ner", "Wi\u00b7der\u00b7sa\u00b7cher", "Heer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Herr! alle H\u00fclfe, die man kennet,", "tokens": ["Herr", "!", "al\u00b7le", "H\u00fcl\u00b7fe", ",", "die", "man", "ken\u00b7net", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PIAT", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Entspringt doch ja von deiner Hand.", "tokens": ["Ent\u00b7springt", "doch", "ja", "von", "dei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du hast des Meeres Flut getrennet,", "tokens": ["Du", "hast", "des", "Mee\u00b7res", "Flut", "ge\u00b7tren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Israel hindurch gesandt.", "tokens": ["Und", "Is\u00b7rael", "hin\u00b7durch", "ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PAV", "VVPP", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Wie schlugest du den Wasserdrachen", "tokens": ["Wie", "schlu\u00b7gest", "du", "den", "Was\u00b7ser\u00b7dra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die stolzen Ha\u00fcpter schnell entzwey,", "tokens": ["Die", "stol\u00b7zen", "Ha\u00fcp\u00b7ter", "schnell", "ent\u00b7zwey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und gabst der wilden Tiere Rachen", "tokens": ["Und", "gabst", "der", "wil\u00b7den", "Tie\u00b7re", "Ra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ihr gr\u00e4\u00dflichs A\u00df zur Speise frey!", "tokens": ["Ihr", "gr\u00e4\u00df\u00b7lichs", "A\u00df", "zur", "Spei\u00b7se", "frey", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du bringst aus Felsen starke Fl\u00fcsse,", "tokens": ["Du", "bringst", "aus", "Fel\u00b7sen", "star\u00b7ke", "Fl\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und trocknest grosse Fluten auf.", "tokens": ["Und", "trock\u00b7nest", "gros\u00b7se", "Flu\u00b7ten", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du schaffest Licht und Finsternisse,", "tokens": ["Du", "schaf\u00b7fest", "Licht", "und", "Fins\u00b7ter\u00b7nis\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ordnest aller Sterne Lauff.", "tokens": ["Und", "ord\u00b7nest", "al\u00b7ler", "Ster\u00b7ne", "Lauff", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die unerforschlich-weiten Grenzen", "tokens": ["Die", "un\u00b7er\u00b7forschlich\u00b7wei\u00b7ten", "Gren\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Des grossen Rundes setzest du.", "tokens": ["Des", "gros\u00b7sen", "Run\u00b7des", "set\u00b7zest", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du ruffest bald des Sommers Gl\u00e4nzen,", "tokens": ["Du", "ruf\u00b7fest", "bald", "des", "Som\u00b7mers", "Gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word",