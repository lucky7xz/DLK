{"dta.poem.10010": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Vergn\u00fcgungen  \n  der Melancholey.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20674-3", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcnglinge! die ihr den Kranz ungl\u00fccklicher Lie-", "tokens": ["J\u00fcng\u00b7lin\u00b7ge", "!", "die", "ihr", "den", "Kranz", "un\u00b7gl\u00fcck\u00b7li\u00b7cher", "Lie"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PRELS", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "++-+--+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Welch Vergn\u00fcgen kan man der s\u00fcssen Schwermuth", "tokens": ["Welch", "Ver\u00b7gn\u00fc\u00b7gen", "kan", "man", "der", "s\u00fcs\u00b7sen", "Schwer\u00b7muth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VMFIN", "PIS", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Deren zaubrische Macht den sanfteren Seelen geschmei-", "tokens": ["De\u00b7ren", "zaub\u00b7ri\u00b7sche", "Macht", "den", "sanf\u00b7te\u00b7ren", "See\u00b7len", "ge\u00b7schmei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJA", "NN", "ART", "ADJA", "NN", "TRUNC"], "meter": "--+--+-+--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Mahlt uns die stille bezaubernde Lust, bey der redenden", "tokens": ["Mahlt", "uns", "die", "stil\u00b7le", "be\u00b7zau\u00b7bern\u00b7de", "Lust", ",", "bey", "der", "re\u00b7den\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "ADJA", "NN", "$,", "APPR", "ART", "ADJA"], "meter": "+--+--+--+-+-+-", "measure": "elegiambus"}, "line.5": {"text": "S\u00fcssem Gesange zu schmelzen; in sanften thauigten", "tokens": ["S\u00fcs\u00b7sem", "Ge\u00b7san\u00b7ge", "zu", "schmel\u00b7zen", ";", "in", "sanf\u00b7ten", "thau\u00b7ig\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$.", "APPR", "ADJA", "NN"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.6": {"text": "Durch die Mitternacht hin, mit irrenden Schritten zu", "tokens": ["Durch", "die", "Mit\u00b7ter\u00b7nacht", "hin", ",", "mit", "ir\u00b7ren\u00b7den", "Schrit\u00b7ten", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,", "APPR", "ADJA", "NN", "PTKZU"], "meter": "+-+-+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Und dem vertraulichen Mond die Schmerzen der Liebe", "tokens": ["Und", "dem", "ver\u00b7trau\u00b7li\u00b7chen", "Mond", "die", "Schmer\u00b7zen", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "ART", "NN"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "Oft vom Vogel der Nacht mit \u00e4hnlichen Seufzern be-", "tokens": ["Oft", "vom", "Vo\u00b7gel", "der", "Nacht", "mit", "\u00e4hn\u00b7li\u00b7chen", "Seuf\u00b7zern", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "TRUNC"], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Oder im schattichten Wald am dunkeln Bache zu irren,", "tokens": ["O\u00b7der", "im", "schat\u00b7tich\u00b7ten", "Wald", "am", "dun\u00b7keln", "Ba\u00b7che", "zu", "ir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "---+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Und allda die nichtigen Freuden der Welt zu vergessen.", "tokens": ["Und", "all\u00b7da", "die", "nich\u00b7ti\u00b7gen", "Freu\u00b7den", "der", "Welt", "zu", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.11": {"text": "Da indes ein gl\u00fccklicher Traum die erscheinende Sch\u00f6ne", "tokens": ["Da", "in\u00b7des", "ein", "gl\u00fcck\u00b7li\u00b7cher", "Traum", "die", "er\u00b7schei\u00b7nen\u00b7de", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.12": {"text": "Vor euch mahlt, \u2014 nun h\u00f6rt ihr nicht mehr das Ge-", "tokens": ["Vor", "euch", "mahlt", ",", "nun", "h\u00f6rt", "ihr", "nicht", "mehr", "das", "Ge"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "$,", "$(", "ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "ART", "NN"], "meter": "+-+-+++--", "measure": "unknown.measure.penta"}, "line.13": {"text": "Und das Auge dringet nicht mehr durch schauernde G\u00e4nge", "tokens": ["Und", "das", "Au\u00b7ge", "drin\u00b7get", "nicht", "mehr", "durch", "schau\u00b7ern\u00b7de", "G\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKNEG", "ADV", "APPR", "ADJA", "NN"], "meter": "--+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Waldichter Linden, bis etwan im Forst vom f\u00e4llendem", "tokens": ["Wal\u00b7dich\u00b7ter", "Lin\u00b7den", ",", "bis", "et\u00b7wan", "im", "Forst", "vom", "f\u00e4l\u00b7len\u00b7dem"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "KOUS", "ADV", "APPRART", "NN", "APPRART", "ADJA"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.15": {"text": "Oder vom fernen Geklingel der Heerden, und von dem", "tokens": ["O\u00b7der", "vom", "fer\u00b7nen", "Ge\u00b7klin\u00b7gel", "der", "Heer\u00b7den", ",", "und", "von", "dem"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "ART", "NN", "$,", "KON", "APPR", "ART"], "meter": "+--+--+--+-+--", "measure": "dactylic.tri.plus"}, "line.16": {"text": "Eines die Str\u00e4uche durcheilenden Stiers, die betroge-", "tokens": ["Ei\u00b7nes", "die", "Str\u00e4u\u00b7che", "durc\u00b7hei\u00b7len\u00b7den", "Stiers", ",", "die", "be\u00b7tro\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "ART", "NN", "ADJA", "NN", "$,", "ART", "TRUNC"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.17": {"text": "Sich ermuntern, und pl\u00f6tzlich der Traum in die L\u00fcfte", "tokens": ["Sich", "er\u00b7mun\u00b7tern", ",", "und", "pl\u00f6tz\u00b7lich", "der", "Traum", "in", "die", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "VVINF", "$,", "KON", "ADJD", "ART", "NN", "APPR", "ART", "NN"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.18": {"text": "Die\u00df sind Vergn\u00fcgen, zu denen mein Herz sich eh-", "tokens": ["Die\u00df", "sind", "Ver\u00b7gn\u00fc\u00b7gen", ",", "zu", "de\u00b7nen", "mein", "Herz", "sich", "eh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "NN", "$,", "APPR", "PRELS", "PPOSAT", "NN", "PRF", "TRUNC"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.19": {"text": "Als den verblendeten Blick die junge Saphira bezaubert,", "tokens": ["Als", "den", "ver\u00b7blen\u00b7de\u00b7ten", "Blick", "die", "jun\u00b7ge", "Sa\u00b7phi\u00b7ra", "be\u00b7zau\u00b7bert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "Und in schwarzer Entfernung von ihr, mein Leben mir", "tokens": ["Und", "in", "schwar\u00b7zer", "Ent\u00b7fer\u00b7nung", "von", "ihr", ",", "mein", "Le\u00b7ben", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "APPR", "PPER", "$,", "PPOSAT", "NN", "PPER"], "meter": "--+-+---+-+-+", "measure": "anapaest.init"}, "line.21": {"text": "Sch\u00f6ner als Flora lachte sie mir, wenn Zephyr sie auf-", "tokens": ["Sch\u00f6\u00b7ner", "als", "Flo\u00b7ra", "lach\u00b7te", "sie", "mir", ",", "wenn", "Ze\u00b7phyr", "sie", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "NE", "VVFIN", "PPER", "PPER", "$,", "KOUS", "NN", "PPER", "TRUNC"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.22": {"text": "Und sie schamhaft err\u00f6thend aus duftenden Lauben heraus-", "tokens": ["Und", "sie", "scham\u00b7haft", "er\u00b7r\u00f6\u00b7thend", "aus", "duf\u00b7ten\u00b7den", "Lau\u00b7ben", "her\u00b7aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJD", "VVPP", "APPR", "ADJA", "NN", "TRUNC"], "meter": "--+--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.23": {"text": "Mit den Kr\u00e4nzen von Veilchen und Rosen die Felder", "tokens": ["Mit", "den", "Kr\u00e4n\u00b7zen", "von", "Veil\u00b7chen", "und", "Ro\u00b7sen", "die", "Fel\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "ART", "NN"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.24": {"text": "Vor unheiligen Seelen sind diese Vergn\u00fcgen verborgen,", "tokens": ["Vor", "un\u00b7hei\u00b7li\u00b7gen", "See\u00b7len", "sind", "die\u00b7se", "Ver\u00b7gn\u00fc\u00b7gen", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PDAT", "NN", "VVPP", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.25": {"text": "Und sie kan nur ein Herz, gew\u00f6hnt zur Schwermuth, em-", "tokens": ["Und", "sie", "kan", "nur", "ein", "Herz", ",", "ge\u00b7w\u00f6hnt", "zur", "Schwer\u00b7muth", ",", "em"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "VMFIN", "ADV", "ART", "NN", "$,", "VVPP", "APPRART", "NN", "$,", "TRUNC"], "meter": "--+--+-+-+--", "measure": "anapaest.di.plus"}}}}}