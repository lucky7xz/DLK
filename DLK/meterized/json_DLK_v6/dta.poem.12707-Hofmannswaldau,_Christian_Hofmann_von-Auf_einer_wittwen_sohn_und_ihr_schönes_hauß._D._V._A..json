{"dta.poem.12707": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einer wittwen sohn und ihr  \n sch\u00f6nes hau\u00df.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Nain war di\u00df werthe trauer-hau\u00df,", "tokens": ["Mein", "Nain", "war", "di\u00df", "wert\u00b7he", "trau\u00b7e\u00b7rhau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PDS", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als es mir hat mein braut-bett aufgeschlagen:", "tokens": ["Als", "es", "mir", "hat", "mein", "braut\u00b7bett", "auf\u00b7ge\u00b7schla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es will auch noch den schbnen namen tragen;", "tokens": ["Es", "will", "auch", "noch", "den", "schb\u00b7nen", "na\u00b7men", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn tr\u00e4gt man nicht der wittwen sohn herau\u00df?", "tokens": ["Denn", "tr\u00e4gt", "man", "nicht", "der", "witt\u00b7wen", "sohn", "her\u00b7au\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PTKNEG", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}