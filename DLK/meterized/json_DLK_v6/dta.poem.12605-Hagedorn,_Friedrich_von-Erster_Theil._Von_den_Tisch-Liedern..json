{"dta.poem.12605": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Erster Theil.  \n Von den Tisch-Liedern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Br\u00fcder! warum trinkt ihr nicht?", "tokens": ["Br\u00fc\u00b7der", "!", "wa\u00b7rum", "trinkt", "ihr", "nicht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was erwartet ihr das Licht?", "tokens": ["Was", "er\u00b7war\u00b7tet", "ihr", "das", "Licht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denkt, wie bald ein Tag verflossen.", "tokens": ["Denkt", ",", "wie", "bald", "ein", "Tag", "ver\u00b7flos\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gebet uns geschwinde Wein!", "tokens": ["Ge\u00b7bet", "uns", "ge\u00b7schwin\u00b7de", "Wein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Grosse Becher bringt herein,", "tokens": ["Gros\u00b7se", "Be\u00b7cher", "bringt", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die verschiedner Weite seyn,", "tokens": ["Die", "ver\u00b7schied\u00b7ner", "Wei\u00b7te", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und vom Weine vollgegossen!", "tokens": ["Und", "vom", "Wei\u00b7ne", "voll\u00b7ge\u00b7gos\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Trinkt den Reben-Saft, und denkt,", "tokens": ["Trinkt", "den", "Re\u00b7ben\u00b7Saft", ",", "und", "denkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wozu Bacchus ihn geschenkt;", "tokens": ["Wo\u00b7zu", "Bac\u00b7chus", "ihn", "ge\u00b7schenkt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf! vergesset Noth und Plagen.", "tokens": ["Auf", "!", "ver\u00b7ges\u00b7set", "Noth", "und", "Pla\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eins, zwey, drey und mehrmal leer!", "tokens": ["Eins", ",", "zwey", ",", "drey", "und", "mehr\u00b7mal", "leer", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "CARD", "$,", "CARD", "KON", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wird euch der Kopf zu schwer,", "tokens": ["Und", "wird", "euch", "der", "Kopf", "zu", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "PTKA", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Gut! so trinket immer mehr.", "tokens": ["Gut", "!", "so", "trin\u00b7ket", "im\u00b7mer", "mehr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "ADV", "VVFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ein Glas soll das andre jagen.", "tokens": ["Ein", "Glas", "soll", "das", "and\u00b7re", "ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "PIS", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}