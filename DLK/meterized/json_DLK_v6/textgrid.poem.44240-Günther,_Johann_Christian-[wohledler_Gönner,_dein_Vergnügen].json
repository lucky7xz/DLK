{"textgrid.poem.44240": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[wohledler G\u00f6nner, dein Vergn\u00fcgen]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohledler G\u00f6nner, dein Vergn\u00fcgen", "tokens": ["Woh\u00b7led\u00b7ler", "G\u00f6n\u00b7ner", ",", "dein", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bl\u00fcht jezo durch ein holdes Ja;", "tokens": ["Bl\u00fcht", "je\u00b7zo", "durch", "ein", "hol\u00b7des", "Ja", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So mu\u00df die keusche Liebe siegen,", "tokens": ["So", "mu\u00df", "die", "keu\u00b7sche", "Lie\u00b7be", "sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Kampf ist aus, der Kranz ist da.", "tokens": ["Der", "Kampf", "ist", "aus", ",", "der", "Kranz", "ist", "da", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "$,", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach, was vor Z\u00e4rtligkeit im K\u00fc\u00dfen", "tokens": ["Ach", ",", "was", "vor", "Z\u00e4rt\u00b7lig\u00b7keit", "im", "K\u00fc\u00b7\u00dfen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PRELS", "APPR", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und was vor Scherz in Arm und Brust", "tokens": ["Und", "was", "vor", "Scherz", "in", "Arm", "und", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPR", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wird ehstens deiner besten Lust", "tokens": ["Wird", "ehs\u00b7tens", "dei\u00b7ner", "bes\u00b7ten", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den Wollustzins entrichten m\u00fc\u00dfen!", "tokens": ["Den", "Wol\u00b7lust\u00b7zins", "ent\u00b7rich\u00b7ten", "m\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Himmel seegne deine Flammen", "tokens": ["Der", "Him\u00b7mel", "seeg\u00b7ne", "dei\u00b7ne", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und halt euch, jezt versprochne Zwey,", "tokens": ["Und", "halt", "euch", ",", "jezt", "ver\u00b7sproch\u00b7ne", "Zwey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Fried und Eintracht so beysammen,", "tokens": ["In", "Fried", "und", "Ein\u00b7tracht", "so", "bey\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da\u00df Neid und Misgunst th\u00f6richt sey.", "tokens": ["Da\u00df", "Neid", "und", "Mis\u00b7gunst", "th\u00f6\u00b7richt", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dies, werther G\u00f6nner, w\u00fcntscht mein Dichten.", "tokens": ["Dies", ",", "wert\u00b7her", "G\u00f6n\u00b7ner", ",", "w\u00fcnt\u00b7scht", "mein", "Dich\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Gef\u00e4llt es k\u00fcnftig dir einmahl,", "tokens": ["Ge\u00b7f\u00e4llt", "es", "k\u00fcnf\u00b7tig", "dir", "ein\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So soll der deutschen Musen Zahl", "tokens": ["So", "soll", "der", "deut\u00b7schen", "Mu\u00b7sen", "Zahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ihr Sorgen auf dein Brautlied richten.", "tokens": ["Ihr", "Sor\u00b7gen", "auf", "dein", "Braut\u00b7lied", "rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Vorjezt ersucht mein Abschiednehmen", "tokens": ["Vor\u00b7jezt", "er\u00b7sucht", "mein", "Ab\u00b7schied\u00b7neh\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die G\u00fcte deiner milden Hand;", "tokens": ["Die", "G\u00fc\u00b7te", "dei\u00b7ner", "mil\u00b7den", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Phoebus darf sich drum nicht sch\u00e4men,", "tokens": ["Mein", "Phoe\u00b7bus", "darf", "sich", "drum", "nicht", "sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PRF", "PAV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Armuth ist bereits bekand.", "tokens": ["Sein", "Ar\u00b7muth", "ist", "be\u00b7reits", "be\u00b7kand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf Reisen lebt man nicht vom Winde;", "tokens": ["Auf", "Rei\u00b7sen", "lebt", "man", "nicht", "vom", "Win\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du aber wirst in Warheit sehn,", "tokens": ["Du", "a\u00b7ber", "wirst", "in", "War\u00b7heit", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df, was Gelehrten Guts geschehn,", "tokens": ["Da\u00df", ",", "was", "Ge\u00b7lehr\u00b7ten", "Guts", "ge\u00b7schehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWS", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sich stets mit Wucher wieder finde.", "tokens": ["Sich", "stets", "mit", "Wu\u00b7cher", "wie\u00b7der", "fin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}