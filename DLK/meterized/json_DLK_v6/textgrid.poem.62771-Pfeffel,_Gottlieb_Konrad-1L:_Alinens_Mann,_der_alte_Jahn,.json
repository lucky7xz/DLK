{"textgrid.poem.62771": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Alinens Mann, der alte Jahn,", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alinens Mann, der alte Jahn,", "tokens": ["A\u00b7li\u00b7nens", "Mann", ",", "der", "al\u00b7te", "Jahn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Trug immer einen Talisman", "tokens": ["Trug", "im\u00b7mer", "ei\u00b7nen", "Ta\u00b7lis\u00b7man"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am Hals. Warum? Das fiel Alinen \u2013", "tokens": ["Am", "Hals", ".", "Wa\u00b7rum", "?", "Das", "fiel", "A\u00b7li\u00b7nen", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "PWAV", "$.", "PDS", "VVFIN", "NE", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er konnte zwier ihr Vater seyn \u2013", "tokens": ["Er", "konn\u00b7te", "zwier", "ihr", "Va\u00b7ter", "seyn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Beym Schlafengehn zu fragen ein.", "tokens": ["Beym", "Schla\u00b7fen\u00b7gehn", "zu", "fra\u00b7gen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es ist, sprach er mit schlauen Mienen,", "tokens": ["Es", "ist", ",", "sprach", "er", "mit", "schlau\u00b7en", "Mie\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Talisman der Fruchtbarkeit,", "tokens": ["Ein", "Ta\u00b7lis\u00b7man", "der", "Frucht\u00b7bar\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.8": {"text": "Und machte schmunzelnd sich bereit", "tokens": ["Und", "mach\u00b7te", "schmun\u00b7zelnd", "sich", "be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "PRF", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ihr einen derben Ku\u00df zu geben.", "tokens": ["Ihr", "ei\u00b7nen", "der\u00b7ben", "Ku\u00df", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Doch nun sieht er beym Kerzenlicht", "tokens": ["Doch", "nun", "sieht", "er", "beym", "Ker\u00b7zen\u00b7licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Auf ihrer Brust ein Bildchen schweben;", "tokens": ["Auf", "ih\u00b7rer", "Brust", "ein", "Bild\u00b7chen", "schwe\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Er hatte gleich die Brille nicht,", "tokens": ["Er", "hat\u00b7te", "gleich", "die", "Bril\u00b7le", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Sie h\u00e4tt ihm sonst in allen Z\u00fcgen", "tokens": ["Sie", "h\u00e4tt", "ihm", "sonst", "in", "al\u00b7len", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Des jungen Neffen Angesicht", "tokens": ["Des", "jun\u00b7gen", "Nef\u00b7fen", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Entdeckt. Was hast du, Kind, hier liegen?", "tokens": ["Ent\u00b7deckt", ".", "Was", "hast", "du", ",", "Kind", ",", "hier", "lie\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PWS", "VAFIN", "PPER", "$,", "NN", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Sprach Jahn. Sie schwieg voll Sch\u00fcchternheit.", "tokens": ["Sprach", "Jahn", ".", "Sie", "schwieg", "voll", "Sch\u00fcch\u00b7tern\u00b7heit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "PPER", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ists etwann auch, mein trautes Weibchen,", "tokens": ["Ists", "et\u00b7wann", "auch", ",", "mein", "trau\u00b7tes", "Weib\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Ein Talisman der Fruchtbarkeit?", "tokens": ["Ein", "Ta\u00b7lis\u00b7man", "der", "Frucht\u00b7bar\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.19": {"text": "Getroffen! rief das fromme T\u00e4ubchen.", "tokens": ["Ge\u00b7trof\u00b7fen", "!", "rief", "das", "from\u00b7me", "T\u00e4ub\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}