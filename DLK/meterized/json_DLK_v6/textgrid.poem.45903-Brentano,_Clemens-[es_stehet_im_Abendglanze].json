{"textgrid.poem.45903": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[es stehet im Abendglanze]", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es stehet im Abendglanze", "tokens": ["Es", "ste\u00b7het", "im", "A\u00b7bend\u00b7glan\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein freies heiliges Haus", "tokens": ["Ein", "frei\u00b7es", "hei\u00b7li\u00b7ges", "Haus"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da sehen mit schimmernden Augen", "tokens": ["Da", "se\u00b7hen", "mit", "schim\u00b7mern\u00b7den", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Viel Knaben und Jungfraun heraus,", "tokens": ["Viel", "Kna\u00b7ben", "und", "Jung\u00b7fraun", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Dort hab' ich mein Liebchen gesehen", "tokens": ["Dort", "hab'", "ich", "mein", "Lieb\u00b7chen", "ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Ein freundliches zierliches Kind,", "tokens": ["Ein", "freund\u00b7li\u00b7ches", "zier\u00b7li\u00b7ches", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Sie konnte wohl schweben und drehen,", "tokens": ["Sie", "konn\u00b7te", "wohl", "schwe\u00b7ben", "und", "dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "KON", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Wie fallende Bl\u00fcten im Wind.", "tokens": ["Wie", "fal\u00b7len\u00b7de", "Bl\u00fc\u00b7ten", "im", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Und die in dem Hause wohnen", "tokens": ["Und", "die", "in", "dem", "Hau\u00b7se", "woh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind heilig und wissen es nicht", "tokens": ["Sind", "hei\u00b7lig", "und", "wis\u00b7sen", "es", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "KON", "VVFIN", "PPER", "PTKNEG"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie leben mit Kr\u00e4nzen und Kronen", "tokens": ["Sie", "le\u00b7ben", "mit", "Kr\u00e4n\u00b7zen", "und", "Kro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Allt\u00e4glich ein neues Gedicht", "tokens": ["All\u00b7t\u00e4g\u00b7lich", "ein", "neu\u00b7es", "Ge\u00b7dicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Sie sind gleich den G\u00f6ttern und handlen,", "tokens": ["Sie", "sind", "gleich", "den", "G\u00f6t\u00b7tern", "und", "hand\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "KON", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Wohl t\u00e4glich in andrer Gestalt,", "tokens": ["Wohl", "t\u00e4g\u00b7lich", "in", "an\u00b7drer", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Mein Liebchen wird auch sich verwandlen", "tokens": ["Mein", "Lieb\u00b7chen", "wird", "auch", "sich", "ver\u00b7wand\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das tut meinem Herzen Gewalt.", "tokens": ["Das", "tut", "mei\u00b7nem", "Her\u00b7zen", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "O Liebchen, wo bist du geblieben,", "tokens": ["O", "Lieb\u00b7chen", ",", "wo", "bist", "du", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWAV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich steh' vor dem schimmernden Haus,", "tokens": ["Ich", "steh'", "vor", "dem", "schim\u00b7mern\u00b7den", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und will dich bescheiden nur lieben", "tokens": ["Und", "will", "dich", "be\u00b7schei\u00b7den", "nur", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "ADV", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "O Liebchen o sehe heraus", "tokens": ["O", "Lieb\u00b7chen", "o", "se\u00b7he", "he\u00b7raus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "FM", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich will dein pflegen und warten,", "tokens": ["Ich", "will", "dein", "pfle\u00b7gen", "und", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Im Herzen so treu, als ich kann,", "tokens": ["Im", "Her\u00b7zen", "so", "treu", ",", "als", "ich", "kann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "$,", "KOUS", "PPER", "VMFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Da seh' ich dich sitzen im Garten", "tokens": ["Da", "seh'", "ich", "dich", "sit\u00b7zen", "im", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVFIN", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Wohl bei einem reichen Mann.", "tokens": ["Wohl", "bei", "ei\u00b7nem", "rei\u00b7chen", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "So kauf' ich mir Rechen und Spaten", "tokens": ["So", "kauf'", "ich", "mir", "Re\u00b7chen", "und", "Spa\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Bind' mir ein gr\u00fcn Sch\u00fcrzelein vor", "tokens": ["Bind'", "mir", "ein", "gr\u00fcn", "Sch\u00fcr\u00b7zel\u00b7ein", "vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "APPR"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und gehe wohl als ein G\u00e4rtner", "tokens": ["Und", "ge\u00b7he", "wohl", "als", "ein", "G\u00e4rt\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "KOUS", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "An des reichen Mannes Tor", "tokens": ["An", "des", "rei\u00b7chen", "Man\u00b7nes", "Tor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tu auf, tu auf den Garten,", "tokens": ["Tu", "auf", ",", "tu", "auf", "den", "Gar\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Ich will dir wohl ohne Sold", "tokens": ["Ich", "will", "dir", "wohl", "oh\u00b7ne", "Sold"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Die Blumen all pflegen und warten", "tokens": ["Die", "Blu\u00b7men", "all", "pfle\u00b7gen", "und", "war\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "VVINF", "KON", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Sie sind ja mein Silber und Gold.", "tokens": ["Sie", "sind", "ja", "mein", "Sil\u00b7ber", "und", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "So sei mir o G\u00e4rtner willkommen", "tokens": ["So", "sei", "mir", "o", "G\u00e4rt\u00b7ner", "will\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "FM", "NN", "ADJD"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Zieh hoch die Blumen mir,", "tokens": ["Zieh", "hoch", "die", "Blu\u00b7men", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zieh lang sie zu bl\u00fchenden Ketten", "tokens": ["Zieh", "lang", "sie", "zu", "bl\u00fc\u00b7hen\u00b7den", "Ket\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ich habe ein V\u00f6gelchen hier,", "tokens": ["Ich", "ha\u00b7be", "ein", "V\u00f6\u00b7gel\u00b7chen", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Zieh hoch und dicht eine Laube", "tokens": ["Zieh", "hoch", "und", "dicht", "ei\u00b7ne", "Lau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Zieh mir ein Gitterhaus", "tokens": ["Zieh", "mir", "ein", "Git\u00b7ter\u00b7haus"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df keiner mein V\u00f6gelchen raube,", "tokens": ["Da\u00df", "kei\u00b7ner", "mein", "V\u00f6\u00b7gel\u00b7chen", "rau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Und es nicht fliege aus,", "tokens": ["Und", "es", "nicht", "flie\u00b7ge", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da klingt wohl sanft und s\u00fc\u00dfe", "tokens": ["Da", "klingt", "wohl", "sanft", "und", "s\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Garten ein heilig Lied", "tokens": ["Im", "Gar\u00b7ten", "ein", "hei\u00b7lig", "Lied"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJD", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die B\u00e4ume senden Gr\u00fc\u00dfe,", "tokens": ["Die", "B\u00e4u\u00b7me", "sen\u00b7den", "Gr\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Blume lauschend bl\u00fcht,", "tokens": ["Die", "Blu\u00b7me", "lau\u00b7schend", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da seh' ich mein Liebchen so weinen,", "tokens": ["Da", "seh'", "ich", "mein", "Lieb\u00b7chen", "so", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "So blicken zu mir herauf,", "tokens": ["So", "bli\u00b7cken", "zu", "mir", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Die Sonne will nicht mehr scheinen,", "tokens": ["Die", "Son\u00b7ne", "will", "nicht", "mehr", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Die Blumen sie gehen nicht auf.", "tokens": ["Die", "Blu\u00b7men", "sie", "ge\u00b7hen", "nicht", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "So hast du dann verlassen", "tokens": ["So", "hast", "du", "dann", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der G\u00f6tter freies Haus", "tokens": ["Der", "G\u00f6t\u00b7ter", "frei\u00b7es", "Haus"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Locken Gold mu\u00df blassen,", "tokens": ["Der", "Lo\u00b7cken", "Gold", "mu\u00df", "blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Augen Licht geht aus", "tokens": ["Der", "Au\u00b7gen", "Licht", "geht", "aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O Liebchen o sei nicht so munter,", "tokens": ["O", "Lieb\u00b7chen", "o", "sei", "nicht", "so", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "FM", "VAFIN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du hast vergeudet dein Los,", "tokens": ["Du", "hast", "ver\u00b7geu\u00b7det", "dein", "Los", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Dein Sternlein, es gieng ja unter", "tokens": ["Dein", "Stern\u00b7lein", ",", "es", "gieng", "ja", "un\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "ADV", "APPR"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Tief in des Meeres Scho\u00df.", "tokens": ["Tief", "in", "des", "Mee\u00b7res", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ans Meer will ich mich stellen", "tokens": ["Ans", "Meer", "will", "ich", "mich", "stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Betr\u00fcbt im Abendschein,", "tokens": ["Be\u00b7tr\u00fcbt", "im", "A\u00b7bend\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sehn, wie in die Wellen", "tokens": ["Und", "sehn", ",", "wie", "in", "die", "Wel\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Versinkt dein Sternelein,", "tokens": ["Ver\u00b7sinkt", "dein", "Ster\u00b7ne\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und niedersehn und weinen,", "tokens": ["Und", "nie\u00b7der\u00b7sehn", "und", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Tr\u00e4nen all hinab,", "tokens": ["Die", "Tr\u00e4\u00b7nen", "all", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie wollen sich ja vereinen", "tokens": ["Sie", "wol\u00b7len", "sich", "ja", "ver\u00b7ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "ADV", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Mit deines Sternes Grab.", "tokens": ["Mit", "dei\u00b7nes", "Ster\u00b7nes", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Dies Lied hab' ich ersonnen", "tokens": ["Dies", "Lied", "hab'", "ich", "er\u00b7son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl vor dem Zauberhaus,", "tokens": ["Wohl", "vor", "dem", "Zau\u00b7ber\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das gl\u00e4nzt in der Abendsonnen,", "tokens": ["Das", "gl\u00e4nzt", "in", "der", "A\u00b7bend\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Du blickst nicht mehr heraus", "tokens": ["Du", "blickst", "nicht", "mehr", "he\u00b7raus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Als Jugend um Liebe mu\u00dft brennen", "tokens": ["Als", "Ju\u00b7gend", "um", "Lie\u00b7be", "mu\u00dft", "bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "NN", "VMFIN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "In irrem Liebeswahn,", "tokens": ["In", "ir\u00b7rem", "Lie\u00b7bes\u00b7wahn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}