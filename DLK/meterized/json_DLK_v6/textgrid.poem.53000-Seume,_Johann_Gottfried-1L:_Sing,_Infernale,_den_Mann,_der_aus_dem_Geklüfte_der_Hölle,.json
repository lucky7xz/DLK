{"textgrid.poem.53000": {"metadata": {"author": {"name": "Seume, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sing, Infernale, den Mann, der aus dem Gekl\u00fcfte der H\u00f6lle,", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sing, Infernale, den Mann, der aus dem Gekl\u00fcfte der H\u00f6lle,", "tokens": ["Sing", ",", "In\u00b7fer\u00b7na\u00b7le", ",", "den", "Mann", ",", "der", "aus", "dem", "Ge\u00b7kl\u00fcf\u00b7te", "der", "H\u00f6l\u00b7le", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Aus den Schwefelbezirken der Nacht, d\u00e4monisch empor stieg.", "tokens": ["Aus", "den", "Schwe\u00b7fel\u00b7be\u00b7zir\u00b7ken", "der", "Nacht", ",", "d\u00e4\u00b7mo\u00b7nisch", "em\u00b7por", "stieg", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "ADJD", "PTKVZ", "VVFIN", "$."], "meter": "--+--+--+--+-+-", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Und, von dem Geist Adramelechs beseelt, mit blutiger Klugheit", "tokens": ["Und", ",", "von", "dem", "Geist", "Ad\u00b7ra\u00b7me\u00b7lechs", "be\u00b7seelt", ",", "mit", "blu\u00b7ti\u00b7ger", "Klug\u00b7heit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "APPR", "ART", "NN", "NE", "VVPP", "$,", "APPR", "ADJA", "NN"], "meter": "---+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Einem der F\u00fcrsten zuerst das Kreuzige, Kreuzige! zurief.", "tokens": ["Ei\u00b7nem", "der", "F\u00fcrs\u00b7ten", "zu\u00b7erst", "das", "Kreu\u00b7zi\u00b7ge", ",", "Kreu\u00b7zi\u00b7ge", "!", "zu\u00b7rief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIS", "ART", "NN", "ADV", "ART", "NN", "$,", "NN", "$.", "VVFIN", "$."], "meter": "+--+--+-+--+-+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Als, versto\u00dfen von Gott, der in ihm Verpestung der Erde", "tokens": ["Als", ",", "ver\u00b7sto\u00b7\u00dfen", "von", "Gott", ",", "der", "in", "ihm", "Ver\u00b7pes\u00b7tung", "der", "Er\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "VVFIN", "APPR", "NN", "$,", "PRELS", "APPR", "PPER", "NN", "ART", "NN"], "meter": "--+--+-++-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und des noch \u00fcbrigen Gl\u00fccks sah, kalt der Embryo dort lag", "tokens": ["Und", "des", "noch", "\u00fcb\u00b7ri\u00b7gen", "Gl\u00fccks", "sah", ",", "kalt", "der", "Emb\u00b7ryo", "dort", "lag"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADV", "ADJA", "NN", "VVFIN", "$,", "ADJD", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+--+-+-++-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "In der Sch\u00f6pfungen Stoff, nahm Satan das k\u00fcnftige Wesen,", "tokens": ["In", "der", "Sch\u00f6p\u00b7fun\u00b7gen", "Stoff", ",", "nahm", "Sa\u00b7tan", "das", "k\u00fcnf\u00b7ti\u00b7ge", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--++--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Es mit teuflischer Plastik zu formen zu seinem Geweihten,", "tokens": ["Es", "mit", "teuf\u00b7li\u00b7scher", "Plas\u00b7tik", "zu", "for\u00b7men", "zu", "sei\u00b7nem", "Ge\u00b7weih\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "PTKZU", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Und durch ihn zu schaffen das schneidende Gift der Akzise.", "tokens": ["Und", "durch", "ihn", "zu", "schaf\u00b7fen", "das", "schnei\u00b7den\u00b7de", "Gift", "der", "Ak\u00b7zi\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PTKZU", "VVINF", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.6": {"text": "Staaten entstanden und Staaten vergingen von Sclaven und Freyen,", "tokens": ["Staa\u00b7ten", "ent\u00b7stan\u00b7den", "und", "Staa\u00b7ten", "ver\u00b7gin\u00b7gen", "von", "Scla\u00b7ven", "und", "Frey\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.7": {"text": "Rechts und links den S\u00e4ulen Herkuls, und Nero verbrannte", "tokens": ["Rechts", "und", "links", "den", "S\u00e4u\u00b7len", "Her\u00b7kuls", ",", "und", "Ne\u00b7ro", "ver\u00b7brann\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KON", "ADV", "ART", "NN", "NE", "$,", "KON", "NE", "VVFIN"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Zum Vergn\u00fcgen die Stadt, und Phalaris warf in das Gl\u00fchthier,", "tokens": ["Zum", "Ver\u00b7gn\u00fc\u00b7gen", "die", "Stadt", ",", "und", "Pha\u00b7la\u00b7ris", "warf", "in", "das", "Gl\u00fch\u00b7thier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "KON", "NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Und der Gallische Carl scho\u00df bey der blutigen Hochzeit;", "tokens": ["Und", "der", "Gal\u00b7li\u00b7sche", "Carl", "scho\u00df", "bey", "der", "blu\u00b7ti\u00b7gen", "Hoch\u00b7zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NE", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+--++", "measure": "anapaest.di.plus"}, "line.10": {"text": "Und die Z\u00f6llner nannte die Sprache des heiligen Mythus", "tokens": ["Und", "die", "Z\u00f6ll\u00b7ner", "nann\u00b7te", "die", "Spra\u00b7che", "des", "hei\u00b7li\u00b7gen", "My\u00b7thus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "--+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "S\u00fcndergesellen: doch keiner verstand das politische Sangwerk", "tokens": ["S\u00fcn\u00b7der\u00b7ge\u00b7sel\u00b7len", ":", "doch", "kei\u00b7ner", "ver\u00b7stand", "das", "po\u00b7li\u00b7ti\u00b7sche", "Sang\u00b7werk"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "PIS", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+--+--+-+-+-+-", "measure": "dactylic.tri.plus"}, "line.12": {"text": "So in das Mark der V\u00f6lker zu setzen, als er, der es wagte,", "tokens": ["So", "in", "das", "Mark", "der", "V\u00f6l\u00b7ker", "zu", "set\u00b7zen", ",", "als", "er", ",", "der", "es", "wag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.13": {"text": "Aus dem kleinsten Ge\u00e4der des Lebens die Kr\u00e4fte zu ziehen,", "tokens": ["Aus", "dem", "kleins\u00b7ten", "Ge\u00b7\u00e4\u00b7der", "des", "Le\u00b7bens", "die", "Kr\u00e4f\u00b7te", "zu", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.14": {"text": "Der in die Penetralen der leisesten H\u00e4uslichkeit eindrang,", "tokens": ["Der", "in", "die", "Pe\u00b7ne\u00b7tra\u00b7len", "der", "lei\u00b7ses\u00b7ten", "H\u00e4us\u00b7lich\u00b7keit", "ein\u00b7drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.15": {"text": "Und die M\u00e4chtigen speiste bis zu der Schwindsucht des Landes.", "tokens": ["Und", "die", "M\u00e4ch\u00b7ti\u00b7gen", "speis\u00b7te", "bis", "zu", "der", "Schwind\u00b7sucht", "des", "Lan\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.16": {"text": "Was der Staat bedarf, nicht was die F\u00fcrsten verschwelgen,", "tokens": ["Was", "der", "Staat", "be\u00b7darf", ",", "nicht", "was", "die", "F\u00fcrs\u00b7ten", "ver\u00b7schwel\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "PTKNEG", "PWS", "ART", "NN", "VVINF", "$,"], "meter": "--+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Oder in eiserne Kasten verschlie\u00dfen, und nicht was das Hofheer,", "tokens": ["O\u00b7der", "in", "ei\u00b7ser\u00b7ne", "Kas\u00b7ten", "ver\u00b7schlie\u00b7\u00dfen", ",", "und", "nicht", "was", "das", "Hof\u00b7heer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF", "$,", "KON", "PTKNEG", "PWS", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Goldbeblecht und ohne Seele mit Dumpfsinn vergeudet,", "tokens": ["Gold\u00b7be\u00b7blecht", "und", "oh\u00b7ne", "See\u00b7le", "mit", "Dumpf\u00b7sinn", "ver\u00b7geu\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.19": {"text": "Fordert des St\u00e4dters Flei\u00df und fordert die Schwei\u00dfe des Pfl\u00fcgers.", "tokens": ["For\u00b7dert", "des", "St\u00e4d\u00b7ters", "Flei\u00df", "und", "for\u00b7dert", "die", "Schwei\u00b7\u00dfe", "des", "Pfl\u00fc\u00b7gers", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "KON", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Wenn uns der K\u00fcnstler ein Werk von gro\u00dfer und herrlicher Wirkung", "tokens": ["Wenn", "uns", "der", "K\u00fcnst\u00b7ler", "ein", "Werk", "von", "gro\u00b7\u00dfer", "und", "herr\u00b7li\u00b7cher", "Wir\u00b7kung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "APPR", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Nur aus wenigen R\u00e4dern erbaut, verdienet er Beyfall:", "tokens": ["Nur", "aus", "we\u00b7ni\u00b7gen", "R\u00e4\u00b7dern", "er\u00b7baut", ",", "ver\u00b7die\u00b7net", "er", "Bey\u00b7fall", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "VVPP", "$,", "VVFIN", "PPER", "NN", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.22": {"text": "An den Staatsmaschinen wird alles unendlich vervielfacht,", "tokens": ["An", "den", "Staats\u00b7ma\u00b7schi\u00b7nen", "wird", "al\u00b7les", "un\u00b7end\u00b7lich", "ver\u00b7viel\u00b7facht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PIS", "ADJD", "VVPP", "$,"], "meter": "--+-+--+-+---+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Da\u00df kein schlichterer Sinn sich aus den Verwirrungen findet.", "tokens": ["Da\u00df", "kein", "schlich\u00b7te\u00b7rer", "Sinn", "sich", "aus", "den", "Ver\u00b7wir\u00b7run\u00b7gen", "fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.24": {"text": "Keiner vermochte das Labyrinth so d\u00e4dalisch zu flechten,", "tokens": ["Kei\u00b7ner", "ver\u00b7moch\u00b7te", "das", "La\u00b7by\u00b7rinth", "so", "d\u00e4\u00b7da\u00b7lisch", "zu", "flech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.25": {"text": "Als der Blutgeist der neuen Mauthe mit t\u00e4glicher Schr\u00f6pfung,", "tokens": ["Als", "der", "Blut\u00b7geist", "der", "neu\u00b7en", "Mau\u00b7the", "mit", "t\u00e4g\u00b7li\u00b7cher", "Schr\u00f6p\u00b7fung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.26": {"text": "Die so viel S\u00e4fte verzehrt und dabey so wenig Gewinn gibt.", "tokens": ["Die", "so", "viel", "S\u00e4f\u00b7te", "ver\u00b7zehrt", "und", "da\u00b7bey", "so", "we\u00b7nig", "Ge\u00b7winn", "gibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VVPP", "KON", "PAV", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.27": {"text": "Keine der Taxen stempelt den B\u00fcrger sichrer zur Knechtschaft,", "tokens": ["Kei\u00b7ne", "der", "Ta\u00b7xen", "stem\u00b7pelt", "den", "B\u00fcr\u00b7ger", "sich\u00b7rer", "zur", "Knecht\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VVFIN", "ART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "+--+-++-+-+--+-", "measure": "iambic.septa.invert"}, "line.28": {"text": "Oder wecket ihn sichrer zur Wuth, den Dolchen entgegen!", "tokens": ["O\u00b7der", "we\u00b7cket", "ihn", "sich\u00b7rer", "zur", "Wuth", ",", "den", "Dol\u00b7chen", "ent\u00b7ge\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.29": {"text": "Schreckliches Alternativ f\u00fcr V\u00f6lker und V\u00f6lkerbeherrscher.", "tokens": ["Schreck\u00b7li\u00b7ches", "Al\u00b7ter\u00b7na\u00b7tiv", "f\u00fcr", "V\u00f6l\u00b7ker", "und", "V\u00f6l\u00b7ker\u00b7be\u00b7herr\u00b7scher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}}, "stanza.3": {"line.1": {"text": "Sp\u00fcrer lauern in Horden am Thor, und lauern am Heerweg,", "tokens": ["Sp\u00fc\u00b7rer", "lau\u00b7ern", "in", "Hor\u00b7den", "am", "Thor", ",", "und", "lau\u00b7ern", "am", "Heer\u00b7weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NE", "APPRART", "NN", "$,", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Da\u00df der einsame Wandler dem Auge den Sack nicht verberge,", "tokens": ["Da\u00df", "der", "ein\u00b7sa\u00b7me", "Wand\u00b7ler", "dem", "Au\u00b7ge", "den", "Sack", "nicht", "ver\u00b7ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.3": {"text": "In dem er auf morgen den hungrigen Kleinen die Handvoll", "tokens": ["In", "dem", "er", "auf", "mor\u00b7gen", "den", "hung\u00b7ri\u00b7gen", "Klei\u00b7nen", "die", "Hand\u00b7voll"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "APPR", "ADV", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.4": {"text": "Linsen und Erbsen zum Mittagsbrote verbothen zur Stadt tr\u00e4gt.", "tokens": ["Lin\u00b7sen", "und", "Erb\u00b7sen", "zum", "Mit\u00b7tags\u00b7bro\u00b7te", "ver\u00b7bo\u00b7then", "zur", "Stadt", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "wo