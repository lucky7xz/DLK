{"textgrid.poem.62043": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[wer die Bangigkeit]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer die Bangigkeit", "tokens": ["Wer", "die", "Ban\u00b7gig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Jemals hat gef\u00fchlt,", "tokens": ["Je\u00b7mals", "hat", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Jene Ewigkeit,", "tokens": ["Je\u00b7ne", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Die im Schmerze w\u00fchlt \u2013", "tokens": ["Die", "im", "Schmer\u00b7ze", "w\u00fchlt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Der nur, der allein", "tokens": ["Der", "nur", ",", "der", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADV", "$,", "PRELS", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Kennt die ew'ge Macht:", "tokens": ["Kennt", "die", "ew'\u00b7ge", "Macht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ueber seinem Sein", "tokens": ["Ue\u00b7ber", "sei\u00b7nem", "Sein"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Hat sie doch gewacht. \u2013", "tokens": ["Hat", "sie", "doch", "ge\u00b7wacht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}