{"dta.poem.5624": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom Geist und C\u00f6rper", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df nicht ob es im-obs ohne Traum geschehe;", "tokens": ["Ich", "wei\u00df", "nicht", "ob", "es", "im\u00b7obs", "oh\u00b7ne", "Traum", "ge\u00b7sche\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "KOUS", "PPER", "NE", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sehe, wenigstens bed\u00fcnckt mich, da\u00df ich sehe", "tokens": ["Und", "se\u00b7he", ",", "we\u00b7nigs\u00b7tens", "be\u00b7d\u00fcnckt", "mich", ",", "da\u00df", "ich", "se\u00b7he"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein\u2019 Erd\u2019, ein Sonnen-Licht, Gebirge voller W\u00e4lder,", "tokens": ["Ein'", "Erd'", ",", "ein", "Son\u00b7nen\u00b7Licht", ",", "Ge\u00b7bir\u00b7ge", "vol\u00b7ler", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bebl\u00fchmte H\u00fcgel hier, dort angenehme Felder.", "tokens": ["Be\u00b7bl\u00fchm\u00b7te", "H\u00fc\u00b7gel", "hier", ",", "dort", "an\u00b7ge\u00b7neh\u00b7me", "Fel\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich h\u00f6r\u2019, wie mit des Bachs sanfft rauschendem Gezische", "tokens": ["Ich", "h\u00f6r'", ",", "wie", "mit", "des", "Bachs", "sanfft", "rau\u00b7schen\u00b7dem", "Ge\u00b7zi\u00b7sche"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "APPR", "ART", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der liebliche Gesang der V\u00f6gel sich vermische.", "tokens": ["Der", "lieb\u00b7li\u00b7che", "Ge\u00b7sang", "der", "V\u00f6\u00b7gel", "sich", "ver\u00b7mi\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ist dies ein s\u00fcsser Traum? nichts wei\u00df ich, was geschehe,", "tokens": ["Ist", "dies", "ein", "s\u00fcs\u00b7ser", "Traum", "?", "nichts", "wei\u00df", "ich", ",", "was", "ge\u00b7sche\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "ADJA", "NN", "$.", "PIS", "VVFIN", "PPER", "$,", "PWS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Aufmercksam, gantz erstaunt, vernehm ich und ich sehe.", "tokens": ["Auf\u00b7merck\u00b7sam", ",", "gantz", "er\u00b7staunt", ",", "ver\u00b7nehm", "ich", "und", "ich", "se\u00b7he", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "ADJD", "$,", "VVFIN", "PPER", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wer bin ich? Wo bin ich? Woher bin ich gekommen?", "tokens": ["Wer", "bin", "ich", "?", "Wo", "bin", "ich", "?", "Wo\u00b7her", "bin", "ich", "ge\u00b7kom\u00b7men", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$.", "PWAV", "VAFIN", "PPER", "$.", "PWAV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und was geschicht in mir?", "tokens": ["Und", "was", "ge\u00b7schicht", "in", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVPP", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Ich wancke hin und her und zweifle f\u00fcr und f\u00fcr.", "tokens": ["Ich", "wan\u00b7cke", "hin", "und", "her", "und", "zweif\u00b7le", "f\u00fcr", "und", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "KON", "VVFIN", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Von einem Ding ist mir der Zweifel doch benommen;", "tokens": ["Von", "ei\u00b7nem", "Ding", "ist", "mir", "der", "Zwei\u00b7fel", "doch", "be\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich ", "tokens": ["Ich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}}}}}