{"textgrid.poem.60733": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Colombos Geist", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch die Fluten bahnte, durch die dunkeln,", "tokens": ["Durch", "die", "Flu\u00b7ten", "bahn\u00b7te", ",", "durch", "die", "dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sich das Schiff die feuchte Stra\u00dfe leicht:", "tokens": ["Sich", "das", "Schiff", "die", "feuch\u00b7te", "Stra\u00b7\u00dfe", "leicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "St\u00fcrme ruhn und alle Sterne funkeln,", "tokens": ["St\u00fcr\u00b7me", "ruhn", "und", "al\u00b7le", "Ster\u00b7ne", "fun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Als den Wendepunkt die Nacht erreicht.", "tokens": ["Als", "den", "Wen\u00b7de\u00b7punkt", "die", "Nacht", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Und der neuentthronte Kaiser st\u00fctzte", "tokens": ["Und", "der", "neu\u00b7ent\u00b7thron\u00b7te", "Kai\u00b7ser", "st\u00fctz\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Seine Stirne mit der tapfern Hand,", "tokens": ["Sei\u00b7ne", "Stir\u00b7ne", "mit", "der", "tap\u00b7fern", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Eine Welle nach der andern spr\u00fctzte", "tokens": ["Ei\u00b7ne", "Wel\u00b7le", "nach", "der", "an\u00b7dern", "spr\u00fctz\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Um das Steuer des Northumberland.", "tokens": ["Um", "das", "Steu\u00b7er", "des", "Nor\u00b7thum\u00b7ber\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "An die Schlachten denkt der Held im Geiste,", "tokens": ["An", "die", "Schlach\u00b7ten", "denkt", "der", "Held", "im", "Geis\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die er schlug, an sein erprobtes Heer;", "tokens": ["Die", "er", "schlug", ",", "an", "sein", "er\u00b7prob\u00b7tes", "Heer", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Doch um ihn und seine Tr\u00e4ume kreiste,", "tokens": ["Doch", "um", "ihn", "und", "sei\u00b7ne", "Tr\u00e4u\u00b7me", "kreis\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUI", "PPER", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Einer Riesenschlange Gleich, das Meer.", "tokens": ["Ei\u00b7ner", "Rie\u00b7sen\u00b7schlan\u00b7ge", "Gleich", ",", "das", "Meer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Den des S\u00fcdens Steppen nicht bezwangen,", "tokens": ["Den", "des", "S\u00fc\u00b7dens", "Step\u00b7pen", "nicht", "be\u00b7zwan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Den der Frost des Nordens kaum besiegt,", "tokens": ["Den", "der", "Frost", "des", "Nor\u00b7dens", "kaum", "be\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "F\u00fchlt sich nun im engen Raum gefangen,", "tokens": ["F\u00fchlt", "sich", "nun", "im", "en\u00b7gen", "Raum", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Auf dem Schaum sich hin und her gewiegt.", "tokens": ["Auf", "dem", "Schaum", "sich", "hin", "und", "her", "ge\u00b7wiegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "PTKVZ", "KON", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Als er hadernd solchem Truggeschicke", "tokens": ["Als", "er", "ha\u00b7dernd", "sol\u00b7chem", "Trug\u00b7ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gottes Ratschlu\u00df fodert vor Gericht,", "tokens": ["Got\u00b7tes", "Rat\u00b7schlu\u00df", "fo\u00b7dert", "vor", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sieh, da zeigt sich seinem nassen Blicke", "tokens": ["Sieh", ",", "da", "zeigt", "sich", "sei\u00b7nem", "nas\u00b7sen", "Bli\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "PRF", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Eines Helden Schattenbild und spricht:", "tokens": ["Ei\u00b7nes", "Hel\u00b7den", "Schat\u00b7ten\u00b7bild", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Klage nicht, wenn auch die Seele duldet,", "tokens": ["Kla\u00b7ge", "nicht", ",", "wenn", "auch", "die", "See\u00b7le", "dul\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Klage nicht, dir ist ein Trost bereit:", "tokens": ["Kla\u00b7ge", "nicht", ",", "dir", "ist", "ein", "Trost", "be\u00b7reit", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PPER", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Was du leidest, litt ich unverschuldet,", "tokens": ["Was", "du", "lei\u00b7dest", ",", "litt", "ich", "un\u00b7ver\u00b7schul\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und Colombo nannte mich die Zeit.", "tokens": ["Und", "Co\u00b7lom\u00b7bo", "nann\u00b7te", "mich", "die", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Ich zuerst durchschnitt die Wasserw\u00fcste,", "tokens": ["Ich", "zu\u00b7erst", "durch\u00b7schnitt", "die", "Was\u00b7ser\u00b7w\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "\u00dcber der du deine Z\u00e4hren weinst,", "tokens": ["\u00dc\u00b7ber", "der", "du", "dei\u00b7ne", "Z\u00e4h\u00b7ren", "weinst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Der Atlantis fr\u00fchverlorne K\u00fcste,", "tokens": ["Der", "At\u00b7lan\u00b7tis", "fr\u00fch\u00b7ver\u00b7lor\u00b7ne", "K\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJA", "NN", "$,"], "meter": "-++-+-+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Dieser Fu\u00df betrat zuerst sie einst.", "tokens": ["Die\u00b7ser", "Fu\u00df", "be\u00b7trat", "zu\u00b7erst", "sie", "einst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "ADV", "PPER", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Nun ergl\u00e4nzt in heller Morgenstunden", "tokens": ["Nun", "er\u00b7gl\u00e4nzt", "in", "hel\u00b7ler", "Mor\u00b7gen\u00b7stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Auferstehung jenes teure Land,", "tokens": ["Auf\u00b7er\u00b7ste\u00b7hung", "je\u00b7nes", "teu\u00b7re", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Das der Menschheit ich zum Heil gefunden,", "tokens": ["Das", "der", "Menschheit", "ich", "zum", "Heil", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht zum Frondienst einem Ferdinand!", "tokens": ["Nicht", "zum", "Fron\u00b7di\u00b7enst", "ei\u00b7nem", "Fer\u00b7di\u00b7nand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Du erlagst dem unbezwingbarn Norden;", "tokens": ["Du", "er\u00b7lagst", "dem", "un\u00b7be\u00b7zwing\u00b7barn", "Nor\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aber jene, die darob sich freun,", "tokens": ["A\u00b7ber", "je\u00b7ne", ",", "die", "da\u00b7rob", "sich", "freun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "PRELS", "PAV", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Werden zitternd vor entmenschten Horden", "tokens": ["Wer\u00b7den", "zit\u00b7ternd", "vor", "ent\u00b7menschten", "Hor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "APPR", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ihren blinden Jubel bald bereun!", "tokens": ["Ih\u00b7ren", "blin\u00b7den", "Ju\u00b7bel", "bald", "be\u00b7re\u00b7un", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.10": {"line.1": {"text": "Aber kommt der gro\u00dfe Tag der Schmerzen,", "tokens": ["A\u00b7ber", "kommt", "der", "gro\u00b7\u00dfe", "Tag", "der", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und es hemmt ja nichts der Zeiten Lauf,", "tokens": ["Und", "es", "hemmt", "ja", "nichts", "der", "Zei\u00b7ten", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PIS", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nimm, Columbia, dann die freien Herzen,", "tokens": ["Nimm", ",", "Co\u00b7lum\u00b7bia", ",", "dann", "die", "frei\u00b7en", "Her\u00b7zen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "------+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Nimm Europas letzte Helden auf!", "tokens": ["Nimm", "Eu\u00b7ro\u00b7pas", "letz\u00b7te", "Hel\u00b7den", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Wann das gro\u00dfe Henkerschwert geschliffen,", "tokens": ["Wann", "das", "gro\u00b7\u00dfe", "Hen\u00b7ker\u00b7schwert", "ge\u00b7schlif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Meinen Kindern dann ein werter Gast,", "tokens": ["Mei\u00b7nen", "Kin\u00b7dern", "dann", "ein", "wer\u00b7ter", "Gast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Kommt die Freiheit auf bekr\u00e4nzten Schiffen,", "tokens": ["Kommt", "die", "Frei\u00b7heit", "auf", "be\u00b7kr\u00e4nz\u00b7ten", "Schif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ihre M\u00fctze pflanzt sie auf den Mast!", "tokens": ["Ih\u00b7re", "M\u00fct\u00b7ze", "pflanzt", "sie", "auf", "den", "Mast", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Segle westw\u00e4rts, sonne dich am Lichte,", "tokens": ["Seg\u00b7le", "west\u00b7w\u00e4rts", ",", "son\u00b7ne", "dich", "am", "Lich\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Das umgl\u00e4nzt den stillen Ozean;", "tokens": ["Das", "um\u00b7gl\u00e4nzt", "den", "stil\u00b7len", "O\u00b7ze\u00b7an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Denn nach Westen flieht die Weltgeschichte:", "tokens": ["Denn", "nach", "Wes\u00b7ten", "flieht", "die", "Welt\u00b7ge\u00b7schich\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie ein Herold segelst du voran!", "tokens": ["Wie", "ein", "He\u00b7rold", "se\u00b7gelst", "du", "vo\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.13": {"line.1": {"text": "Sprach's das Schattenbild und schien vergangen,", "tokens": ["Sprach's", "das", "Schat\u00b7ten\u00b7bild", "und", "schien", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "KON", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie ein Stern, der im Verl\u00f6schen blinkt:", "tokens": ["Wie", "ein", "Stern", ",", "der", "im", "Ver\u00b7l\u00f6\u00b7schen", "blinkt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Freude f\u00e4rbt des gro\u00dfen W\u00fcrgers Wangen,", "tokens": ["Freu\u00b7de", "f\u00e4rbt", "des", "gro\u00b7\u00dfen", "W\u00fcr\u00b7gers", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Weil Europa hinter ihm versinkt.", "tokens": ["Weil", "Eu\u00b7ro\u00b7pa", "hin\u00b7ter", "ihm", "ver\u00b7sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}