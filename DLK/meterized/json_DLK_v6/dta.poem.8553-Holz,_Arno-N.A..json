{"dta.poem.8553": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Oben, im siebenten Sommerhimmel, angenehm nackt,", "tokens": ["O\u00b7ben", ",", "im", "sie\u00b7ben\u00b7ten", "Som\u00b7mer\u00b7him\u00b7mel", ",", "an\u00b7ge\u00b7nehm", "nackt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "APPRART", "ADJA", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "+--++-+-+-+--+", "measure": "iambic.septa.chol"}, "line.2": {"text": "residirt heute der ganze Olymp.", "tokens": ["re\u00b7si\u00b7dirt", "heu\u00b7te", "der", "gan\u00b7ze", "O\u00b7lymp", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+---+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "In einem amethystblauen See,", "tokens": ["In", "ei\u00b7nem", "a\u00b7met\u00b7hyst\u00b7blau\u00b7en", "See", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "nicht im Mindesten genirt, dass ich ihr hier von Unten zukucke,", "tokens": ["nicht", "im", "Min\u00b7des\u00b7ten", "ge\u00b7nirt", ",", "dass", "ich", "ihr", "hier", "von", "Un\u00b7ten", "zu\u00b7ku\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "VVPP", "$,", "KOUS", "PPER", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-++--+-+-+-+--+-", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "badet Frau Venus.", "tokens": ["ba\u00b7det", "Frau", "Ve\u00b7nus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Dort die Dicke, die dem Schwan winkt, ist Juno.", "tokens": ["Dort", "die", "Di\u00b7cke", ",", "die", "dem", "Schwan", "winkt", ",", "ist", "Ju\u00b7no", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "VAFIN", "NE", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Um Gottes Willen!", "tokens": ["Um", "Got\u00b7tes", "Wil\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "NN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Welche verf\u00e4ngliche Positur! Wenn Das der Herr Gemahl sieht!", "tokens": ["Wel\u00b7che", "ver\u00b7f\u00e4ng\u00b7li\u00b7che", "Po\u00b7si\u00b7tur", "!", "Wenn", "Das", "der", "Herr", "Ge\u00b7mahl", "sieht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "$.", "KOUS", "PDS", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+--+-+--+-+-+--+", "measure": "iambic.septa.invert"}}, "stanza.5": {"line.1": {"text": "Der dreht ihr den R\u00fccken,", "tokens": ["Der", "dreht", "ihr", "den", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "liegt behaglich wiederk\u00e4uend mitten auf einer Smaragdwiese", "tokens": ["liegt", "be\u00b7hag\u00b7lich", "wie\u00b7der\u00b7k\u00e4u\u00b7end", "mit\u00b7ten", "auf", "ei\u00b7ner", "Sma\u00b7ragd\u00b7wie\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "VVPP", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "und l\u00e4sst sich von liebensw\u00fcrdigen Nymphen", "tokens": ["und", "l\u00e4sst", "sich", "von", "lie\u00b7bens\u00b7w\u00fcr\u00b7di\u00b7gen", "Nym\u00b7phen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Lorbeern, Weinlaub und gef\u00fcllte Veilchen", "tokens": ["Lor\u00b7beern", ",", "Wein\u00b7laub", "und", "ge\u00b7f\u00fcll\u00b7te", "Veil\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "um die riesigen H\u00f6rner winden.", "tokens": ["um", "die", "rie\u00b7si\u00b7gen", "H\u00f6r\u00b7ner", "win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}}}}