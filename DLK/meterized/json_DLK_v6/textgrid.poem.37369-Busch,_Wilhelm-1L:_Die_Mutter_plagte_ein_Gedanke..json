{"textgrid.poem.37369": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Mutter plagte ein Gedanke.", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Mutter plagte ein Gedanke.", "tokens": ["Die", "Mut\u00b7ter", "plag\u00b7te", "ein", "Ge\u00b7dan\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie kramt im alten Kleiderschranke,", "tokens": ["Sie", "kramt", "im", "al\u00b7ten", "Klei\u00b7der\u00b7schran\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo kurz und lang, obschon gedr\u00e4ngt,", "tokens": ["Wo", "kurz", "und", "lang", ",", "ob\u00b7schon", "ge\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "$,", "KOUS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch friedlich beieinander h\u00e4ngt.", "tokens": ["Doch", "fried\u00b7lich", "bei\u00b7ein\u00b7an\u00b7der", "h\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf einmal ruft sie: Ei, sieh da,", "tokens": ["Auf", "ein\u00b7mal", "ruft", "sie", ":", "Ei", ",", "sieh", "da", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "$.", "NN", "$,", "VVIMP", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Schwalbenschwanz, da ist er ja!", "tokens": ["Der", "Schwal\u00b7ben\u00b7schwanz", ",", "da", "ist", "er", "ja", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den blauen, l\u00e4ngst nicht mehr ben\u00fctzten,", "tokens": ["Den", "blau\u00b7en", ",", "l\u00e4ngst", "nicht", "mehr", "be\u00b7n\u00fctz\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADV", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den hinten zwiefach zugespitzten,", "tokens": ["Den", "hin\u00b7ten", "zwie\u00b7fach", "zu\u00b7ge\u00b7spitz\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Mit blanken Kn\u00f6pfen sch\u00f6n geschm\u00fcckt,", "tokens": ["Mit", "blan\u00b7ken", "Kn\u00f6p\u00b7fen", "sch\u00f6n", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der einst so manches Herz ber\u00fcckt,", "tokens": ["Der", "einst", "so", "man\u00b7ches", "Herz", "be\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ihn tr\u00e4gt sie klug und \u00fcberlegt", "tokens": ["Ihn", "tr\u00e4gt", "sie", "klug", "und", "\u00fc\u00b7ber\u00b7legt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Dahin, wo sie zu schneidern pflegt", "tokens": ["Da\u00b7hin", ",", "wo", "sie", "zu", "schnei\u00b7dern", "pflegt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "$,", "PWAV", "PPER", "PTKZU", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und trennt und wendet, n\u00e4ht und mi\u00dft,", "tokens": ["Und", "trennt", "und", "wen\u00b7det", ",", "n\u00e4ht", "und", "mi\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Bis da\u00df das Werk vollendet ist.", "tokens": ["Bis", "da\u00df", "das", "Werk", "voll\u00b7en\u00b7det", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Auf die Art aus des Vaters Fracke", "tokens": ["Auf", "die", "Art", "aus", "des", "Va\u00b7ters", "Fra\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.16": {"text": "Kriegt Fritzchen eine neue Jacke.", "tokens": ["Kriegt", "Fritz\u00b7chen", "ei\u00b7ne", "neu\u00b7e", "Ja\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Grad so behilft sich der Poet.", "tokens": ["Grad", "so", "be\u00b7hilft", "sich", "der", "Po\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.18": {"text": "Du liebe Zeit, was soll er machen?", "tokens": ["Du", "lie\u00b7be", "Zeit", ",", "was", "soll", "er", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Gebraucht sind die Gedankensachen", "tokens": ["Ge\u00b7braucht", "sind", "die", "Ge\u00b7dan\u00b7ken\u00b7sa\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Schon alle, seit die Welt besteht.", "tokens": ["Schon", "al\u00b7le", ",", "seit", "die", "Welt", "be\u00b7steht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}