{"dta.poem.13534": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Ankl\u00e4nge .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Doch durch dieses Rauschen wieder", "tokens": ["Doch", "durch", "die\u00b7ses", "Rau\u00b7schen", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rt er heimlich Stimmen ziehen,", "tokens": ["H\u00f6rt", "er", "heim\u00b7lich", "Stim\u00b7men", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ein Fall verlorner Lieder", "tokens": ["Wie", "ein", "Fall", "ver\u00b7lor\u00b7ner", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und er schaut betroffen nieder:", "tokens": ["Und", "er", "schaut", "be\u00b7trof\u00b7fen", "nie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u201e", "tokens": ["\u201e"], "token_info": ["punct"], "pos": ["$("]}, "line.6": {"text": "In den Wogen s\u00fc\u00dfer Lust,", "tokens": ["In", "den", "Wo\u00b7gen", "s\u00fc\u00b7\u00dfer", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach! nach tiefern Melodieen", "tokens": ["Ach", "!", "nach", "tie\u00b7fern", "Me\u00b7lo\u00b7di\u00b7e\u00b7en"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "APPR", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Sehnt sich einsam oft die Brust!&#8220;", "tokens": ["Sehnt", "sich", "ein\u00b7sam", "oft", "die", "Brust", "!", "&#8220;"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "XML_entity"], "pos": ["VVFIN", "PRF", "ADJD", "ADV", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}