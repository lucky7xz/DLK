{"textgrid.poem.24818": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingstag", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als winterlich umnachtet,", "tokens": ["Als", "win\u00b7ter\u00b7lich", "um\u00b7nach\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erstarrt die Erde lag,", "tokens": ["Er\u00b7starrt", "die", "Er\u00b7de", "lag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie hab' ich nicht geschmachtet", "tokens": ["Wie", "hab'", "ich", "nicht", "ge\u00b7schmach\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "PTKNEG", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach dir, o Fr\u00fchlingstag!", "tokens": ["Nach", "dir", ",", "o", "Fr\u00fch\u00b7lings\u00b7tag", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich dachte: Wenn im linden", "tokens": ["Ich", "dach\u00b7te", ":", "Wenn", "im", "lin\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "KOUS", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Lenzhauch der Himmel blaut,", "tokens": ["Lenz\u00b7hauch", "der", "Him\u00b7mel", "blaut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Dann wird mein Kummer schwinden,", "tokens": ["Dann", "wird", "mein", "Kum\u00b7mer", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So wie die Flocke taut.", "tokens": ["So", "wie", "die", "Flo\u00b7cke", "taut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nun bist du da, Erflehter,", "tokens": ["Nun", "bist", "du", "da", ",", "Er\u00b7fleh\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Duft und Farb' und Klang!", "tokens": ["Mit", "Duft", "und", "Fa\u00b7rb'", "und", "Klang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Hoch aus dem blauen Aether", "tokens": ["Hoch", "aus", "dem", "blau\u00b7en", "A\u00b7e\u00b7ther"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ert\u00f6nt der Lerche Sang!", "tokens": ["Er\u00b7t\u00f6nt", "der", "Ler\u00b7che", "Sang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es l\u00e4cheln deine Kinder,", "tokens": ["Es", "l\u00e4\u00b7cheln", "dei\u00b7ne", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Bl\u00fcten, froh erwacht;", "tokens": ["Die", "Bl\u00fc\u00b7ten", ",", "froh", "er\u00b7wacht", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch trauernd, wie ein Blinder,", "tokens": ["Doch", "trau\u00b7ernd", ",", "wie", "ein", "Blin\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Steh' ich vor all der Pracht.", "tokens": ["Steh'", "ich", "vor", "all", "der", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}