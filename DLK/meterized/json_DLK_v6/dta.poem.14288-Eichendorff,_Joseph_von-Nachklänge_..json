{"dta.poem.14288": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Nachkl\u00e4nge .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Mir tr\u00e4umt', ich ruhte wieder", "tokens": ["Mir", "tr\u00e4umt'", ",", "ich", "ruh\u00b7te", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor meines Vaters Haus", "tokens": ["Vor", "mei\u00b7nes", "Va\u00b7ters", "Haus"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schaute fr\u00f6hlich nieder", "tokens": ["Und", "schau\u00b7te", "fr\u00f6h\u00b7lich", "nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In's alte Thal hinaus,", "tokens": ["In's", "al\u00b7te", "Thal", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APZR", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Luft mit lindem Spielen", "tokens": ["Die", "Luft", "mit", "lin\u00b7dem", "Spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ging durch das Fr\u00fchlingslaub,", "tokens": ["Ging", "durch", "das", "Fr\u00fch\u00b7lings\u00b7laub", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und Bl\u00fcten-Flocken fielen", "tokens": ["Und", "Bl\u00fc\u00b7ten\u00b7Flo\u00b7cken", "fie\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mir \u00fcber Brust und Haupt.", "tokens": ["Mir", "\u00fc\u00b7ber", "Brust", "und", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}