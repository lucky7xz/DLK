{"dta.poem.21346": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gottlob! Der Hermann gewann die Schlacht,", "tokens": ["Gott\u00b7lob", "!", "Der", "Her\u00b7mann", "ge\u00b7wann", "die", "Schlacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "++-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Die R\u00f6mer wurden vertrieben,", "tokens": ["Die", "R\u00f6\u00b7mer", "wur\u00b7den", "ver\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Varus mit seinen Legionen erlag,", "tokens": ["Va\u00b7rus", "mit", "sei\u00b7nen", "Le\u00b7gi\u00b7o\u00b7nen", "er\u00b7lag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und wir sind Deutsche geblieben!", "tokens": ["Und", "wir", "sind", "Deut\u00b7sche", "ge\u00b7blie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Wir blieben deutsch, wir sprechen deutsch,", "tokens": ["Wir", "blie\u00b7ben", "deutsch", ",", "wir", "spre\u00b7chen", "deutsch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie wir es gesprochen haben;", "tokens": ["Wie", "wir", "es", "ge\u00b7spro\u00b7chen", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVPP", "VAINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Esel hei\u00dft Esel, nicht ", "tokens": ["Der", "E\u00b7sel", "hei\u00dft", "E\u00b7sel", ",", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "NN", "$,", "PTKNEG"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Die Schwaben blieben Schwaben.", "tokens": ["Die", "Schwa\u00b7ben", "blie\u00b7ben", "Schwa\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Raumer blieb ein deutscher Lump", "tokens": ["Der", "Rau\u00b7mer", "blieb", "ein", "deut\u00b7scher", "Lump"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In unserm deutschen Norden.", "tokens": ["In", "un\u00b7serm", "deut\u00b7schen", "Nor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In Reimen dichtet Freiligrath,", "tokens": ["In", "Rei\u00b7men", "dich\u00b7tet", "Frei\u00b7li\u00b7gra\u00b7th", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ist kein Horaz geworden.", "tokens": ["Ist", "kein", "Ho\u00b7raz", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VAPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.4": {"line.1": {"text": "Gottlob, der Ma\u00dfman spricht kein Latein,", "tokens": ["Gott\u00b7lob", ",", "der", "Ma\u00df\u00b7man", "spricht", "kein", "La\u00b7tein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Birch-Pfeifer schreibt nur Dramen,", "tokens": ["Birch\u00b7Pfei\u00b7fer", "schreibt", "nur", "Dra\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und s\u00e4uft nicht schn\u00f6den Terpentin,", "tokens": ["Und", "s\u00e4uft", "nicht", "schn\u00f6\u00b7den", "Ter\u00b7pen\u00b7tin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Roms galante Damen.", "tokens": ["Wie", "Roms", "ga\u00b7lan\u00b7te", "Da\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}