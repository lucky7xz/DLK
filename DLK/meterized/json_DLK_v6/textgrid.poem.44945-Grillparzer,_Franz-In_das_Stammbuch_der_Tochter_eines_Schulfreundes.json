{"textgrid.poem.44945": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "In das Stammbuch der Tochter eines Schulfreundes", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst auf denselben B\u00e4nken", "tokens": ["Einst", "auf", "den\u00b7sel\u00b7ben", "B\u00e4n\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sa\u00dfen dein Vater und ich,", "tokens": ["Sa\u00b7\u00dfen", "dein", "Va\u00b7ter", "und", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "PPER", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Des Guten und Sch\u00f6nen zu denken,", "tokens": ["Des", "Gu\u00b7ten", "und", "Sch\u00f6\u00b7nen", "zu", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der Vorsatz uns nimmer entwich.", "tokens": ["Der", "Vor\u00b7satz", "uns", "nim\u00b7mer", "ent\u00b7wich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Und da\u00df wirs nicht g\u00e4nzlich verfehlten,", "tokens": ["Und", "da\u00df", "wirs", "nicht", "g\u00e4nz\u00b7lich", "ver\u00b7fehl\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Zeigte die Zeit, die verstrich,", "tokens": ["Zeig\u00b7te", "die", "Zeit", ",", "die", "ver\u00b7strich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "All, was wir schufen und w\u00e4hlten,", "tokens": ["All", ",", "was", "wir", "schu\u00b7fen", "und", "w\u00e4hl\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Und jeder l\u00e4\u00dft sterbend nach sich:", "tokens": ["Und", "je\u00b7der", "l\u00e4\u00dft", "ster\u00b7bend", "nach", "sich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADJD", "APPR", "PRF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "Die Kinder voll Anmut und Sitten \u2013", "tokens": ["Die", "Kin\u00b7der", "voll", "An\u00b7mut", "und", "Sit\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "KON", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Neid, wei\u00dft du es anders, so sprich! \u2013", "tokens": ["Neid", ",", "wei\u00dft", "du", "es", "an\u00b7ders", ",", "so", "sprich", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PPER", "ADV", "$,", "ADV", "ADJD", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.11": {"text": "Ich Sapphon und Melitten,", "tokens": ["Ich", "Sap\u00b7phon", "und", "Me\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Dein Vater, o Liebliche, dich.", "tokens": ["Dein", "Va\u00b7ter", ",", "o", "Lieb\u00b7li\u00b7che", ",", "dich", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "NN", "$,", "PPER", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}