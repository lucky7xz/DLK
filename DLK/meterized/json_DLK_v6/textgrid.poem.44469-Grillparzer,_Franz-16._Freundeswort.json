{"textgrid.poem.44469": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "16. Freundeswort", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbmag dein Schmerz sich roh entladen,", "tokens": ["\u00bb", "mag", "dein", "Schmerz", "sich", "roh", "ent\u00b7la\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPOSAT", "NN", "PRF", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zeigst du ihn durch stummes Toben?", "tokens": ["Zeigst", "du", "ihn", "durch", "stum\u00b7mes", "To\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wen die Musen so begnaden,", "tokens": ["Wen", "die", "Mu\u00b7sen", "so", "be\u00b7gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADV", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchle h\u00f6her sich erhoben!", "tokens": ["F\u00fch\u00b7le", "h\u00f6\u00b7her", "sich", "er\u00b7ho\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PRF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bist ja Maler, brauche Farben!", "tokens": ["Bist", "ja", "Ma\u00b7ler", ",", "brau\u00b7che", "Far\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bist ja Dichter, brauch das Wort!", "tokens": ["Bist", "ja", "Dich\u00b7ter", ",", "brauch", "das", "Wort", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gram und Herz, wenn beide starben,", "tokens": ["Gram", "und", "Herz", ",", "wenn", "bei\u00b7de", "star\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dauern so geheiligt fort.\u00ab", "tokens": ["Dau\u00b7ern", "so", "ge\u00b7hei\u00b7ligt", "fort", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "VVPP", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, die Worte und die Bilder", "tokens": ["Ach", ",", "die", "Wor\u00b7te", "und", "die", "Bil\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind f\u00fcr selbstgemachte Leiden!", "tokens": ["Sind", "f\u00fcr", "selbst\u00b7ge\u00b7mach\u00b7te", "Lei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer kann Flammen, wild und wilder,", "tokens": ["Wer", "kann", "Flam\u00b7men", ",", "wild", "und", "wil\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "NN", "$,", "ADJD", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In Gewand, verh\u00fcllend, kleiden?", "tokens": ["In", "Ge\u00b7wand", ",", "ver\u00b7h\u00fcl\u00b7lend", ",", "klei\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVPP", "$,", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Drum mein Wort, es sei der Aufschrei,", "tokens": ["Drum", "mein", "Wort", ",", "es", "sei", "der", "Auf\u00b7schrei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht an Ton und Ma\u00df gebunden,", "tokens": ["Nicht", "an", "Ton", "und", "Ma\u00df", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Farbe, die mir gut d\u00e4ucht,", "tokens": ["Und", "die", "Far\u00b7be", ",", "die", "mir", "gut", "d\u00e4ucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Hier! das Blut aus meinen Wunden.", "tokens": ["Hier", "!", "das", "Blut", "aus", "mei\u00b7nen", "Wun\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}