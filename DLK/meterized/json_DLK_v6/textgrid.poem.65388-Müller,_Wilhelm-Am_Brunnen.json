{"textgrid.poem.65388": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Am Brunnen", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie schreiten fremd an mir vorbei,", "tokens": ["Sie", "schrei\u00b7ten", "fremd", "an", "mir", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich frage Keinen, wer er sei;", "tokens": ["Ich", "fra\u00b7ge", "Kei\u00b7nen", ",", "wer", "er", "sei", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir wandeln auf und wandeln nieder,", "tokens": ["Wir", "wan\u00b7deln", "auf", "und", "wan\u00b7deln", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sehn vielleicht uns nimmer wieder.", "tokens": ["Und", "sehn", "viel\u00b7leicht", "uns", "nim\u00b7mer", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und ziehen dennoch allzumal", "tokens": ["Und", "zie\u00b7hen", "den\u00b7noch", "all\u00b7zu\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach ", "tokens": ["Nach"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Dem Erdenquell, dem ewig vollen,", "tokens": ["Dem", "Er\u00b7den\u00b7quell", ",", "dem", "e\u00b7wig", "vol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus dem das Heil wir trinken wollen.", "tokens": ["Aus", "dem", "das", "Heil", "wir", "trin\u00b7ken", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PPER", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Aus ", "tokens": ["Aus"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "La\u00df Hand in Hand uns hier verbinden:", "tokens": ["La\u00df", "Hand", "in", "Hand", "uns", "hier", "ver\u00b7bin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "APPR", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Am Himmelsquell auf Wiederfinden!", "tokens": ["Am", "Him\u00b7mels\u00b7quell", "auf", "Wie\u00b7der\u00b7fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}