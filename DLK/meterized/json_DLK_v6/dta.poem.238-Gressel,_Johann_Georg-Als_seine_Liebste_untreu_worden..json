{"dta.poem.238": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als seine Liebste untreu worden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Was vor ein Strahl wil doch das Schiff zerschellen?", "tokens": ["Was", "vor", "ein", "Strahl", "wil", "doch", "das", "Schiff", "zer\u00b7schel\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das fast den Port und Haven sieht/", "tokens": ["Das", "fast", "den", "Port", "und", "Ha\u00b7ven", "sieht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NE", "KON", "NE", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was vor ein Blitz will meine Hoffnung f\u00e4llen?", "tokens": ["Was", "vor", "ein", "Blitz", "will", "mei\u00b7ne", "Hoff\u00b7nung", "f\u00e4l\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die sich um deine Gunst bem\u00fcht.", "tokens": ["Die", "sich", "um", "dei\u00b7ne", "Gunst", "be\u00b7m\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was ist es doch/ das die Verehrung st\u00f6hrt/", "tokens": ["Was", "ist", "es", "doch", "/", "das", "die", "Ver\u00b7eh\u00b7rung", "st\u00f6hrt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "$(", "PRELS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und mich nicht h\u00f6rt?", "tokens": ["Und", "mich", "nicht", "h\u00f6rt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Ach weh! es ist ein fremdes Bild/", "tokens": ["Ach", "weh", "!", "es", "ist", "ein", "frem\u00b7des", "Bild", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das dich mit Gluht und Flammen angef\u00fcllt.", "tokens": ["Das", "dich", "mit", "Gluht", "und", "Flam\u00b7men", "an\u00b7ge\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ach bittrer Schmertz/ der meine Glieder r\u00fchret/", "tokens": ["Ach", "bit\u00b7trer", "Schmertz", "/", "der", "mei\u00b7ne", "Glie\u00b7der", "r\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wie ein Gifft zum Hertzen dringt.", "tokens": ["Und", "wie", "ein", "Gifft", "zum", "Hert\u00b7zen", "dringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach falscher Sinn! der mich an Oerter f\u00fchret/", "tokens": ["Ach", "fal\u00b7scher", "Sinn", "!", "der", "mich", "an", "O\u00b7er\u00b7ter", "f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$.", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+--++--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wo man nur Trauer-Lieder singt.", "tokens": ["Wo", "man", "nur", "Trau\u00b7e\u00b7rLie\u00b7der", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach sch\u00f6nstes Bild! f\u00e4lt denn kein s\u00fcsser Blick", "tokens": ["Ach", "sch\u00f6ns\u00b7tes", "Bild", "!", "f\u00e4lt", "denn", "kein", "s\u00fcs\u00b7ser", "Blick"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADJA", "NN", "$.", "VVFIN", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Auf mich zur\u00fcck?", "tokens": ["Auf", "mich", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Nein! dieses st\u00f6hrt ein fremdes Bild/", "tokens": ["Nein", "!", "die\u00b7ses", "st\u00f6hrt", "ein", "frem\u00b7des", "Bild", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PDS", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das dich mit Gluht und Flammen angef\u00fcllt.", "tokens": ["Das", "dich", "mit", "Gluht", "und", "Flam\u00b7men", "an\u00b7ge\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Erbarme dich/ o G\u00f6ttin! meines Lebens/", "tokens": ["Er\u00b7bar\u00b7me", "dich", "/", "o", "G\u00f6t\u00b7tin", "!", "mei\u00b7nes", "Le\u00b7bens", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$(", "FM", "NN", "$.", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erbarme dich doch meiner Pein;", "tokens": ["Er\u00b7bar\u00b7me", "dich", "doch", "mei\u00b7ner", "Pein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hilfst du mir nicht? so ist die Gluht vergebens/", "tokens": ["Hilfst", "du", "mir", "nicht", "?", "so", "ist", "die", "Gluht", "ver\u00b7ge\u00b7bens", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "$.", "ADV", "VAFIN", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch deine Gunst kan ich nur seyn.", "tokens": ["Durch", "dei\u00b7ne", "Gunst", "kan", "ich", "nur", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "ADV", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch nein! du h\u00f6rst mich jetzo nicht.", "tokens": ["Doch", "nein", "!", "du", "h\u00f6rst", "mich", "jet\u00b7zo", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$.", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach sch\u00f6nstes Licht", "tokens": ["Ach", "sch\u00f6ns\u00b7tes", "Licht"], "token_info": ["word", "word", "word"], "pos": ["ITJ", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Warum? dich hat ein fremdes Bild/", "tokens": ["Wa\u00b7rum", "?", "dich", "hat", "ein", "frem\u00b7des", "Bild", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit Gluht und Flammen angef\u00fcllt.", "tokens": ["Mit", "Gluht", "und", "Flam\u00b7men", "an\u00b7ge\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}