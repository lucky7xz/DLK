{"textgrid.poem.59732": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Die deutschen auswandernden Krieger", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O mein Deutschland, will dein Jammer", "tokens": ["O", "mein", "Deutschland", ",", "will", "dein", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "$,", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Breiter, t\u00e4glich breiter werden?", "tokens": ["Brei\u00b7ter", ",", "t\u00e4g\u00b7lich", "brei\u00b7ter", "wer\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Finden deine besten S\u00f6hne", "tokens": ["Fin\u00b7den", "dei\u00b7ne", "bes\u00b7ten", "S\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keinen Platz auf deutscher Erden?", "tokens": ["Kei\u00b7nen", "Platz", "auf", "deut\u00b7scher", "Er\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Klingt der bittre Fluch des Fl\u00fcchtlings", "tokens": ["Klingt", "der", "bitt\u00b7re", "Fluch", "des", "Fl\u00fccht\u00b7lings"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch der Angeln Land und Hessen?", "tokens": ["Durch", "der", "An\u00b7geln", "Land", "und", "Hes\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wird so deutsche Lieb' und Treue", "tokens": ["Wird", "so", "deut\u00b7sche", "Lieb'", "und", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Deinen Tapfern zugemessen?", "tokens": ["Dei\u00b7nen", "Tap\u00b7fern", "zu\u00b7ge\u00b7mes\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jammer, den kein Lied kann singen!", "tokens": ["Jam\u00b7mer", ",", "den", "kein", "Lied", "kann", "sin\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unheil, das kein Wort kann fassen!", "tokens": ["Un\u00b7heil", ",", "das", "kein", "Wort", "kann", "fas\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Also m\u00fcssen deine Streiter,", "tokens": ["Al\u00b7so", "m\u00fcs\u00b7sen", "dei\u00b7ne", "Strei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kampfs- und gl\u00fccks- und landsverlassen,", "tokens": ["Kampfs", "und", "gl\u00fccks", "und", "lands\u00b7ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "TRUNC", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nach Utopien, nach Brasilien", "tokens": ["Nach", "U\u00b7to\u00b7pi\u00b7en", ",", "nach", "Bra\u00b7si\u00b7li\u00b7en"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "$,", "APPR", "NE"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Bettelnd durch die L\u00e4nder streichen?", "tokens": ["Bet\u00b7telnd", "durch", "die", "L\u00e4n\u00b7der", "strei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ihre nackten Ehrennarben", "tokens": ["Ih\u00b7re", "nack\u00b7ten", "Eh\u00b7ren\u00b7nar\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zeigen als ein deutsches Zeichen?", "tokens": ["Zei\u00b7gen", "als", "ein", "deut\u00b7sches", "Zei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihr von Siebzehnhundertachtzig", "tokens": ["Ihr", "von", "Sieb\u00b7zehn\u00b7hun\u00b7der\u00b7tacht\u00b7zig"], "token_info": ["word", "word", "word"], "pos": ["PPER", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kassellieder, Stuttgartlieder,", "tokens": ["Kas\u00b7sel\u00b7lie\u00b7der", ",", "Stutt\u00b7gart\u00b7lie\u00b7der", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihr des Aspergskerkers\u00e4ngers", "tokens": ["Ihr", "des", "As\u00b7pergs\u00b7ker\u00b7ker\u00b7s\u00e4n\u00b7gers"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Alte Lieder, t\u00f6nt ihr wieder?", "tokens": ["Al\u00b7te", "Lie\u00b7der", ",", "t\u00f6nt", "ihr", "wie\u00b7der", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die bei Saratoga fielen,", "tokens": ["Die", "bei", "Sa\u00b7ra\u00b7to\u00b7ga", "fie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die die Mohrensonn' verbrannte,", "tokens": ["Die", "die", "Moh\u00b7ren\u00b7sonn'", "ver\u00b7brann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Werden sie uns heute wieder", "tokens": ["Wer\u00b7den", "sie", "uns", "heu\u00b7te", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "ADV"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Neugeborne, Neugenannte?", "tokens": ["Neu\u00b7ge\u00b7bor\u00b7ne", ",", "Neu\u00b7ge\u00b7nann\u00b7te", "?"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Heute Achtzehnhundertf\u00fcnfzig", "tokens": ["Heu\u00b7te", "Acht\u00b7zehn\u00b7hun\u00b7dert\u00b7f\u00fcnf\u00b7zig"], "token_info": ["word", "word"], "pos": ["ADV", "CARD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hessen, Angeln, Sachsen, Friesen", "tokens": ["Hes\u00b7sen", ",", "An\u00b7geln", ",", "Sach\u00b7sen", ",", "Frie\u00b7sen"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NE", "$,", "NN", "$,", "NE", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Laufen in die Welt des Elends", "tokens": ["Lau\u00b7fen", "in", "die", "Welt", "des", "E\u00b7lends"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ehr- und gl\u00fccks- und landsverwiesen?", "tokens": ["Ehr", "und", "gl\u00fccks", "und", "lands\u00b7ver\u00b7wie\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "TRUNC", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob dem Jammer bricht das Wort ab,", "tokens": ["Ob", "dem", "Jam\u00b7mer", "bricht", "das", "Wort", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Wo die Ehre will zerbrechen \u2013", "tokens": ["Wo", "die", "Eh\u00b7re", "will", "zer\u00b7bre\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo der Helfer? Wo der R\u00e4cher,", "tokens": ["Wo", "der", "Hel\u00b7fer", "?", "Wo", "der", "R\u00e4\u00b7cher", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$.", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Solche grimme Schmach zu r\u00e4chen?", "tokens": ["Sol\u00b7che", "grim\u00b7me", "Schmach", "zu", "r\u00e4\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Still! Es rufet: Du sollst beten,", "tokens": ["Still", "!", "Es", "ru\u00b7fet", ":", "Du", "sollst", "be\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "PPER", "VVFIN", "$.", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Christ, sollst glauben, lieben, hoffen;", "tokens": ["Christ", ",", "sollst", "glau\u00b7ben", ",", "lie\u00b7ben", ",", "hof\u00b7fen", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sperrt sich dir die deutsche Welt auch,", "tokens": ["Sperrt", "sich", "dir", "die", "deut\u00b7sche", "Welt", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPER", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ewig steht der Himmel offen.", "tokens": ["E\u00b7wig", "steht", "der", "Him\u00b7mel", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Drum la\u00df alles durcheinander", "tokens": ["Drum", "la\u00df", "al\u00b7les", "durch\u00b7ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PIS", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fallen, st\u00fcrzen, krachen, brechen,", "tokens": ["Fal\u00b7len", ",", "st\u00fcr\u00b7zen", ",", "kra\u00b7chen", ",", "bre\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Droben, glaube, waltet einer,", "tokens": ["Dro\u00b7ben", ",", "glau\u00b7be", ",", "wal\u00b7tet", "ei\u00b7ner", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "$,", "VVFIN", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der wird letztes Urteil sprechen.", "tokens": ["Der", "wird", "letz\u00b7tes", "Ur\u00b7teil", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}