{"textgrid.poem.45113": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[ich glaube nun selbst: ich bin ein Tor]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich glaube nun selbst: ich bin ein Tor,", "tokens": ["Ich", "glau\u00b7be", "nun", "selbst", ":", "ich", "bin", "ein", "Tor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$.", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In Deutschland sagt man mirs t\u00e4glich vor,", "tokens": ["In", "Deutschland", "sagt", "man", "mirs", "t\u00e4g\u00b7lich", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PIS", "NE", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die aber sich \u00d6streichs Weise nennen,", "tokens": ["Die", "a\u00b7ber", "sich", "\u00d6s\u00b7treichs", "Wei\u00b7se", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PRF", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Geben mirs pantomimisch zu erkennen.", "tokens": ["Ge\u00b7ben", "mirs", "pan\u00b7to\u00b7mi\u00b7misch", "zu", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da \u00fcbrall nur Wert die Wahl bestimmt,", "tokens": ["Da", "\u00fcb\u00b7rall", "nur", "Wert", "die", "Wahl", "be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Man mich \u00fcbergeht und andre nimmt.", "tokens": ["Man", "mich", "\u00fc\u00b7ber\u00b7geht", "und", "and\u00b7re", "nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "VVFIN", "KON", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Ich f\u00fcge mich und entsage dem Grimme,", "tokens": ["Ich", "f\u00fc\u00b7ge", "mich", "und", "ent\u00b7sa\u00b7ge", "dem", "Grim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Der Mehrheit Wort ist der Gottheit Stimme.", "tokens": ["Der", "Mehr\u00b7heit", "Wort", "ist", "der", "Got\u00b7theit", "Stim\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Doch war die Zeit schon manchmal verwirrt,", "tokens": ["Doch", "war", "die", "Zeit", "schon", "manch\u00b7mal", "ver\u00b7wirrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Drum seht nur zu, da\u00df ihr nicht irrt,", "tokens": ["Drum", "seht", "nur", "zu", ",", "da\u00df", "ihr", "nicht", "irrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der Zeitgeist w\u00e4re samt euch verloren,", "tokens": ["Der", "Zeit\u00b7geist", "w\u00e4\u00b7re", "samt", "euch", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Dann w\u00e4r ich klug und ihr alle Toren.", "tokens": ["Dann", "w\u00e4r", "ich", "klug", "und", "ihr", "al\u00b7le", "To\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "PPOSAT", "PIAT", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}