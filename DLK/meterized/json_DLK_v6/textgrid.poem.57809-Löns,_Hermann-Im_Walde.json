{"textgrid.poem.57809": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Im Walde", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Wind auf der Heide,", "tokens": ["Der", "Wind", "auf", "der", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der wei\u00df allerhand,", "tokens": ["Der", "wei\u00df", "al\u00b7ler\u00b7hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Im Wind auf der Heide", "tokens": ["Im", "Wind", "auf", "der", "Hei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ein Jungfr\u00e4ulein stand.", "tokens": ["Ein", "Jung\u00b7fr\u00e4u\u00b7lein", "stand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Guten Tag, sch\u00f6ne Jungfer,", "tokens": ["Gu\u00b7ten", "Tag", ",", "sch\u00f6\u00b7ne", "Jung\u00b7fer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Du allerliebstes Kind,", "tokens": ["Du", "al\u00b7ler\u00b7liebs\u00b7tes", "Kind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da drau\u00dfen auf der Heide,", "tokens": ["Da", "drau\u00b7\u00dfen", "auf", "der", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da wehet der Wind.", "tokens": ["Da", "we\u00b7het", "der", "Wind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Und der Wind und der wehet,", "tokens": ["Und", "der", "Wind", "und", "der", "we\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "ART", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und der Wind, der ist kalt,", "tokens": ["Und", "der", "Wind", ",", "der", "ist", "kalt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "VAFIN", "ADJD", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Was willst du hier frieren,", "tokens": ["Was", "willst", "du", "hier", "frie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Komm' mit in den Wald.", "tokens": ["Komm'", "mit", "in", "den", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Im Wald ist es stille,", "tokens": ["Im", "Wald", "ist", "es", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Da r\u00fchrt sich kein Zweig,", "tokens": ["Da", "r\u00fchrt", "sich", "kein", "Zweig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PIAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Da bl\u00fchen die Blumen,", "tokens": ["Da", "bl\u00fc\u00b7hen", "die", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Da ruht es sich weich.", "tokens": ["Da", "ruht", "es", "sich", "weich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Da l\u00e4\u00dft es sich lieben,", "tokens": ["Da", "l\u00e4\u00dft", "es", "sich", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Kein Mensch wei\u00df darum,", "tokens": ["Kein", "Mensch", "wei\u00df", "da\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PAV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Da stehn lauter B\u00e4ume,", "tokens": ["Da", "stehn", "lau\u00b7ter", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die sind still und stumm.", "tokens": ["Die", "sind", "still", "und", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}