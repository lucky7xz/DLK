{"textgrid.poem.52138": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der alles hat verkocht, der setzt der Weisen Stein,", "tokens": ["Der", "al\u00b7les", "hat", "ver\u00b7kocht", ",", "der", "setzt", "der", "Wei\u00b7sen", "Stein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "VVPP", "$,", "PRELS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der so viel Narren macht, in seinen Tiegel ein:", "tokens": ["Der", "so", "viel", "Nar\u00b7ren", "macht", ",", "in", "sei\u00b7nen", "Tie\u00b7gel", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr Bettler setzet ihn zuerst in euren Orden,", "tokens": ["Ihr", "Bett\u00b7ler", "set\u00b7zet", "ihn", "zu\u00b7erst", "in", "eu\u00b7ren", "Or\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der hat ein Vorthel ja, der wei\u00dflich arm ist worden.", "tokens": ["Der", "hat", "ein", "Vor\u00b7thel", "ja", ",", "der", "wei\u00df\u00b7lich", "arm", "ist", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PTKANT", "$,", "PRELS", "ADJD", "ADJD", "VAFIN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}