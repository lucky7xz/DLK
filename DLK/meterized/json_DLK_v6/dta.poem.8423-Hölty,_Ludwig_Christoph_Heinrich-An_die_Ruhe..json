{"dta.poem.8423": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An die Ruhe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Stiller Haine bewohnst, unter der D\u00e4mmerung", "tokens": ["Stil\u00b7ler", "Hai\u00b7ne", "be\u00b7wohnst", ",", "un\u00b7ter", "der", "D\u00e4m\u00b7me\u00b7rung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "+-+---+--+--", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Mondversilberter Pappeln", "tokens": ["Mond\u00b7ver\u00b7sil\u00b7ber\u00b7ter", "Pap\u00b7peln"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Mit verschlungenen Armen weilst,", "tokens": ["Mit", "ver\u00b7schlun\u00b7ge\u00b7nen", "Ar\u00b7men", "weilst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Mit dem Sch\u00e4fer am Bach fl\u00f6test, der Sch\u00e4ferin", "tokens": ["Mit", "dem", "Sch\u00e4\u00b7fer", "am", "Bach", "fl\u00f6\u00b7test", ",", "der", "Sch\u00e4\u00b7fe\u00b7rin"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "VVFIN", "$,", "ART", "NN"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Unter Blumen der Au singest und Kr\u00e4nze flichst,", "tokens": ["Un\u00b7ter", "Blu\u00b7men", "der", "Au", "sin\u00b7gest", "und", "Kr\u00e4n\u00b7ze", "flichst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "KON", "NN", "VVFIN", "$,"], "meter": "--+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und dem Schellengeklingel", "tokens": ["Und", "dem", "Schel\u00b7len\u00b7ge\u00b7klin\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ihrer tanzenden Sch\u00e4fchen horchst!", "tokens": ["Ih\u00b7rer", "tan\u00b7zen\u00b7den", "Sch\u00e4f\u00b7chen", "horchst", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Wie der J\u00fcngling die Braut liebet, so lieb' ich dich,", "tokens": ["Wie", "der", "J\u00fcng\u00b7ling", "die", "Braut", "lie\u00b7bet", ",", "so", "lieb'", "ich", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PRF", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Allgef\u00e4llige Ruh! sp\u00e4hte dir immer nach,", "tokens": ["All\u00b7ge\u00b7f\u00e4l\u00b7li\u00b7ge", "Ruh", "!", "sp\u00e4h\u00b7te", "dir", "im\u00b7mer", "nach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Bald auf duftenden Wiesen,", "tokens": ["Bald", "auf", "duf\u00b7ten\u00b7den", "Wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Bald im Busche der Nachtigall!", "tokens": ["Bald", "im", "Bu\u00b7sche", "der", "Nach\u00b7ti\u00b7gall", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Endlich bietest du mir, Herzenerfreuerin,", "tokens": ["End\u00b7lich", "bie\u00b7test", "du", "mir", ",", "Her\u00b7ze\u00b7ner\u00b7freu\u00b7e\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Deinen himmlischen Kranz, ach! und umarmest mich,", "tokens": ["Dei\u00b7nen", "himm\u00b7li\u00b7schen", "Kranz", ",", "ach", "!", "und", "um\u00b7ar\u00b7mest", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ITJ", "$.", "KON", "VVFIN", "PPER", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wie den fl\u00f6tenden Sch\u00e4fer,", "tokens": ["Wie", "den", "fl\u00f6\u00b7ten\u00b7den", "Sch\u00e4\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wie die singende Sch\u00e4ferin!", "tokens": ["Wie", "die", "sin\u00b7gen\u00b7de", "Sch\u00e4\u00b7fe\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Jeden Lispel des Baums, jedes Ger\u00e4usch des Bachs,", "tokens": ["Je\u00b7den", "Lis\u00b7pel", "des", "Baums", ",", "je\u00b7des", "Ge\u00b7r\u00e4usch", "des", "Bachs", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,", "PIAT", "NN", "ART", "NN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Jedes l\u00e4ndliche Lied, welches dem Dorf' entweht,", "tokens": ["Je\u00b7des", "l\u00e4nd\u00b7li\u00b7che", "Lied", ",", "wel\u00b7ches", "dem", "Dor\u00b7f'", "ent\u00b7weht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wandelt, G\u00f6ttin, dein Oden", "tokens": ["Wan\u00b7delt", ",", "G\u00f6t\u00b7tin", ",", "dein", "O\u00b7den"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Mir in Sf\u00e4rengesangeston.", "tokens": ["Mir", "in", "Sf\u00e4\u00b7ren\u00b7ge\u00b7san\u00b7ge\u00b7ston", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Hingegossen auf Thau, blick' ich den Abendstern,", "tokens": ["Hin\u00b7ge\u00b7gos\u00b7sen", "auf", "Thau", ",", "blick'", "ich", "den", "A\u00b7bends\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Deinen Liebling, o Ruh, blick' ich den Mond hinan,", "tokens": ["Dei\u00b7nen", "Lieb\u00b7ling", ",", "o", "Ruh", ",", "blick'", "ich", "den", "Mond", "hi\u00b7nan", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Der so freundlich, so freundlich", "tokens": ["Der", "so", "freund\u00b7lich", ",", "so", "freund\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADV", "ADJD", "$,", "ADV", "ADJD"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Durch die nickenden Wipfel schaut!", "tokens": ["Durch", "die", "ni\u00b7cken\u00b7den", "Wip\u00b7fel", "schaut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Ruhe, l\u00e4chle mir stets, wie du mir l\u00e4cheltest,", "tokens": ["Ru\u00b7he", ",", "l\u00e4ch\u00b7le", "mir", "stets", ",", "wie", "du", "mir", "l\u00e4\u00b7chel\u00b7test", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Als mein Knabengelock, mit der entknospeten", "tokens": ["Als", "mein", "Kna\u00b7ben\u00b7ge\u00b7lock", ",", "mit", "der", "ent\u00b7knos\u00b7pe\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "APPR", "ART", "ADJA"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Rosenblume bekr\u00e4nzet,", "tokens": ["Ro\u00b7sen\u00b7blu\u00b7me", "be\u00b7kr\u00e4n\u00b7zet", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Abendl\u00fcftchen zum Spiele flog!", "tokens": ["A\u00b7ben\u00b7dl\u00fcft\u00b7chen", "zum", "Spie\u00b7le", "flog", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "Keiner St\u00e4dterin Reiz, weder ein blaues Aug,", "tokens": ["Kei\u00b7ner", "St\u00e4d\u00b7te\u00b7rin", "Reiz", ",", "we\u00b7der", "ein", "blau\u00b7es", "Aug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Noch ein kusslicher Mund, soll mich aus deinem Arm", "tokens": ["Noch", "ein", "kuss\u00b7li\u00b7cher", "Mund", ",", "soll", "mich", "aus", "dei\u00b7nem", "Arm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "VMFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Zu den Hallen des Tanzes", "tokens": ["Zu", "den", "Hal\u00b7len", "des", "Tan\u00b7zes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Locken, oder des Opernspiels!", "tokens": ["Lo\u00b7cken", ",", "o\u00b7der", "des", "O\u00b7pern\u00b7spiels", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Hier bei Fr\u00fcchten und Milch unter dem Halmendach", "tokens": ["Hier", "bei", "Fr\u00fcch\u00b7ten", "und", "Milch", "un\u00b7ter", "dem", "Hal\u00b7men\u00b7dach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "APPR", "ART", "NN"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Weil, o Freundin, bei mir, bis du mich, an der Hand", "tokens": ["Weil", ",", "o", "Freun\u00b7din", ",", "bei", "mir", ",", "bis", "du", "mich", ",", "an", "der", "Hand"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "$,", "FM", "NN", "$,", "APPR", "PPER", "$,", "KOUS", "PPER", "PRF", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Eines l\u00e4chelnden M\u00e4dchens,", "tokens": ["Ei\u00b7nes", "l\u00e4\u00b7cheln\u00b7den", "M\u00e4d\u00b7chens", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Edens H\u00fctten entgegen f\u00fchrst.", "tokens": ["E\u00b7dens", "H\u00fct\u00b7ten", "ent\u00b7ge\u00b7gen", "f\u00fchrst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}