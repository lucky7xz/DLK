{"textgrid.poem.48040": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Begegnung", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und neulich traf sich's auf der Reise:", "tokens": ["Und", "neu\u00b7lich", "traf", "sich's", "auf", "der", "Rei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "du bogst dein bleiches Angesicht", "tokens": ["du", "bogst", "dein", "blei\u00b7ches", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "am Fenster vor und nicktest leise \u2013", "tokens": ["am", "Fens\u00b7ter", "vor", "und", "nick\u00b7test", "lei\u00b7se", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "dann pfiff der Zug; du sahst mich nicht.", "tokens": ["dann", "pfiff", "der", "Zug", ";", "du", "sahst", "mich", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "War's so, da\u00df wir uns finden sollten", "tokens": ["Wa\u00b7r's", "so", ",", "da\u00df", "wir", "uns", "fin\u00b7den", "soll\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "$,", "KOUS", "PPER", "PRF", "VVINF", "VMFIN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "nach langer Irrfahrt wunderlich?!", "tokens": ["nach", "lan\u00b7ger", "Irr\u00b7fahrt", "wun\u00b7der\u00b7lich", "?!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wei\u00df nicht, wem dein Gru\u00df gegolten,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wem", "dein", "Gru\u00df", "ge\u00b7gol\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und nahm ihn l\u00e4chelnd an \u2013 f\u00fcr mich.", "tokens": ["und", "nahm", "ihn", "l\u00e4\u00b7chelnd", "an", "\u2013", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$(", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}