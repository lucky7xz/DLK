{"textgrid.poem.37684": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Auf der Erde ist es schw\u00fcl", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf der Erde ist es schw\u00fcl,", "tokens": ["Auf", "der", "Er\u00b7de", "ist", "es", "schw\u00fcl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In den Wassern ist es k\u00fchl,", "tokens": ["In", "den", "Was\u00b7sern", "ist", "es", "k\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sonne, Mond und alle Sterne", "tokens": ["Son\u00b7ne", ",", "Mond", "und", "al\u00b7le", "Ster\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00fcrzen sich hinein so gerne,", "tokens": ["St\u00fcr\u00b7zen", "sich", "hin\u00b7ein", "so", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADV", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn im Wasser wird's so klar,", "tokens": ["Denn", "im", "Was\u00b7ser", "wird's", "so", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie's auf Erden traurig war.", "tokens": ["Wie's", "auf", "Er\u00b7den", "trau\u00b7rig", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ruhig schlaft ihr bei uns ein", "tokens": ["Ru\u00b7hig", "schlaft", "ihr", "bei", "uns", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PPER", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Wasser gr\u00fcnem Schein,", "tokens": ["In", "der", "Was\u00b7ser", "gr\u00fc\u00b7nem", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6ret keine Kinder schrein,", "tokens": ["H\u00f6\u00b7ret", "kei\u00b7ne", "Kin\u00b7der", "schrein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchlet keine Liebespein,", "tokens": ["F\u00fch\u00b7let", "kei\u00b7ne", "Lie\u00b7be\u00b7spein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebet ohne Eifersucht,", "tokens": ["Lie\u00b7bet", "oh\u00b7ne", "Ei\u00b7fer\u00b7sucht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Findet alles, was ihr sucht.", "tokens": ["Fin\u00b7det", "al\u00b7les", ",", "was", "ihr", "sucht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was verloren in dem Meer,", "tokens": ["Was", "ver\u00b7lo\u00b7ren", "in", "dem", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Stehet da im Haus umher,", "tokens": ["Ste\u00b7het", "da", "im", "Haus", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alter Zeiten Sch\u00e4tz und Kunst", "tokens": ["Al\u00b7ter", "Zei\u00b7ten", "Sch\u00e4tz", "und", "Kunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Brauchet ihr durch unsre Gunst,", "tokens": ["Brau\u00b7chet", "ihr", "durch", "uns\u00b7re", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jeder Sturm bringt neue G\u00e4st'", "tokens": ["Je\u00b7der", "Sturm", "bringt", "neu\u00b7e", "G\u00e4st'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu dem ew'gen Freudenfest.", "tokens": ["Zu", "dem", "ew'\u00b7gen", "Freu\u00b7den\u00b7fest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn wir tanzen in dem Kreis,", "tokens": ["Wenn", "wir", "tan\u00b7zen", "in", "dem", "Kreis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wirbelt sich die Welle wei\u00df,", "tokens": ["Wir\u00b7belt", "sich", "die", "Wel\u00b7le", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wenn wir unten lustig sind,", "tokens": ["Wenn", "wir", "un\u00b7ten", "lus\u00b7tig", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00fcrmet \u00fcber uns der Wind,", "tokens": ["St\u00fcr\u00b7met", "\u00fc\u00b7ber", "uns", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "St\u00fcrmt in unsrer Haare Glanz,", "tokens": ["St\u00fcrmt", "in", "uns\u00b7rer", "Haa\u00b7re", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und das k\u00fchlet in dem Tanz.", "tokens": ["Und", "das", "k\u00fch\u00b7let", "in", "dem", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}