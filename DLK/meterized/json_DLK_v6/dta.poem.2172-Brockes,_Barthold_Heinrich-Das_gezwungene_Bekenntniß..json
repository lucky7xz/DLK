{"dta.poem.2172": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das gezwungene Bekenntni\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die\u00df sind die Triebe, die sich fast bey allen Menschen", "tokens": ["Die\u00df", "sind", "die", "Trie\u00b7be", ",", "die", "sich", "fast", "bey", "al\u00b7len", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PRF", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "finden lassen:", "tokens": ["fin\u00b7den", "las\u00b7sen", ":"], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Sie suchen zu erniedrigen, was sie bewundern, und sie", "tokens": ["Sie", "su\u00b7chen", "zu", "er\u00b7nied\u00b7ri\u00b7gen", ",", "was", "sie", "be\u00b7wun\u00b7dern", ",", "und", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVINF", "PTKZU", "VVINF", "$,", "PRELS", "PPER", "VVINF", "$,", "KON", "PPER"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.4": {"text": "hassen", "tokens": ["has\u00b7sen"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Die, so sie nicht verachten k\u00f6nnen; sie sind, an denen,", "tokens": ["Die", ",", "so", "sie", "nicht", "ver\u00b7ach\u00b7ten", "k\u00f6n\u00b7nen", ";", "sie", "sind", ",", "an", "de\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "PPER", "PTKNEG", "VVFIN", "VMINF", "$.", "PPER", "VAFIN", "$,", "APPR", "PRELS", "$,"], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.6": {"text": "die sie sch\u00e4tzen,", "tokens": ["die", "sie", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Bem\u00fchet, Fehler zu entdecken, an ihnen etwas auszu-", "tokens": ["Be\u00b7m\u00fc\u00b7het", ",", "Feh\u00b7ler", "zu", "ent\u00b7de\u00b7cken", ",", "an", "ih\u00b7nen", "et\u00b7was", "aus\u00b7zu"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "PTKZU", "VVINF", "$,", "APPR", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "setzen.", "tokens": ["set\u00b7zen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Doch zwinget sie die Macht der Tugend und der Ver-", "tokens": ["Doch", "zwin\u00b7get", "sie", "die", "Macht", "der", "Tu\u00b7gend", "und", "der", "Ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "KON", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "dienste zum Vertrauen", "tokens": ["diens\u00b7te", "zum", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Zu der in ihnen anerkannten aufrichtigen Beschaffenheit.", "tokens": ["Zu", "der", "in", "ih\u00b7nen", "an\u00b7er\u00b7kann\u00b7ten", "auf\u00b7rich\u00b7ti\u00b7gen", "Be\u00b7schaf\u00b7fen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.12": {"text": "Die\u00df zeigt der Tugend wahren Werth und ist nicht an-", "tokens": ["Die\u00df", "zeigt", "der", "Tu\u00b7gend", "wah\u00b7ren", "Werth", "und", "ist", "nicht", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADJA", "NN", "KON", "VAFIN", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "ders anzuschauen,", "tokens": ["ders", "an\u00b7zu\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVIZU", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Als ein gezwungenes Bekenutni\u00df von ihrer Ungerech-", "tokens": ["Als", "ein", "ge\u00b7zwun\u00b7ge\u00b7nes", "Be\u00b7ke\u00b7nut\u00b7ni\u00df", "von", "ih\u00b7rer", "Un\u00b7ge\u00b7rech"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+---+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.15": {"text": "tigkeit.", "tokens": ["tig\u00b7keit", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+", "measure": "iambic.single"}}}}}