{"textgrid.poem.38147": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Hans in allen Gassen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich will einmal spaziren gehn,", "tokens": ["Ich", "will", "ein\u00b7mal", "spa\u00b7zi\u00b7ren", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und suchen meine Freud,", "tokens": ["Und", "su\u00b7chen", "mei\u00b7ne", "Freud", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Begegnet mir ja alsobald,", "tokens": ["Be\u00b7geg\u00b7net", "mir", "ja", "al\u00b7so\u00b7bald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ha ha, ja ja, ja alsobald,", "tokens": ["Ha", "ha", ",", "ja", "ja", ",", "ja", "al\u00b7so\u00b7bald", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$,", "ADV", "ADV", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Kn\u00e4blein war sch\u00f6n bekleidt.", "tokens": ["Ein", "Kn\u00e4\u00b7blein", "war", "sch\u00f6n", "be\u00b7kleidt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Zwei Fl\u00fcglein th\u00e4t er tragen,", "tokens": ["Zwei", "Fl\u00fcg\u00b7lein", "th\u00e4t", "er", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Bogen in seiner Hand,", "tokens": ["Ein", "Bo\u00b7gen", "in", "sei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er th\u00e4t gleich zu mir sagen,", "tokens": ["Er", "th\u00e4t", "gleich", "zu", "mir", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ha ha, ja ja, ja sagen,", "tokens": ["Ha", "ha", ",", "ja", "ja", ",", "ja", "sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$,", "ADV", "ADV", "$,", "ADV", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Schenk mir dein Herz zum Pfand.", "tokens": ["Schenk", "mir", "dein", "Herz", "zum", "Pfand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was thust du da, du kleiner Bub?", "tokens": ["Was", "thust", "du", "da", ",", "du", "klei\u00b7ner", "Bub", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was machst du hier im Wald?", "tokens": ["Was", "machst", "du", "hier", "im", "Wald", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du g'h\u00f6rst nach Haus in deine Ruh,", "tokens": ["Du", "g'\u00b7h\u00f6rst", "nach", "Haus", "in", "dei\u00b7ne", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Ha ha, ja ja, in deine Ruh,", "tokens": ["Ha", "ha", ",", "ja", "ja", ",", "in", "dei\u00b7ne", "Ruh", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$,", "ADV", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Nacht ist dir zu kalt.", "tokens": ["Die", "Nacht", "ist", "dir", "zu", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Seine Aeuglein hat er verbunden,", "tokens": ["Sei\u00b7ne", "A\u00b7e\u00b7u\u00b7glein", "hat", "er", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Mit einem schwarzen Flor,", "tokens": ["Mit", "ei\u00b7nem", "schwar\u00b7zen", "Flor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du machst mir ja viel Wunden,", "tokens": ["Du", "machst", "mir", "ja", "viel", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ha ha, ja ja, viel Wunden,", "tokens": ["Ha", "ha", ",", "ja", "ja", ",", "viel", "Wun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$,", "ADV", "ADV", "$,", "PIAT", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Du kleiner Kupido.", "tokens": ["Du", "klei\u00b7ner", "Ku\u00b7pi\u00b7do", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Itzt will ich erst recht lieben,", "tokens": ["Itzt", "will", "ich", "erst", "recht", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weils die Leut verdriessen thut,", "tokens": ["Weils", "die", "Leut", "ver\u00b7dries\u00b7sen", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich wills nicht mehr aufschieben,", "tokens": ["Ich", "wills", "nicht", "mehr", "auf\u00b7schie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ha ha, ja ja, aufschieben,", "tokens": ["Ha", "ha", ",", "ja", "ja", ",", "auf\u00b7schie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "ITJ", "$,", "ADV", "ADV", "$,", "VVIZU", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Wills nehmen f\u00fcr mein Bu\u00df.", "tokens": ["Wills", "neh\u00b7men", "f\u00fcr", "mein", "Bu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}