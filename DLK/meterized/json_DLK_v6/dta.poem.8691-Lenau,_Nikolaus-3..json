{"dta.poem.8691": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Er hat geliebt! &#8212; vor langer, tr\u00fcber Zeit,", "tokens": ["Er", "hat", "ge\u00b7liebt", "!", "&#8212;", "vor", "lan\u00b7ger", ",", "tr\u00fc\u00b7ber", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "XML_entity", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "$(", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da ging er einst, ein fr\u00f6hlicher Geselle,", "tokens": ["Da", "ging", "er", "einst", ",", "ein", "fr\u00f6h\u00b7li\u00b7cher", "Ge\u00b7sel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit seinem Lieb durch diese Einsamkeit,", "tokens": ["Mit", "sei\u00b7nem", "Lieb", "durch", "die\u00b7se", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und kam mit ihr zur stillen Waldkapelle.", "tokens": ["Und", "kam", "mit", "ihr", "zur", "stil\u00b7len", "Wald\u00b7ka\u00b7pel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie traten ein, sie knieten hin; da glomm", "tokens": ["Sie", "tra\u00b7ten", "ein", ",", "sie", "knie\u00b7ten", "hin", ";", "da", "glomm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch's Fenster hell herein die Abendr\u00f6the,", "tokens": ["Durch's", "Fens\u00b7ter", "hell", "her\u00b7ein", "die", "A\u00b7bend\u00b7r\u00f6\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "PTKVZ", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er betete mit ihr so selig fromm,", "tokens": ["Er", "be\u00b7te\u00b7te", "mit", "ihr", "so", "se\u00b7lig", "fromm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und drau\u00dfen sang des Hirten weiche Fl\u00f6te.", "tokens": ["Und", "drau\u00b7\u00dfen", "sang", "des", "Hir\u00b7ten", "wei\u00b7che", "Fl\u00f6\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da hob die Hand sie schnell und feierlich,", "tokens": ["Da", "hob", "die", "Hand", "sie", "schnell", "und", "fei\u00b7er\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sprach, so schien's, mit tiefbewegter Stimme:", "tokens": ["Und", "sprach", ",", "so", "schien's", ",", "mit", "tief\u00b7be\u00b7weg\u00b7ter", "Stim\u00b7me", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "ADJA", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u201elieb ich nicht warm, und treu, und ewig dich,", "tokens": ["\u201e", "lieb", "ich", "nicht", "warm", ",", "und", "treu", ",", "und", "e\u00b7wig", "dich", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "PPER", "PTKNEG", "ADJD", "$,", "KON", "ADJD", "$,", "KON", "ADJD", "PPER", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "\u201eso strafe mich der Herr mit seinem Grimme!\u201c", "tokens": ["\u201e", "so", "stra\u00b7fe", "mich", "der", "Herr", "mit", "sei\u00b7nem", "Grim\u00b7me", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und h\u00f6her glomm der helle Abendstrahl,", "tokens": ["Und", "h\u00f6\u00b7her", "glomm", "der", "hel\u00b7le", "A\u00b7bend\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So wie sein Herz, sich ewig ihr zu weihen,", "tokens": ["So", "wie", "sein", "Herz", ",", "sich", "e\u00b7wig", "ihr", "zu", "wei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPOSAT", "NN", "$,", "PRF", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und drau\u00dfen klang im stillen Waldesthal", "tokens": ["Und", "drau\u00b7\u00dfen", "klang", "im", "stil\u00b7len", "Wal\u00b7dest\u00b7hal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Hirten Lied wie Himmelsmelodeien. \u2014", "tokens": ["Des", "Hir\u00b7ten", "Lied", "wie", "Him\u00b7mels\u00b7me\u00b7lo\u00b7dei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "KOKOM", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wie bald, wie bald, da\u00df ihn ihr Herz vergi\u00dft!", "tokens": ["Wie", "bald", ",", "wie", "bald", ",", "da\u00df", "ihn", "ihr", "Herz", "ver\u00b7gi\u00dft", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "PWAV", "ADV", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ihr ein Andrer schon des falschen Eides", "tokens": ["Da\u00df", "ihr", "ein", "A\u00b7ndrer", "schon", "des", "fal\u00b7schen", "Ei\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das lezte Wort von falscher Lippe k\u00fc\u00dft,", "tokens": ["Das", "lez\u00b7te", "Wort", "von", "fal\u00b7scher", "Lip\u00b7pe", "k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie mit dem Glanze schm\u00fcckt des Brautgeschmeides!", "tokens": ["Sie", "mit", "dem", "Glan\u00b7ze", "schm\u00fcckt", "des", "Braut\u00b7ge\u00b7schmei\u00b7des", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "Und all' ihr Leben, Freudentaumel nur,", "tokens": ["Und", "all'", "ihr", "Le\u00b7ben", ",", "Freu\u00b7den\u00b7tau\u00b7mel", "nur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "PPOSAT", "NN", "$,", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den noch kein fl\u00fcchtig Leid ihr jemals st\u00f6rte,", "tokens": ["Den", "noch", "kein", "fl\u00fcch\u00b7tig", "Leid", "ihr", "je\u00b7mals", "st\u00f6r\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "ADJD", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zieht, unverfolgt von ihrem falschen Schwur,", "tokens": ["Zieht", ",", "un\u00b7ver\u00b7folgt", "von", "ih\u00b7rem", "fal\u00b7schen", "Schwur", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "APPR", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und frech am Gott vor\u00fcber, der ihn h\u00f6rte. \u2014", "tokens": ["Und", "frech", "am", "Gott", "vor\u00b7\u00fc\u00b7ber", ",", "der", "ihn", "h\u00f6r\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "PTKVZ", "$,", "PRELS", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Das war's, o Schicksal, was der Mensch gethan,", "tokens": ["Das", "wa\u00b7r's", ",", "o", "Schick\u00b7sal", ",", "was", "der", "Mensch", "ge\u00b7than", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "FM", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Da\u00df mit des Wahnsinns bangen Finsternissen", "tokens": ["Da\u00df", "mit", "des", "Wahn\u00b7sinns", "ban\u00b7gen", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du ihm versch\u00fcttet hast die Lebensbahn,", "tokens": ["Du", "ihm", "ver\u00b7sch\u00fct\u00b7tet", "hast", "die", "Le\u00b7bens\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aus seiner Seele seinen Gott gerissen!", "tokens": ["Aus", "sei\u00b7ner", "See\u00b7le", "sei\u00b7nen", "Gott", "ge\u00b7ris\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "D'rum flucht er nun empor mit wildem Spott,", "tokens": ["D'\u00b7rum", "flucht", "er", "nun", "em\u00b7por", "mit", "wil\u00b7dem", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Gequ\u00e4lt von seinem Schmerz, an jener Stelle,", "tokens": ["Ge\u00b7qu\u00e4lt", "von", "sei\u00b7nem", "Schmerz", ",", "an", "je\u00b7ner", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo er so selig einst gekniet vor Gott;", "tokens": ["Wo", "er", "so", "se\u00b7lig", "einst", "ge\u00b7kniet", "vor", "Gott", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "ADV", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "D'rum irrt er, wie gebannt, um die Kapelle.", "tokens": ["D'\u00b7rum", "irrt", "er", ",", "wie", "ge\u00b7bannt", ",", "um", "die", "Ka\u00b7pel\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "PWAV", "VVPP", "$,", "KOUI", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}