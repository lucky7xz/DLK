{"textgrid.poem.58908": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "127. Ein Kind hat Ruh", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kummer, Furcht und Zweifelswinde", "tokens": ["Kum\u00b7mer", ",", "Furcht", "und", "Zwei\u00b7fels\u00b7win\u00b7de"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00fcrmen \u00f6fters auf dich zu;", "tokens": ["St\u00fcr\u00b7men", "\u00f6f\u00b7ters", "auf", "dich", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nnt'st du werden recht zum Kinde,", "tokens": ["K\u00f6nnt'st", "du", "wer\u00b7den", "recht", "zum", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VAFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Glaube, Seel', du bliebst in Ruh!", "tokens": ["Glau\u00b7be", ",", "Seel'", ",", "du", "bliebst", "in", "Ruh", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}