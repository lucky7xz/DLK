{"textgrid.poem.49830": {"metadata": {"author": {"name": "Sachs, Hans", "birth": "N.A.", "death": "N.A."}, "title": "Schwank: Der gute montag", "genre": "verse", "period": "N.A.", "pub_year": 1559, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nachdem ich meim hantwerk nachzug", "tokens": ["Nach\u00b7dem", "ich", "meim", "hant\u00b7werk", "nach\u00b7zug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "NN"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "an dem reinstram, es sich zutrug,", "tokens": ["an", "dem", "rein\u00b7stram", ",", "es", "sich", "zu\u00b7trug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "$,", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "eins morgens fr\u00fc zu bet ich lag,", "tokens": ["eins", "mor\u00b7gens", "fr\u00fc", "zu", "bet", "ich", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADJD", "PTKZU", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "gedacht: heut ist guter montag,", "tokens": ["ge\u00b7dacht", ":", "heut", "ist", "gu\u00b7ter", "mon\u00b7tag", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "ADV", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "da wil dem meister feiren ich.", "tokens": ["da", "wil", "dem", "meis\u00b7ter", "fei\u00b7ren", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "in dem entschlief ich senftiglich", "tokens": ["in", "dem", "ent\u00b7schlief", "ich", "senf\u00b7tig\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "wider ein stunde oder zwu,", "tokens": ["wi\u00b7der", "ein", "stun\u00b7de", "o\u00b7der", "zwu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "bi\u00df der tag rucket ba\u00df herzu.", "tokens": ["bi\u00df", "der", "tag", "ru\u00b7cket", "ba\u00df", "her\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "im schlaf erschin mir ein gesicht,", "tokens": ["im", "schlaf", "er\u00b7schin", "mir", "ein", "ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PPER", "ART", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "des inhalts ich mit kurz bericht:", "tokens": ["des", "in\u00b7halts", "ich", "mit", "kurz", "be\u00b7richt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "ich sach gar selzamer manier", "tokens": ["ich", "sach", "gar", "sel\u00b7za\u00b7mer", "ma\u00b7nier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.12": {"text": "ein wundergro\u00dfes selzams tier,", "tokens": ["ein", "wun\u00b7der\u00b7gro\u00b7\u00dfes", "sel\u00b7zams", "tier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "das tet her auf sechs f\u00fc\u00dfen gen,", "tokens": ["das", "tet", "her", "auf", "sechs", "f\u00fc\u00b7\u00dfen", "gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "CARD", "NN", "VVPP", "$,"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.14": {"text": "im maul het es scharpf eberzen,", "tokens": ["im", "maul", "het", "es", "scharpf", "e\u00b7ber\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+--+---", "measure": "iambic.di.relaxed"}, "line.15": {"text": "sein bauch war als ein f\u00fcdrig fa\u00df,", "tokens": ["sein", "bauch", "war", "als", "ein", "f\u00fcd\u00b7rig", "fa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "sein schwanz schebig und reudig was.", "tokens": ["sein", "schwanz", "sche\u00b7big", "und", "reu\u00b7dig", "was", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADJD", "KON", "ADJD", "PWS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "ich erschrak und floch hin von im;", "tokens": ["ich", "er\u00b7schrak", "und", "floch", "hin", "von", "im", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "APPR", "APPRART", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "da redt das tier menschliche stim:", "tokens": ["da", "redt", "das", "tier", "menschli\u00b7che", "stim", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "fleuch nit, du hast mich doch aus gnaden", "tokens": ["fleuch", "nit", ",", "du", "hast", "mich", "doch", "aus", "gna\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKNEG", "$,", "PPER", "VAFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "auf heut freuntlich zu dir geladen.", "tokens": ["auf", "heut", "freunt\u00b7lich", "zu", "dir", "ge\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.21": {"text": "ich sprach: wer bist? zeig mir das an,", "tokens": ["ich", "sprach", ":", "wer", "bist", "?", "zeig", "mir", "das", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWS", "VAFIN", "$.", "VVFIN", "PPER", "PDS", "PTKVZ", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.22": {"text": "ich mag dich schlecht nit bei mir han,", "tokens": ["ich", "mag", "dich", "schlecht", "nit", "bei", "mir", "han", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "PTKNEG", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "weil du so gar unbschaffen bist,", "tokens": ["weil", "du", "so", "gar", "unb\u00b7schaf\u00b7fen", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "des rechten haus hast du vermist,", "tokens": ["des", "rech\u00b7ten", "haus", "hast", "du", "ver\u00b7mist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "sichst mich f\u00fcr ein unrechten an.", "tokens": ["sichst", "mich", "f\u00fcr", "ein", "un\u00b7rech\u00b7ten", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "das tier antwort mir: lieber man,", "tokens": ["das", "tier", "ant\u00b7wort", "mir", ":", "lie\u00b7ber", "man", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "$.", "ADV", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "du tust mich aus der ma\u00df wol kennen,", "tokens": ["du", "tust", "mich", "aus", "der", "ma\u00df", "wol", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "voraus balt ich mich dir tu nennen,", "tokens": ["vo\u00b7raus", "balt", "ich", "mich", "dir", "tu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PPER", "NE", "VVINF", "$,"], "meter": "--+-+-+--", "measure": "anapaest.init"}, "line.29": {"text": "wi\u00df, das ich der gut montag bin.", "tokens": ["wi\u00df", ",", "das", "ich", "der", "gut", "mon\u00b7tag", "bin", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "PPER", "ART", "ADJD", "NN", "VAFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.30": {"text": "wolauf ins wirtshaus mit mir hin", "tokens": ["wo\u00b7lauf", "ins", "wirts\u00b7haus", "mit", "mir", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "zu andern gsellen, die dein warten", "tokens": ["zu", "an\u00b7dern", "gsel\u00b7len", ",", "die", "dein", "war\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIS", "VVINF", "$,", "PRELS", "PPOSAT", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "mit speis und trank, w\u00fcrfel und karten!", "tokens": ["mit", "speis", "und", "trank", ",", "w\u00fcr\u00b7fel", "und", "kar\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "VVFIN", "$,", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "die haben mich geschickt nach dir,", "tokens": ["die", "ha\u00b7ben", "mich", "ge\u00b7schickt", "nach", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVPP", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "und ob du nit wolst gen mit mir,", "tokens": ["und", "ob", "du", "nit", "wolst", "gen", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "VMFIN", "APPR", "APPR", "PPER", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.35": {"text": "so wolt ich dich mit gwalt hintragen.", "tokens": ["so", "wolt", "ich", "dich", "mit", "gwalt", "hin\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "ich tet zum guten montag sagen:", "tokens": ["ich", "tet", "zum", "gu\u00b7ten", "mon\u00b7tag", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.37": {"text": "wie bist du denn so stark und kreftig?", "tokens": ["wie", "bist", "du", "denn", "so", "stark", "und", "kref\u00b7tig", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "der gut montag sprach: ich bin scheftig", "tokens": ["der", "gut", "mon\u00b7tag", "sprach", ":", "ich", "bin", "schef\u00b7tig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$.", "PPER", "VAFIN", "ADJD"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.39": {"text": "in merk und steten \u00fcberal,", "tokens": ["in", "merk", "und", "ste\u00b7ten", "\u00fc\u00b7be\u00b7ral", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "die hantwerksbursch mit \u00fcberschwal", "tokens": ["die", "hant\u00b7werks\u00b7bursch", "mit", "\u00fc\u00b7bersc\u00b7hwal"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "hab ich all unter meinen fanen,", "tokens": ["hab", "ich", "all", "un\u00b7ter", "mei\u00b7nen", "fa\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.42": {"text": "dergleich hersch ich vil hantwerksmanen,", "tokens": ["derg\u00b7leich", "hersch", "ich", "vil", "hant\u00b7werks\u00b7ma\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "die mir gar willig zu hof reiten,", "tokens": ["die", "mir", "gar", "wil\u00b7lig", "zu", "hof", "rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "samt den gsellen zu allen zeiten.", "tokens": ["samt", "den", "gsel\u00b7len", "zu", "al\u00b7len", "zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.45": {"text": "ich sprach: wie das du hast sechs bein?", "tokens": ["ich", "sprach", ":", "wie", "das", "du", "hast", "sechs", "bein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWAV", "ART", "PPER", "VAFIN", "CARD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "er sprach: mein gang ist schwind, allein", "tokens": ["er", "sprach", ":", "mein", "gang", "ist", "schwind", ",", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "kom allemal \u00fcber sechs tag;", "tokens": ["kom", "al\u00b7le\u00b7mal", "\u00fc\u00b7ber", "sechs", "tag", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "CARD", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.48": {"text": "oft man mich nit austreiben mag", "tokens": ["oft", "man", "mich", "nit", "aus\u00b7trei\u00b7ben", "mag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "PPER", "PTKNEG", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "bi\u00df gar hinein auf die mitwochen", "tokens": ["bi\u00df", "gar", "hin\u00b7ein", "auf", "die", "mit\u00b7wo\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "APPR", "ART", "ADJA"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.50": {"text": "weder mit schelten noch mit pochen,", "tokens": ["we\u00b7der", "mit", "schel\u00b7ten", "noch", "mit", "po\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "VVFIN", "ADV", "APPR", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.51": {"text": "wiewol ich bring gar wenig nutz,", "tokens": ["wie\u00b7wol", "ich", "bring", "gar", "we\u00b7nig", "nutz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "wo man mir flei\u00dfig heltet schutz.", "tokens": ["wo", "man", "mir", "flei\u00b7\u00dfig", "hel\u00b7tet", "schutz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "ADJD", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "ich sprach: wie hast so scharpfe zen?", "tokens": ["ich", "sprach", ":", "wie", "hast", "so", "scharp\u00b7fe", "zen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWAV", "VAFIN", "ADV", "VVFIN", "VVINF", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.54": {"text": "