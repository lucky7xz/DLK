{"dta.poem.11837": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie fern der Wirklichkeit, wie fern der Ahnung liegt", "tokens": ["Wie", "fern", "der", "Wirk\u00b7lich\u00b7keit", ",", "wie", "fern", "der", "Ah\u00b7nung", "liegt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "$,", "PWAV", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Unschuld Friedenswelt, wonach die Sehnsucht fliegt!", "tokens": ["Der", "Un\u00b7schuld", "Frie\u00b7dens\u00b7welt", ",", "wo\u00b7nach", "die", "Sehn\u00b7sucht", "fliegt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wo mit dem Aeu\u00dfern nicht im Streit das Innre war,", "tokens": ["Wo", "mit", "dem", "A\u00b7e\u00b7u\u00b7\u00dfern", "nicht", "im", "Streit", "das", "Inn\u00b7re", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PTKNEG", "APPRART", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Dem Geiste klar die Welt, und er sich selber klar.", "tokens": ["Dem", "Geis\u00b7te", "klar", "die", "Welt", ",", "und", "er", "sich", "sel\u00b7ber", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ART", "NN", "$,", "KON", "PPER", "PRF", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wo rein im Wunschgenu\u00df war Wunschbefriedigung,", "tokens": ["Wo", "rein", "im", "Wunschge\u00b7nu\u00df", "war", "Wunschbe\u00b7frie\u00b7di\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPRART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von Erdenschwere nicht behindert Himmelschwung.", "tokens": ["Von", "Er\u00b7den\u00b7schwe\u00b7re", "nicht", "be\u00b7hin\u00b7dert", "Him\u00b7mel\u00b7schwung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wir w\u00fcnschen, Kindern gleich, nun Fest um Fest heran;", "tokens": ["Wir", "w\u00fcn\u00b7schen", ",", "Kin\u00b7dern", "gleich", ",", "nun", "Fest", "um", "Fest", "he\u00b7ran", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "ADV", "$,", "ADV", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wie es ist erreicht, so ist es abgethan.", "tokens": ["Und", "wie", "es", "ist", "er\u00b7reicht", ",", "so", "ist", "es", "ab\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VAFIN", "VVPP", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "In n\u00e4chster Zukunft scheint das goldne Gl\u00fcck zu liegen,", "tokens": ["In", "n\u00e4chs\u00b7ter", "Zu\u00b7kunft", "scheint", "das", "gold\u00b7ne", "Gl\u00fcck", "zu", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wird sie Gegenwart, so sehn wirs weiter fliegen.", "tokens": ["Und", "wird", "sie", "Ge\u00b7gen\u00b7wart", ",", "so", "sehn", "wirs", "wei\u00b7ter", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "$,", "ADV", "VVFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Dein ganzes Leben ist verfallen dem Geschick,", "tokens": ["Dein", "gan\u00b7zes", "Le\u00b7ben", "ist", "ver\u00b7fal\u00b7len", "dem", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "VVINF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gewinnen mu\u00dft du's neu in jedem Augenblick.", "tokens": ["Ge\u00b7win\u00b7nen", "mu\u00dft", "du's", "neu", "in", "je\u00b7dem", "Au\u00b7gen\u00b7blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Aus jedem Pl\u00e4tzchen l\u00e4\u00dft ein Paradies sich machen,", "tokens": ["Aus", "je\u00b7dem", "Pl\u00e4tz\u00b7chen", "l\u00e4\u00dft", "ein", "Pa\u00b7ra\u00b7dies", "sich", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und neugeschaffen f\u00fchlt sich t\u00e4glich dein Erwachen.", "tokens": ["Und", "neu\u00b7ge\u00b7schaf\u00b7fen", "f\u00fchlt", "sich", "t\u00e4g\u00b7lich", "dein", "Er\u00b7wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVFIN", "PRF", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Und neugeboren schl\u00e4ft die Welt in jedem Kinde,", "tokens": ["Und", "neu\u00b7ge\u00b7bo\u00b7ren", "schl\u00e4ft", "die", "Welt", "in", "je\u00b7dem", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr Alter f\u00fchlt sich jung in jedem Fr\u00fchlingswinde.", "tokens": ["Ihr", "Al\u00b7ter", "f\u00fchlt", "sich", "jung", "in", "je\u00b7dem", "Fr\u00fch\u00b7lings\u00b7win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Das Alles ist ein Hauch, ein Schatten und ein Traum,", "tokens": ["Das", "Al\u00b7les", "ist", "ein", "Hauch", ",", "ein", "Schat\u00b7ten", "und", "ein", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch kann das Ewige nicht anders stehn im Raum.", "tokens": ["Doch", "kann", "das", "E\u00b7wi\u00b7ge", "nicht", "an\u00b7ders", "stehn", "im", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "PTKNEG", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}