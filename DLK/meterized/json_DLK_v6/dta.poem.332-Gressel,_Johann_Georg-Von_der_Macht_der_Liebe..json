{"dta.poem.332": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Von der Macht der Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "S\u00fcsse Liebe!", "tokens": ["S\u00fcs\u00b7se", "Lie\u00b7be", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Deine Triebe", "tokens": ["Dei\u00b7ne", "Trie\u00b7be"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Nehmen Hertz und Sinnen ein.", "tokens": ["Neh\u00b7men", "Hertz", "und", "Sin\u00b7nen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "All\u2019 und jede Geister m\u00fcssen", "tokens": ["All'", "und", "je\u00b7de", "Geis\u00b7ter", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "KON", "PIAT", "NN", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich von dir gefesselt wissen", "tokens": ["Sich", "von", "dir", "ge\u00b7fes\u00b7selt", "wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "PPER", "VVPP", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Keiner kan verschonet seyn.", "tokens": ["Kei\u00b7ner", "kan", "ver\u00b7scho\u00b7net", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "S\u00fcsse Liebe!", "tokens": ["S\u00fcs\u00b7se", "Lie\u00b7be", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Deine Triebe", "tokens": ["Dei\u00b7ne", "Trie\u00b7be"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Nehmen Hertz und Sinnen ein", "tokens": ["Neh\u00b7men", "Hertz", "und", "Sin\u00b7nen", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Simsons grosse Wunder-St\u00e4rcke", "tokens": ["Sim\u00b7sons", "gros\u00b7se", "Wun\u00b7der\u00b7St\u00e4r\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "K\u00f6nnen deine Wunder-Macht", "tokens": ["K\u00f6n\u00b7nen", "dei\u00b7ne", "Wun\u00b7der\u00b7Macht"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Niemahls werden gleich geacht.", "tokens": ["Nie\u00b7mahls", "wer\u00b7den", "gleich", "ge\u00b7acht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Stein und Eysen mu\u00df sich schwiegen", "tokens": ["Stein", "und", "Ey\u00b7sen", "mu\u00df", "sich", "schwie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VMFIN", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ein Felsen Hertze biegen/", "tokens": ["Und", "ein", "Fel\u00b7sen", "Hert\u00b7ze", "bie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn der ", "tokens": ["Wenn", "der"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Und sie Gnad und Huld verkennt.", "tokens": ["Und", "sie", "Gnad", "und", "Huld", "ver\u00b7kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Keiner kan ihr widersiehen", "tokens": ["Kei\u00b7ner", "kan", "ihr", "wi\u00b7der\u00b7sie\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Jeder mu\u00df Fu\u00df-f\u00e4llig seyn.", "tokens": ["Je\u00b7der", "mu\u00df", "Fu\u00df\u00b7f\u00e4l\u00b7lig", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Jhr Entschliessen mu\u00df ergehen", "tokens": ["Ihr", "Ent\u00b7schlies\u00b7sen", "mu\u00df", "er\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Gehts auch noch so widrig ein.", "tokens": ["Gehts", "auch", "noch", "so", "wid\u00b7rig", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Der grosse ", "tokens": ["Der", "gros\u00b7se"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Den Liebes-Printz nimmt ", "tokens": ["Den", "Lie\u00b7bes\u00b7Printz", "nimmt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "So mu\u00df der Donner-Gott/ der Kriegs- und Liebes-F\u00fchrer", "tokens": ["So", "mu\u00df", "der", "Don\u00b7ner\u00b7Gott", "/", "der", "Kriegs", "und", "Lie\u00b7bes\u00b7F\u00fch\u00b7rer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "$(", "ART", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "An ", "tokens": ["An"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.17": {"text": "Sie kan als Sieger prangen/", "tokens": ["Sie", "kan", "als", "Sie\u00b7ger", "pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KOUS", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Ein Knecht/ ein F\u00fcrst/ ein K\u00f6nig und Regierer", "tokens": ["Ein", "Knecht", "/", "ein", "F\u00fcrst", "/", "ein", "K\u00f6\u00b7nig", "und", "Re\u00b7gie\u00b7rer"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "NN", "$(", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Mu\u00df in den Liebes-Gluhten bl\u00fchn.", "tokens": ["Mu\u00df", "in", "den", "Lie\u00b7bes\u00b7Gluh\u00b7ten", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Denckt einer schon auf heute sich zu zwingen/", "tokens": ["Denckt", "ei\u00b7ner", "schon", "auf", "heu\u00b7te", "sich", "zu", "zwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADV", "APPR", "ADV", "PRF", "PTKZU", "VVINF", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Jhr mu\u00df der Streich doch morgen wohl gelingen/", "tokens": ["Ihr", "mu\u00df", "der", "Streich", "doch", "mor\u00b7gen", "wohl", "ge\u00b7lin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und fliehet man schon einst vor ", "tokens": ["Und", "flie\u00b7het", "man", "schon", "einst", "vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "ADV", "ADV", "APPR"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wird man durch ihn bald unversehns betrogen/", "tokens": ["Wird", "man", "durch", "ihn", "bald", "un\u00b7ver\u00b7sehns", "be\u00b7tro\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "PPER", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ja sch\u00fctzt man sich schon einst vor seinen Pfeilen", "tokens": ["Ja", "sch\u00fctzt", "man", "sich", "schon", "einst", "vor", "sei\u00b7nen", "Pfei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "VVFIN", "PIS", "PRF", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie wissen uns doch wol zu \u00fcbereilen.", "tokens": ["Sie", "wis\u00b7sen", "uns", "doch", "wol", "zu", "\u00fc\u00b7be\u00b7rei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Denckt einer schon auf heute sich zu zwingen/", "tokens": ["Denckt", "ei\u00b7ner", "schon", "auf", "heu\u00b7te", "sich", "zu", "zwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADV", "APPR", "ADV", "PRF", "PTKZU", "VVINF", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Jhr mu\u00df der Streich doch morgen wohl gelingen.", "tokens": ["Ihr", "mu\u00df", "der", "Streich", "doch", "mor\u00b7gen", "wohl", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "So mu\u00df der Liebe-Triebe sp\u00fchren", "tokens": ["So", "mu\u00df", "der", "Lie\u00b7be\u00b7Trie\u00b7be", "sp\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die gantze Welt/", "tokens": ["Die", "gant\u00b7ze", "Welt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Sie will an ihrem Seile f\u00fchren", "tokens": ["Sie", "will", "an", "ih\u00b7rem", "Sei\u00b7le", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Was Odem-lo\u00df/ und Athem in sich h\u00e4lt.", "tokens": ["Was", "O\u00b7dem\u00b7lo\u00df", "/", "und", "A\u00b7them", "in", "sich", "h\u00e4lt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "$(", "KON", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Die Sterne sind verliebt am Himmel/", "tokens": ["Die", "Ster\u00b7ne", "sind", "ver\u00b7liebt", "am", "Him\u00b7mel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPRART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "In ihnen glimmt die Liebes Gluth/", "tokens": ["In", "ih\u00b7nen", "glimmt", "die", "Lie\u00b7bes", "Gluth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Es machet der ", "tokens": ["Es", "ma\u00b7chet", "der"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Beym Ahle in der k\u00fchlen Fluth.", "tokens": ["Beym", "Ah\u00b7le", "in", "der", "k\u00fch\u00b7len", "Fluth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und der Magnet-Stein liebt das Eysen", "tokens": ["Und", "der", "Ma\u00b7gnet\u00b7Stein", "liebt", "das", "Ey\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.18": {"text": "Wie auch den Stern aus Norden/", "tokens": ["Wie", "auch", "den", "Stern", "aus", "Nor\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Der Agtstein ist dem gleich geworden/", "tokens": ["Der", "Agt\u00b7stein", "ist", "dem", "gleich", "ge\u00b7wor\u00b7den", "/"], "t