{"dta.poem.10654": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Epigramma.  \n  Von seiner Buhlschafft Winter Rosen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Was wunder ists mein Lieb/ da\u00df wir dir bl\u00fchen sehen", "tokens": ["Was", "wun\u00b7der", "ists", "mein", "Lieb", "/", "da\u00df", "wir", "dir", "bl\u00fc\u00b7hen", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "VAFIN", "PPOSAT", "NN", "$(", "KOUS", "PPER", "PPER", "VVINF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Rosen/ da wir doch im kalten Winter sein/", "tokens": ["Die", "Ro\u00b7sen", "/", "da", "wir", "doch", "im", "kal\u00b7ten", "Win\u00b7ter", "sein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KOUS", "PPER", "ADV", "APPRART", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es ist genug da\u00df sie dein Athem an thut wehen/", "tokens": ["Es", "ist", "ge\u00b7nug", "da\u00df", "sie", "dein", "A\u00b7them", "an", "thut", "we\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KOUS", "PPER", "PPOSAT", "NN", "APPR", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd deiner Augen Liecht ist jhnen Sonnenschein.", "tokens": ["Vnd", "dei\u00b7ner", "Au\u00b7gen", "Liecht", "ist", "jh\u00b7nen", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}