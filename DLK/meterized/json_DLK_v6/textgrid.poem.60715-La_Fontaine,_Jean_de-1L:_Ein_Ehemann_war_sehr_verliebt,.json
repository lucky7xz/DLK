{"textgrid.poem.60715": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Ehemann war sehr verliebt,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Ehemann war sehr verliebt,", "tokens": ["Ein", "E\u00b7he\u00b7mann", "war", "sehr", "ver\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verliebt in seine eigne Frau.", "tokens": ["Ver\u00b7liebt", "in", "sei\u00b7ne", "eig\u00b7ne", "Frau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So war er gl\u00fccklich \u2013 doch sein Gl\u00fcck war lau,", "tokens": ["So", "war", "er", "gl\u00fcck\u00b7lich", "\u2013", "doch", "sein", "Gl\u00fcck", "war", "lau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$(", "KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Weil es der Dame nicht beliebt,", "tokens": ["Weil", "es", "der", "Da\u00b7me", "nicht", "be\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit Herzensblick ihn zu erfreuen,", "tokens": ["Mit", "Her\u00b7zens\u00b7blick", "ihn", "zu", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit L\u00e4cheln oder gutem Wort;", "tokens": ["Mit", "L\u00e4\u00b7cheln", "o\u00b7der", "gu\u00b7tem", "Wort", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie schien den armen Mann zu scheuen,", "tokens": ["Sie", "schien", "den", "ar\u00b7men", "Mann", "zu", "scheu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und nahte er sich z\u00e4rtlich, lief sie fort.", "tokens": ["Und", "nah\u00b7te", "er", "sich", "z\u00e4rt\u00b7lich", ",", "lief", "sie", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADJD", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Er f\u00fchlte wohl, da\u00df dies nicht rechte Liebe w\u00e4re.", "tokens": ["Er", "f\u00fchl\u00b7te", "wohl", ",", "da\u00df", "dies", "nicht", "rech\u00b7te", "Lie\u00b7be", "w\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PDS", "PTKNEG", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich glaub's: er war ja nur ein Ehemann!", "tokens": ["Ich", "glaub's", ":", "er", "war", "ja", "nur", "ein", "E\u00b7he\u00b7mann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$.", "PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Er weinte heimlich manche Z\u00e4hre", "tokens": ["Er", "wein\u00b7te", "heim\u00b7lich", "man\u00b7che", "Z\u00e4h\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und schm\u00fcckte nicht des Liebesgotts Alt\u00e4re,", "tokens": ["Und", "schm\u00fcck\u00b7te", "nicht", "des", "Lie\u00b7bes\u00b7gotts", "Al\u00b7t\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und Hymen schien ihm ein Tyrann.", "tokens": ["Und", "Hy\u00b7men", "schien", "ihm", "ein", "Ty\u00b7rann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Denn wenn die Liebe nicht verkl\u00e4rt", "tokens": ["Denn", "wenn", "die", "Lie\u00b7be", "nicht", "ver\u00b7kl\u00e4rt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PTKNEG", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Die Freuden, die der Gott gew\u00e4hrt,", "tokens": ["Die", "Freu\u00b7den", ",", "die", "der", "Gott", "ge\u00b7w\u00e4hrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "So ist die Ehe ein tr\u00fcbseliges Gespann. \u2013", "tokens": ["So", "ist", "die", "E\u00b7he", "ein", "tr\u00fcb\u00b7se\u00b7li\u00b7ges", "Ge\u00b7spann", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Da unser Weibchen also so beschaffen", "tokens": ["Da", "un\u00b7ser", "Weib\u00b7chen", "al\u00b7so", "so", "be\u00b7schaf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und nie dem Gatten freundlich tat,", "tokens": ["Und", "nie", "dem", "Gat\u00b7ten", "freund\u00b7lich", "tat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Vielmehr ihn ferne hielt mit allen Waffen,", "tokens": ["Viel\u00b7mehr", "ihn", "fer\u00b7ne", "hielt", "mit", "al\u00b7len", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Beklagte er einst nachts sein Z\u00f6libat,", "tokens": ["Be\u00b7klag\u00b7te", "er", "einst", "nachts", "sein", "Z\u00f6\u00b7li\u00b7bat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Als sacht ein Dieb ins Zimmer trat,", "tokens": ["Als", "sacht", "ein", "Dieb", "ins", "Zim\u00b7mer", "trat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Um gute Beute zu erraffen,", "tokens": ["Um", "gu\u00b7te", "Beu\u00b7te", "zu", "er\u00b7raf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Ein Dieb, der unser armes Weibchen so erschreckte,", "tokens": ["Ein", "Dieb", ",", "der", "un\u00b7ser", "ar\u00b7mes", "Weib\u00b7chen", "so", "er\u00b7schreck\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da\u00df es in Angst zum Gatten lief", "tokens": ["Da\u00df", "es", "in", "Angst", "zum", "Gat\u00b7ten", "lief"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Und sich an seiner Brust versteckte.", "tokens": ["Und", "sich", "an", "sei\u00b7ner", "Brust", "ver\u00b7steck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Der aber rief beseligt aus:", "tokens": ["Der", "a\u00b7ber", "rief", "be\u00b7se\u00b7ligt", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "\u00bbfreund Dieb, so greife denn recht tief", "tokens": ["\u00bb", "freund", "Dieb", ",", "so", "grei\u00b7fe", "denn", "recht", "tief"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "NN", "$,", "ADV", "VVFIN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "In Truhen und in Kasten", "tokens": ["In", "Tru\u00b7hen", "und", "in", "Kas\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Und trage, was du willst, nach Haus,", "tokens": ["Und", "tra\u00b7ge", ",", "was", "du", "willst", ",", "nach", "Haus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Denn ohne dich m\u00fc\u00dft ich noch lange fasten!\u00ab", "tokens": ["Denn", "oh\u00b7ne", "dich", "m\u00fc\u00dft", "ich", "noch", "lan\u00b7ge", "fas\u00b7ten", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "PPER", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Ein Dieb ist keine scheue Maus,", "tokens": ["Ein", "Dieb", "ist", "kei\u00b7ne", "scheu\u00b7e", "Maus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Auch dieser machte einen guten Schnitt,", "tokens": ["Auch", "die\u00b7ser", "mach\u00b7te", "ei\u00b7nen", "gu\u00b7ten", "Schnitt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Und wohl die halbe Wohnung mu\u00dfte mit.", "tokens": ["Und", "wohl", "die", "hal\u00b7be", "Woh\u00b7nung", "mu\u00df\u00b7te", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VMFIN", "APPR", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und das, was die Geschichte lehrt,", "tokens": ["Und", "das", ",", "was", "die", "Ge\u00b7schich\u00b7te", "lehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist, da\u00df die Furcht ein starker Trieb,", "tokens": ["Ist", ",", "da\u00df", "die", "Furcht", "ein", "star\u00b7ker", "Trieb", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Widerwillen oft schon aus dem Felde hieb.", "tokens": ["Der", "Wi\u00b7der\u00b7wil\u00b7len", "oft", "schon", "aus", "dem", "Fel\u00b7de", "hieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zwar trifft's auch zu, da\u00df Furcht die Liebe \u00fcberwindet,", "tokens": ["Zwar", "trifft's", "auch", "zu", ",", "da\u00df", "Furcht", "die", "Lie\u00b7be", "\u00fc\u00b7berw\u00b7in\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$,", "KOUS", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch manchmal ist's auch umgekehrt,", "tokens": ["Doch", "manch\u00b7mal", "ist's", "auch", "um\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie dies ein andres Beispiel k\u00fcndet:", "tokens": ["Wie", "dies", "ein", "and\u00b7res", "Bei\u00b7spiel", "k\u00fcn\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "War einer so in Lieb entbrannt", "tokens": ["War", "ei\u00b7ner", "so", "in", "Lieb", "ent\u00b7brannt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADV", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu seinem Weib, da\u00df er mit k\u00fchner Hand", "tokens": ["Zu", "sei\u00b7nem", "Weib", ",", "da\u00df", "er", "mit", "k\u00fch\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das Haus in Flammen legte,", "tokens": ["Das", "Haus", "in", "Flam\u00b7men", "leg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Um alle Sehnsucht, die er hegte,", "tokens": ["Um", "al\u00b7le", "Sehn\u00b7sucht", ",", "die", "er", "heg\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zu stillen und durch Rauch und Brand", "tokens": ["Zu", "stil\u00b7len", "und", "durch", "Rauch", "und", "Brand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "KON", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Liebste auf dem Arm zu tragen.", "tokens": ["Die", "Liebs\u00b7te", "auf", "dem", "Arm", "zu", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ich sch\u00e4tze solche Tat, und ich mu\u00df sagen,", "tokens": ["Ich", "sch\u00e4t\u00b7ze", "sol\u00b7che", "Tat", ",", "und", "ich", "mu\u00df", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "KON", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Da\u00df ich an der Erz\u00e4hlung stets Gefallen fand:", "tokens": ["Da\u00df", "ich", "an", "der", "Er\u00b7z\u00e4h\u00b7lung", "stets", "Ge\u00b7fal\u00b7len", "fand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Echt spanisch klingt's, und scheint die Tat auch toll,", "tokens": ["Echt", "spa\u00b7nisch", "klingt's", ",", "und", "scheint", "die", "Tat", "auch", "toll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "$,", "KON", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ist sie doch gro\u00df und wundervoll.", "tokens": ["Ist", "sie", "doch", "gro\u00df", "und", "wun\u00b7der\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}