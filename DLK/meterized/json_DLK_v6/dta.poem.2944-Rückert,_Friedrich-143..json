{"dta.poem.2944": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "143.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir sind in einem Streit, der nicht zu schlichten ist,", "tokens": ["Wir", "sind", "in", "ei\u00b7nem", "Streit", ",", "der", "nicht", "zu", "schlich\u00b7ten", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "$,", "PRELS", "PTKNEG", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der neu erwacht, wann er geschlummert eine Frist.", "tokens": ["Der", "neu", "er\u00b7wacht", ",", "wann", "er", "ge\u00b7schlum\u00b7mert", "ei\u00b7ne", "Frist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "$,", "PWAV", "PPER", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Wunde, bricht sie auf, ist schlimmer als gewesen;", "tokens": ["Die", "Wun\u00b7de", ",", "bricht", "sie", "auf", ",", "ist", "schlim\u00b7mer", "als", "ge\u00b7we\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "VAFIN", "ADJD", "KOKOM", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Tode sind wir nah, und glaubten uns genesen.", "tokens": ["Dem", "To\u00b7de", "sind", "wir", "nah", ",", "und", "glaub\u00b7ten", "uns", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$,", "KON", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sie eitert innen, wenn sie au\u00dfen scheint geheilt,", "tokens": ["Sie", "ei\u00b7tert", "in\u00b7nen", ",", "wenn", "sie", "au\u00b7\u00dfen", "scheint", "ge\u00b7heilt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "ADV", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Wunde, die uns tief ins Mark des Lebens theilt,", "tokens": ["Die", "Wun\u00b7de", ",", "die", "uns", "tief", "ins", "Mark", "des", "Le\u00b7bens", "theilt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "An der, o Vaterland, du krankest lang genug,", "tokens": ["An", "der", ",", "o", "Va\u00b7ter\u00b7land", ",", "du", "kran\u00b7kest", "lang", "ge\u00b7nug", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "FM", "NN", "$,", "PPER", "VVFIN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die nicht des Feindes Schwert, die dir der Glaube schlug.", "tokens": ["Die", "nicht", "des", "Fein\u00b7des", "Schwert", ",", "die", "dir", "der", "Glau\u00b7be", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ART", "NN", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "La\u00dft endlich, um den Streit ums Wahre zu vers\u00f6hnen,", "tokens": ["La\u00dft", "end\u00b7lich", ",", "um", "den", "Streit", "ums", "Wah\u00b7re", "zu", "ver\u00b7s\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "KOUI", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O la\u00dft zum Guten uns vereinigen im Sch\u00f6nen!", "tokens": ["O", "la\u00dft", "zum", "Gu\u00b7ten", "uns", "ver\u00b7ei\u00b7ni\u00b7gen", "im", "Sch\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ein friedliches Gebiet ist gro\u00df genug verliehn;", "tokens": ["Ein", "fried\u00b7li\u00b7ches", "Ge\u00b7biet", "ist", "gro\u00df", "ge\u00b7nug", "ver\u00b7liehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00dft aus dem streitigen dahin zur\u00fcck uns ziehn!", "tokens": ["La\u00dft", "aus", "dem", "strei\u00b7ti\u00b7gen", "da\u00b7hin", "zu\u00b7r\u00fcck", "uns", "ziehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "VVFIN", "PAV", "PTKVZ", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Nicht was in Kirch' und Staat heillos die Menschheit spaltet,", "tokens": ["Nicht", "was", "in", "Kirch'", "und", "Staat", "heil\u00b7los", "die", "Menschheit", "spal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PWS", "APPR", "NN", "KON", "NN", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wir lehren Menschliches, vom G\u00f6ttlichen durchwaltet,", "tokens": ["Wir", "leh\u00b7ren", "Menschli\u00b7ches", ",", "vom", "G\u00f6tt\u00b7li\u00b7chen", "durch\u00b7wal\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Damit zum Himmlischen das Ird'sche sei entfaltet.", "tokens": ["Da\u00b7mit", "zum", "Himm\u00b7li\u00b7schen", "das", "Ird'\u00b7sche", "sei", "ent\u00b7fal\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPRART", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}