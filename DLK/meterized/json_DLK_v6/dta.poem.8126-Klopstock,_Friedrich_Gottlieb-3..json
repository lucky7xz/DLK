{"dta.poem.8126": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1773", "urn": "urn:nbn:de:kobv:b4-200905199504", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sie versinkt, sie versinkt Babel! Der T\u00e4uscherin", "tokens": ["Sie", "ver\u00b7sinkt", ",", "sie", "ver\u00b7sinkt", "Ba\u00b7bel", "!", "Der", "T\u00e4u\u00b7sche\u00b7rin"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "NE", "$.", "ART", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Gef\u00fcllt ist mit Gifttrunk, schnellt\u00f6dend sch\u00e4umt", "tokens": ["Ge\u00b7f\u00fcllt", "ist", "mit", "Gift\u00b7trunk", ",", "schnell\u00b7t\u00f6\u00b7dend", "sch\u00e4umt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "VAFIN", "APPR", "NN", "$,", "ADJD", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Jhr Kelch auf! O es f\u00fcllt dir, Babel, daf\u00fcr", "tokens": ["Ihr", "Kelch", "auf", "!", "O", "es", "f\u00fcllt", "dir", ",", "Ba\u00b7bel", ",", "da\u00b7f\u00fcr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "PTKVZ", "$.", "NE", "PPER", "VVFIN", "PPER", "$,", "NE", "$,", "PAV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Des Gerichts Kelch vollmessend, der wiedervergilt!", "tokens": ["Des", "Ge\u00b7richts", "Kelch", "voll\u00b7mes\u00b7send", ",", "der", "wie\u00b7der\u00b7ver\u00b7gilt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,", "PRELS", "VVFIN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}}}}