{"textgrid.poem.63114": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "8", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun steigt der Morgen \u00fcbern Zaun", "tokens": ["Nun", "steigt", "der", "Mor\u00b7gen", "\u00fc\u00b7bern", "Zaun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Graugr\u00fcn wie ein Askete.", "tokens": ["Grau\u00b7gr\u00fcn", "wie", "ein", "As\u00b7ke\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In heller Sonn, wenn Veilchen blaun,", "tokens": ["In", "hel\u00b7ler", "Sonn", ",", "wenn", "Veil\u00b7chen", "blaun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gilt Rausch nicht und Rakete.", "tokens": ["Gilt", "Rausch", "nicht", "und", "Ra\u00b7ke\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKNEG", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und was dir heut am Halse hing:", "tokens": ["Und", "was", "dir", "heut", "am", "Hal\u00b7se", "hing", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dies Heut ward schon zum Gestern.", "tokens": ["Dies", "Heut", "ward", "schon", "zum", "Ge\u00b7stern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Was ich mir fing: es ging das Ding", "tokens": ["Was", "ich", "mir", "fing", ":", "es", "ging", "das", "Ding"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu seinen toten Schwestern.", "tokens": ["Zu", "sei\u00b7nen", "to\u00b7ten", "Schwes\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Traum st\u00fcrzt und Tr\u00e4ne feuerhei\u00df", "tokens": ["Traum", "st\u00fcrzt", "und", "Tr\u00e4\u00b7ne", "feu\u00b7er\u00b7hei\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KON", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus meinem blinden Blicke.", "tokens": ["Aus", "mei\u00b7nem", "blin\u00b7den", "Bli\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mir winken, was ich will und wei\u00df:", "tokens": ["Mir", "win\u00b7ken", ",", "was", "ich", "will", "und", "wei\u00df", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dolch, Fallbeil, Gift und Stricke.", "tokens": ["Dolch", ",", "Fall\u00b7beil", ",", "Gift", "und", "Stri\u00b7cke", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", 