{"dta.poem.20165": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Lieb-erfreute  \n  FJLAMON .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1666", "urn": "urn:nbn:de:kobv:b4-20424-1", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch-Historischer Lustgarten. Frankfurt (Main), 1666."}, "poem": {"stanza.1": {"line.1": {"text": "Die ich manchem Liebes-Strick\u2019/ auch so manchen grossen", "tokens": ["Die", "ich", "man\u00b7chem", "Lie\u00b7bes\u00b7Strick'", "/", "auch", "so", "man\u00b7chen", "gros\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PIAT", "NN", "$(", "ADV", "ADV", "PIAT", "ADJA"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Amors List entgongen bin/ und Cupidens Witz entlauffen/", "tokens": ["A\u00b7mors", "List", "ent\u00b7gon\u00b7gen", "bin", "/", "und", "Cu\u00b7pi\u00b7dens", "Witz", "ent\u00b7lauf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVPP", "VAFIN", "$(", "KON", "NE", "NN", "VVPP", "$("], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Mu\u00df nun rennen/ \u00e4chtzen/ schreyen: Ach mein hochbetr\u00fcb-", "tokens": ["Mu\u00df", "nun", "ren\u00b7nen", "/", "\u00e4cht\u00b7zen", "/", "schre\u00b7yen", ":", "Ach", "mein", "hoch\u00b7be\u00b7tr\u00fcb"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "ADV", "VVFIN", "$(", "VVINF", "$(", "VVINF", "$.", "ITJ", "PPOSAT", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "Weil Cupido mich getroffen und erreget grossen Schmertz.", "tokens": ["Weil", "Cu\u00b7pi\u00b7do", "mich", "ge\u00b7trof\u00b7fen", "und", "er\u00b7re\u00b7get", "gros\u00b7sen", "Schmertz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "VVPP", "KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Niemand kan als Fliamon meine Wunden wieder heilen/", "tokens": ["Nie\u00b7mand", "kan", "als", "Flia\u00b7mon", "mei\u00b7ne", "Wun\u00b7den", "wie\u00b7der", "hei\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "KOUS", "NN", "PPOSAT", "NN", "ADV", "VVINF", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "So mir Venus blindes Kind hat gemacht mit seinen Pfeilen/", "tokens": ["So", "mir", "Ve\u00b7nus", "blin\u00b7des", "Kind", "hat", "ge\u00b7macht", "mit", "sei\u00b7nen", "Pfei\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "NN", "ADJA", "NN", "VAFIN", "VVPP", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Filamon Hertzallerliebster kommet/ kommet nur herzu/", "tokens": ["Fi\u00b7la\u00b7mon", "Hert\u00b7zal\u00b7ler\u00b7liebs\u00b7ter", "kom\u00b7met", "/", "kom\u00b7met", "nur", "her\u00b7zu", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$(", "VVFIN", "ADV", "ADV", "$("], "meter": "+---+-+-+-+-+-+", "measure": "dactylic.init"}, "line.8": {"text": "Schaffet zwey verliebten Hertzen/ meinem und dem eurem", "tokens": ["Schaf\u00b7fet", "zwey", "ver\u00b7lieb\u00b7ten", "Hert\u00b7zen", "/", "mei\u00b7nem", "und", "dem", "eu\u00b7rem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "CARD", "ADJA", "NN", "$(", "PPOSAT", "KON", "ART", "PPOSAT"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}}}}