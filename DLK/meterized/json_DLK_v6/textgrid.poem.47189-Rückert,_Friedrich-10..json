{"textgrid.poem.47189": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Rolle, denk' ich, ist nun ausgespielt,", "tokens": ["Mei\u00b7ne", "Rol\u00b7le", ",", "denk'", "ich", ",", "ist", "nun", "aus\u00b7ge\u00b7spielt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "$,", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Ausgewuchert mit dem Pfund, das ich erhielt,", "tokens": ["Aus\u00b7ge\u00b7wu\u00b7chert", "mit", "dem", "Pfund", ",", "das", "ich", "er\u00b7hielt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Ausgerungen, ausgestritten, ausgewagt,", "tokens": ["Aus\u00b7ge\u00b7run\u00b7gen", ",", "aus\u00b7ge\u00b7strit\u00b7ten", ",", "aus\u00b7ge\u00b7wagt", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Ausgesungen, ausgelitten, ausgeklagt.", "tokens": ["Aus\u00b7ge\u00b7sun\u00b7gen", ",", "aus\u00b7ge\u00b7lit\u00b7ten", ",", "aus\u00b7ge\u00b7klagt", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Denn in Klage geht zuletzt der Jubel aus;", "tokens": ["Denn", "in", "Kla\u00b7ge", "geht", "zu\u00b7letzt", "der", "Ju\u00b7bel", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "La\u00dft mich nur, denn ich bin satt, hinweg vom Schmaus!", "tokens": ["La\u00dft", "mich", "nur", ",", "denn", "ich", "bin", "satt", ",", "hin\u00b7weg", "vom", "Schmaus", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "KON", "PPER", "VAFIN", "ADJD", "$,", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Stecket weiter nicht hinaus mein Lebensziel!", "tokens": ["Ste\u00b7cket", "wei\u00b7ter", "nicht", "hin\u00b7aus", "mein", "Le\u00b7bens\u00b7ziel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "APZR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Oder meiner Klagen w\u00fcrde gar zu viel!", "tokens": ["O\u00b7der", "mei\u00b7ner", "Kla\u00b7gen", "w\u00fcr\u00b7de", "gar", "zu", "viel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "PTKA", "PIS", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.9": {"text": "Denn ich f\u00fchle, da\u00df mein Herz, seit es gewann", "tokens": ["Denn", "ich", "f\u00fch\u00b7le", ",", "da\u00df", "mein", "Herz", ",", "seit", "es", "ge\u00b7wann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.10": {"text": "Diesen Schaden, nur mit Seufzen atmen kann.", "tokens": ["Die\u00b7sen", "Scha\u00b7den", ",", "nur", "mit", "Seuf\u00b7zen", "at\u00b7men", "kann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "ADV", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}