{"textgrid.poem.27881": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "96.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heil erfolgte durch die Wunde;", "tokens": ["Heil", "er\u00b7folg\u00b7te", "durch", "die", "Wun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kranckheit diente zum Gesunde;", "tokens": ["Kran\u00b7ck\u00b7heit", "dien\u00b7te", "zum", "Ge\u00b7sun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Freude wuchs au\u00df Traurigkeit,", "tokens": ["Freu\u00b7de", "wuchs", "au\u00df", "Trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00e4rcke von Gebrechligkeit;", "tokens": ["St\u00e4r\u00b7cke", "von", "Ge\u00b7brech\u00b7lig\u00b7keit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sterben brachte zu dem Leben,", "tokens": ["Ster\u00b7ben", "brach\u00b7te", "zu", "dem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und das fallen zum erheben.", "tokens": ["Und", "das", "fal\u00b7len", "zum", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVINF", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}