{"dta.poem.18737": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Wie kommt\u2019s dass man an jedem Orte", "tokens": ["Wie", "kommt's", "dass", "man", "an", "je\u00b7dem", "Or\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "KOUS", "PIS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So viel Gutes, so viel Dummes h\u00f6rt?", "tokens": ["So", "viel", "Gu\u00b7tes", ",", "so", "viel", "Dum\u00b7mes", "h\u00f6rt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Die J\u00fcngsten wiederholen der Aeltesten Worte,", "tokens": ["Die", "J\u00fcng\u00b7sten", "wie\u00b7der\u00b7ho\u00b7len", "der", "A\u00b7el\u00b7tes\u00b7ten", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und glauben dass es ihnen angeh\u00f6rt.", "tokens": ["Und", "glau\u00b7ben", "dass", "es", "ih\u00b7nen", "an\u00b7ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}