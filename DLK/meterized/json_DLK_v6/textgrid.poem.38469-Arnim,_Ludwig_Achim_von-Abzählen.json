{"textgrid.poem.38469": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Abz\u00e4hlen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eins, zwey, drey, vier, f\u00fcnf, sechs, sieben, acht, neun,", "tokens": ["Eins", ",", "zwey", ",", "drey", ",", "vier", ",", "f\u00fcnf", ",", "sechs", ",", "sie\u00b7ben", ",", "acht", ",", "neun", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "CARD", "$,", "CARD", "$,", "CARD", "$,", "CARD", "$,", "CARD", "$,", "VVFIN", "$,", "CARD", "$,", "CARD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Geh ich in das G\u00e4ssel h'nein,", "tokens": ["Geh", "ich", "in", "das", "G\u00e4s\u00b7sel", "h'n\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Schlag dem Bauer die Fenster ein,", "tokens": ["Schlag", "dem", "Bau\u00b7er", "die", "Fens\u00b7ter", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Kommt der B\u00fcttel, setzt mich ein,", "tokens": ["Kommt", "der", "B\u00fct\u00b7tel", ",", "setzt", "mich", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Setzt mich in das Narrenhaus,", "tokens": ["Setzt", "mich", "in", "das", "Nar\u00b7ren\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Geb' ich drey, vier Batzen aus,", "tokens": ["Geb'", "ich", "drey", ",", "vier", "Bat\u00b7zen", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "CARD", "$,", "CARD", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ri ra Ofenloch,", "tokens": ["Ri", "ra", "O\u00b7fen\u00b7loch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "H\u00e4tt' ich mein' drey Batzen noch!", "tokens": ["H\u00e4tt'", "ich", "mein'", "drey", "Bat\u00b7zen", "noch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "CARD", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}