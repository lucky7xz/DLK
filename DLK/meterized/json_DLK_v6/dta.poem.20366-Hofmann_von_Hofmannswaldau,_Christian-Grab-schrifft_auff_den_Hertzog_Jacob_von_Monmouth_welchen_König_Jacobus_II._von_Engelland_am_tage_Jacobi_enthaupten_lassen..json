{"dta.poem.20366": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Grab-schrifft auff den Hertzog/ Jacob  \n von Monmouth/ welchen K\u00f6nig Jacobus  II.  von  \n Engelland am tage Jacobi enthaupten  \n lassen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du wustest/ da\u00df kein reich zwey sonnen leiden kan;", "tokens": ["Du", "wus\u00b7test", "/", "da\u00df", "kein", "reich", "zwey", "son\u00b7nen", "lei\u00b7den", "kan", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PIAT", "ADJD", "CARD", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch spann dein aberwitz ein liederlich gewebe.", "tokens": ["Doch", "spann", "dein", "a\u00b7ber\u00b7witz", "ein", "lie\u00b7der\u00b7lich", "ge\u00b7we\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ART", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drum setzt man dir ein beil mit diesen worten an:", "tokens": ["Drum", "setzt", "man", "dir", "ein", "beil", "mit", "die\u00b7sen", "wor\u00b7ten", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "PPER", "ART", "NN", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Stirb/ kleiner Jacob/ stirb/ auff da\u00df der grosse lebe!", "tokens": ["Stirb", "/", "klei\u00b7ner", "Ja\u00b7cob", "/", "stirb", "/", "auff", "da\u00df", "der", "gros\u00b7se", "le\u00b7be", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "ADJA", "NE", "$(", "VVIMP", "$(", "APPR", "KOUS", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}