{"dta.poem.10189": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Der Gouverneur.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Auf einer Forscherfahrt im Ocean             ", "tokens": ["Auf", "ei\u00b7ner", "For\u00b7scher\u00b7fahrt", "im", "O\u00b7cean"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Fand ich ein Inselchen, so leer und \u00f6de,", "tokens": ["Fand", "ich", "ein", "In\u00b7sel\u00b7chen", ",", "so", "leer", "und", "\u00f6\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Als h\u00e4tte j\u00fcngst das Schwert des Tamerlan", "tokens": ["Als", "h\u00e4t\u00b7te", "j\u00fcngst", "das", "Schwert", "des", "Ta\u00b7mer\u00b7lan"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den letzten Keim gebrochen, hart und schn\u00f6de,", "tokens": ["Den", "letz\u00b7ten", "Keim", "ge\u00b7bro\u00b7chen", ",", "hart", "und", "schn\u00f6\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Pest gezogen ihre Beulenbahn,", "tokens": ["Die", "Pest", "ge\u00b7zo\u00b7gen", "ih\u00b7re", "Beu\u00b7len\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df wenig Menschen blieben, bla\u00df und bl\u00f6de.", "tokens": ["Da\u00df", "we\u00b7nig", "Men\u00b7schen", "blie\u00b7ben", ",", "bla\u00df", "und", "bl\u00f6\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "$,", "ADJD", "KON", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Kein Pflug, ern\u00e4hrend, ri\u00df die Ackerkrume,", "tokens": ["Kein", "Pflug", ",", "er\u00b7n\u00e4h\u00b7rend", ",", "ri\u00df", "die", "A\u00b7cker\u00b7kru\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "VVPP", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kein J\u00e4ger sang, am Hut die Feder keck.", "tokens": ["Kein", "J\u00e4\u00b7ger", "sang", ",", "am", "Hut", "die", "Fe\u00b7der", "keck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sp\u00e4rlich wuchs Gras und Moos und Hundeblume,", "tokens": ["Sp\u00e4r\u00b7lich", "wuchs", "Gras", "und", "Moos", "und", "Hun\u00b7de\u00b7blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Zwergobst verkroch in\u2019s Blatt sich, gr\u00fcn vor Schreck.", "tokens": ["Zwer\u00b7gobst", "ver\u00b7kroch", "in's", "Blatt", "sich", ",", "gr\u00fcn", "vor", "Schreck", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "PRF", "$,", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein St\u00e4dtchen lag, verlassen im Wehtume,", "tokens": ["Ein", "St\u00e4dt\u00b7chen", "lag", ",", "ver\u00b7las\u00b7sen", "im", "Weh\u00b7tu\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Am ganz verschlammten Hafen im Versteck.", "tokens": ["Am", "ganz", "ver\u00b7schlamm\u00b7ten", "Ha\u00b7fen", "im", "Ver\u00b7steck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Voran geht immer der Herr B\u00fcrgermeister,", "tokens": ["Vo\u00b7ran", "geht", "im\u00b7mer", "der", "Herr", "B\u00fcr\u00b7ger\u00b7meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Er litt am Stein, war gr\u00e4mlich, matt und mager.", "tokens": ["Er", "litt", "am", "Stein", ",", "war", "gr\u00e4m\u00b7lich", ",", "matt", "und", "ma\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "VAFIN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es folgt der Richter, ein weit hergereister", "tokens": ["Es", "folgt", "der", "Rich\u00b7ter", ",", "ein", "weit", "her\u00b7ge\u00b7reis\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJD", "ADJA"], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Und sehr gerechter Mann, auch etwas hager.", "tokens": ["Und", "sehr", "ge\u00b7rech\u00b7ter", "Mann", ",", "auch", "et\u00b7was", "ha\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "NN", "$,", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Arzt, des wack\u2019ren Todes Hilfeleister,", "tokens": ["Der", "Arzt", ",", "des", "wack'\u00b7ren", "To\u00b7des", "Hil\u00b7fe\u00b7leis\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "War lange schon des Apothekers Schwager.", "tokens": ["War", "lan\u00b7ge", "schon", "des", "A\u00b7pot\u00b7he\u00b7kers", "Schwa\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Z\u00f6llner spielte t\u00e4glich seinen Skat", "tokens": ["Der", "Z\u00f6ll\u00b7ner", "spiel\u00b7te", "t\u00e4g\u00b7lich", "sei\u00b7nen", "Skat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Acht Stunden mit den beiden Herrn Pastoren.", "tokens": ["Acht", "Stun\u00b7den", "mit", "den", "bei\u00b7den", "Herrn", "Pas\u00b7to\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ART", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie Dornenr\u00f6schen schlief der Advokat,", "tokens": ["Wie", "Dor\u00b7nen\u00b7r\u00f6\u00b7schen", "schlief", "der", "Ad\u00b7vo\u00b7kat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kein Kundenprinz hat je sich hinverloren.", "tokens": ["Kein", "Kun\u00b7den\u00b7prinz", "hat", "je", "sich", "hin\u00b7ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Im Sitzungssaale g\u00e4hnt der hohe Rath,", "tokens": ["Im", "Sit\u00b7zungs\u00b7saa\u00b7le", "g\u00e4hnt", "der", "ho\u00b7he", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Boten schnarchen auf den Korridoren.", "tokens": ["Die", "Bo\u00b7ten", "schnar\u00b7chen", "auf", "den", "Kor\u00b7ri\u00b7do\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch nein, das that er nicht. Im Gegentheil,", "tokens": ["Doch", "nein", ",", "das", "that", "er", "nicht", ".", "Im", "Ge\u00b7gen\u00b7theil", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "$.", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er war ein milder und humaner Herr.", "tokens": ["Er", "war", "ein", "mil\u00b7der", "und", "hu\u00b7ma\u00b7ner", "Herr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihm folgten M\u00e4nner ohne Schwert und Beil.", "tokens": ["Ihm", "folg\u00b7ten", "M\u00e4n\u00b7ner", "oh\u00b7ne", "Schwert", "und", "Beil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Umdr\u00e4ngten ihn mit Hin- und Hergezerr", "tokens": ["Um\u00b7dr\u00e4ng\u00b7ten", "ihn", "mit", "Hin", "und", "Her\u00b7ge\u00b7zerr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die guten Leute, riefen alle Heil!", "tokens": ["Die", "gu\u00b7ten", "Leu\u00b7te", ",", "rie\u00b7fen", "al\u00b7le", "Heil", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Heil! auch die Kinder mitt\u2019 im Schulgepl\u00e4rr.", "tokens": ["Heil", "!", "auch", "die", "Kin\u00b7der", "mitt'", "im", "Schul\u00b7ge\u00b7pl\u00e4rr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "ART", "NN", "VVPP", "APPRART", "NN", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}}, "stanza.6": {"line.1": {"text": "Es hatte auch das St\u00e4dtchen Garnison,", "tokens": ["Es", "hat\u00b7te", "auch", "das", "St\u00e4dt\u00b7chen", "Gar\u00b7ni\u00b7son", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "An jedem Mittwoch war Parolausgabe.", "tokens": ["An", "je\u00b7dem", "Mitt\u00b7woch", "war", "Pa\u00b7ro\u00b7laus\u00b7ga\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann zog die Wache auf vom Bataillon", "tokens": ["Dann", "zog", "die", "Wa\u00b7che", "auf", "vom", "Ba\u00b7tail\u00b7lon"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit Tschingdada, Dienstm\u00e4dchen, Schusterknabe.", "tokens": ["Mit", "Tsching\u00b7da\u00b7da", ",", "Dienst\u00b7m\u00e4d\u00b7chen", ",", "Schus\u00b7ter\u00b7kna\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+++-+-+-", "measure": "unknown.measure.hexa"}, "line.5": {"text": "\u201edie Herrn Offiziere!\u201c rief mit Donnerton", "tokens": ["\u201e", "die", "Herrn", "Of\u00b7fi\u00b7zie\u00b7re", "!", "\u201c", "rief", "mit", "Don\u00b7ner\u00b7ton"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "ART", "NN", "NN", "$.", "$(", "VVFIN", "APPR", "NE"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Der Gouverneur, umringt von seinem Stabe.", "tokens": ["Der", "Gou\u00b7ver\u00b7neur", ",", "um\u00b7ringt", "von", "sei\u00b7nem", "Sta\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Warum er hier, das konnte Keiner sagen.", "tokens": ["Wa\u00b7rum", "er", "hier", ",", "das", "konn\u00b7te", "Kei\u00b7ner", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "$,", "PDS", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er lebte nun seit vierzig Jahren schon,", "tokens": ["Er", "leb\u00b7te", "nun", "seit", "vier\u00b7zig", "Jah\u00b7ren", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "CARD", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Sommer hei\u00df, im Winter hoch den Kragen,", "tokens": ["Im", "Som\u00b7mer", "hei\u00df", ",", "im", "Win\u00b7ter", "hoch", "den", "Kra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$,", "APPRART", "NN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf diesem allerliebsten kleinen Thron.", "tokens": ["Auf", "die\u00b7sem", "al\u00b7ler\u00b7liebs\u00b7ten", "klei\u00b7nen", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die einen sprachen, da\u00df in fr\u00fchern Tagen", "tokens": ["Die", "ei\u00b7nen", "spra\u00b7chen", ",", "da\u00df", "in", "fr\u00fc\u00b7hern", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ihn sehr gekannt Herr Levy Nathansohn.", "tokens": ["Ihn", "sehr", "ge\u00b7kannt", "Herr", "Le\u00b7vy", "Na\u00b7than\u00b7sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "NN", "NE", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}}, "stanza.8": {"line.1": {"text": "In einer Sommernacht im alten Garten", "tokens": ["In", "ei\u00b7ner", "Som\u00b7mer\u00b7nacht", "im", "al\u00b7ten", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des K\u00f6nigs stand ein junger Offizier.", "tokens": ["Des", "K\u00f6\u00b7nigs", "stand", "ein", "jun\u00b7ger", "Of\u00b7fi\u00b7zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es schlug die Nachtigall, die Fr\u00f6sche quarrten,", "tokens": ["Es", "schlug", "die", "Nach\u00b7ti\u00b7gall", ",", "die", "Fr\u00f6\u00b7sche", "quarr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Mond beschien am Schlo\u00df den Grenadier.", "tokens": ["Der", "Mond", "be\u00b7schien", "am", "Schlo\u00df", "den", "Gre\u00b7na\u00b7di\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Auf Muschelwegen, harten, leise knarrten", "tokens": ["Auf", "Mu\u00b7schel\u00b7we\u00b7gen", ",", "har\u00b7ten", ",", "lei\u00b7se", "knarr\u00b7ten"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "ADJA", "$,", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zwei Stiefelchen . . Pst . . 