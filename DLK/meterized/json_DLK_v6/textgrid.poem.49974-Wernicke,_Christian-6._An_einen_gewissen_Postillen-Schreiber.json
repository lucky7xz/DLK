{"textgrid.poem.49974": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "6. An einen gewissen Postillen-Schreiber", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer sagt, da\u00df sich dein ", "tokens": ["Wer", "sagt", ",", "da\u00df", "sich", "dein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PRF", "PPOSAT"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Weil eine ", "tokens": ["Weil", "ei\u00b7ne"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Man kenn't dich nicht so wol, wenn man dein Bild anblicket,", "tokens": ["Man", "kenn't", "dich", "nicht", "so", "wol", ",", "wenn", "man", "dein", "Bild", "an\u00b7bli\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKNEG", "ADV", "ADV", "$,", "KOUS", "PIS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als Straton man erkennt,", "tokens": ["Als", "Stra\u00b7ton", "man", "er\u00b7kennt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PIS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die deinen ", "tokens": ["Die", "dei\u00b7nen"], "token_info": ["word", "word"], "pos": ["ART", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Die haben nicht zu theur dein k\u00fchnes Buch bezahlt,", "tokens": ["Die", "ha\u00b7ben", "nicht", "zu", "theur", "dein", "k\u00fch\u00b7nes", "Buch", "be\u00b7zahlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "PTKA", "ADJD", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn jener ist hier ", "tokens": ["Denn", "je\u00b7ner", "ist", "hier"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Und diese sind hier ", "tokens": ["Und", "die\u00b7se", "sind", "hier"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "ADV"], "meter": "--+-+", "measure": "anapaest.init"}}}}}