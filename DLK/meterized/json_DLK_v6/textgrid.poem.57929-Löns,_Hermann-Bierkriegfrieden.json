{"textgrid.poem.57929": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Bierkriegfrieden", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Wirte und die Brauerein,", "tokens": ["Die", "Wir\u00b7te", "und", "die", "Brau\u00b7er\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des langen Haders m\u00fcde,", "tokens": ["Des", "lan\u00b7gen", "Ha\u00b7ders", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Erweichten ihren harten Sinn", "tokens": ["Er\u00b7weich\u00b7ten", "ih\u00b7ren", "har\u00b7ten", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schlossen endlich Friede.", "tokens": ["Und", "schlos\u00b7sen", "end\u00b7lich", "Frie\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und jeder J\u00fcngling, jeder Mann,", "tokens": ["Und", "je\u00b7der", "J\u00fcng\u00b7ling", ",", "je\u00b7der", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Knabe auch, der es schon kann,", "tokens": ["Der", "Kna\u00b7be", "auch", ",", "der", "es", "schon", "kann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PPER", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Tasche voller D\u00e4user,", "tokens": ["Die", "Ta\u00b7sche", "vol\u00b7ler", "D\u00e4u\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bezog die alten H\u00e4user.", "tokens": ["Be\u00b7zog", "die", "al\u00b7ten", "H\u00e4u\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da sitzen sie und halten fest", "tokens": ["Da", "sit\u00b7zen", "sie", "und", "hal\u00b7ten", "fest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die lang' entbehrten T\u00f6pfe,", "tokens": ["Die", "lang'", "ent\u00b7behr\u00b7ten", "T\u00f6p\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die B\u00f6rsen werden ihnen d\u00fcnn,", "tokens": ["Die", "B\u00f6r\u00b7sen", "wer\u00b7den", "ih\u00b7nen", "d\u00fcnn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch dicker stets die K\u00f6pfe.", "tokens": ["Doch", "di\u00b7cker", "stets", "die", "K\u00f6p\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es ist die schlimme Zeit vorbei", "tokens": ["Es", "ist", "die", "schlim\u00b7me", "Zeit", "vor\u00b7bei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Ringbiertrinkersp\u00e4herei,", "tokens": ["Der", "Ring\u00b7bier\u00b7trin\u00b7ker\u00b7sp\u00e4\u00b7he\u00b7rei", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Man darf jetzt ohne Zagen", "tokens": ["Man", "darf", "jetzt", "oh\u00b7ne", "Za\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit Ringbier f\u00fcll'n den Magen.", "tokens": ["Mit", "Ring\u00b7bier", "f\u00fcll'n", "den", "Ma\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nicht mehr mi\u00dftraut der Freund dem Freund,", "tokens": ["Nicht", "mehr", "mi\u00df\u00b7traut", "der", "Freund", "dem", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Vater nicht dem Sohne,", "tokens": ["Der", "Va\u00b7ter", "nicht", "dem", "Soh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und fragt ihn f\u00fcrchterlichen Blicks:", "tokens": ["Und", "fragt", "ihn", "f\u00fcrch\u00b7ter\u00b7li\u00b7chen", "Blicks", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbtrankst mit du oder ohne?\u00ab", "tokens": ["\u00bb", "trankst", "mit", "du", "o\u00b7der", "oh\u00b7ne", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "APPR", "NE", "KON", "KOUI", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Man darf jetzt trinken, was man will,", "tokens": ["Man", "darf", "jetzt", "trin\u00b7ken", ",", "was", "man", "will", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "VVINF", "$,", "PRELS", "PIS", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und ist man kn\u00fcll, dann ist man kn\u00fcll,", "tokens": ["Und", "ist", "man", "kn\u00fcll", ",", "dann", "ist", "man", "kn\u00fcll", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "ADJD", "$,", "ADV", "VAFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und niemand darf es wagen,", "tokens": ["Und", "nie\u00b7mand", "darf", "es", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dem Stoffe nachzufragen.", "tokens": ["Dem", "Stof\u00b7fe", "nach\u00b7zu\u00b7fra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Drum Br\u00fcder, sto\u00dft die Gl\u00e4ser an,", "tokens": ["Drum", "Br\u00fc\u00b7der", ",", "sto\u00dft", "die", "Gl\u00e4\u00b7ser", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir k\u00f6nnen wohl noch einen,", "tokens": ["Wir", "k\u00f6n\u00b7nen", "wohl", "noch", "ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ART", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nach Hause darf heut' keiner gehn", "tokens": ["Nach", "Hau\u00b7se", "darf", "heut'", "kei\u00b7ner", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "ADV", "PIS", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit zielbewu\u00dften Beinen;", "tokens": ["Mit", "ziel\u00b7be\u00b7wu\u00df\u00b7ten", "Bei\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wir haben es der Welt gezeigt,", "tokens": ["Wir", "ha\u00b7ben", "es", "der", "Welt", "ge\u00b7zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was Solidarit\u00e4t erreicht,", "tokens": ["Was", "So\u00b7li\u00b7da\u00b7ri\u00b7t\u00e4t", "er\u00b7reicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVPP", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.7": {"text": "La\u00dft uns den Sieg genie\u00dfen,", "tokens": ["La\u00dft", "uns", "den", "Sieg", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Nasen uns begie\u00dfen.", "tokens": ["Die", "Na\u00b7sen", "uns", "be\u00b7gie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}