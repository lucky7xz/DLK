{"textgrid.poem.66815": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich stieg aus bl\u00fchendem Tale", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich stieg aus bl\u00fchendem Tale", "tokens": ["Ich", "stieg", "aus", "bl\u00fc\u00b7hen\u00b7dem", "Ta\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und stand mit einem Male", "tokens": ["Und", "stand", "mit", "ei\u00b7nem", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In einem Meer von Schutt.", "tokens": ["In", "ei\u00b7nem", "Meer", "von", "Schutt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vom blutigen Purpur beschimmert,", "tokens": ["Vom", "blu\u00b7ti\u00b7gen", "Pur\u00b7pur", "be\u00b7schim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Lag eine Welt zertr\u00fcmmert", "tokens": ["Lag", "ei\u00b7ne", "Welt", "zer\u00b7tr\u00fcm\u00b7mert"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In einem Meer von Schutt.", "tokens": ["In", "ei\u00b7nem", "Meer", "von", "Schutt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Im scheidenden Abendstrahle", "tokens": ["Im", "schei\u00b7den\u00b7den", "A\u00b7bend\u00b7strah\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Geborstene Ideale!", "tokens": ["Ge\u00b7bors\u00b7te\u00b7ne", "I\u00b7dea\u00b7le", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nachteulen schwankten empor.", "tokens": ["Nach\u00b7teu\u00b7len", "schwank\u00b7ten", "em\u00b7por", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und Finsternis deckte die Scherben,", "tokens": ["Und", "Fins\u00b7ter\u00b7nis", "deck\u00b7te", "die", "Scher\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Ich aber lehnte zu sterben", "tokens": ["Ich", "a\u00b7ber", "lehn\u00b7te", "zu", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Am ersten zerschmetterten Tor.", "tokens": ["Am", "ers\u00b7ten", "zer\u00b7schmet\u00b7ter\u00b7ten", "Tor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Doch als der Morgen, der fahle,", "tokens": ["Doch", "als", "der", "Mor\u00b7gen", ",", "der", "fah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Erblinkte am morschen Portale,", "tokens": ["Er\u00b7blink\u00b7te", "am", "mor\u00b7schen", "Por\u00b7ta\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Fuhr ich auf aus dumpftr\u00e4umendem Tod.", "tokens": ["Fuhr", "ich", "auf", "aus", "dumpf\u00b7tr\u00e4u\u00b7men\u00b7dem", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Ein Sturmwind pfiff durchs Vergreiste,", "tokens": ["Ein", "Sturm\u00b7wind", "pfiff", "durchs", "Ver\u00b7greis\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und das Feld erdr\u00f6hnte und krei\u00dfte,", "tokens": ["Und", "das", "Feld", "er\u00b7dr\u00f6hn\u00b7te", "und", "krei\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Eine Lerche rankte ins Rosenrot.", "tokens": ["Ei\u00b7ne", "Ler\u00b7che", "rank\u00b7te", "ins", "Ro\u00b7sen\u00b7rot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}}}}