{"textgrid.poem.66701": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Winzer Tod", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn jetzt der Tod, der gro\u00dfe Winzer, k\u00e4me,", "tokens": ["Wenn", "jetzt", "der", "Tod", ",", "der", "gro\u00b7\u00dfe", "Win\u00b7zer", ",", "k\u00e4\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mich abzuschneiden von dem Stock der Zeit \u2013", "tokens": ["Mich", "ab\u00b7zu\u00b7schnei\u00b7den", "von", "dem", "Stock", "der", "Zeit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVIZU", "APPR", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Eh er die Traube mit dem Messer n\u00e4hme,", "tokens": ["Eh", "er", "die", "Trau\u00b7be", "mit", "dem", "Mes\u00b7ser", "n\u00e4h\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "S\u00e4nk' ihm der Arm: \u00bbNoch ist die Stunde weit.", "tokens": ["S\u00e4nk'", "ihm", "der", "Arm", ":", "\u00bb", "Noch", "ist", "die", "Stun\u00b7de", "weit", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "$(", "ADV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zwar Sturm und Sonnenschein ward dir beschieden,", "tokens": ["Zwar", "Sturm", "und", "Son\u00b7nen\u00b7schein", "ward", "dir", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Genossen hast du Qual und Lust der Welt,", "tokens": ["Ge\u00b7nos\u00b7sen", "hast", "du", "Qual", "und", "Lust", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Emp\u00f6rung kennst du, und du kennst den Frieden,", "tokens": ["Em\u00b7p\u00f6\u00b7rung", "kennst", "du", ",", "und", "du", "kennst", "den", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den reiferen Fr\u00fcchten hast du dich gesellt.", "tokens": ["Den", "rei\u00b7fe\u00b7ren", "Fr\u00fcch\u00b7ten", "hast", "du", "dich", "ge\u00b7sellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch tiefer sollst du deine Beeren neigen,", "tokens": ["Doch", "tie\u00b7fer", "sollst", "du", "dei\u00b7ne", "Bee\u00b7ren", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und s\u00fc\u00df wie Honig will ich deinen Saft,", "tokens": ["Und", "s\u00fc\u00df", "wie", "Ho\u00b7nig", "will", "ich", "dei\u00b7nen", "Saft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "NN", "VMFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gedeihe noch im Licht- und Schattenreigen \u2013", "tokens": ["Ge\u00b7dei\u00b7he", "noch", "im", "Licht", "und", "Schat\u00b7ten\u00b7rei\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "TRUNC", "KON", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erst wenn du k\u00f6stlich, wirst du heimgerafft.\u00ab", "tokens": ["Erst", "wenn", "du", "k\u00f6st\u00b7lich", ",", "wirst", "du", "heim\u00b7ge\u00b7rafft", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADJD", "$,", "VAFIN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}