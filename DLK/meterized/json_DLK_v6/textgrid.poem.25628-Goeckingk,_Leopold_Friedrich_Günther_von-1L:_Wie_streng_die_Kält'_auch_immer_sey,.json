{"textgrid.poem.25628": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie streng die K\u00e4lt' auch immer sey,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie streng die K\u00e4lt' auch immer sey,", "tokens": ["Wie", "streng", "die", "K\u00e4lt'", "auch", "im\u00b7mer", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wird er seinen Hut doch unterm Arme f\u00fchren.", "tokens": ["So", "wird", "er", "sei\u00b7nen", "Hut", "doch", "un\u00b7term", "Ar\u00b7me", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich k\u00f6nnte mein Gehirn erfrieren;", "tokens": ["Ich", "k\u00f6nn\u00b7te", "mein", "Ge\u00b7hirn", "er\u00b7frie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er waget aber nichts dabei.", "tokens": ["Er", "wa\u00b7get", "a\u00b7ber", "nichts", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}