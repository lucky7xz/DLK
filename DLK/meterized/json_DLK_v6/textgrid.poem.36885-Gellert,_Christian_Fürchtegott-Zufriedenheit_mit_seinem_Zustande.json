{"textgrid.poem.36885": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Zufriedenheit mit seinem Zustande", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du klagst, und f\u00fchlest die Beschwerden", "tokens": ["Du", "klagst", ",", "und", "f\u00fch\u00b7lest", "die", "Be\u00b7schwer\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Stands, in dem du d\u00fcrftig lebst;", "tokens": ["Des", "Stands", ",", "in", "dem", "du", "d\u00fcrf\u00b7tig", "lebst", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du strebest, gl\u00fccklicher zu werden,", "tokens": ["Du", "stre\u00b7best", ",", "gl\u00fcck\u00b7li\u00b7cher", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und siehst, da\u00df du vergebens strebst.", "tokens": ["Und", "siehst", ",", "da\u00df", "du", "ver\u00b7ge\u00b7bens", "strebst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ja, klage! Gott erlaubt die Z\u00e4hren;", "tokens": ["Ja", ",", "kla\u00b7ge", "!", "Gott", "er\u00b7laubt", "die", "Z\u00e4h\u00b7ren", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "$.", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch denk im Klagen auch zur\u00fcck.", "tokens": ["Doch", "denk", "im", "Kla\u00b7gen", "auch", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist denn das Gl\u00fcck, das wir begehren,", "tokens": ["Ist", "denn", "das", "Gl\u00fcck", ",", "das", "wir", "be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr uns auch stets ein wahres Gl\u00fcck?", "tokens": ["F\u00fcr", "uns", "auch", "stets", "ein", "wah\u00b7res", "Gl\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nie schenkt der Stand, nie schenken G\u00fcter", "tokens": ["Nie", "schenkt", "der", "Stand", ",", "nie", "schen\u00b7ken", "G\u00fc\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Dem Menschen die Zufriedenheit.", "tokens": ["Dem", "Men\u00b7schen", "die", "Zu\u00b7frie\u00b7den\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die wahre Ruhe der Gem\u00fcter", "tokens": ["Die", "wah\u00b7re", "Ru\u00b7he", "der", "Ge\u00b7m\u00fc\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist Tugend und Gen\u00fcgsamkeit.", "tokens": ["Ist", "Tu\u00b7gend", "und", "Ge\u00b7n\u00fcg\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Genie\u00dfe, was dir Gott beschieden,", "tokens": ["Ge\u00b7nie\u00b7\u00dfe", ",", "was", "dir", "Gott", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entbehre gern, was du nicht hast.", "tokens": ["Ent\u00b7beh\u00b7re", "gern", ",", "was", "du", "nicht", "hast", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWS", "PPER", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein jeder Stand hat seinen Frieden,", "tokens": ["Ein", "je\u00b7der", "Stand", "hat", "sei\u00b7nen", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein jeder Stand auch seine Last.", "tokens": ["Ein", "je\u00b7der", "Stand", "auch", "sei\u00b7ne", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Gott ist der Herr, und seinen Segen", "tokens": ["Gott", "ist", "der", "Herr", ",", "und", "sei\u00b7nen", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verteilt er stets mit weiser Hand;", "tokens": ["Ver\u00b7teilt", "er", "stets", "mit", "wei\u00b7ser", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht so, wie wir's zu w\u00fcnschen pflegen,", "tokens": ["Nicht", "so", ",", "wie", "wir's", "zu", "w\u00fcn\u00b7schen", "pfle\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "PWAV", "PIS", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch so, wie er's uns heilsam fand.", "tokens": ["Doch", "so", ",", "wie", "er's", "uns", "heil\u00b7sam", "fand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "PIS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Willst du zu denken dich erk\u00fchnen,", "tokens": ["Willst", "du", "zu", "den\u00b7ken", "dich", "er\u00b7k\u00fch\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKZU", "VVINF", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df seine Liebe dich vergi\u00dft?", "tokens": ["Da\u00df", "sei\u00b7ne", "Lie\u00b7be", "dich", "ver\u00b7gi\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er gibt uns mehr, als wir verdienen,", "tokens": ["Er", "gibt", "uns", "mehr", ",", "als", "wir", "ver\u00b7die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und niemals, was uns sch\u00e4dlich ist.", "tokens": ["Und", "nie\u00b7mals", ",", "was", "uns", "sch\u00e4d\u00b7lich", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Verzehre nicht des Lebens Kr\u00e4fte", "tokens": ["Ver\u00b7zeh\u00b7re", "nicht", "des", "Le\u00b7bens", "Kr\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In tr\u00e4ger Unzufriedenheit;", "tokens": ["In", "tr\u00e4\u00b7ger", "Un\u00b7zu\u00b7frie\u00b7den\u00b7heit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Besorge deines Stands Gesch\u00e4fte,", "tokens": ["Be\u00b7sor\u00b7ge", "dei\u00b7nes", "Stands", "Ge\u00b7sch\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und n\u00fctze deine Lebenszeit.", "tokens": ["Und", "n\u00fct\u00b7ze", "dei\u00b7ne", "Le\u00b7bens\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Bei Pflicht und Flei\u00df sich Gott ergeben,", "tokens": ["Bei", "Pflicht", "und", "Flei\u00df", "sich", "Gott", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PRF", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein ewig Gl\u00fcck in Hoffnung sehn,", "tokens": ["Ein", "e\u00b7wig", "Gl\u00fcck", "in", "Hoff\u00b7nung", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dies ist der Weg zu Ruh und Leben.", "tokens": ["Dies", "ist", "der", "Weg", "zu", "Ruh", "und", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Herr, lehre diesen Weg mich gehn!", "tokens": ["Herr", ",", "leh\u00b7re", "die\u00b7sen", "Weg", "mich", "gehn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PDAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}