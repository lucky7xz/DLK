{"textgrid.poem.57218": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Unabl\u00e4ssig Sinken", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unabl\u00e4ssig Sinken", "tokens": ["Un\u00b7ab\u00b7l\u00e4s\u00b7sig", "Sin\u00b7ken"], "token_info": ["word", "word"], "pos": ["ADJD", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "wei\u00dfer Wogenwucht,", "tokens": ["wei\u00b7\u00dfer", "Wo\u00b7gen\u00b7wucht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "la\u00df mich, deine Bucht,", "tokens": ["la\u00df", "mich", ",", "dei\u00b7ne", "Bucht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "dein Geheimnis trinken.", "tokens": ["dein", "Ge\u00b7heim\u00b7nis", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Engel w\u00f6lken leise", "tokens": ["En\u00b7gel", "w\u00f6l\u00b7ken", "lei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "aus der Wasser Scho\u00df,", "tokens": ["aus", "der", "Was\u00b7ser", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "l\u00f6sen gro\u00df sich los", "tokens": ["l\u00f6\u00b7sen", "gro\u00df", "sich", "los"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PRF", "PTKVZ"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "nach D\u00e4monenweise.", "tokens": ["nach", "D\u00e4\u00b7mo\u00b7nen\u00b7wei\u00b7se", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Strahlen bis zum bleichen", "tokens": ["Strah\u00b7len", "bis", "zum", "blei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "APPRART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Mond der H\u00e4upter Firn ...", "tokens": ["Mond", "der", "H\u00e4up\u00b7ter", "Firn", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und auf Schl\u00e4fer-Stirn", "tokens": ["Und", "auf", "Schl\u00e4\u00b7fer\u00b7Stirn"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "--+-+", "measure": "anapaest.init"}, "line.4": {"text": "malen sie das Zeichen ...", "tokens": ["ma\u00b7len", "sie", "das", "Zei\u00b7chen", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Taufen gern Erh\u00f6rten", "tokens": ["Tau\u00b7fen", "gern", "Er\u00b7h\u00f6r\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "mit der Weisheit Tau.", "tokens": ["mit", "der", "Weis\u00b7heit", "Tau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und von ferner Schau", "tokens": ["Und", "von", "fer\u00b7ner", "Schau"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "d\u00e4mmert dem Entt\u00f6rten.", "tokens": ["d\u00e4m\u00b7mert", "dem", "Ent\u00b7t\u00f6r\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}