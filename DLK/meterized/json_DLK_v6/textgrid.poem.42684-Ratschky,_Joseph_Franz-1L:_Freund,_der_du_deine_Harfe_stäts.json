{"textgrid.poem.42684": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freund, der du deine Harfe st\u00e4ts", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund, der du deine Harfe st\u00e4ts", "tokens": ["Freund", ",", "der", "du", "dei\u00b7ne", "Har\u00b7fe", "st\u00e4ts"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Dem Dienste strenger Wahrheit weihtest,", "tokens": ["Dem", "Diens\u00b7te", "stren\u00b7ger", "Wahr\u00b7heit", "weih\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und laut und ernst das eiserne Gesetz", "tokens": ["Und", "laut", "und", "ernst", "das", "ei\u00b7ser\u00b7ne", "Ge\u00b7setz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Allgewaltigen mit deutschem Muth bestreitest!", "tokens": ["Der", "All\u00b7ge\u00b7wal\u00b7ti\u00b7gen", "mit", "deut\u00b7schem", "Muth", "be\u00b7strei\u00b7test", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Lass mich den Unmuth, der mein Herz", "tokens": ["Lass", "mich", "den", "Un\u00b7muth", ",", "der", "mein", "Herz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.2": {"text": "In bangen Stunden oft zernaget,", "tokens": ["In", "ban\u00b7gen", "Stun\u00b7den", "oft", "zer\u00b7na\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In deine Brust ergiessen; denn der Schmerz", "tokens": ["In", "dei\u00b7ne", "Brust", "er\u00b7gies\u00b7sen", ";", "denn", "der", "Schmerz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$.", "KON", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wirkt schw\u00e4cher, wenn man ihn f\u00fchlbaren Seelen klaget.", "tokens": ["Wirkt", "schw\u00e4\u00b7cher", ",", "wenn", "man", "ihn", "f\u00fchl\u00b7ba\u00b7ren", "See\u00b7len", "kla\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "KOUS", "PIS", "PPER", "VVFIN", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Oft steigt das wallende Gebl\u00fct", "tokens": ["Oft", "steigt", "das", "wal\u00b7len\u00b7de", "Ge\u00b7bl\u00fct"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir auf die Wange, wenn, geschm\u00fccket", "tokens": ["Mir", "auf", "die", "Wan\u00b7ge", ",", "wenn", ",", "ge\u00b7schm\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "APPR", "ART", "NN", "$,", "KOUS", "$,", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit schn\u00f6dem Gold, ein m\u00e4chtiger Bandit", "tokens": ["Mit", "schn\u00f6\u00b7dem", "Gold", ",", "ein", "m\u00e4ch\u00b7ti\u00b7ger", "Ban\u00b7dit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem armen Biedermann den Nacken stolz zerdr\u00fccket.", "tokens": ["Dem", "ar\u00b7men", "Bie\u00b7der\u00b7mann", "den", "Na\u00b7cken", "stolz", "zer\u00b7dr\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sieh dort! ein goldner Mandarin", "tokens": ["Sieh", "dort", "!", "ein", "gold\u00b7ner", "Man\u00b7da\u00b7rin"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Rollt mit vier Rappen durch die Strassen:", "tokens": ["Rollt", "mit", "vier", "Rap\u00b7pen", "durch", "die", "Stras\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "CARD", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Lass mich die Mask' ihm von dem Antlitz ziehn,", "tokens": ["Lass", "mich", "die", "Mas\u00b7k'", "ihm", "von", "dem", "Ant\u00b7litz", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+--++-+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "So fegt er als ein Schelm im braunen Filz die Gassen.", "tokens": ["So", "fegt", "er", "als", "ein", "Schelm", "im", "brau\u00b7nen", "Filz", "die", "Gas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "ART", "NN", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Indess der Bosheit Knechte sich", "tokens": ["In\u00b7dess", "der", "Bos\u00b7heit", "Knech\u00b7te", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Wanst mit leckern Speisen f\u00fcllen,", "tokens": ["Den", "Wanst", "mit", "le\u00b7ckern", "Spei\u00b7sen", "f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u00c4chzt mancher Freund der Tugend k\u00fcmmerlich,", "tokens": ["\u00c4chzt", "man\u00b7cher", "Freund", "der", "Tu\u00b7gend", "k\u00fcm\u00b7mer\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und kann des Hungers Wuth oft kaum mit Brode stillen.", "tokens": ["Und", "kann", "des", "Hun\u00b7gers", "Wuth", "oft", "kaum", "mit", "Bro\u00b7de", "stil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "NN", "ADV", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Freund! wem vor krummen Pfaden graut,", "tokens": ["Freund", "!", "wem", "vor", "krum\u00b7men", "Pfa\u00b7den", "graut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird st\u00e4ts mit Noth und Mangel kriegen.", "tokens": ["Wird", "st\u00e4ts", "mit", "Noth", "und", "Man\u00b7gel", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ward denn die Welt f\u00fcr B\u00f6se nur gebaut,", "tokens": ["Ward", "denn", "die", "Welt", "f\u00fcr", "B\u00f6\u00b7se", "nur", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und muss der brave Mann sich st\u00e4ts im Staube schmiegen?", "tokens": ["Und", "muss", "der", "bra\u00b7ve", "Mann", "sich", "st\u00e4ts", "im", "Stau\u00b7be", "schmie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "PRF", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Zwar schwingt (ein seltnes Meteor!)", "tokens": ["Zwar", "schwingt", "(", "ein", "selt\u00b7nes", "Me\u00b7te\u00b7or", "!", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "$(", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl auch die Tugend sich: doch ziehet", "tokens": ["Wohl", "auch", "die", "Tu\u00b7gend", "sich", ":", "doch", "zie\u00b7het"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "PRF", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie Billigkeit auch dann noch R\u00e4nken vor,", "tokens": ["Sie", "Bil\u00b7lig\u00b7keit", "auch", "dann", "noch", "R\u00e4n\u00b7ken", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADV", "ADV", "ADV", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So hat, kaum halb gesehn, der neue Stern vergl\u00fchet.", "tokens": ["So", "hat", ",", "kaum", "halb", "ge\u00b7sehn", ",", "der", "neu\u00b7e", "Stern", "ver\u00b7gl\u00fc\u00b7het", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "ADV", "ADJD", "VVPP", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Sieh! Aristid, der Biedermann,", "tokens": ["Sieh", "!", "A\u00b7ris\u00b7tid", ",", "der", "Bie\u00b7der\u00b7mann", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00e4llt, weil er seine B\u00fcrgerpflichten", "tokens": ["F\u00e4llt", ",", "weil", "er", "sei\u00b7ne", "B\u00fcr\u00b7ger\u00b7pflich\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu treu erf\u00fcllt, unschuldig in den Bann,", "tokens": ["Zu", "treu", "er\u00b7f\u00fcllt", ",", "un\u00b7schul\u00b7dig", "in", "den", "Bann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVPP", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und muss sich aus Athen mit Schimpf und Schande fl\u00fcchten.", "tokens": ["Und", "muss", "sich", "aus", "A\u00b7then", "mit", "Schimpf", "und", "Schan\u00b7de", "fl\u00fcch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "APPR", "NE", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.9": {"line.1": {"text": "So pflegt man, Freund! der Redlichkeit", "tokens": ["So", "pflegt", "man", ",", "Freund", "!", "der", "Red\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "$,", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von jeher unterm Mond zu lohnen:", "tokens": ["Von", "je\u00b7her", "un\u00b7term", "Mond", "zu", "loh\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch tr\u00f6ste dich! denn Selbstzufriedenheit,", "tokens": ["Doch", "tr\u00f6s\u00b7te", "dich", "!", "denn", "Selbst\u00b7zu\u00b7frie\u00b7den\u00b7heit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Tugend uns gew\u00e4hrt, hat h\u00f6hern Werth, als Kronen.", "tokens": ["Die", "Tu\u00b7gend", "uns", "ge\u00b7w\u00e4hrt", ",", "hat", "h\u00f6\u00b7hern", "Werth", ",", "als", "Kro\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$,", "VAFIN", "ADJA", "NN", "$,", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}