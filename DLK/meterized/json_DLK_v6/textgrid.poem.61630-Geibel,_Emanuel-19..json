{"textgrid.poem.61630": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht war tr\u00e4umerisch, wir zogen", "tokens": ["Die", "Nacht", "war", "tr\u00e4u\u00b7me\u00b7risch", ",", "wir", "zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hinab des Parnes dunkle Schlucht,", "tokens": ["Hin\u00b7ab", "des", "Par\u00b7nes", "dunk\u00b7le", "Schlucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da gr\u00fc\u00dft' uns pl\u00f6tzlich weit im Bogen", "tokens": ["Da", "gr\u00fc\u00dft'", "uns", "pl\u00f6tz\u00b7lich", "weit", "im", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Eleusis' mondbegl\u00e4nzte Bucht.", "tokens": ["E\u00b7leu\u00b7sis'", "mond\u00b7be\u00b7gl\u00e4nz\u00b7te", "Bucht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wir sahn Kith\u00e4rons Gipfel winken,", "tokens": ["Wir", "sahn", "Kit\u00b7h\u00e4\u00b7rons", "Gip\u00b7fel", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und unsrer Rosse Huf betrat,", "tokens": ["Und", "uns\u00b7rer", "Ros\u00b7se", "Huf", "be\u00b7trat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Bergwand rechts, das Meer zur Linken,", "tokens": ["Die", "Berg\u00b7wand", "rechts", ",", "das", "Meer", "zur", "Lin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des heil'gen Wegs uralten Pfad.", "tokens": ["Des", "heil'\u00b7gen", "Wegs", "ur\u00b7al\u00b7ten", "Pfad", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Hier flo\u00df, die Feier zu bereiten,", "tokens": ["Hier", "flo\u00df", ",", "die", "Fei\u00b7er", "zu", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Haupt bekr\u00e4nzt mit Asphodil,", "tokens": ["Das", "Haupt", "be\u00b7kr\u00e4nzt", "mit", "As\u00b7pho\u00b7dil", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dereinst der Festzug der Geweihten", "tokens": ["De\u00b7reinst", "der", "Fest\u00b7zug", "der", "Ge\u00b7weih\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bei Fackelglanz und Fl\u00f6tenspiel.", "tokens": ["Bei", "Fa\u00b7ckel\u00b7glanz", "und", "Fl\u00f6\u00b7ten\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Fromm zu Demeters Heiligtume", "tokens": ["Fromm", "zu", "De\u00b7me\u00b7ters", "Hei\u00b7lig\u00b7tu\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Den Strand hin wallten sie die Bahn,", "tokens": ["Den", "Strand", "hin", "wall\u00b7ten", "sie", "die", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Rebenbluts, der Weizenkrume", "tokens": ["Des", "Re\u00b7ben\u00b7bluts", ",", "der", "Wei\u00b7zen\u00b7kru\u00b7me"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tiefdeutig Sinnbild zu empfahn.", "tokens": ["Tief\u00b7deu\u00b7tig", "Sinn\u00b7bild", "zu", "em\u00b7pfahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbin Flammen wird das Korn zum Brote,", "tokens": ["\u00bb", "in", "Flam\u00b7men", "wird", "das", "Korn", "zum", "Bro\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Traube g\u00e4rt zermalmt zum Wein,", "tokens": ["Die", "Trau\u00b7be", "g\u00e4rt", "zer\u00b7malmt", "zum", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Lebens Bl\u00fcte reift im Tode.\u00ab", "tokens": ["Des", "Le\u00b7bens", "Bl\u00fc\u00b7te", "reift", "im", "To\u00b7de", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So klang das Chorlied durch die Reihn.", "tokens": ["So", "klang", "das", "Chor\u00b7lied", "durch", "die", "Reihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So klang's, und tausend Herzen schwollen,", "tokens": ["So", "klang's", ",", "und", "tau\u00b7send", "Her\u00b7zen", "schwol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KON", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Graus der Schattenwelt befreit,", "tokens": ["Vom", "Graus", "der", "Schat\u00b7ten\u00b7welt", "be\u00b7freit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Getr\u00f6stet von dem r\u00e4tselvollen", "tokens": ["Ge\u00b7tr\u00f6s\u00b7tet", "von", "dem", "r\u00e4t\u00b7sel\u00b7vol\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gedanken der Unsterblichkeit. \u2013 \u2013", "tokens": ["Ge\u00b7dan\u00b7ken", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "ART", "NN", "$.", "$(", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Da pl\u00f6tzlich hielten unsre Pferde,", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "hiel\u00b7ten", "uns\u00b7re", "Pfer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Eleusis war erreicht; es bot", "tokens": ["E\u00b7leu\u00b7sis", "war", "er\u00b7reicht", ";", "es", "bot"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VAFIN", "VVPP", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Gastfreund uns den Platz am Herde", "tokens": ["Der", "Gast\u00b7freund", "uns", "den", "Platz", "am", "Her\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und bracht' uns dienend \u2013 Wein und Brot.", "tokens": ["Und", "bracht'", "uns", "die\u00b7nend", "\u2013", "Wein", "und", "Brot", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}