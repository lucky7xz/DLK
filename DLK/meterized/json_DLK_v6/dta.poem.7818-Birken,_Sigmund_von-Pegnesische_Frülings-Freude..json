{"dta.poem.7818": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "Pegnesische  \n Fr\u00fclings-Freude.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1665", "urn": "urn:nbn:de:kobv:b4-200905198447", "language": ["de:0.99"], "booktitle": "Birken, Sigmund von: Pegnesische Gespr\u00e4chspiel-Gesellschaft von Nymfen und Hirten. N\u00fcrnberg, 1665."}, "poem": {"stanza.1": {"line.1": {"text": "Sitz hier/ wie du gethan/ O Paris/ zu Ge-", "tokens": ["Sitz", "hier", "/", "wie", "du", "ge\u00b7than", "/", "O", "Pa\u00b7ris", "/", "zu", "Ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "$(", "PWAV", "PPER", "VVPP", "$(", "NE", "NE", "$(", "APPR", "TRUNC"], "meter": "+-+--++-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "betrachte/ die du sihst/ in Einer/ drey G\u00f6t-", "tokens": ["be\u00b7trach\u00b7te", "/", "die", "du", "sihst", "/", "in", "Ei\u00b7ner", "/", "drey", "G\u00f6t"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$(", "PRELS", "PPER", "VVFIN", "$(", "APPR", "PIS", "$(", "CARD", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "An Stand/ sie Juno ist; und an Verstand", "tokens": ["An", "Stand", "/", "sie", "Ju\u00b7no", "ist", ";", "und", "an", "Ver\u00b7stand"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$(", "PPER", "NN", "VAFIN", "$.", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "(sicht", "tokens": ["(", "sicht"], "token_info": ["punct", "word"], "pos": ["$(", "VVFIN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "die Pallas; Venus schaut aus ihrem Ange", "tokens": ["die", "Pal\u00b7las", ";", "Ve\u00b7nus", "schaut", "aus", "ih\u00b7rem", "An\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Stand/ Sch\u00f6nheit/ und Verstand/ sie in", "tokens": ["Stand", "/", "Sch\u00f6n\u00b7heit", "/", "und", "Ver\u00b7stand", "/", "sie", "in"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$(", "NN", "$(", "KON", "NN", "$(", "PPER", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "wie solt ihr dreymal nicht das Apfel-gold", "tokens": ["wie", "solt", "ihr", "drey\u00b7mal", "nicht", "das", "Ap\u00b7fel\u00b7gold"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}