 "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "$.", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}}, "stanza.5": {"line.1": {"text": "Wird auf ihrem weissen Zelter,", "tokens": ["Wird", "auf", "ih\u00b7rem", "weis\u00b7sen", "Zel\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "l\u00e4chelnd,", "tokens": ["l\u00e4\u00b7chelnd", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "auf mein klopfendes Herz zu,", "tokens": ["auf", "mein", "klop\u00b7fen\u00b7des", "Herz", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "die Prinzessin reiten?", "tokens": ["die", "Prin\u00b7zes\u00b7sin", "rei\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Nichts.", "tokens": ["Nichts", "."], "token_info": ["word", "punct"], "pos": ["PIS", "$."], "meter": "+", "measure": "single.up"}}, "stanza.7": {"line.1": {"text": "Wie schwarze Urweltkr\u00f6ten,", "tokens": ["Wie", "schwar\u00b7ze", "Ur\u00b7welt\u00b7kr\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "regungslos,", "tokens": ["re\u00b7gungs\u00b7los", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "hockt am Weg der Wachholder.", "tokens": ["hockt", "am", "Weg", "der", "Wach\u00b7hol\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.8": {"line.1": {"text": "Zwischendurch", "tokens": ["Zwi\u00b7schen\u00b7durch"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "giftrot", "tokens": ["gif\u00b7trot"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "leuchten Fliegenpilze.", "tokens": ["leuch\u00b7ten", "Flie\u00b7gen\u00b7pil\u00b7ze", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}