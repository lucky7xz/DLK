{"dta.poem.14461": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An den heiligen Joseph.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Du warst ja auch einmal hier unten,", "tokens": ["Du", "warst", "ja", "auch", "ein\u00b7mal", "hier", "un\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hast ew'ger Treue Schmerz empfunden;", "tokens": ["Hast", "ew'\u00b7ger", "Treu\u00b7e", "Schmerz", "emp\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "L\u00e4ngst war Maria fortgezogen,", "tokens": ["L\u00e4ngst", "war", "Ma\u00b7ria", "fort\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie einsam rauschten rings die dunklen Wogen!", "tokens": ["Wie", "ein\u00b7sam", "rauschten", "rings", "die", "dunk\u00b7len", "Wo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Da breitet oben Sie die Arme aus:", "tokens": ["Da", "brei\u00b7tet", "o\u00b7ben", "Sie", "die", "Ar\u00b7me", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Komm', treuer Pilger, endlich auch nach Haus!", "tokens": ["Komm'", ",", "treu\u00b7er", "Pil\u00b7ger", ",", "end\u00b7lich", "auch", "nach", "Haus", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}