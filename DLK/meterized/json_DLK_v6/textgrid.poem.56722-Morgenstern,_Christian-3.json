{"textgrid.poem.56722": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "3", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4sche ist heute wohl,", "tokens": ["W\u00e4\u00b7sche", "ist", "heu\u00b7te", "wohl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "gro\u00dfe W\u00e4sche,", "tokens": ["gro\u00b7\u00dfe", "W\u00e4\u00b7sche", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "droben im Himmelreich.", "tokens": ["dro\u00b7ben", "im", "Him\u00b7mel\u00b7reich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Denn seht nur, seht!", "tokens": ["Denn", "seht", "nur", ",", "seht", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "wie viele Hemdlein,", "tokens": ["wie", "vie\u00b7le", "Hemd\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "H\u00f6slein, R\u00f6cklein,", "tokens": ["H\u00f6s\u00b7lein", ",", "R\u00f6ck\u00b7lein", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "und zierliche Str\u00fcmpflein", "tokens": ["und", "zier\u00b7li\u00b7che", "Str\u00fcm\u00b7pflein"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "die gute Schaffnerin", "tokens": ["die", "gu\u00b7te", "Schaff\u00b7ne\u00b7rin"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "\u00fcber die blaue Himmelswiese", "tokens": ["\u00fc\u00b7ber", "die", "blau\u00b7e", "Him\u00b7mels\u00b7wie\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "zum Trocknen breitet.", "tokens": ["zum", "Trock\u00b7nen", "brei\u00b7tet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Die kleinen Nixen,", "tokens": ["Die", "klei\u00b7nen", "Ni\u00b7xen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+---", "measure": "dactylic.init"}, "line.12": {"text": "Gnomen, Elben,", "tokens": ["Gno\u00b7men", ",", "El\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NE", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Engelchen, Teufelchen,", "tokens": ["En\u00b7gel\u00b7chen", ",", "Teu\u00b7fel\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJA", "$,"], "meter": "+--++-", "measure": "dactylic.init"}, "line.14": {"text": "oder wie sie ihr Vater nennt,", "tokens": ["o\u00b7der", "wie", "sie", "ihr", "Va\u00b7ter", "nennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "liegen wohl alle nun", "tokens": ["lie\u00b7gen", "wohl", "al\u00b7le", "nun"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PIS", "ADV"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.16": {"text": "in ihren Bettchen,", "tokens": ["in", "ih\u00b7ren", "Bett\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "bis ans Kinn", "tokens": ["bis", "ans", "Kinn"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.18": {"text": "die Decken gezogen,", "tokens": ["die", "De\u00b7cken", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.19": {"text": "und sehnlich lugend,", "tokens": ["und", "sehn\u00b7lich", "lu\u00b7gend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.20": {"text": "ob denn die Alte", "tokens": ["ob", "denn", "die", "Al\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.21": {"text": "ihren einzigen Staat,", "tokens": ["ih\u00b7ren", "ein\u00b7zi\u00b7gen", "Staat", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.22": {"text": "ihre wei\u00dfen Kleidchen,", "tokens": ["ih\u00b7re", "wei\u00b7\u00dfen", "Kleid\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.23": {"text": "nicht bald", "tokens": ["nicht", "bald"], "token_info": ["word", "word"], "pos": ["PTKNEG", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.24": {"text": "ihnen wiederbringe.", "tokens": ["ih\u00b7nen", "wie\u00b7der\u00b7brin\u00b7ge", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.25": {"text": "Die aber legt", "tokens": ["Die", "a\u00b7ber", "legt"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADV", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.26": {"text": "ernst und bed\u00e4chtig", "tokens": ["ernst", "und", "be\u00b7d\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "ADJD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.27": {"text": "ein St\u00fcck nach dem andern", "tokens": ["ein", "St\u00fcck", "nach", "dem", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.28": {"text": "noch auf den Rasen.", "tokens": ["noch", "auf", "den", "Ra\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "++-+-", "measure": "iambic.di"}}}}}