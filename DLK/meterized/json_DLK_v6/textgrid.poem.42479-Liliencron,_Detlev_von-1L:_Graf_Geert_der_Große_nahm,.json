{"textgrid.poem.42479": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Graf Geert der Gro\u00dfe nahm,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Graf Geert der Gro\u00dfe nahm,", "tokens": ["Graf", "Geert", "der", "Gro\u00b7\u00dfe", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ritt er mit Schwert und Schild,", "tokens": ["Ritt", "er", "mit", "Schwert", "und", "Schild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Vom Altar in die Schlacht", "tokens": ["Vom", "Al\u00b7tar", "in", "die", "Schlacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Stets ein Madonnenbild.", "tokens": ["Stets", "ein", "Ma\u00b7don\u00b7nen\u00b7bild", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von Silber, kleingeformt,", "tokens": ["Von", "Sil\u00b7ber", ",", "klein\u00b7ge\u00b7formt", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Des Bischofs reiche Spende,", "tokens": ["Des", "Bi\u00b7schofs", "rei\u00b7che", "Spen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mu\u00df oft Sunte Marie", "tokens": ["Mu\u00df", "oft", "Sun\u00b7te", "Ma\u00b7rie"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "NN", "NE"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "In Pfeilepfad und Br\u00e4nde.", "tokens": ["In", "Pfei\u00b7le\u00b7pfad", "und", "Br\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Graf tr\u00e4gt vorn am Hals", "tokens": ["Der", "Graf", "tr\u00e4gt", "vorn", "am", "Hals"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die hochgelobte Frau.", "tokens": ["Die", "hoch\u00b7ge\u00b7lob\u00b7te", "Frau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo sein Geschwader stampft,", "tokens": ["Wo", "sein", "Ge\u00b7schwa\u00b7der", "stampft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Welkt ab die Blumenau.", "tokens": ["Welkt", "ab", "die", "Blu\u00b7me\u00b7nau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Einst schlug ein D\u00e4nenmars", "tokens": ["Einst", "schlug", "ein", "D\u00e4\u00b7nen\u00b7mars"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Im w\u00fctendsten Gefechte", "tokens": ["Im", "w\u00fc\u00b7tends\u00b7ten", "Ge\u00b7fech\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Der Himmelsk\u00f6nigin", "tokens": ["Der", "Him\u00b7mels\u00b7k\u00f6\u00b7ni\u00b7gin"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Mit Mordhieb ab die Rechte.", "tokens": ["Mit", "Mord\u00b7hieb", "ab", "die", "Rech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Als nun der Krieg vorbei,", "tokens": ["Als", "nun", "der", "Krieg", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wird schnell die Hand geflickt.", "tokens": ["Wird", "schnell", "die", "Hand", "ge\u00b7flickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch kaum ist sie geschwei\u00dft,", "tokens": ["Doch", "kaum", "ist", "sie", "ge\u00b7schwei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ist auch sie abgeknickt.", "tokens": ["Ist", "auch", "sie", "ab\u00b7ge\u00b7knickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hilft Gottes Liebe nicht?", "tokens": ["Hilft", "Got\u00b7tes", "Lie\u00b7be", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Kein Zeichen? Kein Mirakel?", "tokens": ["Kein", "Zei\u00b7chen", "?", "Kein", "Mi\u00b7ra\u00b7kel", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Hand f\u00e4llt immer ab,", "tokens": ["Die", "Hand", "f\u00e4llt", "im\u00b7mer", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Als w\u00e4r sie voller Makel.", "tokens": ["Als", "w\u00e4r", "sie", "vol\u00b7ler", "Ma\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da gibt der Graf es auf,", "tokens": ["Da", "gibt", "der", "Graf", "es", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und zieht in Fehd und Feld;", "tokens": ["Und", "zieht", "in", "Fehd", "und", "Feld", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist, sattelstolz, voll Kraft,", "tokens": ["Ist", ",", "sat\u00b7tel\u00b7stolz", ",", "voll", "Kraft", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ADJD", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Allein auf sich gestellt.", "tokens": ["Al\u00b7lein", "auf", "sich", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "VVPP", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Im Kloster Itzehoe", "tokens": ["Im", "Klos\u00b7ter", "It\u00b7ze\u00b7hoe"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Kniet er dann auf den Stufen;", "tokens": ["Kniet", "er", "dann", "auf", "den", "Stu\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Was schaut sein Auge zag?", "tokens": ["Was", "schaut", "sein", "Au\u00b7ge", "zag", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "H\u00f6rt er die Heiligen rufen?", "tokens": ["H\u00f6rt", "er", "die", "Hei\u00b7li\u00b7gen", "ru\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Wer ruft ihn? Ob ers wagt?", "tokens": ["Wer", "ruft", "ihn", "?", "Ob", "ers", "wagt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Er wagt den Blick empor:", "tokens": ["Er", "wagt", "den", "Blick", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Mutter Jesu zeigt", "tokens": ["Die", "Mut\u00b7ter", "Je\u00b7su", "zeigt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die rechte Hand ihm vor.", "tokens": ["Die", "rech\u00b7te", "Hand", "ihm", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es singt die reine Magd", "tokens": ["Es", "singt", "die", "rei\u00b7ne", "Magd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ganz leise, zart und leise.", "tokens": ["Ganz", "lei\u00b7se", ",", "zart", "und", "lei\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Doch klar vernimmt der Graf", "tokens": ["Doch", "klar", "ver\u00b7nimmt", "der", "Graf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Den Atem ihrer Weise:", "tokens": ["Den", "A\u00b7tem", "ih\u00b7rer", "Wei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Was trugst du mich ins Blut?", "tokens": ["Was", "trugst", "du", "mich", "ins", "Blut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der, der gestorben ist,", "tokens": ["Der", ",", "der", "ge\u00b7stor\u00b7ben", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gab hin sein Blut f\u00fcr dich,", "tokens": ["Gab", "hin", "sein", "Blut", "f\u00fcr", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mein Sohn, der hohe Christ.", "tokens": ["Mein", "Sohn", ",", "der", "ho\u00b7he", "Christ", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er tat es still und gro\u00df,", "tokens": ["Er", "tat", "es", "still", "und", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fcr deine Schuld hienieden.", "tokens": ["F\u00fcr", "dei\u00b7ne", "Schuld", "hien\u00b7ie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "La\u00df ab von Zank und Zorn,", "tokens": ["La\u00df", "ab", "von", "Zank", "und", "Zorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Er starb f\u00fcr deinen Frieden.", "tokens": ["Er", "starb", "f\u00fcr", "dei\u00b7nen", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}