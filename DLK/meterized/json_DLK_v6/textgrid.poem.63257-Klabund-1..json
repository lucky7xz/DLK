{"textgrid.poem.63257": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer wird einst deinen s\u00fcssen Namen wissen,", "tokens": ["Wer", "wird", "einst", "dei\u00b7nen", "s\u00fcs\u00b7sen", "Na\u00b7men", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn nicht mein Wille ihn in Wort gepr\u00e4gt?", "tokens": ["Wenn", "nicht", "mein", "Wil\u00b7le", "ihn", "in", "Wort", "ge\u00b7pr\u00e4gt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "NN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn ich ihn nicht in Elfenbein gelegt", "tokens": ["Wenn", "ich", "ihn", "nicht", "in", "El\u00b7fen\u00b7bein", "ge\u00b7legt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und mit dem Schattenspiel des Ruhms umrissen?", "tokens": ["Und", "mit", "dem", "Schat\u00b7ten\u00b7spiel", "des", "Ruhms", "um\u00b7ris\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Einst wird man Wimpel auf dem \u00bbCoelius\u00ab hissen!", "tokens": ["Einst", "wird", "man", "Wim\u00b7pel", "auf", "dem", "\u00bb", "Coe\u00b7lius", "\u00ab", "his\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "NN", "APPR", "ART", "$(", "NE", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Coelius heissen Kinder, die erregt", "tokens": ["Und", "Coe\u00b7lius", "heis\u00b7sen", "Kin\u00b7der", ",", "die", "er\u00b7regt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NE", "ADJA", "NN", "$,", "PRELS", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Muttertraum zu Heldentaten w\u00e4gt.", "tokens": ["Ein", "Mut\u00b7ter\u00b7traum", "zu", "Hel\u00b7den\u00b7ta\u00b7ten", "w\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Coelius seufzt es zwischen tausend K\u00fcssen.", "tokens": ["Und", "Coe\u00b7lius", "seufzt", "es", "zwi\u00b7schen", "tau\u00b7send", "K\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dann wirst du l\u00e4ngst im feuchten Grabe liegen,", "tokens": ["Dann", "wirst", "du", "l\u00e4ngst", "im", "feuch\u00b7ten", "Gra\u00b7be", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo Mohn allein die tr\u00fcbe Tafel weist.", "tokens": ["Wo", "Mohn", "al\u00b7lein", "die", "tr\u00fc\u00b7be", "Ta\u00b7fel", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Vogel wird sich auf der Weide wiegen,", "tokens": ["Ein", "Vo\u00b7gel", "wird", "sich", "auf", "der", "Wei\u00b7de", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Von fernen Tropen zwitschernd zugereist \u2013", "tokens": ["Von", "fer\u00b7nen", "Tro\u00b7pen", "zwit\u00b7schernd", "zu\u00b7ge\u00b7reist", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Um ihn von Spatz und Nachtigall ein Reigen \u2013", "tokens": ["Um", "ihn", "von", "Spatz", "und", "Nach\u00b7ti\u00b7gall", "ein", "Rei\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "APPR", "NN", "KON", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er geistert schillernd \u2013 Geist von deinem Geist.", "tokens": ["Er", "geis\u00b7tert", "schil\u00b7lernd", "\u2013", "Geist", "von", "dei\u00b7nem", "Geist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$(", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}