{"textgrid.poem.63506": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "Vader Arndt", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oll Vader Arndt, du oll d\u00fctsch Blot,", "tokens": ["Oll", "Va\u00b7der", "Arndt", ",", "du", "oll", "d\u00fctsch", "Blot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,", "PPER", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "h\u00fct warst du n\u00e4gentig Johr!", "tokens": ["h\u00fct", "warst", "du", "n\u00e4\u00b7gen\u00b7tig", "Johr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADJD", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wat lett din slohwitt Poll di got,", "tokens": ["Wat", "lett", "din", "sloh\u00b7witt", "Poll", "di", "got", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wat is din \u00d6ller ror!", "tokens": ["wat", "is", "din", "\u00d6l\u00b7ler", "ror", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dor liggt sonn kloren Fr\u00fchjohrsschin", "tokens": ["Dor", "liggt", "sonn", "klo\u00b7ren", "Fr\u00fch\u00b7johrs\u00b7schin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00fcm Ogen di un Bran.", "tokens": ["\u00fcm", "O\u00b7gen", "di", "un", "Bran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sonn blank Kron dreggst, keen K\u00f6nig sin", "tokens": ["Sonn", "blank", "Kron", "dreggst", ",", "ke\u00b7en", "K\u00f6\u00b7nig", "sin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "NN", "VVFIN", "$,", "VVFIN", "NN", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "kann neben din bestahn.", "tokens": ["kann", "ne\u00b7ben", "din", "be\u00b7stahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Den Ihrenkranz un Austkranz hett", "tokens": ["Den", "Ih\u00b7ren\u00b7kranz", "un", "Aust\u00b7kranz", "hett"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "FM", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00fcns' leewe Herrgott jo", "tokens": ["\u00fcns'", "lee\u00b7we", "Herr\u00b7gott", "jo"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "up dine breede Stirn di sett't,", "tokens": ["up", "di\u00b7ne", "bree\u00b7de", "Stirn", "di", "sett't", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "un dorvon l\u00fccht't se so.", "tokens": ["un", "dor\u00b7von", "l\u00fccht't", "se", "so", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "De l\u00fccht't as Eddelsteen un Gold", "tokens": ["De", "l\u00fccht't", "as", "Ed\u00b7del\u00b7ste\u00b7en", "un", "Gold"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "hen d\u00f6rch ganz D\u00fctschland h\u00fct.", "tokens": ["hen", "d\u00f6rch", "ganz", "D\u00fct\u00b7schland", "h\u00fct", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ADV", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Glik got, wat Landsmann, jung un olt,", "tokens": ["Glik", "got", ",", "wat", "Lands\u00b7mann", ",", "jung", "un", "olt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PRELS", "NE", "$,", "ADJD", "FM", "FM", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "de freugt sick, de dat s\u00fcht.", "tokens": ["de", "freugt", "sick", ",", "de", "dat", "s\u00fcht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,", "FM.da", "FM.da", "FM.da", "$."], "meter": "--+-+-", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Glik v\u00e4l, ob He\u00df een odder Preu\u00df,", "tokens": ["Glik", "v\u00e4l", ",", "ob", "He\u00df", "e\u00b7en", "od\u00b7der", "Preu\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "KOUS", "NE", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "ob ut dat baiersch Land,", "tokens": ["ob", "ut", "dat", "bai\u00b7ersch", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "FM", "FM", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "ob ut't Hannoversch odder Reu\u00df", "tokens": ["ob", "ut't", "Han\u00b7no\u00b7ver\u00b7sch", "od\u00b7der", "Reu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "NE", "KON", "NE"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "un vun de ditmarsch Kant,", "tokens": ["un", "vun", "de", "dit\u00b7marsch", "Kant", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "glik v\u00e4l, ob ut Land M\u00e4kelb\u00f6rg,", "tokens": ["glik", "v\u00e4l", ",", "ob", "ut", "Land", "M\u00e4\u00b7kel\u00b7b\u00f6rg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "KOUS", "FM", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "...", "tokens": ["..."], "token_info": ["punct"], "pos": ["$("]}, "line.3": {"text": "dat heel oll D\u00fctschland d\u00f6rch un d\u00f6rch", "tokens": ["dat", "heel", "oll", "D\u00fct\u00b7schland", "d\u00f6rch", "un", "d\u00f6rch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "dat freugt sick h\u00fct mit di.", "tokens": ["dat", "freugt", "sick", "h\u00fct", "mit", "di."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Wi weet' dat woll jo l\u00fctt un grot", "tokens": ["Wi", "weet'", "dat", "woll", "jo", "l\u00fctt", "un", "grot"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "un in de Schol dat Kind,", "tokens": ["un", "in", "de", "Schol", "dat", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "APPR", "NE", "NE", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "wi weet' dat alltosam recht got,", "tokens": ["wi", "weet'", "dat", "all\u00b7to\u00b7sam", "recht", "got", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "wat wi di sch\u00fcllig s\u00fcnd;", "tokens": ["wat", "wi", "di", "sch\u00fcl\u00b7lig", "s\u00fcnd", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "in Storm un an dat Spill,", "tokens": ["in", "Storm", "un", "an", "dat", "Spill", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "FM", "APPR", "ART", "NN", "$,"], "meter": "-++-+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "wua du to \u00fcns hest stahn, oll Mann,", "tokens": ["wua", "du", "to", "\u00fcns", "hest", "stahn", ",", "oll", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "FM", "FM", "FM", "FM", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "mit Bl\u00fccher un mit Schill,", "tokens": ["mit", "Bl\u00fc\u00b7cher", "un", "mit", "Schill", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "FM", "APPR", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "wua du in'n Storm hest stahn.", "tokens": ["wua", "du", "in'n", "Storm", "hest", "stahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Dat W\u00e4der dat was rusig slimm,", "tokens": ["Dat", "W\u00e4\u00b7der", "dat", "was", "ru\u00b7sig", "slimm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "de Blitz sl\u00f6g v\u00f6r di dal,", "tokens": ["de", "Blitz", "sl\u00f6g", "v\u00f6r", "di", "dal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "di \u0153werst sl\u00f6g de Slag nich kr\u00fcmm;", "tokens": ["di", "\u0153werst", "sl\u00f6g", "de", "Slag", "nich", "kr\u00fcmm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "NN", "PTKNEG", "ADJD", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "fast st\u00fcnnst du as een Pahl.", "tokens": ["fast", "st\u00fcnnst", "du", "as", "e\u00b7en", "Pahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "V\u00e4l stiwe Kirls smet \u00fcm un \u00fcm", "tokens": ["V\u00e4l", "sti\u00b7we", "Kirls", "smet", "\u00fcm", "un", "\u00fcm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "dat W\u00e4der, sl\u00f6g dat dal;", "tokens": ["dat", "W\u00e4\u00b7der", ",", "sl\u00f6g", "dat", "dal", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "di \u0153werst sl\u00f6g keen Slag nich kr\u00fcmm,", "tokens": ["di", "\u0153werst", "sl\u00f6g", "ke\u00b7en", "Slag", "nich", "kr\u00fcmm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "VVFIN", "NN", "PTKNEG", "ADJD", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "fast st\u00fcnnst du as een Pahl.", "tokens": ["fast", "st\u00fcnnst", "du", "as", "e\u00b7en", "Pahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "Fast st\u00fcnnst du dor in Rat un Dat,", "tokens": ["Fast", "st\u00fcnnst", "du", "dor", "in", "Rat", "un", "Dat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "de rechte Mann wast du.", "tokens": ["de", "rech\u00b7te", "Mann", "wast", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VAFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Din Wurt dat was de rechte Dat;", "tokens": ["Din", "Wurt", "dat", "was", "de", "rech\u00b7te", "Dat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "din Aust des \u00fchst du nu.", "tokens": ["din", "Aust", "des", "\u00fchst", "du", "nu", "."], "token_info": ["word", "word", "word", "word", "word", "wor