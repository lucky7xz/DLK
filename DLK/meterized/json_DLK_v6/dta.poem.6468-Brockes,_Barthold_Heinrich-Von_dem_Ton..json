{"dta.poem.6468": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Ton.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was durch Bewegen riecht, sind C\u00f6rper, deren Dufft", "tokens": ["Was", "durch", "Be\u00b7we\u00b7gen", "riecht", ",", "sind", "C\u00f6r\u00b7per", ",", "de\u00b7ren", "Dufft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWS", "APPR", "NN", "VVFIN", "$,", "VAFIN", "NE", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Getragen zwischen, mit, und in der Lufft.", "tokens": ["Ge\u00b7tra\u00b7gen", "zwi\u00b7schen", ",", "mit", ",", "und", "in", "der", "Lufft", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "APPR", "$,", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Allein der Ton ist nichts, als Lufft, die sich beweget,", "tokens": ["Al\u00b7lein", "der", "Ton", "ist", "nichts", ",", "als", "Lufft", ",", "die", "sich", "be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PIS", "$,", "KOUS", "NN", "$,", "PRELS", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der sich durch C\u00f6rper zeugt, ersch\u00fcttert, und erreget.", "tokens": ["Der", "sich", "durch", "C\u00f6r\u00b7per", "zeugt", ",", "er\u00b7sch\u00fct\u00b7tert", ",", "und", "er\u00b7re\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NE", "VVFIN", "$,", "VVPP", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}