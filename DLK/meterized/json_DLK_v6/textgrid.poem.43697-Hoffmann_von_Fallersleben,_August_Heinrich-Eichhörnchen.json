{"textgrid.poem.43697": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Eichh\u00f6rnchen", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hei\u00dfa, wer tanzt mit mir?", "tokens": ["Hei\u00b7\u00dfa", ",", "wer", "tanzt", "mit", "mir", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VVFIN", "APPR", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Lustig und munter!", "tokens": ["Lus\u00b7tig", "und", "mun\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Kopf\u00fcber, kopfunter", "tokens": ["Kopf\u00b7\u00fc\u00b7ber", ",", "ko\u00b7pfun\u00b7ter"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mit Manier!", "tokens": ["Mit", "Ma\u00b7nier", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Immerfort", "tokens": ["Im\u00b7mer\u00b7fort"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Von Ort zu Ort,", "tokens": ["Von", "Ort", "zu", "Ort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Jetzo hier,", "tokens": ["Jet\u00b7zo", "hier", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Jetzo dort! Hopp!", "tokens": ["Jet\u00b7zo", "dort", "!", "Hopp", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "$.", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ohne Ruh, ohne Rast,", "tokens": ["Oh\u00b7ne", "Ruh", ",", "oh\u00b7ne", "Rast", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Vom Zweig auf den Ast,", "tokens": ["Vom", "Zweig", "auf", "den", "Ast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Vom Ast auf den Wipfel hoch in die Luft,", "tokens": ["Vom", "Ast", "auf", "den", "Wip\u00b7fel", "hoch", "in", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Im Bl\u00e4tters\u00e4usel und Bl\u00fcthenduft!", "tokens": ["Im", "Bl\u00e4t\u00b7ter\u00b7s\u00e4u\u00b7sel", "und", "Bl\u00fc\u00b7then\u00b7duft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Immerzu", "tokens": ["Im\u00b7mer\u00b7zu"], "token_info": ["word"], "pos": ["ADV"], "meter": "+OO", "measure": "unknown.measure.single"}, "line.2": {"text": "Ohne Rast, ohne Ruh!", "tokens": ["Oh\u00b7ne", "Rast", ",", "oh\u00b7ne", "Ruh", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Heut ist Kirms und heut ist Ball!", "tokens": ["Heut", "ist", "Kirms", "und", "heut", "ist", "Ball", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "KON", "ADV", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Spielet, Drossel, Nachtigall,", "tokens": ["Spie\u00b7let", ",", "Dros\u00b7sel", ",", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Stieglitz, Amsel, Fink und Specht,", "tokens": ["Stieg\u00b7litz", ",", "Am\u00b7sel", ",", "Fink", "und", "Specht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Pfeift und geigt und macht es recht!", "tokens": ["Pfeift", "und", "geigt", "und", "macht", "es", "recht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ich bin ein Mann,", "tokens": ["Ich", "bin", "ein", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Der tanzen kann.", "tokens": ["Der", "tan\u00b7zen", "kann", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VMFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "H\u00e4nschen Eichhorn hei\u00df' ich,", "tokens": ["H\u00e4n\u00b7schen", "Eich\u00b7horn", "hei\u00df'", "ich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Was ich gelernt hab', wei\u00df ich.", "tokens": ["Was", "ich", "ge\u00b7lernt", "hab'", ",", "wei\u00df", "ich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VAFIN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kommt der J\u00e4ger in Wald hinein,", "tokens": ["Kommt", "der", "J\u00e4\u00b7ger", "in", "Wald", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Will mir kein Vogel singen;", "tokens": ["Will", "mir", "kein", "Vo\u00b7gel", "sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "H\u00e4nschen l\u00e4\u00dft das Tanzen sein,", "tokens": ["H\u00e4n\u00b7schen", "l\u00e4\u00dft", "das", "Tan\u00b7zen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Tanzen, H\u00fcpfen und Springen;", "tokens": ["Tan\u00b7zen", ",", "H\u00fcp\u00b7fen", "und", "Sprin\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "H\u00e4nschen schl\u00fcpft hinein zum Haus,", "tokens": ["H\u00e4n\u00b7schen", "schl\u00fcpft", "hin\u00b7ein", "zum", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00e4nschen schaut zum Haus heraus,", "tokens": ["H\u00e4n\u00b7schen", "schaut", "zum", "Haus", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "H\u00e4nschen lacht den J\u00e4ger aus.", "tokens": ["H\u00e4n\u00b7schen", "lacht", "den", "J\u00e4\u00b7ger", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}