{"textgrid.poem.48813": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "38. Auf ein Kleinot", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was, Seele, war es not so einer reichen Gaben,", "tokens": ["Was", ",", "See\u00b7le", ",", "war", "es", "not", "so", "ei\u00b7ner", "rei\u00b7chen", "Ga\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "NN", "$,", "VAFIN", "PPER", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "darzu so manche Welt ihr Bestes hat gesandt?", "tokens": ["dar\u00b7zu", "so", "man\u00b7che", "Welt", "ihr", "Bes\u00b7tes", "hat", "ge\u00b7sandt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PIAT", "NN", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": ". . . . . . . . . . . . . . . . . . . . . . das braune Mohrenland", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "das", "brau\u00b7ne", "Moh\u00b7ren\u00b7land"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "word", "word", "word"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "sein reingewaschnes Gold, der Buzarether Knaben,", "tokens": ["sein", "rein\u00b7ge\u00b7waschnes", "Gold", ",", "der", "Bu\u00b7za\u00b7re\u00b7ther", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "die haben das Gestein' hierzu fern' ausgegraben,", "tokens": ["die", "ha\u00b7ben", "das", "Ge\u00b7stein'", "hier\u00b7zu", "fern'", "aus\u00b7ge\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PAV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Basora das, was f\u00fchrt sein reicher Perlenstrand.", "tokens": ["Ba\u00b7so\u00b7ra", "das", ",", "was", "f\u00fchrt", "sein", "rei\u00b7cher", "Per\u00b7len\u00b7strand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "$,", "PWS", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So scheints auch, da\u00df es mehr als eine Menschenhand", "tokens": ["So", "scheints", "auch", ",", "da\u00df", "es", "mehr", "als", "ei\u00b7ne", "Men\u00b7schen\u00b7hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PIAT", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "in ein so sch\u00f6nes Tun zusammenbracht mu\u00df haben.", "tokens": ["in", "ein", "so", "sch\u00f6\u00b7nes", "Tun", "zu\u00b7sam\u00b7men\u00b7bracht", "mu\u00df", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "ADV", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "La\u00df, Liebste, la\u00df der Welt, der armen, ihre Sch\u00e4tze,", "tokens": ["La\u00df", ",", "Liebs\u00b7te", ",", "la\u00df", "der", "Welt", ",", "der", "ar\u00b7men", ",", "ih\u00b7re", "Sch\u00e4t\u00b7ze", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "VVIMP", "ART", "NN", "$,", "ART", "ADJA", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "das wird nicht weit geholt, daran ich mich ergetze.", "tokens": ["das", "wird", "nicht", "weit", "ge\u00b7holt", ",", "da\u00b7ran", "ich", "mich", "er\u00b7get\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "ADJD", "VVPP", "$,", "PAV", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auch ists was solches nicht, dran man viel wenden mu\u00df.", "tokens": ["Auch", "ists", "was", "sol\u00b7ches", "nicht", ",", "dran", "man", "viel", "wen\u00b7den", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PWS", "PIS", "PTKNEG", "$,", "PAV", "PIS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du hast es allezeit bei dir, mein ander Leben,", "tokens": ["Du", "hast", "es", "al\u00b7le\u00b7zeit", "bei", "dir", ",", "mein", "an\u00b7der", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "darvon ich leben kan. Je mehr du mirs wirst geben,", "tokens": ["dar\u00b7von", "ich", "le\u00b7ben", "kan", ".", "Je", "mehr", "du", "mirs", "wirst", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVINF", "VMFIN", "$.", "ADV", "ADV", "PPER", "NE", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "je mehr beh\u00e4ltst du es. Was ist es denn? Ein Ku\u00df!", "tokens": ["je", "mehr", "be\u00b7h\u00e4ltst", "du", "es", ".", "Was", "ist", "es", "denn", "?", "Ein", "Ku\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "$.", "PWS", "VAFIN", "PPER", "ADV", "$.", "ART", "NN", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}}}}}