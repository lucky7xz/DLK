{"textgrid.poem.65026": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es geht in Israel die Sage:", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es geht in Israel die Sage:", "tokens": ["Es", "geht", "in", "Is\u00b7rael", "die", "Sa\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auf nachtumh\u00fclltem Himmelplan,", "tokens": ["Auf", "nach\u00b7tum\u00b7h\u00fcll\u00b7tem", "Him\u00b7mel\u00b7plan", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bei Blitzesgluht und Donnerschlage", "tokens": ["Bei", "Blit\u00b7zes\u00b7gluht", "und", "Don\u00b7ner\u00b7schla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zieht der Messias einst heran.", "tokens": ["Zieht", "der", "Mes\u00b7si\u00b7as", "einst", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "O du, des Liedes Gottessegen!", "tokens": ["O", "du", ",", "des", "Lie\u00b7des", "Got\u00b7tes\u00b7se\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auch du erscheinst in Nacht und Graus,", "tokens": ["Auch", "du", "er\u00b7scheinst", "in", "Nacht", "und", "Graus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und, so wie er, auf Flammenwegen", "tokens": ["Und", ",", "so", "wie", "er", ",", "auf", "Flam\u00b7men\u00b7we\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "ADV", "KOKOM", "PPER", "$,", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ziehst du in mein ge\u00f6ffnet Haus. \u2013", "tokens": ["Ziehst", "du", "in", "mein", "ge\u00b7\u00f6ff\u00b7net", "Haus", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "VVPP", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Schwachen m\u00f6gen vor dir zagen,", "tokens": ["Die", "Schwa\u00b7chen", "m\u00f6\u00b7gen", "vor", "dir", "za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Thoren beben deinem Zug: \u2013", "tokens": ["Die", "Tho\u00b7ren", "be\u00b7ben", "dei\u00b7nem", "Zug", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Glanz der Herrlichkeit zu tragen", "tokens": ["Den", "Glanz", "der", "Herr\u00b7lich\u00b7keit", "zu", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist meine Seele stark genug.", "tokens": ["Ist", "mei\u00b7ne", "See\u00b7le", "stark", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Sturm mit seinem Siegessange", "tokens": ["Der", "Sturm", "mit", "sei\u00b7nem", "Sie\u00b7ges\u00b7san\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Flamme, die an Flamme brennt,", "tokens": ["Die", "Flam\u00b7me", ",", "die", "an", "Flam\u00b7me", "brennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das Jauchzen nach dem Untergange", "tokens": ["Das", "Jauch\u00b7zen", "nach", "dem", "Un\u00b7ter\u00b7gan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ist mir verwandtes Element.", "tokens": ["Ist", "mir", "ver\u00b7wand\u00b7tes", "E\u00b7le\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zur Ruhe ward ich nicht geboren,", "tokens": ["Zur", "Ru\u00b7he", "ward", "ich", "nicht", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was sie gew\u00e4hrt, mir scheint es schaal;", "tokens": ["Was", "sie", "ge\u00b7w\u00e4hrt", ",", "mir", "scheint", "es", "schaal", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Kampf hab' ich mir auserkoren,", "tokens": ["Den", "Kampf", "hab'", "ich", "mir", "au\u00b7ser\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die H\u00f6hen such' ich, nicht das Thal.", "tokens": ["Die", "H\u00f6\u00b7hen", "such'", "ich", ",", "nicht", "das", "Thal", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wie den Riesen alter Zeiten", "tokens": ["Und", "wie", "den", "Rie\u00b7sen", "al\u00b7ter", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erkr\u00e4ftigte der Erde Herz,", "tokens": ["Er\u00b7kr\u00e4f\u00b7tig\u00b7te", "der", "Er\u00b7de", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So sch\u00f6pf' ich neue Kraft zum Streiten", "tokens": ["So", "sch\u00f6pf'", "ich", "neu\u00b7e", "Kraft", "zum", "Strei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Aus dir, o du mein heil'ger Schmerz! \u2013", "tokens": ["Aus", "dir", ",", "o", "du", "mein", "heil'\u00b7ger", "Schmerz", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "PPER", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Zuckt denn, ihr Gnadenblitze! nieder,", "tokens": ["Zuckt", "denn", ",", "ihr", "Gna\u00b7den\u00b7blit\u00b7ze", "!", "nie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "NN", "$.", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und schlingt, o schlinget wie vordem", "tokens": ["Und", "schlingt", ",", "o", "schlin\u00b7get", "wie", "vor\u00b7dem"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "FM", "VVFIN", "KOKOM", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Um meine bleiche Stirne wieder,", "tokens": ["Um", "mei\u00b7ne", "blei\u00b7che", "Stir\u00b7ne", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein weithin leuchtend Diadem! \u2013", "tokens": ["Ein", "weit\u00b7hin", "leuch\u00b7tend", "Di\u00b7a\u00b7dem", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "VVPP", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Umstrahlt von diesem Schmerzensglanze,", "tokens": ["Um\u00b7strahlt", "von", "die\u00b7sem", "Schmer\u00b7zens\u00b7glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Geschm\u00fcckt mit meinem Liebesweh,", "tokens": ["Ge\u00b7schm\u00fcckt", "mit", "mei\u00b7nem", "Lie\u00b7bes\u00b7weh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Streb' ich nach keinem andern Kranze,", "tokens": ["Streb'", "ich", "nach", "kei\u00b7nem", "an\u00b7dern", "Kran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nach keiner andern Krone je! \u2013", "tokens": ["Nach", "kei\u00b7ner", "an\u00b7dern", "Kro\u00b7ne", "je", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}