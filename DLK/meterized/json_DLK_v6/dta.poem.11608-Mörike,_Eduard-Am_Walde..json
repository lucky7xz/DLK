{"dta.poem.11608": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Am Walde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Am Waldsaum kann ich lange Nachmittage,", "tokens": ["Am", "Wald\u00b7saum", "kann", "ich", "lan\u00b7ge", "Nach\u00b7mit\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Gukuk horchend, in dem Grase liegen,", "tokens": ["Dem", "Gu\u00b7kuk", "hor\u00b7chend", ",", "in", "dem", "Gra\u00b7se", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er scheint das Thal gem\u00e4chlich einzuwiegen", "tokens": ["Er", "scheint", "das", "Thal", "ge\u00b7m\u00e4ch\u00b7lich", "ein\u00b7zu\u00b7wie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im friedevollen Gleichklang seiner Klage.", "tokens": ["Im", "frie\u00b7de\u00b7vol\u00b7len", "Gleich\u00b7klang", "sei\u00b7ner", "Kla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da ist mir wohl, und meine schlimmste Plage,", "tokens": ["Da", "ist", "mir", "wohl", ",", "und", "mei\u00b7ne", "schlimms\u00b7te", "Pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Fratzen der Gesellschaft mich zu f\u00fcgen,", "tokens": ["Den", "Frat\u00b7zen", "der", "Ge\u00b7sell\u00b7schaft", "mich", "zu", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hier wird sie mich doch endlich nicht bekriegen,", "tokens": ["Hier", "wird", "sie", "mich", "doch", "end\u00b7lich", "nicht", "be\u00b7krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADV", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo ich auf eigne Weise mich behage.", "tokens": ["Wo", "ich", "auf", "eig\u00b7ne", "Wei\u00b7se", "mich", "be\u00b7ha\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und wenn die feinen Leute nur erst d\u00e4chten,", "tokens": ["Und", "wenn", "die", "fei\u00b7nen", "Leu\u00b7te", "nur", "erst", "d\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie sch\u00f6n Poeten ihre Zeit verschwenden,", "tokens": ["Wie", "sch\u00f6n", "Po\u00b7et\u00b7en", "ih\u00b7re", "Zeit", "ver\u00b7schwen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie w\u00fcrden mich zuletzt noch gar beneiden.", "tokens": ["Sie", "w\u00fcr\u00b7den", "mich", "zu\u00b7letzt", "noch", "gar", "be\u00b7nei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Denn des Sonetts gedr\u00e4ngte Kr\u00e4nze flechten", "tokens": ["Denn", "des", "So\u00b7netts", "ge\u00b7dr\u00e4ng\u00b7te", "Kr\u00e4n\u00b7ze", "flech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich wie von selber unter meinen H\u00e4nden,", "tokens": ["Sich", "wie", "von", "sel\u00b7ber", "un\u00b7ter", "mei\u00b7nen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KOKOM", "APPR", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Inde\u00df die Augen in der Ferne weiden.", "tokens": ["In\u00b7de\u00df", "die", "Au\u00b7gen", "in", "der", "Fer\u00b7ne", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}