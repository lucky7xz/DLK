{"textgrid.poem.38632": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Traum", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stets im freundlichen Traum zaubert mein Genius", "tokens": ["Stets", "im", "freund\u00b7li\u00b7chen", "Traum", "zau\u00b7bert", "mein", "Ge\u00b7nius"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Mir ein H\u00fcttchen am See, unter der D\u00e4mmerung", "tokens": ["Mir", "ein", "H\u00fctt\u00b7chen", "am", "See", ",", "un\u00b7ter", "der", "D\u00e4m\u00b7me\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "APPRART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+--+---+--", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Deiner Felsengestade", "tokens": ["Dei\u00b7ner", "Fel\u00b7sen\u00b7ge\u00b7sta\u00b7de"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wildromantisches Meillerie!", "tokens": ["Wild\u00b7ro\u00b7man\u00b7ti\u00b7sches", "Meil\u00b7le\u00b7rie", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Pappeln gr\u00fcnen umher, h\u00e4usliche Schwalben baun", "tokens": ["Pap\u00b7peln", "gr\u00fc\u00b7nen", "um\u00b7her", ",", "h\u00e4us\u00b7li\u00b7che", "Schwal\u00b7ben", "baun"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PTKVZ", "$,", "ADJA", "NN", "VVINF"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Unterm Strohdach, ein Quell sprudelt im Fruchtbaumwald,", "tokens": ["Un\u00b7term", "Stroh\u00b7dach", ",", "ein", "Quell", "spru\u00b7delt", "im", "Frucht\u00b7baum\u00b7wald", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Heimlich dunkelt ein Gr\u00f6ttchen", "tokens": ["Heim\u00b7lich", "dun\u00b7kelt", "ein", "Gr\u00f6tt\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Hinter wankendem Efeulaub.", "tokens": ["Hin\u00b7ter", "wan\u00b7ken\u00b7dem", "E\u00b7feu\u00b7laub", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "V\u00f6gel schlagen am Teich, schw\u00e4rmendes Wollenvieh", "tokens": ["V\u00f6\u00b7gel", "schla\u00b7gen", "am", "Teich", ",", "schw\u00e4r\u00b7men\u00b7des", "Wol\u00b7len\u00b7vieh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,", "ADJA", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Grast im Nachtigallbusch, wo der ged\u00e4mpfte Klang", "tokens": ["Grast", "im", "Nach\u00b7ti\u00b7gall\u00b7busch", ",", "wo", "der", "ge\u00b7d\u00e4mpf\u00b7te", "Klang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Meiner l\u00e4ndlichen Leier", "tokens": ["Mei\u00b7ner", "l\u00e4nd\u00b7li\u00b7chen", "Lei\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Oft im Schimmer des Mondes bebt.", "tokens": ["Oft", "im", "Schim\u00b7mer", "des", "Mon\u00b7des", "bebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}