{"textgrid.poem.63373": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Kukuli", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kleiner Vogel Kukuli,", "tokens": ["Klei\u00b7ner", "Vo\u00b7gel", "Ku\u00b7ku\u00b7li", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Flieh den grauen Norden, flieh,", "tokens": ["Flieh", "den", "grau\u00b7en", "Nor\u00b7den", ",", "flieh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Flieg nach Indien, nach \u00c4gypten", "tokens": ["Flieg", "nach", "In\u00b7di\u00b7en", ",", "nach", "\u00c4\u00b7gyp\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "NE", "$,", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00dcber Gr\u00e4ber, \u00fcber Krypten,", "tokens": ["\u00dc\u00b7ber", "Gr\u00e4\u00b7ber", ",", "\u00fc\u00b7ber", "Kryp\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00dcber L\u00e4nder, \u00fcber Meere,", "tokens": ["\u00dc\u00b7ber", "L\u00e4n\u00b7der", ",", "\u00fc\u00b7ber", "Mee\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kleiner Vogel,", "tokens": ["Klei\u00b7ner", "Vo\u00b7gel", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "La\u00df die schwere Erde unter dir", "tokens": ["La\u00df", "die", "schwe\u00b7re", "Er\u00b7de", "un\u00b7ter", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "ADJA", "NN", "APPR", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Und wiege dich im Himmels\u00e4ther \u2013", "tokens": ["Und", "wie\u00b7ge", "dich", "im", "Him\u00b7mel\u00b7s\u00e4t\u00b7her", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Fliege zwischen Monden, zwischen Sternen", "tokens": ["Flie\u00b7ge", "zwi\u00b7schen", "Mon\u00b7den", ",", "zwi\u00b7schen", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "NN", "$,", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Bis zum Sonnenthron, dem fernen,", "tokens": ["Bis", "zum", "Son\u00b7nen\u00b7thron", ",", "dem", "fer\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Flieg zum Flammengott der Schmerzen", "tokens": ["Flieg", "zum", "Flam\u00b7men\u00b7gott", "der", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und verbrenn' in seinem Herzen!", "tokens": ["Und", "ver\u00b7brenn'", "in", "sei\u00b7nem", "Her\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}