{"dta.poem.7812": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "Ihre sch\u00f6ne Arme.  \n Sonnet.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1665", "urn": "urn:nbn:de:kobv:b4-200905198447", "language": ["de:0.99"], "booktitle": "Birken, Sigmund von: Pegnesische Gespr\u00e4chspiel-Gesellschaft von Nymfen und Hirten. N\u00fcrnberg, 1665."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr Arme/ die ihr nicht an Sch\u00f6nheit ar-", "tokens": ["Ihr", "Ar\u00b7me", "/", "die", "ihr", "nicht", "an", "Sch\u00f6n\u00b7heit", "ar"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "PPER", "PTKNEG", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ihr Liebesf\u00e4ssel ihr/ dergleichen Venus dorte", "tokens": ["ihr", "Lie\u00b7bes\u00b7f\u00e4s\u00b7sel", "ihr", "/", "derg\u00b7lei\u00b7chen", "Ve\u00b7nus", "dor\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPOSAT", "$(", "PIS", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "warf \u00fcm Adonis Hals! ihr macht/ die stum-", "tokens": ["warf", "\u00fcm", "A\u00b7do\u00b7nis", "Hals", "!", "ihr", "macht", "/", "die", "stum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "NN", "$.", "PPER", "VVFIN", "$(", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "der Augen/ redend oft; gebt angenehmes Leid.", "tokens": ["der", "Au\u00b7gen", "/", "re\u00b7dend", "oft", ";", "gebt", "an\u00b7ge\u00b7neh\u00b7mes", "Leid", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADJD", "ADV", "$.", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Beseeltes Marmor du/ Gr\u00e4nz jener Treff-", "tokens": ["Be\u00b7seel\u00b7tes", "Mar\u00b7mor", "du", "/", "Gr\u00e4nz", "je\u00b7ner", "Treff"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "PPER", "$(", "NN", "PDAT", "TRUNC"], "meter": "-+-+-+++-+", "measure": "zehnsilber"}, "line.6": {"text": "des sch\u00f6nen H\u00fcgel-paars dort an der Her-", "tokens": ["des", "sch\u00f6\u00b7nen", "H\u00fc\u00b7gel\u00b7paars", "dort", "an", "der", "Her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "zu neugefallner Schnee/ der bleibet fort und", "tokens": ["zu", "neu\u00b7ge\u00b7fall\u00b7ner", "Schnee", "/", "der", "blei\u00b7bet", "fort", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$(", "ART", "VVFIN", "PTKVZ", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "ach! solt ich sehen dich und k\u00fcssen allezeit/", "tokens": ["ach", "!", "solt", "ich", "se\u00b7hen", "dich", "und", "k\u00fcs\u00b7sen", "al\u00b7le\u00b7zeit", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VMFIN", "PPER", "VVFIN", "PPER", "KON", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "wie ich dann thu\u2019 itzund. Di\u00df sey dan\u0304 mein", "tokens": ["wie", "ich", "dann", "thu'", "it\u00b7zund", ".", "Di\u00df", "sey", "dan\u0304", "mein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "ADV", "$.", "PDS", "VAFIN", "PAV", "PPOSAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "da\u00df ich von euch allzeit/ ihr F\u00e4ssel/ werd \u00fcm-", "tokens": ["da\u00df", "ich", "von", "euch", "all\u00b7zeit", "/", "ihr", "F\u00e4s\u00b7sel", "/", "werd", "\u00fcm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "$(", "PPOSAT", "NN", "$(", "VAFIN", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Die gr\u00f6ste Freyheit ist/ also gefangen seyn.", "tokens": ["Die", "gr\u00f6s\u00b7te", "Frey\u00b7heit", "ist", "/", "al\u00b7so", "ge\u00b7fan\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$(", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Will meine Wangen dann der blasse Tod", "tokens": ["Will", "mei\u00b7ne", "Wan\u00b7gen", "dann", "der", "blas\u00b7se", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "so schlaff\u2019 ich ja bey euch auch sanft und seelig", "tokens": ["so", "schlaff'", "ich", "ja", "bey", "euch", "auch", "sanft", "und", "see\u00b7lig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "ihr himmelische Arm\u2019! ich kan nit s\u00fcsser ster-", "tokens": ["ihr", "him\u00b7me\u00b7li\u00b7sche", "Arm'", "!", "ich", "kan", "nit", "s\u00fcs\u00b7ser", "ster"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "PPER", "VMFIN", "PTKNEG", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}