{"textgrid.poem.54654": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "68. Auff eben dieselbige und meine Tauffe", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gebohren zu dem Tod/ und Leben au\u00dferkohren/", "tokens": ["Ge\u00b7boh\u00b7ren", "zu", "dem", "Tod", "/", "und", "Le\u00b7ben", "au\u00b7\u00dfer\u00b7koh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$(", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verlohren da ich kam/ durch Christum new gebohren/", "tokens": ["Ver\u00b7loh\u00b7ren", "da", "ich", "kam", "/", "durch", "Chris\u00b7tum", "new", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "VVFIN", "$(", "APPR", "NE", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dring ich durchs finster Thal des tr\u00fcben Todes hin/", "tokens": ["Dring", "ich", "durchs", "fins\u00b7ter", "Thal", "des", "tr\u00fc\u00b7ben", "To\u00b7des", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu Gott der Leben heist/ und lebend mein Gewinn", "tokens": ["Zu", "Gott", "der", "Le\u00b7ben", "heist", "/", "und", "le\u00b7bend", "mein", "Ge\u00b7winn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "$(", "KON", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So kam ich auff die Welt in dunckel voller Nacht/", "tokens": ["So", "kam", "ich", "auff", "die", "Welt", "in", "dun\u00b7ckel", "vol\u00b7ler", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und ward in Christus Kirch in hellem Tage bracht.", "tokens": ["Und", "ward", "in", "Chris\u00b7tus", "Kirch", "in", "hel\u00b7lem", "Ta\u00b7ge", "bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NE", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}