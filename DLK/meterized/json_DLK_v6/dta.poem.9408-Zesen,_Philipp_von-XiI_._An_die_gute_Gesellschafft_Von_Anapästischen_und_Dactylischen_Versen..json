{"dta.poem.9408": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "XiI .  \n An die gute Gesellschafft/  \n Von Anap\u00e4stischen und Dactylischen Versen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Liebesten Br\u00fcder/ seyd lustig und munter/", "tokens": ["Ihr", "Lie\u00b7bes\u00b7ten", "Br\u00fc\u00b7der", "/", "seyd", "lus\u00b7tig", "und", "mun\u00b7ter", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "VAFIN", "ADJD", "KON", "ADJD", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "und schlucket den Malvasier wacker hinunter/", "tokens": ["und", "schluc\u00b7ket", "den", "Mal\u00b7va\u00b7sier", "wa\u00b7cker", "hin\u00b7un\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Erzeiget Euch fr\u00f6lich/", "tokens": ["Er\u00b7zei\u00b7get", "Euch", "fr\u00f6\u00b7lich", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "und ob Jhr gleich ehlich", "tokens": ["und", "ob", "Ihr", "gleich", "eh\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Des meistentheils lebet/", "tokens": ["Des", "meis\u00b7ten\u00b7theils", "le\u00b7bet", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Denn sehet wie schwebet", "tokens": ["Denn", "se\u00b7het", "wie", "schwe\u00b7bet"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Doch alles in freuden/", "tokens": ["Doch", "al\u00b7les", "in", "freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Dr\u00fcmb meidet das Leiden/", "tokens": ["Dr\u00fcmb", "mei\u00b7det", "das", "Lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Lebet in freuden bi\u00df kommet die Nacht/", "tokens": ["Le\u00b7bet", "in", "freu\u00b7den", "bi\u00df", "kom\u00b7met", "die", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "KON", "VVFIN", "ART", "NN", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.10": {"text": "Da sich ein jeder nach Bettlehem macht.", "tokens": ["Da", "sich", "ein", "je\u00b7der", "nach", "Bett\u00b7le\u00b7hem", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "PIS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}