{"textgrid.poem.39478": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus w\u00fcsten Meeren und beeisten Zonen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus w\u00fcsten Meeren und beeisten Zonen", "tokens": ["Aus", "w\u00fcs\u00b7ten", "Mee\u00b7ren", "und", "be\u00b7eis\u00b7ten", "Zo\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zieht ein Verh\u00e4ngni\u00df, wunderbar gewunden,", "tokens": ["Zieht", "ein", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", ",", "wun\u00b7der\u00b7bar", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein sittsam Paar, dem keines gleich erfunden,", "tokens": ["Ein", "sitt\u00b7sam", "Paar", ",", "dem", "kei\u00b7nes", "gleich", "er\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "PRELS", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hin zu des S\u00fcdens heitern Regionen.", "tokens": ["Hin", "zu", "des", "S\u00fc\u00b7dens", "hei\u00b7tern", "Re\u00b7gi\u00b7o\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gekr\u00f6nt mit Sch\u00f6nheit statt ererbter Kronen,", "tokens": ["Ge\u00b7kr\u00f6nt", "mit", "Sch\u00f6n\u00b7heit", "statt", "er\u00b7erb\u00b7ter", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Trennt ein Gel\u00fcbd' sie lang', obschon verbunden,", "tokens": ["Trennt", "ein", "Ge\u00b7l\u00fcbd'", "sie", "lang'", ",", "ob\u00b7schon", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADV", "$,", "KOUS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis sie begr\u00fc\u00dft in andachtvollen Stunden", "tokens": ["Bis", "sie", "be\u00b7gr\u00fc\u00dft", "in", "an\u00b7dacht\u00b7vol\u00b7len", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Stadt, wo alle Glorien Christi thronen.", "tokens": ["Die", "Stadt", ",", "wo", "al\u00b7le", "Glo\u00b7ri\u00b7en", "Chris\u00b7ti", "thro\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PIAT", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Gefahr und Lust lockt sie vom Ziel vergebens,", "tokens": ["Ge\u00b7fahr", "und", "Lust", "lockt", "sie", "vom", "Ziel", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und um sie spielt der Menschen weltlich Handeln", "tokens": ["Und", "um", "sie", "spielt", "der", "Men\u00b7schen", "welt\u00b7lich", "Han\u00b7deln"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie bunte Muscheln an der Pilgerhaube.", "tokens": ["Wie", "bun\u00b7te", "Mu\u00b7scheln", "an", "der", "Pil\u00b7ger\u00b7hau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Zur Wallfahrt macht die Wellenfahrt des Lebens,", "tokens": ["Zur", "Wall\u00b7fahrt", "macht", "die", "Wel\u00b7len\u00b7fahrt", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein sichrer Stab den keine Zeiten wandeln,", "tokens": ["Ein", "sich\u00b7rer", "Stab", "den", "kei\u00b7ne", "Zei\u00b7ten", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Edler Muth, reine Lieb' und heil'ger Glaube.", "tokens": ["Ed\u00b7ler", "Muth", ",", "rei\u00b7ne", "Lieb'", "und", "heil'\u00b7ger", "Glau\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}}}}