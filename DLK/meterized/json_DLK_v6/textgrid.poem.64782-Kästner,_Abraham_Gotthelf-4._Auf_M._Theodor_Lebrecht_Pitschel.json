{"textgrid.poem.64782": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "4. Auf M. Theodor Lebrecht Pitschel", "genre": "verse", "period": "N.A.", "pub_year": 1743, "urn": "N.A.", "language": ["de:0.57", "sv:0.42"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu lange fast, o ", "tokens": ["Zu", "lan\u00b7ge", "fast", ",", "o"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "ADV", "ADV", "$,", "FM"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Und bringt dir noch verj\u00e4hrte Thr\u00e4nen dar,", "tokens": ["Und", "bringt", "dir", "noch", "ver\u00b7j\u00e4hr\u00b7te", "Thr\u00e4\u00b7nen", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein Dichterleid, mit unbetr\u00fcbtem Herzen,", "tokens": ["Kein", "Dich\u00b7ter\u00b7leid", ",", "mit", "un\u00b7be\u00b7tr\u00fcb\u00b7tem", "Her\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nein, treu und stark, wie unsre Freundschaft war;", "tokens": ["Nein", ",", "treu", "und", "stark", ",", "wie", "uns\u00b7re", "Freund\u00b7schaft", "war", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "KON", "ADJD", "$,", "PWAV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nicht wie der Sturm erregter Sinnen w\u00fcthet,", "tokens": ["Nicht", "wie", "der", "Sturm", "er\u00b7reg\u00b7ter", "Sin\u00b7nen", "w\u00fct\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOKOM", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der bald sich legt, wenn man vern\u00fcnftig denkt,", "tokens": ["Der", "bald", "sich", "legt", ",", "wenn", "man", "ver\u00b7n\u00fcnf\u00b7tig", "denkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PRF", "VVFIN", "$,", "KOUS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nein, ein Affect, den die Vernunft gebietet,", "tokens": ["Nein", ",", "ein", "Af\u00b7fect", ",", "den", "die", "Ver\u00b7nunft", "ge\u00b7bie\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Weil dein Verlust, erkannt, nur st\u00e4rker kr\u00e4nkt.", "tokens": ["Weil", "dein", "Ver\u00b7lust", ",", "er\u00b7kannt", ",", "nur", "st\u00e4r\u00b7ker", "kr\u00e4nkt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "VVPP", "$,", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ja, ", "tokens": ["Ja", ","], "token_info": ["word", "punct"], "pos": ["PTKANT", "$,"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Erpre\u00dfte sie gleich keiner Freundschaft Pflicht:", "tokens": ["Er\u00b7pre\u00df\u00b7te", "sie", "gleich", "kei\u00b7ner", "Freund\u00b7schaft", "Pflicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da wo den Schmerz Verlust und Sehnsucht n\u00e4hren,", "tokens": ["Da", "wo", "den", "Schmerz", "Ver\u00b7lust", "und", "Sehn\u00b7sucht", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da lindert ihn der Zeiten L\u00e4nge nicht.", "tokens": ["Da", "lin\u00b7dert", "ihn", "der", "Zei\u00b7ten", "L\u00e4n\u00b7ge", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ach wollt' ich doch als Freund dich gerne missen,", "tokens": ["Ach", "wollt'", "ich", "doch", "als", "Freund", "dich", "ger\u00b7ne", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "KOUS", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Verl\u00f6r' ich nichts, als einen Freund in dir!", "tokens": ["Ver\u00b7l\u00f6r'", "ich", "nichts", ",", "als", "ei\u00b7nen", "Freund", "in", "dir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "$,", "KOUS", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dein redlich Herz, dein viel und n\u00fctzlich Wissen", "tokens": ["Dein", "red\u00b7lich", "Herz", ",", "dein", "viel", "und", "n\u00fctz\u00b7lich", "Wis\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "PPOSAT", "ADV", "KON", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Entgeht mir auch, doch nicht allein nur mir.", "tokens": ["Ent\u00b7geht", "mir", "auch", ",", "doch", "nicht", "al\u00b7lein", "nur", "mir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ADV", "PTKNEG", "ADV", "ADV", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gott, dessen Macht, nach uns zu hohen Gr\u00fcnden,", "tokens": ["Gott", ",", "des\u00b7sen", "Macht", ",", "nach", "uns", "zu", "ho\u00b7hen", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELAT", "NN", "$,", "APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit weiser Huld die beste Welt regiert,", "tokens": ["Mit", "wei\u00b7ser", "Huld", "die", "bes\u00b7te", "Welt", "re\u00b7giert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verzeihst du wohl ein k\u00fchnes Unterwinden", "tokens": ["Ver\u00b7zeihst", "du", "wohl", "ein", "k\u00fch\u00b7nes", "Un\u00b7ter\u00b7win\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Menschlichkeit, die herber Schmerz verf\u00fchrt?", "tokens": ["Der", "Menschlich\u00b7keit", ",", "die", "her\u00b7ber", "Schmerz", "ver\u00b7f\u00fchrt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ach d\u00fcrfte dich gekr\u00e4nkte Neugier fragen:", "tokens": ["Ach", "d\u00fcrf\u00b7te", "dich", "ge\u00b7kr\u00e4nk\u00b7te", "Neu\u00b7gier", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Warum du stets das Gute zeitig nimmst,", "tokens": ["Wa\u00b7rum", "du", "stets", "das", "Gu\u00b7te", "zei\u00b7tig", "nimmst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und oft den Lauf von allzuwenig Tagen", "tokens": ["Und", "oft", "den", "Lauf", "von", "all\u00b7zu\u00b7we\u00b7nig", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dem besten Theil der Sterblichen bestimmst?", "tokens": ["Dem", "bes\u00b7ten", "Theil", "der", "Sterb\u00b7li\u00b7chen", "be\u00b7stimmst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie reizet uns des Geistes muntre Jugend,", "tokens": ["Wie", "rei\u00b7zet", "uns", "des", "Geis\u00b7tes", "mun\u00b7tre", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der viel gew\u00e4hrt, und mehr dadurch verspricht!", "tokens": ["Der", "viel", "ge\u00b7w\u00e4hrt", ",", "und", "mehr", "da\u00b7durch", "ver\u00b7spricht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "$,", "KON", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er brennt allein f\u00fcr Wahrheit und f\u00fcr Tugend,", "tokens": ["Er", "brennt", "al\u00b7lein", "f\u00fcr", "Wahr\u00b7heit", "und", "f\u00fcr", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "St\u00e4rkt den Verstand, und \u00fcbet seine Pflicht.", "tokens": ["St\u00e4rkt", "den", "Ver\u00b7stand", ",", "und", "\u00fc\u00b7bet", "sei\u00b7ne", "Pflicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Wie wird er einst, o Herr, dein Reich zu mehren,", "tokens": ["Wie", "wird", "er", "einst", ",", "o", "Herr", ",", "dein", "Reich", "zu", "meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sich voll von Kraft, von Eifer voll, bem\u00fchn!", "tokens": ["Sich", "voll", "von", "Kraft", ",", "von", "Ei\u00b7fer", "voll", ",", "be\u00b7m\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "NN", "$,", "APPR", "NN", "ADJD", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Zu eitler Wahn! da wir ihn hoffend ehren,", "tokens": ["Zu", "eit\u00b7ler", "Wahn", "!", "da", "wir", "ihn", "hof\u00b7fend", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "KOUS", "PPER", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zeigt er sich uns, und mu\u00df von uns entfliehn.", "tokens": ["Zeigt", "er", "sich", "uns", ",", "und", "mu\u00df", "von", "uns", "ent\u00b7fliehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PPER", "$,", "KON", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Giebst du uns denn nicht darum edle Seelen,", "tokens": ["Giebst", "du", "uns", "denn", "nicht", "da\u00b7rum", "ed\u00b7le", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "PTKNEG", "PAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die deine Huld mit h\u00f6h'rer Einsicht ziert,", "tokens": ["Die", "dei\u00b7ne", "Huld", "mit", "h\u00f6h'\u00b7rer", "Ein\u00b7sicht", "ziert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ihre Zahl, die wir zu bald durchz\u00e4hlen,", "tokens": ["Da\u00df", "ih\u00b7re", "Zahl", ",", "die", "wir", "zu", "bald", "durch\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "PRELS", "PPER", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das gro\u00dfe Heer gemeiner Geister f\u00fchrt?", "tokens": ["Das", "gro\u00b7\u00dfe", "Heer", "ge\u00b7mei\u00b7ner", "Geis\u00b7ter", "f\u00fchrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Stark an Vernunft, im Glauben fest gegr\u00fcndet,", "tokens": ["Stark", "an", "Ver\u00b7nunft", ",", "im", "Glau\u00b7ben", "fest", "ge\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "F\u00fcr beyder Werth, und beyder Eintracht k\u00e4mpft,", "tokens": ["F\u00fcr", "bey\u00b7der", "Werth", ",", "und", "bey\u00b7der", "Ein\u00b7tracht", "k\u00e4mpft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Durch sichern Schlu\u00df den leichten Zweifler bindet,", "tokens": ["Durch", "si\u00b7chern", "Schlu\u00df", "den", "leich\u00b7ten", "Zweif\u00b7ler", "bin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "Des Irrgeists Wahn durch deine Worte d\u00e4mpft?", "tokens": ["Des", "Irr\u00b7geists", "Wahn", "durch", "dei\u00b7ne", "Wor\u00b7te", "d\u00e4mpft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Doch wer, o Herr, darf deine Wege meistern?", "tokens": ["Doch", "wer", ",", "o", "Herr", ",", "darf", "dei\u00b7ne", "We\u00b7ge", "meis\u00b7tern", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "$,", "FM", "NN", "$,", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein Rathschlu\u00df liegt f\u00fcr unsern Blick zu tief;", "tokens": ["Dein", "Rath\u00b7schlu\u00df", "liegt", "f\u00fcr", "un\u00b7sern", "Blick", "zu", "tief", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gehorchen nur, geb\u00fchrt erschaffnen Geistern,", "tokens": ["Ge\u00b7hor\u00b7chen", "nur", ",", "ge\u00b7b\u00fchrt", "er\u00b7schaff\u00b7nen", "Geis\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf dessen Wink, der sie zum Daseyn rief:", "tokens": ["Auf", "des\u00b7sen", "Wink", ",", "der", "sie", "zum", "Da\u00b7seyn", "rief", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie sp\u00e4t, wie fr\u00fch sie f\u00fcr den Himmel reifen,", "tokens": ["Wie", "sp\u00e4t", ",", "wie", "fr\u00fch", "sie", "f\u00fcr", "den", "Him\u00b7mel", "rei\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie lang' ihr Dienst der K\u00f6rperwelt geh\u00f6rt,", "tokens": ["Wie", "lang'", "ihr", "Dienst", "der", "K\u00f6r\u00b7per\u00b7welt", "ge\u00b7h\u00f