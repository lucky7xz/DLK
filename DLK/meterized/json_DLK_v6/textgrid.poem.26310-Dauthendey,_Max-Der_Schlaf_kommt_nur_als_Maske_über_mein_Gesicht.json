{"textgrid.poem.26310": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Der Schlaf kommt nur als Maske \u00fcber mein Gesicht", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Schlaf kommt nur als Maske \u00fcber mein Gesicht,", "tokens": ["Der", "Schlaf", "kommt", "nur", "als", "Mas\u00b7ke", "\u00fc\u00b7ber", "mein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "KOUS", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darunter wallt mein Blutstrom, der heimwehfiebrig spricht.", "tokens": ["Da\u00b7run\u00b7ter", "wallt", "mein", "Bluts\u00b7trom", ",", "der", "heim\u00b7weh\u00b7fieb\u00b7rig", "spricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Er bringt mir in dem Traum den liebsten Leib.", "tokens": ["Er", "bringt", "mir", "in", "dem", "Traum", "den", "liebs\u00b7ten", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich finde heim im Wahn zu meinem Weib,", "tokens": ["Ich", "fin\u00b7de", "heim", "im", "Wahn", "zu", "mei\u00b7nem", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bis da\u00df des Schlafes Maske spr\u00f6d wie Gips zerbricht.", "tokens": ["Bis", "da\u00df", "des", "Schla\u00b7fes", "Mas\u00b7ke", "spr\u00f6d", "wie", "Gips", "zer\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "NN", "VVFIN", "KOKOM", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wieder stier' ich taumelnd ins leere Tageslicht.", "tokens": ["Und", "wie\u00b7der", "stier'", "ich", "tau\u00b7melnd", "ins", "lee\u00b7re", "Ta\u00b7ges\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}