{"dta.poem.9821": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Wider das frauenzimmer au\u00df dem  \n Frantz\u00f6ischen.  \n \u2020 \u2020 \u2020", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als GOtt/ das grosse werck der sch\u00f6pfung zu beschliessen/", "tokens": ["Als", "Gott", "/", "das", "gros\u00b7se", "werck", "der", "sch\u00f6p\u00b7fung", "zu", "be\u00b7schlies\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$(", "ART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Adam und in ihm sein ebenbild gemacht/", "tokens": ["Den", "A\u00b7dam", "und", "in", "ihm", "sein", "e\u00b7ben\u00b7bild", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "APPR", "PPER", "PPOSAT", "ADJD", "VVPP", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Stund der begl\u00fcckte mensch aus nichts hervorgebracht/", "tokens": ["Stund", "der", "be\u00b7gl\u00fcck\u00b7te", "mensch", "aus", "nichts", "her\u00b7vor\u00b7ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "APPR", "PIS", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und sah die gantze welt/ als herr/ zu seinen f\u00fcssen.", "tokens": ["Und", "sah", "die", "gant\u00b7ze", "welt", "/", "als", "herr", "/", "zu", "sei\u00b7nen", "f\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$(", "KOUS", "NN", "$(", "APPR", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was erd und Paradies/ was thier und vogel hiessen/", "tokens": ["Was", "erd", "und", "Pa\u00b7ra\u00b7dies", "/", "was", "thier", "und", "vo\u00b7gel", "hies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "$(", "PWS", "ADJD", "KON", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "War alles ingesamt auf seine ruh bedacht.", "tokens": ["War", "al\u00b7les", "in\u00b7ge\u00b7samt", "auf", "sei\u00b7ne", "ruh", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er lebt\u2019 auch h\u00f6chst vergn\u00fcgt; allein o kurtze pracht!", "tokens": ["Er", "lebt'", "auch", "h\u00f6chst", "ver\u00b7gn\u00fcgt", ";", "al\u00b7lein", "o", "kurt\u00b7ze", "pracht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "VVPP", "$.", "ADV", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sein gl\u00fccke war zu gro\u00df es lange zu geniessen.", "tokens": ["Sein", "gl\u00fc\u00b7cke", "war", "zu", "gro\u00df", "es", "lan\u00b7ge", "zu", "ge\u00b7nies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKA", "ADJD", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In meinung/ wie man sprach/ er w\u00e4re gantz allein/", "tokens": ["In", "mei\u00b7nung", "/", "wie", "man", "sprach", "/", "er", "w\u00e4\u00b7re", "gantz", "al\u00b7lein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "PWAV", "PIS", "VVFIN", "$(", "PPER", "VAFIN", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Gab man ihm eine frau; kont\u2019 auch was \u00e4rgers seyn?", "tokens": ["Gab", "man", "ihm", "ei\u00b7ne", "frau", ";", "kont'", "auch", "was", "\u00e4r\u00b7gers", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "ART", "NN", "$.", "VMFIN", "ADV", "PWS", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der arme lag und schlief/ und konte sich nicht wehren/", "tokens": ["Der", "ar\u00b7me", "lag", "und", "schlief", "/", "und", "kon\u00b7te", "sich", "nicht", "weh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "KON", "VVFIN", "$(", "KON", "VMFIN", "PRF", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Man schuf aus ihm ein weib/ das brachte man ihm zu.", "tokens": ["Man", "schuf", "aus", "ihm", "ein", "weib", "/", "das", "brach\u00b7te", "man", "ihm", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPER", "ART", "NN", "$(", "PDS", "VVFIN", "PIS", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Er nahm; doch leyder nur sich ewig zu beschweren!", "tokens": ["Er", "nahm", ";", "doch", "ley\u00b7der", "nur", "sich", "e\u00b7wig", "zu", "be\u00b7schwe\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "ADV", "ADV", "PRF", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sein allererster schlaf war seine letzte ruh.", "tokens": ["Sein", "al\u00b7le\u00b7rers\u00b7ter", "schlaf", "war", "sei\u00b7ne", "letz\u00b7te", "ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}