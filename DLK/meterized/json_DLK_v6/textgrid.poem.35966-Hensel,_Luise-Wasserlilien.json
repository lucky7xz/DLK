{"textgrid.poem.35966": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "Wasserlilien", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir steigen auf aus feuchten Tiefen,", "tokens": ["Wir", "stei\u00b7gen", "auf", "aus", "feuch\u00b7ten", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da tr\u00e4umten wir so langen Traum", "tokens": ["Da", "tr\u00e4um\u00b7ten", "wir", "so", "lan\u00b7gen", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Umsp\u00fclt von linder Wellen Schaum,", "tokens": ["Um\u00b7sp\u00fclt", "von", "lin\u00b7der", "Wel\u00b7len", "Schaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als uns des Lenzes Stimmen riefen.", "tokens": ["Als", "uns", "des", "Len\u00b7zes", "Stim\u00b7men", "rie\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und manch' ein sinniges Gem\u00fcth", "tokens": ["Und", "man\u00b7ch'", "ein", "sin\u00b7ni\u00b7ges", "Ge\u00b7m\u00fcth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ersieht in uns die sch\u00f6ne Kunde", "tokens": ["Er\u00b7sieht", "in", "uns", "die", "sch\u00f6\u00b7ne", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von dem, was unten Sch\u00f6nes bl\u00fcht,", "tokens": ["Von", "dem", ",", "was", "un\u00b7ten", "Sch\u00f6\u00b7nes", "bl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADV", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo's Fischlein spielt auf Meeresgrunde. \u2013", "tokens": ["Wo's", "Fisc\u00b7hlein", "spielt", "auf", "Mee\u00b7res\u00b7grun\u00b7de", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie ist's hier oben all so bunt \u2013", "tokens": ["Wie", "ist's", "hier", "o\u00b7ben", "all", "so", "bunt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ADV", "PIAT", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch unser Haus ist stiller, milder.", "tokens": ["Doch", "un\u00b7ser", "Haus", "ist", "stil\u00b7ler", ",", "mil\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Lebt wohl, ihr Tages reiche Bilder,", "tokens": ["Lebt", "wohl", ",", "ihr", "Ta\u00b7ges", "rei\u00b7che", "Bil\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir sinken wieder in den Grund.", "tokens": ["Wir", "sin\u00b7ken", "wie\u00b7der", "in", "den", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}