{"textgrid.poem.59566": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du lieber heil'ger frommer Christ,", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du lieber heil'ger frommer Christ,", "tokens": ["Du", "lie\u00b7ber", "heil'\u00b7ger", "from\u00b7mer", "Christ", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der f\u00fcr uns Kinder kommen ist,", "tokens": ["Der", "f\u00fcr", "uns", "Kin\u00b7der", "kom\u00b7men", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "NN", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Damit wir sollen wei\u00df und rein", "tokens": ["Da\u00b7mit", "wir", "sol\u00b7len", "wei\u00df", "und", "rein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und rechte Kinder Gottes sein.", "tokens": ["Und", "rech\u00b7te", "Kin\u00b7der", "Got\u00b7tes", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du Licht, vom lieben Gott gesandt", "tokens": ["Du", "Licht", ",", "vom", "lie\u00b7ben", "Gott", "ge\u00b7sandt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "APPRART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In unser dunkles Erdenland,", "tokens": ["In", "un\u00b7ser", "dunk\u00b7les", "Er\u00b7den\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du Himmelskind und Himmelsschein,", "tokens": ["Du", "Him\u00b7mel\u00b7skind", "und", "Him\u00b7mels\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Damit wir sollen himmlisch sein.", "tokens": ["Da\u00b7mit", "wir", "sol\u00b7len", "himm\u00b7lisch", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du lieber heil'ger frommer Christ,", "tokens": ["Du", "lie\u00b7ber", "heil'\u00b7ger", "from\u00b7mer", "Christ", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil heute dein Geburtstag ist,", "tokens": ["Weil", "heu\u00b7te", "dein", "Ge\u00b7burts\u00b7tag", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum ist auf Erden weit und breit", "tokens": ["Drum", "ist", "auf", "Er\u00b7den", "weit", "und", "breit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "APPR", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei allen Kindern frohe Zeit.", "tokens": ["Bei", "al\u00b7len", "Kin\u00b7dern", "fro\u00b7he", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O segne mich! Ich bin noch klein,", "tokens": ["O", "seg\u00b7ne", "mich", "!", "Ich", "bin", "noch", "klein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O mache mir den Busen rein!", "tokens": ["O", "ma\u00b7che", "mir", "den", "Bu\u00b7sen", "rein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O bade mir die Seele hell", "tokens": ["O", "ba\u00b7de", "mir", "die", "See\u00b7le", "hell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In deinem reichen Himmelsquell!", "tokens": ["In", "dei\u00b7nem", "rei\u00b7chen", "Him\u00b7mels\u00b7quell", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da\u00df ich wie Engel Gottes sei", "tokens": ["Da\u00df", "ich", "wie", "En\u00b7gel", "Got\u00b7tes", "sei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOKOM", "NE", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Demut und in Liebe treu,", "tokens": ["In", "De\u00b7mut", "und", "in", "Lie\u00b7be", "treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich dein bleibe f\u00fcr und f\u00fcr,", "tokens": ["Da\u00df", "ich", "dein", "blei\u00b7be", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "VVFIN", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du Heil'ger Christ, das schenke mir!", "tokens": ["Du", "Heil'\u00b7ger", "Christ", ",", "das", "schen\u00b7ke", "mir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PDS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}