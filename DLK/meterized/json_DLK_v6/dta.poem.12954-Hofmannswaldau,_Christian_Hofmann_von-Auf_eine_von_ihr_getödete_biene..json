{"dta.poem.12954": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine von ihr get\u00f6dete biene.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als unl\u00e4ngst eine zarte biene", "tokens": ["Als", "un\u00b7l\u00e4ngst", "ei\u00b7ne", "zar\u00b7te", "bie\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die sch\u00f6ne Sylvia auf ihre lippen stach,", "tokens": ["Die", "sch\u00f6\u00b7ne", "Syl\u00b7via", "auf", "ih\u00b7re", "lip\u00b7pen", "stach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und nun begriffen war sich weiter zu begeben,", "tokens": ["Und", "nun", "be\u00b7grif\u00b7fen", "war", "sich", "wei\u00b7ter", "zu", "be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "PRF", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So folgt ihr Sylviens erz\u00f6rnter finger nach,", "tokens": ["So", "folgt", "ihr", "Syl\u00b7viens", "er\u00b7z\u00f6rn\u00b7ter", "fin\u00b7ger", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und brachte sie durch einen druck ums leben.", "tokens": ["Und", "brach\u00b7te", "sie", "durch", "ei\u00b7nen", "druck", "ums", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NE", "APPRART", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch allzuwohl vergoltner stich!", "tokens": ["Doch", "all\u00b7zu\u00b7wohl", "ver\u00b7golt\u00b7ner", "stich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wer w\u00e4re gl\u00fccklicher als ich?", "tokens": ["Wer", "w\u00e4\u00b7re", "gl\u00fcck\u00b7li\u00b7cher", "als", "ich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJD", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn ich von ihr dergleichen gunst erw\u00fcrbe,", "tokens": ["Wenn", "ich", "von", "ihr", "derg\u00b7lei\u00b7chen", "gunst", "er\u00b7w\u00fcr\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und also an der th\u00fcr des paradieses st\u00fcrbe.", "tokens": ["Und", "al\u00b7so", "an", "der", "th\u00fcr", "des", "pa\u00b7ra\u00b7die\u00b7ses", "st\u00fcr\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}