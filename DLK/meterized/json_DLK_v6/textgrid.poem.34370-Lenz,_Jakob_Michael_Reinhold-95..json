{"textgrid.poem.34370": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "95.", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willkommen kleine B\u00fcrgerin", "tokens": ["Will\u00b7kom\u00b7men", "klei\u00b7ne", "B\u00fcr\u00b7ge\u00b7rin"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im bunten Thal der L\u00fcgen!", "tokens": ["Im", "bun\u00b7ten", "Thal", "der", "L\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du gehst dahin du L\u00e4chlerin!", "tokens": ["Du", "gehst", "da\u00b7hin", "du", "L\u00e4ch\u00b7le\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich ewig zu betr\u00fcgen.", "tokens": ["Dich", "e\u00b7wig", "zu", "be\u00b7tr\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was weinest du? die Welt ist rund", "tokens": ["Was", "wei\u00b7nest", "du", "?", "die", "Welt", "ist", "rund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$.", "ART", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und nichts darauf best\u00e4ndig.", "tokens": ["Und", "nichts", "da\u00b7rauf", "be\u00b7st\u00e4n\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PAV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Weinen nur ist ungesund", "tokens": ["Das", "Wei\u00b7nen", "nur", "ist", "un\u00b7ge\u00b7sund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und der Verlust nothwendig.", "tokens": ["Und", "der", "Ver\u00b7lust", "noth\u00b7wen\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Einst wirst du, kleine L\u00e4chlerin!", "tokens": ["Einst", "wirst", "du", ",", "klei\u00b7ne", "L\u00e4ch\u00b7le\u00b7rin", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit s\u00fcsserm Schmerze weinen,", "tokens": ["Mit", "s\u00fcs\u00b7serm", "Schmer\u00b7ze", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn alle deinen treuen Sinn", "tokens": ["Wenn", "al\u00b7le", "dei\u00b7nen", "treu\u00b7en", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gott! zu verkennen scheinen.", "tokens": ["Gott", "!", "zu", "ver\u00b7ken\u00b7nen", "schei\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Dann wirst du stehn auf deinem Werth", "tokens": ["Dann", "wirst", "du", "stehn", "auf", "dei\u00b7nem", "Werth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und blicken, wie die Sonne", "tokens": ["Und", "bli\u00b7cken", ",", "wie", "die", "Son\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von der ein jeder weg sich kehrt", "tokens": ["Von", "der", "ein", "je\u00b7der", "weg", "sich", "kehrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "PIS", "ADV", "PRF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu blind f\u00fcr ihre Wonne.", "tokens": ["Zu", "blind", "f\u00fcr", "ih\u00b7re", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Bis da\u00df der Adler kommen wird", "tokens": ["Bis", "da\u00df", "der", "Ad\u00b7ler", "kom\u00b7men", "wird"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "ART", "NN", "VVINF", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus f\u00fcrchterlichen B\u00fcschen,", "tokens": ["Aus", "f\u00fcrch\u00b7ter\u00b7li\u00b7chen", "B\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Welten ohne Trost durchirrt \u2013", "tokens": ["Der", "Wel\u00b7ten", "oh\u00b7ne", "Trost", "durch\u00b7irrt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie wirst du ihn erfrischen!", "tokens": ["Wie", "wirst", "du", "ihn", "er\u00b7fri\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}