{"dta.poem.16226": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "280.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gar manche Schale mu\u00df von deinem Ich sich l\u00f6sen,", "tokens": ["Gar", "man\u00b7che", "Scha\u00b7le", "mu\u00df", "von", "dei\u00b7nem", "Ich", "sich", "l\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VMFIN", "APPR", "PPOSAT", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zuf\u00e4llig Irdisches, und mancher Rost des B\u00f6sen.", "tokens": ["Zu\u00b7f\u00e4l\u00b7lig", "Ir\u00b7di\u00b7sches", ",", "und", "man\u00b7cher", "Rost", "des", "B\u00f6\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "$,", "KON", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch w\u00e4hrend immermehr dein Ich sich also reinigt,", "tokens": ["Doch", "w\u00e4h\u00b7rend", "im\u00b7mer\u00b7mehr", "dein", "Ich", "sich", "al\u00b7so", "rei\u00b7nigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PPOSAT", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird immer mehr mit ihm des Neuen auch vereinigt.", "tokens": ["Wird", "im\u00b7mer", "mehr", "mit", "ihm", "des", "Neu\u00b7en", "auch", "ver\u00b7ei\u00b7nigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "APPR", "PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du strebest Tag f\u00fcr Tag durch Lernen wie durch Lehren,", "tokens": ["Du", "stre\u00b7best", "Tag", "f\u00fcr", "Tag", "durch", "Ler\u00b7nen", "wie", "durch", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "APPR", "NN", "KOKOM", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch Denken wie durch Thun, den Kern des Ichs zu mehren.", "tokens": ["Durch", "Den\u00b7ken", "wie", "durch", "Thun", ",", "den", "Kern", "des", "Ichs", "zu", "meh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KOKOM", "APPR", "NN", "$,", "ART", "NN", "ART", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Edelstein bedarf viel Mittel, sich zu schleifen;", "tokens": ["Der", "E\u00b7del\u00b7stein", "be\u00b7darf", "viel", "Mit\u00b7tel", ",", "sich", "zu", "schlei\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$,", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Viel Nahrungsmittel braucht der Saamen, um zu reifen.", "tokens": ["Viel", "Nah\u00b7rungs\u00b7mit\u00b7tel", "braucht", "der", "Saa\u00b7men", ",", "um", "zu", "rei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$,", "KOUI", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wer kann zuletzt mit Lust im fert'gen Ich beruhn?", "tokens": ["Wer", "kann", "zu\u00b7letzt", "mit", "Lust", "im", "fert'\u00b7gen", "Ich", "be\u00b7ruhn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "APPR", "NN", "APPRART", "ADJA", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer nichts hinzuthut, was er wieder weg mu\u00df thun.", "tokens": ["Wer", "nichts", "hin\u00b7zu\u00b7thut", ",", "was", "er", "wie\u00b7der", "weg", "mu\u00df", "thun", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "$,", "PWS", "PPER", "ADV", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}