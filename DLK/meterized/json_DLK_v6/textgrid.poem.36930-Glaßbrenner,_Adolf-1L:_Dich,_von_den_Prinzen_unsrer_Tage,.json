{"textgrid.poem.36930": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dich, von den Prinzen unsrer Tage,", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich, von den Prinzen unsrer Tage,", "tokens": ["Dich", ",", "von", "den", "Prin\u00b7zen", "uns\u00b7rer", "Ta\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der edelste vielleicht,", "tokens": ["Der", "e\u00b7dels\u00b7te", "viel\u00b7leicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "$,"], "meter": "-+---+", "measure": "dactylic.init"}, "line.3": {"text": "Dich, o gerecht ist meine Klage!", "tokens": ["Dich", ",", "o", "ge\u00b7recht", "ist", "mei\u00b7ne", "Kla\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hat schon der Tod erreicht!", "tokens": ["Hat", "schon", "der", "Tod", "er\u00b7reicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dich, von Europa's Prinzen allen,", "tokens": ["Dich", ",", "von", "Eu\u00b7ro\u00b7pa's", "Prin\u00b7zen", "al\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "NE", "NN", "PIAT", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Dich einzig fa\u00dft er an:", "tokens": ["Dich", "ein\u00b7zig", "fa\u00dft", "er", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So Viel' sind auf den Kopf gefallen,", "tokens": ["So", "Viel'", "sind", "auf", "den", "Kopf", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch Keiner starb daran!", "tokens": ["Doch", "Kei\u00b7ner", "starb", "da\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}