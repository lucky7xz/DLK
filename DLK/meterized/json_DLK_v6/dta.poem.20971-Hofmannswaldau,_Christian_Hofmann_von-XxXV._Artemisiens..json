{"dta.poem.20971": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XxXV.  \n Artemisiens.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Wer Land und See durchstreicht/ und diese grosse", "tokens": ["Wer", "Land", "und", "See", "durch\u00b7streicht", "/", "und", "die\u00b7se", "gros\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "NN", "KON", "NN", "VVPP", "$(", "KON", "PDAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Welt/", "tokens": ["Welt", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Jhm als ein grosses Buch vor das Gesichte h\u00e4lt/", "tokens": ["Jhm", "als", "ein", "gros\u00b7ses", "Buch", "vor", "das", "Ge\u00b7sich\u00b7te", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der mu\u00df nicht wie man pflegt alhier vor\u00fcber drabe\u0303/", "tokens": ["Der", "mu\u00df", "nicht", "wie", "man", "pflegt", "al\u00b7hier", "vor\u00b7\u00fc\u00b7ber", "drab\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PTKNEG", "PWAV", "PIS", "VVFIN", "ADV", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In diesem Grabe liegt ein ander Grab begraben.", "tokens": ["In", "die\u00b7sem", "Gra\u00b7be", "liegt", "ein", "an\u00b7d