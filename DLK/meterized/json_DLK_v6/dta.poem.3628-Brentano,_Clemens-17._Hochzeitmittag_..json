{"dta.poem.3628": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "17.  Hochzeitmittag .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn die Seele sich befindet               ", "tokens": ["Wenn", "die", "See\u00b7le", "sich", "be\u00b7fin\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PRF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In des Br\u00e4utgams Keller stehn,", "tokens": ["In", "des", "Br\u00e4ut\u00b7gams", "Kel\u00b7ler", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird sie als vom Wein entz\u00fcndet,", "tokens": ["Wird", "sie", "als", "vom", "Wein", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KOKOM", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jauchzet voll einherzugehn,", "tokens": ["Jauch\u00b7zet", "voll", "ein\u00b7her\u00b7zu\u00b7gehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVIZU", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ihr Leib und ganzer Geist", "tokens": ["Da\u00df", "ihr", "Leib", "und", "gan\u00b7zer", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Trunken und entz\u00fccket hei\u00dft.", "tokens": ["Trun\u00b7ken", "und", "ent\u00b7z\u00fc\u00b7cket", "hei\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Alsdann wird sie aufgezogen,", "tokens": ["Als\u00b7dann", "wird", "sie", "auf\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und in stille Luft gef\u00fchrt,", "tokens": ["Und", "in", "stil\u00b7le", "Luft", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus den wilden Meereswogen,", "tokens": ["Aus", "den", "wil\u00b7den", "Mee\u00b7res\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus den Dingen, die sie sp\u00fcrt.", "tokens": ["Aus", "den", "Din\u00b7gen", ",", "die", "sie", "sp\u00fcrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unertr\u00e4glich leer zu seyn,", "tokens": ["Un\u00b7er\u00b7tr\u00e4g\u00b7lich", "leer", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn die Sinnen dringen ein.", "tokens": ["Wenn", "die", "Sin\u00b7nen", "drin\u00b7gen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Alles liegt zu ihren F\u00fcssen,", "tokens": ["Al\u00b7les", "liegt", "zu", "ih\u00b7ren", "F\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was zu dieser Welt geh\u00f6rt,", "tokens": ["Was", "zu", "die\u00b7ser", "Welt", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja sie kann auch leichtlich missen,", "tokens": ["Ja", "sie", "kann", "auch", "leicht\u00b7lich", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VMFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was durch guten Schein beth\u00f6rt;", "tokens": ["Was", "durch", "gu\u00b7ten", "Schein", "be\u00b7th\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn sie hat den klugen Geist,", "tokens": ["Denn", "sie", "hat", "den", "klu\u00b7gen", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der ihr bessre G\u00fcter weist.", "tokens": ["Der", "ihr", "bess\u00b7re", "G\u00fc\u00b7ter", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie ein Trunkner liegt sie stille,", "tokens": ["Wie", "ein", "Trunk\u00b7ner", "liegt", "sie", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der wie unempfindlich scheint,", "tokens": ["Der", "wie", "un\u00b7emp\u00b7find\u00b7lich", "scheint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df der sonst zertheilte Wille", "tokens": ["Da\u00df", "der", "sonst", "zer\u00b7theil\u00b7te", "Wil\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aufgeopfert nicht mehr meint,", "tokens": ["Auf\u00b7ge\u00b7o\u00b7pfert", "nicht", "mehr", "meint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als nur Gott und seine Kraft,", "tokens": ["Als", "nur", "Gott", "und", "sei\u00b7ne", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die den Sohn der Liebe schafft.", "tokens": ["Die", "den", "Sohn", "der", "Lie\u00b7be", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}