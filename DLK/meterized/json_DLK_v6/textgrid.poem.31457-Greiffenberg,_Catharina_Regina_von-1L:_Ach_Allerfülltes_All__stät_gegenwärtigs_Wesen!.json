{"textgrid.poem.31457": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach Allerf\u00fclltes All / st\u00e4t gegenw\u00e4rtigs Wesen!", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach Allerf\u00fclltes All / st\u00e4t gegenw\u00e4rtigs Wesen!", "tokens": ["Ach", "Al\u00b7ler\u00b7f\u00fcll\u00b7tes", "All", "/", "st\u00e4t", "ge\u00b7gen\u00b7w\u00e4r\u00b7tigs", "We\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$(", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "was w\u00fcnsch' ich Ziel und Ort? du aller Orten bist.", "tokens": ["was", "w\u00fcn\u00b7sch'", "ich", "Ziel", "und", "Ort", "?", "du", "al\u00b7ler", "Or\u00b7ten", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "KON", "NN", "$.", "PPER", "PIAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Vmsonst mir von der Welt dis Gl\u00fcck verst\u00f6ret ist.", "tokens": ["Vm\u00b7sonst", "mir", "von", "der", "Welt", "dis", "Gl\u00fcck", "ver\u00b7st\u00f6\u00b7ret", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "PDS", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "H\u00f6r' ich dich reden nicht: kan ich dein wort doch lesen.", "tokens": ["H\u00f6r'", "ich", "dich", "re\u00b7den", "nicht", ":", "kan", "ich", "dein", "wort", "doch", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "VVINF", "PTKNEG", "$.", "VMFIN", "PPER", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "+--+-++--+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Ey was! du predtgst selbst / und machst die Seel genesen.", "tokens": ["Ey", "was", "!", "du", "predt\u00b7gst", "selbst", "/", "und", "machst", "die", "Seel", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "$.", "PPER", "VVFIN", "ADV", "$(", "KON", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mein Herz zum Predigtstul und Kirchlein ist erkiest.", "tokens": ["Mein", "Herz", "zum", "Pre\u00b7digt\u00b7stul", "und", "Kirch\u00b7lein", "ist", "er\u00b7kiest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "KON", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Trotz / Welt und Teuffel / wehr / mit aller deiner List!", "tokens": ["Trotz", "/", "Welt", "und", "Teuf\u00b7fel", "/", "wehr", "/", "mit", "al\u00b7ler", "dei\u00b7ner", "List", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$(", "NN", "KON", "NN", "$(", "ADV", "$(", "APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "der H\u00f6chste macht das b\u00e4st' aus deinem gifftigstb\u00f6sen.", "tokens": ["der", "H\u00f6chs\u00b7te", "macht", "das", "b\u00e4st'", "aus", "dei\u00b7nem", "giff\u00b7tigst\u00b7b\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "g\u00f6nnst mir den Schatten nicht / so gibt er gar den Schein.", "tokens": ["g\u00f6nnst", "mir", "den", "Schat\u00b7ten", "nicht", "/", "so", "gibt", "er", "gar", "den", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "$(", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.10": {"text": "Dunkt dich der Stern zu viel / so schenkt er gar die Sonne.", "tokens": ["Dunkt", "dich", "der", "Stern", "zu", "viel", "/", "so", "schenkt", "er", "gar", "die", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PIAT", "$(", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Fort / fort / entsagter Feind / mir kleines Gl\u00fcck mi\u00dfgonne!", "tokens": ["Fort", "/", "fort", "/", "ent\u00b7sag\u00b7ter", "Feind", "/", "mir", "klei\u00b7nes", "Gl\u00fcck", "mi\u00df\u00b7gon\u00b7ne", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PTKVZ", "$(", "ADJA", "NN", "$(", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "beneid' ein Tr\u00f6pflein so: schenkt Gott ein Schalen ein.", "tokens": ["be\u00b7neid'", "ein", "Tr\u00f6pf\u00b7lein", "so", ":", "schenkt", "Gott", "ein", "Scha\u00b7len", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$.", "VVFIN", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Gott l\u00e4st kein Gl\u00fcck zu r\u00fcck' / ohn reich' erstattung / gehen.", "tokens": ["Gott", "l\u00e4st", "kein", "Gl\u00fcck", "zu", "r\u00fcck", "/", "ohn", "reich'", "er\u00b7stat\u00b7tung", "/", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "APPR", "NE", "$(", "APPR", "ADJA", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das st\u00fcrmen / so mich irrt / mu\u00df mich in Hafen wehen.", "tokens": ["Das", "st\u00fcr\u00b7men", "/", "so", "mich", "irrt", "/", "mu\u00df", "mich", "in", "Ha\u00b7fen", "we\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "ADV", "PPER", "VVFIN", "$(", "VMFIN", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}