{"textgrid.poem.56487": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr, denen ward das Blut vor Trauer bleich,", "tokens": ["Ihr", ",", "de\u00b7nen", "ward", "das", "Blut", "vor", "Trau\u00b7er", "bleich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PDS", "VAFIN", "ART", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr, die der Sturm der Qualen stets durchrast,", "tokens": ["Ihr", ",", "die", "der", "Sturm", "der", "Qua\u00b7len", "stets", "durc\u00b7hrast", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr, deren Stirn der Lasten weites Reich,", "tokens": ["Ihr", ",", "de\u00b7ren", "Stirn", "der", "Las\u00b7ten", "wei\u00b7tes", "Reich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihr, deren Auge Kummer schon verglast,", "tokens": ["Ihr", ",", "de\u00b7ren", "Au\u00b7ge", "Kum\u00b7mer", "schon", "ver\u00b7glast", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ihr, denen auf der jungen Schl\u00e4fe brennt", "tokens": ["Ihr", ",", "de\u00b7nen", "auf", "der", "jun\u00b7gen", "Schl\u00e4\u00b7fe", "brennt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie Aussatz schon das gro\u00dfe Totenmal,", "tokens": ["Wie", "Aus\u00b7satz", "schon", "das", "gro\u00b7\u00dfe", "To\u00b7ten\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Tretet heran, empfangt das Sakrament", "tokens": ["Tre\u00b7tet", "he\u00b7ran", ",", "emp\u00b7fangt", "das", "Sak\u00b7ra\u00b7ment"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Verfluchter Hostien in dem Haus der Qual.", "tokens": ["Ver\u00b7fluch\u00b7ter", "Hos\u00b7ti\u00b7en", "in", "dem", "Haus", "der", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Besteigt die Br\u00fccke auf dem schwarzen Flu\u00df,", "tokens": ["Be\u00b7steigt", "die", "Br\u00fc\u00b7cke", "auf", "dem", "schwar\u00b7zen", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dar\u00fcber wallet der Verfluchten Schar.", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "wal\u00b7let", "der", "Ver\u00b7fluch\u00b7ten", "Schar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und dunkel gr\u00fc\u00dft euch gro\u00df der Portikus,", "tokens": ["Und", "dun\u00b7kel", "gr\u00fc\u00dft", "euch", "gro\u00df", "der", "Por\u00b7ti\u00b7kus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durch den in D\u00e4mmrung gl\u00e4nzt der Hochaltar,", "tokens": ["Durch", "den", "in", "D\u00e4mm\u00b7rung", "gl\u00e4nzt", "der", "Hoc\u00b7hal\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Den tausend Kerzen schm\u00fccken, die von Blut", "tokens": ["Den", "tau\u00b7send", "Ker\u00b7zen", "schm\u00fc\u00b7cken", ",", "die", "von", "Blut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "CARD", "NN", "VVINF", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Fett der Ungebornen sind gedreht.", "tokens": ["Und", "Fett", "der", "Un\u00b7ge\u00b7bor\u00b7nen", "sind", "ge\u00b7dreht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo Knochen h\u00e4ngen, und der rote Sud", "tokens": ["Wo", "Kno\u00b7chen", "h\u00e4n\u00b7gen", ",", "und", "der", "ro\u00b7te", "Sud"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVINF", "$,", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Teuflischen Weihrauchs euch entgegenweht.", "tokens": ["Teuf\u00b7li\u00b7schen", "Weih\u00b7rauchs", "euch", "ent\u00b7ge\u00b7gen\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Wo Priester in der h\u00f6llischen Soutane", "tokens": ["Wo", "Pries\u00b7ter", "in", "der", "h\u00f6l\u00b7li\u00b7schen", "Sou\u00b7ta\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Reihen knien, zu hellem Me\u00dfgel\u00e4ut,", "tokens": ["In", "Rei\u00b7hen", "kni\u00b7en", ",", "zu", "hel\u00b7lem", "Me\u00df\u00b7ge\u00b7l\u00e4ut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+----+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Wo von den Kanzeln Fahne \u00fcber Fahne", "tokens": ["Wo", "von", "den", "Kan\u00b7zeln", "Fah\u00b7ne", "\u00fc\u00b7ber", "Fah\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie rote H\u00f6llenflamme euch bedr\u00e4ut.", "tokens": ["Wie", "ro\u00b7te", "H\u00f6l\u00b7len\u00b7flam\u00b7me", "euch", "be\u00b7dr\u00e4ut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ein nackter Abt bl\u00e4ht vor dem G\u00f6tterbild", "tokens": ["Ein", "nack\u00b7ter", "Abt", "bl\u00e4ht", "vor", "dem", "G\u00f6t\u00b7ter\u00b7bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den feisten Bauch, da er die Messe singt.", "tokens": ["Den", "feis\u00b7ten", "Bauch", ",", "da", "er", "die", "Mes\u00b7se", "singt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er greift den Kelch, mit rotem Blut gef\u00fcllt,", "tokens": ["Er", "greift", "den", "Kelch", ",", "mit", "ro\u00b7tem", "Blut", "ge\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den hoch er auf das Haupt der Menge schwingt.", "tokens": ["Den", "hoch", "er", "auf", "das", "Haupt", "der", "Men\u00b7ge", "schwingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "\u00bbtrinket mein Blut.\u00ab Er trinkt den Becher leer,", "tokens": ["\u00bb", "trin\u00b7ket", "mein", "Blut", ".", "\u00ab", "Er", "trinkt", "den", "Be\u00b7cher", "leer", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPOSAT", "NN", "$.", "$(", "PPER", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Der in sein Herz wie rote Lava quillt.", "tokens": ["Der", "in", "sein", "Herz", "wie", "ro\u00b7te", "La\u00b7va", "quillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "KOKOM", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sein Gaumen leuchtet wie ein rotes Meer,", "tokens": ["Sein", "Gau\u00b7men", "leuch\u00b7tet", "wie", "ein", "ro\u00b7tes", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der von dem Glanz des G\u00f6tterblutes schwillt.", "tokens": ["Der", "von", "dem", "Glanz", "des", "G\u00f6t\u00b7ter\u00b7blu\u00b7tes", "schwillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Auf euren Schl\u00e4fen, wo der Horst der Qual,", "tokens": ["Auf", "eu\u00b7ren", "Schl\u00e4\u00b7fen", ",", "wo", "der", "Horst", "der", "Qual", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die schwarze Bastion der H\u00f6lle droht,", "tokens": ["Die", "schwar\u00b7ze", "Bas\u00b7ti\u00b7on", "der", "H\u00f6l\u00b7le", "droht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Springt eine Flamme auf, die spitz und schmal", "tokens": ["Springt", "ei\u00b7ne", "Flam\u00b7me", "auf", ",", "die", "spitz", "und", "schmal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie der Skorpione schwarze Zunge loht.", "tokens": ["Wie", "der", "Skor\u00b7pi\u00b7o\u00b7ne", "schwar\u00b7ze", "Zun\u00b7ge", "loht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.9": {"line.1": {"text": "Nachtschwarze Wolken dr\u00e4ngen in den Dom", "tokens": ["Nacht\u00b7schwar\u00b7ze", "Wol\u00b7ken", "dr\u00e4n\u00b7gen", "in", "den", "Dom"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Voll Sturm und Blitzen durch das gro\u00dfe Tor.", "tokens": ["Voll", "Sturm", "und", "Blit\u00b7zen", "durch", "das", "gro\u00b7\u00dfe", "Tor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Wetter tost. Im schwarzen Regenstrom", "tokens": ["Ein", "Wet\u00b7ter", "tost", ".", "Im", "schwar\u00b7zen", "Re\u00b7gen\u00b7strom"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Versinkt der Orgel Ton im fernen Chor.", "tokens": ["Ver\u00b7sinkt", "der", "Or\u00b7gel", "Ton", "im", "fer\u00b7nen", "Chor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Die Gr\u00e4ber springen auf. Der Toten Hand", "tokens": ["Die", "Gr\u00e4\u00b7ber", "sprin\u00b7gen", "auf", ".", "Der", "To\u00b7ten", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Streckt wei\u00df und kalt die Knochenfinger aus.", "tokens": ["Streckt", "wei\u00df", "und", "kalt", "die", "Kno\u00b7chen\u00b7fin\u00b7ger", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie winken euch aus ihrem dunklen Land.", "tokens": ["Sie", "win\u00b7ken", "euch", "aus", "ih\u00b7rem", "dunk\u00b7len", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ihr Geschrei erf\u00fcllt das Riesenhaus.", "tokens": ["Und", "ihr", "Ge\u00b7schrei", "er\u00b7f\u00fcllt", "das", "Rie\u00b7sen\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Die Fliesen brechen auf. Und Lethe braust", "tokens": ["Die", "Flie\u00b7sen", "bre\u00b7chen", "auf", ".", "Und", "Le\u00b7the", "braust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "KON", "NE", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Tief unten \u00fcber einen Wasserfall.", "tokens": ["Tief", "un\u00b7ten", "\u00fc\u00b7ber", "ei\u00b7nen", "Was\u00b7ser\u00b7fall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Abgrund schwindelt Meilen tief und saust", "tokens": ["Der", "Ab\u00b7grund", "schwin\u00b7delt", "Mei\u00b7len", "tief", "und", "saust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Voll ungeheurer St\u00fcrme weitem Hall.", "tokens": ["Voll", "un\u00b7ge\u00b7heu\u00b7rer", "St\u00fcr\u00b7me", "wei\u00b7tem", "Hall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Die H\u00f6llens\u00f6hne fahren ihn herab", "tokens": ["Die", "H\u00f6l\u00b7len\u00b7s\u00f6h\u00b7ne", "fah\u00b7ren", "ihn", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit schwarzem Takelwerk durch den Typhon.", "tokens": ["Mit", "schwar\u00b7zem", "Ta\u00b7kel\u00b7werk", "durch", "den", "Ty\u00b7phon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie schauen singend in das weite Grab", "tokens": ["Sie", "schau\u00b7en", "sin\u00b7gend", "in", "das", "wei\u00b7te", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vom Totenkopfe ihrer Schiffs-Galion.", "tokens": ["Vom", "To\u00b7ten\u00b7kop\u00b7fe", "ih\u00b7rer", "Schiffs\u00b7Ga\u00b7li\u00b7on", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}