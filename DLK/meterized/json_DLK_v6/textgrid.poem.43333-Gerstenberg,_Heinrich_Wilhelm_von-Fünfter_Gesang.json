{"textgrid.poem.43333": {"metadata": {"author": {"name": "Gerstenberg, Heinrich Wilhelm von", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcnfter Gesang", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie sind gefallen, die G\u00f6tter, gefallen!", "tokens": ["Sie", "sind", "ge\u00b7fal\u00b7len", ",", "die", "G\u00f6t\u00b7ter", ",", "ge\u00b7fal\u00b7len", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ART", "NN", "$,", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "La\u00dfts Erd und Himmel wiederhallen!", "tokens": ["La\u00dfts", "Erd", "und", "Him\u00b7mel", "wie\u00b7der\u00b7hal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sind gefallen! gefallen! gefallen!", "tokens": ["Sie", "sind", "ge\u00b7fal\u00b7len", "!", "ge\u00b7fal\u00b7len", "!", "ge\u00b7fal\u00b7len", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "VVPP", "$.", "VVPP", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Hrymur fuhr, auf sieben Donner-Wagen", "tokens": ["Hry\u00b7mur", "fuhr", ",", "auf", "sie\u00b7ben", "Don\u00b7ner\u00b7Wa\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "APPR", "CARD", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Vom Aufgang herunter getragen!", "tokens": ["Vom", "Auf\u00b7gang", "her\u00b7un\u00b7ter", "ge\u00b7tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APZR", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Da w\u00e4lzte sich der Ocean!", "tokens": ["Da", "w\u00e4lz\u00b7te", "sich", "der", "O\u00b7cean", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Da w\u00e4lzte Jormungandur in Blut", "tokens": ["Da", "w\u00e4lz\u00b7te", "Jor\u00b7mun\u00b7gan\u00b7dur", "in", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "APPR", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Mit schreckenvoller Wuth", "tokens": ["Mit", "schre\u00b7cken\u00b7vol\u00b7ler", "Wuth"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Sich auf der Wogen sch\u00e4umender Bahn!", "tokens": ["Sich", "auf", "der", "Wo\u00b7gen", "sch\u00e4u\u00b7men\u00b7der", "Bahn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Der Adler t\u00f6nt', und zerri\u00df die Leiche!", "tokens": ["Der", "Ad\u00b7ler", "t\u00f6nt'", ",", "und", "zer\u00b7ri\u00df", "die", "Lei\u00b7che", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Und Naglfahr scheitert, das Geb\u00e4u der Eiche!", "tokens": ["Und", "Nagl\u00b7fahr", "schei\u00b7tert", ",", "das", "Ge\u00b7b\u00e4u", "der", "Ei\u00b7che", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Woher der Untergang der Asen?", "tokens": ["Wo\u00b7her", "der", "Un\u00b7ter\u00b7gang", "der", "A\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Wer hat die Alfen wie Spreu hinweggeblasen?", "tokens": ["Wer", "hat", "die", "Al\u00b7fen", "wie", "Spreu", "hin\u00b7weg\u00b7ge\u00b7bla\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "KOKOM", "NN", "VVPP", "$."], "meter": "-+----+-+-+-", "measure": "dactylic.init"}, "line.14": {"text": "Vom Krachen heult die Riesenwelt!", "tokens": ["Vom", "Kra\u00b7chen", "heult", "die", "Rie\u00b7sen\u00b7welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Des Himmels Tr\u00fcmmer sind ein Waffenfeld!", "tokens": ["Des", "Him\u00b7mels", "Tr\u00fcm\u00b7mer", "sind", "ein", "Waf\u00b7fen\u00b7feld", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Die Zwerge seufzen vor den Thoren,", "tokens": ["Die", "Zwer\u00b7ge", "seuf\u00b7zen", "vor", "den", "Tho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "In z\u00e4hneklappernde Schrecken verlohren!", "tokens": ["In", "z\u00e4h\u00b7ne\u00b7klap\u00b7pern\u00b7de", "Schre\u00b7cken", "ver\u00b7loh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Das Sonnenschwert des R\u00e4chers blitzt", "tokens": ["Das", "Son\u00b7nen\u00b7schwert", "des", "R\u00e4\u00b7chers", "blitzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Auf Riesenweiber, die im Fliehn", "tokens": ["Auf", "Rie\u00b7sen\u00b7wei\u00b7ber", ",", "die", "im", "Fliehn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Sich hinter einer Wolke Ruin", "tokens": ["Sich", "hin\u00b7ter", "ei\u00b7ner", "Wol\u00b7ke", "Ru\u00b7in"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Vergebens, vergebens gesch\u00fctzt!", "tokens": ["Ver\u00b7ge\u00b7bens", ",", "ver\u00b7ge\u00b7bens", "ge\u00b7sch\u00fctzt", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.22": {"text": "Da wankte, da erzitterte Hlin,", "tokens": ["Da", "wank\u00b7te", ",", "da", "er\u00b7zit\u00b7ter\u00b7te", "Hlin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.23": {"text": "Und rang die H\u00e4nde noch einmal!", "tokens": ["Und", "rang", "die", "H\u00e4n\u00b7de", "noch", "ein\u00b7mal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Vergebens verletzt der Sohn des Odin", "tokens": ["Ver\u00b7ge\u00b7bens", "ver\u00b7letzt", "der", "Sohn", "des", "O\u00b7din"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "ART", "NN", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Das Ungeheuer mit triefendem Stahl!", "tokens": ["Das", "Un\u00b7ge\u00b7heu\u00b7er", "mit", "trie\u00b7fen\u00b7dem", "Stahl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Vergebens w\u00fcrgt auf seinem Riesengange", "tokens": ["Ver\u00b7ge\u00b7bens", "w\u00fcrgt", "auf", "sei\u00b7nem", "Rie\u00b7sen\u00b7gan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Der Helden-Same des Hlodin", "tokens": ["Der", "Hel\u00b7den\u00b7Sa\u00b7me", "des", "Hlo\u00b7din"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.28": {"text": "Den Zwillingswolf, und die Midgardische Schlange!", "tokens": ["Den", "Zwil\u00b7lings\u00b7wolf", ",", "und", "die", "Mid\u00b7gar\u00b7di\u00b7sche", "Schlan\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "$,", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Sie alle, die G\u00f6tter, die Helden, sie alle", "tokens": ["Sie", "al\u00b7le", ",", "die", "G\u00f6t\u00b7ter", ",", "die", "Hel\u00b7den", ",", "sie", "al\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "PIS", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "PPER", "PIAT"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.30": {"text": "Sind hingegossen dem Falle", "tokens": ["Sind", "hin\u00b7ge\u00b7gos\u00b7sen", "dem", "Fal\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.31": {"text": "Furchtbar billt aus dampfender Grotte", "tokens": ["Furcht\u00b7bar", "billt", "aus", "damp\u00b7fen\u00b7der", "Grot\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.32": {"text": "Mit weit ge\u00f6ffnetem Schlund", "tokens": ["Mit", "weit", "ge\u00b7\u00f6ff\u00b7ne\u00b7tem", "Schlund"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.33": {"text": "Hinter dem fallenden Gotte", "tokens": ["Hin\u00b7ter", "dem", "fal\u00b7len\u00b7den", "Got\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.34": {"text": "Garm der H\u00f6llenhund!", "tokens": ["Garm", "der", "H\u00f6l\u00b7len\u00b7hund", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.35": {"text": "Mit schwarzem Antlitz entsteigt die Sonne dem Dunkeln,", "tokens": ["Mit", "schwar\u00b7zem", "Ant\u00b7litz", "ent\u00b7steigt", "die", "Son\u00b7ne", "dem", "Dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "ADJA", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "Und Sterne h\u00f6ren auf zu funkeln!", "tokens": ["Und", "Ster\u00b7ne", "h\u00f6\u00b7ren", "auf", "zu", "fun\u00b7keln", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Da w\u00fcten Meere, flammende Berge w\u00fcten,", "tokens": ["Da", "w\u00fc\u00b7ten", "Mee\u00b7re", ",", "flam\u00b7men\u00b7de", "Ber\u00b7ge", "w\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.38": {"text": "Wo ihre Fackeln gl\u00fchten! \u2013", "tokens": ["Wo", "ih\u00b7re", "Fa\u00b7ckeln", "gl\u00fch\u00b7ten", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "In neue Gegenden entr\u00fcckt", "tokens": ["In", "neu\u00b7e", "Ge\u00b7gen\u00b7den", "ent\u00b7r\u00fcckt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.40": {"text": "Schaut mein begeistertes Aug umher \u2013 erblickt", "tokens": ["Schaut", "mein", "be\u00b7geis\u00b7ter\u00b7tes", "Aug", "um\u00b7her", "\u2013", "er\u00b7blickt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$(", "VVPP"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.41": {"text": "Den Abglanz h\u00f6hrer Gottheit, ihre Welt,", "tokens": ["Den", "Ab\u00b7glanz", "h\u00f6h\u00b7rer", "Got\u00b7theit", ",", "ih\u00b7re", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Und diese Himmel, ihr Gezelt!", "tokens": ["Und", "die\u00b7se", "Him\u00b7mel", ",", "ihr", "Ge\u00b7zelt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Mein schwacher Geist, in Staub gebeugt,", "tokens": ["Mein", "schwa\u00b7cher", "Geist", ",", "in", "Staub", "ge\u00b7beugt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Fa\u00dft ihre Wunder nicht, und schweigt.", "tokens": ["Fa\u00dft", "ih\u00b7re", "Wun\u00b7der", "nicht", ",", "und", "schweigt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}