{"textgrid.poem.27996": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df nicht Justinian uns allewege zeiget,", "tokens": ["Da\u00df", "nicht", "Jus\u00b7ti\u00b7ni\u00b7an", "uns", "al\u00b7le\u00b7we\u00b7ge", "zei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "NE", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Wodurch man was erwirbt und viel davon verschweiget,", "tokens": ["Wo\u00b7durch", "man", "was", "er\u00b7wirbt", "und", "viel", "da\u00b7von", "ver\u00b7schwei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PWS", "VVFIN", "KON", "ADV", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Geschah vielleicht au\u00df Neid, vielleicht au\u00df unbewust,", "tokens": ["Ge\u00b7schah", "viel\u00b7leicht", "au\u00df", "Neid", ",", "viel\u00b7leicht", "au\u00df", "un\u00b7be\u00b7wust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$,", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vielleicht au\u00df \u00dcberdru\u00df, dieweil es M\u00fch gekost.", "tokens": ["Viel\u00b7leicht", "au\u00df", "\u00dc\u00b7berd\u00b7ru\u00df", ",", "die\u00b7weil", "es", "M\u00fch", "ge\u00b7kost", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "KOUS", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mars aber ist so treu, so klug, so unverdrossen", "tokens": ["Mars", "a\u00b7ber", "ist", "so", "treu", ",", "so", "klug", ",", "so", "un\u00b7ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADV", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zu \u00f6ffnen alles das, was sonsten heist verschlossen;", "tokens": ["Zu", "\u00f6ff\u00b7nen", "al\u00b7les", "das", ",", "was", "sons\u00b7ten", "heist", "ver\u00b7schlos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PIS", "PDS", "$,", "PRELS", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er suchet alles au\u00df, er weiset allen Grieff", "tokens": ["Er", "su\u00b7chet", "al\u00b7les", "au\u00df", ",", "er", "wei\u00b7set", "al\u00b7len", "Grieff"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "PTKVZ", "$,", "PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Zu n\u00e4hren sich bey Tag, und wann man sonsten schlieff.", "tokens": ["Zu", "n\u00e4h\u00b7ren", "sich", "bey", "Tag", ",", "und", "wann", "man", "sons\u00b7ten", "schlieff", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PRF", "APPR", "NN", "$,", "KON", "PWAV", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}