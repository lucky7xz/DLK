{"textgrid.poem.41693": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem Kinde, das entz\u00fcckt von jedem Stich und Bilde,", "tokens": ["Dem", "Kin\u00b7de", ",", "das", "ent\u00b7z\u00fcckt", "von", "je\u00b7dem", "Stich", "und", "Bil\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erscheint die Erde weit wie seine Tr\u00e4umerein.", "tokens": ["Er\u00b7scheint", "die", "Er\u00b7de", "weit", "wie", "sei\u00b7ne", "Tr\u00e4u\u00b7me\u00b7rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie ist die Welt so gro\u00df bei lichter Lampen Milde!", "tokens": ["Wie", "ist", "die", "Welt", "so", "gro\u00df", "bei", "lich\u00b7ter", "Lam\u00b7pen", "Mil\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "ADJD", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "F\u00fcr der Erinnrung Blick, wie ist die Welt so klein!", "tokens": ["F\u00fcr", "der", "E\u00b7rinn\u00b7rung", "Blick", ",", "wie", "ist", "die", "Welt", "so", "klein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "PWAV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Entflammten Geistes sind wir pl\u00f6tzlich auf dem Wege,", "tokens": ["Ent\u00b7flamm\u00b7ten", "Geis\u00b7tes", "sind", "wir", "pl\u00f6tz\u00b7lich", "auf", "dem", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Herz von dumpfem Groll und herber Sehnsucht schwer,", "tokens": ["Das", "Herz", "von", "dum\u00b7pfem", "Groll", "und", "her\u00b7ber", "Sehn\u00b7sucht", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df unsre Seele bang im Takt der Ruderschl\u00e4ge", "tokens": ["Da\u00df", "uns\u00b7re", "See\u00b7le", "bang", "im", "Takt", "der", "Ru\u00b7der\u00b7schl\u00e4\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihr Unbegrenztes wiegt auf dem begrenzten Meer:", "tokens": ["Ihr", "Un\u00b7be\u00b7grenz\u00b7tes", "wiegt", "auf", "dem", "be\u00b7grenz\u00b7ten", "Meer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die einen fliehn ihr Land, um Ehr und Gl\u00fcck betrogen,", "tokens": ["Die", "ei\u00b7nen", "fliehn", "ihr", "Land", ",", "um", "Ehr", "und", "Gl\u00fcck", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPOSAT", "NN", "$,", "KOUI", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die andern jagt der Fluch der Heimat, andre gehn,", "tokens": ["Die", "an\u00b7dern", "jagt", "der", "Fluch", "der", "Hei\u00b7mat", ",", "and\u00b7re", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "ART", "NN", "$,", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In Augen einer Frau versunkne Astrologen,", "tokens": ["In", "Au\u00b7gen", "ei\u00b7ner", "Frau", "ver\u00b7sunk\u00b7ne", "Ast\u00b7ro\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Circe, die verlockt mit s\u00fc\u00dfer D\u00fcfte Wehn.", "tokens": ["Der", "Cir\u00b7ce", ",", "die", "ver\u00b7lockt", "mit", "s\u00fc\u00b7\u00dfer", "D\u00fcf\u00b7te", "Wehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Um nicht in Tiere sich zu wandeln, trinken Wonne", "tokens": ["Um", "nicht", "in", "Tie\u00b7re", "sich", "zu", "wan\u00b7deln", ",", "trin\u00b7ken", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUI", "PTKNEG", "APPR", "ADJA", "PRF", "PTKZU", "VVFIN", "$,", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie aus der Himmel Glanz, aus Weite, Licht und Strahl;", "tokens": ["Sie", "aus", "der", "Him\u00b7mel", "Glanz", ",", "aus", "Wei\u00b7te", ",", "Licht", "und", "Strahl", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "NN", "$,", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Eisluft, die sie st\u00e4hlt, der Feuerbrand der Sonne", "tokens": ["Die", "Eis\u00b7luft", ",", "die", "sie", "st\u00e4hlt", ",", "der", "Feu\u00b7er\u00b7brand", "der", "Son\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verwischen allgemach der K\u00fcsse haftend Mal.", "tokens": ["Ver\u00b7wi\u00b7schen", "all\u00b7ge\u00b7mach", "der", "K\u00fcs\u00b7se", "haf\u00b7tend", "Mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch wahre Wandrer sind, die den Ballons gleich reisen,", "tokens": ["Doch", "wah\u00b7re", "Wand\u00b7rer", "sind", ",", "die", "den", "Bal\u00b7lons", "gleich", "rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "$,", "PRELS", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nur um zu reisen, die leichtherzig nie den Bann,", "tokens": ["Nur", "um", "zu", "rei\u00b7sen", ",", "die", "leicht\u00b7her\u00b7zig", "nie", "den", "Bann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUI", "PTKZU", "VVINF", "$,", "PRELS", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den ihnen das Geschick auflegte, von sich weisen,", "tokens": ["Den", "ih\u00b7nen", "das", "Ge\u00b7schick", "auf\u00b7leg\u00b7te", ",", "von", "sich", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie wissen nicht den Grund und sagen doch: Voran!", "tokens": ["Sie", "wis\u00b7sen", "nicht", "den", "Grund", "und", "sa\u00b7gen", "doch", ":", "Vo\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "KON", "VVFIN", "ADV", "$.", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die, deren W\u00fcnsche gleich den Wolken sich entfalten,", "tokens": ["Die", ",", "de\u00b7ren", "W\u00fcn\u00b7sche", "gleich", "den", "Wol\u00b7ken", "sich", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELAT", "NN", "ADV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie ein Rekrut, der tr\u00e4umt von der Kanonen Ruf,", "tokens": ["Wie", "ein", "Re\u00b7krut", ",", "der", "tr\u00e4umt", "von", "der", "Ka\u00b7no\u00b7nen", "Ruf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erhoffen Freuden sie, die stets sich neu gestalten,", "tokens": ["Er\u00b7hof\u00b7fen", "Freu\u00b7den", "sie", ",", "die", "stets", "sich", "neu", "ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "$,", "PRELS", "ADV", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "F\u00fcr die des Menschen Geist noch niemals Namen schuf!", "tokens": ["F\u00fcr", "die", "des", "Men\u00b7schen", "Geist", "noch", "nie\u00b7mals", "Na\u00b7men", "schuf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "NN", "ADV", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}