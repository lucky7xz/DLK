{"textgrid.poem.65657": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Lied vor der Schlacht", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer f\u00fcr die Freiheit k\u00e4mpft und f\u00e4llt, de\u00df Ruhm wird bl\u00fchend stehn,", "tokens": ["Wer", "f\u00fcr", "die", "Frei\u00b7heit", "k\u00e4mpft", "und", "f\u00e4llt", ",", "de\u00df", "Ruhm", "wird", "bl\u00fc\u00b7hend", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "So lange frei die Winde noch durch freie L\u00fcfte wehn,", "tokens": ["So", "lan\u00b7ge", "frei", "die", "Win\u00b7de", "noch", "durch", "frei\u00b7e", "L\u00fcf\u00b7te", "wehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "So lange frei der B\u00e4ume Laub noch rauscht im gr\u00fcnen Wald,", "tokens": ["So", "lan\u00b7ge", "frei", "der", "B\u00e4u\u00b7me", "Laub", "noch", "rauscht", "im", "gr\u00fc\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "ART", "NN", "NN", "ADV", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "So lang' des Stromes Woge noch frei nach dem Meere wallt,", "tokens": ["So", "lang'", "des", "Stro\u00b7mes", "Wo\u00b7ge", "noch", "frei", "nach", "dem", "Mee\u00b7re", "wallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "ADV", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "So lang' des Adlers Fittig frei noch durch die Wolken fleugt,", "tokens": ["So", "lang'", "des", "Ad\u00b7lers", "Fit\u00b7tig", "frei", "noch", "durch", "die", "Wol\u00b7ken", "fleugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "ADJD", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "So lang' ein freier Odem noch aus freiem Herzen steigt.", "tokens": ["So", "lang'", "ein", "frei\u00b7er", "O\u00b7dem", "noch", "aus", "frei\u00b7em", "Her\u00b7zen", "steigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Wer f\u00fcr die Freiheit k\u00e4mpft und f\u00e4llt, de\u00df Ruhm wird bl\u00fchend stehn,", "tokens": ["Wer", "f\u00fcr", "die", "Frei\u00b7heit", "k\u00e4mpft", "und", "f\u00e4llt", ",", "de\u00df", "Ruhm", "wird", "bl\u00fc\u00b7hend", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "So lange freie Geister noch durch Erd' und Himmel gehn.", "tokens": ["So", "lan\u00b7ge", "frei\u00b7e", "Geis\u00b7ter", "noch", "durch", "Erd'", "und", "Him\u00b7mel", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Durch Erd' und Himmel schwebt er noch, der Helden Schattenreihn,", "tokens": ["Durch", "Erd'", "und", "Him\u00b7mel", "schwebt", "er", "noch", ",", "der", "Hel\u00b7den", "Schat\u00b7ten\u00b7reihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Und rauscht um uns in stiller Nacht, in hellem Sonnenschein,", "tokens": ["Und", "rauscht", "um", "uns", "in", "stil\u00b7ler", "Nacht", ",", "in", "hel\u00b7lem", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Im Sturm, der stolze Tannen bricht, und in dem L\u00fcftchen auch,", "tokens": ["Im", "Sturm", ",", "der", "stol\u00b7ze", "Tan\u00b7nen", "bricht", ",", "und", "in", "dem", "L\u00fcft\u00b7chen", "auch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,", "KON", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Das durch das Gras auf Gr\u00e4bern spielt mit seinem leisen Hauch.", "tokens": ["Das", "durch", "das", "Gras", "auf", "Gr\u00e4\u00b7bern", "spielt", "mit", "sei\u00b7nem", "lei\u00b7sen", "Hauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "APPR", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "In ferner Enkel Hause noch um alle Wiegen kreist", "tokens": ["In", "fer\u00b7ner", "En\u00b7kel", "Hau\u00b7se", "noch", "um", "al\u00b7le", "Wie\u00b7gen", "kreist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN", "ADV", "APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Auf Hellas heldenreicher Flur der freien Ahnen Geist;", "tokens": ["Auf", "Hel\u00b7las", "hel\u00b7den\u00b7rei\u00b7cher", "Flur", "der", "frei\u00b7en", "Ah\u00b7nen", "Geist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.9": {"text": "Der haucht in Wundertr\u00e4umen schon den zarten S\u00e4ugling an,", "tokens": ["Der", "haucht", "in", "Wun\u00b7der\u00b7tr\u00e4u\u00b7men", "schon", "den", "zar\u00b7ten", "S\u00e4ug\u00b7ling", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "Und weiht in seinem ersten Schlaf das Kind zu einem Mann.", "tokens": ["Und", "weiht", "in", "sei\u00b7nem", "ers\u00b7ten", "Schlaf", "das", "Kind", "zu", "ei\u00b7nem", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.11": {"text": "Den J\u00fcngling lockt sein Ruf hinaus mit nie gef\u00fchlter Lust", "tokens": ["Den", "J\u00fcng\u00b7ling", "lockt", "sein", "Ruf", "hin\u00b7aus", "mit", "nie", "ge\u00b7f\u00fchl\u00b7ter", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "APZR", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.12": {"text": "Zur St\u00e4tte, wo ein Freier fiel, da greift er in die Brust", "tokens": ["Zur", "St\u00e4t\u00b7te", ",", "wo", "ein", "Frei\u00b7er", "fiel", ",", "da", "greift", "er", "in", "die", "Brust"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.13": {"text": "Dem Zitternden, und Schauer ziehn ihm durch das tiefe Herz:", "tokens": ["Dem", "Zit\u00b7tern\u00b7den", ",", "und", "Schau\u00b7er", "ziehn", "ihm", "durch", "das", "tie\u00b7fe", "Herz", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "NE", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "Er wei\u00df nicht, ob es Wonne sei, ob es der erste Schmerz.", "tokens": ["Er", "wei\u00df", "nicht", ",", "ob", "es", "Won\u00b7ne", "sei", ",", "ob", "es", "der", "ers\u00b7te", "Schmerz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "NN", "VAFIN", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.15": {"text": "Herab, du heil'ge Geisterschaar, schwell' unsre Fahnen auf,", "tokens": ["Her\u00b7ab", ",", "du", "heil'\u00b7ge", "Geis\u00b7ter\u00b7schaar", ",", "schwell'", "uns\u00b7re", "Fah\u00b7nen", "auf", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.16": {"text": "Befl\u00fcgle unsrer Herzen Schlag und unsrer F\u00fc\u00dfe Lauf!", "tokens": ["Be\u00b7fl\u00fcg\u00b7le", "uns\u00b7rer", "Her\u00b7zen", "Schlag", "und", "uns\u00b7rer", "F\u00fc\u00b7\u00dfe", "Lauf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.17": {"text": "Wir ziehen nach der Freiheit aus, die Waffen in der Hand,", "tokens": ["Wir", "zie\u00b7hen", "nach", "der", "Frei\u00b7heit", "aus", ",", "die", "Waf\u00b7fen", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.18": {"text": "Wir ziehen aus auf Kampf und Tod f\u00fcr Gott, f\u00fcr's Vaterland.", "tokens": ["Wir", "zie\u00b7hen", "aus", "auf", "Kampf", "und", "Tod", "f\u00fcr", "Gott", ",", "f\u00fcr's", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPR", "NN", "KON", "NN", "APPR", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.19": {"text": "Ihr seid mit uns, ihr rauscht um uns, eur Geisterodem zieht", "tokens": ["Ihr", "seid", "mit", "uns", ",", "ihr", "rauscht", "um", "uns", ",", "eur", "Geis\u00b7te\u00b7ro\u00b7dem", "zieht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$,", "PPER", "VVFIN", "APPR", "PPER", "$,", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.20": {"text": "Mit zauberischen T\u00f6nen hin durch unser Jubellied.", "tokens": ["Mit", "zau\u00b7be\u00b7ri\u00b7schen", "T\u00f6\u00b7nen", "hin", "durch", "un\u00b7ser", "Ju\u00b7bel\u00b7lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.21": {"text": "Ihr seid mit uns, ihr schwebt daher, ihr aus Thermopyl\u00e4,", "tokens": ["Ihr", "seid", "mit", "uns", ",", "ihr", "schwebt", "da\u00b7her", ",", "ihr", "aus", "Ther\u00b7mo\u00b7py\u00b7l\u00e4", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$,", "PPER", "VVFIN", "PAV", "$,", "PPER", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.22": {"text": "Ihr aus dem gr\u00fcnen Marathon, ihr von der blauen See", "tokens": ["Ihr", "aus", "dem", "gr\u00fc\u00b7nen", "Ma\u00b7ra\u00b7thon", ",", "ihr", "von", "der", "blau\u00b7en", "See"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$,", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.23": {"text": "Am Wolkenfelsen Mykale, am Salaminerstrand,", "tokens": ["Am", "Wol\u00b7ken\u00b7fel\u00b7sen", "My\u00b7ka\u00b7le", ",", "am", "Sa\u00b7la\u00b7mi\u00b7ner\u00b