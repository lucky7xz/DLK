{"textgrid.poem.57058": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Die N\u00e4he", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die N\u00e4he ging vertr\u00e4umt umher ...", "tokens": ["Die", "N\u00e4\u00b7he", "ging", "ver\u00b7tr\u00e4umt", "um\u00b7her", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie kam nie zu den Dingen selber.", "tokens": ["Sie", "kam", "nie", "zu", "den", "Din\u00b7gen", "sel\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Antlitz wurde gelb und gelber,", "tokens": ["Ihr", "Ant\u00b7litz", "wur\u00b7de", "gelb", "und", "gel\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und ihren Leib ergriff die Zehr.", "tokens": ["und", "ih\u00b7ren", "Leib", "er\u00b7griff", "die", "Zehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch eines Nachts, derweil sie schlief,", "tokens": ["Doch", "ei\u00b7nes", "Nachts", ",", "der\u00b7weil", "sie", "schlief", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "da trat wer an ihr Bette hin", "tokens": ["da", "trat", "wer", "an", "ihr", "Bet\u00b7te", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PWS", "APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und sprach: \u00bbSteh auf, mein Kind, ich bin", "tokens": ["und", "sprach", ":", "\u00bb", "Steh", "auf", ",", "mein", "Kind", ",", "ich", "bin"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$.", "$(", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "der kategorische Komparativ!", "tokens": ["der", "ka\u00b7te\u00b7go\u00b7ri\u00b7sche", "Kom\u00b7pa\u00b7ra\u00b7tiv", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich werde dich zum N\u00e4her steigern,", "tokens": ["Ich", "wer\u00b7de", "dich", "zum", "N\u00e4\u00b7her", "stei\u00b7gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ja, wenn du willst, zur N\u00e4herin!\u00ab \u2013", "tokens": ["ja", ",", "wenn", "du", "willst", ",", "zur", "N\u00e4\u00b7he\u00b7rin", "!", "\u00ab", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "VMFIN", "$,", "APPRART", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die N\u00e4he, ohne sich zu weigern,", "tokens": ["Die", "N\u00e4\u00b7he", ",", "oh\u00b7ne", "sich", "zu", "wei\u00b7gern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "sie nahm auch dies als Schicksal hin.", "tokens": ["sie", "nahm", "auch", "dies", "als", "Schick\u00b7sal", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PDS", "KOUS", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Als N\u00e4herin jedoch verga\u00df", "tokens": ["Als", "N\u00e4\u00b7he\u00b7rin", "je\u00b7doch", "ver\u00b7ga\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "sie leider v\u00f6llig, was sie wollte,", "tokens": ["sie", "lei\u00b7der", "v\u00f6l\u00b7lig", ",", "was", "sie", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "$,", "PRELS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und n\u00e4hte Putz und hie\u00df Frau Nolte", "tokens": ["und", "n\u00e4h\u00b7te", "Putz", "und", "hie\u00df", "Frau", "Nol\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "KON", "VVFIN", "NN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und hielt all Obiges f\u00fcr Spa\u00df.", "tokens": ["und", "hielt", "all", "O\u00b7bi\u00b7ges", "f\u00fcr", "Spa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}