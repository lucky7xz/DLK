{"textgrid.poem.61879": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Traurig schritt ich hin am Bach,", "tokens": ["Trau\u00b7rig", "schritt", "ich", "hin", "am", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieh, da trat auf leichten F\u00fc\u00dfen", "tokens": ["Sieh", ",", "da", "trat", "auf", "leich\u00b7ten", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sanft zu mir der Lenz und sprach:", "tokens": ["Sanft", "zu", "mir", "der", "Lenz", "und", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbdeine Jugend l\u00e4\u00dft dich gr\u00fc\u00dfen.\u00ab", "tokens": ["\u00bb", "dei\u00b7ne", "Ju\u00b7gend", "l\u00e4\u00dft", "dich", "gr\u00fc\u00b7\u00dfen", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "PRF", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und es blies mich an, und j\u00e4h", "tokens": ["Und", "es", "blies", "mich", "an", ",", "und", "j\u00e4h"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Brach durch meines Tr\u00fcbsinns Kruste", "tokens": ["Brach", "durch", "mei\u00b7nes", "Tr\u00fcb\u00b7sinns", "Krus\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Solch' Gef\u00fchl von Wonn' und Weh,", "tokens": ["Solch'", "Ge\u00b7f\u00fchl", "von", "Wonn'", "und", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich lautauf weinen mu\u00dfte.", "tokens": ["Da\u00df", "ich", "laut\u00b7auf", "wei\u00b7nen", "mu\u00df\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "All mein Wesen dehnte sich,", "tokens": ["All", "mein", "We\u00b7sen", "dehn\u00b7te", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gleich als sollt' es Fl\u00fcgel breiten,", "tokens": ["Gleich", "als", "sollt'", "es", "Fl\u00fc\u00b7gel", "brei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "VMFIN", "PPER", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein Klang durchbebte mich", "tokens": ["Und", "ein", "Klang", "durch\u00b7beb\u00b7te", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie von angeschlagnen Saiten.", "tokens": ["Wie", "von", "an\u00b7ge\u00b7schlag\u00b7nen", "Sai\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wirf denn ab des Zweifels Last,", "tokens": ["Wirf", "denn", "ab", "des", "Zwei\u00b7fels", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Herz, du darfst noch nicht verzichten!", "tokens": ["Herz", ",", "du", "darfst", "noch", "nicht", "ver\u00b7zich\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "PAV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun du wieder Tr\u00e4nen hast,", "tokens": ["Nun", "du", "wie\u00b7der", "Tr\u00e4\u00b7nen", "hast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Magst du wieder bl\u00fchn und dichten.", "tokens": ["Magst", "du", "wie\u00b7der", "bl\u00fchn", "und", "dich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "KON", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}