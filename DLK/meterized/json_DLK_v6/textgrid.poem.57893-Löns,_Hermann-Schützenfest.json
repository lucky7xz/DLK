{"textgrid.poem.57893": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Sch\u00fctzenfest", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So f\u00e4ngt denn morgen das Schie\u00dfen an,", "tokens": ["So", "f\u00e4ngt", "denn", "mor\u00b7gen", "das", "Schie\u00b7\u00dfen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Um dreie beginnt der Trubel,", "tokens": ["Um", "drei\u00b7e", "be\u00b7ginnt", "der", "Tru\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die gro\u00dfe Spektakelsymphonie,", "tokens": ["Die", "gro\u00b7\u00dfe", "Spek\u00b7ta\u00b7kel\u00b7symp\u00b7ho\u00b7nie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.4": {"text": "Der L\u00e4rm und das Gejubel.", "tokens": ["Der", "L\u00e4rm", "und", "das", "Ge\u00b7ju\u00b7bel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Als ruhiger Schlag ist sonst bekannt", "tokens": ["Als", "ru\u00b7hi\u00b7ger", "Schlag", "ist", "sonst", "be\u00b7kannt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "VAFIN", "ADV", "ADJD"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Das Volk am Strand der Leine,", "tokens": ["Das", "Volk", "am", "Strand", "der", "Lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Beim Freischie\u00dfen aber, da kocht ihm das Blut,", "tokens": ["Beim", "Frei\u00b7schie\u00b7\u00dfen", "a\u00b7ber", ",", "da", "kocht", "ihm", "das", "Blut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und lebhafter werden die Beine.", "tokens": ["Und", "leb\u00b7haf\u00b7ter", "wer\u00b7den", "die", "Bei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Dritte ist morgen, da hat jeder Geld,", "tokens": ["Der", "Drit\u00b7te", "ist", "mor\u00b7gen", ",", "da", "hat", "je\u00b7der", "Geld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADV", "$,", "ADV", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da hei\u00dft es \u00bbRein ins Vergn\u00fcgen!\u00ab", "tokens": ["Da", "hei\u00dft", "es", "\u00bb", "Rein", "ins", "Ver\u00b7gn\u00fc\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und wenn vom Elften bis Letzten wir", "tokens": ["Und", "wenn", "vom", "Elf\u00b7ten", "bis", "Letz\u00b7ten", "wir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPRART", "NN", "APPR", "NN", "PPER"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dann alle krumm auch liegen.", "tokens": ["Dann", "al\u00b7le", "krumm", "auch", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Jedoch das Wetter, das sage ich euch,", "tokens": ["Je\u00b7doch", "das", "Wet\u00b7ter", ",", "das", "sa\u00b7ge", "ich", "euch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PDS", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wird schlecht in jedem Falle,", "tokens": ["Wird", "schlecht", "in", "je\u00b7dem", "Fal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn bleiben wir auch vom Regen verschont \u2013", "tokens": ["Denn", "blei\u00b7ben", "wir", "auch", "vom", "Re\u00b7gen", "ver\u00b7schont", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Verhageln tun wir doch alle.", "tokens": ["Ver\u00b7ha\u00b7geln", "tun", "wir", "doch", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PIS", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}