{"textgrid.poem.39927": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der Graf von Zollern", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der junge Hohenzoller Graf", "tokens": ["Der", "jun\u00b7ge", "Ho\u00b7hen\u00b7zol\u00b7ler", "Graf"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem W\u00fcrttemberger dient' er brav,", "tokens": ["Dem", "W\u00fcrt\u00b7tem\u00b7ber\u00b7ger", "dient'", "er", "brav", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch zu der Wittwe stolz er spricht:", "tokens": ["Doch", "zu", "der", "Witt\u00b7we", "stolz", "er", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbfahrt wohl, dem Weibe dien' ich nicht!\u00ab", "tokens": ["\u00bb", "fahrt", "wohl", ",", "dem", "Wei\u00b7be", "dien'", "ich", "nicht", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "ADV", "$,", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sein Knappe fl\u00fcstert: \u00bbHerr, wie schlimm!", "tokens": ["Sein", "Knap\u00b7pe", "fl\u00fcs\u00b7tert", ":", "\u00bb", "Herr", ",", "wie", "schlimm", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "$(", "NN", "$,", "PWAV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verwahret euch vor Frauengrimm!\u00ab", "tokens": ["Ver\u00b7wah\u00b7ret", "euch", "vor", "Frau\u00b7en\u00b7grimm", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er aber schrie ganz trotziglich:", "tokens": ["Er", "a\u00b7ber", "schrie", "ganz", "trot\u00b7zig\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbes wird kein Weib verschlingen mich!\u00ab", "tokens": ["\u00bb", "es", "wird", "kein", "Weib", "ver\u00b7schlin\u00b7gen", "mich", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "PIAT", "NN", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch als er auf dem Zollern sa\u00df,", "tokens": ["Doch", "als", "er", "auf", "dem", "Zol\u00b7lern", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Traf ihn ein Brief am vollen Glas:", "tokens": ["Traf", "ihn", "ein", "Brief", "am", "vol\u00b7len", "Glas", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbverschlingen alleweg will ich", "tokens": ["\u00bb", "ver\u00b7schlin\u00b7gen", "al\u00b7le\u00b7weg", "will", "ich"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "ADV", "VMFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein ", "tokens": ["Dein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "Und indem zog ein heller Hauf,", "tokens": ["Und", "in\u00b7dem", "zog", "ein", "hel\u00b7ler", "Hauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die St\u00e4dter, aus dem Thal herauf.", "tokens": ["Die", "St\u00e4d\u00b7ter", ",", "aus", "dem", "Thal", "her\u00b7auf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist auf ihr beladnes Ro\u00df", "tokens": ["Es", "ist", "auf", "ihr", "be\u00b7lad\u00b7nes", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gefallen gar zu oft sein Tro\u00df.", "tokens": ["Ge\u00b7fal\u00b7len", "gar", "zu", "oft", "sein", "Tro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKA", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da zog der Graf die Br\u00fcck' empor,", "tokens": ["Da", "zog", "der", "Graf", "die", "Br\u00fcck", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verriegelt wohl sein neunfach Thor.", "tokens": ["Ver\u00b7rie\u00b7gelt", "wohl", "sein", "neun\u00b7fach", "Thor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er schmaus't und zecht ein ganzes Jahr,", "tokens": ["Er", "schmaus't", "und", "zecht", "ein", "gan\u00b7zes", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Abzug bl\u00e4s't der Feinde Schar.", "tokens": ["Zum", "Ab\u00b7zug", "bl\u00e4s't", "der", "Fein\u00b7de", "Schar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Da naht es schwarz, ein neues Heer,", "tokens": ["Da", "naht", "es", "schwarz", ",", "ein", "neu\u00b7es", "Heer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zweitausend sind es, oder mehr.", "tokens": ["Zweit\u00b7au\u00b7send", "sind", "es", ",", "o\u00b7der", "mehr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "$,", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Knappe spricht: \u00bbGnad' uns, o Christ,", "tokens": ["Der", "Knap\u00b7pe", "spricht", ":", "\u00bb", "Gnad'", "uns", ",", "o", "Christ", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "VVIMP", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die wirtemberg'sche Fahn' es ist!\u00ab", "tokens": ["Die", "wir\u00b7tem\u00b7ber\u00b7g'\u00b7sche", "Fahn'", "es", "ist", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VAFIN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Der k\u00fchne Graf k\u00e4mpft noch ein Jahr,", "tokens": ["Der", "k\u00fch\u00b7ne", "Graf", "k\u00e4mpft", "noch", "ein", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bis Scheune leer und Keller war.", "tokens": ["Bis", "Scheu\u00b7ne", "leer", "und", "Kel\u00b7ler", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er bei\u00dft die Lippen sich vor Wut:", "tokens": ["Er", "bei\u00dft", "die", "Lip\u00b7pen", "sich", "vor", "Wut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbverschlungen hat sie doch mein ", "tokens": ["\u00bb", "ver\u00b7schlun\u00b7gen", "hat", "sie", "doch", "mein"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVPP", "VAFIN", "PPER", "ADV", "PPOSAT"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Die Thore schlie\u00dft er traurig auf,", "tokens": ["Die", "Tho\u00b7re", "schlie\u00dft", "er", "trau\u00b7rig", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es zieht herein der Feinde Hauf,", "tokens": ["Es", "zieht", "her\u00b7ein", "der", "Fein\u00b7de", "Hauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Ulmer brechen Stein um Stein,", "tokens": ["Die", "Ul\u00b7mer", "bre\u00b7chen", "Stein", "um", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wirtemberger lachen drein.", "tokens": ["Die", "Wir\u00b7tem\u00b7ber\u00b7ger", "la\u00b7chen", "drein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.9": {"line.1": {"text": "Gen Stuttgart f\u00fchrt man ihn zu Ro\u00df:", "tokens": ["Gen", "Stutt\u00b7gart", "f\u00fchrt", "man", "ihn", "zu", "Ro\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PIS", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbverschlungen habt Ihr, Frau, mein ", "tokens": ["\u00bb", "ver\u00b7schlun\u00b7gen", "habt", "Ihr", ",", "Frau", ",", "mein"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["$(", "VVPP", "VAFIN", "PPER", "$,", "NN", "$,", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr lie\u00dfet mir kein L\u00f6sepfand,", "tokens": ["Ihr", "lie\u00b7\u00dfet", "mir", "kein", "L\u00f6\u00b7se\u00b7pfand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Leben steht in Eurer Hand.\u00ab", "tokens": ["Mein", "Le\u00b7ben", "steht", "in", "Eu\u00b7rer", "Hand", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Die stolze Gr\u00e4fin winket stumm,", "tokens": ["Die", "stol\u00b7ze", "Gr\u00e4\u00b7fin", "win\u00b7ket", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und l\u00e4chelt arg und kehrt sich um,", "tokens": ["Und", "l\u00e4\u00b7chelt", "arg", "und", "kehrt", "sich", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In's ferne Land, in einen Thurm", "tokens": ["In's", "fer\u00b7ne", "Land", ",", "in", "ei\u00b7nen", "Thurm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schickt sie den Feind zu Molch und Wurm.", "tokens": ["Schickt", "sie", "den", "Feind", "zu", "Molch", "und", "Wurm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Zehn Jahre wohnt der Graf im Graus,", "tokens": ["Zehn", "Jah\u00b7re", "wohnt", "der", "Graf", "im", "Graus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Haar wird grau, sein Blick lischt aus,", "tokens": ["Sein", "Haar", "wird", "grau", ",", "sein", "Blick", "lischt", "aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sinkt er traurig in das Knie:", "tokens": ["Da", "sinkt", "er", "trau\u00b7rig", "in", "das", "Knie", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbverschlungen hat mein ", "tokens": ["\u00bb", "ver\u00b7schlun\u00b7gen", "hat", "mein"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "VVPP", "VAFIN", "PPOSAT"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.12": {"line.1": {"text": "Jetzt thut sich auf das Kerkerthor,", "tokens": ["Jetzt", "thut", "sich", "auf", "das", "Ker\u00b7ker\u00b7thor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Knappe richtet ihn empor:", "tokens": ["Sein", "Knap\u00b7pe", "rich\u00b7tet", "ihn", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbfrei seid ihr, Herr, an Seel' und Leib,", "tokens": ["\u00bb", "frei", "seid", "ihr", ",", "Herr", ",", "an", "Seel'", "und", "Leib", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PPER", "$,", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Grabe liegt das grimme Weib.\u00ab", "tokens": ["Im", "Gra\u00b7be", "liegt", "das", "grim\u00b7me", "Weib", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Er tritt hervor au's Himmelslicht", "tokens": ["Er", "tritt", "her\u00b7vor", "au's", "Him\u00b7mels\u00b7licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hebt sein bleiches Angesicht,", "tokens": ["Und", "hebt", "sein", "blei\u00b7ches", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und reckt empor zum Schwur die Hand:", "tokens": ["Und", "reckt", "em\u00b7por", "zum", "Schwur", "die", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKVZ", "APPRART", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbfort, fort in das gelobte Land!", "tokens": ["\u00bb", "fort", ",", "fort", "in", "das", "ge\u00b7lob\u00b7te", "Land", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Er schwingt, wie sonst, zu Rosse sich,", "tokens": ["Er", "schwingt", ",", "wie", "sonst", ",", "zu", "Ros\u00b7se", "sich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADV", "$,", "APPR", "NN", "PRF", "$,"]