{"textgrid.poem.59953": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sch\u00f6n bist du, fruchtreiche Kyrosstadt!", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n bist du, fruchtreiche Kyrosstadt!", "tokens": ["Sch\u00f6n", "bist", "du", ",", "fruch\u00b7trei\u00b7che", "Ky\u00b7ros\u00b7stadt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Sch\u00f6n sind deine T\u00f6chter und S\u00f6hne zumal!", "tokens": ["Sch\u00f6n", "sind", "dei\u00b7ne", "T\u00f6ch\u00b7ter", "und", "S\u00f6h\u00b7ne", "zu\u00b7mal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "KON", "NN", "ADV", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Du Meer meiner Wonne, du Meer meiner Qual,", "tokens": ["Du", "Meer", "mei\u00b7ner", "Won\u00b7ne", ",", "du", "Meer", "mei\u00b7ner", "Qual", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$,", "PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Drin mein Herz seine Perle gefunden hat:", "tokens": ["Drin", "mein", "Herz", "sei\u00b7ne", "Per\u00b7le", "ge\u00b7fun\u00b7den", "hat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Dich sing' ich, dich gr\u00fc\u00df' ich beim vollen Pokal!", "tokens": ["Dich", "sing'", "ich", ",", "dich", "gr\u00fc\u00df'", "ich", "beim", "vol\u00b7len", "Po\u00b7kal", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Siehe, Felsen und Berge umschlie\u00dfen dich,", "tokens": ["Sie\u00b7he", ",", "Fel\u00b7sen", "und", "Ber\u00b7ge", "um\u00b7schlie\u00b7\u00dfen", "dich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "KON", "NN", "VVFIN", "PPER", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Befruchtende Wasser durchflie\u00dfen dich;", "tokens": ["Be\u00b7fruch\u00b7ten\u00b7de", "Was\u00b7ser", "durch\u00b7flie\u00b7\u00dfen", "dich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Es w\u00e4chst auf knorrigen B\u00e4umen,", "tokens": ["Es", "w\u00e4chst", "auf", "knor\u00b7ri\u00b7gen", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In gr\u00fcnen sonnigen R\u00e4umen", "tokens": ["In", "gr\u00fc\u00b7nen", "son\u00b7ni\u00b7gen", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Dein s\u00fc\u00dfer Feuerwein.", "tokens": ["Dein", "s\u00fc\u00b7\u00dfer", "Feu\u00b7er\u00b7wein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Es w\u00e4lzen warme Quellen", "tokens": ["Es", "w\u00e4l\u00b7zen", "war\u00b7me", "Quel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ihre wundert\u00e4tigen Wellen", "tokens": ["Ih\u00b7re", "wun\u00b7der\u00b7t\u00e4\u00b7ti\u00b7gen", "Wel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Aus rauhem Felsgestein.", "tokens": ["Aus", "rau\u00b7hem", "Fels\u00b7ge\u00b7stein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es klettern die Saklis, die grauen,", "tokens": ["Es", "klet\u00b7tern", "die", "Sak\u00b7lis", ",", "die", "grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Rings aus dem gr\u00fcnen Plan", "tokens": ["Rings", "aus", "dem", "gr\u00fc\u00b7nen", "Plan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die gelben Berge hinan.", "tokens": ["Die", "gel\u00b7ben", "Ber\u00b7ge", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vom steilen Felshang schauen", "tokens": ["Vom", "stei\u00b7len", "Fels\u00b7hang", "schau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ruinen, Schl\u00f6sser und Festen", "tokens": ["Ru\u00b7i\u00b7nen", ",", "Schl\u00f6s\u00b7ser", "und", "Fes\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "In das weite Kyrostal", "tokens": ["In", "das", "wei\u00b7te", "Ky\u00b7ros\u00b7tal"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit seinen stolzen Pal\u00e4sten", "tokens": ["Mit", "sei\u00b7nen", "stol\u00b7zen", "Pa\u00b7l\u00e4s\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und H\u00e4usern ohne Zahl", "tokens": ["Und", "H\u00e4u\u00b7sern", "oh\u00b7ne", "Zahl"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und dem bunten Menschengewimmel", "tokens": ["Und", "dem", "bun\u00b7ten", "Men\u00b7schen\u00b7ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Auf M\u00e4rkten und Basar \u2013", "tokens": ["Auf", "M\u00e4rk\u00b7ten", "und", "Ba\u00b7sar", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Dar\u00fcber w\u00f6lbt sich klar", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "w\u00f6lbt", "sich", "klar"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PRF", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Der warme, blaue Himmel.", "tokens": ["Der", "war\u00b7me", ",", "blau\u00b7e", "Him\u00b7mel", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und zu der Sch\u00f6nheit Throne", "tokens": ["Und", "zu", "der", "Sch\u00f6n\u00b7heit", "Thro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Viel luftige Balkone", "tokens": ["Viel", "luf\u00b7ti\u00b7ge", "Bal\u00b7ko\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Galerien winden sich", "tokens": ["Und", "Ga\u00b7le\u00b7ri\u00b7en", "win\u00b7den", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um deiner H\u00e4user Reihn;", "tokens": ["Um", "dei\u00b7ner", "H\u00e4u\u00b7ser", "Reihn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auf den Balkonen finden sich", "tokens": ["Auf", "den", "Bal\u00b7ko\u00b7nen", "fin\u00b7den", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Allabendlich bei Mondenschein", "tokens": ["Al\u00b7la\u00b7bend\u00b7lich", "bei", "Mon\u00b7den\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Viel schmucke, schlanke M\u00e4dchen ein.", "tokens": ["Viel", "schmu\u00b7cke", ",", "schlan\u00b7ke", "M\u00e4d\u00b7chen", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie lehnen \u00fcber die R\u00e4nder,", "tokens": ["Sie", "leh\u00b7nen", "\u00fc\u00b7ber", "die", "R\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Im Antlitz Huld und S\u00fc\u00dfe \u2013", "tokens": ["Im", "Ant\u00b7litz", "Huld", "und", "S\u00fc\u00b7\u00dfe", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Es flattern die bunten Gew\u00e4nder,", "tokens": ["Es", "flat\u00b7tern", "die", "bun\u00b7ten", "Ge\u00b7w\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.11": {"text": "Es zucken die kleinen F\u00fc\u00dfe \u2013", "tokens": ["Es", "zu\u00b7cken", "die", "klei\u00b7nen", "F\u00fc\u00b7\u00dfe", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Der dunklen Augen Feuer", "tokens": ["Der", "dunk\u00b7len", "Au\u00b7gen", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Blitzt durch die hellen Schleier ...", "tokens": ["Blitzt", "durch", "die", "hel\u00b7len", "Schlei\u00b7er", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sch\u00f6n bist du, fruchtreiche Kyrosstadt!", "tokens": ["Sch\u00f6n", "bist", "du", ",", "fruch\u00b7trei\u00b7che", "Ky\u00b7ros\u00b7stadt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Sch\u00f6n sind deine T\u00f6chter und S\u00f6hne zumal!", "tokens": ["Sch\u00f6n", "sind", "dei\u00b7ne", "T\u00f6ch\u00b7ter", "und", "S\u00f6h\u00b7ne", "zu\u00b7mal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "KON", "NN", "ADV", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Du Meer meiner Wonne, du Meer meiner Qual!", "tokens": ["Du", "Meer", "mei\u00b7ner", "Won\u00b7ne", ",", "du", "Meer", "mei\u00b7ner", "Qual", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$,", "PPER", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Drin mein Herz seine Perle gefunden hat:", "tokens": ["Drin", "mein", "Herz", "sei\u00b7ne", "Per\u00b7le", "ge\u00b7fun\u00b7den", "hat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Dich sing' ich, dich gr\u00fc\u00df' ich beim vollen Pokal!", "tokens": ["Dich", "sing'", "ich", ",", "dich", "gr\u00fc\u00df'", "ich", "beim", "vol\u00b7len", "Po\u00b7kal", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}