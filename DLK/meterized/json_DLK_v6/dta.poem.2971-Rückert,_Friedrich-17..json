{"dta.poem.2971": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Landes Grenz' ist nicht geschickt ein Flu\u00df zu bilden,", "tokens": ["Des", "Lan\u00b7des", "Grenz'", "ist", "nicht", "ge\u00b7schickt", "ein", "Flu\u00df", "zu", "bil\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "PTKNEG", "VVPP", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der immer abnimmt hier, dort zusetzt den Gefilden.", "tokens": ["Der", "im\u00b7mer", "ab\u00b7nimmt", "hier", ",", "dort", "zu\u00b7setzt", "den", "Ge\u00b7fil\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ADV", "$,", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Es k\u00f6nnte seyn zuletzt dem einen Volk genommen", "tokens": ["Es", "k\u00f6nn\u00b7te", "seyn", "zu\u00b7letzt", "dem", "ei\u00b7nen", "Volk", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADV", "ART", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ganze Land, und ganz dem andern angeschwommen.", "tokens": ["Das", "gan\u00b7ze", "Land", ",", "und", "ganz", "dem", "an\u00b7dern", "an\u00b7ge\u00b7schwom\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ADV", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch wenn dasselbe Volk an beiden Ufern wohnt,", "tokens": ["Doch", "wenn", "das\u00b7sel\u00b7be", "Volk", "an", "bei\u00b7den", "U\u00b7fern", "wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PDAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Trifft es kein Schaden hier, der dort nicht ist belohnt.", "tokens": ["Trifft", "es", "kein", "Scha\u00b7den", "hier", ",", "der", "dort", "nicht", "ist", "be\u00b7lohnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "ADV", "$,", "PRELS", "ADV", "PTKNEG", "VAFIN", "VVPP", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Wie wenn in einem Haus der Haushalt wird ger\u00fcckt;", "tokens": ["Wie", "wenn", "in", "ei\u00b7nem", "Haus", "der", "Haus\u00b7halt", "wird", "ge\u00b7r\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "APPR", "ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier wird ein Eckchen leer, ein andres dort geschm\u00fcckt.", "tokens": ["Hier", "wird", "ein", "Eck\u00b7chen", "leer", ",", "ein", "and\u00b7res", "dort", "ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$,", "ART", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}