{"dta.poem.7839": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "Die Sch\u00f6ne/ ohne die Z\u00e4hne.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1665", "urn": "urn:nbn:de:kobv:b4-200905198447", "language": ["de:0.99"], "booktitle": "Birken, Sigmund von: Pegnesische Gespr\u00e4chspiel-Gesellschaft von Nymfen und Hirten. N\u00fcrnberg, 1665."}, "poem": {"stanza.1": {"line.1": {"text": "Die Aeuglein geben Blitz/ die schwarzen", "tokens": ["Die", "A\u00b7e\u00b7u\u00b7glein", "ge\u00b7ben", "Blitz", "/", "die", "schwar\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVINF", "NN", "$(", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Lippen Purpur gl\u00fcht. Die Wangen", "tokens": ["Der", "Lip\u00b7pen", "Pur\u00b7pur", "gl\u00fcht", ".", "Die", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "$.", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Solang du \u00f6ffnest nicht des Munds Corallen-", "tokens": ["So\u00b7lang", "du", "\u00f6ff\u00b7nest", "nicht", "des", "Munds", "Coral\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "TRUNC"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "solang sagt man/ da\u00df du wohl k\u00f6ndest Ve-", "tokens": ["so\u00b7lang", "sagt", "man", "/", "da\u00df", "du", "wohl", "k\u00f6n\u00b7dest", "Ve"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PIS", "$(", "KOUS", "PPER", "ADV", "VMFIN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wann aber zeigen sich im Mund die", "tokens": ["Wann", "a\u00b7ber", "zei\u00b7gen", "sich", "im", "Mund", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PRF", "APPRART", "NN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "die Z\u00e4hne: dann beginnt dein Sch\u00f6nheit-", "tokens": ["die", "Z\u00e4h\u00b7ne", ":", "dann", "be\u00b7ginnt", "dein", "Sch\u00f6n\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ADV", "VVFIN", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "bald es komt in dem Mund/ da f\u00e4ngt es an", "tokens": ["bald", "es", "komt", "in", "dem", "Mund", "/", "da", "f\u00e4ngt", "es", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "ART", "NN", "$(", "ADV", "VVFIN", "PPER", "APPR"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Ach! da\u00df ihn die Natur/ nit auch mit Hel-", "tokens": ["Ach", "!", "da\u00df", "ihn", "die", "Na\u00b7tur", "/", "nit", "auch", "mit", "Hel"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "KOUS", "PPER", "ART", "NN", "$(", "PTKNEG", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "geziert/ wie deinen Leib! Die Z\u00e4hne beissen", "tokens": ["ge\u00b7ziert", "/", "wie", "dei\u00b7nen", "Leib", "!", "Die", "Z\u00e4h\u00b7ne", "beis\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "$(", "KOKOM", "PPOSAT", "NN", "$.", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "f\u00fcr war kein kleines Stuck von deiner grossen", "tokens": ["f\u00fcr", "war", "kein", "klei\u00b7nes", "Stuck", "von", "dei\u00b7ner", "gros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "VAFIN", "PIAT", "ADJA", "NN", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Dr\u00fcm h\u00e4ttest b\u00e4sser du ein stummes Bild ge-", "tokens": ["Dr\u00fcm", "h\u00e4t\u00b7test", "b\u00e4s\u00b7ser", "du", "ein", "stum\u00b7mes", "Bild", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJD", "PPER", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "das mit den Augen redt. Das Reden zwar", "tokens": ["das", "mit", "den", "Au\u00b7gen", "redt", ".", "Das", "Re\u00b7den", "zwar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$.", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "so gar verstellen nicht. Nur merke di\u00df/ mein", "tokens": ["so", "gar", "ver\u00b7stel\u00b7len", "nicht", ".", "Nur", "mer\u00b7ke", "di\u00df", "/", "mein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "VVFIN", "PTKNEG", "$.", "ADV", "VVFIN", "PDS", "$(", "PPOSAT"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "wann ich dich lieben soll/ so lache mich nit an!", "tokens": ["wann", "ich", "dich", "lie\u00b7ben", "soll", "/", "so", "la\u00b7che", "mich", "nit", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "VMFIN", "$(", "ADV", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}