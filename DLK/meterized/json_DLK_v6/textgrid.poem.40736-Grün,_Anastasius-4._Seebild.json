{"textgrid.poem.40736": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "4. Seebild", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch die Wellen steuert ein Schwan so einsam,", "tokens": ["Durch", "die", "Wel\u00b7len", "steu\u00b7ert", "ein", "Schwan", "so", "ein\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Hell und blank, wie die schimmernde Wasserlilie,", "tokens": ["Hell", "und", "blank", ",", "wie", "die", "schim\u00b7mern\u00b7de", "Was\u00b7ser\u00b7li\u00b7lie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "PTKVZ", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wie im Azur die ziehende Silberwolke,", "tokens": ["Wie", "im", "A\u00b7zur", "die", "zie\u00b7hen\u00b7de", "Sil\u00b7ber\u00b7wol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Blume der Erde zugleich und Bote des Himmels.", "tokens": ["Blu\u00b7me", "der", "Er\u00b7de", "zu\u00b7gleich", "und", "Bo\u00b7te", "des", "Him\u00b7mels", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "KON", "NN", "ART", "NN", "$."], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Von Balkonen herab und Bl\u00fcthenterassen", "tokens": ["Von", "Bal\u00b7ko\u00b7nen", "her\u00b7ab", "und", "Bl\u00fc\u00b7then\u00b7ter\u00b7as\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "KON", "NN"], "meter": "-++--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Streuen ihm wei\u00dfe H\u00e4nde n\u00e4hrende Brodsaat.", "tokens": ["Streu\u00b7en", "ihm", "wei\u00b7\u00dfe", "H\u00e4n\u00b7de", "n\u00e4h\u00b7ren\u00b7de", "Brod\u00b7saat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "---+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Feierlich schwebt er heran, fast ohne Regung,", "tokens": ["Fei\u00b7er\u00b7lich", "schwebt", "er", "he\u00b7ran", ",", "fast", "oh\u00b7ne", "Re\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "APPR", "NN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "St\u00e4te Bewegung doch in seliger Ruhe,", "tokens": ["St\u00e4\u00b7te", "Be\u00b7we\u00b7gung", "doch", "in", "se\u00b7li\u00b7ger", "Ru\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.9": {"text": "Gleich dem r\u00fcckenden Zeiger auf dem Uhrblatt,", "tokens": ["Gleich", "dem", "r\u00fc\u00b7cken\u00b7den", "Zei\u00b7ger", "auf", "dem", "Uhr\u00b7blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Gleich dem reisenden Mondesnachen im Aether.", "tokens": ["Gleich", "dem", "rei\u00b7sen\u00b7den", "Mon\u00b7des\u00b7na\u00b7chen", "im", "A\u00b7e\u00b7ther", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+--+-+--+--", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie du feierlich stolz, o Schwan, dahinziehst,", "tokens": ["Wie", "du", "fei\u00b7er\u00b7lich", "stolz", ",", "o", "Schwan", ",", "da\u00b7hin\u00b7ziehst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "ADJD", "$,", "FM", "NN", "$,", "VVFIN", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "In dem flimmernden See ein einsamer Segler,", "tokens": ["In", "dem", "flim\u00b7mern\u00b7den", "See", "ein", "ein\u00b7sa\u00b7mer", "Seg\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Unter dir die gl\u00e4nzenden Spiegelbilder", "tokens": ["Un\u00b7ter", "dir", "die", "gl\u00e4n\u00b7zen\u00b7den", "Spie\u00b7gel\u00b7bil\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Bl\u00fchender Ufer, goldener Himmelsw\u00f6lbung,", "tokens": ["Bl\u00fc\u00b7hen\u00b7der", "U\u00b7fer", ",", "gol\u00b7de\u00b7ner", "Him\u00b7mels\u00b7w\u00f6l\u00b7bung", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "M\u00e4chtiger Berge, die Natur rings th\u00fcrmte,", "tokens": ["M\u00e4ch\u00b7ti\u00b7ger", "Ber\u00b7ge", ",", "die", "Na\u00b7tur", "rings", "th\u00fcrm\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Freundlicher St\u00e4tten, die der Mensch hier geschaffen,", "tokens": ["Freund\u00b7li\u00b7cher", "St\u00e4t\u00b7ten", ",", "die", "der", "Mensch", "hier", "ge\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Wird des See's kristallener blanker Spiegel", "tokens": ["Wird", "des", "See's", "kris\u00b7tal\u00b7le\u00b7ner", "blan\u00b7ker", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mir zum Spiegel der Zeiten und Geschicke,", "tokens": ["Mir", "zum", "Spie\u00b7gel", "der", "Zei\u00b7ten", "und", "Ge\u00b7schi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Wirst du selbst mir ein hehr und mahnend Sinnbild.", "tokens": ["Wirst", "du", "selbst", "mir", "ein", "hehr", "und", "mah\u00b7nend", "Sinn\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPER", "ART", "ADJD", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wenn dir Sturm den schneeigen Flaum emporstr\u00e4ubt,", "tokens": ["Wenn", "dir", "Sturm", "den", "schne\u00b7e\u00b7i\u00b7gen", "Flaum", "em\u00b7por\u00b7str\u00e4ubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Weithin flattert sein schwarzer Wolkenmantel", "tokens": ["Weit\u00b7hin", "flat\u00b7tert", "sein", "schwar\u00b7zer", "Wol\u00b7ken\u00b7man\u00b7tel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und die Wellen wie drohende F\u00e4uste sich ballen", "tokens": ["Und", "die", "Wel\u00b7len", "wie", "dro\u00b7hen\u00b7de", "F\u00e4us\u00b7te", "sich", "bal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KOKOM", "ADJA", "NN", "PRF", "VVINF"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Sieh, dann liegt der Spiegel zerschlagen, in Splittern,", "tokens": ["Sieh", ",", "dann", "liegt", "der", "Spie\u00b7gel", "zer\u00b7schla\u00b7gen", ",", "in", "Split\u00b7tern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "NN", "VVINF", "$,", "APPR", "NN", "$,"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "All die gl\u00e4nzenden Bilder sind zerstoben", "tokens": ["All", "die", "gl\u00e4n\u00b7zen\u00b7den", "Bil\u00b7der", "sind", "zer\u00b7sto\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "VAFIN", "VVPP"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Und versunken in die chaotische Brandung.", "tokens": ["Und", "ver\u00b7sun\u00b7ken", "in", "die", "chao\u00b7ti\u00b7sche", "Bran\u00b7dung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch auch wenn in sonniger Ruhe lautlos", "tokens": ["Doch", "auch", "wenn", "in", "son\u00b7ni\u00b7ger", "Ru\u00b7he", "laut\u00b7los"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOUS", "APPR", "ADJA", "NN", "ADJD"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ueber dir tiefblau der Aether sich breitet,", "tokens": ["Ue\u00b7ber", "dir", "tief\u00b7blau", "der", "A\u00b7e\u00b7ther", "sich", "brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+--++-+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Seines Lebens wollusthauchender Athem", "tokens": ["Sei\u00b7nes", "Le\u00b7bens", "wol\u00b7lust\u00b7hau\u00b7chen\u00b7der", "A\u00b7them"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Leise, leise, wie Blumenduft, den See streift,", "tokens": ["Lei\u00b7se", ",", "lei\u00b7se", ",", "wie", "Blu\u00b7men\u00b7duft", ",", "den", "See", "streift", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "PWAV", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Der so glatt und blank, wie metallgegossen,", "tokens": ["Der", "so", "glatt", "und", "blank", ",", "wie", "me\u00b7tall\u00b7ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "KON", "PTKVZ", "$,", "PWAV", "VVINF", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Da\u00df er sich sanft zu regen beginnt und zu kr\u00e4useln;", "tokens": ["Da\u00df", "er", "sich", "sanft", "zu", "re\u00b7gen", "be\u00b7ginnt", "und", "zu", "kr\u00e4u\u00b7seln", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "APPR", "ADJA", "VVFIN", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Da auch \u00fcber den Spiegel wallt ein Zittern,", "tokens": ["Da", "auch", "\u00fc\u00b7ber", "den", "Spie\u00b7gel", "wallt", "ein", "Zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Wellengeriesel und glitzernde Flimmerlichter", "tokens": ["Wel\u00b7len\u00b7ge\u00b7rie\u00b7sel", "und", "glit\u00b7zern\u00b7de", "Flim\u00b7mer\u00b7lich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.9": {"text": "Rei\u00dfen tanzende Furchen in seine Fl\u00e4chen,", "tokens": ["Rei\u00b7\u00dfen", "tan\u00b7zen\u00b7de", "Fur\u00b7chen", "in", "sei\u00b7ne", "Fl\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Und die Risse durchziehn der Bilder Konturen,", "tokens": ["Und", "die", "Ris\u00b7se", "durch\u00b7ziehn", "der", "Bil\u00b7der", "Kon\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "Da\u00df ihr Band sich l\u00f6st in St\u00fccke zerfallend,", "tokens": ["Da\u00df", "ihr", "Band", "sich", "l\u00f6st", "in", "St\u00fc\u00b7cke", "zer\u00b7fal\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "VVFIN", "APPR", "NN", "VVPP", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Da\u00df der Berge S\u00e4ulen quer\u00fcber gespalten,", "tokens": ["Da\u00df", "der", "Ber\u00b7ge", "S\u00e4u\u00b7len", "quer\u00b7\u00fc\u00b7ber", "ge\u00b7spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "ADV", "VVPP", "$,"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Wie geborsten die Gletscher, durchs\u00e4gt die W\u00e4lder,", "tokens": ["Wie", "ge\u00b7bors\u00b7ten", "die", "Glet\u00b7scher", ",", "durch\u00b7s\u00e4gt", "die", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.14": {"text": "Wie geknickt und zerpfl\u00fcckt die Blumen des Ufers.", "tokens": ["Wie", "ge\u00b7knickt", "und", "zer\u00b7pfl\u00fcckt", "die", "Blu\u00b7men", "des", "U\u00b7fers", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "KON", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "Auf den H\u00f6hen die Burg, im Thal die H\u00fctte,", "tokens": ["Auf", "den", "H\u00f6\u00b7hen", "die", "Burg", ",", "im", "Thal", "die", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Neben dem R\u00f6merstein der schimmernde Kirchthurm,", "tokens": ["Ne\u00b7ben", "dem", "R\u00f6\u00b7mer\u00b7stein", "der", "schim\u00b7mern\u00b7de", "Kirch\u00b7thurm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.17": {"text": "Altes und Neues, sowie die Menschlein dazwischen,", "tokens": ["Al\u00b7tes", "und", "Neu\u00b7es", ",", "so\u00b7wie", "die", "Mensc\u00b7hlein", "da\u00b7zwi\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "KON", "ART", "NN", "PAV", "$,"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.18": {"text": "Alles zerschwankend, zerbr\u00f6ckelnd und zerflie\u00dfend!", "tokens": ["Al\u00b7les", "zer\u00b7schwan\u00b7kend", ",", "zer\u00b7br\u00f6\u00b7ckelnd", "und", "zer\u00b7flie\u00b7\u00dfend", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVPP", "$,", "VVPP", "KON", "VVPP", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.19": {"text": "Aber feierlich \u00fcber den Bildertr\u00fcmmern,", "tokens": ["A\u00b7ber", "fei\u00b7er\u00b7lich", "\u00fc\u00b7ber", "den", "Bil\u00b7der\u00b7tr\u00fcm\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "Ueber dem Schwankenden ziehst du, einsamer Lootse,", "tokens": ["Ue\u00b7ber", "dem", "Schwan\u00b7ken\u00b7den", "ziehst", "du", ",", "ein\u00b7sa\u00b7mer", "Loot\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.21": {"text": "Deine Bahnen