{"textgrid.poem.47607": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[stehe, steh']", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stehe, steh'", "tokens": ["Ste\u00b7he", ",", "steh'"], "token_info": ["word", "punct", "word"], "pos": ["VVFIN", "$,", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Fl\u00fcchtiges Reh!", "tokens": ["Fl\u00fcch\u00b7ti\u00b7ges", "Reh", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Nicht, wie der Wind,", "tokens": ["Nicht", ",", "wie", "der", "Wind", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Eile hinweg,", "tokens": ["Ei\u00b7le", "hin\u00b7weg", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Liebliches Kind;", "tokens": ["Lieb\u00b7li\u00b7ches", "Kind", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Scheue zu gr\u00fc\u00dfen nicht,", "tokens": ["Scheu\u00b7e", "zu", "gr\u00fc\u00b7\u00dfen", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "PTKNEG", "$,"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.7": {"text": "F\u00fcrchte Hafisen nicht!", "tokens": ["F\u00fcrch\u00b7te", "Ha\u00b7fi\u00b7sen", "nicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Glaub', er ist fromm,", "tokens": ["Glaub'", ",", "er", "ist", "fromm", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Glaub', er ist gut,", "tokens": ["Glaub'", ",", "er", "ist", "gut", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Heget und pflegt", "tokens": ["He\u00b7get", "und", "pflegt"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "VVFIN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Sch\u00f6ne Gef\u00fchle,", "tokens": ["Sch\u00f6\u00b7ne", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Klappert er auch", "tokens": ["Klap\u00b7pert", "er", "auch"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Nicht, wie die M\u00fchle,", "tokens": ["Nicht", ",", "wie", "die", "M\u00fch\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Plappert er auch", "tokens": ["Plap\u00b7pert", "er", "auch"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Hohle Gebete nicht,", "tokens": ["Hoh\u00b7le", "Ge\u00b7be\u00b7te", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.16": {"text": "Blicke du ihm", "tokens": ["Bli\u00b7cke", "du", "ihm"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Kecklich in's Angesicht,", "tokens": ["Keck\u00b7lich", "in's", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.18": {"text": "Und du verlachst,", "tokens": ["Und", "du", "ver\u00b7lachst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Was der Zelote spricht.", "tokens": ["Was", "der", "Ze\u00b7lo\u00b7te", "spricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}