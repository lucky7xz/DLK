{"textgrid.poem.34817": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein jeder hat zu diesem Feste", "tokens": ["Ein", "je\u00b7der", "hat", "zu", "die\u00b7sem", "Fes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VAFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein liebes Liebchen mitgebracht,", "tokens": ["Sein", "lie\u00b7bes", "Lieb\u00b7chen", "mit\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und freut sich der bl\u00fchenden Sommernacht; \u2013", "tokens": ["Und", "freut", "sich", "der", "bl\u00fc\u00b7hen\u00b7den", "Som\u00b7mer\u00b7nacht", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ich wandle allein, mir fehlt das Beste.", "tokens": ["Ich", "wand\u00b7le", "al\u00b7lein", ",", "mir", "fehlt", "das", "Bes\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich wandle allein gleich einem Kranken!", "tokens": ["Ich", "wand\u00b7le", "al\u00b7lein", "gleich", "ei\u00b7nem", "Kran\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich fliehe die Lust, ich fliehe den Tanz", "tokens": ["Ich", "flie\u00b7he", "die", "Lust", ",", "ich", "flie\u00b7he", "den", "Tanz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und die sch\u00f6ne Musik und den Lampenglanz; \u2013", "tokens": ["Und", "die", "sch\u00f6\u00b7ne", "Mu\u00b7sik", "und", "den", "Lam\u00b7pen\u00b7glanz", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "KON", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "In England sind meine Gedanken.", "tokens": ["In", "En\u00b7gland", "sind", "mei\u00b7ne", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Ich breche Rosen, ich breche Nelken,", "tokens": ["Ich", "bre\u00b7che", "Ro\u00b7sen", ",", "ich", "bre\u00b7che", "Nel\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zerstreuten Sinnes und kummervoll;", "tokens": ["Zer\u00b7streu\u00b7ten", "Sin\u00b7nes", "und", "kum\u00b7mer\u00b7voll", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich wei\u00df nicht, wem ich sie geben soll; \u2013", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wem", "ich", "sie", "ge\u00b7ben", "soll", ";", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "PPER", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mein Herz und die Blumen verwelken.", "tokens": ["Mein", "Herz", "und", "die", "Blu\u00b7men", "ver\u00b7wel\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}