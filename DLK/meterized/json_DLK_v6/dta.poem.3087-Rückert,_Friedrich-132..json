{"dta.poem.3087": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "132.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du thust, begl\u00fcckter Freund, ein B\u00fcchlein leichter ab,", "tokens": ["Du", "thust", ",", "be\u00b7gl\u00fcck\u00b7ter", "Freund", ",", "ein", "B\u00fcch\u00b7lein", "leich\u00b7ter", "ab", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sobald sich dir der Gr\u00fcnd' Unhaltbarkeit ergab.", "tokens": ["So\u00b7bald", "sich", "dir", "der", "Gr\u00fcnd'", "Un\u00b7halt\u00b7bar\u00b7keit", "er\u00b7gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich habe l\u00e4nger mich damit herumzuschlagen,", "tokens": ["Ich", "ha\u00b7be", "l\u00e4n\u00b7ger", "mich", "da\u00b7mit", "her\u00b7um\u00b7zu\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PRF", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil mich die Meinungen mehr als die Gr\u00fcnde plagen.", "tokens": ["Weil", "mich", "die", "Mei\u00b7nun\u00b7gen", "mehr", "als", "die", "Gr\u00fcn\u00b7de", "pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PIAT", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Meinungen, ob auf ob ohne Grund sie stehn,", "tokens": ["Die", "Mei\u00b7nun\u00b7gen", ",", "ob", "auf", "ob", "oh\u00b7ne", "Grund", "sie", "stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "APPR", "KOUS", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ziehn oder sto\u00dfen mich, dem kann ich nicht entgehn.", "tokens": ["Ziehn", "o\u00b7der", "sto\u00b7\u00dfen", "mich", ",", "dem", "kann", "ich", "nicht", "ent\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "$,", "PDS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich frage nicht, warum, nur was und wie man's meint,", "tokens": ["Ich", "fra\u00b7ge", "nicht", ",", "wa\u00b7rum", ",", "nur", "was", "und", "wie", "man's", "meint", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "$,", "ADV", "PWS", "KON", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wie dis Meinen dann mit meinem sich vereint.", "tokens": ["Und", "wie", "dis", "Mei\u00b7nen", "dann", "mit", "mei\u00b7nem", "sich", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PDS", "VVFIN", "ADV", "APPR", "PPOSAT", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und dieser Meinungstreit ist schwerer mir zu schlichten,", "tokens": ["Und", "die\u00b7ser", "Mei\u00b7nungs\u00b7treit", "ist", "schwe\u00b7rer", "mir", "zu", "schlich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als siegreich dir ein Heer von Gr\u00fcnden zu vernichten.", "tokens": ["Als", "sieg\u00b7reich", "dir", "ein", "Heer", "von", "Gr\u00fcn\u00b7den", "zu", "ver\u00b7nich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}