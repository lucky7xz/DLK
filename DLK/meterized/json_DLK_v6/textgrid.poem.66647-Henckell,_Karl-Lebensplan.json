{"textgrid.poem.66647": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Lebensplan", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rein zu genie\u00dfen begehr' ich mein Leben,", "tokens": ["Rein", "zu", "ge\u00b7nie\u00b7\u00dfen", "be\u00b7gehr'", "ich", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Weise durchwandelnd die Tage des Lichts,", "tokens": ["Wei\u00b7se", "durch\u00b7wan\u00b7delnd", "die", "Ta\u00b7ge", "des", "Lichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Meine Gedanken will ich erheben", "tokens": ["Mei\u00b7ne", "Ge\u00b7dan\u00b7ken", "will", "ich", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Zu den Wipfeln des Weltgedichts.", "tokens": ["Zu", "den", "Wip\u00b7feln", "des", "Welt\u00b7ge\u00b7dichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Saat des Elends begehr' ich zu mindern,", "tokens": ["Saat", "des", "E\u00b7lends", "be\u00b7gehr'", "ich", "zu", "min\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Weil sie die Freiheit der Seele mir st\u00f6rt,", "tokens": ["Weil", "sie", "die", "Frei\u00b7heit", "der", "See\u00b7le", "mir", "st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Tat des Unrechts begehr' ich zu hindern,", "tokens": ["Tat", "des", "Un\u00b7rechts", "be\u00b7gehr'", "ich", "zu", "hin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Weil sie das horchende Herz emp\u00f6rt.", "tokens": ["Weil", "sie", "das", "hor\u00b7chen\u00b7de", "Herz", "em\u00b7p\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Blumen zu pfl\u00fccken ist mein Begehren,", "tokens": ["Blu\u00b7men", "zu", "pfl\u00fc\u00b7cken", "ist", "mein", "Be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Blumen der Liebe, der Kunst und der Flur,", "tokens": ["Blu\u00b7men", "der", "Lie\u00b7be", ",", "der", "Kunst", "und", "der", "Flur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Kinder und Enkel will ich es lehren:", "tokens": ["Kin\u00b7der", "und", "En\u00b7kel", "will", "ich", "es", "leh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Folgt der erl\u00f6senden Sch\u00f6nheit Spur!", "tokens": ["Folgt", "der", "er\u00b7l\u00f6\u00b7sen\u00b7den", "Sch\u00f6n\u00b7heit", "Spur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}