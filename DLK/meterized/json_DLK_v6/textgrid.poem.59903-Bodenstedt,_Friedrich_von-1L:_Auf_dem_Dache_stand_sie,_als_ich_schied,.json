{"textgrid.poem.59903": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf dem Dache stand sie, als ich schied,", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf dem Dache stand sie, als ich schied,", "tokens": ["Auf", "dem", "Da\u00b7che", "stand", "sie", ",", "als", "ich", "schied", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Mit Gewand und Locken spielt der Wind \u2013", "tokens": ["Mit", "Ge\u00b7wand", "und", "Lo\u00b7cken", "spielt", "der", "Wind", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sang ich scheidend ihr mein letztes Lied:", "tokens": ["Sang", "ich", "schei\u00b7dend", "ihr", "mein", "letz\u00b7tes", "Lied", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Nun leb wohl, du wunders\u00fc\u00dfes Kind!", "tokens": ["Nun", "leb", "wohl", ",", "du", "wun\u00b7der\u00b7s\u00fc\u00b7\u00dfes", "Kind", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Mu\u00df von dannen gehn", "tokens": ["Mu\u00df", "von", "dan\u00b7nen", "gehn"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ADV", "VVINF"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Doch auf Wiedersehn,", "tokens": ["Doch", "auf", "Wie\u00b7der\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Wenn das Hochzeitsbett bereitet steht!", "tokens": ["Wenn", "das", "Hoch\u00b7zeits\u00b7bett", "be\u00b7rei\u00b7tet", "steht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Kamel, beladen, bring' ich dir,", "tokens": ["Ein", "Ka\u00b7mel", ",", "be\u00b7la\u00b7den", ",", "bring'", "ich", "dir", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "$,", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Reichen Stoff zu Kleidern und Schalw\u00e1r,", "tokens": ["Rei\u00b7chen", "Stoff", "zu", "Klei\u00b7dern", "und", "Schal\u00b7w\u00e1r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Echte Chenna", "tokens": ["Ech\u00b7te", "Chen\u00b7na"], "token_info": ["word", "word"], "pos": ["NN", "NE"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Schmuck und Narden f\u00fcr dein Ambrahaar,", "tokens": ["Schmuck", "und", "Nar\u00b7den", "f\u00fcr", "dein", "Am\u00b7bra\u00b7haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Feines Seidenzeug,", "tokens": ["Fei\u00b7nes", "Sei\u00b7den\u00b7zeug", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Sammet dick und weich,", "tokens": ["Sam\u00b7met", "dick", "und", "weich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und die Mutter wird zufrieden sein!", "tokens": ["Und", "die", "Mut\u00b7ter", "wird", "zu\u00b7frie\u00b7den", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Auf dem Dache stand sie, als ich schied,", "tokens": ["Auf", "dem", "Da\u00b7che", "stand", "sie", ",", "als", "ich", "schied", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Winkt herab mit ihrer kleinen Hand. \u2013", "tokens": ["Winkt", "her\u00b7ab", "mit", "ih\u00b7rer", "klei\u00b7nen", "Hand", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Weht der Wind ihr zu mein Scheidelied,", "tokens": ["Weht", "der", "Wind", "ihr", "zu", "mein", "Schei\u00b7de\u00b7lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Spielt der Wind mit Locken und Gewand;", "tokens": ["Spielt", "der", "Wind", "mit", "Lo\u00b7cken", "und", "Ge\u00b7wand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Fahre wohl, mein Gl\u00fcck!", "tokens": ["Fah\u00b7re", "wohl", ",", "mein", "Gl\u00fcck", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Kehre bald zur\u00fcck.", "tokens": ["Keh\u00b7re", "bald", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Wenn das Hochzeitsbett bereitet steht!", "tokens": ["Wenn", "das", "Hoch\u00b7zeits\u00b7bett", "be\u00b7rei\u00b7tet", "steht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}