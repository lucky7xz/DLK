{"textgrid.poem.47650": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[es sind der K\u00f6niginnen]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sind der K\u00f6niginnen", "tokens": ["Es", "sind", "der", "K\u00f6\u00b7ni\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In ihrer Reize Strahl", "tokens": ["In", "ih\u00b7rer", "Rei\u00b7ze", "Strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht weniger, denn sechzig,", "tokens": ["Nicht", "we\u00b7ni\u00b7ger", ",", "denn", "sech\u00b7zig", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "KON", "CARD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Kebsinnen sind achtzig", "tokens": ["Der", "Keb\u00b7sin\u00b7nen", "sind", "acht\u00b7zig"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "CARD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In meinem Freudensaal;", "tokens": ["In", "mei\u00b7nem", "Freu\u00b7den\u00b7saal", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Es eifern au\u00dfer ihnen,", "tokens": ["Es", "ei\u00b7fern", "au\u00b7\u00dfer", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mich bl\u00fchend zu bedienen", "tokens": ["Mich", "bl\u00fc\u00b7hend", "zu", "be\u00b7die\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVPP", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ausb\u00fcndig sch\u00f6ne M\u00e4dchen ohne Zahl \u2013", "tokens": ["Aus\u00b7b\u00fcn\u00b7dig", "sch\u00f6\u00b7ne", "M\u00e4d\u00b7chen", "oh\u00b7ne", "Zahl", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Alleine", "tokens": ["Al\u00b7lei\u00b7ne"], "token_info": ["word"], "pos": ["ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "So feine", "tokens": ["So", "fei\u00b7ne"], "token_info": ["word", "word"], "pos": ["ADV", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "So bl\u00fcthenhold, wie keine", "tokens": ["So", "bl\u00fc\u00b7then\u00b7hold", ",", "wie", "kei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "PWAV", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "In diesem Rosenhaine,", "tokens": ["In", "die\u00b7sem", "Ro\u00b7sen\u00b7hai\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Ist eine", "tokens": ["Ist", "ei\u00b7ne"], "token_info": ["word", "word"], "pos": ["VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Mein T\u00e4ubchen ist, ja meine", "tokens": ["Mein", "T\u00e4ub\u00b7chen", "ist", ",", "ja", "mei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "ADV", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Begehrte Wonn' ist eine", "tokens": ["Be\u00b7gehr\u00b7te", "Wonn'", "ist", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Von ihnen allzumal.", "tokens": ["Von", "ih\u00b7nen", "all\u00b7zu\u00b7mal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Die K\u00f6niginnen schau'n sie,", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin\u00b7nen", "schau'n", "sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Die Kebsinnen, die M\u00e4dchen,", "tokens": ["Die", "Keb\u00b7sin\u00b7nen", ",", "die", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.19": {"text": "Und loben sie und preisen sie", "tokens": ["Und", "lo\u00b7ben", "sie", "und", "prei\u00b7sen", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und nennen dreimal selig", "tokens": ["Und", "nen\u00b7nen", "drei\u00b7mal", "se\u00b7lig"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Die gl\u00fcckliche Begl\u00fcckende,", "tokens": ["Die", "gl\u00fcck\u00b7li\u00b7che", "Be\u00b7gl\u00fc\u00b7cken\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.22": {"text": "Die mir das Aug' Entz\u00fcckende,", "tokens": ["Die", "mir", "das", "Aug'", "Ent\u00b7z\u00fc\u00b7cken\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.23": {"text": "Die mir den Geist Ber\u00fcckende,", "tokens": ["Die", "mir", "den", "Geist", "Be\u00b7r\u00fc\u00b7cken\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.24": {"text": "Die mir das Herz Umstrickende,", "tokens": ["Die", "mir", "das", "Herz", "Um\u00b7stri\u00b7cken\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.25": {"text": "Des K\u00f6niges sch\u00f6nste Wahl.", "tokens": ["Des", "K\u00f6\u00b7ni\u00b7ges", "sch\u00f6ns\u00b7te", "Wahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}