{"dta.poem.6379": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So ist kein Feur, das so, wie sie, durchdringet,", "tokens": ["So", "ist", "kein", "Feur", ",", "das", "so", ",", "wie", "sie", ",", "durch\u00b7drin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$,", "PRELS", "ADV", "$,", "PWAV", "PPER", "$,", "VVFIN", "$,"], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "So wild und hefftig ist: wie wir es kl\u00e4rlich k\u00f6nnen", "tokens": ["So", "wild", "und", "heff\u00b7tig", "ist", ":", "wie", "wir", "es", "kl\u00e4r\u00b7lich", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VAFIN", "$.", "PWAV", "PPER", "PPER", "ADJD", "VMFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "An Steinen und Metallen sehn,", "tokens": ["An", "Stei\u00b7nen", "und", "Me\u00b7tal\u00b7len", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sie geschmoltzen sind, da sie viel sch\u00e4rffer brennen,", "tokens": ["Wenn", "sie", "ge\u00b7schmolt\u00b7zen", "sind", ",", "da", "sie", "viel", "sch\u00e4rf\u00b7fer", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,", "KOUS", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und gr\u00f6ssre Quaal und Pein dadurch entstehn,", "tokens": ["Und", "gr\u00f6ss\u00b7re", "Qua\u00b7al", "und", "Pein", "da\u00b7durch", "ent\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "PAV", "VVINF", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Auch W\u00fcrckungen durch sie, die hefftiger, geschehn,", "tokens": ["Auch", "W\u00fcr\u00b7ckun\u00b7gen", "durch", "sie", ",", "die", "heff\u00b7ti\u00b7ger", ",", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "APPR", "PPER", "$,", "PRELS", "ADJD", "$,", "VVPP", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.7": {"text": "Als wie wir durch die leichten Theil empfinden", "tokens": ["Als", "wie", "wir", "durch", "die", "leich\u00b7ten", "Theil", "emp\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "PPER", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der C\u00f6rper, die sich leicht entz\u00fcnden.", "tokens": ["Der", "C\u00f6r\u00b7per", ",", "die", "sich", "leicht", "ent\u00b7z\u00fcn\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}