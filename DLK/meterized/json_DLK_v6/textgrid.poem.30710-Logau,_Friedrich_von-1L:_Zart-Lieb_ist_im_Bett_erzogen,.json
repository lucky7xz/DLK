{"textgrid.poem.30710": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zart-Lieb ist im Bett erzogen,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zart-Lieb ist im Bett erzogen,", "tokens": ["Zar\u00b7tLieb", "ist", "im", "Bett", "er\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat sechs Ammen au\u00dfgesogen,", "tokens": ["Hat", "sechs", "Am\u00b7men", "au\u00df\u00b7ge\u00b7so\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die von H\u00fcnern, Mandeln, Wein", "tokens": ["Die", "von", "H\u00fc\u00b7nern", ",", "Man\u00b7deln", ",", "Wein"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "APPR", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00fcsten stets gem\u00e4stet seyn.", "tokens": ["M\u00fcs\u00b7ten", "stets", "ge\u00b7m\u00e4s\u00b7tet", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zartlieb ist der Welt zu z\u00e4rtig;", "tokens": ["Zart\u00b7lieb", "ist", "der", "Welt", "zu", "z\u00e4r\u00b7tig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "PTKA", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eh er, d\u00fcnckt mich, noch wird b\u00e4rtig,", "tokens": ["Eh", "er", ",", "d\u00fcnckt", "mich", ",", "noch", "wird", "b\u00e4r\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "VVFIN", "PPER", "$,", "ADV", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Werden mit ihm ihren Mut", "tokens": ["Wer\u00b7den", "mit", "ihm", "ih\u00b7ren", "Mut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPER", "PPOSAT", "NN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.8": {"text": "W\u00fcrm und Schlangen machen gut.", "tokens": ["W\u00fcrm", "und", "Schlan\u00b7gen", "ma\u00b7chen", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}