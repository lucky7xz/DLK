{"dta.poem.17705": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "327.  Warumb GOtt de\u00df Regiments nicht  \n m\u00fcde wird.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Gotts und seins Geistesreich ist Liebe/ Freude/ Fride:", "tokens": ["Gotts", "und", "seins", "Geis\u00b7tes\u00b7reich", "ist", "Lie\u00b7be", "/", "Freu\u00b7de", "/", "Fri\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "KON", "CARD", "NN", "VAFIN", "NN", "$(", "NN", "$(", "NE", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Drumb wird Er de\u00df Regierns in Ewigkeit nicht m\u00fcde.", "tokens": ["Drumb", "wird", "Er", "de\u00df", "Re\u00b7gierns", "in", "E\u00b7wig\u00b7keit", "nicht", "m\u00fc\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ART", "NN", "APPR", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}