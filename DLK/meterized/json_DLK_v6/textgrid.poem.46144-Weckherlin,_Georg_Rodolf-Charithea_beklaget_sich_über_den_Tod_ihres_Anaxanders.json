{"textgrid.poem.46144": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Charithea beklaget sich \u00fcber den Tod ihres Anaxanders", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein glanz, o sonn, ist leider! nicht f\u00fcr mich,", "tokens": ["Dein", "glanz", ",", "o", "sonn", ",", "ist", "lei\u00b7der", "!", "nicht", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "FM", "$,", "VAFIN", "ADV", "$.", "PTKNEG", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "die ich nicht mehr bei leben;", "tokens": ["die", "ich", "nicht", "mehr", "bei", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "PTKNEG", "ADV", "APPR", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "dan er kan ja der todten angesicht", "tokens": ["dan", "er", "kan", "ja", "der", "tod\u00b7ten", "an\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VMFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "kein liecht, noch trost mehr geben:", "tokens": ["kein", "liecht", ",", "noch", "trost", "mehr", "ge\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADV", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "ich bin nu tod zu aller freud", "tokens": ["ich", "bin", "nu", "tod", "zu", "al\u00b7ler", "freud"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und lebendig nur zu dem leid.", "tokens": ["und", "le\u00b7ben\u00b7dig", "nur", "zu", "dem", "leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPR", "ART", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auch williglich vermeid ich deinen schein,", "tokens": ["Auch", "wil\u00b7lig\u00b7lich", "ver\u00b7meid", "ich", "dei\u00b7nen", "schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "als ab dem liecht verdrossen,", "tokens": ["als", "ab", "dem", "liecht", "ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "und lig alhie in der ein\u00f6d allein,", "tokens": ["und", "lig", "al\u00b7hie", "in", "der", "ei\u00b7n\u00f6d", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPR", "ART", "NN", "ADV", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "als in ein grab beschlossen,", "tokens": ["als", "in", "ein", "grab", "be\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "alda mich angst, sorg, pein und plag,", "tokens": ["al\u00b7da", "mich", "angst", ",", "sorg", ",", "pein", "und", "plag", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "VVFIN", "$,", "NE", "KON", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "wie w\u00fcrm, verzehren nacht und tag.", "tokens": ["wie", "w\u00fcrm", ",", "ver\u00b7zeh\u00b7ren", "nacht", "und", "tag", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nu wei\u00df ich recht, wie schmerzlich der wollust", "tokens": ["Nu", "wei\u00df", "ich", "recht", ",", "wie", "schmerz\u00b7lich", "der", "wol\u00b7lust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "PWAV", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "das angedenken r\u00fchret,", "tokens": ["das", "an\u00b7ge\u00b7den\u00b7ken", "r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "wan man so bald, ohn der begird verlust,", "tokens": ["wan", "man", "so", "bald", ",", "ohn", "der", "be\u00b7gird", "ver\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "ADV", "$,", "KOUI", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "nur den genu\u00df verlieret,", "tokens": ["nur", "den", "ge\u00b7nu\u00df", "ver\u00b7lie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "und wie vil besser jede frist", "tokens": ["und", "wie", "vil", "bes\u00b7ser", "je\u00b7de", "frist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "ADJD", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "nicht haben, dan verlieren, ist.", "tokens": ["nicht", "ha\u00b7ben", ",", "dan", "ver\u00b7lie\u00b7ren", ",", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "VAINF", "$,", "ADV", "VVINF", "$,", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach meine freud, die ich in gutem gl\u00fcck", "tokens": ["Ach", "mei\u00b7ne", "freud", ",", "die", "ich", "in", "gu\u00b7tem", "gl\u00fcck"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PPOSAT", "VVFIN", "$,", "PRELS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "nach herzens wunsch genossen,", "tokens": ["nach", "her\u00b7zens", "wunsch", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "ist wie die flut in einem augenblick", "tokens": ["ist", "wie", "die", "flut", "in", "ei\u00b7nem", "au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "mit ungest\u00fcm verflossen", "tokens": ["mit", "un\u00b7ge\u00b7st\u00fcm", "ver\u00b7flos\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "und lie\u00df mir nichts dan finsternus", "tokens": ["und", "lie\u00df", "mir", "nichts", "dan", "fins\u00b7ter\u00b7nus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PIS", "ADV", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und des verlornen guts verdru\u00df.", "tokens": ["und", "des", "ver\u00b7lor\u00b7nen", "guts", "ver\u00b7dru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach, weh! verdru\u00df! wilt du auch in dem grab", "tokens": ["Ach", ",", "weh", "!", "ver\u00b7dru\u00df", "!", "wilt", "du", "auch", "in", "dem", "grab"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PTKVZ", "$.", "NN", "$.", "VMFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "mein leben noch beschweren?", "tokens": ["mein", "le\u00b7ben", "noch", "be\u00b7schwe\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "soll dan, wan ich kein leben in mir hab,", "tokens": ["soll", "dan", ",", "wan", "ich", "kein", "le\u00b7ben", "in", "mir", "hab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "$,", "PWAV", "PPER", "PIAT", "VVFIN", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-----+-+", "measure": "dactylic.init"}, "line.4": {"text": "gleichwol mein \u00fcbel wehren?", "tokens": ["gleich\u00b7wol", "mein", "\u00fc\u00b7bel", "weh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "ach, la\u00df den armen todten zu,", "tokens": ["ach", ",", "la\u00df", "den", "ar\u00b7men", "tod\u00b7ten", "zu", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["XY", "$,", "VVIMP", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "zu ligen, traurig, in der ruh!", "tokens": ["zu", "li\u00b7gen", ",", "trau\u00b7rig", ",", "in", "der", "ruh", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ADJD", "$,", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Wan ich schon oft dein t\u00f6dlich schwere streich", "tokens": ["Wan", "ich", "schon", "oft", "dein", "t\u00f6d\u00b7lich", "schwe\u00b7re", "streich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADV", "PPOSAT", "ADJD", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "vor meinem tod empfunden;", "tokens": ["vor", "mei\u00b7nem", "tod", "emp\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "wan ich mich oft beklagend schwach und bleich", "tokens": ["wan", "ich", "mich", "oft", "be\u00b7kla\u00b7gend", "schwach", "und", "bleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "ADV", "ADJD", "ADJD", "KON", "ADJD"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.4": {"text": "die himmel taub erfunden;", "tokens": ["die", "him\u00b7mel", "taub", "er\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "sol dan auch tr\u00fcbsal und gefahr", "tokens": ["sol", "dan", "auch", "tr\u00fcb\u00b7sal", "und", "ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "mir noch beiwohnen in der bahr?", "tokens": ["mir", "noch", "bei\u00b7woh\u00b7nen", "in", "der", "bahr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Warum, ach weh! kan ich doch nimmermehr", "tokens": ["Wa\u00b7rum", ",", "ach", "weh", "!", "kan", "ich", "doch", "nim\u00b7mer\u00b7mehr"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "ADV", "PTKVZ", "$.", "VMFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "der s\u00fc\u00dfen zeit vergessen,", "tokens": ["der", "s\u00fc\u00b7\u00dfen", "zeit", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "darin mein herz mit ruhm, freud, lust und ehr", "tokens": ["da\u00b7rin", "mein", "herz", "mit", "ruhm", ",", "freud", ",", "lust", "und", "ehr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PAV", "PPOSAT", "NN", "APPR", "NN", "$,", "VVFIN", "$,", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "gl\u00fcckselig war besessen,", "tokens": ["gl\u00fcck\u00b7se\u00b7lig", "war", "be\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "nu da mein herz und seel zumal", "tokens": ["nu", "da", "mein", "herz", "und", "seel", "zu\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "KON", "ADV", "KOUS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "voll jamer, greuel, angst und qual?", "tokens": ["voll", "ja\u00b7mer", ",", "greu\u00b7el", ",", "angst", "und", "qual", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "NE", "$,", "VVPP", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ach, sihst du nicht, jemehr mein armes herz", "tokens": ["Ach", ",", "sihst", "du", "nicht", ",", "je\u00b7mehr", "mein", "ar\u00b7mes", "herz"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "PTKNEG", "$,", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "was es gehabt bedenket,", "tokens": ["was", "es", "ge\u00b7habt", "be\u00b7den\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAPP", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "da\u00df desto mehr der seelerm\u00f6rdend schmerz", "tokens": ["da\u00df", "des\u00b7to", "mehr", "der", "see\u00b7ler\u00b7m\u00f6r\u00b7dend", "schmerz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "es racket und bekr\u00e4nket,", "tokens": ["es", "ra\u00b7cket", "und", "be\u00b7kr\u00e4n\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "und da\u00df, indem ich bin nichts mehr,", "tokens": ["und", "da\u00df", ",", "in\u00b7dem", "ich", "bin", "nichts", "mehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "KOUS", "PPER", "VAFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "gewest zu sein mir allein schwer!", "tokens": ["ge\u00b7west", "zu", "sein", "mir", "al\u00b7lein", "schwer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PTKZU", "VAINF", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Dan weil also der schwer untr\u00e4glich zorn", "tokens": ["Dan", "weil", "al\u00b7so", "der", "schwer", "un\u00b7tr\u00e4g\u00b7lich", "zorn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ADV", "ART", "ADJD", "ADJD", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "des himmels mich geschlagen,", "tokens": ["des", "him\u00b7mels", "mich", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "verkehren sich die rosen all in dorn", "tokens": ["ver\u00b7keh\u00b7ren", "sich", "die", "ro\u00b7sen", "all", "in", "dorn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "PIAT", "APPR", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und mein gesang in klagen.", "tokens": ["und", "mein", "ge\u00b7sang", "in", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJD", "APPR", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "gl\u00fcckselig vor gewest zu sein", "tokens": ["gl\u00fcck\u00b7se\u00b7lig", "vor", "ge\u00b7west", "zu", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJD", "PTKZU", "VAINF"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.6": {"text": "ist jetzund meine gr\u00f6ste pein.", "tokens": ["ist", "je\u00b7tzund", "mei\u00b7ne", "gr