{"textgrid.poem.35141": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Eine Freundesstimme", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du warst bei mir, an meinem Grabe,", "tokens": ["Du", "warst", "bei", "mir", ",", "an", "mei\u00b7nem", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hast nach dem Blumenkranz geschaut.", "tokens": ["Hast", "nach", "dem", "Blu\u00b7men\u00b7kranz", "ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er war die letzte Erdengabe,", "tokens": ["Er", "war", "die", "letz\u00b7te", "Er\u00b7den\u00b7ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor der im Leben mir gegraut.", "tokens": ["Vor", "der", "im", "Le\u00b7ben", "mir", "ge\u00b7graut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O, w\u00fc\u00dftest du, wie man empfindet,", "tokens": ["O", ",", "w\u00fc\u00df\u00b7test", "du", ",", "wie", "man", "emp\u00b7fin\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn solchen Kranz man liegen sieht", "tokens": ["Wenn", "sol\u00b7chen", "Kranz", "man", "lie\u00b7gen", "sieht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "PIS", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sich hinausgetragen findet", "tokens": ["Und", "sich", "hin\u00b7aus\u00b7ge\u00b7tra\u00b7gen", "fin\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PRF", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beim Sterbe-, beim Begr\u00e4bni\u00dflied!", "tokens": ["Beim", "Ster\u00b7be", ",", "beim", "Be\u00b7gr\u00e4b\u00b7ni\u00df\u00b7lied", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "TRUNC", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O, k\u00f6nntest du \u2013 \u2013 \u2013 doch mu\u00df ich schweigen;", "tokens": ["O", ",", "k\u00f6nn\u00b7test", "du", "\u2013", "\u2013", "\u2013", "doch", "mu\u00df", "ich", "schwei\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "PPER", "$(", "$(", "$(", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verstorbenen versagt das Wort,", "tokens": ["Ver\u00b7stor\u00b7be\u00b7nen", "ver\u00b7sagt", "das", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Denn wi\u00df, es giebt lebendge Leichen", "tokens": ["Denn", "wi\u00df", ",", "es", "giebt", "le\u00b7bend\u00b7ge", "Lei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und todte Geister hier wie dort. \u2013 \u2013 \u2013", "tokens": ["Und", "tod\u00b7te", "Geis\u00b7ter", "hier", "wie", "dort", ".", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "KOKOM", "ADV", "$.", "$(", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du warst bei mir, an meinem Grabe,", "tokens": ["Du", "warst", "bei", "mir", ",", "an", "mei\u00b7nem", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hast nach dem letzten Kranz geschaut.", "tokens": ["Hast", "nach", "dem", "letz\u00b7ten", "Kranz", "ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie hat mir einst vor dieser Gabe", "tokens": ["Wie", "hat", "mir", "einst", "vor", "die\u00b7ser", "Ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und vor dem letzten Lied gegraut!", "tokens": ["Und", "vor", "dem", "letz\u00b7ten", "Lied", "ge\u00b7graut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und dieses Graun blieb unverstanden,", "tokens": ["Und", "die\u00b7ses", "Graun", "blieb", "un\u00b7ver\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie's auch zu dir vergebens spricht;", "tokens": ["Wie's", "auch", "zu", "dir", "ver\u00b7ge\u00b7bens", "spricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Mahnung Gottes war vorhanden,", "tokens": ["Die", "Mah\u00b7nung", "Got\u00b7tes", "war", "vor\u00b7han\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jedoch bei uns der Glaube nicht.", "tokens": ["Je\u00b7doch", "bei", "uns", "der", "Glau\u00b7be", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Nun m\u00f6cht ich dir wie gern gestehen,", "tokens": ["Nun", "m\u00f6cht", "ich", "dir", "wie", "gern", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "KOKOM", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df wir gefehlt, da\u00df wir geirrt,", "tokens": ["Da\u00df", "wir", "ge\u00b7fehlt", ",", "da\u00df", "wir", "ge\u00b7irrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sonst mu\u00df es dir wie mir ergehen,", "tokens": ["Sonst", "mu\u00df", "es", "dir", "wie", "mir", "er\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn dir nicht baldigst H\u00fclfe wird. \u2013 \u2013 \u2013", "tokens": ["Wenn", "dir", "nicht", "bal\u00b7digst", "H\u00fcl\u00b7fe", "wird", ".", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "NN", "VAFIN", "$.", "$(", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O, komm noch oft zu meinem Grabe;", "tokens": ["O", ",", "komm", "noch", "oft", "zu", "mei\u00b7nem", "Gra\u00b7be", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Knie nieder dort, und bete still,", "tokens": ["Knie", "nie\u00b7der", "dort", ",", "und", "be\u00b7te", "still", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "ADV", "$,", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und was ich dir zu sagen habe,", "tokens": ["Und", "was", "ich", "dir", "zu", "sa\u00b7gen", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PPER", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sagt dir dein Herz \u2013 \u2013 \u2013 so Gott es will!", "tokens": ["Sagt", "dir", "dein", "Herz", "\u2013", "\u2013", "\u2013", "so", "Gott", "es", "will", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$(", "$(", "$(", "ADV", "NN", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}