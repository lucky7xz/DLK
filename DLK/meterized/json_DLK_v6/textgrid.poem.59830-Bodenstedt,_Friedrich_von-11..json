{"textgrid.poem.59830": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O selig, wem von Urbeginn", "tokens": ["O", "se\u00b7lig", ",", "wem", "von", "Ur\u00b7be\u00b7ginn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJD", "$,", "PWS", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Schicksalsbuch geschrieben ist,", "tokens": ["Im", "Schick\u00b7sals\u00b7buch", "ge\u00b7schrie\u00b7ben", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df er bestimmt zu leichtem Sinn,", "tokens": ["Da\u00df", "er", "be\u00b7stimmt", "zu", "leich\u00b7tem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Trinken und zum Leben ist!", "tokens": ["Zum", "Trin\u00b7ken", "und", "zum", "Le\u00b7ben", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Zorn des Bonzen st\u00f6rt ihn nicht,", "tokens": ["Der", "Zorn", "des", "Bon\u00b7zen", "st\u00f6rt", "ihn", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Moscheenduft bet\u00f6rt ihn nicht,", "tokens": ["Mo\u00b7scheen\u00b7duft", "be\u00b7t\u00f6rt", "ihn", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob er allein \u2013 beim Becher Wein,", "tokens": ["Ob", "er", "al\u00b7lein", "\u2013", "beim", "Be\u00b7cher", "Wein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$(", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ob er beim Lieb geblieben ist!", "tokens": ["Ob", "er", "beim", "Lieb", "ge\u00b7blie\u00b7ben", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Solch Los ist dein, Mirza-Schaffy!", "tokens": ["Solch", "Los", "ist", "dein", ",", "Mir\u00b7za\u00b7Schaffy", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPOSAT", "$,", "NE", "$."], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Genie\u00df es ganz und klage nie!", "tokens": ["Ge\u00b7nie\u00df", "es", "ganz", "und", "kla\u00b7ge", "nie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denk beim Pokal \u2013 da\u00df stets die Zahl", "tokens": ["Denk", "beim", "Po\u00b7kal", "\u2013", "da\u00df", "stets", "die", "Zahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "$(", "KOUS", "ADV", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Der Wochentage sieben ist!", "tokens": ["Der", "Wo\u00b7chen\u00b7ta\u00b7ge", "sie\u00b7ben", "ist", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "CARD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Am ersten Tag beginnt der Lauf", "tokens": ["Am", "ers\u00b7ten", "Tag", "be\u00b7ginnt", "der", "Lauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und erst am letzten h\u00f6rt er auf \u2013", "tokens": ["Und", "erst", "am", "letz\u00b7ten", "h\u00f6rt", "er", "auf", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "VVFIN", "PPER", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie's kommt, so geht's \u2013 bedenke stets", "tokens": ["Wie's", "kommt", ",", "so", "geht's", "\u2013", "be\u00b7den\u00b7ke", "stets"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "$,", "ADV", "VVFIN", "$(", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df Gl\u00fcck nicht aufzuschieben ist!", "tokens": ["Da\u00df", "Gl\u00fcck", "nicht", "auf\u00b7zu\u00b7schie\u00b7ben", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKNEG", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ein leichter Sinn, ein frohes Lied", "tokens": ["Ein", "leich\u00b7ter", "Sinn", ",", "ein", "fro\u00b7hes", "Lied"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist alles, was dir Gott beschied;", "tokens": ["Ist", "al\u00b7les", ",", "was", "dir", "Gott", "be\u00b7schied", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PWS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum la\u00df den Wahn \u2013 verfolg die Bahn,", "tokens": ["Drum", "la\u00df", "den", "Wahn", "\u2013", "ver\u00b7folg", "die", "Bahn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$(", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf die dein Fu\u00df getrieben ist!", "tokens": ["Auf", "die", "dein", "Fu\u00df", "ge\u00b7trie\u00b7ben", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}