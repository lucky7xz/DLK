{"textgrid.poem.49501": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "An Phillis", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.85", "en:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Amor bot einst zu Cythere", "tokens": ["A\u00b7mor", "bot", "einst", "zu", "Cy\u00b7the\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "APPR", "NE"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Mich, als seinen Sklaven feil;", "tokens": ["Mich", ",", "als", "sei\u00b7nen", "Skla\u00b7ven", "feil", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich ward, zu meiner Ehre,", "tokens": ["Und", "ich", "ward", ",", "zu", "mei\u00b7ner", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Holde Phillis, dir zu Theil.", "tokens": ["Hol\u00b7de", "Phil\u00b7lis", ",", "dir", "zu", "Theil", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dann die meine Treue kannten", "tokens": ["Dann", "die", "mei\u00b7ne", "Treu\u00b7e", "kann\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Boten ihm ein Purpurkleid.", "tokens": ["Bo\u00b7ten", "ihm", "ein", "Pur\u00b7pur\u00b7kleid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Eine bot durch Abgesandten", "tokens": ["Ei\u00b7ne", "bot", "durch", "Ab\u00b7ge\u00b7sand\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Einen Korb voll Diamanten:", "tokens": ["Ei\u00b7nen", "Korb", "voll", "Di\u00b7a\u00b7man\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Aber Amor war gescheut.", "tokens": ["A\u00b7ber", "A\u00b7mor", "war", "ge\u00b7scheut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Er verwarf so schlechte Gaben", "tokens": ["Er", "ver\u00b7warf", "so", "schlech\u00b7te", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und entschied f\u00fcr dich den Streit!", "tokens": ["Und", "ent\u00b7schied", "f\u00fcr", "dich", "den", "Streit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Dann du botest mich zu haben", "tokens": ["Dann", "du", "bo\u00b7test", "mich", "zu", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "PTKZU", "VAINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Einen Blick voll Z\u00e4rtlichkeit.", "tokens": ["Ei\u00b7nen", "Blick", "voll", "Z\u00e4rt\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}