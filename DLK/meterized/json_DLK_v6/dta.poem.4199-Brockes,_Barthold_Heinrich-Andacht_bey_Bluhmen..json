{"dta.poem.4199": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Andacht bey Bluhmen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Oew'ger Sch\u00f6pfer aller Welt,", "tokens": ["O\u00b7e\u00b7w'\u00b7ger", "Sch\u00f6p\u00b7fer", "al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PIAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Durch Den sich G\u00e4rten, Wald und Feld,", "tokens": ["Durch", "Den", "sich", "G\u00e4r\u00b7ten", ",", "Wald", "und", "Feld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PRF", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In tausend-f\u00e4rb\u2019ger Pracht, bebl\u00fchmen,", "tokens": ["In", "tau\u00b7sen\u00b7df\u00e4r\u00b7b'\u00b7ger", "Pracht", ",", "be\u00b7bl\u00fch\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den keine Creatur gnug r\u00fchmen,", "tokens": ["Den", "kei\u00b7ne", "Crea\u00b7tur", "gnug", "r\u00fch\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Erheben und verehren kann!", "tokens": ["Er\u00b7he\u00b7ben", "und", "ver\u00b7eh\u00b7ren", "kann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich seh der sch\u00f6nen Bluhmen Pracht,", "tokens": ["Ich", "seh", "der", "sch\u00f6\u00b7nen", "Bluh\u00b7men", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als einen Ausflu\u00df Deiner Macht,", "tokens": ["Als", "ei\u00b7nen", "Aus\u00b7flu\u00df", "Dei\u00b7ner", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auch Deiner Lieb\u2019 und Weisheit, an.", "tokens": ["Auch", "Dei\u00b7ner", "Lieb'", "und", "Weis\u00b7heit", ",", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "KON", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dein Wollen l\u00e4\u00dft, zum Schmuck der Erden,", "tokens": ["Dein", "Wol\u00b7len", "l\u00e4\u00dft", ",", "zum", "Schmuck", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "So viele bunte Wunder werden.", "tokens": ["So", "vie\u00b7le", "bun\u00b7te", "Wun\u00b7der", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Es stammen blo\u00df aus Deinem Willen", "tokens": ["Es", "stam\u00b7men", "blo\u00df", "aus", "Dei\u00b7nem", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die Sch\u00e4tze, so die G\u00e4rten f\u00fcllen;", "tokens": ["Die", "Sch\u00e4t\u00b7ze", ",", "so", "die", "G\u00e4r\u00b7ten", "f\u00fcl\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Durch Dich ist ihre Zierlichkeit,", "tokens": ["Durch", "Dich", "ist", "ih\u00b7re", "Zier\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "In Form- und Farben, zubereit\u2019t;", "tokens": ["In", "For\u00b7m", "und", "Far\u00b7ben", ",", "zu\u00b7be\u00b7reit't", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "$,", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Durch Dich nur f\u00fcllen ihre D\u00fcfte,", "tokens": ["Durch", "Dich", "nur", "f\u00fcl\u00b7len", "ih\u00b7re", "D\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Mit einem Balsam-Schwall, die L\u00fcfte.", "tokens": ["Mit", "ei\u00b7nem", "Bal\u00b7sam\u00b7Schwall", ",", "die", "L\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Sind denn die Bluhmen, ist die Bl\u00fchte,", "tokens": ["Sind", "denn", "die", "Bluh\u00b7men", ",", "ist", "die", "Bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Der Ausbruch Seiner Macht und G\u00fcte,", "tokens": ["Der", "Aus\u00b7bruch", "Sei\u00b7ner", "Macht", "und", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Die GOtt, nur uns zur Lust, beschehrt,", "tokens": ["Die", "Gott", ",", "nur", "uns", "zur", "Lust", ",", "be\u00b7schehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "APPRART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und werden l\u00e4\u00dft, nicht so viel wehrt,", "tokens": ["Und", "wer\u00b7den", "l\u00e4\u00dft", ",", "nicht", "so", "viel", "wehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAINF", "VVFIN", "$,", "PTKNEG", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Da\u00df man, in unsrer Lust, Jhn ehrt?", "tokens": ["Da\u00df", "man", ",", "in", "uns\u00b7rer", "Lust", ",", "Jhn", "ehrt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "APPR", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es ist des Sch\u00f6pfers Gnaden-Wille,", "tokens": ["Es", "ist", "des", "Sch\u00f6p\u00b7fers", "Gna\u00b7den\u00b7Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Menschheit soll vergn\u00fcget seyn.", "tokens": ["Die", "Menschheit", "soll", "ver\u00b7gn\u00fc\u00b7get", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Man siehts, in Bluhmen, an der F\u00fclle", "tokens": ["Man", "siehts", ",", "in", "Bluh\u00b7men", ",", "an", "der", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "APPR", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Bildung, und der Farben Schein,", "tokens": ["Der", "Bil\u00b7dung", ",", "und", "der", "Far\u00b7ben", "Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da beydes wunderw\u00fcrdig pranget.", "tokens": ["Da", "bey\u00b7des", "wun\u00b7der\u00b7w\u00fcr\u00b7dig", "pran\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nun h\u00f6rt, was Er zum Dank verlanget:", "tokens": ["Nun", "h\u00f6rt", ",", "was", "Er", "zum", "Dank", "ver\u00b7lan\u00b7get", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sein Werk empfinden, schmecken, sehn,", "tokens": ["Sein", "Werk", "emp\u00b7fin\u00b7den", ",", "schme\u00b7cken", ",", "sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Erwegen, wie Sein Werk so sch\u00f6n,", "tokens": ["Er\u00b7we\u00b7gen", ",", "wie", "Sein", "Werk", "so", "sch\u00f6n", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sich ihrer freu\u2019n, so viel wir k\u00f6nnen,", "tokens": ["Sich", "ih\u00b7rer", "freu'n", ",", "so", "viel", "wir", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "ADJA", "$,", "ADV", "ADV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erkennen, da\u00df nur Er allein", "tokens": ["Er\u00b7ken\u00b7nen", ",", "da\u00df", "nur", "Er", "al\u00b7lein"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ADV", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der ist, Der sie uns wollen g\u00f6nnen,", "tokens": ["Der", "ist", ",", "Der", "sie", "uns", "wol\u00b7len", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "ART", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch sie Jhn unsern Vater nennen,", "tokens": ["Durch", "sie", "Jhn", "un\u00b7sern", "Va\u00b7ter", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und, Jhm zu Ehren, fr\u00f6hlich seyn,", "tokens": ["Und", ",", "Jhm", "zu", "Eh\u00b7ren", ",", "fr\u00f6h\u00b7lich", "seyn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "APPR", "NN", "$,", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Jm Danken Jhm zu Ehren leben,", "tokens": ["Jm", "Dan\u00b7ken", "Jhm", "zu", "Eh\u00b7ren", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Jhm zu gefallen sich bestreben,", "tokens": ["Jhm", "zu", "ge\u00b7fal\u00b7len", "sich", "be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hei\u00dft Seine Lieb\u2019 und Macht erheben,", "tokens": ["Hei\u00dft", "Sei\u00b7ne", "Lieb'", "und", "Macht", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Hei\u00dft GOtt, was GOttes ist, gegeben.", "tokens": ["Hei\u00dft", "Gott", ",", "was", "Got\u00b7tes", "ist", ",", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PRELS", "NN", "VAFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}