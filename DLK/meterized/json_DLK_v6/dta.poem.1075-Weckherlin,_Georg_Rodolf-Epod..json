, "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "$(", "ADJD", "$(", "KON", "ADV", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Zu den himmeln meinen flug", "tokens": ["Zu", "den", "him\u00b7meln", "mei\u00b7nen", "flug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Seiner gunst fl\u00fcgeln vertrawen/", "tokens": ["Sei\u00b7ner", "gunst", "fl\u00fc\u00b7geln", "ver\u00b7tra\u00b7wen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VVINF", "$("], "meter": "+--+-++-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Fl\u00fcgel/ deren glantz belebet", "tokens": ["Fl\u00fc\u00b7gel", "/", "de\u00b7ren", "glantz", "be\u00b7le\u00b7bet"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "PRELAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Vnder beeden himmeln schwebet.", "tokens": ["Vn\u00b7der", "bee\u00b7den", "him\u00b7meln", "schwe\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}