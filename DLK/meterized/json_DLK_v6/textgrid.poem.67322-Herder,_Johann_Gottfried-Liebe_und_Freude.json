{"textgrid.poem.67322": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Liebe und Freude", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbh\u00fcte Dich,\u00ab sprach einst die Weisheit,", "tokens": ["\u00bb", "h\u00fc\u00b7te", "Dich", ",", "\u00ab", "sprach", "einst", "die", "Weis\u00b7heit", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "$(", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbdu, der Liebe sch\u00f6ner Sohn,", "tokens": ["\u00bb", "du", ",", "der", "Lie\u00b7be", "sch\u00f6\u00b7ner", "Sohn", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und Du, seine Schwester, Freude,", "tokens": ["Und", "Du", ",", "sei\u00b7ne", "Schwes\u00b7ter", ",", "Freu\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil Euch Beiden Uebel drohn.", "tokens": ["Weil", "Euch", "Bei\u00b7den", "Ue\u00b7bel", "drohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Flieh, o Knabe, jene blinde", "tokens": ["Flieh", ",", "o", "Kna\u00b7be", ",", "je\u00b7ne", "blin\u00b7de"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "FM", "NN", "$,", "PDAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlaugesinnte Eifersucht;", "tokens": ["Schlau\u00b7ge\u00b7sinn\u00b7te", "Ei\u00b7fer\u00b7sucht", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und Du, M\u00e4dchen, flieh den Reichthum,", "tokens": ["Und", "Du", ",", "M\u00e4d\u00b7chen", ",", "flieh", "den", "Reicht\u00b7hum", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der, auch blind, Dir immer flucht!\u00ab", "tokens": ["Der", ",", "auch", "blind", ",", "Dir", "im\u00b7mer", "flucht", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "$,", "ADV", "ADJD", "$,", "PPER", "ADV", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Also sprach die gute Weisheit;", "tokens": ["Al\u00b7so", "sprach", "die", "gu\u00b7te", "Weis\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch vergebens war ihr Wort;", "tokens": ["Doch", "ver\u00b7ge\u00b7bens", "war", "ihr", "Wort", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Reichthum ri\u00df so bald die Freude,", "tokens": ["Reicht\u00b7hum", "ri\u00df", "so", "bald", "die", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eifersucht den Amor fort.", "tokens": ["Ei\u00b7fer\u00b7sucht", "den", "A\u00b7mor", "fort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NE", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und seitdem sie zu Gesellen,", "tokens": ["Und", "seit\u00b7dem", "sie", "zu", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zu Geliebten Die gew\u00e4hlt,", "tokens": ["Zu", "Ge\u00b7lieb\u00b7ten", "Die", "ge\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer ist, der die Uebel alle", "tokens": ["Wer", "ist", ",", "der", "die", "Ue\u00b7bel", "al\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "ART", "NN", "PIAT"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dieser Trugverbindung z\u00e4hlt?", "tokens": ["Die\u00b7ser", "Trug\u00b7ver\u00b7bin\u00b7dung", "z\u00e4hlt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Eifersucht betrog den Amor", "tokens": ["Ei\u00b7fer\u00b7sucht", "be\u00b7trog", "den", "A\u00b7mor"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und gab Qualen ihm zu Lohn,", "tokens": ["Und", "gab", "Qua\u00b7len", "ihm", "zu", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nahm ihm seine holden Augen,", "tokens": ["Nahm", "ihm", "sei\u00b7ne", "hol\u00b7den", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denen nie ein Herz entflohn.", "tokens": ["De\u00b7nen", "nie", "ein", "Herz", "ent\u00b7flohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "In des blinden Reichthums Armen", "tokens": ["In", "des", "blin\u00b7den", "Reicht\u00b7hums", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ward die Freud' ein blindes Gl\u00fcck,", "tokens": ["Ward", "die", "Freud'", "ein", "blin\u00b7des", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und an ihrem todten Bilde", "tokens": ["Und", "an", "ih\u00b7rem", "tod\u00b7ten", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00e4rft' sich ihres M\u00f6rders Blick,", "tokens": ["Sch\u00e4rft'", "sich", "ih\u00b7res", "M\u00f6r\u00b7ders", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "So da\u00df Eifersucht und Reichthum", "tokens": ["So", "da\u00df", "Ei\u00b7fer\u00b7sucht", "und", "Reicht\u00b7hum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jetzt allein scharfsehend sind.", "tokens": ["Jetzt", "al\u00b7lein", "scharf\u00b7se\u00b7hend", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Ist es Wunder? Die Betrognen,", "tokens": ["Ist", "es", "Wun\u00b7der", "?", "Die", "Be\u00b7trog\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$.", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Amor und das Gl\u00fcck, sind blind.", "tokens": ["A\u00b7mor", "und", "das", "Gl\u00fcck", ",", "sind", "blind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "NN", "$,", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}