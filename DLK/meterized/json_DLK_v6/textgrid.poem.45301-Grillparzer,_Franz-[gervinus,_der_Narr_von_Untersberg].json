{"textgrid.poem.45301": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[gervinus, der Narr von Untersberg]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gervinus, der Narr von Untersberg,", "tokens": ["Ger\u00b7vi\u00b7nus", ",", "der", "Narr", "von", "Un\u00b7ters\u00b7berg", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Hat einen Namensvetter in Heidelberg,", "tokens": ["Hat", "ei\u00b7nen", "Na\u00b7mens\u00b7vet\u00b7ter", "in", "Hei\u00b7del\u00b7berg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der spielt wie jener alle Rollen,", "tokens": ["Der", "spielt", "wie", "je\u00b7ner", "al\u00b7le", "Rol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "PDAT", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Drob Deutschlands Narren ihm Beifall zollen.", "tokens": ["Drob", "Deutschlands", "Nar\u00b7ren", "ihm", "Bei\u00b7fall", "zol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PPER", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}