{"textgrid.poem.28274": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "89.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist sehr gut,", "tokens": ["Es", "ist", "sehr", "gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Durch Christus Blut", "tokens": ["Durch", "Chris\u00b7tus", "Blut"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Das Ewig-seyn im Himmel erben;", "tokens": ["Das", "E\u00b7wig\u00b7seyn", "im", "Him\u00b7mel", "er\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dann, was die Welt", "tokens": ["Dann", ",", "was", "die", "Welt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PRELS", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Zum h\u00f6chsten h\u00e4lt,", "tokens": ["Zum", "h\u00f6chs\u00b7ten", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ist t\u00e4glich Tod und endlich sterben.", "tokens": ["Ist", "t\u00e4g\u00b7lich", "Tod", "und", "end\u00b7lich", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "NN", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}