{"dta.poem.11423": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Gott wird ein mensch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott ward aus lauter lieb ein armes menschen-kind;", "tokens": ["Gott", "ward", "aus", "lau\u00b7ter", "lieb", "ein", "ar\u00b7mes", "men\u00b7schen\u00b7kind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PIAT", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir dancken ihm mit ha\u00df, und bleiben, wie wir sind.", "tokens": ["Wir", "dan\u00b7cken", "ihm", "mit", "ha\u00df", ",", "und", "blei\u00b7ben", ",", "wie", "wir", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "VVIMP", "$,", "KON", "VVINF", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}