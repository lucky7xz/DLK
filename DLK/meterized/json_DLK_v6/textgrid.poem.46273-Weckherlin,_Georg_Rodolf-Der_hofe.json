{"textgrid.poem.46273": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Der hofe", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Lustig wolte mich gern lehren,", "tokens": ["Der", "Lus\u00b7tig", "wol\u00b7te", "mich", "gern", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "da\u00df der hof gleich dem paradis:", "tokens": ["da\u00df", "der", "hof", "gleich", "dem", "pa\u00b7ra\u00b7dis", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "dem Greuner, den spruch zu verkehren,", "tokens": ["dem", "Greu\u00b7ner", ",", "den", "spruch", "zu", "ver\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "ist der hof wie die h\u00f6ll gewi\u00df,", "tokens": ["ist", "der", "hof", "wie", "die", "h\u00f6ll", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KOKOM", "ART", "NN", "ADV", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "weil da kein unterschid, sprechen sie, der personen.", "tokens": ["weil", "da", "kein", "un\u00b7ter\u00b7schid", ",", "spre\u00b7chen", "sie", ",", "der", "per\u00b7so\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "$,", "VVFIN", "PPER", "$,", "ART", "ADJA", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Ich sag, der hof ist hof, nicht h\u00f6ll noch himmelreich,", "tokens": ["Ich", "sag", ",", "der", "hof", "ist", "hof", ",", "nicht", "h\u00f6ll", "noch", "him\u00b7mel\u00b7reich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VAFIN", "NN", "$,", "PTKNEG", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "da man, weil s\u00fcnd der tugend gleich,", "tokens": ["da", "man", ",", "weil", "s\u00fcnd", "der", "tu\u00b7gend", "gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "KOUS", "ADJD", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "was b\u00f6s, nicht strafen will, noch auch, was gut, belohnen.", "tokens": ["was", "b\u00f6s", ",", "nicht", "stra\u00b7fen", "will", ",", "noch", "auch", ",", "was", "gut", ",", "be\u00b7loh\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADJD", "$,", "PTKNEG", "VVINF", "VMFIN", "$,", "ADV", "ADV", "$,", "PRELS", "ADJD", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}