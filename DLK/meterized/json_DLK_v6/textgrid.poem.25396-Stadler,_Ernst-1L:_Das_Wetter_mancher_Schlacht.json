{"textgrid.poem.25396": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Wetter mancher Schlacht", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Wetter mancher Schlacht", "tokens": ["Das", "Wet\u00b7ter", "man\u00b7cher", "Schlacht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "hat um unsre Nasen gepfiffen,", "tokens": ["hat", "um", "uns\u00b7re", "Na\u00b7sen", "ge\u00b7pfif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wir haben die S\u00e4bel zum Sto\u00df", "tokens": ["Wir", "ha\u00b7ben", "die", "S\u00e4\u00b7bel", "zum", "Sto\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "f\u00fcr manchen Feindesnacken geschliffen", "tokens": ["f\u00fcr", "man\u00b7chen", "Fein\u00b7des\u00b7na\u00b7cken", "ge\u00b7schlif\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVPP"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und unser Blut auf kochen h\u00f6ren,", "tokens": ["Und", "un\u00b7ser", "Blut", "auf", "ko\u00b7chen", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "wenn Hieb und Kugelmusik uns umsausten.", "tokens": ["wenn", "Hieb", "und", "Ku\u00b7gel\u00b7mu\u00b7sik", "uns", "um\u00b7saus\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dann waren N\u00e4chte,", "tokens": ["Dann", "wa\u00b7ren", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "die wir friedsamer durchbrausten,", "tokens": ["die", "wir", "fried\u00b7sa\u00b7mer", "durch\u00b7braus\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Im Feldlager, wenn die Becher \u00fcberliefen,", "tokens": ["Im", "Feld\u00b7la\u00b7ger", ",", "wenn", "die", "Be\u00b7cher", "\u00fc\u00b7berl\u00b7ie\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Kessel schmorten und die W\u00fcrfel rollten \u2013", "tokens": ["Kes\u00b7sel", "schmor\u00b7ten", "und", "die", "W\u00fcr\u00b7fel", "roll\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Das waren Stunden, die wir f\u00fcr alle Seligkeit Mariae", "tokens": ["Das", "wa\u00b7ren", "Stun\u00b7den", ",", "die", "wir", "f\u00fcr", "al\u00b7le", "Se\u00b7lig\u00b7keit", "Ma\u00b7ri\u00b7ae"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "NN", "$,", "PRELS", "PPER", "APPR", "PIAT", "NN", "NE"], "meter": "-+-+----+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "nicht tauschen wollten.", "tokens": ["nicht", "tau\u00b7schen", "woll\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Der Rauch von H\u00f6fen und D\u00f6rfern", "tokens": ["Der", "Rauch", "von", "H\u00f6\u00b7fen", "und", "D\u00f6r\u00b7fern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "hat in unsern Augen gehangen,", "tokens": ["hat", "in", "un\u00b7sern", "Au\u00b7gen", "ge\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.15": {"text": "Um manchen Galgen sind wir", "tokens": ["Um", "man\u00b7chen", "Gal\u00b7gen", "sind", "wir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PIAT", "NN", "VAFIN", "PPER"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.16": {"text": "behutsam herumgegangen.", "tokens": ["be\u00b7hut\u00b7sam", "her\u00b7um\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Oft hat uns der Tod", "tokens": ["Oft", "hat", "uns", "der", "Tod"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.18": {"text": "schon an der Gurgel gesessen,", "tokens": ["schon", "an", "der", "Gur\u00b7gel", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Dann haben wir uns gesch\u00fcttelt,", "tokens": ["Dann", "ha\u00b7ben", "wir", "uns", "ge\u00b7sch\u00fct\u00b7telt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "unsern Schimmel vorgezogen und sind aufgesessen.", "tokens": ["un\u00b7sern", "Schim\u00b7mel", "vor\u00b7ge\u00b7zo\u00b7gen", "und", "sind", "auf\u00b7ge\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "KON", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.21": {"text": "Wir sind in allen L\u00e4ndern herumgefahren,", "tokens": ["Wir", "sind", "in", "al\u00b7len", "L\u00e4n\u00b7dern", "her\u00b7um\u00b7ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "blutige Kesseltreiber,", "tokens": ["blu\u00b7ti\u00b7ge", "Kes\u00b7sel\u00b7trei\u00b7ber", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.23": {"text": "Frankreich lehrte uns die Wollust feiner Betten", "tokens": ["Fran\u00b7kreich", "lehr\u00b7te", "uns", "die", "Wol\u00b7lust", "fei\u00b7ner", "Bet\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.24": {"text": "und das wei\u00dfe Fleisch der Weiber \u2013", "tokens": ["und", "das", "wei\u00b7\u00dfe", "Fleisch", "der", "Wei\u00b7ber", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Aber immer mu\u00dfte Leben \u00fcbersch\u00e4umen,", "tokens": ["A\u00b7ber", "im\u00b7mer", "mu\u00df\u00b7te", "Le\u00b7ben", "\u00fc\u00b7bersc\u00b7h\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.26": {"text": "um sich zu f\u00fchlen,", "tokens": ["um", "sich", "zu", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.27": {"text": "Und keine Schlacht und keine Umarmung", "tokens": ["Und", "kei\u00b7ne", "Schlacht", "und", "kei\u00b7ne", "Um\u00b7ar\u00b7mung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "wollte den Brand in unserm Leibe k\u00fchlen.", "tokens": ["woll\u00b7te", "den", "Brand", "in", "un\u00b7serm", "Lei\u00b7be", "k\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.29": {"text": "Nun rinnt das Blut gemacher", "tokens": ["Nun", "rinnt", "das", "Blut", "ge\u00b7ma\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "in den Adern innen,", "tokens": ["in", "den", "A\u00b7dern", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.31": {"text": "Mein Herz l\u00e4uft durch die alten Bilder nur,", "tokens": ["Mein", "Herz", "l\u00e4uft", "durch", "die", "al\u00b7ten", "Bil\u00b7der", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "um sich zur Einkehr zu besinnen.", "tokens": ["um", "sich", "zur", "Ein\u00b7kehr", "zu", "be\u00b7sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Vor meinem Fenster die gr\u00fcnen Schwarzwaldtannen", "tokens": ["Vor", "mei\u00b7nem", "Fens\u00b7ter", "die", "gr\u00fc\u00b7nen", "Schwarz\u00b7wald\u00b7tan\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "rauschen, als wollten sie von neuen Fahrten sprechen.", "tokens": ["rau\u00b7schen", ",", "als", "woll\u00b7ten", "sie", "von", "neu\u00b7en", "Fahr\u00b7ten", "spre\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "VMFIN", "PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.35": {"text": "Die Holzplanken meiner H\u00fctte krachen in den", "tokens": ["Die", "Holz\u00b7plan\u00b7ken", "mei\u00b7ner", "H\u00fct\u00b7te", "kra\u00b7chen", "in", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "APPR", "ART"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "Novemberst\u00fcrmen und drohen in St\u00fccke zu brechen \u2013", "tokens": ["No\u00b7vem\u00b7bers\u00b7t\u00fcr\u00b7men", "und", "dro\u00b7hen", "in", "St\u00fc\u00b7cke", "zu", "bre\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPR", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Aber ich sitze in Frieden, unbewegt,", "tokens": ["A\u00b7ber", "ich", "sit\u00b7ze", "in", "Frie\u00b7den", ",", "un\u00b7be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "$,", "ADJD", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.38": {"text": "so wie in Engelsr\u00fcstung eingeschlossen.", "tokens": ["so", "wie", "in", "En\u00b7gels\u00b7r\u00fcs\u00b7tung", "ein\u00b7ge\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Nicht Reue und nicht Sehnsucht sollen mir schm\u00e4lern,", "tokens": ["Nicht", "Reu\u00b7e", "und", "nicht", "Sehn\u00b7sucht", "sol\u00b7len", "mir", "schm\u00e4\u00b7lern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "PTKNEG", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "was einst ", "tokens": ["was", "einst"], "token_info": ["word", "word"], "pos": ["PWS", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.41": {"text": "Um mich her, auf dem Tisch,", "tokens": ["Um", "mich", "her", ",", "auf", "dem", "Tisch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APZR", "$,", "APPR", "ART", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.42": {"text": "sind meine lieben B\u00fccher aufgebaut,", "tokens": ["sind", "mei\u00b7ne", "lie\u00b7ben", "B\u00fc\u00b7cher", "auf\u00b7ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Und mein Herz voll ruhiger Freude", "tokens": ["Und", "mein", "Herz", "voll", "ru\u00b7hi\u00b7ger", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "ADJA", "NN"], "meter": "--+-++-+-", "measure": "anapaest.init"}, "line.44": {"text": "in den klaren Himmel hin\u00fcberschaut.", "tokens": ["in", "den", "kla\u00b7ren", "Him\u00b7mel", "hin\u00b7\u00fc\u00b7bersc\u00b7haut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.45": {"text": "Fr\u00fcher hab ich meinem Gott gedient", "tokens": ["Fr\u00fc\u00b7her", "hab", "ich", "mei\u00b7nem", "Gott", "ge\u00b7dient"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.46": {"text": "mit Hieb und Narben so wie heute mit Gebeten,", "tokens": ["mit", "Hieb", "und", "Nar\u00b7ben", "so", "wie", "heu\u00b7te", "mit", "Ge\u00b7be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "KOKOM", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Ich brauche nicht zu zittern, wenn er einst mich ruft,", "tokens": ["Ich", "brau\u00b7che", "nicht", "zu", "zit\u00b7tern", ",", "wenn", "er", "einst", "mich", "ruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "vor seinen Stuhl zu treten.", "tokens": ["vor", "sei\u00b7nen", "Stuhl", "zu", "tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}