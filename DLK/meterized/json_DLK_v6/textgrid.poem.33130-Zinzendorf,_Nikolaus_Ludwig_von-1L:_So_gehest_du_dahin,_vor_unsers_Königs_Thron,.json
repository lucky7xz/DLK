{"textgrid.poem.33130": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "1L: So gehest du dahin, vor unsers K\u00f6nigs Thron,", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So gehest du dahin, vor unsers K\u00f6nigs Thron,", "tokens": ["So", "ge\u00b7hest", "du", "da\u00b7hin", ",", "vor", "un\u00b7sers", "K\u00f6\u00b7nigs", "Thron", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des lieben Gottes-Lamms, um welches L\u00f6wen-Herzen", "tokens": ["Des", "lie\u00b7ben", "Got\u00b7tes\u00b7Lamms", ",", "um", "wel\u00b7ches", "L\u00f6\u00b7wen\u00b7Her\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOUI", "PWAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gem\u00e4chlichkeit und Ruhm, Natur und Kunst verscherzen:", "tokens": ["Ge\u00b7m\u00e4ch\u00b7lich\u00b7keit", "und", "Ruhm", ",", "Na\u00b7tur", "und", "Kunst", "ver\u00b7scher\u00b7zen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das macht, sie sehen Ihn in Lieb und Glauben schon.", "tokens": ["Das", "macht", ",", "sie", "se\u00b7hen", "Ihn", "in", "Lieb", "und", "Glau\u00b7ben", "schon", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "NN