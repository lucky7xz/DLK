{"textgrid.poem.39492": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was ist die Liebe? Les't es, zart geschrieben,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist die Liebe? Les't es, zart geschrieben,", "tokens": ["Was", "ist", "die", "Lie\u00b7be", "?", "Les't", "es", ",", "zart", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "VVFIN", "PPER", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Laut des Worts: es ist ein innig Leben;", "tokens": ["Im", "Laut", "des", "Worts", ":", "es", "ist", "ein", "in\u00b7nig", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "APPR", "ART", "NN", "$.", "PPER", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und Leben ein im Leib gefe\u00dfelt Streben,", "tokens": ["Und", "Le\u00b7ben", "ein", "im", "Leib", "ge\u00b7fe\u00b7\u00dfelt", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "APPRART", "NN", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein sinnlich Bild von ewig geist'gen Trieben.", "tokens": ["Ein", "sinn\u00b7lich", "Bild", "von", "e\u00b7wig", "geist'\u00b7gen", "Trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Mensch nur liebt: doch ist sein erstes Lieben", "tokens": ["Der", "Mensch", "nur", "liebt", ":", "doch", "ist", "sein", "ers\u00b7tes", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "$.", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Lieblichkeit des Leibes hingegeben.", "tokens": ["Der", "Lieb\u00b7lich\u00b7keit", "des", "Lei\u00b7bes", "hin\u00b7ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Will sich, als Leibes Gast, der Geist erheben,", "tokens": ["Will", "sich", ",", "als", "Lei\u00b7bes", "Gast", ",", "der", "Geist", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "$,", "KOUS", "NN", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So wird von Willk\u00fcr die Begier vertrieben.", "tokens": ["So", "wird", "von", "Will\u00b7k\u00fcr", "die", "Be\u00b7gier", "ver\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Doch unaufl\u00f6slich Leib und Geist verweben", "tokens": ["Doch", "un\u00b7auf\u00b7l\u00f6s\u00b7lich", "Leib", "und", "Geist", "ver\u00b7we\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist das Geheimni\u00df aller Lust und Liebe;", "tokens": ["Ist", "das", "Ge\u00b7heim\u00b7ni\u00df", "al\u00b7ler", "Lust", "und", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Leiblich und geistig wird sie Quell des Lebens.", "tokens": ["Leib\u00b7lich", "und", "geis\u00b7tig", "wird", "sie", "Quell", "des", "Le\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "PPER", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Im Manne waltet die Gewalt des Strebens;", "tokens": ["Im", "Man\u00b7ne", "wal\u00b7tet", "die", "Ge\u00b7walt", "des", "Stre\u00b7bens", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Weibes F\u00fcll' umh\u00fcllet stille Triebe:", "tokens": ["Des", "Wei\u00b7bes", "F\u00fcll'", "um\u00b7h\u00fcl\u00b7let", "stil\u00b7le", "Trie\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo Liebe lebt und labt, ist lieb das Leben.", "tokens": ["Wo", "Lie\u00b7be", "lebt", "und", "labt", ",", "ist", "lieb", "das", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "KON", "VVFIN", "$,", "VAFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}